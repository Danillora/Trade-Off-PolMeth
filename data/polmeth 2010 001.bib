@article{ WOS:000277193200013,
Author = {Ferguson, Kennan},
Title = {Subfield Hockey: A Reaction to Matthew Moore's National Survey of
   Political Theorists},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2010},
Volume = {43},
Number = {2},
Pages = {275-277},
Month = {APR},
Abstract = {This survey's importance comes not from what it informs us about
   political theory as a field, but rather from the function it serves
   within debates over the component areas of the field of political
   science. Rather than answering whether political theory ``belongs{''}
   within political science (an unanswerable question), the survey uses
   quantification of qualitative experience and data collection to
   consolidate political theory as a subfield. Thus success of this project
   relies upon and reinforces disciplinary norms, operating as a process
   that attempts to bring a normative political theory into existence. The
   rank ordering of departments, journals, and individual theorists proves
   appealing not only for the competitive, horse-race valuation of those
   people and institutions, but also for how they ultimately resist this
   project by showing the anti-normative heart of political theory: an
   important and useful survey indeed.},
