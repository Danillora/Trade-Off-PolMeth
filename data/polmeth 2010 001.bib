@article{ WOS:000212630500008,
Author = {Brack, Nathalie and Pilet, Jean-Benoit},
Title = {One Country, Two Party Systems? The 2009 Belgian Regional Elections},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2010},
Volume = {20},
Number = {4-5},
Pages = {549-559},
Abstract = {The 2009 regional elections confirmed two trends in Belgian politics.
   First, the political landscape in Flanders remains very fragmented as a
   result of eight competing political parties each having some chance to
   gain representation. The largest party, the Christian Democrats,
   attracted less than 25\% of the votes. Secondly, the electoral results
   also confirmed that Belgium has two different political landscapes
   (Pilet and van Haute, 2008). In Flanders, the political landscape
   remains orientated largely towards the right, with less than one-fifth
   of the total seats going to left-wing parties. The Francophone political
   landscape, on the other hand, confirmed its leftist tendencies when the
   Socialists re-established their leadership in Wallonia.
   Furthermore, two developments are noteworthy: the remarkable score of
   the Greens in the Francophone region and the historical defeat of the
   extreme-right throughout the country. Finally, Belgian politics are
   influenced by asymmetrical and incongruent coalitions amongst the
   various levels of governance.},
