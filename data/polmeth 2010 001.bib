@article{ WOS:000210617500009,
Author = {Callari, Antonio and Ruccio, David F.},
Title = {Rethinking Socialism: Community, Democracy, and Social Agency},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2010},
Volume = {22},
Number = {3},
Pages = {403-419},
Month = {JUL},
Abstract = {In this essay, we show how Marxian theory can contribute to the ongoing
   rethinking of the concepts of ``community'' and ``democracy,''
   especially in relation to the question of ``social agency.'' Our
   discussion is organized around a particular reading of Marx's concept of
   commodity fetishism and broadens the notions of ``the social'' and of
   ``the economy'' beyond the unidimensional concepts that have undergirded
   much of orthodox thinking, transforming them into spaces that are both
   multidimensional (consisting of plural agencies) and polymorphous
   (comprising plural forms of agency).},
