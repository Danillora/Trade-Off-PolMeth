@article{ WOS:000217301200006,
Author = {Gougou, Florent and Labouret, Simon},
Title = {The 2010 French regional elections: Transitional elections in a
   realignment era},
Journal = {FRENCH POLITICS},
Year = {2010},
Volume = {8},
Number = {3},
Pages = {321-341},
Month = {SEP},
Abstract = {This article presents and analyzes original data on the 2010 French
   regional elections. While displaying most of the characteristics of
   `intermediary elections', especially a `sanction vote', the elections
   held on 14 and 21 March 2010 were also a new phase into the realignment
   process initiated by the 2007 presidential and legislative elections.
   Ecology Europe confirmed its third-largest-party status acquired less
   than 1 year ago in the 2009 European elections, resisting the recovery
   of the Socialists by gaining votes from the collapse of the MoDem. The
   shift among centrist voters towards the Left produced a historic balance
   of power between Left and Right. And contrary to President Sarkozy's
   plan, the Far Right enjoyed a strong revival whereas the Far Left was
   unable to make a breakthrough.},
