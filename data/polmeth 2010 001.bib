   Univ Michigan, Interuniv Consortium Polit \& Social Res, Ann Arbor, MI 48106 USA.},
DOI = {10.1177/1354068809345856},
ISSN = {1354-0688},
EISSN = {1460-3683},
Keywords = {ideological stance; industrialized democracies; left-right
