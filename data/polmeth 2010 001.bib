@article{ WOS:000283404200003,
Author = {Marichalar, Pascal},
Title = {Occupational Medicine without Occupational Doctors? French Employers'
   Attitude toward Preventive Services from 1971 to 2010},
Journal = {POLITIX},
Year = {2010},
Volume = {23},
Number = {91},
Pages = {27+},
Abstract = {Based on a historical and sociological inquiry, the article shows that
   the recent reform of French occupational medicine services is part of a
   broader scheme, imagined by French employers since the 1970s. The
   industry-supported organization which runs these services (CISME) has
   used various political and managerial levers to limit professional
   autonomy for occupational physicians, officially because of necessary
   ``pluridisciplinary{''} action in the new paradigm of ``health at
   work{''}. The aim of this shift is to limit the costs of prevention and
   compensation of occupational hazards, as well as the legal risk for
   employers.},
