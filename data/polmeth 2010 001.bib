@article{ WOS:000212878800004,
Author = {Mackin, Glenn},
Title = {Aporia, attentiveness, and the politics of social welfare},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2010},
Volume = {13},
Number = {4},
Pages = {517-539},
Abstract = {Much of the discussion about welfare practice in the US assumes a
   conceptual opposition between democracy and social welfare, with
   democracy associated with autonomy, and welfare associated with
   paternalism. This essay examines the limits of this understanding of the
   problem through an alternative conceptualization, what I call the
   `aporia of social rights'. In the context of political inequality,
   citizens must hear and respond to those needs and claims that are not
   understood. Otherwise, the political process re-creates the very
   exclusions it is meant to overcome. This aporia names the very condition
   of welfare politics, both in the sense that it is the starting point for
   welfare politics, and a description of what must be involved in this
   practice. This conceptualization has the effect of avoiding two
   fundamental problems: treating people as abstractions or stereotypes,
   and the problem in which democratic processes become empty formalities.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
