@article{ WOS:000441109200016,
Author = {Glavachek, P.},
Title = {Political Parties and Society in Contemporary Belarus},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2010},
Number = {2},
Pages = {64+},
Abstract = {The text deals with the conditions of development of the political
   parties in contemporary Belarus. The article sets itself the target of
   analyzing and evaluating the parties' positions not only in the
   political system as such, but mainly from the point of view of the
   perspective of their formation and of their mutual relations in society
   and with society. The author finds that there exist no prolonged steady
   relations between the political parties and most part of society. It
   means that people do not regard the parties as natural and functional
   representatives of their political interests and that the parties, in
   their turn, have not the possibility to function as agents able to make
   political life of society streamlined.},
