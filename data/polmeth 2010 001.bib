Language = {English},
Affiliation = {Calfano, BR (Corresponding Author), Missouri State Univ, Dept Polit Sci, 901 S Natl Ave, Springfield, MO 65807 USA.
   Calfano, Brian Robert, Missouri State Univ, Dept Polit Sci, Springfield, MO 65807 USA.
   Paolino, Philip, Univ N Texas, Dept Polit Sci, Denton, TX 76203 USA.},
DOI = {10.1007/s11109-009-9095-z},
ISSN = {0190-9320},
EISSN = {1573-6687},
