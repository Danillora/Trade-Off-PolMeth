@article{ WOS:000209068300024,
Author = {Bacilo, Marina},
Title = {Talking with One Another, Learning, Remembering A Minsk Contemporary
   Witness Project},
Journal = {OSTEUROPA},
Year = {2010},
Volume = {60},
Number = {5},
Pages = {285+},
Month = {MAY},
Abstract = {At the ``History Workshop,{''} former ghetto inhabitants, concentration
   camp inmates, and forced labourers speak with pupils, students, and
   historians about their fate. Both sides profit from these encounters.
   For the contemporary witnesses, the conversations provide recognition
   and often a first opportunity to tell of their experiences. The old
   offer the young an example of optimism, wisdom, and strength despite all
   physical afflictions. Misery and persecution under National Socialism
   and Stalinism have not made most of them betray their values and
   integrity. That makes the survivors all the more credible.},
