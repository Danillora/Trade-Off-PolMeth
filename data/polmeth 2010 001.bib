@article{ WOS:000281072000002,
Author = {Nichols, Curt and Myers, Adam S.},
Title = {Exploiting the Opportunity for Reconstructive Leadership: Presidential
   Responses to Enervated Political Regimes},
Journal = {AMERICAN POLITICS RESEARCH},
Year = {2010},
Volume = {38},
Number = {5},
Pages = {806-841},
Month = {SEP},
Abstract = {We seek to extend discourse on ``the reconstructive presidency{''} to
   the edge of new frontiers in two interrelated ways. First, we argue that
   reconstructive presidents act within critical junctures in which they
   exploit periodic opportunities to revitalize enervated political
   regimes, but that failure to exploit such opportunities can also occur.
   Second, we clarify the tasks necessary for reconstructive success,
   contending that reconstructive presidents must (a) shift the main axis
   of partisan cleavage, (b) assemble a new majority coalition, and (c)
   institutionalize a new political regime. Through conducting typical and
   crucial case studies, we show how reconstructive dynamics unfold in
   either a straightforward or protracted manner depending on whether
   presidents initially handed reconstructive opportunities, via
   encountering enervated political conditions, succeed in accomplishing
   the tasks we delineate. In doing so, we depart from previous
   interpretations and recast the ``System of 1896{''} as a successful
   reconstruction.},
