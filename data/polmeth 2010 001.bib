@article{ WOS:000420539100003,
Author = {Klenk, Tanja and Nullmeier, Frank},
Title = {Welfare industries: enterprises as providers of public goods},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2010},
Volume = {4},
Number = {1},
Pages = {29-52},
Month = {APR},
Abstract = {Business interests and social policy interests are not mutually
   exclusive-the Varieties of Capitalism-literature (VOC) has demonstrated
   this convincingly. This paper shares this assumption, but stresses
   another explanation. In contrast to VOC literature, it focuses on
   enterprises not as employers, but as providers of social security
   products. In doing so, the paper tries to highlight a development which
   has been neglected by the recent discussion on behalf of the role of
   enterprises in welfare provision: the rise of a welfare industry. The
   paper discusses the role of enterprises as providers of welfare goods in
   a comparative perspective taking the case of private hospitals in
   Germany and the US as an example.},
