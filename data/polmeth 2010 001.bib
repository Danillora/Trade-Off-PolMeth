@article{ WOS:000410202700005,
Author = {Hillygus, D. Sunshine and Henderson, Michael},
Title = {Political Issues and the Dynamics of Vote Choice in 2008},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2010},
Volume = {20},
Number = {2},
Pages = {241-269},
Abstract = {The 2008 American presidential contest occurred amidst economic
   conditions unlike any seen in decades. Media assessments have often
   attributed Barack Obama's victory to the faltering economy, particularly
   the financial crisis that erupted just seven weeks before election day.
   In this article we assess the role of the economy and other political
   issues on vote choice, and find that the impact of the economic crisis
   is more nuanced than is often assumed. We find that while the economy
   did matter for the general election, so too did social issues. More
   interestingly, the collapse itself seemed to have only a minor impact
   because so many people had already made up their minds before the
   collapse. Finally, we show that while Obama benefited from the economy
   in the general election, it may have actually worked against him in the
   primary phase of the contest.},
