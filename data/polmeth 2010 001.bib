@article{ WOS:000210514400005,
Author = {Peschek, Joseph},
Title = {American Politics Today: An Interview with Frances Fox Piven},
Journal = {NEW POLITICAL SCIENCE},
Year = {2010},
Volume = {32},
Number = {3},
Pages = {427-430},
Abstract = {Readers of New Political Science know Frances Fox Piven as the author or
   co- author of a series of path-breaking articles and books on the
   development of American social policies, political mobilization and
   voter turnout, and the attack on the welfare state. She is also a
   supporter of long standing of this journal and its sponsor, the Caucus
   for a New Political Science. In the last year Piven has come under
   attack by FOX News host Glenn Beck and other right-wing activists for
   ideas she developed in the 1960s with the late Richard Cloward. Beck and
   others have been keen to link Piven and Cloward's analysis in farfetched
   ways to the economic policies of the Obama administration. In this
   interview Piven interprets the meaning of the right-wing attacks and
   discusses the state of contemporary American politics more broadly. She
   comments on the Obama administration's major policies and the prospects
   for political mobilization and offers advice for progressive scholars in
   the early stages of their careers.},
