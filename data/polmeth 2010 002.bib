@article{ WOS:000282177800011,
Author = {Graves, Scott E. and Howard, Robert M.},
Title = {Ignoring Advice and Consent? The Uses of Judicial Recess Appointments},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2010},
Volume = {63},
Number = {3},
Pages = {640-653},
Month = {SEP},
Abstract = {The authors seek to answer the questions of why presidents use the power
   to temporarily seat federal court judges during recesses of the Senate.
   The use of the recess power can upset the carefully calculated
   separation of powers envisioned by the framers, shifting power away from
   one branch of government toward another. Examining every judicial recess
   appointment from 1789 to 2004, the authors discover that presidents are
   conditionally strategic in their use of the unilateral authority to
   appoint federal court judges during Senate recesses but that the use of
   this power is careful and spare, especially in the modern era.},
