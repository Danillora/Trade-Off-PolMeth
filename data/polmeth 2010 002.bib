@article{ WOS:000281049500004,
Author = {Jessee, Stephen and Malhotra, Neil},
Title = {Are Congressional Leaders Middlepersons or Extremists? Yes.},
Journal = {LEGISLATIVE STUDIES QUARTERLY},
Year = {2010},
Volume = {35},
Number = {3},
Pages = {361-392},
Month = {AUG},
Abstract = {Influential theories of legislative organization predict that
   congressional leaders will be selected from the center of their parties.
   Yet previous research has generally rejected the ``middleperson
   hypothesis,{''} finding leaders to be extremists. We challenged these
   findings by testing more-appropriate null hypotheses via Monte Carlo
   simulation. We found that congressional leaders (and leadership
   candidates as a whole) tend to be closer to their party's median than
   would occur by chance, but leaders also tend to be selected from the
   left of the median for Democrats and to the right for Republicans.
   Compared to the pool of announced candidates for leadership positions,
   winners are not ideologically distinctive. This result suggests that
   factors affecting the ideology of leaders tend to operate more at the
   candidate emergence stage.},
