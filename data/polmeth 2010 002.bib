@article{ WOS:000277143000001,
Author = {Gel'man, Vladimir},
Title = {Dead End Authoritarian Modernisation in Russia},
Journal = {OSTEUROPA},
Year = {2010},
Volume = {60},
Number = {1},
Pages = {3+},
Month = {JAN},
Abstract = {Russia has to modernise. There is a widespread assumption that this can
   happen only by way of an authoritarian modernisation. Only the
   bureaucracy, the army, and the ruling party come into question as the
   supporters of such a course of modernisation. But in Russia, none of
   these institutions is interested in modernisation, to say nothing of
   being in a position to implement it. The Putin regime has manoeuvred
   Russia into a dead end. Only insight into the necessity of a
   simultaneous reform of the political system, the state structure, and
   the economy offers Russia the chance of embarking on the path of
   modernisation.},
