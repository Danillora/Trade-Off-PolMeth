@article{ WOS:000281950800010,
Author = {Bradbury, Jonathan},
Title = {Wales and the 2010 General Election},
Journal = {PARLIAMENTARY AFFAIRS},
Year = {2010},
Volume = {63},
Number = {4},
Pages = {726-741},
Month = {OCT},
Abstract = {Wales is distinctive in British general elections for its higher than
   average levels of support for the Labour Party. However, following
   evidence of Labour decline and public opposition after its unprecedented
   13 years of office, there were questions over whether perceived Labour
   one-partyism in Wales would be finally superseded. In practice, although
   the election resulted in an increased fragmentation of vote share, the
   Labour Party lost only three seats net and its dominance in the Welsh
   party system was basically preserved. The implications of the election
   for politics in the National Assembly for Wales, where a more
   proportional electoral system and multi-level voting have contributed to
   lower vote shares for Labour and higher ones for Plaid Cymru, also
   appeared to favour the Labour Party. Party campaigns in the British-wide
   parties to a large extent followed leads set by the British-level
   leadership, although there were specific Welsh dimensions. Plaid Cymru
   shared a platform with the SNP in seeking to stand up for Wales and
   Scotland against expected expenditure cuts. Overall, while the
   Conservative Party enjoyed some renewed success, the election in Wales
   was primarily notable for the Labour Party holding on to its dominant
   position in Westminster elections despite very adverse circumstances and
   an historic drop in vote share.},
