@article{ WOS:000217302300001,
Author = {Kuhn, Raymond},
Title = {`Les medias, c'est moi.' President Sarkozy and news media management},
Journal = {FRENCH POLITICS},
Year = {2010},
Volume = {8},
Number = {4},
Pages = {355-376},
Month = {DEC},
Abstract = {This article examines the news media management activities of President
   Sarkozy through a selective analysis of the agenda building, issue
   framing and image projection functions carried out by the Elysee. The
   article argues that in the context of Fifth Republic presidentialism
   Sarkozy has devoted more attention to media management than any of his
   predecessors. Yet although within a nationally bounded historical
   framework on, the media-related activities of the Sarkozy presidency are
   unprecedented in their scale and scope, from a cross-national
   perspective the more prosaic conclusion is that the practices and style
   of executive news management in contemporary France have increasingly
   converged with those to be found in other major Western democracies.},
