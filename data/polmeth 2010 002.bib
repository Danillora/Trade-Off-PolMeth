@article{ WOS:000281851400004,
Author = {Lubbers, Marcel and Scheepers, Peer},
Title = {Divergent trends of euroscepticism in countries and regions of the
   European Union},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
Year = {2010},
Volume = {49},
Number = {6},
Pages = {787-817},
Month = {OCT},
Abstract = {Changes in different aspects of euroscepticism developed at different
   paces and in varying directions in the regions and countries of the
   European Union (EU) from 1994 to 2004. Using Eurobarometer data, along
   with data on country and region characteristics, information on the
   positions of the political parties and media attention paid to the EU,
   it is tested in detail whether opposite developments in euroscepticism
   were associated with opposite developments in influencing contextual
   characteristics. The authors found that the Netherlands became
   systematically more sceptical towards the EU, whereas the opposite trend
   was found in Spain. The introduction of the Euro partially explains
   these divergent trends, but the direction of this effect varies with
   countries' GDP. Changes in media attention on the EU further explain the
   changes in the public's attitude. However, this effect is contingent
   upon specific circumstances. Growing media attention increases political
   euroscepticism in countries with a negative EU budget balance, whereas
   it decreases such scepticism in countries with a positive balance. The
   effect of left-right ideological placement is contingent upon the EU
   budget balance as well. Finally, the effect of education on
   euroscepticism is found to be smaller in countries with a higher GDP.},
