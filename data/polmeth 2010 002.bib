@article{ WOS:000445526800004,
Author = {Maskarinec, Pavel},
Title = {The Mongolian Transition to Democracy from the Point of View of
   Transition Theory},
Journal = {POLITOLOGICKY CASOPIS-CZECH JOURNAL OF POLITICAL SCIENCE},
Year = {2010},
Number = {3},
Pages = {271-297},
Abstract = {This article deals with the democratic transition in Mongolia. In the
   first part the author analyses the character of the Mongolian communist
   regime, following the theory of nondemocratic regimes developed by Juan
   J. Linz and Alfred Stepan. The paper argues that the Mongolian communist
   regime can be classified as early post-totalitarian. The second part of
   the article focuses on the transition process. We apply theoretical
   concepts developed by Dankwart A. Rustow and Adam Przeworski, and
   typologies of transition by Samuel P. Huntington, Terry Lynn Karl and
   Phillippe Schmitter. The author's conclusion is that the political
   change in Mongolia is possible to designate as a reform, using
   Karl-Schmitter's typology, which the most advantage is the ability to
   evaluate the role of mass mobilization during the transition process.},
