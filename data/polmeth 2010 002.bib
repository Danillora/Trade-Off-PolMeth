@article{ WOS:000210612100006,
Author = {Tedman, Gary},
Title = {Origins of Kitsch},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2010},
Volume = {22},
Number = {1},
Pages = {56-67},
Month = {JAN},
Abstract = {Kitsch, literally ``scraped-up rubbish from the streets'' or ``bad
   art,'' implies a hierarchy of quality, in ``taste,'' in sensing, in art.
   There are many controversies over the use of the term-/about elitism,
   snobbery/inverted snobbery, the ``eye of the beholder,''
   ``self-conscious kitsch,'' and so on. These largely philosophical issues
   are left to arguments elsewhere. The claim here is that, by looking at
   kitsch's origins as a social phenomenon, these issues are (mostly)
   either solved by default or rendered irrelevant. The essay ``accounts
   for taste'': kitsch is a material manifestation of class struggle. It
   judges its political effect and its historical agency.},
