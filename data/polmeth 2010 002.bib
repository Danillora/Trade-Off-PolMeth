@article{ WOS:000283875800002,
Author = {Moroff, Anika},
Title = {Party bans in Africa - an empirical overview},
Journal = {DEMOCRATIZATION},
Year = {2010},
Volume = {17},
Number = {4, SI},
Pages = {618-641},
Abstract = {With the re-introduction of multiparty politics at the beginning of the
   1990s most African countries started to regulate political parties
   extensively, especially ethnic and other particularistic parties. This
   article presents a new database on particularistic and other party bans
   in sub-Saharan Africa, drawing on a comprehensive collection of African
   party regulations and records of their implementation since 1990. After
   demonstrating the magnitude and complexity of party bans in Africa the
   article deals in depth with enforcement institutions and legal
   procedures. It shows that party bans are especially pervasive in less
   democratic countries and have often targeted politically relevant
   parties.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
