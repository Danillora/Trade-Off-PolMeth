@article{ WOS:000287350100006,
Author = {Lutz, Georg},
Title = {The Electoral Success of Beauties and Beasts},
Journal = {SWISS POLITICAL SCIENCE REVIEW},
Year = {2010},
Volume = {16},
Number = {3, SI},
Pages = {457-480},
Abstract = {Issue positions, political programmes or past performances of candidates
   and parties explain a voter's choice only partially. Other more simple
   mechanisms play a role in a voter's choice too. In this article we
   explore the influence of physical attractiveness on the number of
   preference votes cast for 744 candidates in the National Council
   elections of Switzerland in 2007. The Swiss open ballot PR electoral
   system offers a good opportunity to study such effects because voters
   not only choose between parties or between candidates, but they also
   choose candidates from within parties. We find a robust influence of
   physical attractiveness on the number of preference votes a candidate
   received and find that attractiveness is equally important for men
   and/or women. This influence of attractiveness is connected to the
   higher likelihood of attractive candidates being noticed.},
Publisher = {WILEY},
