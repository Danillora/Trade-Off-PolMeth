@article{ WOS:000212024300004,
Author = {Lilja, Mona},
Title = {Discourses of Hybrid Democracy: The Case of Cambodia},
Journal = {ASIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2010},
Volume = {18},
Number = {3},
Pages = {289-309},
Abstract = {When implementing democracy, local discourses of decision-making affect
   the ways in which the liberal democracy is comprehended, realized and
   practiced. One problem with the so-called `transition paradigm' is then
   the neglect of local cultures and institutions and their impact on
   implemented democratic systems. Given this, the aim of the article is
   therefore to give a deep(er) understanding of the processes of change in
   implemented democracies through a close empirical reading of interviews
   with Cambodian politicians and non-governmental organizations (NGOs). A
   critical examination of the conditions in Cambodia reveals how liberal
   democracy is not only re-interpreted and hybridized but also
   occasionally resisted in line with the local discourses of power.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
