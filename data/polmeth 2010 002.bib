@article{ WOS:000281348900006,
Author = {Flender, Reinhard},
Title = {The Story of a Friendship Dmitrii Shostakovich and Mieczyslaw Weinberg},
Journal = {OSTEUROPA},
Year = {2010},
Volume = {60},
Number = {7},
Pages = {79+},
Month = {JUL},
Abstract = {Longstanding friendships between artists are rare, for artists are to a
   high degree egocentric. A close friendship bound Dmitrii Shostakovich
   and Mieczyslaw Weinberg for over 32 years. The polarity of temperaments
   enhanced their creative powers. The work on their own compositions, the
   uplifting experience of give and take, bound both composers. This
   productive and creative use of ``common stylistic devices{''} is an
   exception in the history of music in the 20th century.},
Publisher = {BWV-BERLINER WISSENSCHAFTS-VERLAG GMBH},
Address = {AXEL-SPRINGER-STR 54 A, BERLIN, D-10117, GERMANY},
Type = {Article},
