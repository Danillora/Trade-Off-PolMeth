@article{ WOS:000282457600001,
Author = {Auer, Stefan},
Title = {The European Union and the Origins of Free Europe German-French
   Reconciliation and the Legacy of 1989},
Journal = {OSTEUROPA},
Year = {2010},
Volume = {60},
Number = {8},
Pages = {3+},
Month = {AUG},
Abstract = {The European Union puts a great deal of effort into encouraging a
   European identity among its citizens. To this end, it draws on the means
   and methods of nation-states. But this identity policy has its limits,
   for Europe is not a nation, and the EU is not a nation-state. Above all,
   the different historical experiences can hardly be reduced to a single
   story line. Since eastern enlargement, German-French reconciliation has
   lost its bonding force as the legitimacy-creating, historical core of
   European integration. How the quest for freedom in East Central Europe
   before 1989 is handled is decisive. Those who try to instrumentalise the
   legacy of 1989 can end up damaging the credibility of the EU instead of
   boosting its legitimacy.},
