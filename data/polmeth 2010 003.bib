DOI = {10.1177/0010414010376915},
ISSN = {0010-4140},
EISSN = {1552-3829},
Keywords = {democratization; inequality; regime change; modernization theory},
Keywords-Plus = {INCOME INEQUALITY; DEMOCRACY},
