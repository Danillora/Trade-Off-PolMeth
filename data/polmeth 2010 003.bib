@article{ WOS:000285118800002,
Author = {Ingold, Karin},
Title = {Learning for the future: An Analysis of Swiss Climate Policy},
Journal = {SWISS POLITICAL SCIENCE REVIEW},
Year = {2010},
Volume = {16},
Number = {1},
Pages = {43-76},
Abstract = {This article contributes to the public policy analysis in Swiss
   political science. Applying a two-dimensional approach, it explains the
   current output of the Swiss climate policy, taking into account
   structures and preferences of the Swiss political elite. Following
   the-Advocacy Coalition Framework, the author studies the existence of
   coalitions, power relations and policy preferences and proceeds in two
   steps: First, social network analysis frames the empirical study of
   structures and relations within the actors' network around the CO(2) law
   decision making process. Second, using a sophisticated mathematical
   algorithm, the multicriteria analysis furnishes a systematic evaluation
   of elite's belief system. This methodological combination constitutes
   the added value of this research and allows complementary conclusions
   explaining current climate policy choice. The main goal of this
   contribution finally, is to outline what can be learnt for climate
   policy formulation after 2010/2012.},
