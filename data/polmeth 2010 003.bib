@article{ WOS:000273803900002,
Author = {Armijo, Leslie Elliott and Burges, Sean W.},
Title = {Brazil, the Entrepreneurial and Democratic BRIC},
Journal = {POLITY},
Year = {2010},
Volume = {42},
Number = {1},
Pages = {14-37},
Month = {JAN},
Abstract = {By most objective metrics, Brazil is the least imposing of the ``BRICs
   countries{''}-less populous than China and Indio, slower-growing in
   recent years than China, India, or Russia, and the only member of the
   group locking nuclear weapons. We argue that Brazil material
   capabilities are more significant than commonly supposed. Moreover
   Brazil democratic transition in the mid-1980s, along with that of its
   neighbors, has for the first time enabled Brazil to realize its promise
   of becoming a regional leader in South America. On the basis of its
   democratic and regional prominence, Brazil has become on effective
   political entrepreneur at the global level, initiating and participating
   in multilateral fora as diverse as the trade G20, the financial G20, and
   now the BRICs club. On issues of style, inclusion, and distributive
   justice, Brazil reliably sides with the ``South.{''} Yet its core public
   policy instincts embrace familiar ``Northern{''} preferences: liberal,
   and mixed-capitalist, democracy. Polity (2010) 42, 14-37.
