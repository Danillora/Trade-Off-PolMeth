@article{ WOS:000278111600005,
Author = {Giammo, Joseph D. and Brox, Brian J.},
Title = {Reducing the Costs of Participation Are States Getting a Return on Early
   Voting?},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2010},
Volume = {63},
Number = {2},
Pages = {295-303},
Month = {JUN},
Abstract = {The authors address the puzzle of why governments have implemented
   methods of early voting when those methods appear not to have an effect
   on turnout. Using an aggregate analysis, the authors find that early
   voting seems to produce a short-lived increase in turnout that
   disappears by the second presidential election in which it is available.
   They also address whether the additional costs to government are worth
   the negligible increase in participation. They conclude that these
   reforms merely offer additional convenience for those already likely to
   vote.},
