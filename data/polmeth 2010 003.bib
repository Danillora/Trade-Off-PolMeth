@incollection{ WOS:000279880000012,
Author = {Bhattarai, Keshav and Conway, Dennis},
Editor = {Devkota, SR},
Title = {INCREASED URBAN GROWTH RESULTS IN INCREASED VULNERABILITY: HUMAN/HAZARDS
   INTERACTIONS IN THE UNCONTROLLED KATHMANDU VALLEY URBAN CONURBATION},
Booktitle = {NEPAL IN THE 21ST CENTURY},
Series = {Global Politcal Studies},
Year = {2010},
Pages = {259-284},
Abstract = {This chapter analyzes how rapid urban growth in the Kathmandu Valley has
   increased urban vulnerabilities. This study uses aerial photos,
   cartographic maps, satellite images, and aspatial data on GIS platforms
   to evaluate vulnerabilities associated with the Valley's extremely rapid
   urban spatial growth from 1955 to the present- 2008. We also analyze
   urban population growth dynamics and rapid infilling of open spaces,
   vehicular congestion, and severe problems of accesses for emergency
   vehicles to provide both general and emergency services. Our spatial
   analyses using GIS/Remote Sensing(RS) technologies suggest that urban
   spatial growth has been haphazard within the core area and rural-urban
   fringes; with unsafe building structures increasing in and around the
   Valley's many urban cores and along the side of transportation routes.
   While much of the Valley's agricultural land has been converted to urban
   uses, the infilling of open spaces has further damaged the regional
   balance and threatened its overall environmental quality. In addition, a
   proliferation of poorly constructed housing in spontaneous squatter
   settlements, which are sited in marginal locations throughout the
   Valley, has only exacerbated the vulnerability of their populations to
   seismic disturbances, flooding and landslides. Further analyses of high
   resolution aerial and World View-1 images reveals that unplanned
   infrastructure developments, narrow streets and the lack of accessible
   passages, increasing air and water pollution, and problems of waste
   disposal have made the Valley- once known for the harmony of its
   architecture and supporting fertile lands-more vulnerable than ever
   before. Taken together, these GIS/RS analyses lead us to the conclusion
   that considerable urban restructuring is desperately needed, under the
   management, GIS/RS monitoring and guidance of a much better informed and
   stricter regulatory regime than is presently in place, to ensure the
   sustainability of this historic city, and reverse, or at least hold up,
   the current dysfunctional slide into a heightened state of vulnerability
   and urban crisis.},
