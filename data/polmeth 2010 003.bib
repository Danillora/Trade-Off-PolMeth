@article{ WOS:000410205900001,
Author = {Iyengar, Shanto and Curran, James and Lund, Anker Brink and
   Salovaara-Moring, Inka and Hahn, Kyu S. and Coen, Sharon},
Title = {Cross-National versus Individual-Level Differences in Political
   Information: A Media Systems Perspective},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2010},
Volume = {20},
Number = {3},
Pages = {291-309},
Abstract = {We propose a context-dependent approach to the study of political
   information. Combining a content analysis of broadcast news with a
   national survey measuring public awareness of various events, issues,
   and individuals in the news, we show that properties of national media
   systems influence both the supply of news and citizens' awareness of
   events in the news. Public service-oriented media systems deliver hard
   news more frequently than market-based systems. It follows that for
   citizens living under public service regimes, the opportunity costs of
   exposure to hard news are significantly lowered. Lowered costs allow
   less interested citizens to acquire political knowledge. Our analyses
   demonstrate that the knowledge gap between the more and less interested
   is widest in the US and smallest in Scandinavia.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
