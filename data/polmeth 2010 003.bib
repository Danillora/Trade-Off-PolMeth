@article{ WOS:000208579600006,
Author = {Moschella, Manuela},
Title = {International financial governance in hard times: tracing the
   transformations},
Journal = {CONTEMPORARY POLITICS},
Year = {2010},
Volume = {16},
Number = {4},
Pages = {421-436},
Abstract = {This article reviews the tenets of the intellectual consensus on how to
   organize the international financial system that came to crystallize at
   the end of the 1990s and the contestation of such a consensus in the
   aftermath of the global financial crisis of 2007-09. Illustrating the
   path of ideational transformation from the early 1990s until the present
   time, the article builds on recent constructivist works in international
   political economy that take economic ideas held by agents as the
   principal unit of analysis. In doing so, it brings to the surface both
   the substantive changes that had taken place in the principles
   underlying the governance of the international financial system and the
   dynamics of ideational change. Specifically, the article suggests a
   shift away from a governance project based on the dispersion of
   supervisory authority and finds that new policy ideas of regulation and
   political centralization have all been conceived with negative reference
   to the past.},
