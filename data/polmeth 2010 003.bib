@article{ WOS:000212962000005,
Author = {Chamberlain, Adam},
Title = {An Inside-Outsider or an Outside-Insider? The Republican Primary
   Campaign of Ron Paul from a Third-Party Perspective},
Journal = {POLITICS \& POLICY},
Year = {2010},
Volume = {38},
Number = {1},
Pages = {97-116},
Month = {FEB},
Abstract = {The 2007/2008 Republican presidential primary run for Ron Paul was a
   relatively unique candidacy, as he was a former third-party presidential
   candidate for the Libertarian Party in 1988. In this article, I argue
   that his recent run as a Republican needs to be studied as a third-party
   movement within a major party. To do so, I conduct a state-level
   analysis of support for Paul using a dependent variable measuring the
   number of online Paul donors. The results indicate that the strongest
   explanation for Paul support is a state's vote for Paul in 1988. This
   finding shows that a third-party campaign can help a politician's
   national-level career rather than serving merely as an outlet for
   popular discontent. It also shows that candidate-centered support can
   carry over into a major-party campaign, even if almost 20 years has
   elapsed and a candidate's third-party vote percentage was quite low.},
