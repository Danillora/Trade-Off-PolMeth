@incollection{ WOS:000279537100013,
Author = {Wedeen, Lisa},
Editor = {Levi, M and Jackman, S and Rosenblum, N},
Title = {Reflections on Ethnographic Work in Political Science},
Booktitle = {ANNUAL REVIEW OF POLITICAL SCIENCE, VOL 13},
Series = {Annual Review of Political Science},
Year = {2010},
Volume = {13},
Pages = {255-272},
Abstract = {The objectivist truth claims traditionally pressed by most political
   scientists have made the use of ethnographic methods particularly
   fraught in the discipline. This article explores what ethnography as a
   method entails. It makes distinctions between positivist and
   interpretivist ethnographies and highlights sonic of the substantive
   contributions ethnography has made to the study of politics. Lamenting
   the discipline's abandonment of a conversation with anthropology after
   Geertz, this review also insists on moving beyond the anthropological
   controversies so powerfully expressed in the edited volume Writing
   Culture (1986) and other texts of the 1980s and 1990. I contend that
   interpretive social science does not have to forswear generalizations or
   causal explanations and that ethnographic methods can be used in the
   service of establishing them. Rather than fleeing from abstractions,
   ethnographies can and should help ground them.},
