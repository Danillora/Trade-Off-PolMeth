@article{ WOS:000274916300008,
Author = {Cassino, Dan and Erisen, Cengiz},
Title = {Priming Bush and Iraq in 2008:A Survey Experiment},
Journal = {AMERICAN POLITICS RESEARCH},
Year = {2010},
Volume = {38},
Number = {2, SI},
Pages = {372-394},
Month = {MAR},
Abstract = {Using a question-order experiment, half the respondents in a national
   RDD (random digit dial) likely voter survey taken just prior to the 2008
   Presidential Primary election were primed to think about President Bush
   and the war in Iraq before making their candidate choice. Results show
   that the priming had a significant effect on their candidate choice, and
   that priming individuals to think about the war significantly aided the
   candidacy of eventual Democratic nominee Barack Obama, more than
   doubling his support, and hurt then Republican front-runner Rudy
   Giuliani, cutting his support almost in half.},
