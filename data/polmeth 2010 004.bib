@article{ WOS:000218360400007,
Author = {Cassidy, Robert M.},
Title = {THE AFGHANISTAN CHOICE PEACE OR PUNISHMENT IN THE PASHTUN BELT},
Journal = {RUSI JOURNAL},
Year = {2010},
Volume = {155},
Number = {4},
Pages = {38-44},
Abstract = {The war presses steadily on in Afghanistan, with proponents arguing the
   rationale for either counter-terrorism or counter-insurgency as the
   approach for achieving the most favourable outcome. But neither alone
   can unseat the enduring alliance between the Taliban, AI-Qa'ida and the
   Pashtun people who straddle the Durand Line. The Pashtun Belt is the key
   to success in this conflict: winning the support of the population there
   requires a combination of both approaches in a complementary way to
   stabilise the region, and to deny it to the likes of AI-Qa'ida and its
   ilk, while aiming for a broader regional rapprochement in the longer
   term.},
