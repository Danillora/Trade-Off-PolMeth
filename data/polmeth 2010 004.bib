@article{ WOS:000283018200005,
Author = {Tenscher, Jens},
Title = {Modes of Regional Political Communication. Political Leadership as
   Communicative Task in German Federal States},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2010},
Volume = {39},
Number = {3},
Pages = {303+},
Abstract = {Communicative competencies have always been essential resources for
   political leaders' success both in spheres of political negotiation and
   decision-making as well as in public arenas of political presentation.
   Exchanges with external reference groups and followers particularly ask
   for adequate strategies of political communication; all the more since
   mass media and new media have rapidly been expanding over the last years
   while ``classical{''} intermediary powers have been confronted with
   dealignment processes and alienated citizens. Against the backdrop of
   such fundamental transformations in the media and sociocultural
   environment political leadership has become more and more dependent on
   communicative and performative tasks. Yet, there are variations in the
   way such obligations are interpreted depending on political actors'
   personality and institutional contexts. Those contexts are somewhat
   special on the regional level. This becomes obvious when we empirically
   look at individual modes of regional political communication which this
   article offers for the first time. In addition to a typology it provides
   individual and institutional explanations for variations in the way
   political leadership develops in regional arenas of political publics.},
Publisher = {FACULTAS VERLAGS \& BUCHHANDELS AG},
Address = {BERGGASSE 5, VIENNA, A-1090, AUSTRIA},
