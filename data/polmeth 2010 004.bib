@article{ WOS:000212422600002,
Author = {Bacon, Michael and Sloam, James},
Title = {John Dewey and the Democratic Role of Higher Education in England},
Journal = {JOURNAL OF POLITICAL SCIENCE EDUCATION},
Year = {2010},
Volume = {6},
Number = {4},
Pages = {336-352},
Abstract = {Education policy in England has been criticized for an overemphasis on
   narrow performance targets and the adoption of private sector
   principles. Universities and colleges have increasingly been viewed in
   terms of their value for the U.K. economy. This article argues that
   there are sound ethical (philosophical), political, and pedagogical
   reasons for a more rounded approach. To reassert a broader vision of
   education we require an ethical base, which we argue is provided by
   political philosophy in the writings of John Dewey. For Dewey, education
   must be viewed in the context of its role in democratic society. The
   article identifies basic principles that relate to Dewey's belief in
   democratic education and develops general indicators to test these
   principles. These indicators are then used to evaluate higher education
   and political science education in England.},
