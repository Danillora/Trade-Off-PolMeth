@article{ WOS:000281309700002,
Author = {Rushing, Sara},
Title = {Preparing for politics: Judith Butler's ethical dispositions},
Journal = {CONTEMPORARY POLITICAL THEORY},
Year = {2010},
Volume = {9},
Number = {3},
Pages = {284-303},
Month = {AUG},
Abstract = {The question of Judith Butler's `politics' and their normative
   justification has been raised by critics and supporters alike for some
   time. The number of recent texts dedicated to this topic suggests that
   it remains an unresolved and still pressing question. I argue that in
   order to identify and evaluate the political implications of Butler's
   work, we must first recognize the relationship and distinction between
   four vectors of her thinking: her diagnosis of the human condition, her
   expression of specific normative aspirations, her defense of a distinct
   ethical comportment and finally her theory of political engagement. 1
   conclude that Butler implicitly counsels the cultivation of certain
   ethical dispositions, including generosity, humility, patience and
   restraint, as part of a practice of preparing to engage in a kind of
   politics that breaks radically with the mastery- and sovereignty-seeking
   variety all too familiar to us in contemporary times. Contemporary
   Political Theory (2010) 9, 284-303. doi:10.1057/cpt.2009.14},
