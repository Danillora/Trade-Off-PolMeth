@article{ WOS:000215599900008,
Author = {Gomez Navas, Diana and Serna Dimas, Adrian},
Title = {Speeches, subjects and pedagogies about human rights},
Journal = {CIUDAD PAZ-ANDO},
Year = {2010},
Volume = {3},
Number = {2},
Pages = {93-99},
Abstract = {The investigation Human rights and pedagogic subject was oriented to
   establish epistemological, theoretical, methodological and pedagogic
   budgets located in formation projects in human rights in different
   institutions of the Bogota. The investigation proposed an analysis of
   the textual constructions that support these formative projects. The
   analyses showed that the formation projects spread to obviate the
   complexities of subject and pedagogy, while they stand out more the
   instrumental planes of the qualification. Subject and pedagogy are
   fundamental so that the formation redounds in a culture of the human
   rights in the city.},
