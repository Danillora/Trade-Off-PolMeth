@article{ WOS:000217860000004,
Author = {Herrmann, Irene},
Title = {From polemical topics to magnetic concepts: humanitarianism and
   anti-Semitism in Switzerland},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2010},
Volume = {15},
Number = {1},
Pages = {51-68},
Abstract = {Humanitarianism and anti-Semitism are explored here, as representing two
   special kinds of `Kampfbegriffe' that I propose to call magnetic
   concepts, since they are not only difficult to retort in the political
   field but also exert strong forces on society as a whole. The first
   concept constitutes an example of `positive magnetic concepts', as it is
   irresistibly attractive whereas the second one may be considered a
   negative magnetic concept, as it is unmistakably repulsive. These
   concepts are opposites and therefore tend to function differently. Their
   main differences stem from their moral posture, the positive ones being
   positioned within the field of what could/should be done whereas the
   negative ones are positioned outside it. However, both of them draw
   their force from the values they carry. Though these values may seem
   universal and timeless, they vary according to time, history and
   ideology. Therefore, if humanitarianism and anti-Semitism nowadays act
   as magnetic concepts, it is merely because they coincide with current
   Western values.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
