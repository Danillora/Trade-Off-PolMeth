@article{ WOS:000210513400007,
Author = {Abboud, Samer},
Title = {Image, Rhetoric and Reality: The Obama Administration and the Middle
   East},
Journal = {NEW POLITICAL SCIENCE},
Year = {2010},
Volume = {32},
Number = {2},
Pages = {278-284},
Abstract = {As president, Barack Obama has engaged in a number of high profile
   public diplomacy efforts that have suggested his administration's desire
   to radically change US foreign policy in the Middle East. In contrast to
   this dominant perception, this paper argues that the Obama
   administration does not represent the radical change in US foreign
   policy that many had expected. Rather, this administration is pursuing a
   dual policy of continuity and correction. Continuity is evident in the
   administration's extension of Bushera policies, while correction
   manifests in the attempt to reestablish a relationship between the US
   and Middle East that is based on three pillars: the security, stability
   and military superiority of Israel; the protection of Arab authoritarian
   regimes that acquiesce to Israeli hegemony; and the suppression of
   actors who challenge this order. The key difference between this
   administration and its predecessor lies in a public diplomacy campaign
   focused on cultural sensitivity.},
