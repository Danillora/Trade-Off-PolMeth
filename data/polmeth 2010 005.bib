@article{ WOS:000209068100006,
Author = {Poerzgen, Gemma},
Title = {Image Cultivation for Authoritarian Regimes PR Agencies, Journalism, and
   the Belarus Case},
Journal = {OSTEUROPA},
Year = {2010},
Volume = {60},
Number = {12},
Pages = {61+},
Month = {DEC},
Abstract = {Autocratic regimes want to gain international sympathy. Western
   consultants help to develop the ``country image.{''} One instrument to
   this end is the paid press trip. In times of sinking editorial travel
   budges, they are gaining in importance. In November 2010, a Berlin PR
   agency acting on behalf of the Belarusian regime tried to demonstrate
   for German journalists the liberalisation of the country. The brutal
   actions of the Lukashenka regime after the election on 19 December 2010
   demonstrate the absurdity of such efforts to create a better image.},
