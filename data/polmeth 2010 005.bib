@article{ WOS:000420539900003,
Author = {Grotz, Florian and Poier, Klaus},
Title = {Between joint project, institutional bargaining and symbolic politics:
   initiatives for federalism reform in Germany, Austria and Switzerland},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2010},
Volume = {4},
Number = {2},
Pages = {233-259},
Month = {OCT},
Abstract = {Over the past years, Germany, Austria and Switzerland almost
   simultaneously embarked on a fundamental change of their federal
   constitution. These reform attempts not only varied in their final
   results but already showed considerable differences from the very
   beginning. This paper argues that an ideal-type heuristic-federalism
   reform as `joint project', `institutional bargaining' or `symbolic
   politics'-proves helpful in capturing the main procedural and
   policy-related characteristics of the three reform initiatives in a
   systematic manner. The perceptible differences result from the specific
   interplay between functionally relevant contextual changes, interest
   constellations of political decision-makers and institutional
   preconditions.},
Publisher = {SPRINGER VIEWEG-SPRINGER FACHMEDIEN WIESBADEN GMBH},
