@article{ WOS:000421671900002,
Author = {Ribeiro, Ludmila and Patricio, Luciane},
Title = {Local governments and citizen security in Brazil: the role of technical
   consultants in the development of municipal plans for violence
   prevention},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
Year = {2010},
Number = {9},
Pages = {21-35},
Month = {JUN},
Abstract = {The decentralization of public policies in Brazil, which occurred during
   the 1980s, has not taken directly to public safety. However, in recent
   years, because of rising crime and insecurity, the municipalities
   started to be pressured to assume an important role towards this regard.
   Nevertheless, due to a lack of specialized staff for the administration
   of this issue at the local level, several municipalities have resorted
   to consulting offered by research institutions and non-governmental
   organizations to carry out such activities. This aid has made several
   consultancies to conform to public-private partnerships, whose success
   is related to the ability of these entities to define the
   responsibilities of each in implementing an activity of this nature.},
