@article{ WOS:000284041600007,
Author = {Kim, Sangjoon and Suh, Seung-Won},
Title = {Expanding Focus on Japan and the Bounded Dynamism of Japanese
   International Relations Studies in South Korea},
Journal = {JAPANESE JOURNAL OF POLITICAL SCIENCE},
Year = {2010},
Volume = {11},
Number = {3},
Pages = {367-387},
Month = {DEC},
Abstract = {Unlike in Western academia, Japanese studies in Korea have not receded,
   but rather have rather vigorously refocused on Japan during the last two
   decades. This paper examines the rapid development and governing
   dynamics of the Japanese politics studies (JPS), with a particular focus
   on Japanese international relations studies (JIRS). It answers the
   following four questions. How does JIRS perceive, measure, and interpret
   Japan? What are the outstanding features of JIRS? What are the internal
   dynamics in the JPS community? How does the JPS community interact with
   the real political world? The study results emphasize the
   policy-oriented research style, prevailing realistic nationalism, and
   vigorous interaction between academia and politics in JPS/JIRS behind
   the refocusing on Japan.},
