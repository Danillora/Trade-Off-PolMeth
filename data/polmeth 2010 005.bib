@article{ WOS:000281309700004,
Author = {Martin, James},
Title = {A radical freedom? Gianni Vattimo's `emancipatory nihilism'},
Journal = {CONTEMPORARY POLITICAL THEORY},
Year = {2010},
Volume = {9},
Number = {3},
Pages = {325-344},
Month = {AUG},
Abstract = {What scope is there for emancipatory politics in light of the
   post-modern critique of philosophical foundations? This paper examines
   the response to this question by Italian philosopher, Gianni Vattimo,
   who for over two decades has defended the emancipatory prospects of what
   he terms `nihilism'. Vattimo conceives the retreat of metaphysics as a
   progressive weakening of ontological claims and an opening towards new
   and diverse modes of being. In his view, far from an exclusively tragic
   experience of loss or meaninglessness, nihilism is a steadily expanding
   narrative that invites us to face up to our own radical freedom. The
   paper sets out Vattimo's central arguments and contrasts the ethical
   thrust of his `emancipatory nihilism' with the distinctly political take
   on emancipation presented by Ernesto Laclau. While the two converge on
   the withdrawal of metaphysics as the premise for a radical and
   democratic freedom, Vattimo's vision remains insufficiently focused on
   the dynamics of contestation that a generalized nihilism implies.
   Contemporary Political Theory (2010) 9, 325-344. doi:10.1057/cpt.2009.7},
