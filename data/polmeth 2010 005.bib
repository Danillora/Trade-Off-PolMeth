@article{ WOS:000282177800003,
Author = {Basurto, Xavier and Kingsley, Gordon and McQueen, Kelly and Smith,
   Mshadoni and Weible, Christopher M.},
Title = {A Systematic Approach to Institutional Analysis: Applying Crawford and
   Ostrom's Grammar},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2010},
Volume = {63},
Number = {3},
Pages = {523-537},
Month = {SEP},
Abstract = {In 1995, Crawford and Ostrom proposed a grammatical syntax for examining
   institutional statements (i.e., rules, norms, and strategies) as part of
   the institutional analysis and development framework. This article
   constitutes the first attempt at applying the grammatical syntax to code
   institutional statements using two pieces of U.S. legislation. The
   authors illustrate how the grammatical syntax can serve as a basis for
   collecting, presenting, and analyzing data in a way that is reliable and
   conveys valid and substantive meaning for the researcher. The article
   concludes by describing some implementation challenges and ideas for
