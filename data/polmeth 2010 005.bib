@article{ WOS:000284830800004,
Author = {Mulholland, Marc},
Title = {`Modernising Conservatism': The Northern Ireland Young Unionist Movement
   in the 1960s},
Journal = {IRISH POLITICAL STUDIES},
Year = {2010},
Volume = {25},
Number = {1},
Pages = {67-94},
Abstract = {The origins of neo-liberalism and neo-conservatism may be found in the
   1960s, when radical conservatives orientated towards the young and
   idealised the middle class as the primary agent of modernity. In
   Northern Ireland, a struggle for the soul of the Young Unionist Movement
   pitted a liberal anti-sectarianism against a neo-conservative belief in
   unionist destiny. The triumph of the latter contributed to the collapse
   of Ulster Unionist moral authority to govern the nationalist
   constituency of Northern Ireland.},
