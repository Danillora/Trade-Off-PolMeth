@article{ WOS:000212877600002,
Author = {Mastnak, Tomaz},
Title = {Schmitt's Behemoth},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2010},
Volume = {13},
Number = {2-3},
Pages = {275-296},
Abstract = {The article deals with what Schmitt wrote about Behemoth. Most of what
   Schmitt wrote about Behemoth and its mythical counterpart Leviathan was
   said in the context of his critique of Hobbes, which was focused on the
   symbolic nature of politics and touched upon the questions of political
   mythology, the state and its representations, and rights of the
   individual. A detailed analysis of how Schmitt articulated his arguments
   and used his sources, and of how his conceptualization corresponded with
   political reality, leads to a sharp critique of both Schmitt's
   scholarship and politics.},
