@article{ WOS:000283992400003,
Author = {Vliegenthart, Rens and Boomgaarden, Hajo G. and Van Aelst, Peter and de
   Vreese, Claes H.},
Title = {Covering the US presidential election in Western Europe: A
   cross-national comparison},
Journal = {ACTA POLITICA},
Year = {2010},
Volume = {45},
Number = {4},
Pages = {444-467},
Month = {DEC},
Abstract = {Only few studies have investigated media coverage of one and the same
   election campaign in a cross-national, comparative perspective. This
   study takes the 2008 US Presidential election as a case and investigates
   the way it is covered in eight European countries. We explain
   differences in visibility, tone and framing by considering country
   characteristics, media features and temporal aspects. Results
   demonstrate that all three aspects determine campaign coverage to some
   extent, yet temporal aspects, and in particular the stage of the
   campaign and polling trends, are of considerable influence. Acta
   Politica (2010) 45, 444-467. doi:10.1057/ap.2010.2},
