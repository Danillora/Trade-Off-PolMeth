@article{ WOS:000283404200005,
Author = {Barthe, Yannick},
Title = {Political Cause and ``Politics of Causation{''}: The Mobilization of
   French Nuclear Test Veterans},
Journal = {POLITIX},
Year = {2010},
Volume = {23},
Number = {91},
Pages = {77+},
Abstract = {In most controversies in occupational and environmental health, to
   support a cause is primarily to establish causal links between facts and
   damages. From the study of the recent mobilization of the French nuclear
   test veterans and the controversy surrounding their health impact, this
   paper analyzes the processes that are central to this ``politics of
   causation{''} and the forms it may take. It also shows that this
   politics of causation has a feedback effect on the definition of
   political cause and identity of the group. Capturing the dynamics of
   mobilization comes when studying the relationship between the often
   sensitive political issue and politics of causation.},
