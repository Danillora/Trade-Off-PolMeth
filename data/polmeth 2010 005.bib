@article{ WOS:000277571700008,
Author = {Beauvallet, Willy and Michon, Sebastien},
Title = {The European Parliament's unfinished institutionalization. National
   heterogeneity, recruitment specialization and autonomization},
Journal = {POLITIX},
Year = {2010},
Volume = {23},
Number = {89},
Pages = {147+},
Abstract = {We study the institutionalization of the European Parliament using a
   quantitative analysis based on biographies of elected members of the
   European Parliament over the period 2004 to 2009. The data underline the
   emerging of political work specialists able to ensure the direction and
   control of the parliamentary space. The relative unattractiveness of the
   European Parliament compare to the national political space favored the
   recruitment of actors less endowed with legitimate resources. For a
   growing number of actors, the European Parliament represents a
   professionalization and the opportunity to acquire political capital.
   These sociopolitical transformations, along with institutional and
   juridical evolutions, favor the constitution of a core of specialists,
   able to acquire the space's specific resources and to direct it.},
