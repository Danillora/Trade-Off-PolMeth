@article{ WOS:000279196900005,
Author = {Kesler, Christel and Bloemraad, Irene},
Title = {Does Immigration Erode Social Capital? The Conditional Effects of
   Immigration-Generated Diversity on Trust, Membership, and Participation
   across 19 Countries, 1981-2000},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2010},
Volume = {43},
Number = {2},
Pages = {319-347},
Month = {JUN},
Abstract = {This article is an attempt to qualify existing evidence that increasing
   diversity is detrimental to a vibrant civil society. We focus
   specifically on immigration-generated diversity, and argue that while it
   may have negative effects on some specific civic and political outcomes
   in some contexts, these effects vary widely across advanced democracies.
   Our argument rests on analysis of a cross-national, cross-sectional
   time-series dataset that brings together individual-level World Values
   Survey data with country-level variables. With these data, we track
   within-country changes over time in trust and engagement. We show that
   immigration can have a negative effect on social trust, organizational
   membership and political engagement, but that institutional arrangements
   shape this relationship in systematic ways. In more economically equal
   societies and in more multicultural countries (where cultural minorities
   are recognized and accommodated), the negative effects of immigration on
   trust and engagement are mitigated or even reversed. We conclude that
   there is no general link between immigration-generated diversity and
   collective-mindedness. Rather, the direction and strength of the
   relationship depend on institutional and policy contexts.},
