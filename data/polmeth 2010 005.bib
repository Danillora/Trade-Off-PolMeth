@article{ WOS:000276229700005,
Author = {Steel, Gill},
Title = {Images of Government, Business, and Citizen Identity in the United
   States},
Journal = {JAPANESE JOURNAL OF POLITICAL SCIENCE},
Year = {2010},
Volume = {11},
Number = {1},
Pages = {99-123},
Month = {APR},
Abstract = {This paper presents a country profile of the United States using data
   from the AsiaBarometer (2008) survey. I first examine how citizens see
   themselves, their government and big business. My findings show that
   Americans remain ambivalent toward politics, their government, and big
   business. Citizens overwhelmingly support democracy as a political
   system and are satisfied with a broad range of specific democratic
   rights, but, at the same time, they complain about the workings of their
   democratic system, policy output, and many distrust government and big
   business. I then examine the role citizens play in politics, analyzing
   who participates and why.},
