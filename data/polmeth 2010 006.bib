@article{ WOS:000280497400006,
Author = {Arrington, Theodore S.},
Title = {Redistricting in the US: A Review of Scholarship and Plan for Future
   Research},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
Year = {2010},
Volume = {8},
Number = {2},
Abstract = {The nation-wide cycle of redistricting is the most consequential,
   repetitive decision-making process shaping the nature of American
   democracy. Because the arrangement of the districts forms an essential
   part of the ``rules of the game,{''} the process by which they are drawn
   is meta-politics or a quasi-constitution making activity. The research
   on this process is voluminous and includes bits and pieces of the
   process. But the process is complicated, in part because the criteria
   for redistricting are in conflict with one another. The criteria include
   considerations of political geography, physical features, community of
   interest, one person one vote, race and ethnicity, partisanship,
   contiguity, compactness, recognition of local political boundaries,
   incumbents' addresses, and desires for minimal change. The literature
   also covers the use of geographic information systems in the process and
   possible reforms. But no overall theory of redistricting has been
   developed which includes all these features.},
