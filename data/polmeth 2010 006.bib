@article{ WOS:000212967800002,
Author = {Lockhart, Charles and Giles-Sims, Jean and Camp, Bayliss},
Title = {States' Senior Residential Property Tax Abatements: Uncontroversial
   Benefit or Looming but Unrecognized Problem?},
Journal = {POLITICS \& POLICY},
Year = {2010},
Volume = {38},
Number = {4},
Pages = {677-704},
Month = {AUG},
Abstract = {All the U. S. states create residential property tax abatement policies
   that provide important benefits for a rapidly growing older population.
   These policies vary sharply, and we seek to explain variation in their
   generosity. We employ regression in conjunction with a cross- sectional
   data set of the 50 states focused early in the current decade. Several
   variables prominent in explaining cross- state variation in other public
   policies exert only modest effects on these programs. Rather, the
   primary influence is a positive one for a cultural orientation toward
   government helping a broad swath of the citizenry. We attribute this
   unusual pattern of explanatory factors to (1) abatement programs' older
   target populations; (2) their tax expenditure status; and (3) their
   tendency to be categorized as economic development rather than social
   programs. This explanatory pattern holds theoretical importance inasmuch
   as it provides few constraints on the growth of at least some programs
   serving older citizens.},
