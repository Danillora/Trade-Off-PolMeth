@article{ WOS:000278320300002,
Author = {King, Anthony and Allen, Nicholas},
Title = {`Off With Their Heads': British Prime Ministers and the Power to Dismiss},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2010},
Volume = {40},
Number = {2},
Pages = {249-278},
Month = {APR},
Abstract = {The British prime minister's power to appoint and dismiss ministers is
   probably his most important single power. This article explores how
   prime ministers from Macmillan to Blair have used that power. The
   article considers the criteria that prime ministers use when choosing to
   appoint or dismiss individuals from office before examining the
   calculations and miscalculations that prime ministers have made in
   practice. Finally, the article analyses the way that prime ministers
   have exercised, in particular, their power to dismiss and finds that
   Thatcher was far more likely than others to sack cabinet colleagues on
   ideological or policy grounds. The article emphasizes that prime
   ministers' relationships with especially powerful ministers - `big
   beasts of the jungle' - are crucial to an understanding of British
   government at the top.},
Publisher = {CAMBRIDGE UNIV PRESS},
