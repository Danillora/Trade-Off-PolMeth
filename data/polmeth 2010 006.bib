@article{ WOS:000422106700003,
Author = {Brown, Kimberly Linkous},
Title = {Highliners and Moneymakers: Understanding Accommodation and Resistance
   in the Sto: lo Commercial Fishery},
Journal = {NEW PROPOSALS-JOURNAL OF MARXISM AND INTERDISCIPLINARY INQUIRY},
