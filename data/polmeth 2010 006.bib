@article{ WOS:000287867200006,
Author = {Mayer, Kenneth R. l},
Title = {Is Political Science Relevant? Ask an Expert Witness},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
Year = {2010},
Volume = {8},
Number = {3},
Month = {OCT},
Abstract = {Working as an expert witness is one of the most direct ways for a
   political scientist to affect actual policy. However, the worlds of
   academia and litigation are vastly different environments, with
   different norms, goals, and rules. I explore some of these differences,
   as well as the implications for straddling the two worlds as an expert.
   Ultimately, the parties in election law, redistricting, and voting
   rights litigation have found political scientists to be valuable
   partners. It is a very satisfying experience to find that our methods
   and skills have value outside the friendly confines of the ivory tower.},
