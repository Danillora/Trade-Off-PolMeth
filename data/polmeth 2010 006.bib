@article{ WOS:000421219200005,
Author = {Ruiz-Rico Ruiz, Gerardo and Anguita Susi, Alberto},
Title = {The statutory and legal reform of the parliamentary autonomous
   commissioners},
Journal = {TEORIA Y REALIDAD CONSTITUCIONAL},
Year = {2010},
Volume = {26},
Pages = {167-190},
Abstract = {Compared to the initial trend to mimetically recreate the basic scheme
   of functions of the State's Ombudsman, the new Statutes of autonomy have
   made efforts, that lead to different results by categorizing the
   autonomical Ombudsman as institutions of statuary nature and independent
   instruments of guardianship of rights. However, these Statutes of
   autonomy have chosen a minimal regulation which, inevitably, left in the
   hands of the regional legislator, a wide margin of discretion to
   regulate the autonomical Ombudsman. Precisely, this situation should
   serve to raise a legal reform of the Andalusian Ombudsman to achieve a
   more effective, objective and nearby protection related to the problems
   affecting citizens.},
