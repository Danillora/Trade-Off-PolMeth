@article{ WOS:000284842600009,
Author = {Grant, J. Tobin and Mockabee, Stephen T. and Monson, J. Quin},
Title = {Campaign Effects on the Accessibility of Party Identification},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2010},
Volume = {63},
Number = {4},
Pages = {811-821},
Month = {DEC},
Abstract = {This study uses response latency, the time required for a survey
   respondent to formulate an answer upon hearing a question, to examine
   the accessibility of partisan self-identifications over the course of a
   political campaign season. Although the aggregate distribution of
   partisanship remains fairly stable during the campaign, party
   identifications become more accessible to individuals with weaker party
   identifications as the election approaches. Consistent with theoretical
   expectations, the authors find that partisan orientations are more
   useful in forming political judgments when those orientations are more
   accessible to the voter. The effect of partisanship on vote choice is a
   third greater for voters with highly accessible party identifications
   than for those with less accessible party identifications.},
