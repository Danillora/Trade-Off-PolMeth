@article{ WOS:000446452900003,
Author = {Livermore, Celia Romm},
Title = {When the Virtual and the Real Clash Power and Politics in a Social
   Networking Community},
Journal = {INTERNATIONAL JOURNAL OF E-POLITICS},
Year = {2010},
Volume = {1},
Number = {4},
Pages = {42-54},
Month = {OCT-DEC},
Abstract = {This paper focuses on two related aspects-the politics of social
   networking communities and the impact that clashes between real and
   virtual spheres on relationships in social networking communities. The
   starting point for this paper is a series of events that took place in a
   social networking community involving a dominant member of the community
   attempting to overpower others, leading to several of the most active
   members, including the dominant member himself, leaving the community.
   Following a review of the literature on social networking and politics,
   the Virtuality/Reality Conflict (VRC) model is presented. The model is
   used in this paper as a basis for explaining the events in the case and
   for discussing the implications from this study to research politics of
   the virtual communities.},
