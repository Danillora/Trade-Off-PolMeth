@incollection{ WOS:000279880000010,
Author = {Guatam, Upendra and Khaniya, Ganesh},
Editor = {Devkota, SR},
Title = {FARMER MANAGED IRRIGATION SYSTEMS IN NEPAL: PEOPLE-CENTERED GOVERNANCE
   ALTERNATIVE},
Booktitle = {NEPAL IN THE 21ST CENTURY},
Series = {Global Politcal Studies},
Year = {2010},
Pages = {225-239},
Abstract = {Farmer managed irrigation units are an indigenous system. A number of
   these systems have been in operation since the beginning of the
   millennium. They manage irrigation water in more than two third of area
   in the country; significantly contribute to the country's food security;
   and act as a mainstay of farmers' livelihood. Member farmers of the
   farmer managed irrigation systems (FMIS) organization themselves
   construct, maintain, manage and rehabilitate their irrigation system.
   They value water as a community property. As such, equitable access to
   and contribution in system management by each member farmer provide
   sustenance to the FMIS. Equity in irrigation resource generation and
   distribution bolsters the FMIS's self-governance capability that is
   reflected in their fundamental organizational features of
   non-partisanship and autonomy in decision making. For their role in the
   country's political economy in general and irrigated agricultural
   production in particular, FMIS are national socio-economic heritage of
   Nepal. In the first decade of the 21st century when political society in
   Nepal has started talking about newly ``restructured{''} and
   ``reconstructed{''} Nepal, what it is essentially rediscovering are the
   values and fundamentals of the nation's own FMIS.},
Publisher = {NOVA SCIENCE PUBLISHERS, INC},
Address = {400 OSER AVE, STE 1600, HAUPPAUGE, NY 11788-3635 USA},
Type = {Article; Book Chapter},
Language = {English},
ISBN = {978-1-60741-348-6},
Research-Areas = {Government \& Law},
