@article{ WOS:000276673700008,
Author = {Schoen, Harald},
Title = {Good Sides, Bad Sides. An Analysis of the Effects of Attitudinal
   Ambivalence on Political Judgment in Germany},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2010},
Volume = {39},
Number = {1},
Pages = {105+},
Abstract = {This paper addresses the effects of attitudinal ambivalence on party and
   leader evaluations in Germany. Building on models of political
   information processing, it proposes hypotheses on the role of
   attitudinal ambivalence in affecting the extremity and stability of
   party and politician appraisal. Relying on panel data gathered in the
   1987 German federal election, the analysis shows that ambivalence toward
   a political party or candidate moderates and destabilizes evaluations of
   that actor. Moreover, ambivalence toward a party exhibits comparable
   effects on its candidate for chancellor and vice versa. At the same
   time, ambivalence toward a party or a politician ceases to be
   influential when it comes to evaluations of actors of another political
   party. In line with the memory-based model, ambivalence, thus, plays a
   role in conditioning political evaluations, though its effects appears
   to be limited to assessments of political actors belonging to the same
   political party.},
Publisher = {FACULTAS VERLAGS \& BUCHHANDELS AG},
