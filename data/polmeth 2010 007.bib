@article{ WOS:000283604600017,
Author = {Hutchings, Vincent L. and Walton, Jr., Hanes and Benjamin, Andrea},
Title = {The Impact of Explicit Racial Cues on Gender Differences in Support for
   Confederate Symbols and Partisanship},
Journal = {JOURNAL OF POLITICS},
Year = {2010},
Volume = {72},
Number = {4},
Pages = {1175-1188},
Month = {OCT},
Abstract = {Researchers have argued that explicit racial appeals are rejected in
   contemporary American politics because they are perceived as violating
   the norm of racial equality. We test this claim with an experimental
   design, embedded in a representative survey of Georgia where, until
   recently, the state flag featured the Confederate battle emblem. In our
   experiment, we manipulate the salience of racial cues in news accounts
   of the state flag controversy in Georgia. We hypothesize that women are
   more likely than men to reject explicit racial appeals. We focus on the
   effects of explicit messages in two areas: support for Confederate
   symbols and identification with the Democratic Party. As hypothesized,
   when the racial significance of this debate is made explicit support for
   the Confederate flag declines, but only among women. Similarly, explicit
   appeals lead to lower levels of Democratic identification among men, but
   among women the effects are weaker and less consistent.},
