   Smith, William, Chinese Univ Hong Kong, Shatin, Hong Kong, Peoples R China.},
DOI = {10.1177/1474885109355890},
ISSN = {1474-8851},
EISSN = {1741-2730},
Keywords = {civil disobedience; Hannah Arendt; republicanism; revolutionary spirit},
