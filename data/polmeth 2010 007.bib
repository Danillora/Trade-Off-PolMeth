@article{ WOS:000435231400003,
Author = {Nelson, Travis},
Title = {Why Justify? Explaining Variation in Conflict Justification, 1980-2008},
Journal = {DEMOCRACY \& SECURITY},
Year = {2010},
Volume = {6},
Number = {2},
Pages = {128-146},
Abstract = {This article is about state justifications for conflict initiation, an
   understudied yet nonetheless significant element of the foreign policy
   behavior of sovereign states. To what degree do such justifications vary
   among states? And what drives any variation that does exist? Regime
   type? Power status? Or, as implicitly assumed by some constructivist
   scholars, normative identification? Through an analysis of conflict
   justifications between 1980 and 2008, I find not only that states almost
   always publicly justify their actions and that conflict justifications
   always include at the very least an assertion of self-defense, but that
   there is also substantial variation regarding whether states make claims
   of humanitarianism and multilateralism and whether they utilize
   international law in their justificatory statements. The former is best
   explained through normative identification and the latter by the power
   status of the justifying state. Differences in regime type, however, do
   not play a significant role in conflict justification variation.},
