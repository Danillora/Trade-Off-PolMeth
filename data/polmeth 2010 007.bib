@article{ WOS:000273229800013,
Author = {Tonge, Jonathan and Mycock, Andrew},
Title = {Citizenship and Political Engagement Among Young People: The Workings
   and Findings of the Youth Citizenship Commission},
Journal = {PARLIAMENTARY AFFAIRS},
Year = {2010},
Volume = {63},
Number = {1},
Pages = {182-200},
Month = {JAN},
Abstract = {In 2008, the Labour government established the Youth Citizenship
   Commission (YCC). The (independent) Commission, staffed by individuals
   of varying backgrounds and serviced by the Ministry of Justice, was
   asked to examine how young people define citizenship and to explore how
   that citizenship might better be connected to political activity.
   Additionally, the YCC was required to lead a consultation on whether the
   voting age should be lowered. From the perspective of two participants,
   this article examines the key debates which exercised the YCC and
   discusses some of its most important recommendations, but also looks at
   its workings. Could the Commission (or other independent Commissions)
   operate differently in terms of evidence-gathering, deliberations,
   recommendations or implementation of proposals?.},
