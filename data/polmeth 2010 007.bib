@article{ WOS:000273299800004,
Author = {Tarrow, Sidney},
Title = {The Strategy of Paired Comparison: Toward a Theory of Practice},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2010},
Volume = {43},
Number = {2},
Pages = {230-259},
Month = {FEB},
Abstract = {Paired comparison is a strategy of political analysis that has been
   widely used but seldom theorized. This is because it is often
   assimilated to single-case studies or regarded as a degenerate form of
   multicase analysis. This article argues that paired comparison is a
   distinct strategy of comparative analysis with advantages that both
   single-case and multicase comparisons lack. After reviewing how paired
   comparison has been dealt with in comparative politics, the article
   details a number of its advantages and pitfalls, illustrates them
   through the work of four major pairing comparativists, and proposes what
   is distinct about the strategy. It closes with a number of suggestions
   for using paired comparison more effectively.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Tarrow, S (Corresponding Author), Cornell Univ, Dept Govt, Ithaca, NY 14851 USA.
   Cornell Univ, Dept Govt, Ithaca, NY 14851 USA.},
DOI = {10.1177/0010414009350044},
