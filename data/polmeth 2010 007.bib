@article{ WOS:000279483300006,
Author = {Burke, Brendan and Ferguson, Margaret},
Title = {Going Alone or Moving Together: Canadian and American Middle Tier
   Strategies on Climate Change},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2010},
Volume = {40},
Number = {3},
Pages = {436-459},
Month = {SUM},
Abstract = {This article builds upon previous research that indicates that
   multilevel governance, especially at subnational levels and in the
   private sector, has the greatest potential to address the problem of
   climate change. Specifically, this article updates the study of climate
   change efforts among the Canadian provinces and the American states
   through case analysis. We identify two longitudinal strategies found in
   both the provinces and the states-a fragmented response to portions of
   the climate change problem, and a comprehensive strategy based on
   stakeholder education and participation. Institutional variations
   between the provinces and the states lead to some differences in the
   process of policy development, but not in the resulting policy tools
   used to address the issue.},
