@article{ WOS:000283876100003,
Author = {Emmanuel, Nikolas},
Title = {Undermining cooperation: donor-patrons and the failure of political
   conditionality},
Journal = {DEMOCRATIZATION},
Year = {2010},
Volume = {17},
Number = {5},
Pages = {856-877},
Abstract = {This study extends the debate concerning the feasibility of using
   foreign assistance to leverage political reforms in developing
   countries. This research combines both quantitative and qualitative
   methodologies in an effort to expand on the work done by previous
   scholars. Conclusions of the empirical analysis point to the finding
   that political conditionality is most effective when close cooperation
   exists between donors. This coordination, however, is frequently
   sabotaged by self-interested donor-patrons, illustrated by the case of
   Cameroon and France. This pattern is generalizable and significantly
   undermines the overall success rate of a political conditionality
   approach.},
