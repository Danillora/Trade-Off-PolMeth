@article{ WOS:000276673700003,
Author = {Larsen, Lars Thorup},
Title = {The Tactical Displacement of Ideas, or why I am Becoming Lakatos
   Intolerant},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2010},
Volume = {39},
Number = {1},
Pages = {27+},
Abstract = {Several ideational approaches in the policy literature draw on models
   imported from the philosophy of science such as Kuhn's model of
   scientific paradigms or Lakatos' concept of scientific research
   programs. A key example of the latter is Sabatier's Advocacy Coalition
   Framework, which builds its understanding of policy-relevant ideas on an
   analogy with Lakatos' model of deep core postulates and protective
   layers. This article gives a critical assessment of the Lakatos analogy
   in Sabatier's and similar ideational approaches. Theorizing ideas
   according to this model may incur problems on the practice of conducting
   policy analysis and the ambition to achieve a dynamic understanding of
   policy change and ideas. While the initial motivation for bringing ideas
   back into policy analysis was the dissatisfaction with the
   ``black-boxing{''} in institutionalist and interest-based explanations,
   the Lakatos analogy brings ideas back into the black box. As a result of
   the positioning of policy ideas as fundamental deep core values, it is
   very difficult to see how they can facilitate change much less function
   as the source of ambivalence often characterized as the political aspect
   of ideas as such. After a critical discussion of the analytical
   motivations behind the Lakatos analogy, the article concludes with a
   Foucauldian emphasis on tactical displacements as the key to unleash the
   analytical strength of ideas in public policy.},
Publisher = {FACULTAS VERLAGS \& BUCHHANDELS AG},
Address = {BERGGASSE 5, VIENNA, A-1090, AUSTRIA},
