@article{ WOS:000278761800006,
Author = {Howorth, Jolyon},
Title = {Sarkozy and the `american mirage' or why gaullist continuity will
   overshadow transcendence},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2010},
Volume = {9},
Number = {2},
Pages = {199-212},
Month = {JUN},
Abstract = {This article argues that the continuity in French approaches to the
   transatlantic relationship and to the North Atlantic Alliance is
   structured by deep historical forces and is relatively unaffected by the
   personal preferences of individual Presidents or other political actors.
   Nicolas Sarkozy's much discussed `return to NATO' will not affect French
   foreign policy in any significant way. Indeed, having France inside the
