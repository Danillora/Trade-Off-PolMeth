@article{ WOS:000445511500003,
Author = {Kaniok, Petr and Smekal, Hubert},
Title = {Czech EU Council Presidency: political standards, medial catastrophe},
Journal = {POLITOLOGICKY CASOPIS-CZECH JOURNAL OF POLITICAL SCIENCE},
Year = {2010},
Number = {1},
Pages = {39-58},
Abstract = {This article analyses the EU Council Presidency carried out by the Czech
   Republic in the first six months of 2009. This presidency was
   accompanied by deep contempt, even before it began. After reviewing the
   theory of presidency and possible evaluating criteria, it analyzes the
   course of the Czech presidency and compares it with the low
   expectations. In addition to the political process and context of the
   presidency, media perception is also taken into account. The article
   argues that the Czech term fulfilled the basic tasks and functions
   expected from an EU Council presidency. However, weak media presentation
   of the Czech presidency and several controversial events led to a
   perception of the presidency as a catastrophe and confirmed previously
   expressed concerns.},
