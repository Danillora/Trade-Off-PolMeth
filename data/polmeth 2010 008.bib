@article{ WOS:000274906300010,
Author = {Martin, Shane},
Title = {Electoral Rewards for Personal Vote Cultivation under PR-STV},
Journal = {WEST EUROPEAN POLITICS},
Year = {2010},
Volume = {33},
Number = {2},
Pages = {369-380},
Abstract = {In the past, economic integration in Europe was largely compatible with
   the preservation of different national varieties of capitalism. While
   product market integration intensified competition, member states could
   build on and foster their respective comparative advantage. Today, this
   no longer unequivocally holds true. This article contends that a new,
   `post-Ricardian' phase of European integration has emerged in which the
   Commission's and the European Court of Justice's (ECJ's) attempts to
   promote economic integration systematically challenge the institutions
   of organised capitalism. It demonstrates this by discussing recent
   disputes over the Services Directive, the Takeover Directive, and
   company law. In the current phase of European integration, the
   Commission's and the ECJ's liberalisation attempts either transform the
   institutional foundations on which some of the member states' economic
   systems rely or they create political resistance to an extent that
   challenges the viability of the European project.},
