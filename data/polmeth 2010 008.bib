@article{ WOS:000210513400008,
Author = {Lightcap, Tracy},
Title = {To Be or Not to Be: Torture, American Politics, and America's Future},
Journal = {NEW POLITICAL SCIENCE},
Year = {2010},
Volume = {32},
Number = {2},
Pages = {284-290},
Abstract = {The sequence of events leading to the use of torture in interrogations
   by US intelligence and military interrogators during the ``War on
   Terror{''} is now well known. What is less evident is why that sequence
   occurred. This paper presents a preliminary explanation for the decision
   to officially sanction torture. It postulates that the 9/11 attacks
   presented the Bush administration with an opportunity to overcome both
   domestic and foreign political obstacles. It responded by creating an
   ideological narrative that provided cover for its domestic policy agenda
   and justified a unilateral attack on terrorist organizations and states
   aiding them. When that narrative began to come undone in Iraq, the
   administration, rather than see its political arrangements threatened,
   created informal institutions that created a torture regime used to
   interrogate terrorist suspects throughout all theaters of the War on
   Terror. The article concludes with some thoughts on what changes have
   been wrought by the Obama administration and what needs to be done to
   eliminate the official sanctioning of torture in the future.},
