@article{ WOS:000208579500002,
Author = {Choi, Eunjung and Woo, Jongseok},
Title = {Political corruption, economic performance, and electoral outcomes: a
   cross-national analysis},
Journal = {CONTEMPORARY POLITICS},
Year = {2010},
Volume = {16},
Number = {3},
Pages = {249-262},
Abstract = {The current study examines how the incumbent government's economic
   performance plays a role in mediating the impact of political corruption
   on electoral outcomes in 115 developing countries with relatively higher
   levels of corruption than Western consolidated democracies. Borrowing
   theoretical insights from the information-processing theory of voting,
   this study finds that political corruption becomes a formative electoral
   factor when the regime fails to sustain a sufficient level of economic
   growth. Otherwise, political corruption is not a significant factor that
   shapes electoral outcomes, irrespective of the level of perceived
   corruption, because the economy occupies voters' minds as the most
   important issue, making it a more accessible issue than political
   corruption.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
