@article{ WOS:000212629100006,
Author = {Kernalegenn, Tudi},
Title = {Regions as Spaces for Social Movements: The Role of Trade Unions in the
   Construction of Territory},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2010},
Volume = {20},
Number = {3, SI},
Pages = {371-387},
Abstract = {This article examines the role of trade unions in the co-production of
   regions as spaces for politics. Focusing on the role of the
   Confederation Francaise Democratique du Travail (CFDT) in the social
   construction of Brittany in the 1970s, it demonstrates that a region is
   not a given entity, but is a collective construction whose co-producers
   include trade unions. Through deploying a regional discourse to give a
   territorial dimension to class-based analysis of social issues, trade
   union contributions have been threefold: setting up of regional networks
   and discussion fora, diffusion of banal regionalism, mobilization of
   images and usages of the region as a cognitive tool.},
