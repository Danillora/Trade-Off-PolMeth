@article{ WOS:000285485900003,
Author = {Eduardo Herrera, Hugo},
Title = {Carl Schmitt s concept of the political in relation to a
   political philosophy rooted in Aristotle},
Journal = {REVISTA DE ESTUDIOS POLITICOS},
Year = {2010},
Number = {150},
Pages = {87-110},
Month = {OCT-DEC},
Abstract = {The aim of this paper is to attempt to determine Carl Schmitt's position
   regarding the tradition of practical philosophy, which is that which
   develops from the Aristotelian differentiation between two types of
   knowledge one that is purely theoretical and another which alms to study
   and orient human action, politics included The thesis I wish to prove is
   that Schmitt, in his The Concept of the Political, has a position that
   is quite close to that tradition, notwithstanding the important
   differences that arise In order to prove this thesis, I will refer to a
   series of criticisms to this Schmitt text, which would appear to
   separate the author from that tradition},
