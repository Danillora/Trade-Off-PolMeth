@article{ WOS:000218360400004,
Author = {Blackham, Jeremy and Prins, Gwyn},
Title = {WHY THINGS DON'T HAPPEN SILENT PRINCIPLES OF NATIONAL SECURITY},
Journal = {RUSI JOURNAL},
Year = {2010},
Volume = {155},
Number = {4},
Pages = {14-22},
Abstract = {The defence review is occurring at a time of extreme financial pressure
   at home and considerable military risk in Afghanistan. Yet it would be a
   grave failure if it attended principally - or worse, exclusively - to
   the clamour of those issues. The deepest principles of national security
   are silent. They explain why bad things don't happen and they must be
   given voice. This article also argues that geopolitics prescribe a
   primarily maritime framework for any British SDSR, and that the core
   strategic challenges are naval. The Royal Navy, however, has become
   dangerously weak. Urgent steps must be taken to reverse this trend
   before it is too late.},
