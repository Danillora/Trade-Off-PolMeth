@article{ WOS:000280454100010,
Author = {Schaeffer, Denise},
Title = {Realism, Rhetoric and the Possibility of Reform in Rousseau's
   Considerations on the Government of Poland},
Journal = {POLITY},
Year = {2010},
Volume = {42},
Number = {3},
Pages = {377-397},
Month = {JUL},
Abstract = {Rousseau's Considerations on the Government of Poland is generally
   deemed to be a practical rather than philosophical work, exemplifying
   Rousseau's willingness to compromise his own moral and political
   principles in the face of concrete realities and limitations. The
   objective of this work, though, is much more radical and transformative
   than is typically recognized. What is on the surface a hard-nosed appeal
   to self-interest is, more subtly an education of self-interest by means
   of a transformed political imagination. In this way Rousseau himself
   performs a true legislator's function in attending to cultivating the
   morals, customs, and beliefs that are the precondition of sound
   political institutions. However, while the Social Contract presents the
   culture-building dimension of the legislator's art as pre-political and
   prior to actual institution building, in Poland Rousseau explores the
   possibility of cultural transformation that is simultaneous with
   institutional reforms. Polity (2010) 42, 377-397.
   doi:10.1057/pol.2010.12; published online 7 June 2010},
