@article{ WOS:000276260600001,
Author = {Niedermayer, Oskar},
Title = {From Two-party Dominance to Pluralism: The Development of the German
   Party System in West European Comparative Perspective},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2010},
Volume = {51},
Number = {1},
Pages = {1-13},
Month = {MAR},
Abstract = {With the federal election of 2009, the German party system no longer
   belongs to the party systems with two-party dominance but to the
   pluralistic systems. The article develops a typology of party systems,
   decribes the change of the type of Germany's party system, shows that
   the development of the German party system is an exception in Western
