@article{ WOS:000443216600008,
Author = {Uzelac, Milan},
Title = {THE ENIGMATIC FATE OF PHILOSOPHICAL SUBSISTENCE ON THE GROUND OF
   TRANSCENDENTAL PHENOMENOLOGY},
