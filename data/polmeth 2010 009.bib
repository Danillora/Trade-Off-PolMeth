@article{ WOS:000283404200002,
Author = {Rainhorn, Judith},
Title = {The Labour Movement against Lead Pain: When Dissenting Voices and Local
   Experiments Contravene the Prevailing Views of the Trade Unions (France,
   Early 20th c.)},
Journal = {POLITIX},
Year = {2010},
Volume = {23},
Number = {91},
Pages = {9+},
Abstract = {The French labour movement and especially the main trade union (CGT)
   suddenly demanded the banning of lead paint, responsible for industrial
   poisoning of house painters, at the beginning of 20th century. This
   article aims to investigate the late and sudden emergence of the issue
   of occupational health in the trade union strategy, making it a
   temporary tool for workers mobilization, as during 1906 painters strike
   in North of France. The white lead issue also emphasizes the role of
   national key figures as Abel Craissac and the importance of local
   collaborative experiments between doctors and workers (Lille). These
   figures and experiments clearly contravene the prevailing union views
   before WW1. They take part in the reform circles, making the fight
   against lead paint the occasion for building a new attitude towards
   health in the workplace.},
