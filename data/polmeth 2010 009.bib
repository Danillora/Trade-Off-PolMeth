@article{ WOS:000278111600010,
Author = {Miller, Melissa K.},
Title = {Membership Has Its Privileges How Voluntary Groups Exacerbate the
   Participatory Bias},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2010},
Volume = {63},
Number = {2},
Pages = {356-372},
Month = {JUN},
Abstract = {It is well established that individuals who belong to voluntary
   associations are more likely to participate in democratic politics. This
   article examines the possibility that the participation-promoting
   aspects of America's nonpolitical voluntary organizations are not
   experienced equally by all members. Not only are privileged Americans
   more likely to join nonpolitical voluntary organizations, but they are
   more likely to benefit from their membership after the point of joining.
   The participation-promoting benefits of membership accrue
   disproportionately to white, male, wealthy, and well-educated members,
   thus reinforcing participatory stratification. Evidence suggests that
   stratification results from both member and leader choices and
   behaviors.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Miller, MK (Corresponding Author), Bowling Green State Univ, Bowling Green, OH 43403 USA.
