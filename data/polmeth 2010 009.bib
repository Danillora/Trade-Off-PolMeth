@article{ WOS:000215199400004,
Author = {Mclntosh, Tom and Ducie, Michael and Burka-Charles, Marcy and Church,
   John and Lavis, John and Pomey, Marie-Pascale and Smith, Neale and
   Tomblin, Stephen},
Title = {Population Health and Health System Reform: Needs-Based Funding for
   Health Services in Five Provinces},
