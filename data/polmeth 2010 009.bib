@article{ WOS:000435231500003,
Author = {Cook, Alethia and Lounsbery, Marie Olson},
Title = {Choose Your Weapon (or Strategy)! An Analysis of the Factors Influencing
   Strategy in Civil Wars},
Journal = {DEMOCRACY \& SECURITY},
Year = {2010},
Volume = {6},
Number = {3},
Pages = {256-277},
Abstract = {Given civil war power asymmetries, rebel groups tend to shy away from
   the more traditional forms of warfare. There are several approaches
   available to groups, and the decision to employ one over the other is
   not arbitrary, nor is it irrelevant. The choice of strategy by a rebel
   group will frequently determine how a government responds and how the
   movement will be received internationally. Further, these decisions are
   reevaluated and potentially change as the conflict progresses. The
   current article puts forth a theoretical framework suggesting that rebel
   group strategy is a function of both static and dynamic conflict
   characteristics.},
