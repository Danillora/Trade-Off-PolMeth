@article{ WOS:000287350200007,
Author = {Bieber, Tonia},
Title = {Europe a la Carte? Swiss Convergence towards European Policy Models in
   Higher Education and Vocational Education and Training},
Journal = {SWISS POLITICAL SCIENCE REVIEW},
Year = {2010},
Volume = {16},
Number = {4, SI},
Pages = {773-800},
Abstract = {In the last decade, European education has experienced far-reaching
   transformation through the international initiatives of the Bologna
   process in higher education (HE) and the Copenhagen process in
   vocational education and training (VET) for enhancing European
   cooperation. This study investigates the mostly underresearched effects
   of these initiatives on Switzerland to discern whether Swiss HE and VET
   policies have converged towards European models, and which mechanisms
   were influential. It combines research on Europeanization and
   convergence and uses process-tracing based on expert interviews and
   document analysis. Results reveal that Swiss HE policy strongly
   converged towards the Bologna model, while the development of a partial
   convergence of VET policy towards the model of the Copenhagen process
   can be observed. The study demonstrates the impact of domestic politics
   on shaping Europeanization effects and reconstructs the processes
   through which the initiatives look effect through transnational
   communication.},
