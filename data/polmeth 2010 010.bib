@article{ WOS:000218363400009,
Author = {Krause, Joachim and Schreer, Benjamin},
Title = {SALVAGING GLOBAL ZERO DIPLOMACY IN THE SECOND NUCLEAR AGE},
Journal = {RUSI JOURNAL},
Year = {2010},
Volume = {155},
Number = {3},
Pages = {42-46},
Abstract = {A year ago Barack Obama proffered a vision of a nuclear-free world. But
   as Iran moves closer to acquiring nuclear weapon technology, the
   existing nuclear order appears to be unravelling. Joachim Krause and
   Benjamin Schreer argue that, despite a convincing vision, both the US
   administration's new bilateral agreement with Russia and its own Nuclear
