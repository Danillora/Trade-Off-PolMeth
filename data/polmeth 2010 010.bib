@article{ WOS:000285467800002,
Author = {Baldwin, Kate and Huber, John D.},
Title = {Economic versus Cultural Differences: Forms of Ethnic Diversity and
   Public Goods Provision},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2010},
Volume = {104},
Number = {4},
Pages = {644-662},
Month = {NOV},
Abstract = {Arguments about how ethnic diversity affects governance typically posit
   that groups differ from each other in substantively important ways and
   that these differences make effective governance more difficult. But
   existing cross-national empirical tests typically use measures of
   ethnolinguistic fractionalization (ELF) that have no information about
   substantive differences between groups. This article examines two
   important ways that groups differ from each other-culturally and
   economically-and assesses how such differences affect public goods
   provision. Across 46 countries, the analysis compares existing measures
   of cultural differences with a new measure that captures economic
   differences between groups: between-group inequality (BGI). We show that
   ELF, cultural fractionalization (CF), and BGI measure different things,
   and that the choice between them has an important impact on our
   understanding of which countries are most ethnically diverse.
   Furthermore, empirical tests reveal that BGI has a large, robust, and
   negative relationship with public goods provision, whereas CF, ELF, and
   overall inequality do not.},
