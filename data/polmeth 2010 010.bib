@article{ WOS:000441108700012,
Author = {Hanna, P.},
Title = {Coming to Middle Ages},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2010},
Number = {1},
Pages = {147+},
Abstract = {In a small article reprinted from McKinsey Quarterly. the US political
   Scientist Parag Hanna. Director. ``Global Governance Initiative{''},
   argues that the middle of the 21st century will resemble nothing so much
   as the Middle Ages of the 5th to the 15th centuries. from the sacking of
   Rome by the Visigoths. in 410, to the fall of Constantinople. in 1453.
   It was a long and uncertain period and thus an ideal metaphor to
   characterize our times. It was multipolar. with the expanding empires on
   Eurasian landmass, and apolar, with no one global leader. The new Middle
   Ages - synonymous with the age of globalization - have already begun.},
