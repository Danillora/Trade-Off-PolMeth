@article{ WOS:000283404200006,
Author = {Pitti, Laure},
Title = {``Self Corporal Experts{''} and Alternative Medical Practitioners: How
   the Debate on Medical Expertise has Changed the Criteria of Lead
   Poisoning's Recognition in France During the 1970s},
Journal = {POLITIX},
Year = {2010},
Volume = {23},
Number = {91},
Pages = {103+},
Abstract = {This article aims to assess how occupational health became a topic of
   mobilizations and debates during the ``1968s{''} in France. In this
   period, workers claimed a better way of preventing occupational
   diseases. At that time medical power has also been scarcely criticized
   and some of medical practitioners helped workers in their fight to
   prevent occupational diseases efficiently. Dealing with the example of
   lead poisoning and a case of mobilization in the French Penarroya plant
   near Lyon, this article analyzes how new social actors, mainly workers
   and committed medical practitioners, managed to set up and promote an
   earlier screening of this occupational disease. It sheds light on a new
   medical appraisal process, which has stemmed from continuing debates
   between workers and medical practitioners. This process produced a
   counter-knowledge that turned out to challenge efficiently the output
   organization in this plant and the official occupational medicine as
   well.},
