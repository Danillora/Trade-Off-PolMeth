@article{ WOS:000212875700002,
Author = {Thompson, Dennis F.},
Title = {Representing future generations: political presentism and democratic
   trusteeship},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2010},
Volume = {13},
Number = {1},
Pages = {17-37},
Abstract = {Democracy is prone to what may be called presentism - a bias in the laws
   in favor of present over future generations. I identify the
   characteristics of democracies that lead to presentism, and examine the
   reasons that make it a serious problem. Then I consider why conventional
   theories are not adequate to deal with it, and develop a more
   satisfactory alternative approach, which I call democratic trusteeship.
   Present generations can represent future generations by acting as
   trustees of the democratic process. The general principle is that
   present generations should act to protect the democratic process itself
   over time. They should try to make sure that future citizens continue to
   have competent control over their collective decision-making.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
