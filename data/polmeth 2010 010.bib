@article{ WOS:000279020100008,
Author = {Grosser, Jens and Schram, Arthur},
Title = {Public Opinion Polls, Voter Turnout, and Welfare: An Experimental Study},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
Year = {2010},
Volume = {54},
Number = {3},
Pages = {700-717},
Month = {JUL},
Abstract = {We experimentally study the impact of public opinion poll releases on
   voter turnout and welfare in a participation game. We find higher
   overall turnout rates when polls inform the electorate about the levels
   of support for the candidates than when polls are prohibited.
   Distinguishing between allied and floating voters, our data show that
   this increase in turnout is entirely due to floating voters. When polls
   indicate equal levels of support for the candidates, turnout is high and
   welfare is low (compared to the situation without polls). In contrast,
   when polls reveal more unequal levels of support, turnout is lower with
   than without this information, while the effect of polls on welfare is
   nonnegative. Finally, many of our results are well predicted by quantal
   response (logit) equilibrium.},
