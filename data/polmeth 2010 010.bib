@article{ WOS:000215053000015,
Author = {Salleh, Anis Shuhaiza Bt Md and Ahmad, Noor Aza Bt},
Title = {Cross Boundary Marriage under Malaysian Family Law: Between a Dream of
   Life and Reality of Legal Requirements},
Journal = {JOURNAL OF POLITICS AND LAW},
Year = {2010},
Volume = {3},
Number = {2},
Pages = {148-155},
Month = {SEP},
Abstract = {The cross boundary marriage is not a new phenomenon particularly
   marriages involving Malaysians and Indonesians. It is not to question on
   happiness, love and affection but most importantly to highlight the
   reality when disputes arise. In this paper we will be highlighting the
   surrounding issues not only on the legality of the marriage itself but
   also on the issue of jurisdiction of the court and the rights of
   Malaysian wife in custody and maintenance. The discussion will be
   focusing on the provisions contained in the main statutes governing
   marriage and divorce matters in Malaysia, i. e. the Islamic Family Law
   (Federal Territories) Act 1984 (Act 303) and the Law Reform (Marriage
   and Divorce) Act 1976 (Act 164). At the end, authors will post
   suggestions for betterment and benefits of parties who might be thinking
   to have a cross boundary marriage.},
