@article{ WOS:000212627000003,
Author = {Hansen, Povl A. and Serin, Goran},
Title = {Rescaling or Institutional Flexibility? The Experience of the
   Cross-border Oresund Region},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2010},
Volume = {20},
Number = {2},
Pages = {201-227},
Abstract = {This article is based on the experience of the cross-border integration
   processes across the Oresund strait which separates Denmark from Sweden,
   and the related institutional and spatial interaction processes. The
   role of `space of flows' and that of `space of place', or territory,
   will be examined in relation to this cross-border integration. Several
   related questions will be addressed: has there been a re-scaling in the
   form of new cross-border institutions or have existing national and
   regional institutional structures been able to adapt to the new
   landscape to which the cross-border integration has given rise? Does a
   process exist whereby the `problem' and its solution are readily
   identified and absorbed by existing institutional structures with
   actions carried out through `selective competence delegation'? Based on
   two case studies, the integration of the labour market and the creation
   of a cross-border public railway network, the article discusses the
   institutional and spatial challenges which are consequences of and a
   context for the co-operation processes in the Oresund Region.},
