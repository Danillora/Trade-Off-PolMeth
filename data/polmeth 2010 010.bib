@article{ WOS:000283433100003,
Author = {Cres, Herve and Uenver, Utku},
Title = {IDEOLOGY AND EXISTENCE OF 50\%-MAJORITY EQUILIBRIA IN MULTIDIMENSIONAL
   SPATIAL VOTING MODELS},
Journal = {JOURNAL OF THEORETICAL POLITICS},
Year = {2010},
Volume = {22},
Number = {4},
Pages = {431-444},
Month = {OCT},
Abstract = {When aggregating individual preferences through the majority rule in an
   n-dimensional spatial voting model, the `worst-case' scenario is a
   social choice configuration where no political equilibrium exists unless
   a super-majority rate as high as 1 - 1/(n + 1) is adopted. In this paper
   we assume that a lower d-dimensional (d < n) linear map spans the
   possible candidates' platforms. These d `ideological' dimensions imply
   some linkages between the n political issues. We randomize over these
   linkages and show that there almost surely exists a 50\%-majority
   equilibria in the above worst-case scenario, when n grows to infinity.
   Moreover, the equilibrium is the mean voter.},
