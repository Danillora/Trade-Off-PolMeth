@article{ WOS:000210006000008,
Author = {Medeiros, Marcelo de Almeida},
Title = {Does Sao Paulo need a foreign policy? Hegemony, diplomacy and
   paradiplomacy in Brazil},
Journal = {AMERICA LATINA HOY-REVISTA DE CIENCIAS SOCIALES},
Year = {2010},
Volume = {56},
Pages = {163-186},
Month = {DEC},
Abstract = {This article argues, on the one hand, that the hegemonic condition of
   the state of Sao Paulo in the the Brazilian federation incites it to
   practice an endogenous paradiplomatic action; on the other hand, it
   alleges that this endogenous paradiplomatic action coincides with a
   phenomenon of des-concentration (and not of federalization) of the
   national diplomacy. The conclusion is that if the gradual
   desconcentration of the Brazilian diplomacy demands a consistent effort
   of the endogenous paradiplomacy, this does not mean that, for the
   maximization of the profits, the exogenous paradiplomacy does not have
   to be implemented.},
