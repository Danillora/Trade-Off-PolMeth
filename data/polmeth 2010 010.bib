@article{ WOS:000280491000006,
Author = {Bluhm, Katharina and Trappmann, Vera},
Title = {Capitalism in East-Central Europe Variants and External Influences: A
   Critique of the Literature},
Journal = {OSTEUROPA},
Year = {2010},
Volume = {60},
Number = {6},
Pages = {61+},
Month = {JUN},
Abstract = {Political economy analyses capitalism in East-Central Europe with two
   approaches. Using the first approach, it looks into the institutional
   varieties of market economies that have come into being in the past 20
   years and places the countries on a continuum from liberal to
   coordinated. In this way, differences between national economies are
   brought to light. The other approach examines similarities. It stresses
   the enormous significance of the European Union, as well as
   transnational companies, and identifies a particular ``dependent{''} or
   ``peripheral{''} capitalism in East-Central Europe. Both approaches have
   their strengths and weaknesses. What is needed is a synthesis.},
