@article{ WOS:000446916100010,
Author = {Atay, Tayfun},
Title = {`Ethnicity within Ethnicity' among the Turkish-Speaking Immigrants in
   London},
Journal = {INSIGHT TURKEY},
Year = {2010},
Volume = {12},
Number = {1},
Pages = {123-138},
Month = {WIN},
Abstract = {This article aims to explore multifaceted interactions among three
   segments of the `Turkish-speaking' immigrant community in London,
   composed of the Turks and the Kurds from Turkey and Turkish Cypriots.
   Drawing on findings of an extensive anthropological fieldwork, this
   paper not only documents that these sub-groups co-exist and cooperate
   with each other creating a sense of collective belonging to
   Turkish-speaking communities versus the others but also shows frictions
   and conflicts often occurring about ethnic, political, and ideological
   issues. As a result, an uneasy symbiosis exists between these sub-groups
   in London where they find themselves in an awkward position of existing
   together while not belonging to a unified group which can be described
   by the notion of `ethnicity within ethnicity'.},
