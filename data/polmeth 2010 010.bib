@article{ WOS:000212967800005,
Author = {Dekker, Fabian},
Title = {Self-Employed without Employees: Managing Risks in Modern Capitalism},
Journal = {POLITICS \& POLICY},
Year = {2010},
Volume = {38},
Number = {4},
Pages = {765-788},
Month = {AUG},
Abstract = {One characteristic of the postindustrial labor market is the rise of the
   self-employed worker without personnel. These workers are usually not
   (fully) included in the welfare system against social risks. Most policy
   makers as well as social scientists are convinced that these workers
   want to develop individual risk strategies, for example on the private
   insurance market. This dominant assumption regarding their risk behavior
   has, however, not been subjected to empirical research. Through 40
   in-depth interviews with self-employed workers in the Netherlands, this
   article examines their risk perception and coping behavior. The main
   conclusion is that the self-employed worker does not show the expected
   individual risk behavior. Exceptional behavior concerns the risk of
   unemployment. From a policy perspective, the findings suggest that these
   ``new{''} flexible workers may require collective social guarantees in
   response to risks.},
