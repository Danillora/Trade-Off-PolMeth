@article{ WOS:000219888900008,
Author = {Poblet, Marta},
Title = {ODR 3.0? Lessons from Sri Lanka, India, Kenya and Haiti},
Journal = {IDP-INTERNET LAW AND POLITICS},
Year = {2010},
Number = {10},
Month = {JUN},
Abstract = {In this article, we review the current state of online dispute
   resolution (ODR) services worldwide, followed by the presentation of
   some of the newer services that may inspire the configuration of a new
   generation of ODR services, what we call ``ODR 3.0{''}. The main
   objective of these 3.0 platforms is not to manage and resolve conflicts
   through mediation, conciliation or arbitration, although negotiation is
   involved in some cases. However, the impact they are having in related
   fields, such as emergency or crisis management, should be mentioned in a
   reflection on the future of ODR.},
