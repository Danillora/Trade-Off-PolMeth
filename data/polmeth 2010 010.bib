@article{ WOS:000277143000002,
Author = {Chepikova, Ksenia and Leisse, Olaf},
Title = {Russia's Simulated Federalism Regional Policy under Medvedev},
Journal = {OSTEUROPA},
Year = {2010},
Volume = {60},
Number = {1},
Pages = {15+},
Month = {JAN},
Abstract = {According to its constitution, Russia is a federation. But this
   federalism exists only on paper. President Vladimir Putin pushed through
   a recentralisation of the state that relegated the regions to the
   political periphery and the regional elites to statists in the political
   process. Contrary to his pronouncements, President Dmitrii Medvedev
   continues to strengthen the vertical structure of power by placing the
   communities under the guardianship of the centre as well. At the same
   time, the party United Russia is increasingly becoming an instrument by
   which the regions are controlled. With that, Putin, as party chairman,
   keeps the reins in his hands.},
