@article{ WOS:000276673700006,
Author = {Marchart, Oliver and Adolphs, Stephan and Hamm, Marion},
Title = {Social Movement Practice and ``Organic Theory{''} - On the Reception and
   Production of Theoretical Discourses in Social Movements with regard to
   the Precarisation Movement},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2010},
Volume = {39},
Number = {1},
Pages = {73+},
Abstract = {This article conceptualises ``organic theories{''} developed in social
   movements as a crucial component in their formation. Organic Theories
   open an interpretative horizon which allows movements to give meaning to
   their protest practices, establish a coherent self-perception and
   analytically contextualise the specific problems/issues.
   For the analysis of production and reception of organic theory, this
   article takes the transnational precarisation movement as an example.
   Since 2004, this movement network organises transnationally around the
   protest format of annual Euromayday Parades on the First of May. Until
   today, these took place in 40 European and several non-european cities.
   In our analysis, we ask how theories developed in academic specialised
   discourses are generalised in political movement discourses, i.e. their
   organic theory (elaborated interdiscourse), and how they are intertwined
   with existing forms of subjectivation and practices (the elementary
   discourse of everyday meaning-making processes and practices of movement
   actors). This is first analysed through a comparative discourse analysis
   of two text corpora. Second, an ethnographic analysis shows how
   discursive elements are produced in the practices of movements and acted
   out as ``embodied theory{''}.},
