@article{ WOS:000288381800003,
Author = {Clarke, John},
Title = {Enrolling ordinary people: governmental strategies and the avoidance of
   politics?},
Journal = {CITIZENSHIP STUDIES},
Year = {2010},
Volume = {14},
Number = {6, SI},
Pages = {637-650},
Abstract = {Recent strategies for `governing the social' have placed a premium on
   recruiting ordinary people to their processes. This commitment to the
   value of ordinary people links UK initiatives in public services and
   citizenship to innovations in development strategies in the global
   south. This paper asks what it is that makes ordinary people such a
   desired object of governmental strategies and suggests that it is their
   assumed a-political character and their capacity to bring values,
   knowledge and other resources that are beyond the state. This article
   suggests that keeping politics out of governing may be a governmental
   ambition, but ordinary people cannot be relied on to perform in such
   ways.},
