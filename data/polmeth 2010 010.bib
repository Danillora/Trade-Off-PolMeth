@article{ WOS:000277071700003,
Author = {Rios-Figueroa, Julio and Pozas-Loyo, Andrea},
Title = {Enacting Constitutionalism The Origins of Independent Judicial
   Institutions in Latin America},
Journal = {COMPARATIVE POLITICS},
Year = {2010},
Volume = {42},
Number = {3},
Pages = {293+},
Month = {APR},
Abstract = {When and why can constitution-making processes be expected to produce an
   institutional framework that formally serves constitutionalism? Based on
   a simple and general typology of constituent processes that captures
   their legal/political character and dynamic nature, constitution-making
   processes controlled by one cohesive and organized political group
   (unilateral) can be distinguished from processes controlled by at least
