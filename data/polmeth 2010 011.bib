@article{ WOS:000210612100004,
Author = {Kjellman-Chapin, Monica},
Title = {The Politics of Kitsch},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2010},
Volume = {22},
Number = {1},
Pages = {27-41},
Month = {JAN},
Abstract = {This essay discusses the problems inherent in the usages of the term
   ``kitsch'' to designate an astonishing variety of artifacts and images.
   The historical origins of the word and concept are addressed, including
   a brief historiographical overview of related literature. Focusing on
   the multivalence of kitsch, the author argues that one of the reasons
   kitsch is so slippery a concept is that it cuts across a variety of
   related terrains, including commercialism, artistic and aesthetic
   fakery, standards of taste as related to class, and conceptualizations
   of camp. Under postmodernism, a different transformation is taking place
   whereby kitsch can be mobilized as critical outside the construct of
   ``camp.''},
