@article{ WOS:000421192900010,
Author = {Ece, Nazife},
Title = {WHERE IS NATO IN TURKEY?},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2010},
Volume = {9},
Number = {1},
Pages = {85-93},
Month = {SPR},
Abstract = {This article will be an overview of the current debates about NATO in
   general that are held in Turkish media. Especially after September 2009,
   relations between NATO and Turkey have entered a new era because of the
   `new' foreign policy of Turkey under the AKP government, Obama's new
   strategy for Afghanistan and due to the `new' structuring inside the
   NATO with the `New Strategic Concept'. This article will focus on the
   most heated topics about NATO in Turkish media and how they are
   discussed. However it also aims to display that there are a lot of
   deficiencies and missing points in these discussions which makes them
