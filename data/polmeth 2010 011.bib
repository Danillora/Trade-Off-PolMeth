@article{ WOS:000457912900007,
Author = {Medushevsky, Nikolay},
Title = {ANALYTICAL CENTERS IN POLITICAL PROCESS: US MODEL OF ``THINK TANKS{''}
   AND ITS FUNCTIONING OUTSIDE THE COUNTRY},
Journal = {POLITEIA-JOURNAL OF POLITICAL THEORY POLITICAL PHILOSOPHY AND SOCIOLOGY
   OF POLITICS},
Year = {2010},
Volume = {57},
Number = {1},
Pages = {118+},
Abstract = {The article conducts the comprehensive analysis of the US ``think
   tanks{''} as specific institutions providing services to the policy
   process through the socio-political application studies and preparation
