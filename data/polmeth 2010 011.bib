@article{ WOS:000420872300007,
Author = {Casella, Antonio},
Title = {Socialism of the 21st Century: Communal Councils and City Halls in
   Venezuela},
Journal = {CUESTIONES POLITICAS},
Year = {2010},
Volume = {26},
Number = {44},
Pages = {115-140},
Month = {JAN-JUN},
Abstract = {Based on some formulations about what the Venezuelan government
   understands as ``socialism of the 21st century,{''} this study analyzes
   the existing relationship between city halls and the communal councils
   that have recently been conceptualized as an organizational form to
   realize the constitutional formula for protagonist participation of the
   population. A study was made of laws related to the theme as well as
   diverse speeches by the president and his ministers. Likewise, several
   interviews were made with people connected to the communal councils. The
   conclusion is that the communal councils and the city halls are in
   contradiction and that communal councils are: 1.- centralized
   authorities for political action, 2.- tools for controlling the
   population by the central government and, 3.- a way for the central
   government to control the city halls in order to finally eliminate them.},
