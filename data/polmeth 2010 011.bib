@article{ WOS:000275307500002,
Author = {Festenstein, Matthew},
Title = {Pragmatism, inquiry and political liberalism},
Journal = {CONTEMPORARY POLITICAL THEORY},
Year = {2010},
Volume = {9},
Number = {1},
Pages = {25-44},
Month = {FEB},
Abstract = {One of the most powerful but elusive motifs in pragmatist philosophy is
   the idea that a liberal democracy should be understood as a community of
   inquirers. This paper offers a critical appraisal of a recent attempt to
   make sense of this intuition in the context of contemporary political
   theory, in what may be called pragmatist political liberalism (PPL).
   Drawing together ideas from Rawlsian political liberalism, epistemic
   democracy and pragmatism, proponents of PPL argue that the pragmatist
   conception of inquiry can provide a satisfying interpretation of the
   idea of justificatory neutrality as it appears in political liberalism.
   This is contrasted with Dewey's understanding of the epistemic character
   of democracy, which is viewed as unacceptably sectarian. This paper
   identifies and criticizes the two principal lilies of argument made in
   support of PPL: the clarification argument and the fixation argument.
   Neither of these lilies of argument, it is argued, passes the test each
   sets itself. I argue that the latter closes down the epistemic openness
   in the justification of democracy that is central to pragmatism.
   Contemporary Political Theory (2010) 9, 25-44. doi:10.1057/cpt.2009.6},
