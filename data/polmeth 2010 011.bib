@article{ WOS:000278111600003,
Author = {Mendez, Jeanette Morehouse and Osborn, Tracy},
Title = {Gender and the Perception of Knowledge in Political Discussion},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2010},
Volume = {63},
Number = {2},
Pages = {269-279},
Month = {JUN},
Abstract = {Differences in knowledge about politics between men and women have the
   potential to affect political discussion. We examine differences in the
   perception of political knowledge between men and women and the effects
   these differences have on how often men and women talk about politics.
   We find both men and women perceive women to be less knowledgeable about
   politics and men to be more knowledgeable, regardless of the actual
   level of knowledge each discussion partner holds. This perceptual
   knowledge gap could have ramifications for discussion as political
   participation, since people turn to those they perceive to be experts to
   gather political information.},
