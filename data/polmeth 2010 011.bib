@article{ WOS:000282915900006,
Author = {Schweitzer, Eva Johanna},
Title = {Global Patterns of Virtual Mudslinging? The Use of Attacks on German
   Party Websites in State, National and European Parliamentary Elections},
Journal = {GERMAN POLITICS},
Year = {2010},
Volume = {19},
Number = {2},
Pages = {200-221},
Abstract = {Is negative campaigning on the internet an American phenomenon or an
   international trend? To explore this question, this article compares the
   use of attacks on German party websites in two state elections, one
   federal election and one election to the European parliament with recent
   American evidence. The results show that virtual mudslinging has
   affected German e-campaigns at all levels of the political system, with
   patterns similar to those found in the United States. This includes the
   amount and frequency of the attacks as well as their differential usage
