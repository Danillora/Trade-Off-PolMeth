@article{ WOS:000281013700002,
Author = {Daunfeldt, Sven-Olov and Praski-Stahlgren, Ulrika and Rudholm, Niklas},
Title = {Do high taxes lock-in capital gains? Evidence from a dual income tax
   system},
Journal = {PUBLIC CHOICE},
Year = {2010},
Volume = {145},
Number = {1-2},
Pages = {25-38},
Month = {OCT},
Abstract = {The purpose of this paper is to study whether investors' willingness to
   realize capital gains falls when the marginal tax rate on capital gains
   is raised. We use a rich register-based panel data set covering almost
   8\% of the Swedish population. The results indicate that a 10\% increase
   in capital gains tax rate reduces the number of realizations of capital
   gains with 8.7\% and the realized amount, given the decision to realize,
   with 1.9\%. In addition, we find that wealthy individuals seem to
   respond more to changes in capital gains tax rates than less-wealthy.},
