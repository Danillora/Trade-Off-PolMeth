@article{ WOS:000420471900001,
Author = {Robert, Cecile},
Title = {Expert groups in the government of Europe. Assessments and new research
   perspectives},
Journal = {POLITIQUE EUROPEENNE},
Year = {2010},
Volume = {32},
Number = {3},
Pages = {7-38},
Abstract = {This article sketches a new perspective on the expert groups of the
   European Commission which is situated in the international literature on
   expertise, roles of knowledge and government by committee in the
   European Union. It focuses on three dimensions: firstly, the main
   political uses of expert groups bearing in mind that knowledge-gathering
   and agreement-making are constantly intertwined in committee meetings
   (to what expert groups are of use?); secondly, the conditions and
   concrete processes in which expertise is built (How do experts work in
   those groups?); thirdly, the - common - trajectories and social
   properties of experts identified by the European Commission (who are the
   European experts and on what grounds?)},
