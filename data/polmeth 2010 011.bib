@article{ WOS:000217599700003,
Author = {Uzum, Baburhan and Uzum, Melike},
Title = {The Historical and Linguistic Analysis of Turkish Politicians' Speech},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
Year = {2010},
Volume = {23},
Number = {4},
Pages = {213-224},
Month = {DEC},
Abstract = {The current study investigates Turkish politicians' code mixing in their
   public speeches and the variation among the speakers as well as the
   historical and linguistic factors influencing this variation. It is
   hypothesized that the variation either consciously or unconsciously
   outlines the identity of the speaker in the direction that s/ he wants
   to be identified by their audience. The findings are interpreted with
   reference to the language reform (a linguistic change during the
   transition from the Ottoman Empire to the Turkish Republic) that Turkish
   language went through in 1928.},
