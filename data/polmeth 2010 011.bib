@article{ WOS:000217019400002,
Author = {Gamboa Rocabado, Franco},
Title = {What the bleep is, at last, the meaning of democracy in Latin America?:
   Politics and populism in times of globalization},
Journal = {REFLEXION POLITICA},
Year = {2010},
Volume = {12},
Number = {23},
Pages = {6-23},
Month = {JUN},
Abstract = {The possibility to define a precise meaning of democracy in the Twenty
   First Century demands answering multiple questions regarding its
   consolidation, which today is highly contested all over Latin America.
   Simultaneously, a definition about the scopes of democracy puts forward
   the necessity to have empirical data to facilitate a systematic
   explanation over a reality that is profoundly heterogeneous and
   contradictory. Hence, if many definitions seem to be clear when we try
   to understand the end of a dictatorship, daily claims always raise a
   confrontation between a minimal conceptualization on democracy vis-a-vis
   a normative or ideal concept of it, especially when we take into account
   different socio-cultural contexts that also have specific imaginaries
   about what is a healthy democratic system. Nowadays, it is important to
