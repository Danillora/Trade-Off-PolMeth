@article{ WOS:000210612100007,
Author = {Stephens, Paul and Weston, Robert},
Title = {Agorachronotistics (Speculations on Market Time)},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2010},
Volume = {22},
Number = {1},
Pages = {68-89},
Month = {JAN},
Abstract = {``Agorachronotistics (Speculations on Market Time)'' explores the
   specters of time that haunt conceptions of market exchange. Bringing
   Marx into contact with a range of contemporary scholarship,
   ``Agorachronotistics'' attempts a Benjamin-inspired ideology critique of
   the movements of speculative capital. ``Agorachronotistics'' breaks with
   direct scholarly exposition and makes its argument concerning the
   phantom character of market relations using the principle of montage.
   The project develops experimental prose genres introduced by Kracauer,
   Benjamin and Adorno, such as the Denkbild (a term used by Adorno to
   describe Benjamin's prose fragments), the thesis, the convolute, and
   found texts. Although these fragments seek to innovate new forms of
   cultural criticism and to challenge conventional modes of academic
   writing, they continue to follow conventional scholarly citation
   procedures. For further details about the Denkbild form, see Paul
   Stephens and Robert Hardwick Weston, ``Free Time: Overwork as
   Ontological Condition'' in Social Text no. 94 (Spring 2008).},
