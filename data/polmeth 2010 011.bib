@article{ WOS:000446917500011,
Author = {Celep, Odul},
Title = {Turkey's Radical Right and the Kurdish Issue: The MHP's Reaction to the
   ``Democratic Opening{''}},
Journal = {INSIGHT TURKEY},
Year = {2010},
Volume = {12},
Number = {2},
Pages = {125-142},
Month = {SPR},
Abstract = {Turkey's current government's `democratic opening' project has led to a
   series of political discussions regarding the cause and resolve of the
   Kurdish issue. One major consequence of this debate has been the
   polarization of opinion between conservatives, represented by the ruling
   Justice and Development Party (Adalet ve Kalkinma Partisi, AKP) and
   nationalists, represented by the Nationalist Action Party (Milliyetci
   Hareket Partisi, MHP). This study elaborates on the major reasons for
   MHP's opposition to AKP on the `democratic opening.' In doing so, the
   study examines the historical, ideological distinctions between the two
   parties and their perception of ethnic and linguistic differences in
   Turkish society. AKP comes from a political tradition that has been
   relatively more accommodating towards such differences. On the contrary,
   MHP has roots in an ethno-nationalist and mono-culturalist ideology,
   which can be observed in its denial of the identity component of the
   Kurdish issue.},
