@article{ WOS:000281079700001,
Author = {Smyth, Russell and Mishra, Vinod},
Title = {The Prestige of Australian State Supreme Courts Over the 20th Century},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2010},
Volume = {45},
Number = {3},
Pages = {323-336},
Abstract = {This article measures the prestige of the Australian State supreme
   courts and examines competing explanations for differentials in their
   prestige over the course of the 20th century. The study finds that the
   prestige of the State supreme courts are positively correlated with the
   reputation of their Bench, proxied by the number of High Court judges
   from that State; socioeconomic diversity in that State, proxied by the
   State's population; and legal capital, proxied by the number of pages of
   the official law reports of the State supreme court.},
