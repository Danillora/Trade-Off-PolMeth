@incollection{ WOS:000284289300004,
Author = {Duran, Juan J. and Ubeda, Fernando},
Editor = {Martin, MAG and Nissan, E},
Title = {EFFECTS OF THE TRANSVERSAL FOREIGN DIRECT INVESTMENT ON THE PRODUCTIVITY
   OF THE MEXICAN ECONOMY},
Booktitle = {INTERNATIONAL POLITICAL ECONOMY},
Year = {2010},
Pages = {41-53},
Abstract = {Multinational companies investing in communications, energy, transport
   and financial services, can have a differentiated effect in the host
   economy comparing with the one attributed to manufacturing investment.
   These companies are characterized by generation of an intermediate input
   aimed at practically the totality of an economy's sectors (intermediate
   input as well as a final good for consumers), reason why we have
   denominated the matter multinational transversal services. We have
   analyzed in this paper if inflow foreign direct investment in
   transversal services over economic growth and productivity is
   significantly higher to the effect caused by inflow foreign direct
   investment in manufacturing sector. During the period 1985-2006, the
   results obtained in owner analysis show a positive and very similar
   effect of inward manufacturing FDI (MFDI) and inward transversal FDI
   (TFDI) on labor productivity (LP).},
