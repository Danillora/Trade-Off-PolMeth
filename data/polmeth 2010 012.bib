@article{ WOS:000288504400005,
Author = {Day, Gail and Edwards, Steve and Mabb, David},
Title = {`What Keeps Mankind Alive?': the Eleventh International Istanbul
   Biennial. Once More on Aesthetics and Politics},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2010},
Volume = {18},
Number = {4},
Pages = {135-171},
Abstract = {Starting from the 2009 Istanbul Biennial, with its Brechtian curatorial
   theme, this essay considers the Left's varying responses to art's
   so-called `political turn'. Discussion ranges from the local and
   regional context of the Biennial's function as part of Turkey's bid to
   join the EU, through to a longer theoretical perspective on the critical
   debates over `art and life', artistic autonomy and heteronomy, and the
   revival in avant-gardism. The authors propose that the standard accounts
   of the intimate connection between the commodity and art have become
   politically counterproductive. They suggest that Marxist analysis needs
   to develop a more complexly-articulated philosophical reflection on the
   relation between economy, politics, and art - and between political and
   aesthetic praxes - if it is to advance its longstanding contributions to
   considerations of `aesthetics and politics'.},
