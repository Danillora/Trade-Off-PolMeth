@incollection{ WOS:000284289300007,
Author = {Madrid Juan, Matilde},
Editor = {Martin, MAG and Nissan, E},
Title = {INTERNATIONAL COMPETITIVENESS: MEASUREMENT INDICATORS AND STRUCTURAL
   ASPECTS WITH SPECIFIC REFERENCE TO THE SITUATION IN SPAIN},
Booktitle = {INTERNATIONAL POLITICAL ECONOMY},
Year = {2010},
Pages = {91-103},
Abstract = {With globalization and the concomitant appearance of new actors in
   international trade, competitiveness has come to play a key role in
   political discourse and to constitute an objective of economic policy in
   both developed and developing nations. The term competitiveness,
   however, can be interpreted in a number of ways and tends to be
   ambiguous. The present article focuses on international competitiveness.
   Its review of the chief ex-ante and ex-post indicators used to measure
   that concept is followed by a discussion of export base growth as an
   element of competitiveness, illustrated with a model applied to the
   Spanish economy. Finally, the competitiveness of Spanish manufacturing
   industries that compete with imports is shown to be related to their
   respective technology intensity.},
