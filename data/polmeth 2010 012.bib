@article{ WOS:000275989800011,
Author = {Kim, Iljoong and Park, Sungkyu},
Title = {Eminent domain power and afterwards: Leviathan's post-taking opportunism},
Journal = {PUBLIC CHOICE},
Year = {2010},
Volume = {143},
Number = {1-2},
Pages = {209-227},
Month = {APR},
Abstract = {This paper investigates post-taking procedures, a research area not yet
   fully explored. It discusses the economic implications underlying the
   landowner's `repurchase right (RR)' which can be invoked upon the
   occurrence of a so-called `change in use after taking.' RR is compared
   with the government's discretion regarding such changes. The lack of
   post-taking accountability tends to create power ripe for abuse, and RR
