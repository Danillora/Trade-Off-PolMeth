@article{ WOS:000277645200013,
Author = {Amoros, Pablo and Puy, M. Socorro},
Title = {Indicators of electoral victory},
Journal = {PUBLIC CHOICE},
Year = {2010},
Volume = {144},
Number = {1-2},
Pages = {239-251},
Month = {JUL},
Abstract = {We explore the role that campaign expenditures play in determining
   electoral outcomes. We study a two-party contest where campaign funds
   can affect the preferences of voters regarding the saliency of two
   political issues. We show that an advantage in campaign resources, a
   pre-campaign partisan advantage, an advantage on every salient issue, or
   a combination of these indicators, do not always guarantee electoral
   victory. By contrast, electoral victory is guaranteed if the sum of the
   proportions of the electorate supporting a party on every salient issue
   is greater than a critical value. For that to happen it is necessary
   (but not sufficient) that the party has an advantage on every salient
   issue.},
