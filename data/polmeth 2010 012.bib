@article{ WOS:000278074100001,
Author = {Bollard, Robert},
Title = {The Great Strike of 1917-Was Defeat Inevitable?},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2010},
Volume = {56},
Number = {2},
Pages = {159-172},
Month = {JUN},
Abstract = {The Great Strike of 1917 was, arguably, the greatest class confrontation
   in Australian history. For five weeks, the eastern states of Australia
   were paralysed by a mass strike, driven from below by a rank and file
   paying little heed to their reluctant leaders. Strike action involved
   nearly 100,000 workers and was only defeated by mass scabbing
   masterminded by conservative state and federal governments. Historians
   have tended to dismiss the Great Strike as mindless militancy. Central
   to this negative assessment has been the belief that the strikers could
   not win because ``there was too much coal at grass{''}. But this may be
   a wrong assumption to make. A careful investigation of the actual
   situation with coal stocks in 1917 demonstrates that the strikers came
   much closer to winning than generally has been accepted.},
