@incollection{ WOS:000284289300008,
Author = {Ramrattan, Lall and Szenberg, Michael},
Editor = {Martin, MAG and Nissan, E},
Title = {VARIATION OF NET BENEFITS IN THE GLOBAL ECONOMY: THE CASE OF NAFTA AND
   EMU},
Booktitle = {INTERNATIONAL POLITICAL ECONOMY},
Year = {2010},
Pages = {105-117},
Abstract = {We present an assessment of net financial benefits for the U.S. and the
   European Monetary Union (EMU) against the rest of the world (ROW). While
   other assessments of regional integration in the Global economy are
   possible, NAFTA and EMU affects nearly 40 percent of world trade. The
   assessment results from Computational General Equilibrium (CGE) method
   using GTAP data sets. On balance, we find positive net benefits, which
   tend to decline in the EMU after the initial impact. A popular
   explanation is that the benefits for a currency union can be sourced to
   the saving from having a single currency. Another explanation is that
   profits fall as convergence takes place. Overall, globalization reflects
   reduction of interest rates, deficits, and unemployment rates.},
