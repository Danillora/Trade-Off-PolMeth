@article{ WOS:000275989800004,
Author = {Cheibub, Jose Antonio and Gandhi, Jennifer and Vreeland, James Raymond},
Title = {Democracy and dictatorship revisited},
Journal = {PUBLIC CHOICE},
Year = {2010},
Volume = {143},
Number = {1-2},
Pages = {67-101},
Month = {APR},
Abstract = {We address the strengths and weaknesses of the main available measures
   of political regime and extend the dichotomous regime classification
   first introduced in Alvarez et al. (Stud. Comp. Int. Dev. 31(2):3-36,
   1996). This extension focuses on how incumbents are removed from office.
   We argue that differences across regime measures must be taken seriously
   and that they should be evaluated in terms of whether they (1) serve to
   address important research questions, (2) can be interpreted
   meaningfully, and (3) are reproducible. We argue that existing measures
   of democracy are not interchangeable and that the choice of measure
   should be guided by its theoretical and empirical underpinnings. We show
   that the choice of regime measure matters by replicating studies
   published in leading journals.},
