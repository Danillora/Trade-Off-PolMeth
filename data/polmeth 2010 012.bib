@article{ WOS:000280434600007,
Author = {Goeckel, Robert F.},
Title = {Soviet Religious Policy in the Baltics under Khrushchev, 1957-1964:
   Domestic Repression and International Engagement},
Journal = {POLITICS AND RELIGION},
Year = {2010},
Volume = {3},
Number = {2},
Pages = {352-388},
Month = {AUG},
Abstract = {The Khrushchev era brought a policy of religious repression in response
   to a resurgence of religious adherence after WWII. But a close
   evaluation of archival sources reveals distinctive features to its
   implementation in the Baltics, compared with other parts of the USSR and
   with the Orthodox Church. The study richly describes the erosion of
   church institutional interests, such as maintenance of registered
   churches and clergy, legal and financial autonomy, publications and
   theological education. But having adapted to their national context,
   republic-level state officials often pursued the campaign
   disproportionately against non-national churches, such as Catholics and
   so-called sects, thereby provoking bureaucratic tensions with party
   hardliners, both in Moscow and at the republic level. Motivated by its
   heightened anti-Vatican stance, Moscow's foreign policy opening-to the
   World Council of Churches, Lutheran World Federation, and the German
   churches-brought the Lutheran churches limited leverage against
   reluctant local and republic officials.},
