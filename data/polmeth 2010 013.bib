@article{ WOS:000281676400006,
Author = {Rueger, Jan},
Title = {OXO: Or, the Challenges of Transnational History},
Journal = {EUROPEAN HISTORY QUARTERLY},
Year = {2010},
Volume = {40},
Number = {4, SI},
Pages = {656-668},
Month = {OCT},
Abstract = {This article discusses the benefits and challenges of transnational
   approaches for modern European history. It reconstructs the origins of a
   particular Anglo-German entanglement: the meat essence OXO, originally a
   German invention made in South America by a London-based company. And it
   links this example to the questions prompted by the rise of
   transnational history. Surveying the recent literature, the article
   argues that the parallel histories of nation states and the
   transnational interest in the space between and beyond them need not be
   mutually exclusive. The microhistory of OXO thus illustrates the
   weaknesses as much as the strengths of `transnationalism'.},
