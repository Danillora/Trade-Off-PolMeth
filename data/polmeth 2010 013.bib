@article{ WOS:000280079600010,
Author = {Buchholz, Wolfgang and Schumacher, Jan},
Title = {Discounting and welfare analysis over time: Choosing the η},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2010},
Volume = {26},
Number = {3, SI},
Pages = {372-385},
Month = {SEP},
Abstract = {Based on the Ramsey equation and the rejection of pure utility time
   discount, the Stern Review on the Economics of Climate Change
   concentrates on the use of the elasticity of marginal utility eta in the
   intergenerational social welfare function (and not on the pure rate of
   time preference p as most of the existing literature) in order to avoid
   excessive saving. In this paper we first show that also from the
   viewpoint of sustainability, it is preferable to make ethically
   motivated decisions on the distribution across generations by the use of
   eta and not of rho. In addition, we examine how different specific
   values of eta correspond to ethically relevant properties as
   ``circumstance solidarity{''} and ``no-envy{''}. Finally, we discuss the
   fundamental critique that has been raised against Stern's ethically
   based welfare theoretic approach to valuation across time. (C) 2009
   Elsevier B.V. All rights reserved.},
