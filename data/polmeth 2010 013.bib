@article{ WOS:000278178100009,
Author = {Geys, Benny and Heinemann, Friedrich and Kalb, Alexander},
Title = {Voter involvement, fiscal autonomy and public sector efficiency:
   Evidence from German municipalities},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2010},
Volume = {26},
Number = {2},
Pages = {265-278},
Month = {JUN},
Abstract = {Social and/or political involvement within the population is often
   argued to enhance public sector performance. The underlying idea is that
   engagement fosters political awareness and interest and increases the
   public's monitoring ability. Still, weak fiscal autonomy can undermine
   voters' interest in and demand for an efficient production of public
   services. In our contribution, we test whether and how voter involvement
   in the political sphere is related to government performance - in terms
   of its efficiency - using a broad panel of German municipalities. Our
   results suggest that voter involvement indeed has a positive impact on
   cost efficiency. Crucially, however, this efficiency-enhancing effect of
   voter involvement is significantly positively affected by local
   governments' fiscal autonomy. (C) 2009 Elsevier B.V. All rights
   reserved.},
Publisher = {ELSEVIER SCIENCE INC},
