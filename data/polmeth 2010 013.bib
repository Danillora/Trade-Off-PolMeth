@article{ WOS:000285319600003,
Author = {Kovanda, Lukas},
Title = {CRITICAL REALISM AS AN ONTOLOGICAL BASIS OF POST-KEYNESIANISM},
Journal = {POLITICKA EKONOMIE},
Year = {2010},
Volume = {58},
Number = {5},
Pages = {608-622},
Abstract = {In the following text we introduce those approaches to the methodology
   of economics that are relevant from the perspective of
   post-Keynesianism. After a partially historical outline of philosophical
   and methodological stances of John Maynard Keynes and Nicholas Kaldor we
   attempt to shed some light on a more recent methodological concept,
   critical realism. This approach is in one way or in other influenced by
   the two economists' ideas in the realm, as well as some other
   post-Keynesians. We argue that this, originally heterodox, concept has
   had an ambition not only to operate as an alternative to the methodology
   of mainstream, orthodox economics, but to substitute it and thus,
   proponents say, make economics as such more relevant.},
