@article{ WOS:000212999200003,
Author = {Min, Seong-Jae},
Title = {From the Digital Divide to the Democratic Divide: Internet Skills,
   Political Interest, and the Second-Level Digital Divide in Political
   Internet Use},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2010},
Volume = {7},
Number = {1},
Pages = {22-35},
Abstract = {Digital divide research is now focused on the so-called second-level
   divide, which concerns Internet ``usage{''} divides. This article
   suggests that while the first-level divide was associated with
   sociodemographic factors, the second-level divide is associated with
   factors such as motivations and Internet skills. It then illustrates an
   example of the second-level digital divide-the democratic divide. The
   democratic divide concerns the differences between those who actively
   use the Web for politics and those who do not. Analysis of General
   Social Survey data shows there is a democratic divide where political
   Internet users are individuals with high Internet skills and political
   interest.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
