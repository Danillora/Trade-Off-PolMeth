@article{ WOS:000281140200004,
Author = {Di Bartolomeo, Giovanni and Manzo, Marco},
Title = {FISCAL POLICY UNDER BALANCED BUDGET AND INDETERMINACY: A NEW KEYNESIAN
   PERSPECTIVE},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2010},
Volume = {57},
Number = {4},
Pages = {455-472},
Month = {SEP},
Abstract = {We investigate the effect of fiscal policy on equilibrium determinacy in
   a New Keynesian economy with rule-of-thumb (liquidity-constrained)
   consumers and capital accumulation by focusing on the inter-action
   between monetary policy and taxation under the assumption of balanced
   budget. Our main finding is that taxation of firms' monopoly rents
   enforce the Taylor principle - the prescription that the nominal
   interest rate must respond more than one for a unitary change in
   inflation - as a criterion for equilibrium determinacy.},
