@article{ WOS:000289912500003,
Author = {Macdonald, Kate and Macdonald, Terry},
Title = {The liberal battlefields of global business regulation},
Journal = {ETHICS \& GLOBAL POLITICS},
Year = {2010},
Volume = {3},
Number = {4},
Pages = {303-324},
Abstract = {The global justice movement has often been associated with opposition to
   the broad programme of `neoliberalism' and associated patterns of
   `corporate globalisation', creating a widespread impression that this
   movement is opposed to liberalism more broadly conceived. Our goal in
   this article is to challenge this widespread view. By engaging in
   critical interpretive analysis of the contemporary `corporate
   accountability' movement, we argue that the corporate accountability
   agenda is not opposed to the core values of a liberal project. Rather,
   it is seeking to reconfigure the design of liberal institutions of
   individual rights-protection, adjusting these for new material
   conditions associated with economic globalisation, under which powerful
   corporations alongside states now pose direct and significant threats to
   individual rights. This activist agenda is, therefore, much less radical
   in its challenge to the prevailing liberal global order than it may
   initially appear, since it functions to buttress rather than corrode
   many core normative commitments underpinning the liberal political
   project.},
