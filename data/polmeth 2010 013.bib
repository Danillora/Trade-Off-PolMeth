@article{ WOS:000286297000008,
Author = {Gizelis, Theodora-Ismene and Wooden, Amanda E.},
Title = {Water resources, institutions, \& intrastate conflict},
Journal = {POLITICAL GEOGRAPHY},
Year = {2010},
Volume = {29},
Number = {8},
Pages = {444-453},
Month = {NOV},
Abstract = {Although linkages between water scarcity and conflict have received a
   great deal of attention, both in qualitative case studies as well as
   quantitative studies, the relationship remains unclear since the
   literature has generally not considered the effectiveness of governance.
   We distinguish between direct effects and indirect effects linking water
   resource scarcity and conflict by systematically examining how
   intervening factors, such as political institutions, might influence the
   impact of water scarcity on the probability of conflict. We find support
   for our hypotheses postulating both direct and indirect relationships
   between water scarcity, governance, and conflict. (C) 2010 Elsevier Ltd.
   All rights reserved.},
