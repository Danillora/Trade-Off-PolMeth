@article{ WOS:000280434600004,
Author = {Lisovskaya, Elena and Karpov, Vyacheslav},
Title = {Orthodoxy, Islam, and the Desecularization of Russia's State Schools},
Journal = {POLITICS AND RELIGION},
Year = {2010},
Volume = {3},
Number = {2},
Pages = {276-302},
Month = {AUG},
Abstract = {This article explores the social origins and consequences of
   Orthodox-Muslim tensions surrounding the attempts to find a place for
   religion in Russia's state schools. It demonstrates that the
   Orthodox-Muslim tensions are an inevitable outcome of what we define as
   Russia's pattern of ``desecularization from above.{''} The attempts to
   restore religious education are carried out by alliances of top
   religious and political elites, which almost by default focus on the
   state-run schools. These attempts run into serious social and
   institutional constraints, and generate considerable Orthodox-Muslim
   tensions and controversies spilling over to public opinion. On the
   surface, the Orthodox-Muslim tensions often appear in the form
   ironically resembling ``culture wars{''} between religious
   traditionalists and secularists in the West. Our survey data indicate
   that public support for religious instruction in state-run schools has
   reached high levels and is infused with a noticeable element of
   religious intolerance. We predict that further attempts to desecularize
   Russia's state schools ``from above{''} may have destabilizing effects
   in society and fuel ethno-religious tensions.},
