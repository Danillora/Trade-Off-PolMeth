@incollection{ WOS:000284289300014,
Author = {Dincer, Oguzhan C. and Payne, James E.},
Editor = {Martin, MAG and Nissan, E},
Title = {RELIGIOUS DIVERSITY AND CORRUPTION: CROSS COUNTRY EVIDENCE},
Booktitle = {INTERNATIONAL POLITICAL ECONOMY},
Year = {2010},
Pages = {203-216},
Abstract = {Using recently developed indices of fractionalization and polarization,
   we investigate the effects of religious diversity on corruption across
   countries. We find a linear and positive relationship between religious
   polarization and corruption and an inverse U-shaped relationship between
   religious fractionalization and corruption, and thus the corruption
   maximizing level of religious fractionalization. According to our
   results, a 1 standard deviation increase in polarization index causes
   the corruption index to increase by 0.17 standard deviations which is
   almost 3 times larger than the effects of some of the control variables
   such as openness and education on corruption. All else constant,
   corruption is maximized when the fractionalization index is equal to
   0.57 which falls well within the range of observed values.},
