@article{ WOS:000281552800005,
Author = {Levine, Paul and Lotti, Emanuela and Pearlman, Joseph and Pierse,
   Richard},
Title = {GROWTH AND WELFARE EFFECTS OF WORLD MIGRATION},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2010},
Volume = {57},
Number = {5},
Pages = {615-643},
Month = {NOV},
Abstract = {Using a two-bloc endogenous growth model calibrated to two generic
   sending and receiving countries of equal size, we assess the growth and
   welfare impact of world migration flows of different skill compositions.
   The sending country (East) has a lower total factor productivity and a
   lower endowment of skilled labour. Migration can induce two
   growth-enhancing effects: an efficiency effect from the more efficient
   use of labour in the receiving country (West) and a sectoral
   reallocation effect from a fall in the host country skilled-unskilled
   wage rates. Despite growth gains, there are both winners (migrants, the
   representative Western non-migrant household) and losers (the
   representative Eastern household remaining). Remittances can see the
   latter group joining the winners.},
