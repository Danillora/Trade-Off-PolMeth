@article{ WOS:000282086900004,
Author = {Leigh, Daniel and Olters, Jan-Peter},
Title = {Natural-Resource Depletion, Habit Formation, and Sustainable Fiscal
   Policy: Lessons from Gabon},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2010},
Volume = {120},
Number = {3},
Pages = {487-508},
Month = {MAY-JUN},
Abstract = {While models based on Friedman's (1957) permanent-income hypothesis can
   provide oil-producing countries with long-run fiscal targets, they
   usually abstract from short-run costs associated with consolidation.
   This paper proposes a model that takes such adjustment costs (or
   ``habits{''}) into consideration. The approach is applied to Gabon,
   where oil reserves are expected to be exhausted within 30 years. The
   results suggest that Gabon's current fiscal-policy stance cannot be
   maintained, but that the presence of habits justifies smoothing
   adjustment towards the sustainable level over three to five years.},
