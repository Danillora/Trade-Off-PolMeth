@article{ WOS:000281013700006,
Author = {Gersbach, Hans and Mueller, Markus},
Title = {Flexible pensions for politicians},
Journal = {PUBLIC CHOICE},
Year = {2010},
Volume = {145},
Number = {1-2},
Pages = {103-124},
Month = {OCT},
Abstract = {It may be difficult to motivate politicians in their last term. To solve
   this problem, we suggest a triple mechanism involving political
   information markets, flexible pensions, and democratic elections. An
   information market is used to predict the potential reelection chances
   of the politician. Pensions depend on the price in the information
   market and thereby motivate the politician to act in a socially optimal
   manner. We show that, on balance, the triple mechanism increases social
   welfare. Finally, we suggest several ways to avoid the manipulation of
   information markets and we discuss possible pitfalls of flexible
   pensions.},
