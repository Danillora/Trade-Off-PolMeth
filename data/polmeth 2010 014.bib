@article{ WOS:000277645200016,
Author = {Torgler, Benno and Schneider, Friedrich and Schaltegger, Christoph A.},
Title = {Local autonomy, tax morale, and the shadow economy},
Journal = {PUBLIC CHOICE},
Year = {2010},
Volume = {144},
Number = {1-2},
Pages = {293-321},
Month = {JUL},
Abstract = {Policymakers often propose strict enforcement strategies to fight the
   shadow economy and to increase tax morale. However, there is an
   alternative bottom-up approach that decentralises political power to
   those who are close to the problems. This paper analyses the
   relationship with local autonomy. We use data on tax morale at the
   individual level and macro data on the size of the shadow economy to
   analyse the relevance of local autonomy and compliance in Switzerland.
   The findings suggest that there is a positive (negative) relationship
   between local autonomy and tax morale (size of the shadow economy).},
