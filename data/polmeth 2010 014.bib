@article{ WOS:000275739100008,
Author = {Wheeler, Angela R. and Stone, Rosalie A. Torres},
Title = {Exploring Stress and Coping Strategies among National Guard Spouses
   during Times of Deployment: A Research Note},
Journal = {ARMED FORCES \& SOCIETY},
Year = {2010},
Volume = {36},
Number = {3},
Pages = {545-557},
Month = {APR},
Abstract = {Studies on active duty military families indicate that deployment
   disrupts normal functioning of the family. Scholars still, however, lack
   the necessary knowledge to fully grasp the impact that the current Iraq
   and Afghanistan wars have had on Army National Guard (ARNG) families who
   have experienced deployment. A grounded theoretical approach to
   interviews with ARNG spouses yields insight into how these families are
   coping with the often-unexpected event of wartime deployment. The
   authors identify stressors and coping strategies used by these spouses
   and offer suggestions for future research that will allow scholars to
   more fully understand the present situation ARNG families are facing..},
