@incollection{ WOS:000284289300005,
Author = {Ruesga Benito, Santos Miguel and Cunha, Andre Moreira and da Silva
   Bichara, Julimar},
Editor = {Martin, MAG and Nissan, E},
Title = {FINANCIAL GLOBALIZATION AND PERIPHERAL STRATEGIES: RECENT EXPERIENCES IN
   LATIN AMERICA AND LESSONS FROM ASIA},
Booktitle = {INTERNATIONAL POLITICAL ECONOMY},
Year = {2010},
Pages = {55-72},
Abstract = {Despite the strong theoretical expected linkages between financial
   openness and economic growth, the financial globalization has been
   primarily a source of economic instability. This article presents a
   strategy of alternative political economy to the passivity of Latin
   American countries' political economy facing up to the globalization. It
   emphasizes the idea that a permanent tension exists between the
   internationalization trend in economy relationships and the search for
   autonomy in the political economies; that tension is developed in an
   unstable external environment and in a new international financial
   liberal order. This paper compares the Asiatic and the Latin American
   trajectories after the financial crisis in 1997, and it analyzes the
   recent reform of the international financial architecture.},
