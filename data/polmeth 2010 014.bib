@incollection{ WOS:000284289300009,
Author = {Castano Martinez, Maria-Soledad},
Editor = {Martin, MAG and Nissan, E},
Title = {COORDINATION OF FISCAL POLICIES IN EMU},
Booktitle = {INTERNATIONAL POLITICAL ECONOMY},
Year = {2010},
Pages = {119-134},
Abstract = {A certain loss in efficient national economic policies has taken place,
   given that the globalization of the economy and steps towards economic
   integration mean that the economic policies of different countries has
   an effect that reaches beyond their own borders, not only positive
   ({''}locomotive effect{''}), but also negative ({''}beggar your
   neighbour{''}).
   In addition, taking into account the efficiency problems of fiscal
   policy and the effects caused by other countries, coupled with the
   fiscal policy effects on monetary policy in the Monetary Union, the
   basis for coordinating national fiscal policies and the conditions set
   out in order to form part of the EMU, were established in the Maastricht
   Treaty. This process of restricting room for manoeuvre in national
   fiscal policies culminated in the Stability and Growth Pact (SPG)
   So, this chapter is devoted to the analysis of the evolution and
   theories that analyze the economic effects of the use of fiscal policy.
   The third section undertakes the analysis of the conditions imposed by
   the Stability and Growth Pact and how it conditions the possibilities of
   using fiscal policy on the part of member states. Section four contains
   an empirical analysis of the evolution of public finances for countries
   in the Euro zone. The chapter ends with some brief conclusions},
