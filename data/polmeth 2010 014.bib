@article{ WOS:000277330300004,
Author = {Chen, Yu-Fu and Funke, Michael},
Title = {BOOMS, RECESSIONS AND FINANCIAL TURMOIL: A FRESH LOOK AT INVESTMENT
   DECISIONS UNDER CYCLICAL UNCERTAINTY},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2010},
Volume = {57},
Number = {3},
Pages = {290-317},
