@article{ WOS:000280479400003,
Author = {Bergan, Daniel E.},
Title = {Estimating the Effect of Tobacco Contributions on Legislative Behavior
   Using Panel Data},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2010},
Volume = {91},
Number = {3},
Pages = {635-648},
Month = {SEP},
Abstract = {Objective. This article uses a data set of California State legislators
   to determine the effect of tobacco contributions on legislative
   behavior. Methods. Although many previous studies have analyzed the
   effect of campaign contributions on legislative behavior, this study
   makes a number of improvements to previous studies, including the use of
   panel data and improved instruments. Results. I find that contrary to
   prior research on tobacco industry contributions, campaign contributions
   from the tobacco industry do not influence legislative support for the
   tobacco industry. Conclusion. This suggests that the tobacco industry is
   not an exception to the literature on the influence of campaign
   contributions, which has shown that campaign contributions generally do
   not influence legislative voting and other behavior.},
