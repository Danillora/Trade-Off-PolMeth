@article{ WOS:000286042900004,
Author = {Mayer, Robert},
Title = {One Step Forward, Two Steps Back: On Lars Lih's Lenin},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2010},
Volume = {18},
Number = {3},
Pages = {47-63},
Abstract = {Lars Lih's Lenin Rediscovered seeks to replace the textbook-myth of
   Leninism with a painstaking reconstruction of `Lenin's Erfurtian drama'.
