@article{ WOS:000277645200017,
Author = {Bjornskov, Christian},
Title = {How does social trust lead to better governance? An attempt to separate
   electoral and bureaucratic mechanisms},
Journal = {PUBLIC CHOICE},
Year = {2010},
Volume = {144},
Number = {1-2},
Pages = {323-346},
Month = {JUL},
Abstract = {This paper explores questions related to the association between social
   trust and governance. In particular, the paper explores whether the
   trust-governance association is mainly a reflection of political
   responsiveness to the demands of the electorate or of the supply of
   honest politicians and bureaucrats. After outlining some simple theory,
   the findings suggest that the association reflects a causal influence of
   both types of trust on institutions of economic-judicial governance
   while electoral institutions are not associated with social trust.
   Assessed at the sample mean, the value of social trust under high
   political competition as evaluated by the compensating income variation
   is substantial.},
