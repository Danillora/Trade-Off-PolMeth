@article{ WOS:000284456500005,
Author = {Beik, William},
Title = {Response to Henry Heller's `The Longue Duree of the French
   Bourgeoisie'},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2010},
Volume = {18},
Number = {2},
Pages = {117-122},
Abstract = {Beik criticises Heller's mechanical view of the dynamic role of the
   bourgeoisie in the rise of capitalism in early-modern France. While they
   agree that the primary class-conflict was between the nobility and the
   peasantry, Beik stresses the slow emergence of genuine capitalist social
   relations and the cooptation of the bourgeoisie by a monarchical state
   which was still propping up the feudal regime, whereas Heller views
   mercantile activity and production increases as evidence of rising
   capitalism.},
