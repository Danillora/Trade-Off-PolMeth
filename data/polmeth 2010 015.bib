@article{ WOS:000273100200003,
Author = {Stromback, Jesper and van Aelst, Peter},
Title = {Exploring Some Antecedents of the Media's Framing of Election News: A
   Comparison of Swedish and Belgian Election News},
Journal = {INTERNATIONAL JOURNAL OF PRESS-POLITICS},
Year = {2010},
Volume = {15},
Number = {1},
Pages = {41-59},
Month = {JAN},
Abstract = {Despite the major importance of the news media's election news coverage,
   there are still only a few cross-national studies on how the media cover
   elections. There are even fewer that include both newspapers and TV news
   and that probe possible antecedents of how the media frame politics in
   their election news. Against this background, this article compares the
