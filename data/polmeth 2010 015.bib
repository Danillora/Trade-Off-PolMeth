@article{ WOS:000275995200003,
Author = {Tsfati, Yariv and Elfassi, Dana Markowitz and Waismel-Manor, Israel},
Title = {Exploring the Association between Israeli Legislators' Physical
   Attractiveness and Their Television News Coverage},
