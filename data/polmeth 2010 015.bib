@article{ WOS:000281013700004,
Author = {Hou, Yilin and Smith, Daniel L.},
Title = {Do state balanced budget requirements matter? Testing two explanatory
   frameworks},
Journal = {PUBLIC CHOICE},
Year = {2010},
Volume = {145},
Number = {1-2},
Pages = {57-79},
Month = {OCT},
Abstract = {Balanced budget requirements (BBRs) affect all aspects of financial
   operations. Previous studies relied on characterizations that highlight
   a constitutional-statutory distinction. Hou and Smith (Public Budgeting
   \& Finance 26(3):22-45, 2006) instead propose a political-technical
   construct. This article uses probit estimation, six measures of balance,
   and long panels to test which framework offers more explanatory power.
   The findings suggest that BBRs matter to varying degrees. Technical
   requirements exert bigger effects than political ones, the effects are
   more obvious on narrower than broader measures of balance and in the
   later phases of the budget cycle, and the political-technical construct
   offers more explanatory power than the constitutional-statutory
   distinction.},
