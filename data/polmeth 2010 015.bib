@article{ WOS:000277645200012,
Author = {Dreher, Axel and Schneider, Friedrich},
Title = {Corruption and the shadow economy: an empirical analysis},
Journal = {PUBLIC CHOICE},
Year = {2010},
Volume = {144},
Number = {1-2},
Pages = {215-238},
Month = {JUL},
Abstract = {This paper analyzes the influence of the shadow economy on corruption
   and vice versa. We hypothesize that corruption and the shadow economy
   are substitutes in high income countries while they are complements in
   low income countries. The hypotheses are tested for a cross-section of
   98 countries. Our results show that there is no robust relationship
   between corruption and the size of the shadow economy when
   perceptions-based indices of corruption are used. Employing an index of
   corruption based on a structural model, however, corruption and the
   shadow economy are complements in countries with low income, but not in
   high income countries.},
