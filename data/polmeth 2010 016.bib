@article{ WOS:000281084600003,
Author = {Gutierrez Sanin, Francisco and Giustozzi, Antonio},
Title = {Networks and Armies: Structuring Rebellion in Colombia and Afghanistan},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2010},
Volume = {33},
Number = {9},
Pages = {836-853},
Abstract = {Until recently, the importance of organizational factors in the
   understanding of the variance of the behavior of state challengers had
   not been recognized. New studies and theories have underscored its
   crucial character. This article contends that challengers can be placed
