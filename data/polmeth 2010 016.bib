@article{ WOS:000409440800003,
Author = {Ehrlich, Sean D.},
Title = {Who Supports Compensation? Individual Preferences for Trade-Related
   Unemployment Insurance},
Journal = {BUSINESS AND POLITICS},
Year = {2010},
Volume = {12},
Number = {1},
Month = {APR},
Abstract = {The political economy of trade literature argues that the policy of
   compensating those who lose from trade is an important component of
   maintaining public support for free-trade, a linkage known as the
   compensation hypothesis or embedded liberalism thesis. This article
   tests the causal mechanisms underlying the compensation hypothesis by
   examining support for trade-related compensation using survey data from
   the United States. Expectations about the effects of trade strongly
   predict support for trade-related unemployment insurance, with those who
   expect to lose more likely to support and those who expect to gain more
   like to oppose, but has no influence on support for general unemployment
   insurance despite previous research suggesting it should.},
