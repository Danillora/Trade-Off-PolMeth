@article{ WOS:000285404800008,
Author = {Ng, Kai Hon},
Title = {Capital in Hong Kong: an overstated face of power?},
Journal = {POLICY AND POLITICS},
Year = {2010},
Volume = {38},
Number = {4},
Pages = {619-637},
Month = {OCT},
Abstract = {Discussions of policy networks are common in Britain and Europe.
   Possibly reflecting the elitist view of politics, these discussions
   stress stability. In a similar vein, policy studies in Hong Kong have
   been carried out within the confines of elitism, with a particular
   emphasis on government/business collusion. This article, however, argues
   that networks and policies do change, and the change does not
   necessarily favour business. In explaining change, exogenous factors are
   important, so is the type of the network involved. The impact of
   exogenous factors depends on the structure of the network, which affects
   the network's ability to resist external pressures.},
