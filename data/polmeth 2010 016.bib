@article{ WOS:000284412300001,
Author = {Davies, Sara E.},
Title = {Reproductive Health as a Human Right: A Matter of Access or Provision?},
Journal = {JOURNAL OF HUMAN RIGHTS},
Year = {2010},
Volume = {9},
Number = {4},
Pages = {387-408},
Abstract = {This article seeks to understand why, despite over three decades of
   claiming women's reproductive health as a human right, we have seen
   little progress in reducing their health inequalities and poor health
   outcomes. I argue that one reason for this lack of progress may be due
   to a failure to clearly articulate the responsibilities of key actors,
   crucially states, in ensuring that women have access to, and provision
   of, services required to realize their reproductive rights. What is
   needed, this article suggests, is a framework that can translate decades
   of rights language into action and specifically identify the provisions
   required to address women's health.},
