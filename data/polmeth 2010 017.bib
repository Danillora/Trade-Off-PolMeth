@article{ WOS:000285316600006,
Author = {Alessandro, Martin and Gilio, Andres},
Title = {When Origen Matters: Presidents and Congresses in the Creation of
   Decentralized Organisms in Argentina (1983-2009)},
Journal = {REVISTA DEL CLAD REFORMA Y DEMOCRACIA},
Year = {2010},
Number = {48},
Pages = {149+},
Month = {OCT},
Abstract = {Presidential studies in the United States have shown that Presidents
   intend to design public agencies in such way as to hold an important
   control over its performance. This article empirically tests this
   proposition with the case of Argentina between 1983 and 2009. For that
   purpose, it has built an index that measures the effective levels of
   autarchy in the institutional design of the decentralized agencies
   created since 1983, comparing those established by congressional law
   with those created by presidential decree.
   The evidence greatly supports the hypothesis. Moreover, data suggest
   that, contrary to certain insights, the periods of divided government
   are prone to the establishment of agencies with low levels of autarchy,
   because such periods foster their creation through presidential decree.},
