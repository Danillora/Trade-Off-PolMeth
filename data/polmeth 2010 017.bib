@article{ WOS:000281676800002,
Author = {Durand, Jorge},
Title = {The Peruvian Diaspora Portrait of a Migratory Process},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2010},
Volume = {37},
Number = {5},
Pages = {12-28},
Month = {SEP},
Abstract = {Since the 1980s and especially the 1990s, Peru has become a nation of
   emigrants. Emigration has become massive over the past two decades, and
   the Peruvian populations of the United States, Japan, and Spain have
   tripled in less than a decade. A survey of households in five
   localities, three urban and two rural, in and around Lima helps to
   reveal the special character of this emigration. It tends to involve
   older and better-educated individuals than are typical of international
   migration and to target a wider variety of destinations. Moreover, it is
   a multiclass phenomenon. The economic, political, and social crisis
   brought about by a change in the economic model, two decades of
   terrorism, and a succession of failed democratic administrations has
   affected the society as a whole, and international migration seems to
   operate as an escape valve.},
