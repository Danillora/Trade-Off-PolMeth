@article{ WOS:000284412300002,
Author = {Graubart, Jonathan},
Title = {Rendering Global Criminal Law an Instrument of Power: Pragmatic Legalism
   and Global Tribunals},
Journal = {JOURNAL OF HUMAN RIGHTS},
Year = {2010},
Volume = {9},
Number = {4},
Pages = {409-426},
Abstract = {Although supporters of global tribunals are often depicted as ojudicial
   romantics,o they generally agree that the legal process should adapt to
   national interests and power politics. Hence, they defend a historical
   pattern of selective justice whereby influential states have shaped the
   legal process for political ends and the trials have targeted
   individuals from regimes defeated in war or otherwise out of favor with
   influential states. I label this perspective pragmatic legalism. It
   holds that adapting the legal process to the political concerns of great
   powers and influential local actors is necessary for enabling a
   desirable legal order to take shape. This article argues that rather
   than rescue legalism from idealistic irrelevancy, the pragmatic-legalist
   approach endorses a global legal order crippled by political
   distortions. To develop this critique, I challenge the
   pragmatic-legalist defense of the post-WWII tribunalswhich established
   the enterpriseand the ongoing International Criminal Tribunal for
   Rwandathat has further legalized the process and relocated it to the
   global periphery. I show a consistent tendency to romanticize the
   benefits of legalization and to minimize the harms of political
   manipulations. I maintain that the tribunals have legitimated and
   institutionalized a corrupted version of global criminal law. I conclude
   that a critical reevaluation is in order of global tribunals and of the
   pragmatic-legalist perspective.},
