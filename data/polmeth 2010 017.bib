@article{ WOS:000436083600003,
Author = {Dominguez, Jorge I.},
Title = {President Barack Obama's Foreign Policy toward Latin America},
Journal = {FORO INTERNACIONAL},
Year = {2010},
Volume = {50},
Number = {2},
Pages = {243+},
Month = {APR-JUN},
Abstract = {During Ronald Reagan's, George H. W. Bush's and William Clinton's terms
   of office a particular policy of State toward Latin America took shape
   in the United States. It was interrupted under George W. Bush and
   reappears with Barack Obama. Bearing in mind that the concept of policy
   of State is empirical, we can see the duration and reappearance of the
   fundamental traits of the United States government's behavior toward
   Latin America under different presidents from both political parties.
   Eight topics are studied: protection of human rights and defense of
   democracy; promotion of free trade; benevolent policy toward legal
   immigration; strengthening multilateral agencies; pacification in
   Central America; readiness to resort to the use of military force and
   other coercive measures; simultaneous cooperation and conflict with
   Cuba; and militarization of drug trafficking policy.},
