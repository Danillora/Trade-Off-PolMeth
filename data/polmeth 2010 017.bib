@article{ WOS:000421266900003,
Author = {Chillaud, Matthieu},
Title = {The improbable obituary of the Treaty on the Armed Forces in Europe},
Journal = {ETUDES INTERNATIONALES},
Year = {2010},
Volume = {41},
Number = {3},
Pages = {361-382},
Abstract = {Sometimes described as being an anachronistic residue of the Cold War,
   sometimes acclaimed in its quality of cornerstone of the European
   security, the Treaty on Armed Forces in Europe (CFE, Treaty on
   Conventional Forces in Europe), has for feature to have survived to a
   set of events which logically would have been to precipitate its end.
   All the more so as, for some commentators of the European security, the
   recent Russian decision to implement a moratorium on the treaty and
   especially the conflict in Georgia have assuredly sealed its end. For
   all that, its disappearance is not definite. As a matter of fact, the
   implementation of the treaty, because it includes a strong dimension
   linked to confidence-building, is an undeniable variable of the nature
   of the relations between the West and the East.},
