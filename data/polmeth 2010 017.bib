@article{ WOS:000280634900005,
Author = {Szarka, Joseph},
Title = {Bringing interests back in: using coalition theories to explain European
   wind power policies},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2010},
Volume = {17},
Number = {6},
Pages = {836-853},
Abstract = {Coalition theories have made a valuable contribution to understanding
   public policy but only partially explain policy change, especially in
   new policy subsystems. To improve empirical explanations and contribute
   to theory-building, this article identifies forms of coalition behaviour
   by drawing on the theoretical literature, applies coalition theory to
   the wind power sector and links coalition preferences with choices of
   policy instrument made by policy-makers. It provides support for the
   `advocacy coalition framework', but argues that its explanatory power is
   increased by bringing both self-interest and the public interest back
   into the analytical frame.},
