@article{ WOS:000276367800007,
Author = {Benmelech, Efraim and Berrebi, Claude and Klor, Esteban F.},
Title = {The Economic Cost of Harboring Terrorism},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2010},
Volume = {54},
Number = {2, SI},
Pages = {331-353},
Month = {APR},
Abstract = {The literature on conflict and terrorism has paid little attention to
   the economic costs of terrorism for the perpetrators. This article aims
   to fill that gap by examining the economic costs of harboring suicide
   terror attacks. Using data covering the universe of Palestinian suicide
   terrorists during the second Palestinian uprising, combined with data
   from the Palestinian Labor Force Survey, the authors identify and
   quantify the impact of a successful attack on unemployment and wages.
   They find robust evidence that terror attacks have important economic
   costs. The results suggest that a successful attack causes an increase
   of 5.3 percent in unemployment, increases the likelihood that the
   district's average wages fall in the quarter following an attack by more
   than 20.0 percent, and reduces the number of Palestinians working in
   Israel by 6.7 percent relative to its mean. Importantly, these effects
   are persistent and last for at least six months after the attack.},
