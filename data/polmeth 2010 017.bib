@article{ WOS:000285316600007,
Author = {Falcao Martins, Humberto and Bigonha Salgado, Valeria Alpino and Graef,
   Aldino},
Title = {Forms of Partnership between the Public Power and Organized Civil
   Society in Brazil},
Journal = {REVISTA DEL CLAD REFORMA Y DEMOCRACIA},
Year = {2010},
Number = {48},
Pages = {175+},
Month = {OCT},
Abstract = {This paper presents data from a national survey on specific partnership
   models between the public sector and nonprofits, namely Autonomous
   Social Work organizations (ASW), Social Organizations (SO) and Public
   Interest Nongovernmental Organizations (PINO). The research was
   conducted between November 2008 and February 2009, and aimed at raising
   census information on the adoption of such partnership models at the
   federal, state and municipal levels as well as identifying the legal and
   contractual arrangements underlying the partnership experiences.
   The study was divided into two phases: the first one comprises a
   documental survey and field research; the second focused on analysis and
   information crossing. At federal government level, contacts were made
   with public organizations that maintain relationships with and/or are in
   charge of supervising each of the three partnership models studied. In
   the case of the Public Interest Nongovernmental Organizations, the
   Ministry of Justice; in the case of Social Organizations, partner public
   organizations; and in the case of Autonomous Social Work organizations,
   the Congress and the Federal Court of Audit. At state level, contacts
   were made with the state secretaries responsible for the management of
   the partnership models and for the signing of management contracts and
   partnership terms with SO and PINO; the State Assemblies, which approve
   and publish legislation on partnerships; and the Audit Offices and the
   State Attorneys General Offices, to which the partner nonprofits are
   accountable. At municipal level, contacts were made with city halls from
   2.719 Brazilian municipalities, equivalent to 49\% of all the country.
   That includes all of the 26 state capitals and the 2.520 largest
   municipalities, covering 90\% of the country's population and 95\% of
   its GDP.
   The text is structured in six parts. Part one provides an introductory
   contextualization to the Brazilian third sector. In the second part,
   considerations are made about the scope of the research, the methodology
   and its application. The third part presents a brief characterization of
   the objects, the three models of partnerships: ASW, SO and PINO. The
   fourth part shows the survey results, revealing the number of legal
   bills, eligible organizations and partnerships of each model per
   government level. The fifth part draws a comparative analysis of the
   federal, state and municipal legislation on ASW, SO and PINO. The sixth
   part constitutes the final considerations on the results of the survey,
   the descriptive data and a comparative analysis of the information
   collected on the ASW, SO and PINO.},
Publisher = {CLAD-LATINOAMERICANO ADMINISTRACION DESARROLLO},
Address = {CALLE HERRERA TORO, QTA CLAD, SECTOR LOS NARANJOS, LAS MERCEDES APARTADO
   4182, CARACAS, 1010-A, VENEZUELA},
