@article{ WOS:000281657400013,
Author = {Grigorescu, Alexandru},
Title = {The Spread of Bureaucratic Oversight Mechanisms across Intergovernmental
   Organizations},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
