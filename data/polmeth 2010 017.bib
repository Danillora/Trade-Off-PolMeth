@article{ WOS:000274399900002,
Author = {Xanthaki, Alexandra},
Title = {Multiculturalism and International Law: Discussing Universal Standards},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2010},
Volume = {32},
Number = {1},
Pages = {21-48},
Month = {FEB},
Abstract = {This article aims to assess the contribution of current international
   human rights law to the multicultural debate. The article argues that
   although international law has not engaged in a sustained way with the
   concept, the basic elements of multiculturalism are in fact promoted by
   current standards Among these discussed are the recognition of cultural
   attachments in the public sphere, the need for interaction among
   cultures, and the understanding of sub-national groups as equal partners
   in the evolution of the society A closer look at the standards and their
   dynamic interpretation by UN bodies also reveals helpful answers to
   difficult challenges currently posed by multiculturalism, including
   extremism and clashes between cultural practices and other human rights.},
