@article{ WOS:000285955300003,
Author = {Neumann, Iver B.},
Title = {Early polities: Extant literature and a nutshell case study (Rus', ca.
   800-1100)},
Journal = {INTERNASJONAL POLITIKK},
Year = {2010},
Volume = {68},
Number = {4},
Pages = {571+},
Abstract = {The article offers an overview of the literature on early polities
   (often known as early or archaic states). The rationale for its
   publication has to do with the dearth of interest in and work on the
   issue in Norwegian scholarly debates generally, and within the social
   sciences specifically. By way of a nutshell case study of early Rus', I
   argue that we need to address what happened in the Eurasian steppe if we
   are to understand early polities in what we now call Europe.
   Understanding early polities will help our understanding of state
   formation as a general phenomenon. The article is a also plug for
   undertaking these studies by focusing on the relations between the
   coterminous polities in question rather than by using the traditional
   inside-out focus.},
