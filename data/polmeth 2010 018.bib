@article{ WOS:000283319300002,
Author = {Scott-Cato, Molly and Hillier, Jean},
Title = {How could we study climate-related social innovation? Applying Deleuzean
   philosophy to Transition Towns},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2010},
Volume = {19},
Number = {6},
Pages = {869-887},
Abstract = {The contribution that the philosophy of Gilles Deleuze might offer to
   researchers studying social innovation in response to climate change is
   explored. Since the publication of the Stern report, it has been
   recognised that climate change requires major changes in the way our
   economy is organised, but it also requires significant social and
   behavioural change. Can this be usefully viewed through the prism of
   theories of social innovation? How might such social innovation affect
   the life chances of the socially excluded, and to what extent does it,
   therefore, offer a space for radical social change? Transition Towns - a
   community movement in response to climate change - is used as a
   test-case of these ideas.},
