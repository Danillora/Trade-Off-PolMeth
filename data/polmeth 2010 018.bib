@article{ WOS:000284179700005,
Author = {Reid, Julian},
Title = {Of nomadic unities: Gilles Deleuze on the nature of sovereignty},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
