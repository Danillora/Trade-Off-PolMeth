@article{ WOS:000277541400006,
Author = {Young, Charles S.},
Title = {POWs: The Hidden Reason for Forgetting Korea},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2010},
Volume = {33},
Number = {2},
Pages = {317-332},
Abstract = {Despite recent attention, the Korean War (1950-53) remains
   underappreciated and further explanation of its `forgottenness' is
   needed. Korea originally faded to gray because there was no satisfying
   victory. National security planners had ample reason to try and
   rehabilitate the war in the mind of the public, but had little success.
   This was because a primary accomplishment of the second half of the
   war-forcing the enemy to accept so-called voluntary repatriation of POWs
   (prisoners of war)-was semi-secret and never declared as a major war
   aim. Since the nation was never united around securing voluntary
   repatriation, there was little raw material for creating a memory of
   success.},
