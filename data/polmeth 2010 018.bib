@article{ WOS:000290901100007,
Author = {Obi, Cyril},
Title = {Oil as the `curse' of conflict in Africa: peering through the smoke and
   mirrors},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2010},
Volume = {37},
Number = {126},
Pages = {483-495},
Abstract = {This article interrogates the framing of the resource curse as a central
   causal mechanism in the resource abundance-conflict nexus in Africa. It
   is argued that explaining such conflicts on the basis of the ways
   natural resources either act as an incentive/motive for rebel groups, or
   erode and weaken states, does not adequately capture the complex
   histories, dimensions and transnational linkages to civil conflict in
   Africa. The article lays bare the attempts by a hegemonic discourse to
   obfuscate the reality of the fundamental and transnational underpinnings
   of the resource-conflict nexus. It is argued that the resource curse
   perspective cannot fully explain conflict in African oil states, and
   rather, a case is made for an alternative model based on radical
   political economy which lays bare the class relations, contradictions
   and conflicts rooted in the subordination of the continent and its
   resources to transnational processes and elites embedded in globalised
   capitalist relations.},
