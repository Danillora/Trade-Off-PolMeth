@article{ WOS:000291002400005,
Author = {Sargeant, Malcolm},
Title = {The UK national minimum wage and age discrimination},
Journal = {POLICY STUDIES},
Year = {2010},
Volume = {31},
Number = {3},
Pages = {351-364},
Abstract = {The national minimum wage in the UK took effect from 1 April 1999. From
