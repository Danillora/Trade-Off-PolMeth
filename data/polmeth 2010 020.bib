@article{ WOS:000279123600006,
Author = {Bassoli, Matteo},
Title = {Local Governance Arrangements and Democratic Outcomes (with Some
   Evidence from the Italian Case)},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2010},
Volume = {23},
Number = {3},
Pages = {485-508},
Month = {JUL},
Abstract = {This article carries a broad definition of public-private concertation
   as a flexible form of governance that is able to overcome the
   distinction between network governance and participatory governance. It
   creates a unified framework relying on a process-related democratic
   approach in order to properly assess these practices in their democratic
   outcomes. This allows the article to fully depict how local governance
   arrangements affect local democracy, both positively and negatively.
   Drawing on four exemplary cases of urban policies developed in the
   Province of Milan, it highlights two variables that give account of the
   democratic implication of these practices: the role of the political
   leadership and the inclusive strategy that they adopt. Moreover, it
   underlines four additional factors that help more democratic outcomes:
   presence of cognitive justification, quality of participation, active
   opposition, and an outward communication.},
