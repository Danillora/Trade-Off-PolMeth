@article{ WOS:000277571100003,
Author = {Bennett, Huw},
Title = {From Direct Rule to Motorman: Adjusting British Military Strategy for
   Northern Ireland in 1972},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2010},
Volume = {33},
Number = {6},
Pages = {511-532},
Abstract = {The British campaign in Northern Ireland in the early 1970s is often
   portrayed as consistent in its repressive character and its failure to
   successfully relate military means to political ends. This article
   argues that British military strategy was adaptable, alternating between
   defensive and offensive means depending on the changing political
   context. The low profile policy allowed the army to consolidate a firm
   basis for later offensive operations against the Provisional Irish
   Republican Army (PIRA). This proved successful because it contrasted
   with Republican violence and was contextualized within the government's
   willingness to negotiate and compromise when necessary.},
