@article{ WOS:000277541400003,
Author = {Barnes, Robert},
Title = {Branding an Aggressor: The Commonwealth, the United Nations and Chinese
   Intervention in the Korean War, November 1950-January 1951},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2010},
Volume = {33},
Number = {2},
Pages = {231-253},
Abstract = {The crisis following China's intervention in the Korean War led to a
   significant rift between the United States and the Commonwealth at the
   United Nations (UN). This article examines the conditions under which
   the Commonwealth became united and was able to directly influence UN
   decision-making. It concludes that, when united, the Commonwealth could
   not easily be ignored by Washington, and thereby acted as an agent of
   constraint upon the Western superpower.},
