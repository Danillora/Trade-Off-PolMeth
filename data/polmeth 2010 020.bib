@article{ WOS:000279846600003,
Author = {Rudner, Martin},
Title = {Hizbullah Terrorism Finance: Fund-Raising and Money-Laundering},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2010},
Volume = {33},
Number = {8},
Pages = {700-715},
Abstract = {Hizbullah, which has been designated a terrorist entity in several
   international jurisdictions, depends upon a substantial mobilization of
   financial resources to support its complex, multi-faceted organizational
   apparatus, its domestic activities in Lebanon, and its far-reaching
   transnational operations. This study surveys Hizbullah's resourcing
   requirements, and examines the role of military assistance from Iran and
   Syria along with Hizbullah's own reliance on front organizations,
   clandestine networks, business enterprises, propaganda media, and local
   exactions to generate funding for operational activities. Particular
   attention is directed at the diversion of charitable contributions, the
   exploitation of non-governmental organizations (NGOs), involvement in
   criminal activities and contraband trade, and the money laundering
   mechanisms utilized to transfer funds to where they are required. It
   also reviews efforts by international authorities to staunch the flow of
   financial resources to terror groups, activities and operations,
   including their effect on Hizbullah terrorism finance.},
