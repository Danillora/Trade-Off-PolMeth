@article{ WOS:000279195400001,
Author = {Miller, David and Mills, Tom},
Title = {Counterinsurgency and terror expertise: the integration of social
   scientists into the war effort},
Journal = {CAMBRIDGE REVIEW OF INTERNATIONAL AFFAIRS},
Year = {2010},
Volume = {23},
Number = {2},
Pages = {203-221},
Abstract = {In this paper the authors draw upon the tradition of Power Structure
   Research to analyse the increased interpenetration of the military and
   the social sciences, particularly the recruitment of anthropologists and
   the adoption and adaptation of counterinsurgency strategies. It is
   argued that such actors should be understood not as disinterested
   `experts' but as being organically embedded in a
   military-industrial-academic complex. The paper considers a number of
   contemporary examples as well as considering the historical roots of
   these trends. It is argued that this interpenetration violates the
   ethical norms of the academy and the moral and social responsibilities
   of intellectuals.},
