@article{ WOS:000208580500002,
Author = {Agne, Hans},
Title = {Why democracy must be global: self-founding and democratic intervention},
Journal = {INTERNATIONAL THEORY},
Year = {2010},
Volume = {2},
Number = {3},
Pages = {381-409},
Month = {NOV},
Abstract = {Globalization, foreign intervention, and failed states have drawn new
   attention to theoretical issues of how political orders and communities
   can be legitimately founded, and what it means for a people to be
   self-governing. In this article, I will challenge an argument in this
   debate saying that the founding of new political orders is always in
   some sense illegitimate insofar as it cannot be decided democratically.
   In opposition to this view, I will suggest that the founding of
   political orders is legitimate even from a democratic point of view when
   decided together by people within as well as beyond the boundaries
   inherent in the foundation. In case of persisting disagreement over
   boundary issues, political decisions can still derive democratic
   legitimacy from global procedures that are equally inclusive of everyone
   capable of contesting those decisions. Elaborating on the implications
   of this argument, I will also reject the notion that foreign
   interventions for establishing democracy are themselves necessarily
   illegitimate or undemocratic.},
