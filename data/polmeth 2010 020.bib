@article{ WOS:000294253100004,
Author = {Zubek, Radoslaw and Stecker, Christian},
Title = {Legislatures and Policy Uncertainty: Evidence from East Central Europe},
Journal = {JOURNAL OF PUBLIC POLICY},
Year = {2010},
Volume = {30},
Number = {1, SI},
Pages = {63-80},
Month = {APR},
Abstract = {Relying on social choice theory, this paper argues that uncertainty
   regarding future public policies is likely to be related to party
   institutionalization and legislative organization. The argument is
   evaluated using survey data from businesses in eight EU member states in
   East Central Europe. It finds that firms report lower concern over
   policy uncertainty in systems with higher party institutionalization.
   There is also some evidence, although less robust, that restrictive
   parliamentary agenda control leads to lower perceptions of policy
   uncertainty and this effect mediates the influence of party
   institutionalization. These results tend to hold if one controls for the
   effect of other national and firm-level factors.},
