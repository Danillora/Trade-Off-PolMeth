@article{ WOS:000284538200004,
Author = {Boyd, Dallas and Scouras, James},
Title = {The Dark Matter of Terrorism},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2010},
Volume = {33},
Number = {12},
Pages = {1124-1139},
Abstract = {The consequences of states' reactions to terrorist attacks can far
   outweigh the effects of the attacks themselves. Yet risk analysis,
   widely accepted as the proper analytical basis for assessing terrorist
   threats, largely ignores the consequences from potential reactions to
   attacks. It also generally fails to consider how the anticipation of
   these reactions may influence terrorists' attack preferences.
   Policymakers should therefore be wary of the limitations of risk
   analysis as currently practiced. Further, states should strive to avoid
   reactions to attacks that unwittingly further their adversaries'
   agendas, a goal that may be aided by strengthening public resilience to
   terrorism.},
