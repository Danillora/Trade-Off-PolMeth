@article{ WOS:000276468700006,
Author = {O'Kane, Eamonn},
Title = {Learning from Northern Ireland? The Uses and Abuses of the Irish `Model'},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
Year = {2010},
Volume = {12},
Number = {2},
Pages = {239-256},
Month = {MAY},
Abstract = {This article examines the increasing use of Northern Ireland as a
   `model' for resolving conflicts. It seeks to identify what may
   constitute an Irish `model' and to examine it in relation to three key
   aspects of existing conflict resolution theory: the timings of peace
   processes, the role of third parties and how to deal with `spoilers'.
   The article argues that the existing theory is currently ill-matched to
   both the Northern Ireland case and the `model' that some have sought to
   extrapolate from it. It stresses the need to examine the Northern
   Ireland case in context if we are to learn any lessons from it that may
   be of use in other conflict resolution attempts.},
