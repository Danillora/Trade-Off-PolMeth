@article{ WOS:000276483200004,
Author = {Protsyk, Oleh and Matichescu, Lupsa Marius},
Title = {Electoral rules and minority representation in Romania},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2010},
