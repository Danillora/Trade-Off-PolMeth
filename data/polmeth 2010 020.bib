@article{ WOS:000279532200004,
Author = {Tsolakis, Andreas},
Title = {Opening Up Open Marxist Theories of the State: A Historical Materialist
   Critique},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
