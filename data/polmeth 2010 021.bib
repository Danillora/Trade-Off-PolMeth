@article{ WOS:000285008100010,
Author = {Shannon, Megan and Morey, Daniel and Boehmke, Frederick J.},
Title = {The Influence of International Organizations on Militarized Dispute
   Initiation and Duration},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
Year = {2010},
Volume = {54},
Number = {4},
Pages = {1123-1141},
Month = {DEC},
Abstract = {We argue that international organizations decrease the duration of
   international conflicts by mitigating commitment problems and
   encouraging combatants to cease hostilities more quickly. Empirical
   analyses of militarized interstate dispute duration (1950-2000) reveal
   that increasing shared international organization (IO) participation
   reduces the length of disputes, even after accounting for selection into
   international conflict. We also find that international organizations
   designed to mitigate commitment problems decrease dispute duration,
   while IOs capable of reducing information asymmetries do not influence
   dispute length.},
