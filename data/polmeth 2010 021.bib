@article{ WOS:000276101700005,
Author = {Ryland, Elisabeth},
Title = {Danish wind power policy: domestic and international forces},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2010},
Volume = {19},
Number = {1},
Pages = {80-85},
Abstract = {Denmark is a windy, low-lying country with a very long coastline where
   global warming, rising sea levels and renewable energy are serious
   national issues. Renewable energy in Denmark is primarily wind power, a
   traditional and popular energy source. Until 2001, Danish energy policy
   was largely synonymous with environmental policy, based on a broad
   popular and political consensus. A European Commission survey published
   in January 2007 showed 93\% of the population supported renewable energy
   sources (EWEA News Release 2007). However, political developments since
   2001 have created a crisis for Danish wind power (see Toke 2002). This
   period has also seen greater interaction between domestic and
   international forces.},
