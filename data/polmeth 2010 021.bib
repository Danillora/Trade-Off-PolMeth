@article{ WOS:000211364500004,
Author = {Wu, Chengqiu},
Title = {Sovereignty, Human Rights, and Responsibility: Changes in China's
   Response to International Humanitarian Crises},
Journal = {JOURNAL OF CHINESE POLITICAL SCIENCE},
