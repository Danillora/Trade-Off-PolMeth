@article{ WOS:000421259700004,
Author = {Dumoulin, Andre and Wasinski, Christophe},
Title = {Justifying nuclear weapons: The French case during the 1990s},
Journal = {ETUDES INTERNATIONALES},
Year = {2010},
Volume = {41},
Number = {1},
Pages = {79-96},
Abstract = {The possession of nuclear weapons is not to be considered as an
   irreversible phenomenon. Taking this idea as a point of departure, we
   have decided to investigate not why France has acquired nuclear weapons
   but instead the reason for which she retains them. More specifically,
   our attention was directed to the years 1990-2000. During those years, a
   relatively important nuclear strategic debate occurred. It was an
   opportunity for strategists to reaffirm how useful the weapon is. Trough
   this debate, strategists described an uncertain international context
   that they consider unfit to question the relevance of the nuclear
   armament.},
Publisher = {INST QUEBECOIS HAUTES ETUDES INT},
Address = {UNIV LAVAL, PAVILLON KONINCK, QUEBEC, QC G1V 0A6, CANADA},
Type = {Article},
Language = {French},
