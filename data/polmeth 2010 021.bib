@article{ WOS:000277571000003,
Author = {Pokalova, Elena},
Title = {Framing Separatism as Terrorism: Lessons from Kosovo},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2010},
Volume = {33},
Number = {5},
Pages = {429-447},
Abstract = {Framing separatism as terrorism presents numerous opportunities for
   governments facing ethno-nationalist challenges. Namely, such framing
   allows states to avoid addressing the ethno-nationalist roots of
   separatist conflicts. This article analyzes incentives and opportunities
   that terrorism presents for states involved in ethno-nationalist
   separatist conflicts. The article investigates how the framing dynamics
   proved to be successful in the cases of the Kurdish and Chechen
   separatist conflicts. The case of Kosovo is examined as illustrative of
   a different outcome: the international presence in the Kosovo conflict
   made it substantially different from the previous cases and served as an
   intervention in the Serbian framing dynamics.},
