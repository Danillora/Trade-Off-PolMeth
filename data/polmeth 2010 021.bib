@article{ WOS:000282377500007,
Author = {Mondou, Matthieu and Montpetit, Eric},
Title = {Policy Styles and Degenerative Politics: Poverty Policy Designs in
   Newfoundland and Quebec},
Journal = {POLICY STUDIES JOURNAL},
Year = {2010},
Volume = {38},
Number = {4},
Pages = {703-722},
Abstract = {This article examines the proposal suggesting that policy designs are
   consistent with the social construction of target groups. Associated
   with policy design theory, the proposal pessimistically suggests that
   underprivileged citizens will be targeted with policies that do little
   to help them, creating a vicious circle of degenerative politics. This
   article argues that the prevalence of degenerative politics depends on
   policy styles. Significant where the adversarial style prevails,
   degenerative politics is less common in consensual systems. This
   proposal is examined through a systematic content analysis of action
   plans to reduce poverty in Newfoundland and Quebec.},
