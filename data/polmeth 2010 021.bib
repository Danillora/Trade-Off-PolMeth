@article{ WOS:000285551000005,
Author = {Hoffman, Aaron M. and Jengelley, Dwaine H. A. and Duncan, Natasha T. and
   Buehler, Melissa and Rees, Meredith L.},
Title = {How Does the Business of News Influence Terrorism Coverage? Evidence
   From The Washington Post and USA Today},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2010},
Volume = {22},
Number = {4},
Pages = {559-580},
Abstract = {Observers argue that the business of news is responsible for worsening
   the coverage of terrorism, but exactly how economic considerations
   influence coverage is controversial. Based on an analysis of over 1600
   articles from The Washington Post and USA Today, we find that concerns
   about the effects of economic pressures on terrorism reporting are
   exaggerated. The business climate affects coverage, but the long-term
   impact of market pressures on coverage is modest. Articles about
   terrorist violence are increasingly prominent, but coverage of
   counterterrorism remains robust. Efforts to inoculate the press against
   the worst excesses of the market are unnecessary.},
