@article{ WOS:000285637500002,
Author = {Harris, Albert},
Title = {Separatist insurgency, objective referents and autonomy},
Journal = {COOPERATION AND CONFLICT},
Year = {2010},
Volume = {45},
Number = {4},
Pages = {387-405},
Month = {DEC},
Abstract = {In certain separatist conflicts there is a greater likelihood of
   external mediation if the political `redefinition' of the state insisted
   upon by the insurgents undergoes a revision, from secession to
   self-determination, understood as a variant of autonomy. In the same
   vein, although it may not happen concurrently, insurgent movements
   become more amenable to external mediation if and when opposing
   governments revise the preferred conflict outcome from a military defeat
   of the insurgents to a `containment' of the movement. These two
   developments - a revised demand from the insurgents for how the state
   should be defined and an altered military strategy adopted by the
   government - can serve as `objective referents' helping external parties
   to identify a ripe moment in the conflict and initiate mediation.},
