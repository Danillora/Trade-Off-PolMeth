@article{ WOS:000435270700005,
Author = {Meredith, III, Spencer B.},
Title = {Toward a Just Peace after the Georgian Civil War},
Journal = {EAST EUROPEAN POLITICS},
Year = {2010},
Volume = {26},
Number = {3},
Pages = {413-434},
Abstract = {The Georgian civil war may be at an end since Russia recognized Abkhazia
   and South Ossetia as independent states, even though no other country
   has followed suit except Nicaragua. Whether or not the current cessation
   of violence leads to peace depends on how these `new' countries are
   treated by the international community. Peace in the region will require
   a radical shift in foreign aid policies, specifically an end to all
   assistance to the separatists, negotiations with Moscow to pick up the
   slack, and Russian compensation made to the nearly half-million Georgian
   `refugees' for their lost property.},
