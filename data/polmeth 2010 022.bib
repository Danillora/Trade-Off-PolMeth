@article{ WOS:000274412900008,
Author = {McLean, Iain},
Title = {Political Science and History: Friends and Neighbours},
Journal = {POLITICAL STUDIES},
Year = {2010},
Volume = {58},
Number = {2},
Pages = {354-367},
Month = {MAR},
Abstract = {Against Anne Norton, it is argued that political science and history are
   good neighbours. Instances are given to show that the two disciplines
   need each other, both in political thought and in comparative
   government. Political scientists need historians to help understand
