@article{ WOS:000409442200001,
Author = {Oatley, Thomas},
Title = {Real Exchange Rates and Trade Protectionism},
Journal = {BUSINESS AND POLITICS},
Year = {2010},
Volume = {12},
Number = {2},
Month = {AUG},
Abstract = {Real exchange rate movements are robustly related to the rise and fall
   of trade protectionism. I demonstrate this by presenting a theoretical
   model that incorporates the real exchange rate into a standard factor
   proportions model of trade policy preferences. The model demonstrates
   why some firms' trade policy preferences, and thus total demands for
   protectionism, change in response to real exchange rate movements. I
   evaluate the model with data on antidumping investigations in six
   industrialized countries between the late 1970s and 2004. The exercise
   suggests that the real exchange rate hypothesis offers a more compelling
   explanation for protectionist waves than the business cycle hypothesis.},
