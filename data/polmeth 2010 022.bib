@article{ WOS:000279123600003,
Author = {Came, Timothy and Campbell, Colin},
Title = {The Dynamics of Top-Down Organizational Change: Donald Rumsfeld's
   Campaign to Transform the US Defense Department},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
