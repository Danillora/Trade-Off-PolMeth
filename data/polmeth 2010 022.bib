@article{ WOS:000279532200006,
Author = {McGrattan, Cillian},
Title = {Community-Based Restorative Justice in Northern Ireland: A
   Neo-Traditionalist Paradigm?},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
Year = {2010},
Volume = {12},
Number = {3},
Pages = {425-441},
Month = {AUG},
Abstract = {This article critically assesses the scholarly representation of
   community-based restorative justice (CBRJ) schemes in Northern Ireland.
   These schemes, which emerged in working-class areas following the
   republican and loyalist ceasefires of the 1990s, have been the subject
   of intense political debate and a growing body of academic literature. I
