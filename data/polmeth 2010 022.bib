@article{ WOS:000214221300004,
Author = {Czarniawska, Barbara},
Title = {The uses of narratology in social and policy studies},
Journal = {CRITICAL POLICY STUDIES},
Year = {2010},
Volume = {4},
Number = {1},
Pages = {58-76},
Abstract = {This article suggests possible multiple uses of narratology in social
   and policy studies. It starts with a brief history of the narrative turn
   in social sciences. The structure of the remainder of the text follows
   the trajectory of a research project. First there is a suggestion that
   researchers may learn from narratologists how to monitor the
   construction of narratives in the field of social practice, how to look
   for narratives in documents, and how to encourage narratives when
   conducting interviews. Then I look at how narratologists can teach
   social scientists various techniques of close reading, useful in
   analyzing field material. There follows an examination of the multiple
   models of writing that narratology and literature have on offer. The
   article ends with a reflection on the difference between the genres of
   literary and scholarly writing.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
