@article{ WOS:000284394400003,
Author = {Howard-Hassmann, Rhoda E.},
Title = {Mugabe's Zimbabwe, 2000-2009: Massive Human Rights Violations and the
   Failure to Protect},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2010},
Volume = {32},
Number = {4},
Pages = {898-920},
Month = {NOV},
Abstract = {This article reviews human rights violations in Zimbabwe from 2000 to
   2009, under the rule of Robert Mugabe. It argues that these violations,
   including state-induced famine, illegal mass expulsions, and systemic
   rape, constituted crimes against humanity. The article considers what
   African regional organizations, including the African Union and the
   Southern African Development Community, and various organs of the
   international community did, and might have done, to restrain Mugabe and
   his inner circle from committing these violations. It concludes that the
   lack of forceful action by African and international organizations
   constituted a failure to protect the people of Zimbabwe.},
