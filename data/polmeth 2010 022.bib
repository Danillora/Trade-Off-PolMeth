@article{ WOS:000285551000004,
Author = {Turner, John},
Title = {From Cottage Industry to International Organisation: The Evolution of
   Salafi-Jihadism and the Emergence of the Al Qaeda Ideology},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2010},
Volume = {22},
Number = {4},
Pages = {541-558},
Abstract = {Al Qaeda after the invasion of Afghanistan can now be understood as not
   only an international terror organisation but an ideology which inspires
   groups with similar goals of a supranational caliphate. The Al Qaeda
