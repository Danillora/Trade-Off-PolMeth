@article{ WOS:000277736400009,
Author = {Cirincione, Joseph},
Title = {Taking the Field: Obama's Nuclear Reforms},
Journal = {SURVIVAL},
Year = {2010},
Volume = {52},
Number = {2},
Pages = {117-128},
Abstract = {US President Barack Obama's nuclear-security agenda is in trouble. It is
   behind schedule, under-staffed, under attack and battered by some
   less-than-cooperative international partners. Critics of the
   administration have dominated the domestic public debate. But after a
   year of analysis, discussion and speeches, the Obama administration has
   reached internal consensus, lined up its nuclear initiatives, and begun
   organising its congressional supporters. The Obama team is finally ready
   to take the field. The new strategy will roll out in a tight sequence of
   reports, events, hearings and votes over the first half of 2010. The
   overall goal is to transition US nuclear policy from one still based on
   a Cold War strategy of massive arsenals to one suited to prevent, deter
   and defeat the more discrete threats of the twenty-first century.},
