@article{ WOS:000421269100008,
Author = {Piquet, Helene},
Title = {Human Rights in China: A Relativistic Government, Universalist
   Justiciables},
Journal = {ETUDES INTERNATIONALES},
Year = {2010},
Volume = {41},
Number = {4},
Pages = {597-616},
Month = {DEC},
Abstract = {This article discusses the myth of the non litigious Chinese in the
   context of the legal reforms in China since 1978. Whereas the Chinese
   government adheres to cultural relativism, the Chinese people seeking
   justice through the court embrace a universalist vision of human rights.
   The Chinese government also affirms that harmony is the dominant
   paradigm. This would lead litigants to avoid the courts and sacrifice
   their rights in order to preserve harmony. This is substantially
   contradicted by the fact that numerous litigants use the courts as forum
   of conflict resolution. Moreover, many in China advocate a universalist
   approach of human rights. Thus, there is a rupture between official
   representations and practice of various segments of the Chinese
   population.},
