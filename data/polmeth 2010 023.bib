@article{ WOS:000282589400009,
Author = {Hunter, Robert E.},
Title = {Rethinking Iran},
Journal = {SURVIVAL},
Year = {2010},
Volume = {52},
Number = {5},
Pages = {135-156},
Abstract = {In keeping Iran from developing nuclear weapons, the United States
   currently faces five possibilities. Three are unlikely to succeed: Iran
   either cannot produce the bomb or decides for its own reasons not to do
   so; economic sanctions work; or a new regime comes to power and adopts
   radically different foreign policies. Two other possibilities are more
   plausible: either the West accepts an Iranian nuclear weapon and
   deters/contains Iran; or the United States or Israel attacks its nuclear
   facilities. War would be catastrophic; but containment also has
   penalties. Washington should thus reassess core Western and Iranian
   strategic interests; offer security guarantees if Iran credibly abandons
   any pursuit of nuclear weapons and stops other unacceptable behaviour
   (for example support for Hizbullah and Hamas); explore possible
   cooperation with Iran in Iraq and (particularly) Afghanistan; and pursue
   confidence-building measures, such as agreements on incidents at sea,
   freedom of navigation, and counter-piracy cooperation. The United States
   has nothing to lose by trying this approach.},
