@incollection{ WOS:000281457700004,
Author = {Jackson, Mark},
Editor = {Patten, NJ and Nugent, BC},
Title = {LIBERTY, JUSTICE, AND NATIONAL SECURITY IN ADAM SMITH'S POSSIBLE WORLD},
Booktitle = {NATIONAL SECURITY: INSTITUTIONAL APPROACHES, POLICY MODELS AND GLOBAL
   IMPACTS},
Series = {Defense Security and Strategy},
Year = {2010},
Pages = {115-124},
Abstract = {After the conclusion of World War lithe United States adopted the
   national-security strategy of containment. Giving force to this strategy
   is the doctrine of Mutually Assured Destruction. But this
   national-security strategy failed to prevent the attack on the United
   States on September 11, 2001. The failure of this strategy has led
   Barnett {[}2004] to develop and propose a new national-security strategy
   as a successor to containment. His new strategy identifies two new
   problems that have arisen in the national-security sphere, and proposes
   military solutions to each of these problems. Jackson {[}2008] has
   explained why rational policy is also a contribution to the solution of
   the second of these problems, where rational policy is public policy
   that makes Adam Smith's simple system of natural liberty a reality by
   removing barriers to entry in product and resource markets.(1)
   This work identifies an additional contribution that rational policy
   makes to the solution of this second problem. The work explains why
   rational policy produces powerful long-run tendencies toward equality of
   average real wages for homogeneous labor across industries, regions,
   races and sexes, and produces evidence from the United States in support
   of these conclusions. In turn, the work argues that increases in
   equality increase justice. Because rational policy produces ordered
   outcomes that are just, and because ``achieving just and ordered
   connectivity is the main struggle of our age{''} {[}Barnett, 2004, P.
   284], rational policy contributes to the success of this struggle.},
