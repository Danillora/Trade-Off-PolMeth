@article{ WOS:000281716100008,
Author = {Dobson, Andrew},
Title = {Democracy and Nature: Speaking and Listening},
Journal = {POLITICAL STUDIES},
Year = {2010},
Volume = {58},
Number = {4},
Pages = {752-768},
Month = {OCT},
Abstract = {This article is about the nature of democracy in environmental politics,
   with special reference to the issue of representation, and to the issues
   of speaking and listening. It is argued that politics has always been
   regarded as concerned with `speechifying', and that this has created
   problems for politicising environmental issues, many of which have to do
   with `dumb nature'. The work of Bruno Latour is examined in this context
   as a way of including nature in politics without resorting to
   extravagant claims about `nature speaking'. Latour's epistemological
   approach to `politicising nature' is discussed, and attention is focused
   on the importance for the political (and especially democratic) process
   of `listening' as well as `speaking'.},
