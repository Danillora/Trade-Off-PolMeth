@article{ WOS:000280993300008,
Author = {Neyer, Juergen},
Title = {Justice, Not Democracy: Legitimacy in the European Union},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2010},
Volume = {48},
Number = {4},
Pages = {903-921},
Month = {SEP},
Abstract = {The EU is often assessed against the standard of democracy, which it has
   no fair chance to fulfil. A new and attractive normative agenda is
   needed if the EU is to escape its deep legitimacy crisis. This article
   proposes to substitute the discourse on the democratic deficit of the EU
   with a discourse on its contribution to transnational justice. Whilst
   the democratic discourse most often focuses on parliamentary competences
   and divided government, the discourse on justice centres on the people,
   puts primary emphasis on power asymmetries and on overcoming the
   obstacles to justifiable political outcomes. The proposal to analyse the
   EU in terms of justice does not lower the normative standard but
   corrects it.},
