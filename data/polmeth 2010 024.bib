@article{ WOS:000285060200002,
Author = {Childs, Sarah and Webb, Paul and Marthaler, Sally},
Title = {Constituting and Substantively Representing Women: Applying New
   Approaches to a UK Case Study},
Journal = {POLITICS \& GENDER},
