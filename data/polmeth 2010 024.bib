@article{ WOS:000274536300003,
Author = {Whitt, Sam},
Title = {Institutions and Ethnic Trust: Evidence from Bosnia},
Journal = {EUROPE-ASIA STUDIES},
Year = {2010},
Volume = {62},
Number = {2},
Pages = {271-292},
Abstract = {This study considers the prospects for rebuilding trust in a
   multi-ethnic society with a history of ethnic violence. Findings are
   based on survey data from a stratified random sample of 681 Bosnian
   Serbs, Croats and Bosniaks, conducted between September 2003 and January
   2004. The data indicate significant trust problems in Bosnian society
   both within and across ethnicity. However, the study also finds strong
   linkages between ethnic trust and trust in institutions, suggesting that
   institutions can play a vital role in restoring ethnic trust and
   promoting reconciliation.},
