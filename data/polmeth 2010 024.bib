@article{ WOS:000274813500011,
Author = {Cotterill, Sarah and Richardson, Liz},
Title = {Expanding the Use of Experiments on Civic Behavior: Experiments with
   Local Government as a Research Partner},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2010},
Volume = {628},
Pages = {148-164},
Month = {MAR},
Abstract = {Despite the acknowledged benefits of experimental methods and growing
   policy and academic interest in evidence-based policy, few researchers
   have attempted to undertake experiments at a local level in cooperation
   with policymakers and public service providers. This paper discusses the
   challenges of implementing and delivering experiments in these contexts,
   drawing on four successfully completed experiments and two current
   experiments and referring to four proposed experiments that did not take
   place. The completed and ongoing experiments investigated canvassing to
   increase household recycling, Internet deliberation to change civic
   attitudes, education to change children's environmental attitudes,
   community support to improve quality of life for drug users, citizen
   pledging to donate to charity, and letter writing to influence the
   response of councilors to citizen interest groups. The paper concludes
   by assessing the benefits of co-produced field experiments against the
   challenges and argues that, overall, co-production has the potential to
   expand the scope, range, and depth of possible experiments; improve
   access to citizens and to data; and lead to research that is more
   credible to policymakers.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Cotterill, S (Corresponding Author), Univ Manchester, IPEG, Manchester M13 9PL, Lancs, England.
   Cotterill, Sarah; Richardson, Liz, Univ Manchester, IPEG, Manchester M13 9PL, Lancs, England.},
DOI = {10.1177/0002716209351521},
ISSN = {0002-7162},
EISSN = {1552-3349},
Keywords = {civic behavior; field experiments; local government; difficulties of
   implementation},
Keywords-Plus = {PARTICIPATION},
Research-Areas = {Government \& Law; Social Sciences - Other Topics},
Web-of-Science-Categories  = {Political Science; Social Sciences, Interdisciplinary},
