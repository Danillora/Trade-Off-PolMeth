@article{ WOS:000284546900002,
Author = {Thoms, Oskar N. T. and Ron, James and Paris, Roland},
Title = {State-Level Effects of Transitional Justice: What Do We Know?},
Journal = {INTERNATIONAL JOURNAL OF TRANSITIONAL JUSTICE},
Year = {2010},
Volume = {4},
Number = {3, SI},
Pages = {329-354},
Month = {NOV},
Abstract = {At the core of policy debates on the state-level effects of transitional
   justice is a series of competing claims about the causal effects of
   various transitional justice mechanisms. A review of recent scholarship
   on transitional justice shows that empirical evidence of positive or
   negative effects is still insufficient to support strong claims. More
   systematic and comparative analysis of the transitional justice record
   is needed in order to move from `faith-based' to `fact-based'
   discussions of transitional justice impacts.},
