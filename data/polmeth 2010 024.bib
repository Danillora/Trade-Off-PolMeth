@article{ WOS:000214190000005,
Author = {Thomas, Edward},
Title = {Between War and Peace: Monitoring Guidelines for Child Soldiers in
   Nepal's Peace Process},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2010},
Volume = {2},
Number = {1},
Pages = {93-121},
Month = {MAR},
Abstract = {The UN Security Council has set up a wide-ranging monitoring regime for
   child recruitment and use for armed conflict. This is a particularly
   complex task for two reasons: first, international law and international
   policy on children's involvement in armed conflict is inconsistent, and
   its application in countries emerging from conflict is problematic.
   Second, children's involvement is part of a wider social and economic
   order that is being reconfigured by violence - that order, and
   children's responses to it, need to be described and interpreted for
   monitoring to be useful. This note illustrates these problems by
   presenting and assessing monitoring guidelines for child recruitment and
   use in one country emerging from a conflict in which children and youth
   were widely involved: Nepal.},
