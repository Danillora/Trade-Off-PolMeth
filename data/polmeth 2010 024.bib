@article{ WOS:000274813500010,
Author = {Martel Garcia, Fernando and Wantchekon, Leonard},
Title = {Theory, External Validity, and Experimental Inference: Some Conjectures},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2010},
Volume = {628},
Pages = {132-147},
Month = {MAR},
Abstract = {It is often argued that experiments are strong on causal identification
   (internal validity) but weak on generalizability (external validity).
   One widely accepted way to limit threats to external validity is to
   incorporate as much variation in the background conditions and in the
   covariates as possible through replication. Another strategy is to make
   the theoretical foundations of the experiment more explicit. The latter
   requires that we develop trajectories of experiments that are consistent
   with a theoretical argument. In other words, new experiments should not
   simply consist of changing the context of old ones, but do so in ways
   that explicitly test various aspects of a theory in a coherent way.},
