@article{ WOS:000274813500005,
Author = {Paluck, Elizabeth Levy},
Title = {The Promising Integration of Qualitative Methods and Field Experiments},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2010},
Volume = {628},
Pages = {59-71},
Month = {MAR},
Abstract = {Randomized field experiments should take a more central place in
   qualitative research. Although field experimentation is often considered
   a quantitative enterprise, this paper illustrates the compatibility of
   field experimentation with various types of qualitative measurement
   tools and research questions. Integrating qualitative and quantitative
   data within field experiments allows investigators to move past simple
   average treatment effects and explore mechanisms of the identified
   causal effect. A more novel proposal is to use field experimentation as
   the organizing methodological framework for archival, ethnographic, or
   interpretive work, and to use ethnographic methods as the primary source
   of measurement in ``experimental ethnography.{''} Sustained research and
   theoretical specificity can address some of the seemingly incompatible
   features of qualitative and field experimental methods. For example,
   small sample sizes are acceptable as part of a research program, and
   some theories of historical patterns or rare events could be
   disaggregated into smaller cause-and-effect linkages to test with field
   experiments in theoretically relevant contexts.},
