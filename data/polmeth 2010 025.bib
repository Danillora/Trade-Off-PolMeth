@article{ WOS:000210778900003,
Author = {Holmstrom, Lauri},
Title = {Finland in American Intelligence 1941-1944},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2010},
Volume = {25},
Number = {4},
Pages = {479-499},
Abstract = {This article analyses the content and nature of American intelligence
   with regard to Finland in 1941-1944. The significance of intelligence
   was that it supported connections between the United States and Finland
   at a time when both external and internal circumstances inhibited the
   functioning of normal state to state relations between the two nations.
   The evolution of wartime conditions guided the interests of American
   intelligence. The main conclusion is that American intelligence in
   Finland can be interpreted as communication on multiple levels between
   two states.},
