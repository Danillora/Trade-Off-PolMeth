@article{ WOS:000281818400002,
Author = {Arfi, Badredine},
Title = {``Euro-Islam{''}: Going Beyond the Aporiatic Politics of Othering},
Journal = {INTERNATIONAL POLITICAL SOCIOLOGY},
Year = {2010},
Volume = {4},
Number = {3},
Pages = {236-252},
Month = {SEP},
Abstract = {Is a ``Euro-Islam{''} possible? Whether at the level of theory, practice
   or policy, very few scholars and practitioners pay enough sustained
   attention to the fact that how we think/not-think, speak/silence,
   inscribe/erase, and address/ignore various aspects of ``Islam in Western
   Europe{''} has much to do with the concepts of ``identity{''} and
   ``other.{''} Whereas policy-makers, activists, peace-advocates, and
   fear-mongers continuously deploy these concepts for or against certain
   strategies, agendas and purposes, it is important to ``deconstruct{''}
   these concepts so as to open up a horizon for thinking the possibility
   of a Euro-Islam. This paper thus argues that thinking of Euro-Islam
   implies an aporiatic politics of ``Othering{''} and that it is possible
   to go beyond this aporia in a certain way by using a new approach for
   thinking about Islam in Europe. I develop this framework through a
   deconstruction of the concepts of ``identity{''} and ``other{''} using a
   non-Aristotelian logic of ``without,{''} the pair of undecidable
   concepts immunity/auto-immunity, and Derrida's notion of
   ``negotiation.{''}.},
