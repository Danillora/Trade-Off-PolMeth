@article{ WOS:000283681800005,
Author = {Parau, Cristina E.},
Title = {East Side Story: How Transnational Coalitions Contested EU
   Conditionality},
Journal = {EUROPE-ASIA STUDIES},
Year = {2010},
Volume = {62},
Number = {9},
Pages = {1527-1554},
Abstract = {The literature on Europeanisation in relation to Eastern Europe has
   posited the material incentives of EU membership as the main driver of
   domestic reforms aimed at adopting EU rules and norms (conditionality).
   But this fails to explain puzzling instances where no EU rule exists yet
   domestic change happens under European influence, or where the rule is a
   condition yet has little impact. As repositories of (dis)information,
   transnational networks can embolden a candidate country to breach the
   rules or influence it to comply with the `extra-conditionality' the
   networks themselves create out of their own agendas.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
