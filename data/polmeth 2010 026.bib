@article{ WOS:000280363300008,
Author = {Whang, Taehee},
Title = {Empirical Implications of Signaling Models: Estimation of Belief
   Updating in International Crisis Bargaining},
Journal = {POLITICAL ANALYSIS},
Year = {2010},
Volume = {18},
Number = {3},
Pages = {381-402},
Month = {SUM},
Abstract = {Signaling models are ubiquitous in political science. An essential
   characteristic of these models is that actors can update their beliefs
   about their opponents. An actor observes the behavior of his opponent,
   and this behavior functions as a signal that allows the actor to learn
   more about his opponent's true ``type.{''} As a result, the actor is
   able to adapt his own behavior. Current statistical models of strategic
   choice based on perfect Bayesian equilibrium, however, allow for very
   little, if any, belief updating. I explain why current models allow for
   little updating and offer in their stead a new, fully strategic choice
   estimator that calculates the correct amount of belief updating.},
