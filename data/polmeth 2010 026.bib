@incollection{ WOS:000288746500005,
Author = {Phoenix, Chris},
Book-Author = {Ekekwe, N},
Title = {Molecular Manufacturing: Nano Building Nano},
Booktitle = {NANOTECHNOLOGY AND MICROELECTRONICS: GLOBAL DIFFUSION, ECONOMICS AND
   POLICY},
Year = {2010},
Pages = {26-40},
Abstract = {A wide variety of nanotechnology programs, both pedagogical and
   research-oriented, can incorporate some aspect of molecular
   manufacturing. Nanotechnology is developing from large tools that give
   us access to the nanoscale, to tools constructed at or near the
   nanoscale. To date, these nanoscale tools are not capable of
