@article{ WOS:000217466100006,
Author = {Christou, George},
Title = {European Union security logics to the east: the European Neighbourhood
   Policy and the Eastern Partnership},
Journal = {EUROPEAN SECURITY},
Year = {2010},
Volume = {19},
Number = {3, SI},
Pages = {413-430},
Abstract = {This article argues that the broad security discourse built into the
   European Union's (EU's) initiatives to the east, and specifically the
   European Neighbourhood Policy (ENP) and Eastern Partnership (EaP) have
   in practice not yielded the stability, prosperity and security sought
   after by the EU. Whilst the EU has pursued bilateral and multilateral
   processes of engagement through the ENP and EaP, the paradox has
   remained within its double security narrative and has often resulted in
   minimal change to the east, as well as contestation from eastern
   partners and regional actors such as Russia. The conclusion suggests
   that the EU must provide a more pragmatic, differentiated and balanced
   narrative, and thus process of engagement with the east if it is serious
   about creating a zone of peace, stability and prosperity.},
