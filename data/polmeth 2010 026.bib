@article{ WOS:000217464600003,
Author = {Kaunert, Christian},
Title = {The area of freedom, security and justice in the Lisbon Treaty:
   commission policy entrepreneurship?},
Journal = {EUROPEAN SECURITY},
Year = {2010},
