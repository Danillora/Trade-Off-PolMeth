@article{ WOS:000284411700001,
Author = {Roll, Stephan},
Title = {`Finance Matters!' The Influence of Financial Sector Reforms on the
   Development of the Entrepreneurial Elite in Egypt},
Journal = {MEDITERRANEAN POLITICS},
Year = {2010},
Volume = {15},
Number = {3},
Pages = {349-370},
Abstract = {Since the beginning of the 1990s, Egypt has conducted comprehensive
   reforms aiming to liberalize the state-controlled financial sector. This
   article analyses the possible impact of these reforms on the political
   sphere, arguing that financial liberalization could enhance the
   political independence of members of the private sector, helping them to
   play a more influential role in Egyptian politics leading to greater
   political liberalization. The empirical analysis of the Egyptian reform
   process does not support this assumed causality. Design and sequencing
   of financial reforms benefited first and foremost members of the
   entrepreneurial elite close to the regime. Nevertheless, financial
   reforms impact on power relations in Egypt by strengthening the position
   of these large-scale entrepreneurs within the political decision-making
   process.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
