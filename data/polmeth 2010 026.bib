@article{ WOS:000279334100006,
Author = {Soler i Lecha, Eduard},
Title = {Converging, Diverging and Instrumentalizing European Security and
   Defence Policy in the Mediterranean},
Journal = {MEDITERRANEAN POLITICS},
Year = {2010},
Volume = {15},
Number = {2},
Pages = {231-248},
Abstract = {This article addresses the degree of convergence, divergence and in some
   cases indifference of Southern and Eastern Mediterranean Countries
   towards the European Security and Defence Policy. Focusing on two cases,
   Morocco and Turkey, but also referring to other Mediterranean partners,
   this contribution analyses the dynamics of this specific issue area,
   arguing that policy convergence in the field of security and defence has
   reflected process-oriented goals rather than a substantive convergence
   of strategic interests. The article concludes by exploring how the EU's
   differentiated geographical approach in security and defence cooperation
   in the Mediterranean impacts on the broader region-building endeavour.},
