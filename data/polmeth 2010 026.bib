@article{ WOS:000210773400004,
Author = {Pfiffner, James P.},
Title = {US Blunders in Iraq: De-Baathification and Disbanding the Army},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2010},
Volume = {25},
Number = {1},
Pages = {76-85},
Abstract = {In May 2003 Paul Bremer issued CPA Orders to exclude from the new Iraq
   government members of the Baath Party (CPA Order 1) and to disband the
   Iraqi Army (CPA Order 2). These two orders severely undermined the
   capacity of the occupying forces to maintain security and continue the
   ordinary functioning of the Iraq government. The decisions reversed
   previous National Security Council judgments and were made over the
   objections of high ranking military and intelligence officers. The
   article concludes that the most likely decision maker was the Vice
   President.},
