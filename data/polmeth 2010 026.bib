@article{ WOS:000280378000003,
Author = {Herrick, Rebekah},
Title = {The Legislative Effectiveness of Gay and Lesbian Legislators},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2010},
Volume = {31},
Number = {3},
Pages = {243-259},
Abstract = {This article examines the legislative effectiveness of gay and lesbian
   state legislators. Legislative effectiveness is measured by legislators'
   perceptions of their effectiveness, their ability to block legislation,
   and their ability to see the bills they introduce pass. Although the
   number of openly gay and lesbian legislators is small, making this study
   exploratory, the findings are illuminating. Without controlling for
   legislators' relationships with colleagues or their ideology, lesbians,
   but not gay men, were less effective than other legislators, but after
   controlling for these factors lesbians were not significantly less
   effective than others.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
