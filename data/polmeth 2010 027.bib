@article{ WOS:000281728700006,
Author = {Allers, Maarten A. and Ishemoi, Lewis J.},
Title = {Fiscal Capacity Equalisation in Tanzania},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2010},
Volume = {36},
Number = {5},
Pages = {697-713},
Abstract = {Fiscal equalisation aims at enabling decentralised governments to supply
   similar services at similar tax rates. In order to equalise fiscal
   disparities, differences in both fiscal capacities and in fiscal needs
   have to be measured. This paper focuses on the measurement of fiscal
   capacity in a developing country. The current intergovernmental transfer
   system in Tanzania does not take differences in fiscal capacity into
   account. As a result, local governments in rich areas are able to
   generate considerably more revenue per capita than those in poor areas.
   Public services in poor areas are hard to finance. We propose a way of
   measuring fiscal capacities of local governments in Tanzania using
   poverty data. We use this measure to derive an equalisation grant that
   would support local governments that have a low fiscal capacity.},
