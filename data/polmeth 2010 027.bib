@article{ WOS:000217771000003,
Author = {Surzhko-Harned, Lena},
Title = {Liberal nationalism, nationalist liberalization, and democracy: the
   cases of post-Soviet Estonia and Ukraine},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2010},
Volume = {38},
Number = {5},
Pages = {623-646},
Abstract = {The bulk of scholarly literature views nationalism as harmful to
   democratic transition. Yet Juan Linz and Alfred Stepan indirectly
   suggest that nationalism may benefit democratization. This study shows
   that under the right conditions nationalism can benefit democratic
   transition. Building on the typology of Linz and Stepan and the liberal
   nationalism tradition of Yael Tamir and David Miller, this study
   examines the transitions in Estonia and Ukraine. It introduces an
   important layer, the multinational federal state, into the typologies
   developed by Linz and Stepan to show that nationalism can prove a useful
   political tool of mobilization in a multiethnic setting.},
