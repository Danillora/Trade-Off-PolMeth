@article{ WOS:000212615600008,
Author = {Royall, Frederic},
Title = {Opportunities and Perception in Pro-Traveller Mobilisations in Ireland
   (1960-2000)},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2010},
Volume = {18},
Number = {2},
Pages = {253-269},
Abstract = {This article looks at the ways the issues of opportunities and
   perception have been used as building blocks for pro-Traveller
   mobilisations in the Irish Republic. I argue that the ways Travellers
   viewed and presented themselves were key factors to account for the
   range and the extent of pro-Traveller mobilisations from the 1960s to
   2000. But I also stress that perception was not enough. Travellers also
   availed of and seized favourable `windows of opportunities'.},
