@article{ WOS:000217467300002,
Author = {Delcour, Laure},
Title = {The European Union, a security provider in the eastern neighbourhood?},
Journal = {EUROPEAN SECURITY},
Year = {2010},
Volume = {19},
Number = {4, SI},
Pages = {535-549},
Abstract = {This article analyses the extent to which and the way in which the
   European Union (EU) has acted as a security provider in its eastern
   vicinity since the European Neighbourhood Policy was launched. The EU's
   capacity to emerge as a security provider and the approaches developed
   in this purpose are gauged against three major EU objectives in the
   eastern neighbourhood: conflict resolution, control of migration flows
   and good governance. The article sustains that EU efforts to reach
   objectives which it considers important for regional security are
   undermined by a lack of coherence in policy implementation across the
   eastern neighbourhood.},
