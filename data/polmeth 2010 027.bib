@incollection{ WOS:000384070300005,
Author = {Cullen, Rowena},
Editor = {Scholl, HJ},
Title = {DEFINING THE TRANSFORMATION OF GOVERNMENT E-Government or E-Governance
   Paradigm?},
Booktitle = {E-GOVERNMENT: INFORMATION, TECHNOLOGY, AND TRANSFORMATION},
Series = {Advances in Management Information Systems},
Year = {2010},
Volume = {17},
Pages = {52-71},
Abstract = {The e-government goals of many countries are to ``transform
   government{''} through the use of information and communication
   technologies (ICT). But such statements have very different meanings.
   This is partly because the concept of transformation is aspirational and
   has not been adequately defined, and partly because the field of
   e-government is emerging from a number of disciplines that approach the
   concept of transformation in very different ways. At the same time, much
   of the literature on e-government confuses the concepts of government
   and governance. This chapter analyzes the concepts of transformation as
   used by government and as it is emerging in the literature, in the
   fields of information systems, public administration, and political
   science. It shows how the concept of transformation can be grounded in
   either e-government or e-governance, and the difference between these
   approaches, and highlights the need for the e-government research
   community to understand the differences between the two in order to
   identify ways to measure achievement within both frameworks.},
