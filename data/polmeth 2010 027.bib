Author = {Biermann, Frank and Betsill, Michele M. and Gupta, Joyeeta and Kanie,
   Norichika and Lebel, Louis and Liverman, Diana and Schroeder, Heike and
   Siebenhuener, Bernd and Zondervan, Ruben},
Title = {Earth system governance: a research framework},
