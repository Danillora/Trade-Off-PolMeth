@incollection{ WOS:000384070300003,
Author = {Scholl, Hans J.},
Editor = {Scholl, HJ},
Title = {ELECTRONIC GOVERNMENT A Study Domain Past Its Infancy},
Booktitle = {E-GOVERNMENT: INFORMATION, TECHNOLOGY, AND TRANSFORMATION},
Series = {Advances in Management Information Systems},
Year = {2010},
Volume = {17},
Pages = {11-30},
Abstract = {The study domain of electronic government (EG) has accumulated a
   relatively large body of knowledge and has formed a well-structured
   research community, which produces a steady flow of research output.
   However, despite its impressive growth, fundamental issues remain in
   this domain of study. The integration of disciplinary contributions and
   interdisciplinary relations within EG have not been sufficiently
   developed. Large areas of EG knowledge remain unconnected. The
   ontological and epistemological stances have not been discussed. This
   chapter presents a short history and inventory of the study domains,
   reviews the discussion of foundations and central research questions in
   EG, and empirically analyzes the structure and composition of the EG
   literature. The chapter concludes that while EG has progressed past
   infancy, certain areas need increased attention and further development
   in order to make EG more mature and more effective as an integrative
   science with great relevance to theory and practice.},
