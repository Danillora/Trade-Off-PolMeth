@article{ WOS:000277173300004,
Author = {Andonova, Liliana B.},
Title = {Public-Private Partnerships for the Earth: Politics and Patterns of
   Hybrid Authority in the Multilateral System},
Journal = {GLOBAL ENVIRONMENTAL POLITICS},
