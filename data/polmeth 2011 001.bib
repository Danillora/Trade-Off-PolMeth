@article{ WOS:000446456800003,
Author = {Pole, Antoinette},
Title = {Would Elizabeth Cady Stanton Blog? Women Bloggers, Politics, and
   Political Participation},
Journal = {INTERNATIONAL JOURNAL OF E-POLITICS},
Year = {2011},
Volume = {2},
Number = {2},
Pages = {37-54},
Month = {APR-JUN},
Abstract = {This study examines the role of women political bloggers and how they
   use their blogs for purposes related to politics, public policy, and
   current events. Based on a combined purposive-snowball sample, in-depth
   interviews were conducted with 20 women political bloggers in October
   2006. Findings show respondents blog about a range of topics, not
   necessarily unique to women. Generally, women use their blogs to inform
   their readers, check the media, engage in advocacy efforts, and solicit
   charitable contributions from their readers and more specifically, women
   ask their readers to vote and contact elected officials. Data show women
   deal with a range of challenges blogging most notably discrimination.
   Though a majority of women political bloggers reported they did not face
   discrimination, interviewees qualified their responses saying they
   witnessed discrimination and discriminatory attitudes, suggesting the
   political blogosphere is somewhat inhospitable to women.},
