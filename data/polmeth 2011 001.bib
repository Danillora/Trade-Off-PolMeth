@article{ WOS:000214665300002,
Author = {Cheng Enfu and Xin Xiangyang},
Title = {Fundamental elements of the China model},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2011},
Volume = {1},
Number = {1},
Pages = {2-10},
Abstract = {The present study takes as its departure point theoretical reflections
   on the concept of `model' for a discussion of the China model in
   particular, a model that has attracted increasing attention all over the
   world due to China's dramatic development. It surveys the origin and
   major conceptualizations of the China model in both the Chinese and the
   international context and presents on that basis a series of fundamental
   constituents of the China model in relation to China's economic,
   political, cultural and social development. Its central point is that
   the China model expresses socialism with Chinese characteristics, and
   therefore constitutes a developing country model with qualities that are
   specific to the Chinese context.},
