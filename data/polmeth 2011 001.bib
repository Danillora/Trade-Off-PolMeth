@article{ WOS:000209059600014,
Author = {Eger, Karl Magnus and Jorgensen-Dahl, Arnfinn},
Title = {Short Way, High Risk The Northern Sea Route: A Euro-Asian Shipping Lane?},
Journal = {OSTEUROPA},
Year = {2011},
Volume = {61},
Number = {2-3},
Pages = {179+},
Month = {FEB-MAR},
Abstract = {For years, the northeast passage has been discussed as a new route for
   commercial shipping between Europe and Asia. Voyages between north
   European and north Pacific ports could thus become clearly shorter.
   However, despite the decrease in summer ice cover in the north polar
   sea, navigation remains difficult. In its central sector, the northern
   sea route, which is controlled by Russia, voyages are subject to
   approval, ice-breaker escorts obligatory. Together with high insurance
   costs, this drives up transport expenses. Despite tremendous savings in
   fuel and time, it depends on each individual case whether the northern
   route is in fact less expensive than the route through the Suez Canal.},
