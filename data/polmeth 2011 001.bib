@article{ WOS:000299907500010,
Author = {Morice, Alain},
Title = {Immigration and illegality: foreigners' informal activities},
Journal = {PENSEE},
Year = {2011},
Number = {368},
Pages = {105+},
Month = {OCT-DEC},
Abstract = {Alain Morice, Immigration and illegality: foreigners' informal
   activities
   The informal/shadow economy is frequently praised as an alternative
   solution for the development in Third World cities. However in rich
   countries, it is viewed with more suspicion and, in the context of a
   growing xenophobia, is generally associated with uncontrolled
   immigration. Consequently a stigmatisation of foreigners can be
   observed, especially of those undocumented. Such hostile an opinion
   relies on misunderstandings regarding the real functioning of the
   informal networks, underpined by logics of solidarity as well as other
   creative aspects, through mechanisms involving dependence, submission
   and exploitation.},
