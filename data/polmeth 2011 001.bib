@article{ WOS:000210627000012,
Author = {Keaney, Gerald},
Title = {A New Avant-Garde?},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2011},
Volume = {23},
Number = {4},
Pages = {556-564},
Month = {OCT},
Abstract = {Many artists who identify as politically committed are suspicious of art
   due to the Situationist argument that revolutionary art is impossible.
   If right, the argument also rules out a future artistic avant-garde. I
   believe that, by concentrating on the truth-telling possibilities of
   art, we can meet the Situationist argument. To do so, it is necessary to
   change the relationship art has to everyday life. We can then speculate
   on the form of a future avant-garde.},
