@article{ WOS:000457920200008,
Author = {Remnev, A. V.},
Title = {NATIONALITY ``SIBERIAN{''}: REGIONAL IDENTITY AND HISTORICAL
   CONSTRUCTIVISM OF 19 th CENTURY},
Journal = {POLITEIA-JOURNAL OF POLITICAL THEORY POLITICAL PHILOSOPHY AND SOCIOLOGY
   OF POLITICS},
Year = {2011},
Volume = {62},
Number = {3},
Pages = {109+},
Abstract = {The article is devoted to the analysis of the historical experience and
   prospects of constructing regional Siberian identity. The author focuses
   his attention on the peculiar social movement that originated in the
   second half of the 19 th century and was known as the ``Siberian
   regionalism{''}. Having conducted the detailed analysis of regionalists'
   theoretical constructions and real activity, A. Remnev shows that the
   factors that prevented the transition of the ``Siberian issue{''} into
   the stage of political separatism were for the most part of a temporary
   character. He draws the conclusion that regional identity of Siberians
   is still open for elaboration, and economic dependence, social and
   cultural abjection of Siberia, incompletion of integration processes
   under the conditions of inequality of territories might turn into the
   ground for protest and separatist sentiments.},
Publisher = {ZHURNAL POLITICHESKOI FILOSOFII \& SOTSIOLOGII POLITIKI POLITIYA},
