@article{ WOS:000300816400006,
Author = {Lambelet, Alexandre},
Title = {Age segregation or generational choice: understanding the elderly
   activism},
Journal = {POLITIX},
Year = {2011},
Volume = {24},
Number = {96},
Pages = {81+},
Abstract = {The life course stories of elderly activists shows that this activism
   has to be linked with to types of explanations. On the one hand, this
   activism has to be read as the product of the exclusion from others
   organisations that these members have get through due to their age. On
   the other hand, this activism has to be read as the product of their
   will to be committed with people from the same generation. Therefore
   elderly organizations are less a place where elderly interests are
   defended than a place where elderly people can continue some kind of
   activism. Finally, this paper demonstrates that the comprehension of
   where elderly people get involved as activist has to be linked with the
   structure of inequality (i.e. the age segregation) that exists in social
   movements as in the whole society.},
