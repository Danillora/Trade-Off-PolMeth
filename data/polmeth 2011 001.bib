@article{ WOS:000291781600001,
Author = {Gibson, Rachel and Lewis-Beck, Michael S.},
Title = {Methodologies of election forecasting: Calling the 2010 UK ``hung
   parliament{''}},
Journal = {ELECTORAL STUDIES},
Year = {2011},
Volume = {30},
Number = {2, SI},
Pages = {247-249},
Month = {JUN},
Abstract = {Election forecasting has experienced considerable growth as a subfield
   within political science. Research work on United Kingdom elections has
   been cutting edge here. The recent 2010 general election afforded the
   opportunity for a trial of different forecasting methodologies. These
   efforts are showcased in this volume, and include standard, and
   not-so-standard, statistical models. Overall, these models perform well,
   foreseeing the unprecedented outcome of a ``hung parliament{''}, as most
   pollsters and pundits did not. Moreover, they achieved this accuracy
   with forecasts well in advance of the election itself. (C) 2010
   Published by Elsevier Ltd.},
