@article{ WOS:000298126000005,
Author = {Wahman, Michael},
Title = {Offices and policies - Why do oppositional parties form pre-electoral
   coalitions in competitive authoritarian regimes?},
Journal = {ELECTORAL STUDIES},
Year = {2011},
Volume = {30},
Number = {4},
Pages = {642-657},
Month = {DEC},
Abstract = {Although previous research has suggested that the opposition's ability
   to form pre-electoral coalitions (PECs) in authoritarian elections is
   crucial for the electoral outcome, little has been written about why and
   when such coalitions are formed. The aim of this article is to fill this
   empirical and theoretical gap. A theory that combines oppositional
   parties' office- and policy-gaining potential when creating such
   coalitions is proposed. The article utilizes a unique database of 111
   competitive authoritarian elections and provides a representative sample
   of strategically chosen cases. It is shown that, coalitions are more
   likely when structural conditions favor oppositional victories and when
   oppositional parties have a distinctive policy agenda in relation to the
   incumbent government. These factors are shown to be more important than
   electoral institutions. (C) 2011 Elsevier Ltd. All rights reserved.},
