@article{ WOS:000294938100004,
Author = {Otjes, Simon},
Title = {The Fortuyn Effect revisited: How did the LPF affect the Dutch
   parliamentary party system?},
Journal = {ACTA POLITICA},
Year = {2011},
Volume = {46},
Number = {4},
Pages = {400-424},
Month = {OCT},
Abstract = {The List Pim Fortuyn (LPF) is a key example of a family of new
   right-wing parties that, according to many observers, have had a strong
   effect on European party systems. This article studies the effect of the
   entry of the LPF into the Dutch Parliament on the parliamentary party
   system. The article looks at two aspects of the party system: first, did
   the LPF affect the issue agenda of the legislature? And second, has the
   LPF been able to create a new significant line of conflict in the
   legislature, as Kriesi and Frey and Pellikaan et al observed for the
   electoral arena? Or, as Van der Brug and Bale have proposed, is the
   Dutch party system after Fortuyn essentially one-dimensional? The study
   looks at new, behavioral, data from the Dutch legislature: parliamentary
   voting behavior and the sponsorship of motions, to answer these
   questions. It employs Poole's Open Classification method to understand
   the patterns underlying voting behavior. Acta Politico (2011) 46,
   400-424. doi:10.1057/ap.2011.12},
