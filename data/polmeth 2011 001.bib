@article{ WOS:000212980800002,
Author = {Ripberger, Joseph T. and Rabovsky, Thomas M. and Herron, Kerry G.},
Title = {Public Opinion and Nuclear Zero: A Domestic Constraint on Ditching the
   Bomb},
Journal = {POLITICS \& POLICY},
Year = {2011},
Volume = {39},
Number = {6},
Pages = {891-923},
Month = {DEC},
Abstract = {Dating back to Harry S. Truman, a number of presidents and prominent
   political leaders have tried, in some way or another, to reduce the
   reliance on nuclear weapons in U.S. foreign policy. Throughout the
   years, such attempts have prompted countless international debates and
   spawned a cottage industry of security research related to the eventual
   elimination of nuclear arms. However, aside from the occasional foray,
   this research paradigm generally overlooks the domestic political
   obstacles that policy makers should expect to encounter if national
   security policy continues down the path toward nuclear zero. In pursuit
   of this goal, this article empirically examines public attitudes
   concerning the continued pursuit of nuclear abolition. In doing so, we
   highlight the beliefs and perceptions that inform public preferences
