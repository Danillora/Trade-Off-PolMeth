@article{ WOS:000304759800003,
Author = {Buchler, Justin},
Title = {Going off the Rails on a Crazy Train: The Causes and Consequences of
   Congressional Infamy},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
Year = {2011},
Volume = {9},
Number = {2},
Month = {JUL},
Abstract = {Legislators like Michele Bachmann and Alan Grayson become nationally
   infamous for their provocative behavior, yet there is little scholarly
   attention to such infamy. This paper examines the predictors of
   congressional infamy, along with its electoral consequences. First,
   infamy is measured through the frequency with which internet users
   conduct searches of legislators' names, paired with epithets attacking
   their intelligence or sanity. Then, ideological extremism and party
   leadership positions are shown to be the best statistical predictors.
   The electoral consequences of infamy follow: infamous legislators raise
   more money than their lower-profile colleagues, but their infamy also
   helps their challengers to raise money. In the case of House
   Republicans, there appears to be an additional and direct negative
   effect of infamy on vote shares. The fundraising effect is larger in
   Senate elections, but there is no evidence of direct electoral cost for
   infamous senatorial candidates.},
