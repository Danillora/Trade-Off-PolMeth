@article{ WOS:000288151800003,
Author = {Jensen, Carsten},
Title = {Negotiated expansion: Left-wing governments, corporatism and social
   expenditure in mature welfare states},
Journal = {COMPARATIVE EUROPEAN POLITICS},
Year = {2011},
Volume = {9},
Number = {2},
Pages = {168-190},
Month = {MAR},
Abstract = {Following the seminal work of Pierson (1994), a general consensus has
   emerged that left-wing governments are no longer able to expand social
   protection. If anything, the strongest partisan effect seems to be
   right-wing governments curtailing social protection, while left-wing
   governments merely secure the status quo. The article presents a new
   argument on `negotiated expansion', suggesting that left-wing
   governments remain the motor behind the expansion of social spending,
   but that their success depends on labor market corporatism. Corporatism
   allows for negotiation with and the socialization of the employers who
   are enjoying an increasingly strong bargaining position vis-a-vis the
   left-wing due to increasing globalization. In countries with extensive
   corporatism, left-wing governments thus continue to increase social
   spending, whereas such expansion is rendered impossible in countries
   with limited corporatism. Comparative European Polities (2011) 9,
   168-190. doi:10.1057/cep.2009.15},
