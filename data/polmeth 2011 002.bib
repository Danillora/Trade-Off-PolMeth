@article{ WOS:000297527700010,
Author = {Ragsdale, Lyn and Rusk, Jerrold G.},
Title = {Casting Votes: The National Campaign Context and State Turnout,
   1920-2008},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2011},
Volume = {64},
Number = {4},
Pages = {840-857},
Month = {DEC},
Abstract = {This study examines voter turnout in the American states in U.S.
   presidential and House elections from 1920 through 2008. A model
   predicts turnout as the sum of the national campaign context, state
   autonomy, and electoral continuity. The national campaign context
   encompasses conditions that prompt turnout to shift similarly across
   states. State autonomy involves state-specific factors that prompt
   turnout to vary across states. Electoral continuity involves people
   voting in successive elections, regardless of other influences. Testing
   the model finds that national campaign context effects have increased,
   but they vary by year, election type, and region and have been mixed
   since the 1970s.},
