@article{ WOS:000306119400001,
Author = {Brazeal, Gregory},
Title = {WEBS OF FAITH AS A SOURCE OF REASONABLE DISAGREEMENT},
Journal = {CRITICAL REVIEW},
Year = {2011},
Volume = {23},
Number = {4},
Pages = {421-448},
Abstract = {An individual's beliefs can be seen as rationally related to one another
   in a kind of web. These beliefs, however, may not form a single,
   seamless web. There may exist smaller, largely self-contained webs with
   few or no rational relations to the larger web. Such ``webs of faith{''}
   make it possible for reasonable deliberators to persist in a
   disagreement even under ideal deliberative conditions. The possibility
   of reasonable disagreement challenges the assumption that rationality
   should lead to consensus and presents an obstacle to the goals of
   liberal democracy.},
