@article{ WOS:000214822900002,
Author = {Zielonka, Jan},
Title = {America and Europe: two contrasting or parallel empires?},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2011},
Volume = {4},
Number = {3, SI},
Pages = {337-354},
Abstract = {This article considers whether an imperial paradigm is useful in aiding
   our understanding of the European Union (EU) and US today. It argues
   that empires are key units in today's world, imposing considerable
   constraints on formally sovereign states, and that this cannot but
   change the nature of International Relations. The article compares some
   of the distinctive characteristics of the EU and US, and considers
   whether their structural features and imperial behaviours generate
   cooperation or conflict. It concludes that their competition for
   supremacy in Europe is potentially conflict-prone. The US treats Europe
   as one of its vital peripheries and this is at odds with the EU's own
   imperial ambitions.},
