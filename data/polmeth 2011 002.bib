@article{ WOS:000438647000001,
Author = {Woolford, Thomas and Matusitz, Jonathan},
Title = {The Memetic Engineering of Anonymous, the Cyberterrorist Group},
Journal = {INTERNATIONAL JOURNAL OF CYBER WARFARE AND TERRORISM},
Year = {2011},
Volume = {1},
