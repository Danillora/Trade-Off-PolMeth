@article{ WOS:000217309200001,
Author = {Ray, Leonard},
Title = {Anti-Americanism and left-right ideology in France},
Journal = {FRENCH POLITICS},
Year = {2011},
Volume = {9},
Number = {3},
Pages = {201-221},
Month = {SEP},
Abstract = {This article examines the shifting relationship between left-right
   ideology and anti-Americanism in France. Neo-Gaullist episodes such as
   French opposition to the invasion of Iraq in 2003 have only underscored
   the weakness of anti-Americanism on the French right, as this policy was
   least popular among the supporters of the center right UMP, and found
   more support on the left. This article traces the decline of right wing
   anti-Americanism in the French public, and the increasing alignment of
   anti-American attitudes with the left end of the leftright ideological
   dimension. This development is placed in the context of the increasing
   emphasis on neoliberal economic policy on the post-Gaullist right, and
   the use of the United States as a model for neoliberal economic policy.},
