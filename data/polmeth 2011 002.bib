@article{ WOS:000441111800013,
Author = {Rutland, P.},
Title = {The Presence of Absence: Ethnicity Policy in Russia},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2011},
Number = {2},
Pages = {172+},
Abstract = {This article looks at Russian national identity through the prism of
   Western theories of nationalism, and finds that Russia is something of a
   special case. The Russian Federation does not have a clear and
   consistent narrative of what is the national identity which unites its
   citizens. A concept based on ethnic Russian (Russkii) identity could
   alienate the one in five of the population who are not ethnic Russians.
   Efforts in the 1990s to create a purely `civic' national identity, based
   on individual rights. were unsuccessful. The legacy of Soviet times.
   where loyalty to the state was separate from ethnic identity. continues
   to shape the Russian state's approach to the nationality question.},
