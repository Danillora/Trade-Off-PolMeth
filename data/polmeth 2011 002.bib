@article{ WOS:000210008900002,
Author = {Miguel, Yolanda Martinez-San},
Title = {<<Sexiles >>: toward a new poetics of the Caribbean erotic},
Journal = {AMERICA LATINA HOY-REVISTA DE CIENCIAS SOCIALES},
Year = {2011},
Volume = {58},
Pages = {15-30},
Month = {AUG},
Abstract = {Taking as a point of departure the definition of sexile used by critics
   of queer Caribbean studies, as well as the definition used by U.S.
   American undergraduate students, this essay studies exile of sexual
   minorities in Cuba and Puerto Rico. This study focuses on masculine
   sexile, and studies how Luis Rafael Sanchez, Reinaldo Arenas, and Manuel
   Ramos Otero depict the traditional definition of sexile to explore how
   this notion can be transformed when we add the new meaning used by
   younger generations of undergraduates. The essay concludes by reflecting
   on the advantages and disadvantages of proposing sexile as a poetic for
   Caribbean eroticism that ultimately redefines collective identity
   discourses in the Caribbean.},
