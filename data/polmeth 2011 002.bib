@article{ WOS:000212423900004,
Author = {Pollock, Philip H. and Hamann, Kerstin and Wilson, Bruce M.},
Title = {Learning Through Discussions: Comparing the Benefits of Small-Group and
   Large-Class Settings},
Journal = {JOURNAL OF POLITICAL SCIENCE EDUCATION},
Year = {2011},
Volume = {7},
Number = {1},
Pages = {48-64},
Abstract = {The literature on teaching and learning heralds the benefits of
   discussion for student learner outcomes, especially its ability to
   improve students' critical thinking skills. Yet, few studies compare the
   effects of different types of face-to-face discussions on learners.
   Using student surveys, we analyze the benefits of small-group and
   large-class discussions in an upper-level political theory course. We
   also analyze whether the same types of students are likely to
   participate and reap the benefits of both types of discussions. We find
   that, overall, participation is higher in small-group discussions, as
   are students' perceptions of learner outcomes. We also find a more equal
   participation of students of different ethnic backgrounds in small-group
   discussions; similarly, previous academic achievements have less
   influence on discussion participation in small groups.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
