@article{ WOS:000288088300002,
Author = {Bartle, John and Dellepiane-Avellaneda, Sebastian and Stimson, James},
Title = {The Moving Centre: Preferences for Government Activity in Britain,
   1950-2005},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2011},
Volume = {41},
Number = {2},
Pages = {259-285},
Month = {APR},
Abstract = {The political `centre' is often discussed in debates about public policy
   and analyses of party strategies and election outcomes. Yet, to date,
   there has been little effort to estimate the political centre outside
   the United States. This article outlines a method of estimating the
   political centre using public opinion data collected for the period
   between 1950 and 2005. It is demonstrated that it is possible to measure
   the centre in Britain, that it moves over time, that it shifts in
   response to government activity and, furthermore, that it has an
   observable association with general election outcomes.},
