@article{ WOS:000438646100002,
Author = {Pickett, Bryan and Lingenfelter, Mike},
Title = {Influence Strategy: Consistency and Legitimacy as Key Factors},
Journal = {INTERNATIONAL JOURNAL OF CYBER WARFARE AND TERRORISM},
