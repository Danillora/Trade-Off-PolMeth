@article{ WOS:000209059800026,
Author = {Loew, Peter Oliver},
Title = {Local Europeanisation: Europe in Gdansk, Gdansk in Europe},
Journal = {OSTEUROPA},
Year = {2011},
Volume = {61},
Number = {5-6},
Pages = {295+},
Month = {MAY-JUN},
Abstract = {Since 1989, Gdansk has taken many steps towards Europe. In addition to
   historical factors, this local Europeanisation has taken place by
   twinning arrangements and participation in communal networks. Europe is
   important for the city's image and marketing efforts. Europe is to be
   integrated in the minds of Gdansk's citizens, and Gdansk in the
   conscious of Europe's. Gdansk has applied to be European Cultural
   Capital for 2016. Economic intertwining and financial subsidies are part
   of European integration. On many levels, Gdansk constitutes an integral
   part of Europe. Sometimes, however, Europe also serves to demonstrate
   cultural and intellectual distance.},
