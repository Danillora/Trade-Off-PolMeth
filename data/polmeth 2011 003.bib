@article{ WOS:000218335000014,
Author = {Simpson, John},
Title = {BRITISH NUCLEAR WEAPON STOCKPILES, 1953-78 A COMMENTARY ON TECHNICAL AND
   POLITICAL DRIVERS},
Journal = {RUSI JOURNAL},
Year = {2011},
Volume = {156},
Number = {5},
Pages = {74-83},
Abstract = {The UK has traditionally described its nuclear weapons stockpile numbers
   in terms of ` less than x weapons'. As more archive material becomes
   available, it is increasingly possible to estimate the number of
   operational weapons during the 1960s and 1970s. However, accounting for
   all UK nuclear weapons in this period is complicated by the separation
   of components, the lack of data on maintenance and assembly, and the
   difficulty of determining whether procurement targets translated into
   operational devices. The difficulty of generating an authoritative
   account suggests that the global disarmament agenda should focus on
   military fissile material, not warheads.},
