@article{ WOS:000214822900007,
Author = {Halden, Peter},
Title = {Understanding the EU, the US and their external spheres of rule:
   republican synergies, destructive feedbacks and dependencies},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2011},
Volume = {4},
Number = {3, SI},
Pages = {433-450},
Abstract = {Both the US and the European Union (EU) have been analysed as empires.
   In contrast, this article argues that a reconstructed republican theory
   centred on the fear of domination reveals deeper dynamics in both
   entities. Both entities have external spheres of influence. While the US
   is not dependent on this sphere to sustain internal stability, the EU
   is. When the EU enlargement ends and the external sphere is absorbed,
   the EU's stability and legitimacy might be at risk. A possible solution
   lies in the European Neighbourhood Policy. Strengthening it would
   increase the EU's control over its environment and its normative
   aspects.},
