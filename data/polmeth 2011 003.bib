@article{ WOS:000212425300003,
Author = {Doherty, David},
Title = {Teaching Experimental Methods: A Framework for Hands-On Modules},
Journal = {JOURNAL OF POLITICAL SCIENCE EDUCATION},
Year = {2011},
Volume = {7},
Number = {2},
Pages = {163-172},
Abstract = {Experiments provide a simple and engaging framework for familiarizing
   students with the process of quantitative social research. In this
   article, I illustrate how experiments can be used in the classroom
   environment by describing a module that was implemented in four high
   school classrooms. The module familiarized students with how the
   scientific method can be applied to political questions and introduced
