@article{ WOS:000218506100002,
Author = {Eijkman, Quirine},
Title = {Preventive counter-terrorism and non-discrimination assessment in the
   European Union},
Journal = {SECURITY AND HUMAN RIGHTS},
Year = {2011},
Volume = {22},
Number = {2},
Pages = {89+},
Abstract = {As a result of the 9/11 terrorist attacks and the bombings in Madrid and
   London, a prevention-focused counter-terrorism approach has developed
   across the European Union. Preventive counter-terrorism is appealing
   because it implies interventions that remove the ability or, better
   still, the motivation of potential terrorists to carry out their lethal
   designs. Member states such as the United Kingdom and the Netherlands
   that primarily have experience in addressing `home-grown' terrorism,
   have developed preventive counter-terrorism measures in response. Even
   though the majority of these laws, regulations and policies recognize
   the importance of the rule of law and human rights, it remains relevant
   to examine whether in theory and in practice particular measures have
   had disproportionate effects on ethnic and religious minorities and
   thereby violate non-discrimination standards.},
