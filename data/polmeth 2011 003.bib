@article{ WOS:000286347100005,
Author = {Bird, Colin},
Title = {Political Theory and Ordinary Language: A Road Not Taken},
Journal = {POLITY},
Year = {2011},
Volume = {43},
Number = {1},
Pages = {106-127},
Month = {JAN},
Abstract = {This article argues that political theory could gain from a revival of
   the form of ordinary language analysis advocated by J. L. Austin. It
   distinguishes three objectionable forms of scholasticism widespread in
   contemporary political theory, and shows how Austinian methods might
   help to combat them. To illustrate the potential of Austinian analysis
   in political theory, the final third of the article considers, in the
   light of pertinent ordinary language, the widely canvassed claim that
   coercion can involve ``disrespect for persons{''}; these considerations
   suggest that this claim is more complicated, less obviously sound, and
   more interesting, than political theorists often assume. Polity (2011)
   43, 106-127. doi:10.1057/pol.2010.20; published online 23 August 2010},
