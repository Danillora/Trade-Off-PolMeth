@article{ WOS:000304987100004,
Author = {Chappell, Louise and Costello, Mayet},
Title = {Australian Federalism and Domestic Violence Policy-Making},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2011},
Volume = {46},
Number = {4},
Pages = {633-650},
Abstract = {The influence of state architecture on gender policy and politics is an
   emergent strand of feminist research. This paper contributes to this
   research by undertaking a detailed case study of one specific gender
   policy area domestic violence policy - in old federation Australia.
   Drawing on the experiences of the past decade, it confirms earlier
   research findings that demonstrate that under certain conditions,
   federal structures can influence the development of gender policy in
   positive ways, such as providing opportunities for `venue shopping' and
   policy innovation. The paper engages, too, with recent research on
   Australian federalism and challenges the consensus about the centralised
   nature of the Australian federal system by demonstrating that in the
   area of gender policy, states and territories are more than just the
   implementation arms of the Commonwealth government. Sub-national
   governments continue to play an important and autonomous role in policy
   relating to women and therefore remain central sites for advancing
   gender equality.},
