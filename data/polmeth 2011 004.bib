@article{ WOS:000209060300021,
Author = {Magyar, Kornelia},
Title = {Misery Roma in Hungary},
Journal = {OSTEUROPA},
Year = {2011},
Volume = {61},
Number = {12},
Pages = {265+},
Month = {DEC},
Abstract = {At least 700,000 Roma live in Hungary. Their situation is precarious.
   Most live in abject poverty and encounter discrimination and isolation.
   Hostility towards Roma is deeply rooted in the Hungarian majority
   population. Model projects show that it is possible to break the vicious
   circle of poverty, lack of education, and unemployment. The framework
   for the integration of the Roma adopted during the Hungary's presidency
   of the European Union gives rise to hope that, finally, steps to improve
   the situation of Roma will follow the rhetoric.},
