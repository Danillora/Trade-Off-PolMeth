@article{ WOS:000287413300004,
Author = {Grube, Dennis},
Title = {Speech Cycle? `Election-Defining Rhetoric' in Westminster Democracies},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2011},
Volume = {46},
Number = {1},
Pages = {35-52},
Abstract = {Leaders of government within Westminster democracies undertake a key
   rhetorical task on the day an election is called. Following a visit to
   the monarch or the vice-regal representative, leaders emerge to give
   their opening speech of the election campaign. These are moments of
   definition; moments when leaders rhetorically frame the battle to come.
   This paper argues that in Westminster democratic systems, these
   statements conform to a set pattern of rhetoric, which reflects the
   length of time the government has been in office. This pattern-or
   `speech cycle'- suggests that electoral rhetoric is not governed solely
   by the policy issues of the day, and that some restraints apply to
   governments in the rhetoric they can legitimately utilise at different
   points in their tenure.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
