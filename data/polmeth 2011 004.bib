@article{ WOS:000217071700004,
Author = {Broad, Dave},
Title = {The Productivity Mantra The Profit Motive Versus The Public Good},
Journal = {SOCIALIST STUDIES},
Year = {2011},
Volume = {7},
Number = {1-2, SI},
Pages = {65-94},
Month = {SPR-FAL},
Abstract = {Capital accumulation is the essence of production in capitalist society.
   Consequently, corporations are constantly driven and workers exhorted to
   increase productivity in the interest of raising profits. Economic
   slumps and recessions are used as reasons to argue that there is a
   productivity crisis and push for increasing productivity at the expense
   of wages, benefits and social programming, as we see with the post-2007
   Great Recession. This essay discusses these trends, theoretical and
   ideological arguments, and the need for a socialist alternative to the
   never-ending push to increase productivity for capital accumulation at
   the expense of workers' rights and social welfare.},
