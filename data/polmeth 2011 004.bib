@article{ WOS:000292899600005,
Author = {Roettger, Bernd},
Title = {Democratization of Regional Economic and Adjustment Policy in Answer to
   Economic Crisis. Initiatives of Trade Unions since the 1970s-revisited},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2011},
Volume = {40},
Number = {2},
Pages = {155+},
Abstract = {This article examines the regulation of crisis by trade unions in the
   regions in a historical comparative perspective: campaigns to convert
   military production to socially useful purposes in the 1970s and 1980s,
   participation in regional cluster policies in the 1990s and new
   approaches to a democratic transition of the socio-economic development
   model in the latest crisis of capitalism. The findings show that
   regional alternatives of local trade unions were often transformed into
   a ``logic of integration{''} to capitalist modernization. Unions can't
   no longer use the given institutional configurations of regional policy;
   they have to rebuild the conditions trought autonomous practices ``from
   below{''}, in which democratic alternatives in the regions will be
   possible again.},
