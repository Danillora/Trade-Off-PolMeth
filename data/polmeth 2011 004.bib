@article{ WOS:000218940500009,
Author = {Garcia Flores, Brenda K.},
Title = {The Strength of the Finance Commission in Mexico's Chamber of Deputies},
Journal = {LATIN AMERICAN POLICY},
Year = {2011},
Volume = {2},
Number = {2},
Pages = {247-265},
Month = {DEC},
Abstract = {A committee is an important filter for turning bills into law. In the
   fiscal arena in Mexico, close to 60\% of bills do not move past the
   committee. This article analyzes the work of the Finance Committee in
   Mexico's Chamber of Deputies from 1997 to 2009 with regard to tax
   proposals. This analysis shows that, despite the fact that committees
   are strong, particularly the Finance Committee in the Chamber, among
   other factors, the inclusion of other actors in public fiscal policy
   making, such as legislative and political leaders, and the executive
   power limit them.},
