@article{ WOS:000209059800020,
Author = {Grzeszczak, Robert},
Title = {Suitability Test: Polish Constitutional Bodies in the EU-Stress Test},
Journal = {OSTEUROPA},
Year = {2011},
Volume = {61},
Number = {5-6},
Pages = {215+},
Month = {MAY-JUN},
Abstract = {Poland's membership in the European Union has far-reaching consequences
   for the interplay of state constitutional bodies. The government is
   strengthened, the parliament weakened. This, however, is accompanied by
   a loss of legitimacy. The participation of Sejm and Senate in Europe
   policy is to counter this. So far, success has been moderate. This also
   applies to the attempt to better coordinate Europe policy among the
   relevant ministries. Institutionalized domestic portfolio logic stands
   in the way of European cross-sectional thinking that is needed. The
   Constitutional Court has set itself up as the guardian of residual
   sovereignty.},
