@article{ WOS:000293733300003,
Author = {Taratoot, Cole D. and Howard, Robert M.},
Title = {The Labor of Judging: Examining Administrative Law Judge Decisions},
Journal = {AMERICAN POLITICS RESEARCH},
Year = {2011},
Volume = {39},
Number = {5},
Pages = {832-858},
Month = {SEP},
Abstract = {There has been significant scholarly research on judicial decision
   making and bureaucratic control but little research on bureaucrats who
   perform a judicial function, namely, administrative law judges. In this
   article, we analyze the influences on the decisions of administrative
   law judges (ALJs) from 1991 to 2006. Using ordered logit, we examine the
   influence of policy preference and hierarchical and political
   constraint. We find that ALJs are comparable to Federal District Court
   judges in that they use ideology in their rulings, are also subject to
   hierarchical control by higher courts, and that they are constrained by
   separation of powers influences.},
