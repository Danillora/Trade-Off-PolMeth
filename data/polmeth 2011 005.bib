@article{ WOS:000285195600003,
Author = {Kincaid, John and Cole, Richard L.},
Title = {Citizen Attitudes Toward Issues of Federalism in Canada, Mexico, and the
   United States},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2011},
Volume = {41},
Number = {1},
Pages = {53-75},
Month = {WIN},
Abstract = {Public opinion is often said to affect the distribution of powers in
   federal systems, the legitimacy of the orders of government, and system
   functions. However, there is little comparative empirical and
   longitudinal research on public attitudes toward federalism. Using
   several years of surveys conducted in Canada, Mexico, and the United
   States, we examine public attitudes about the treatment of constituent
   units, trust in the various orders of government, government value,
   power distribution, and federal political culture. We find many
   similarities, as well as significant differences, between public
   attitudes toward federalism issues in these North American federations.},
