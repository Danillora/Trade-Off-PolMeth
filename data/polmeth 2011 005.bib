@article{ WOS:000435231700004,
Author = {Mulaj, Klejda},
Title = {Dilemmas of Reacting to Mass Atrocities: Humanitarian Intervention to
   End Violent Conflict in the Western Balkans},
Journal = {DEMOCRACY \& SECURITY},
Year = {2011},
Volume = {7},
Number = {2, SI},
Pages = {140-159},
Abstract = {The question of international intervention, which emerged with full
   force in the 1990s conflicts in the Western Balkans, has taken center
   stage in world politics. This article analyzes the way humanitarian
   intervention was carried out in Bosnia and Kosovo, primarily from two
   perspectives: policy and strategy. It considers what issues were
   contested and why, and how their consideration has advanced and/or
   limited the humanitarian intervention debate. Presently, the perception
   of use of international institutions and norms to advance state
   interests other than humanitarian is likely to pose formidable
   challenges to mobilize sufficient international support for dealing with
   humanitarian emergencies.},
