@article{ WOS:000299627400014,
Author = {Kennelly, Jacqueline and Llewellyn, Kristina R.},
Title = {Educating for active compliance: discursive constructions in citizenship
   education},
Journal = {CITIZENSHIP STUDIES},
Year = {2011},
Volume = {15},
Number = {6-7},
Pages = {897-914},
Abstract = {This article examines the discursive construction of `active
   citizenship' within recent civics curriculum documents across three
   provinces in Canada. New secondary school civics curricula have emerged
   across liberal democratic states since the year 2000, presumably in
   response to the perception of youth as disengaged from political
   involvement. Many of the new curricula subsequently emphasize `active'
   engagement within the polity. The central task of this paper is to
   better understand what such `active citizenship' actually means, via the
   methodological tool of discourse analysis. Engaging a theoretical frame
   that incorporates Foucauldian governmentality theory and cultural
   theories of the role of the state in creating subjectivities, the paper
   ultimately argues that the `active citizen' of contemporary civics
   curricula is, in fact, a deeply neoliberal subject. The article then
   draws on feminist theories of citizenship in order to assess the forms
   of exclusion that the curriculum documents inadvertently create, arguing
   that they ultimately participate in a long tradition of devaluing such
   elements of citizenship as relationality and emotional ties. We conclude
   that one of the fundamental goals of citizenship education - to expand
   access to citizenship participation for all - has failed.},
