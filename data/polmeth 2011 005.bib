@article{ WOS:000294703400004,
Author = {Roca i Escoda, Marta},
Title = {From the Mobilization of Law to the Effective Implementation of Law: The
   Actions of the Catalan Association ``Lesbian and Gay Families{''}},
Journal = {POLITIX},
Year = {2011},
Volume = {24},
Number = {94},
Pages = {59+},
Abstract = {This paper aims at showing the reality of law, or ``law in action{''},
   far from a legal belief that takes regulations as a guarantee for
   effectiveness. Law in action has to take into account a range of
   mediation and devices to assure its implementation. To prove this, we
   will focus on the research that analyzes the mobilizations of a Catalan
   association of gay and lesbian families (FLG) in the context of the
   homosexual marriages in Spain and the institutionalization of homosexual
   couples in Catalonia. We will pay attention to the actions and
   situations in which law is referred, and these range from mobilization
   to the implementation of law.},
