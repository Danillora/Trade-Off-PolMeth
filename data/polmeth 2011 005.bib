@article{ WOS:000211989600006,
Author = {Brym, Michelle},
Title = {The enduring importance of national identity in cooperative European
   Union Borderlands: Polish university students' perceptions on
   cross-border cooperation in the Pomerania euro-region},
Journal = {NATIONAL IDENTITIES},
Year = {2011},
Volume = {13},
Number = {3},
Pages = {305-323},
Abstract = {European integration, through euro-regions, voluntary zones of
   cooperation across national borders, and the eastward expansion of the
   Schengen Zone have transformed Europe borderlands into sites of
   bi-national collaboration. This study evaluates how a group of Polish
   university students living in the Pomerania euro-region view the new
   ease of cross-border mobility and the types of cross-border activities
   they engage in. If the experience of students in the border city of
   Szczecin is reflective of other European borderlands, than cross-border
   cooperation has not diminished the importance of the border as a mental
   marker of nation-state boundaries or the importance of national
   identity.},
