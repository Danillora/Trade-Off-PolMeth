@article{ WOS:000218337300012,
Author = {Clarke, Michael and Soria, Valentina},
Title = {CHARGING UP THE VALLEY BRITISH DECISIONS IN AFGHANISTAN},
Journal = {RUSI JOURNAL},
Year = {2011},
Volume = {156},
Number = {4},
Pages = {80-88},
Abstract = {In 2006, the UK government deployed forces into Helmand as part of the
   ISAF expansion into southern Afghanistan. It did so with limited force
   levels; a strategic choice that was further criticised when the move `up
   the valley' into the north of the province left UK forces thinly spread
   across a large and dangerous region. Helmand operations have cost over
   360 lives, but there have been stabilisation gains. Nevertheless,
   important lessons remain to be learned about Britain's strategic
   policy-making from the decisions of 2005-06.},
