@article{ WOS:000299223500009,
Author = {McElroy, Gail and Marsh, Michael},
Title = {Electing Women to the Dail: Gender Cues and the Irish Voter},
Journal = {IRISH POLITICAL STUDIES},
Year = {2011},
Volume = {26},
Number = {4, SI},
Pages = {521-534},
Abstract = {At no time in history has the number of women elected to Dail Eireann
   surpassed 14 per cent of the total membership. In spite of significant
   social changes, the use of a proportional electoral system and no
   obvious bias among voters, the number of female TDs remains stubbornly
   low by international standards. This paper examines why, if the
   prospects for women's election are relatively good, so few women end up
   in public office. Using both aggregate and survey data, the issues of
   incumbency advantage, the electorate's attitudes and the candidates'
   differing experiences of the political process are explored. The
   evidence suggests that, all else equal, female candidates have as good a
   chance of being elected as their male counterparts, and the real
   difficulties in achieving equitable representation lie elsewhere, in the
   candidate emergence and nomination stages of the election game.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
