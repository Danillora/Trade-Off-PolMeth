@article{ WOS:000286682000009,
Author = {Makse, Todd and Volden, Craig},
Title = {The Role of Policy Attributes in the Diffusion of Innovations},
Journal = {JOURNAL OF POLITICS},
Year = {2011},
Volume = {73},
Number = {1},
Pages = {108-124},
Month = {JAN},
Abstract = {Studies of policy diffusion have given insufficient attention to the
   role that characteristics of the policies themselves play in determining
   the speed of policy diffusion and the mechanisms through which diffusion
   occurs. We adopt Everett Rogers' (1983, 2004) attribute typology from
   the diffusion of innovations literature and apply it to a sample of 27
   policy innovations from the sphere of criminal justice policy in the
   U.S. states between 1973 and 2002. We find that policy attributes,
   ranging from the relative advantage of the policy over its predecessors
   to its complexity to its compatibility with past practices, affect the
   likelihood of adoption. Furthermore, policy attributes shape the extent
   to which spatial adoption patterns and learning mechanisms are relevant
   to the policy's diffusion.},
