@article{ WOS:000291011400006,
Author = {Gartner, Scott Sigmund},
Title = {Signs of Trouble: Regional Organization Mediation and Civil War
   Agreement Durability},
Journal = {JOURNAL OF POLITICS},
Year = {2011},
Volume = {73},
Number = {2},
Pages = {380-390},
Month = {APR},
Abstract = {Practitioners and scholars have long championed the positive effects of
   mediation by regional organizations on conflict resolution-an influence
   much quantitative research fails to confirm. Mediation by regional
   organizations signals powerful selection effects that can lead to
   erroneous inferences about the factors observed to influence the
   duration of dispute peace agreements. In order to control for selection,
   I develop a theory that accounts for the nature of the conflict and the
   identity of third-party mediators. Theoretically and statistically
   controlling for the negative effects of selection on agreement
   durability evinced through mediation by regional organizations makes
   clear their positive process effects. That is, by taking into account
   that regional mediators get especially hard-to-manage disputes we can
   observe the positive ways that regional mediation contributes to dispute
   resolution. This study advances our understanding of conflict
   management, regional organizations, and civil war.},
