@article{ WOS:000441114300006,
Author = {Gallarotti, G. M.},
Title = {The Power Curse: the Paradox of Power in World Politics},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2011},
Number = {3},
Pages = {58+},
Abstract = {While the augmentation of power carries obvious benefits for nations,
   the very process itself possesses inherent qualities that can weaken a
   nation. Hence. power can be as much of a curse as a blessing because of
   these weakening effects. To the extent that national leaders do not
   correct for these effects. they are destined to become victims of a
   power illusion - i.e., that in reality their nations will become weaker
   and more vulnerable then they perceive.},
