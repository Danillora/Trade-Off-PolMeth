@article{ WOS:000286051900002,
Author = {Jensen, Carsten},
Title = {Conditional contraction: Globalisation and capitalist systems},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
Year = {2011},
Volume = {50},
Number = {2},
Pages = {168-189},
Month = {MAR},
Abstract = {The effect of globalisation on social spending is one of the most
   intensely studied issues in the political economy literature. Until
   recently, conventional wisdom held that globalisation leads governments
   to expand social spending to compensate workers for increasing risk
   exposure. The latest research shows, however, that globalisation has
   become strongly associated with spending cutbacks since the late 1980s.
   This article adds to this research by arguing that the negative impact
   of globalisation is conditioned on the capitalist system in different
   countries. In coordinated market economies (CMEs), employers are
   dependent on the willingness of the workforce to invest in specific
   skills and therefore become supportive of extensive social spending. Not
   so in liberal market economies (LMEs), where employers are much less
   dependent on social spending because the workforce in general invests
   less in specific skills. Employers in LMEs are therefore likely to use
   increasing globalisation as a means to push through retrenchment,
   whereas employers in CMEs are not. This argument is tested in a
   time-series cross-section regression analysis, which clearly supports
   it.},
