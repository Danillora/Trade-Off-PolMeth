@article{ WOS:000290858400002,
Author = {Akcinaroglu, Seden and DiCicco, Jonathan M. and Radziszewski, Elizabeth},
Title = {Avalanches and Olive Branches: A Multimethod Analysis of Disasters and
   Peacemaking in Interstate Rivalries},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2011},
Volume = {64},
Number = {2},
Pages = {260-275},
Month = {JUN},
Abstract = {Multimethod analysis of earthquakes' effects in two enduring rivalries
   demonstrates that natural disaster can promote rapprochement, political
   steps toward warmer relations that make it difficult for interstate
   rivalry to continue. Public expression of compassion and support for
   rapprochement create audience costs for leaders who otherwise would
   maintain hostile policies toward the rival state. However, routine
   violence, including communal violence, discourages public support for
   postdisaster cooperation and rapprochement. Content analysis and
   time-series analysis of rivalry change in two cases, India-Pakistan and
   Greece-Turkey, demonstrate these phenomena, and comparative case study
   analysis shows that communal violence helps account for divergent
   outcomes between the two cases.},
