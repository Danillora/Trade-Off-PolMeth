@article{ WOS:000442717100013,
Author = {Teterin, A. Ye.},
Title = {Application of Qualitative Methods in Political-Science Research (with
   N.Fairclough's Critical Discourse Analysis as Example)},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2011},
Number = {5},
Pages = {145+},
Abstract = {Qualitative methods are ever wider adopted in political science. The
   fact may be explained by distinctive features both of the qualitative
   paradigm and of the object of political science - political reality. One
   of the methods of the qualitative paradigm that has received wide
   recognition, is media-political discourse-analysis. Norman Fairclough's
   three-dimensional model of critical discourse-analysis helps to avoid
   some typical mistakes of a discource researcher. Illustrated (as
   example) with Internet-media analysis of a communication, this model
   demonstrates its own wide research potential extending far beyond the
   limits of concrete discourse practice.},
