@article{ WOS:000212980800005,
Author = {Southwell, Priscilla L.},
Title = {Letting the Counties Decide: Voter Turnout and the All-Mail Option in
   the State of Washington},
Journal = {POLITICS \& POLICY},
Year = {2011},
Volume = {39},
Number = {6},
Pages = {979-996},
Month = {DEC},
Abstract = {This research examines county-level voter turnout in the state of
   Washington-a state where a majority of its counties have chosen to adopt
   an all-mail electoral format over the past decade. Using the nearest
   neighbor match method to control for demographic and partisan variation
   among counties, this analysis of turnout in eight general and primary
   elections from 2004 to 2008 suggests that vote-by-mail has resulted in a
   modest boost in voter turnout during presidential election years and for
   special elections, but has little or no effect in congressional off-year
   elections.},
