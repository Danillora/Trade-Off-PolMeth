@article{ WOS:000286552700001,
Author = {de Vries, Catherine E. and Tillman, Erik R.},
Title = {European Union issue voting in East and West Europe: The role of
   political context},
Journal = {COMPARATIVE EUROPEAN POLITICS},
Year = {2011},
Volume = {9},
Number = {1},
Pages = {1-17},
Month = {FEB},
Abstract = {Recent research has shown the rise of domestic contention over European
   integration. This paper examines the extent to which preferences over
   European integration influence domestic party support in 19 European
   Union (EU) member states in West and East-Central Europe (ECE). The
   analysis finds broad evidence of EU issue voting across the countries
   included in the analysis, but the effect of the EU issue on party
   preferences is stronger in ECE. These results are consistent with the
   view that the same underlying causal dynamics explain party and voter
   behavior in both West and ECE, but the post-communist legacy shapes the
   political and economic contexts in the ECE states, resulting in
   predictable differences between the two regions. Comparative European
   Politics (2011) 9, 1-17. doi:10.1057/cep.2009.7},
