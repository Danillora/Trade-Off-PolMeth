@article{ WOS:000421293400003,
Author = {Perla Aronson, Paulina},
Title = {Relentless, elusive and fascinating modernity. Sociology and literature},
Journal = {TEMAS Y DEBATES},
Year = {2011},
Number = {21},
Pages = {55-69},
Month = {AUG},
Abstract = {The purpose of this writing is to recapture the contributions of writers
   and sociologists about anomie, displacement, alienation and
   estrangement, both in general terms as in their disturbing and even
   perverse particularities. It intends to register the intersection
   between literary views - comprehensive horizon that often inspires
   social scientists-and distictively sociological conceptualizations. To
   this end, we take a few sources from both fields, so as to discern the
   features of modern distance between freedom and organization and
   identify, at the same time, the conceptual knots of what might be called
   late modernity, issues that both sociologists and writers strive to
   elucidate.},
