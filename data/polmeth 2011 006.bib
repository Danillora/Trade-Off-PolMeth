@article{ WOS:000292899600004,
Author = {Gallas, Alexander and Nowak, Joerg},
Title = {The Crisis of the Democracies: Elective Autocracy, Empty-Handed Class
   Political Strategies, Culturalisation},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2011},
Volume = {40},
Number = {2},
Pages = {139+},
Abstract = {In this article, we sketch out a materialist account of the systematic
   connections between capitalism and democracy, and assess, with reference
   to Britain and Germany, the impact of the current economic crisis on the
   scope of democratic decision-making. In our view, the infrastructure of
   class-based political representation is being removed, which means that
   political strategies taking on board subaltern demands are being blocked
   systematically. The result is a state of elective autocracy: governments
   adopt `empty-handed' class political strategies centred on (a)
   legitimising cuts to state expenditure by invoking fear of fiscal and
   economic crises, and (b) disciplining immigrant groups that purportedly
   hold illiberal values.},
