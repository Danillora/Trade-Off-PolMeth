@article{ WOS:000298126000014,
Author = {Murr, Andreas Erwin},
Title = {``Wisdom of crowds{''}? A decentralised election forecasting model that
   uses citizens' local expectations},
Journal = {ELECTORAL STUDIES},
Year = {2011},
Volume = {30},
Number = {4},
Pages = {771-783},
Month = {DEC},
Abstract = {Many studies report the ``wonders of aggregation{''} and that groups
   (often) yield better decisions than individuals. Can this ``wisdom of
   crowds{''}-effect be used to forecast elections? Forecasting models in
   first-past-the-post systems need to translate vote shares into seat
   shares by some formula; however, the seat-vote ratio alters from
   election to election. To circumvent this problem, this paper proposes
   citizen forecasting, which aggregates citizens' local expectations to
   directly forecast constituencies. Using data from the 2010 British
   Election Study, this paper finds (1) that groups are better forecasters
   than individuals, (2) that citizen forecasting correctly predicts a hung
   parliament, and (3) that marginality and group size are important
   predictors for ``getting it right{''}. (C) 2011 Elsevier Ltd. All rights
   reserved.},
