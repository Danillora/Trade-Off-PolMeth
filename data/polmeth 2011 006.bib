@article{ WOS:000209059700002,
Author = {Koenig, Helmut},
Title = {Paradoxes of Memory On Knowledge and Forgetting},
Journal = {OSTEUROPA},
Year = {2011},
Volume = {61},
Number = {4},
Pages = {43+},
Month = {APR},
Abstract = {Forgetting violence and injustice was long seen as a condition for
   sustainable peace after a war or civil war. But the amnesty clause is
   only realistic where certain rules of war were upheld or were at least
   not systematically abused. The First World War already lies beyond its
   field of application, the National Socialists' war of annihilation all
   the more. Wherever people cannot forget, only remembrance remains.
   Remembrance, however, stands in an indissoluble and paradoxical
   relationship with forgetting: only what is previously known - remembered
   - can actively be forgotten afterwards.},
