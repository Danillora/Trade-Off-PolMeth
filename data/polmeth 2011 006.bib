@article{ WOS:000289330900003,
Author = {Eaton, Kent},
Title = {Conservative Autonomy Movements Territorial Dimensions of Ideological
   Conflict in Bolivia and Ecuador},
Journal = {COMPARATIVE POLITICS},
Year = {2011},
Volume = {43},
Number = {3},
Pages = {291+},
Month = {APR},
Abstract = {Three related arguments can be made about the autonomy drives that have
   gathered strength in the last decade in the most economically vibrant
   subnational regions in Bolivia and Ecuador. First, based on analysis of
   the actions and actors associated with each, these phenomena are
   classifiable as ``conservative autonomy movements.{''} Second, the
   disjuncture between the concentration of political power in national
   capitals and economic power in vibrant subnational regions explains why
   these movements emerged in Bolivia and Ecuador but not elsewhere in
   Latin America and why they have emerged now and not earlier in each
   country. Third, the mobilizing structures that these twin movements draw
   on, as well as the framing choices that each has made, account for the
   greater strength of the autonomy movement in Bolivia.},
