@article{ WOS:000217307200001,
Author = {Nadeau, Richard and Foucault, Martial and Lewis-Beck, Michael S.},
Title = {Assets and risk: A neglected dimension of economic voting},
Journal = {FRENCH POLITICS},
Year = {2011},
Volume = {9},
Number = {2},
Pages = {97-119},
Month = {JUN},
Abstract = {Economic voting studies have been dominated by the classic
   reward-punishment paradigm, in which voters vote for the incumbent under
   good economic performance, but against under bad. This paradigm works
   well when the economic issue is a valence issue, such as prosperity.
   However, it leaves out positional economic voting, in which the voter's
   place in the economic structure influences policy preference, and thus
   party preference. More precisely, we suggest that the better the
   economic location of voters in terms of assets, high-risk assets in
   particular, the more they will vote right, because the right promises a
   better return on their investments. We demonstrate this effect in French
   presidential election data, from three national surveys - 1988, 1995 and
   2002. This assets effect well exceeds other economic effects tested, and
   does so under strong statistical controls.},
Publisher = {PALGRAVE MACMILLAN LTD},
