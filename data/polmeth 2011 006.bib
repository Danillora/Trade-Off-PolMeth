@article{ WOS:000421202100008,
Author = {Kaya, Ayhan},
Title = {o MIGRATION DEBATES IN EUROPE: MIGRANTS AS ANTI-CITIZENS},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2011},
Volume = {10},
Number = {1},
Pages = {79-91},
Month = {SPR},
Abstract = {This article tries to reveal the recent debates revolving around the
   issue of migration in Europe, and claims that migration has recently
   become securitized and stigmatized in a way that prompts the
   autochthonous societies to generate a set of migrant-phobic attitudes.
   This work further argues that this kind of fear does not really have
   material sources; it is on the contrary an artificial fear fabricated by
   the conservative political elite who are likely to use the politics of
   fear as a form of governmentality to sustain their power. Hence, this
   work will specifically concentrate on the processes of othering migrant
   origin individuals as anti-citizens.},
