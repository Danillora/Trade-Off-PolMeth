@article{ WOS:000212105100003,
Author = {Debnath, Angela},
Title = {British perceptions of the East Pakistan Crisis 1971: `hideous
   atrocities on both sides'?},
Journal = {JOURNAL OF GENOCIDE RESEARCH},
Year = {2011},
Volume = {13},
Number = {4, SI},
Pages = {421-450},
Abstract = {British intelligence reports from 1971 describe how Pakistan suffered
   acute instability after central government forces attempted to brutally
   quash the East Pakistan nationalist movement, sparking guerrilla
   resistance and extensive social disorder. Much of the British media,
   political opposition and public sided with the nationalists, accusing
   government forces of violating human rights, and even of genocide. The
   British government, in contrast, declared it would not interfere in
   `hideous atrocities ... being committed on both sides'. While intent on
   neutrality and non-interference, officials continued deliberating on how
   to react to the crisis as it evolved. As a result, British
   responses-which extended to aid and diplomacy-were shaped by a number of
   contextual factors, including: the nature of diplomatic reports from the
   region, public and parliamentary pressure, Cold War geopolitical
   constraints and, as expected, strategic calculations of Britain's
   interests on the subcontinent. Reflecting both self-interest and genuine
   concern, British responses to the East Pakistan crisis highlight the
   complexities in comprehending multi-layered nationalist conflict, which
   government observers readily perceive as civil war.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
