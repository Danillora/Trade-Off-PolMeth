@article{ WOS:000212742100002,
Author = {Jackson, Nigel},
Title = {Perception or Reality: How MPs Believe the Internet Helps Them Win Votes},
Journal = {JOURNAL OF POLITICAL MARKETING},
Year = {2011},
Volume = {10},
Number = {3},
Pages = {230-250},
Abstract = {The orthodox view is that neither the election campaign nor the
   activities of local Members of Parliament (MPs) has any effect on the
   final outcome of UK general elections. However, more recent research has
   suggested that both the election campaign and the activity of individual
   MPs has an effect. This article seeks to ascertain whether MPs believe
   that the Internet can help them gain votes. Data were collected from a
   questionnaire and interviews with MPs. The findings suggest that MPs
   believe that a Web site has an indirect rather than direct impact on
   voting behavior. However, MPs believe that regular e-newsletters between
   elections may increase their vote and that the use of private e-mail
   lists during an election helps mobilize supporters and volunteers.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
