@article{ WOS:000293151800009,
Author = {Pudal, Romain},
Title = {Becoming a Firefighter: Ethnography of a Secondary Socialization},
Journal = {POLITIX},
Year = {2011},
Volume = {24},
Number = {93},
Pages = {167+},
Abstract = {In this article, I intend to analyse the notion of secondary
   socialization through a specific case study: the French firefighters.
   This work is based on an in-depth ethnographic inquiry, and I describe
   the different steps of this social and professional process. I aim to
   provide a better understanding of socialization by examining the role of
   institutions, the ``learning by doing{''} process, the incarnate
   intelligence and the representations of this universe. By reconstructing
   the practical logic of this social life, I show how a specific habitus -
   becoming a firefighter - is built day after day.},
