@article{ WOS:000421202100012,
Author = {Panagis, Germenis},
Title = {RISKS OF APPEASEMENT: THE FAILURE OF THE EAST AND THE WEST IN THE
   MUHAMMAD CARTOON CONTROVERSY},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2011},
Volume = {10},
Number = {1},
Pages = {121-132},
Month = {SPR},
Abstract = {The 12 editorial cartoons of the Danish newspaper, Jyllands-Posten, in
   2005 should not be considered an insult to the Muslim world. Therefore,
   appeasement to extreme reactions by the Muslim community, such as the
   apology of the Politiken newspaper in 2010, does nothing more than
   magnify the problem of admitting and imposing self-censorship. There is
   a great need to redefine what we call freedom of speech and make clear
   that respect and tolerance do not imply deprivation of our most basic
   freedom: to articulate critical opinions freely.},
