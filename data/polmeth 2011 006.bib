@article{ WOS:000212632900009,
Author = {Borchert, Jens and Stolz, Klaus},
Title = {Institutional Order and Career Patterns: Some Comparative Considerations},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2011},
Volume = {21},
Number = {2, SI},
Pages = {271-282},
Abstract = {This article tries to sum up the findings of the country articles and
   put them into a comparative perspective. The major finding is one of
   great diversity among prevailing career patterns in multi-level
   polities. This in itself is considered an advance over frequent
   assumptions about career patterns based on the US case alone. Looking
   for causal explanations the article discards some institutional
   hypotheses. It therefore calls for more empirical research on
   institutional configurations, the time sequence in which institutions
   have emerged historically, and the perception and interpretation of
   institutional arrangements by politicians.},
