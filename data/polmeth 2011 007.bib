@article{ WOS:000209059800004,
Author = {Bingen, Dieter},
Title = {Neighbourly Relations and Partnership: Germany and Poland 1991-2011},
Journal = {OSTEUROPA},
Year = {2011},
Volume = {61},
Number = {5-6},
Pages = {23+},
Month = {MAY-JUN},
Abstract = {Relations between Germany and Poland have changed fundamentally. The
   spirit of reconciliation initially led to a paternalistic relationship:
   Germany cleared Poland's way into NATO and the EU. This ``junior
   partnership{''} soon came to an end. Significant differences in
   interests became clear in the debates over the Iraq conflict, the EU
   constitution, the Baltic Sea pipeline, and the expellee centre. The
   result was irritation and estrangement. Now this has passed. The
   partnership has obtained a quality in which differences are no longer
   denied, but conflicts can be discussed constructively. Moreover, Germany
   and Poland are key countries for the political integration of Europe as
   a whole.},
