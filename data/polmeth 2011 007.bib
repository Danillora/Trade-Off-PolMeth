@article{ WOS:000309292900005,
Author = {Zuern, Michael},
Title = {Four Models of a Global Order With Cosmopolitan Intent},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2011},
Volume = {52},
Number = {1},
Pages = {78-118},
Abstract = {The article is conceived as a contribution to `International Political
   Theory' and pursues two goals. The first is to systematize the
   many-sided discussion on a good global order. Four basic models of
   global order with cosmopolitan intent are reconstructed and the
   empirical premises built into each model identified: the
   intergovernmental model of global order, cosmopolitan pluralism,
   cosmopolitan federalism, and cosmopolitan democracy. The empirical
   evidence for developments favouring these models is then considered.
   Secondly, the relationship between normative and empirical propositions
   is to be established in more general terms in order to promote the
   necessary debate between political theory and international relations.
   It can be shown that differences in the empirical assessment underlying
   the models appear to be more important for the design of a global order
   than differences in the justification-theoretical procedures or moral
   principles behind them.},
