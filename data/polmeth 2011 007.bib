@article{ WOS:000421212200011,
Author = {Younis, Mohamed S.},
Title = {TURKISH DELIGHT? THE FEASIBILITY OF THE ``TURKISH MODEL{''} FOR EGYPT},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2011},
Volume = {10},
Number = {4},
Pages = {107-115},
Month = {WIN},
Abstract = {While comparisons and arguments for why the Turkish Model is relevant to
   burgeoning MENA countries now undergoing transition abound, the
   infeasibilities of ``selling{''} this model have been less explored.
   This article makes the argument that positing Turkey's successes in the
   last decades as a ``model{''} for transitioning MENA countries is
   counter-productive. The author examines this concept primarily through
   the lens of Turkey-Egypt relations. The first section discusses the
   feasibility of the Turkish model from the perspective of Egyptians
   themselves; the second section discusses the ideological challenges and
   discrepancies between the AKP and Egypt's eclectic hues of Islamists,
   including some revealing gender constituency challenges for Egypt's two
   largest Islamist political parties. Finally, the author offers some
   policy recommendations to the proponents of the Turkish model in
   engaging a transitioning Egypt.},
