@article{ WOS:000291065900013,
Author = {Berg-Schlosser, Dirk},
Title = {political science between vision and reality: lessons in times of crises},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2011},
Volume = {10},
Number = {2},
Pages = {259-271},
Month = {JUN},
Abstract = {Political science with its rich history, but varying national traditions
   and contexts, deals with a multi-dimensional and ever-changing subject
   matter of which we are, inevitably, a part. This poses specific
   epistemological problems, but also offers the opportunity to contribute
   to the shaping of political reality by insights and actions. This
   lecture gives a brief outline of this pro-blematique and then presents,
   by way of illustration, the findings of a major international research
   project on the political effects of the Great Depression in Europe in
   the interwar period. Based on this experience, some (tentative and
   personal) lessons will be drawn for the state of political science and
   its potential contributions facing the present world economic crisis.},
