@article{ WOS:000289273000003,
Author = {Werner, Alban},
Title = {Democratic Theory in the Age of Post-neoliberal Austerity},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2011},
Volume = {40},
Number = {1},
Pages = {21+},
Abstract = {The recent signs of crisis in contemporary democracies are rooted in
   structural properties of bourgeois societies. These are marked by
   dynamics of capitalist accumulation and formal organisations as actors
   in societal contexts. Under present conditions of fiscal austerity,
   social forces have not achieved to impose political compromises against
   interests of the economic elite. Hegemonic politics have been replaced
