@article{ WOS:000209059800009,
Author = {Lentz, S. and Herfert, G. and Bergfeld, A.},
Title = {In the Slipstream of Europeanisation: The German-Polish Border Area},
Journal = {OSTEUROPA},
Year = {2011},
Volume = {61},
Number = {5-6},
Pages = {79+},
Month = {MAY-JUN},
Abstract = {German-Polish border regions are struggling with serious structural
   problems. While there are some urban centres of growth, a downward
   spiral is underway in many areas. Economic weakness, job loss, declining
   demand, aging, and migration reinforce one another. More cross-border
   cooperation, especially in education and infrastructure, is the only way
   to break this vicious cycle. However, this means overcoming numerous
   administrative, structural, and mental obstacles.},
