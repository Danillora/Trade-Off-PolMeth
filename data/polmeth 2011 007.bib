@article{ WOS:000211568400005,
Author = {Ayson, Robert},
Title = {An Australasian school of strategic studies? Hedley Bull and changing
   international and regional systems},
Journal = {GLOBAL CHANGE PEACE \& SECURITY},
Year = {2011},
Volume = {23},
Number = {1},
Pages = {47-57},
Abstract = {To the extent that there is a distinctively Australasian school of
   strategic studies it is to be found in the work of Hedley Bull, a
   scholar who is more widely noted for his work as an international
   relations theorist. Bull's lesser known but influential contributions to
   strategic studies dealt with the management of nuclear weapons in
   international strategic relationships and the management of great power
   relations in the Asia-Pacific region. His work in these areas, while
   less well known than some of his other writings, deserves contemporary
   attention as strategic change in Asia increasingly shapes the
   international strategic picture, thereby creating the opportunity for
   Australasian strategic analysts to influence the global debate.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
