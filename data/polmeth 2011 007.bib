@article{ WOS:000210238900005,
Author = {Barajas, Luz Cristina S. and Barten, Francoise},
Title = {A Grey Area of Rights and Knowledge: Displacement in Colombia,
   South-South Migration and Health Equity},
Journal = {DESAFIOS},
