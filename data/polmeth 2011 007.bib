@article{ WOS:000291781600010,
Author = {Shabad, Goldie and Slomczynski, Kazimierz M.},
Title = {Voters' perceptions of government performance and attributions of
   responsibility: Electoral control in Poland},
Journal = {ELECTORAL STUDIES},
Year = {2011},
Volume = {30},
Number = {2, SI},
Pages = {309-320},
Month = {JUN},
Abstract = {We test the claim that elections function as a mechanism of
   accountability in the case of Poland, a new democracy with a fluid party
   system. We ask whether individuals' vote intentions are based on
   assessments of governing parties' performance. Taking into consideration
   attributions of responsibility for such performance, accountability
   exists if assessments of poor performance decrease the probability of
   voting for a ruling party. We use two criteria of performance: perceived
   change in unemployment rate and level of corruption between two
   consecutive elections. Using data from the Polish POLPAN panel survey,
   our results confirm the existence of a heterogeneous sanctioning model.
   (C) 2011 Elsevier Ltd. All rights reserved.},
