@article{ WOS:000218332500005,
Author = {Hansen, Stig Jarle},
Title = {DEBUNKING THE PIRACY MYTH HOW ILLEGAL FISHING REALY INTERACTS WITH
   PIRACY IN EAST AFRICA},
Journal = {RUSI JOURNAL},
Year = {2011},
Volume = {156},
Number = {6},
Pages = {26-31},
Abstract = {Somali pirates have claimed, and some Western analysis repeated the
   idea, that illegal fishing has been an underlying cause and
   justification for piracy. But pirate behaviour is not consistent with
   this idea, and the claim that piracy started as a form of coast guard,
   protecting Somali fisheries, is dubious. Rather, piracy is motivated by
   the quest for profit. Nevertheless, illegal fishing narratives do
   contribute to the pirates' local legitimacy, and need to be undermined
   in order to fight piracy.},
