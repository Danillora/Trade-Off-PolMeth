@article{ WOS:000215057100014,
Author = {Osinakachukwu, Nwokeke P. and Jawan, Jayum A.},
Title = {The Electoral Process and Democratic Consolidation in Nigeria},
Journal = {JOURNAL OF POLITICS AND LAW},
Year = {2011},
Volume = {4},
Number = {2},
Pages = {128-138},
Month = {SEP},
Abstract = {This paper examines election rigging and its effect on democratic
   consolidation in Nigeria relying on qualitative approach, using data
   gathered from secondary sources. It will be analyzed via content and
   historical analysis. Election riggings in Nigeria have dealt a huge blow
   to the development of democracy in Nigeria. Nigeria's elections so far
   have not live up to expectations due to various forms of electoral
   manipulations which have denied the country credible free and fair
   elections in her body politic. Election rigging has not only marred
   democratic consolidation in Nigeria but has also violated the
   fundamental human right (right to life) of so many Nigerians. Election
   riggings since independence reviewed in this paper will be our bench
   mark. The study found that election riggings in Nigeria have resulted
   into the imposition of corrupt and illegitimate leaders who have no
   regard to the principles of democracy which are off-shoots for good
   governance, rule of law, constitutionalism and fundamental human rights.
   This paper provides some important recommendations that would serve as a
   panacea to the problems of election riggings and how those measures can
   help necessitate a democratic environment which is a precondition for
   free and fair elections.},
