@article{ WOS:000212635100005,
Author = {Hombrado, Angustias},
Title = {Learning to Catch the Wave? Regional Demands for Constitutional Change
   in Contexts of Asymmetrical Arrangements},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2011},
Volume = {21},
Number = {4-5, SI},
Pages = {479-501},
Abstract = {Constitutional reforms affecting the asymmetrical allocation of powers
   between the constituent units of a federal or quasi-federal state have
   been generally studied as a bilateral relationship between the federal
   government and the region(s) asking for special treatment. In contrast,
   this paper examines the crucial role that non-specially empowered
   regions can play in these processes by raising anti-asymmetry reactions
   in the form of `catching-up' and `blocking' demands. A theoretical
   argument is developed concerning the causal mechanism linking several
   relevant conditions together (type of asymmetry, the distribution of
   national identities across regions, relative economic development and
   party politics) and lying between them and the alternative outcomes.},
