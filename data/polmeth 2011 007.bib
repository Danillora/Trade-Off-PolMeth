@article{ WOS:000294078800019,
Author = {MacMullen, Ian},
Title = {On Status Quo Bias in Civic Education},
Journal = {JOURNAL OF POLITICS},
Year = {2011},
Volume = {73},
Number = {3},
Pages = {872-886},
Month = {JUL},
Abstract = {Should civic education in a democracy cultivate children's beliefs in
   the substantive merits of existing laws and political institutions? Such
   status quo educational bias can be both a barrier to political progress
   and a threat to the state's legitimacy. Against these objections, I aim
   to show how and when adults' fallible beliefs about the substantive
   merits of existing political arrangements constitute compelling reasons
   for status quo educational bias. And I argue that the values of
   stability, compliance, and patriotism underpin important reasons for
   status quo bias that appeal not to the substantive merits of existing
   institutions but merely to the fact that those institutions are the
   status quo.},
