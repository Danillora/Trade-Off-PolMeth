@article{ WOS:000214821700003,
Author = {Apostolidis, Paul},
Title = {Sex scandals, racial domination and the systemic correlation of
   power-modalities in Foucault},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2011},
Volume = {4},
Number = {2},
Pages = {179-198},
Abstract = {This essay explores relations between discourses of sexuality and race
   in US society today through an analysis of recent sex scandals, in a
   manner informed by Foucault and provoking further critical development
   of his theory. Sex scandal narratives demonstrate how sovereignty,
   discipline, and biopolitics currently combine to form a systemic
   correlation of power-technologies. This power matrix, in turn, enables
   strategies of racial domination through the war on terror, immigration
   control, and economic crisis management. Sex scandals both help bring
   these racial power-dynamics into view and reproduce them by fortifying
   the concatenation of power-modalities on which they rely.},
