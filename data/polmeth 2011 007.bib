@article{ WOS:000217600500006,
Author = {Theophanous, Andreas},
Title = {Ethnic Identity and the Nation-State in the Era of Globalization: the
   Case of Cyprus},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
Year = {2011},
Volume = {24},
Number = {1-2, SI},
Pages = {45-56},
Month = {JUN},
Abstract = {This paper addresses the philosophical question whether the nation-state
   will be entering a final phase of demise or whether it will continue to
   prosper within the framework of the era of regional blocs and
   globalization. In order to do so, we briefly put forward some
   theoretical premises in relation to the nation-state and globalization
   and how they have affected each other. This discussion also draws from
   the recent experiences of the international economic crisis and how the
   EU reacted in relation to the serious economic crisis of Greece. We also
   assess the case of Cyprus within the framework of this discussion. The
   question raised in relation to Cyprus is whether there can be an
   integrated society that encourages a particular common value system and
   also respects the ethnic identity of citizens and communities. A major
   question is whether the EU can offer the framework for such an outcome.},
