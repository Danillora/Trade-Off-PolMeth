@article{ WOS:000294184000016,
Author = {Bellinger, Jr., Paul T. and Arce, Moises},
Title = {Protest and Democracy in Latin America's Market Era},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2011},
Volume = {64},
Number = {3},
Pages = {688-704},
Month = {SEP},
Abstract = {Existing studies hold that Latin America's market turn has had a
   demobilizing effect on collective political activity despite the
   presence of democracy. However, recent work has documented the revival
   of protest in the region, emphasizing the repoliticization of collective
   actors in the wake of economic liberalization, especially when democracy
   is present. This article expands the theoretical scope of the
   repoliticization perspective, providing the most comprehensive test of
   the demobilization and repoliticization hypotheses to date. Using
   time-series data from seventeen Latin American countries, the article
   confirms the repoliticization view by showing that protest increases
   with economic liberalization in democratic settings.},
