@article{ WOS:000292899600003,
Author = {Sauer, Birgit},
Title = {``Only Paradoxes to offer?{''} Feminist Theories of Democracy and
   Representation in the Era of ``Postdemocracy{''}},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2011},
Volume = {40},
Number = {2},
Pages = {125+},
Abstract = {The article first describes the specifications of the feminist political
   science debates about political representation in recent years. However,
   the article claims that these concepts do not grasp recent developments
   in liberal democracies. Hence, the article second discusses critical
   debates about representation in deconstructivist and post-colonial
   traditions. These strands of critique point to three shortcomings of
   hegemonic representation, namely the identity dimension, the dimension
   of social inequality and the dimension of domination. The article
   develops a materialist-feminist approch to representation which suggests
   new spaces of politics and new mechanisms of political participation in
   order to dynamise a feminist notion of democracy.},
