@article{ WOS:000300172100006,
Author = {Hochet, Peter},
Title = {A ``peasant citizen{''}? Comparative ethnography between the Minyanka
   and the Bwaba (Mali, Burkina Faso)},
Journal = {CITIZENSHIP STUDIES},
Year = {2011},
Volume = {15},
Number = {8, SI},
Pages = {1031-1045},
Abstract = {Africanist studies of citizenship tend to approach the issue only from
   the point of view of the state and its relations with organised
   `minorities'. On the contrary the interface between African states and
   relatively structured and autonomous rural societies invites us to
   consider plural citizenships in terms of circulation of semi-autonomous
   political projects, scales of production, contradiction and connection.
   Two ethnographic studies on the distribution of connections between land
   rights and collective obligations between `autochthonous populations'
   and `strangers', conducted amongst the Minyanka (Mali) and the Bwaba
   (Burkina Faso), allow us to show the extent to which rural societies
   develop a local definition of citizenship different from the formal one.
   In addition to the relativisation of universalistic definitions of
   citizenship, this `rural' form of citizenship invites us to reconsider
   non-normatively the classical dichotomy between public and private.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
