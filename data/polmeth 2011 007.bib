@article{ WOS:000420472700005,
Author = {Rumpala, Yannick},
Title = {From the objective of ``sustainable development{''} to the
   governmentalization of change. The expressions and effects of a renewed
   institutional concern in France and the European Union},
Journal = {POLITIQUE EUROPEENNE},
Year = {2011},
Volume = {33},
Number = {1},
Pages = {119-153},
Abstract = {The objective of ``sustainable development{''} contains a form of
   transformative ambition which, faced with the highlighted planetary
   dangers, has tended to push change up to the status of collective
   purpose that would need to be taken on and accompanied by appropriate
   institutions. Based on the government activities that have begun to
   carry out this goal, this article aims to analyze how rationalities,
   devices and procedural arrangements are assembling and helping to make
   the management of change a renewed stake in the institutional sphere. In
   order to understand its logics and orientations, this process of
   ``governmentalization{''} of change is studied from the initiatives of
   French public authorities and the European Union, by examining this
   interpretation of change as a problem, the range of both programmatic
   and instrumental by-products that take the form of documents called
   ``strategies{''}, and the procedural foundations that are beginning to
   provide support.},
