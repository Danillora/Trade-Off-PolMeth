@article{ WOS:000210622200003,
Author = {Brown, Matthew},
Title = {Marx for Medievalists: Rethinking Feudalism and Historicism in
   Capital},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2011},
Volume = {23},
Number = {1},
Pages = {12-21},
Month = {JAN},
Abstract = {This article assesses the value of Marx's Capital for medieval
   historians and literary historicists. The author objects to a widespread
   interpretation of the Marxian concept of feudal exploitation on the
   grounds that it introduces an anachronistic notion of ownership. He
   argues that an approach to historicism closer to that pursued by Marx in
   Capital would permit more productive scholarly work on medieval history
   and social justice. He also argues that Capital raises questions about
   the value of conventional historicism similar to those that are
   presently of concern to many medievalists.},
