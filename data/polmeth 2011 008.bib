@article{ WOS:000208623900004,
Author = {Berlinski, Samuel and Dewan, Torun},
Title = {The Political Consequences of Franchise Extension: Evidence from the
   Second Reform Act},
Journal = {QUARTERLY JOURNAL OF POLITICAL SCIENCE},
Year = {2011},
Volume = {6},
Number = {3-4},
Pages = {329-376},
Abstract = {We use evidence from the Second Reform Act, introduced in the United
   Kingdom in 1867, to analyze the impact on electoral outcomes of
   extending the vote to the unskilled urban population. Exploiting the
   sharp change in the electorate caused by franchise extension, we
   separate the effect of reform from that of underlying constituency level
   traits correlated with the voting population. Although we find that the
   franchise affected electoral competition and candidate selection, there
   is no evidence relating Liberal electoral support to changes in the
   franchise rules. Our results are robust to various sources of
   endogeneity.},
