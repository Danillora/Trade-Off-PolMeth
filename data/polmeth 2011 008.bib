@article{ WOS:000298830200003,
Author = {Clar, Christoph},
Title = {No Space for Austria. The Lack of Critical Concepts of Space After 1945
   and Beyond and its Impact on Austrian School Teaching},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2011},
Volume = {40},
Number = {4},
Pages = {357+},
Abstract = {Social sciences increasingly pay attention to spatial components of
   society. They study the social production of space, as well as its
   significance for social practice. Following the political (mis)use of
   concepts of space by the National Socialists, German-speaking social
   sciences are criticized for having been afraid of even using the term
   `space' after 1945. The missing reception of social science approaches
   dealing with the social production of (political) space has had effects
   on different areas of application, e.g. the design of Austrian school
   teaching. In combination with the structural and personal continuity of
   the Austrian research and education landscape, this resulted in a lack
   of critical communication of existing spatial relations and conditions.
   This article examines possible connections between respective historical
   scientific developments and the contents of Austrian school teaching.},
