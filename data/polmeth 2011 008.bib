@article{ WOS:000210009400006,
Author = {Cecilia Gueemes, Maria},
Title = {State and social capital in Latin America: In which way the features and
   actions of the State explain the social capital levels in Latin America?},
Journal = {AMERICA LATINA HOY-REVISTA DE CIENCIAS SOCIALES},
Year = {2011},
Volume = {59},
Pages = {91-116},
Month = {DEC},
Abstract = {By focusing on the importance of social capital to development, this
   paper examines the role of the State and Public Policy in the creation
   of social capital. After a brief summary about social capital
   definitions, weakness and usefulness, its relevance to development and
   its problematic creation, the paper pays specific attention to the role
   of the State as a promoter of social capital. Second we bear out the
   empiric links between State performance and social capital in Latin
   America. Then, we use state's efficacy indicators, variables
   traditionally associated with welfare stat (especially public spending
   and inequality) and interpersonal trust. We finish by pointing the
   previous findings and shedding light in order to continue the research
   about social capital reproduction.},
