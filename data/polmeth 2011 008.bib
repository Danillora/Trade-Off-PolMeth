@article{ WOS:000218332500003,
Author = {Donald, Dominick},
Title = {SANCTUARY, SHIPOWNERS AND PAYING RANSOMS REFOCUSING COUNTER - PIRACY
   POLICY IN AN ERA OF AUSTERITY},
Journal = {RUSI JOURNAL},
Year = {2011},
Volume = {156},
Number = {6},
Pages = {12-19},
Abstract = {The threat posed by Somali pirates has heightened over the past four
   years, and the mechanisms currently at the disposal of the international
   community can only have limited effects. Here Dominick Donald argues
   that in the short-term, counter-piracy policy must focus on one key
   issue - sanctuary; engage one key actor - shipowners; and avoid one key
   temptation - banning ransoms. This approach may not provide the ultimate
   solution, but will at least allow the international community to
   successfully contain piracy.},
