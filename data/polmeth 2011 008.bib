@article{ WOS:000214666500006,
Author = {Brien, Kevin M.},
Title = {Marx's radical humanism},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2011},
Volume = {1},
Number = {2},
Pages = {186-203},
Abstract = {The different varieties of orthodox Marxism project a radical
   philosophical split between the work of the early and the late Marx.
   However, the argument of this paper rejects the view that there is a
   sharp radical split between the early and late Marx with respect to his
   basic philosophical positions; and it argues that the projection of such
   a philosophical split amounts to a radical distortion of Marx's thought.
   The main methodological instrument for carrying out this argument is
   Marx's mature dialectical method of explanation which he employed
   throughout his monumental work, Capital - that is, the method of
   dialectical explanation that moves from the abstract to the concrete.
   This method of explanation involves the interplay of two fundamental
   canons of interpretation that distinguish this explanatory movement -
   the interrelated notions of the concrete universal and the internal
   relation. The argument proceeds to sketch an explanatory theoretical web
   which allows one to discern how the perspectives of the early and late
   Marx can be synthesized in a coherent way; and how Marx's
   philosophical/scientific paradigm not only provides conceptual room for,
   but also calls for a full-fledged radical humanism.},
