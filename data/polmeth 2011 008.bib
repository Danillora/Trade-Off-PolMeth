@article{ WOS:000288226300011,
Author = {Lipson, Daniel N.},
Title = {The Resilience of Affirmative Action in the 1980s: Innovation,
   Isomorphism, and Institutionalization in University Admissions},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2011},
Volume = {64},
Number = {1},
Pages = {132-144},
Month = {MAR},
Abstract = {This article applies neoinstitutional organization theory to uncover the
   central role of university officials in institutionalizing aggressive,
   race-based affirmative admissions procedures at three selective public
   universities from the late 1970s until the early 1990s. During this
   second stage of affirmative action, admissions and diversity officials
   at the University of California, Berkeley, the University of Texas at
   Austin, and the University of Wisconsin-Madison began to increasingly
   emphasize the diversity rationale and the method of individualized
   review. At a time of increasing judicial and executive scrutiny and
   skepticism of affirmative action, university officials defended and
   transformed race-conscious admissions in innovative ways when they could
   have instead chosen to contribute to its demise.},
