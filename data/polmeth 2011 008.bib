DOI = {10.1177/1065912910379226},
ISSN = {1065-9129},
EISSN = {1938-274X},
Keywords = {party platforms; candidate positioning; voter perceptions; elections},
Keywords-Plus = {STEREOTYPES; COMPETITION; STRATEGY; MODEL},
