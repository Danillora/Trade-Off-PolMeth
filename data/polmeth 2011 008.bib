@article{ WOS:000288746300001,
Author = {Azrout, Rachid and van Spanje, Joost and de Vreese, Claes},
Title = {Talking Turkey: Anti-immigrant attitudes and their effect on support for
   Turkish membership of the EU},
Journal = {EUROPEAN UNION POLITICS},
Year = {2011},
Volume = {12},
Number = {1},
Pages = {3-19},
Month = {MAR},
Abstract = {Recent studies have shown that the most important factor explaining
   opinions on European Union issues is attitudes towards immigrants. Two
   arguments are given to explain this effect. We contend that these
   arguments are both built on the idea that people with anti-immigrant
   attitudes frame other Europeans as an out-group. We then test the
   validity of these arguments by measuring how respondents in a voter
   survey frame the issue of Turkish membership. We find that framing the
   issue in terms of out-groups indeed mediates the effect of
   anti-immigrant attitudes on support for Turkish membership. This finding
   offers new insights into why levels of public support vary over
   different EU issues, because opposition is likely to increase when an
   issue is more easily framed in terms of out-groups.},
Publisher = {SAGE PUBLICATIONS LTD},
