@article{ WOS:000298830200007,
Author = {Durnova, Anna},
Title = {Fieldwork Intimately: Transgressing Experiences in Meanings in
   Interpretive Policy Analysis},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2011},
Volume = {40},
Number = {4},
Pages = {417+},
Abstract = {Interpretive approaches to policy analysis give a key role to meanings
   that institutions and actors exchange when they formulate or implement
   policy agendas. By highlighting the concept of `discourse', these
   approaches include a careful analysis of text and communication. This
   paper discusses the nature of interpretive practice and it focuses,
   through the notion of `Sprachmodus', on the transgression of experience
   during fieldwork towards the analytical category of meaning. Since
   meanings are negotiated, the negotiation appears also as an
   epistemological dimension during the analysis. The exposition of
   Sprachmodus is further illustrated on the emotionally loaded debate
   about the `end of life law' in France between 2003 and 2005. The
   adjective `intimately' has a double meaning here: as a reference to
   emotionally loaded issues that become object of policy analysis; and as
   an insight in the strategies of interpretive researchers.},
