@article{ WOS:000443240700005,
Author = {Popovic, Petar},
Title = {THE CONTEMPORARY CRITIQUE OF THE INTERNATIONAL RELATIONS LIBERAL THEORY:
   THE STATE AND THE `INTERDEPENDENCE' PARADIGM},
Journal = {POLITICKA MISAO-CROATIAN POLITICAL SCIENCE REVIEW},
