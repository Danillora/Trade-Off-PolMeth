@article{ WOS:000211989600005,
Author = {Kulyk, Volodymyr},
Title = {The media, history and identity: competing narratives of the past in the
   Ukrainian popular press},
Journal = {NATIONAL IDENTITIES},
Year = {2011},
Volume = {13},
Number = {3},
Pages = {287-303},
Abstract = {The article studies history-related texts in three popular newspapers as
   contributions to the construction of historical memory and national
   identity in post-Soviet Ukraine. After a quantitative thematic analysis
   of history pages in terms of their geographical and chronological
   priorities, I focus on representations on these and other pages of one
   crucial event of the past: World War Two. The analysis demonstrates that
   in Ukrainian media, the past in general and the last war in particular
   has been a subject of incessant but largely inconspicuous competition
   between the opposing versions of Ukraine's history which I call the
   Soviet and nationalist narratives.},
