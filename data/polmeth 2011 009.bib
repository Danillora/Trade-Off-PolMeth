@incollection{ WOS:000288751600016,
Author = {Farrall, Kenneth},
Editor = {Maret, S},
Title = {SUSPICIOUS ACTIVITY REPORTING: US DOMESTIC INTELLIGENCE IN A POSTPRIVACY
   AGE?},
Booktitle = {GOVERNMENT SECRECY},
