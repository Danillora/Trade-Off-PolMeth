@article{ WOS:000300816400008,
Author = {du Cleuziou, Yann Raison},
Title = {A generation hiding another. Generational differentiations and
   reformative mobilizations in the French Province of the Dominican Order
   at the time around May 68},
Journal = {POLITIX},
Year = {2011},
Volume = {24},
Number = {96},
Pages = {115+},
Abstract = {The crisis in the Catholic clergy in the 1960s-70s has been interpreted
   as a consequence of the generational differentiation of socialization
   ways from the sacerdotal institution. This article reviews this
   interpretation and proposes to think the generation conflict not only as
   the outcome of a generational habitus' struggle, but also as a specific
   battle arena erected by actors explicitly claiming generational
   interests. Indeed, a generation is not merely the product of a specific
   socialization way, it is a social group built out of speeches and
   practices of actors who seize this identification category for
   mobilization purposes. From the example of the Dominican Province of
   France, this article shows how the habitus of clergymen differentiate in
   the 1960s and 1970s, and then how the Order will be plunged into crisis
   in the name of the ``Youngs{''} by a generation of Dominicans accessing
   to leading positions and considering the May 68 uprising as an occasion
   to implement the reforms they wished for.},
