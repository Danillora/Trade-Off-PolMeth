@article{ WOS:000296265300015,
Author = {Albright, Jeremy J.},
Title = {Privacy Protection in Social Science Research: Possibilities and
   Impossibilities},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2011},
Volume = {44},
Number = {4},
Pages = {777-782},
Month = {OCT},
Abstract = {The ubiquity of data in the twenty-first century provides unprecedented
   opportunities for social science research, but it also creates troubling
   possibilities for privacy violations. The emerging field of statistical
   disclosure control (SDC) studies how data collectors and analysts can
   find an optimal solution to balancing privacy protection and data
   utility. This article introduces SDC to readers in the applied
   political. science research community and outlines its implications for
   analyzing individual-level data. The vocabulary of SDC is introduced and
   is followed with a discussion emphasizing just how easy it is to break
   almost any release of supposedly ``anonymized{''} data. The article then
   describes how SDC measures almost always destroy the ability of
   researchers to accurately analyze complex survey data. These results are
   in conflict with increasing trends toward greater transparency in the
   social sciences. A discussion of the future of SDC concludes the
   article.},
