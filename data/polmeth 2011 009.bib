@article{ WOS:000286881700002,
Author = {Markowitz, Lawrence P.},
Title = {Unlootable Resources and State Security Institutions in Tajikistan and
   Uzbekistan},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2011},
Volume = {44},
Number = {2},
Pages = {156-183},
Month = {FEB},
Abstract = {Why, when faced with similar conditions of weakening central control, do
   some institutions of state security fragment into autonomous agents of
   organized violence whereas others cohere around coercive rent seeking
   without challenging the central government? Focusing on Tajikistan and
   Uzbekistan, this article explains these divergent state security
   outcomes as a consequence of resource concentrations and patronage
   pressures that influence the political elites who leverage local offices
   of state security. The article finds that privatizing violence within
   state apparatuses of Tajikistan and Uzbekistan took very different forms
   in the 1990s.},
