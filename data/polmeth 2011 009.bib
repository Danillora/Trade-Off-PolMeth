@article{ WOS:000210623700008,
Author = {Mulder, Catherine P.},
Title = {Wal-Mart's Role in Capitalism},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2011},
Volume = {23},
Number = {2},
Pages = {246-263},
Month = {APR},
Abstract = {Using a Marxian class analysis of Wal-Mart and its workers, this article
   analyzes the term exploitation and its often incorrect usage. Given
   their roles within this merchant capitalist enterprise, Wal-Mart workers
   are frequently heralded by scholars and activists as the poster children
   of ``exploitation.'' This is a misconception. Uncovered here are the
   specific class and nonclass positions that Wal-Mart and its workers
   occupy, which determine whether the workers are indeed exploited in the
   Marxian sense of the term. The intention is to demonstrate and suggest
   possible alliances between Wal-Mart workers and other groups that might
   enable them to improve their working conditions and attain a more
   democratic workplace.},
