@article{ WOS:000299907500008,
Author = {Viti, Fabio},
Title = {From slavery to dependency in Africa},
Journal = {PENSEE},
Year = {2011},
Number = {368},
Pages = {81+},
Month = {OCT-DEC},
Abstract = {Fabio Viti, From slavery to dependency in Africa
   Even after the end of slavery, the African social context is still
   deeply marked by relations of personal dependence. They surface in
   unpaid communal and familial work, a renitence to make use of wage
   labour, and the spread of unpaid employment (for instance
   apprenticeship). A convergence of several different factors resulted in
   generalized long-term devaluation of the work and efforts of producers,
   who never acquired a real status as workers. This picture is not the
   result of an unlikely revival of slavery; it is rather the effect of a
   wholesale failure in developing wage work and the persistence of
   pre-modern forms of personal dependence.},
