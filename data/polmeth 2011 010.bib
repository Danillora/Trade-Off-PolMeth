@article{ WOS:000420543300004,
Author = {Fink, Simon},
Title = {The impact of varieties of capitalism on privatization policy. A
   quantitative test of varieties of capitalism hyptheses},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2011},
Volume = {5},
Number = {2},
Pages = {277-301},
Month = {DEC},
Abstract = {The `varieties of capitalism' approach contributes to the explanation of
   varying privatization trajectories due to the assumption of
   institutional complementarities between coordinated market economies and
   state-owned enterprises. This hypothesis is corroborated by a
   quantitative analysis of privatizations in 19 OECD countries from 1978
   to 2008. Overall, coordinated market economies have privatized fewer
   sectors and have maintained greater state ownership of the shares (even
   when controlling for indicators of corporatism). Thus, the quantitative
   analysis provides additional support to the hypothesis which previously
   has mostly been tested using qualitative methods.},
