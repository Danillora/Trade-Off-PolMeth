@article{ WOS:000296027400003,
Author = {Hummel, Patrick},
Title = {Pre-election polling and sequential elections},
Journal = {JOURNAL OF THEORETICAL POLITICS},
Year = {2011},
Volume = {23},
Number = {4},
Pages = {463-479},
Month = {OCT},
Abstract = {I consider a model in which the winner of a primary election faces a
   third candidate in a general election immediately thereafter. Prior to
   the primary election, there is a pre-election poll on how voters would
   vote in a hypothetical general election between one of the candidates in
   the primary election and the third candidate. I illustrate that voters
   have an incentive to misrepresent their voting intentions in the
