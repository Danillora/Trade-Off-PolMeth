@article{ WOS:000288226300006,
Author = {Djupe, Paul A.},
Title = {Political Pluralism and the Information Search: Determinants of Group
   Opinionation},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2011},
Volume = {64},
Number = {1},
Pages = {68-81},
Month = {MAR},
Abstract = {It is a necessary condition that people have an opinion about groups
   before they can use groups to help make sense of the political world.
   Beyond individual resources and motivation, the strength of social ties
