@article{ WOS:000298444200010,
Author = {Deplaude, Marc-Olivier},
Title = {State Xenophobia? ``Foreign Doctors{''} in France (1945-2006)},
Journal = {POLITIX},
Year = {2011},
Volume = {24},
Number = {95},
Pages = {207+},
Abstract = {Xenophobia is the main foundation of the legal barriers which were set
   up between the late nineteenth century and the 1930s to deter foreigners
   from practicing medicine in France. Should we therefore consider that
   this xenophobia embedded in the law is the main explaining factor of the
   confinement of doctors coming from the former French colonies and
   protectorates in low status jobs within the medical field, and of the
   public decisions which were taken about them before regularization
   measures in the late 1990s? Using sociological and historical materials,
   this paper provides some answers to this question by analyzing the
   evolution of the situation of ``foreign doctors{''} and of the debates
   and measures about this issue between post-war years and the mid-2000s.},
