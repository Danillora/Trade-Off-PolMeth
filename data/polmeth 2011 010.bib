@article{ WOS:000442717400013,
Author = {Pastukhov, V. B.},
Title = {Prodrome of Civil War. From ``Nomenclature{''} to ``Kleptoclature{''}},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2011},
Number = {6},
Pages = {143+},
Abstract = {At first glance, Russia produces the impression of a ``classical{''}
   failed state. But, in actual fact, for the whole extent of its history
   it has been demonstrating capacity for ``creative evolution{''} -
   capacity altogether uncharacteristic of failed states. Russia's
   outstanding ``cultural productivity{''} has been partly conditioned by
   availability of a unique ``compensatory{''} political mechanism
   counterbalancing the chronic weakness of Russian state institutions;
   this mechanism may be admittedly termed ``inner state{''}. Its
   destruction in late 1980s brought about continuous stagnation in
   society's and the state's development.},
