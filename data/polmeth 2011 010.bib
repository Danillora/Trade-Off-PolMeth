@article{ WOS:000289273000002,
Author = {Joerke, Dirk},
Title = {Democracy in New Spaces},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2011},
Volume = {40},
Number = {1},
Pages = {7+},
Abstract = {The article compares the current debate on democratization of the
   processes of global governance with controversies in context of the
   American Revolution. The comparison of these two discussions shows three
   analogies. Firstly, a reconfiguration of the basic democratic
   institutions and practices occurs in both debates, notably in the change
   from a radical into a representative respectively from a representative
   into a deliberative understanding of democracy. Secondly, in both cases
   the reconfiguration is attended by a semantic transformation of the
   concept of democracy. Furthermore, in each case the transfer of
   democracy to a new political order is attended by a weakening of the
   mechanisms of popular control.},
