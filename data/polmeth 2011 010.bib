@article{ WOS:000217309200003,
Author = {Gougou, Florent and Labouret, Simon},
Title = {Participation in the 2010 French regional elections: The minor impact of
   a change in the electoral calendar - A reply to Fauvelle-Aymar},
Journal = {FRENCH POLITICS},
Year = {2011},
Volume = {9},
Number = {3},
Pages = {240-251},
Month = {SEP},
Abstract = {This article is a reply to a recent contribution to this journal arguing
   that the dramatic decrease in turnout in the first round of the 2010
   French regional elections was the consequence of a change in the
   electoral calendar: unlike the previous regional elections, which were
   held on the same day as other elections, the regional elections of 2010
   took place in isolation. In our opinion, this view is not sustained by
   the facts. We argue that the impact on turnout of the isolation of the
   2010 regional elections was only minor. We suggest alternative
   hypotheses that might explain the sharp decline in voter turnout that
   happened in 2010, especially in the light of the results of the 2011
   cantonal elections.},
