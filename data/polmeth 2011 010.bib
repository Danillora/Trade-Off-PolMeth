@article{ WOS:000286051400006,
Author = {Casillas, Christopher J. and Enns, Peter K. and Wohlfarth, Patrick C.},
Title = {How Public Opinion Constrains the US Supreme Court},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
Year = {2011},
Volume = {55},
Number = {1},
Pages = {74-88},
Month = {JAN},
Abstract = {Although scholars increasingly acknowledge a contemporaneous
   relationship between public opinion and Supreme Court decisions, debate
   continues as to why this relationship exists. Does public opinion
   directly influence decisions or do justices simply respond to the same
   social forces that simultaneously shape the public mood? To answer this
   question, we first develop a strategy to control for the justices'
   attitudinal change that stems from the social forces that influence
   public opinion. We then propose a theoretical argument that predicts
   strategic justices should be mindful of public opinion even in cases
   when the public is unlikely to be aware of the Court's activities. The
   results suggest that the influence of public opinion on Supreme Court
   decisions is real, substantively important, and most pronounced in
   nonsalient cases.},
