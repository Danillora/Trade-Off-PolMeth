@article{ WOS:000421209200009,
Author = {Knaus, Gerald},
Title = {LESSONS IN HUMILITY: INTERVENTION FROM SARAJEVO TO KABUL},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2011},
Volume = {10},
Number = {3},
Pages = {83-91},
Month = {FAL},
Abstract = {In 1995, some observers and policymakers might have expected that the
   international deployment in Bosnia would be an exceptional event. It
   soon became obvious, however, that Bosnia was only the first in a
   succession of ambitious military interventions and post-conflict
   missions: first in Kosovo (1999) and East Timor (1999), then, in
   Afghanistan (2001), and finally in Iraq (2003). It became commonplace to
