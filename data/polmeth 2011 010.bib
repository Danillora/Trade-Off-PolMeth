@article{ WOS:000209060100003,
Author = {Volkov, Denis},
Title = {In a Jam Russian Civil Society},
Journal = {OSTEUROPA},
Year = {2011},
Volume = {61},
Number = {10},
Pages = {47+},
Month = {OCT},
Abstract = {An empirical study in six Russian cities shows that the question whether
   an active civil society exists is anachronistic. It exists and is slowly
   growing. Non-governmental organisations and initiatives are engaged in
   many areas. But in everyday life, they run up against numerous problems.
   Financing is precarious, co-operation with state agencies threatens
   their autonomy, and repression is always present. But the civil sector
   has also failed so far to develop a strategy to embrace the interests of
   social groups ``from below{''}, articulate them, and transform them into
   a durable stable social movement or even stable organisations.},
