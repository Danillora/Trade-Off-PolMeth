@article{ WOS:000288088300001,
Author = {Abrams, Samuel and Iversen, Torben and Soskice, David},
Title = {Informal Social Networks and Rational Voting},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2011},
Volume = {41},
Number = {2},
Pages = {229-257},
Month = {APR},
Abstract = {Classical rational choice explanations of voting participation are
   widely thought to have failed. This article argues that the currently
   dominant Group Mobilization and Ethical Agency approaches have serious
   shortcomings in explaining individually rational turnout. It develops an
   informal social network (ISN) model in which people rationally vote if
   their informal networks of family and friends attach enough importance
   to voting, because voting leads to social approval and vice versa. Using
   results from the social psychology literature, research on social groups
   in sociology and their own survey data, the authors argue that the ISN
   model can explain individually rational non-altruistic turnout. If group
   variables that affect whether voting is used as a marker of individual
   standing in groups are included, the likelihood of turnout rises
   dramatically.},
