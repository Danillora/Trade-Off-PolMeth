@article{ WOS:000289273000005,
Author = {Sack, Detlef},
Title = {Post-democracy, Irony and Justive - How to Deal with the Potential of
   Disappointment of Democratic Reign},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2011},
Volume = {40},
Number = {1},
Pages = {49+},
Abstract = {The article deals with the postulate of irony within the theoretical
   debates on democracy. Vantage point of the argument is the
   disappointment immanent to democracy. The current discussion about
   post-democracy depicts the manifest shortcomings of established
   democracies triggering frustration. The inevitability of disappointment
   is being traced back to the divergent strands and models of democracy.
   Since they compete with each other and struggle with internal dilemmas
   respectively they do not accomplish a consensual and stable foundation
   of the democratic rule. Conflicts between and internal dilemmas of
   models of democracy are explored in a comparison of the cosmopolitan
   strand of the debate and the agonistic pluralism. In view of the
   immanent potential for disappointment, the postulate of irony and its
   renaissance is discussed as a governmental requirement for the limited
   alienation of the political subject and for the committed distance to
   democracy. Since irony is not entirely determined in its emancipative or
   regressive character, the institutional embedding of the governmental
   requirement becomes crucial. Referring to the dual concept of social
   justice by Fraser, the argument accentuates that irony should be framed
   by rules supporting recognition and redistribution.},
