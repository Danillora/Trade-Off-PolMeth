@article{ WOS:000218343500005,
Author = {Meyer, Paul},
Title = {CYBER-SECURITY THROUGH ARMS CONTROL AN APPROACH TO INTERNATIONAL
   CO-OPERATION},
Journal = {RUSI JOURNAL},
Year = {2011},
Volume = {156},
Number = {2},
Pages = {22-27},
Abstract = {The risk of cyber-warfare is growing. With a low-level technological
   requirement, it is a form of warfare that can be prosecuted by military
   and civilian, state and non-state actors alike. Its anonymity makes it
   difficult to trace perpetrators, complicating inter-state relations. But
   cyberspace is not yet an active battleground for cyber-warfare, and
   could still be amenable to conflict prevention and restraint measures.
   The time has come to adopt an `arms control approach' to cyber-security.},
