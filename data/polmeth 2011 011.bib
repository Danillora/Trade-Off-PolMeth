@article{ WOS:000212100000005,
Author = {Takayoshi, Ichiro},
Title = {Can philosophy explain Nazi violence? Giorgio Agamben and the problem of
   the `historico-philosophical' method},
Journal = {JOURNAL OF GENOCIDE RESEARCH},
Year = {2011},
Volume = {13},
Number = {1-2},
Pages = {47-66},
Abstract = {This article critically evaluates Italian philosopher Giorgio Agamben's
   Homo Sacer, especially the book's central thesis that the mechanism of
   `inclusive exclusion' was at the origin of Nazi violence. There is
   little factual basis to support Agamben's philosophical arguments, I
   find. His failures and errors organically derive from his methodological
   stance-what he calls the `historico-philosophical method'. This method
   is premised on his monisic view of NS violence that imagines a
   primordial oneness subtending all differing instances of
   Nazi-perpetrated mass killing in discourse, cause, purpose and the
   Knower. In the conclusion I explain how my close-up study of this
   particular philosopher yields useful pointers, both methodological and
   ethical, for further investigation into the global question announced in
   the title of this article, `Can philosophy explain Nazi violence?'},
