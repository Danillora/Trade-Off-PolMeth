@article{ WOS:000297592500005,
Author = {McCarthy, Stephen},
Title = {Soldiers, chiefs and church: unstable democracy in Fiji},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2011},
Volume = {32},
Number = {5, SI},
Pages = {563-578},
Month = {NOV},
Abstract = {The qualities of democracy in Fiji are strongly influenced by ethnic
   divisions and indigenous sources of power and legitimacy in society.
   Periods of constitutional democracy interrupted by successive coups
   garnering conflicting support suggest that a more stable Fijian
   democracy requires a delicate balance of tribal, religious, ethnic and
   military interests. Successful democratic and governance reform requires
   the inclusive deliberation of all major groups in civil and political
   society, and not merely one that purports to represent all. Only by
   improving the qualities of democracy in Fiji will Fijian politics emerge
   from its cycle of coups and offer a more stable form of government.},
