@article{ WOS:000211358200002,
Author = {Brunkhorst, Hauke},
Title = {Critique of Dualism: Hans Kelsen and the Twentieth Century Revolution in
   International Law},
Journal = {CONSTELLATIONS-AN INTERNATIONAL JOURNAL OF CRITICAL AND DEMOCRATIC
   THEORY},
Year = {2011},
Volume = {18},
Number = {4},
Pages = {496-512},
Month = {DEC},
Abstract = {In the following paper I strive for two goals. First I try to
   contextualize Kelsens theory of law in the revolutionary context of the
   first half of the 20th century (I, II, III). This hopefully will
   disclose the internal political, polemical, and even revolutionary
   dimension of Kelsens not that pure theory (II, III). Kelsen himself has
   blocked this historical and political access to his theory because of a
   still too dualistic distinction between normative and scientific
   propositions. This is a bit ironical because of his sharp criticism of
   the metaphysical dualism of German Staatsrecht. But it is due to his
   neo-Kantian sources and the empiricist and semanticist climate of Vienna
   at the turn of the 20th century (V). Second I try to reconstruct his
   criticism of the dualisms of German Staatsrecht systematically (what
   Kelsen never did) and rationally (II, IV, V). The latter means to
   reconstruct Kelsens critique of dualism in a critical distance to his
   own far too narrow idea of a scientific justification that conceives
   ``justification{''} simply as deductive or formal logical inference plus
   empirical observation, based on a strict (and again still too dualistic)
   difference between logical operations and empirical observations.
   Reading Kelsen as a fundamental critic of dualism brings him much closer
   to philosophers such as Dewey or even the young Heidegger than usually
   viewed (V).},
