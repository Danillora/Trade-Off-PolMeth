@article{ WOS:000304987100005,
Author = {Fawcett, Paul and Manwaring, Rob and Marsh, David},
Title = {Network Governance and the 2020 Summit},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2011},
Volume = {46},
Number = {4},
Pages = {651-667},
Abstract = {The idea that governance has replaced government, and that networks have
   replaced hierarchy as the dominant mode of governance, have become
   mainstream views in the public policy literature. In this article, we
   consider a key initiative of the Rudd government in Australia, the 2020
   Summit, which, at first sight, seems like an example of network
   governance in action. Having considered the operation and outcomes of
   the Summit, however, we argue that it more accurately illustrates how
   governments, perhaps particularly in Westminster systems, attempt to
   preserve hierarchy, through a process of metagovernance.},
