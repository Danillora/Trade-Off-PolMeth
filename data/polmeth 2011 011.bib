@article{ WOS:000297884000006,
Author = {Kerby, Matthew},
Title = {Combining the Hazards of Ministerial Appointment AND Ministerial Exit in
   the Canadian Federal Cabinet},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2011},
Volume = {44},
Number = {3},
Pages = {595-612},
Month = {SEP},
Abstract = {The Canadian federal cabinet stands out among Westminster parliamentary
   democracies because of the large number of first-time ministers who are
   appointed to cabinet without any previous parliamentary or political
   experience. Several explanations have been put forward to account for
   this peculiarity but no attempt has been made to examine how Canadian
   prime ministers overcome the information deficit associated with
   appointing ministers with no experience. How can prime ministers be
   confident that they are making the right choice? This paper explores the
   subject by estimating the survival functions of ministerial turnover for
   potential, but not yet appointed, cabinet ministers were they to survive
   to a defined political benchmark; these survival rates are included in a
   logit model of Canadian ministerial appointment following four general
   elections (1957, 1979, 1984 and 2006) in which the prime minister was
   tasked with appointing a cabinet with ministerial neophytes.},
