@article{ WOS:000210287900002,
Author = {Maiz, Ramon},
Title = {Equality, Sustainability and Ecological Citizenship},
Journal = {FORO INTERNO-ANUARIO DE TEORIA POLITICA},
Year = {2011},
Volume = {11},
Pages = {13-43},
Abstract = {In contrast with the usual separation of green political theories from
   theories of equality, here we propose an egalitarian understanding of
   the principle of sustainability. Along with responsibility towards
   future generations and respect for the limits of ecosystems, the concept
   of sustainability should include as key aspects the equality of
   opportunities for well-being within a country, and between north and
   south. The replacement of the hypothesis of abundance with one of
   scarcity requires a refocusing of the debates on equality to include the
   theoretical plane of sustainability and its requirements. This implies a
   concept of ecological-egalitarian citizenship that is very distinct from
   the idea of liberal environmental citizenship.},
