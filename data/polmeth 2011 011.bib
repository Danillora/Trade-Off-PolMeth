@article{ WOS:000215506200005,
Author = {Iraola, Iker and Mateos, Txoli and Zabalo, Julen},
Title = {Discourses on Integration. The Inmigration to the Basque Country from
   the fifties to the seventies},
Journal = {REVISTA DE INVESTIGACIONES POLITICAS Y SOCIOLOGICAS},
Year = {2011},
Volume = {10},
Number = {3},
Pages = {115-130},
Abstract = {Migrants from Spain to the Basque Country between 1950 and 1980 brought
   about a massive change in demographics: by the mid seventies natives
   with autochthonous parents accounted for half the population. The
   political context in which said migratory movement developed gave rise
   to an ongoing debate in Basque society on the integration of these
   migrants. This article aims at analysing the subjective integration of
   said migrants, that is, the migrants' own perception of this process. It
   would appear that migrants underline the social dimension of the
   integration, over and above political and cultural aspects.},
