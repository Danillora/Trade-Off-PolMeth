@article{ WOS:000295384500002,
Author = {Knibiehler, Yvonne},
Title = {Motherhood in democracy},
Journal = {PENSEE},
Year = {2011},
Number = {367},
Pages = {13+},
Month = {JUL-SEP},
Abstract = {Yvonne Knibiehler, Motherhood in democracy
   The definition of democracy, a masculine political experience, has
   ignored for a long time motherhood, a feminine experience, which seemed
   to be a matter of private life. But the access of women to citizenship,
   and freedom to give birth, has changed things. The woman who chooses to
   be a mother imposes life knowingly to the ``desired child{''}. She must
   ``breed{''} (in the highest sense of the word) a future citizen, and
   take care of the world where he will live. She must both as a woman and
   as a mother take part in political life in democracy.},
Publisher = {PENSEE},
Address = {64 BD AUGUSTE-BLANQUI, 75013 PARIS, FRANCE},
Type = {Article},
