@article{ WOS:000435231600003,
Author = {Huber, Daniela},
Title = {Civil Society Against Sanction Busters in Iran: The Cases of Germany and
   Austria},
Journal = {DEMOCRACY \& SECURITY},
Year = {2011},
Volume = {7},
Number = {1},
Pages = {38-56},
Abstract = {Sanction busting of companies is one of the major hurdles for collective
   action on the international level today. Yet, in democracies, a new
   trend is emerging and such sanction busters are increasingly fought in
   the arena of civil society. Research on sanction busting, however, often
   neglects the phenomenon that advocacy groups are more and more
   successful in persuading companies to comply with their own values and
   identities. Asking the question under which conditions and how advocacy
   groups can encourage firms to voluntarily participate in international
   sanctions, this article hypothesizes that the higher the moral authority
   of advocacy groups and the better they are able to win support by
   domestic and foreign governments for their cause, the more likely they
   will be successful in their pursuit. This hypothesis is tested with
   qualitative case studies of German and Austrian companies, which do
   business in Iran and which are stigmatized for this by German and
   Austrian advocacy groups.},
