@article{ WOS:000216064500002,
Author = {Kavanagh, William},
Title = {Identities on the Portuguese-Spanish border: permanence and
   transformation},
Journal = {GEOPOLITICAS-REVISTA DE ESTUDIOS SOBRE ESPACIO Y PODER},
Year = {2011},
Volume = {2},
Number = {1},
Pages = {23-50},
Abstract = {In order to understand the construction and re-creation of identities of
   those who live on a section of the Portuguese-Spanish border,
   specifically that part of the frontier where the northern Portuguese
   region of Tra-os-Montes meets the Spanish region of Galicia, this essay
   examines and attempts to interconnect three elements: the theories used
   by social scientists concerning the concepts of ``identity{''} and of
   ``borders{''}; the ideas (and stereotypes) of the ``Other{''} held by
   the Portuguese and the Spaniards throughout their history since the
   founding of Portugal in the twelfth century; and the ethnography, in
   both senses of product and of process, carried out by the author over
   the past two decades in villages and towns on the Luso-Spanish frontier,
   both before and after the Schengen Agreement.},
