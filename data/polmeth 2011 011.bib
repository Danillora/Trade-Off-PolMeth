@article{ WOS:000299627400005,
Author = {Selby, Don F.},
Title = {`Kat Mai Ploi' - bite and don't let go: motherhood and pursuits
   of justice in Thailand},
Journal = {CITIZENSHIP STUDIES},
Year = {2011},
Volume = {15},
Number = {6-7},
Pages = {711-733},
Abstract = {This article examines the long-term struggles of two Thai mothers to
   define and attain their visions of justice: one to recover land flooded
   by a state dam project and the other to defy a municipal order to
   demolish her house for a vehicle turn-around. In particular, it studies
   how tropes and institutions of motherhood and human rights articulate
   with, and provide social and political resources to one another as these
   two women fight to lay claim to traditions of rights specific to Thai
   society.},
