@article{ WOS:000289633500009,
Author = {Katz, Richard S.},
Title = {Democracy as a Cause of Electoral Reform: Jurisprudence and Electoral
   Change in Canada},
Journal = {WEST EUROPEAN POLITICS},
Year = {2011},
Volume = {34},
Number = {3, SI},
Pages = {587-606},
Abstract = {Unlike other policy-makers, courts are forced to give reasons for their
   decisions that are based in principles rather than raw interests. The
   jurisprudential democratic theory developed as courts decide cases
   concerning elections thus becomes an important source of electoral
   reform. Taking the case of Canada as an example, this article shows not
   only that courts can play a major role in shaping electoral practice,
   but also that they do so in a way that privileges expression over
   decision as the primary democratic purpose of elections.},
