@article{ WOS:000293102900004,
Author = {Belan, Pascal and Cheron, Arnaud},
Title = {Equilibrium Unemployment, General Human Capital Depreciation and Optimal
   Training Subsidy},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2011},
Volume = {121},
Number = {2},
Pages = {209-231},
Month = {MAR-APR},
Abstract = {We analyse the optimal subsidy scheme of continuous vocational training
   in general human capital, considering a frictional labor market with
   general human capital depreciation during unemployment spell. When
   matched with a worker, a firm does not ex ante observe whether the
   worker has up-to-date knowledge or not. The model allows to identify two
   externalities; their respective correlations with the human capital
   depreciation rate have opposite signs. We then show that a rise in the
   depreciation rate during unemployment spell does not necessarily imply
   higher training subsidies.},
