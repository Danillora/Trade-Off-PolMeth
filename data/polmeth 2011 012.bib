@article{ WOS:000209059800025,
Author = {Flis, Jaroslaw},
Title = {Stability Test: The Political Parties and Their Electorates},
Journal = {OSTEUROPA},
Year = {2011},
Volume = {61},
Number = {5-6},
Pages = {279+},
Month = {MAY-JUN},
Abstract = {Since 1989, there has been a crisis in almost every election period in
   Poland: collapses in coalitions, government reshuffles, and new
   elections were the result. At first glance, the Polish political
   landscape is confusing. The positions of the parties are determined by
   ideological differences as well as the social background of voters and
   the history of each region. While political positions and party
   electorates fluctuated wildly in the past, stabilization is now to be
   observed. In the autumn 2011 elections, there should be fewer surprises
   than before.},
