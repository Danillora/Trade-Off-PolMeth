@article{ WOS:000289336500005,
Author = {Schuck, Andreas R. T. and Xezonakis, Georgios and Elenbaas, Matthijs and
   Banducci, Susan A. and de Vreese, Claes H.},
Title = {Party contestation and Europe on the news agenda: The 2009 European
   Parliamentary Elections},
Journal = {ELECTORAL STUDIES},
Year = {2011},
Volume = {30},
Number = {1, SI},
Pages = {41-52},
Month = {MAR},
Abstract = {In this article we analyse the news coverage of the 2009 EP elections in
   all 27 EU member states (N = 52,009). We propose that the extent to
   which these second-order elections are salient to the media depends on
   political parties contesting the elections. Consistent with
   expectations, the findings suggest that the saliency of EP elections is
   increasing and that the degree of political contestation over Europe
   contributes to this development in a nonlinear fashion so that only when
   contestation develops beyond a certain point, does media coverage
   increase. (C) 2010 Elsevier Ltd. All rights reserved.},
Publisher = {ELSEVIER SCI LTD},
