@article{ WOS:000214046400002,
Author = {McAloney, Kareena},
Title = {Life after prison: the experiences of prison officers serving during the
   Troubles in Northern Ireland},
Journal = {BEHAVIORAL SCIENCES OF TERRORISM AND POLITICAL AGGRESSION},
Year = {2011},
Volume = {3},
Number = {1},
Pages = {20-34},
Abstract = {The men and women employed by the Northern Ireland prison service served
   through some of the most infamous events in the course of the
   `Troubles'. In 1998 a downsizing initiative was launched which resulted
   in over 1000 prison personnel accepting early retirement packages. This
   study aimed to explore the experience of service life in Northern
   Ireland and investigate how the circumstances of retirement impact upon
   well-being. Ninty-nine retired prison officers participated in the
   study, 80\% of whom were early retirees. Each participant completed
   several measures of well-being and a series of open-ended questions
   regarding their experiences of service life. Both early retirement from
   the prison service and the perception of being forced to retire were
   associated with greater psychological distress and lower self-esteem.
   Qualitative analysis revealed a high level of loyalty to the prison
   service and pride in the role of prison officer. However, participants
   also expressed feeling of rejection and disappointment in their
   subsequent treatment by the prison service and the government.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
