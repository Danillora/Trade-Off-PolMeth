@article{ WOS:000286328100007,
Author = {Letelier S, Leonardo E.},
Title = {Theory and evidence of municipal borrowing in Chile},
Journal = {PUBLIC CHOICE},
Year = {2011},
Volume = {146},
Number = {3-4},
Pages = {395-411},
Month = {MAR},
Abstract = {Although Chilean municipalities are not permitted to borrow, they do so
   through arrears and leasing contracts. A formal model of municipal
   indebtedness is estimated, based on a sample of 345 municipalities with
   yearly data from 2004 to 2007. Variables that are positively and
   significantly related to borrowing through arrears are: mayors closely
   tied to the ruling central government coalition; politically diverse
   municipal councils; years close to elections; and deeper municipal
   involvement in education. While leasing contracts appear to be
   insensitive to political factors, they are positively and significantly
   related to municipal staff managerial skills and municipal revenues per
   capita.},
