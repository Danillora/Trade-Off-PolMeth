@article{ WOS:000213003900005,
Author = {Noonan, Douglas S. and Baker, Paul M. A. and Seavey, Art and Moon,
   Nathan W.},
Title = {Where the Cathedrals and Bazaars Are: An Index of Open Source Software
   Activity and Potential},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2011},
Volume = {8},
Number = {3, SI},
Pages = {273-303},
Abstract = {This article presents a framework to measure activity and potential for
   open source software development and use at a country level. The
   framework draws on interviews with experts in the open source software
   industry and numerous existing studies in the literature to identify
   relevant indicators. Several indices of diverse variable lists and
   weighting and aggregation methods were developed and tested for
   robustness. The results provide a first step toward more systematically
   understanding the current state of open source software internationally.},
