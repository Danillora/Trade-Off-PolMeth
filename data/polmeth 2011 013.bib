@article{ WOS:000293163600001,
Author = {Roetheli, Tobias F.},
Title = {THE SUPERIORITY OF MONETARY OVER BARTER EXCHANGE: EXPERIMENTAL RESULTS
   AND POLICY IMPLICATIONS},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2011},
Volume = {58},
Number = {4},
Pages = {437-454},
Month = {SEP},
Abstract = {William Stanley Jevons suggested that monetary exchange is socially
   superior to barter exchange because agents' optimization is simplified
   by the use of money. We experimentally study how subjects perform under
   monetary and barter exchange and find that a majority of subjects
   achieve a higher utility level in the monetized economy. The individual
   choices are statistically analyzed in order to track important elements
   of suboptimal decision making like the tendency to under- or over-react
   to price signals. Our laboratory findings indicate that, at a minimum,
   government may have a role in promoting a common unit of account.},
