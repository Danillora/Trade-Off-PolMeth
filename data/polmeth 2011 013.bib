@article{ WOS:000300790000013,
Author = {Arce, Daniel G. and Bakshi, Sneha and Croson, Rachel T. A. and Eckel,
   Catherine C. and Fatas, Enrique and Kass, Malcolm},
Title = {Counterterrorism strategies in the lab},
Journal = {PUBLIC CHOICE},
Year = {2011},
Volume = {149},
Number = {3-4, SI},
Pages = {465-478},
Month = {DEC},
Abstract = {We conduct experiments to test the collective action dilemmas associated
   with defensive and proactive counterterror strategies. Defensive
   policies are associated with creating public `bads' (e. g., a commons)
   whereas proactive policies are akin to the voluntary provision of public
   goods. When combined, the inefficiency of collective action is
   exacerbated, resulting in a situation known as a Prisoner's Dilemma
   squared (PD2). Deterministic versus probabilistic equivalent versions of
   the associated externalities are compared within a laboratory setting.
   Experimental results reveal that the collective action problem
