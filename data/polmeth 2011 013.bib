@article{ WOS:000289606500015,
Author = {Markussen, Thomas},
Title = {Democracy, redistributive taxation and the private provision of public
   goods},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2011},
Volume = {27},
Number = {1},
Pages = {201-213},
Month = {MAR},
Abstract = {The paper studies in a simple, Downsian model of political competition
   the private provision of public goods embedded in a system of democracy
   and redistributive taxation. Results show that the positive effect of
   inequality on production of public goods, to which Olson (1965) pointed,
   is weakened and might even be reversed in this context. Also, the median
   voter may choose a negative tax rate, even if he or she is poorer than
   the mean, in order to stimulate production of public goods. The
   relevance of the model is illustrated with an application to the
   financing of higher education. (C) 2010 Elsevier B.V. All rights
   reserved.},
