@incollection{ WOS:000310749600004,
Author = {Georgalidou, Marianthi},
Editor = {Tsakona, V and Popa, DE},
Title = {``Stop caressing the ears of the hooded{''} Political humour in times of
   conflict},
Booktitle = {STUDIES IN POLITICAL HUMOUR: IN BETWEEN POLITICAL CRITIQUE AND PUBLIC
   ENTERTAINMENT},
Series = {Discourse Approaches to Politics Society and Culture},
Year = {2011},
Volume = {46},
Pages = {83-107},
Abstract = {Based on data coming from media and parliamentary settings, the purpose
   of this study is to examine instances of humour in the discourse of
   politicians during a period of social turmoil in Greece. The analysis
   confirms the impact of humorous contributions, as well as other forms of
   linguistic creativity, in constructing, and resisting the construction
   of, damaging political identities in the discourse of political
   opponents. It also confirms the symbiotic relations among politics and
   media, via the formation of dialogical networks, which are shown to
   contribute to the blurring of ideological boundaries and identities in
   politics.},
