@article{ WOS:000288516500004,
Author = {Benvenuti, Andrea},
Title = {Difficult Partners: Indo-Australian Relations at the Height of the Cold
   War, 1949-1964},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
