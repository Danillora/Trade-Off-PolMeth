@article{ WOS:000300333100004,
Author = {Rouillon, Sebastien},
Title = {A Price-Quantity Mechanism to Implement Lindahl Allocations with
   Forthrightness},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2011},
Volume = {121},
Number = {5},
Pages = {751-766},
Month = {SEP-OCT},
Abstract = {We provide a new price-quantity mechanism to implement the Lindahl
   allocations as Nash equilibria. We show that it satisfies the property
   of forthrightness, introduced by Saijo et al. {[}1996]. Thus, at a Nash
   equilibrium, each player reports directly and honestly his personal
   price and his demand for the public good and obtains the corresponding
   commodity bundle. Using the gradient process to formalize the behaviour
   of the players out-of-equilibrium, we show that, in quasi-linear
   economies, the Nash equilibrium of the game is globally stable.},
