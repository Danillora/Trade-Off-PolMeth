@article{ WOS:000296267300004,
Author = {Frye, Timothy M. and Iwasaki, Ichiro},
Title = {Government directors and business-state relations in Russia},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2011},
Volume = {27},
Number = {4},
Pages = {642-658},
Month = {DEC},
Abstract = {We propose three ideal types of business-state relations in a transition
   economy and explore the impact of government directors on corporate
   boards for firm behavior. Using a unique dataset of joint-stock
   companies in Russia, we find that the presence of government directors
   on corporate boards is more consistent with a ``collusion{''} ideal type
   of relations between firms and the state than with a managerial
   discipline or rent-extraction ideal type. The state sends directors to
   firms that both extract resources from the state, but that also provide
   important benefits and services to the state. (C) 2011 Elsevier B.V. All
   rights reserved.},
