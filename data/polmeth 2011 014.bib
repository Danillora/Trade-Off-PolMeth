@article{ WOS:000294688600003,
Author = {Buser, Whitney},
Title = {The impact of fiscal decentralization on economics performance in
   high-income OECD nations: an institutional approach},
Journal = {PUBLIC CHOICE},
Year = {2011},
Volume = {149},
Number = {1-2, SI},
Pages = {31-48},
Month = {OCT},
Abstract = {\textbackslash{} This paper examines the impact of public sector
   decentralization on per capita income. Controlling for differences in
   institutional arrangements among countries, panel data regressions on a
   sample of observations from 20 high-income OECD nations spanning the
   years 1972 to 2005 indicate that decentralization is positively related
   to income. The empirical analysis shows that institutions consistent
   with economic freedom enhance the positive income effects of
   decentralization. Thus, the impact of public sector decentralization is
   dependent upon a nation's institutional environment.},
