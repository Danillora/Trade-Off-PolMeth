@incollection{ WOS:000310749600009,
Author = {Laineste, Liisi},
Editor = {Tsakona, V and Popa, DE},
Title = {Politics of taste in a post-Socialist state A case study},
Booktitle = {STUDIES IN POLITICAL HUMOUR: IN BETWEEN POLITICAL CRITIQUE AND PUBLIC
   ENTERTAINMENT},
Series = {Discourse Approaches to Politics Society and Culture},
Year = {2011},
Volume = {46},
Pages = {217-241},
Abstract = {The link between jokes and social reality is visible in the way jokes
   adapt to different sociopolitical contexts by dealing with the most
   salient issues of such contexts. This chapter casts light on another
   facet of the relationship of jokes and their social context. Ideas about
   jokes are influenced by their social context, being continuously
   reformulated by social change or political manipulation. This case study
   analyses the official and unofficial media discourses that address the
   issues of taste and sense of humour, as emerging from a recent polemics
   about Estonian ethnic jokes. The different standpoints reflect ideas
   about the content and functions of the jokelore, characterising jokes
   either as an essentially racist or as a funny (i.e. harmless) genre.},
