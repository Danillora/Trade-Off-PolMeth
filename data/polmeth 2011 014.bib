@article{ WOS:000291331800004,
Author = {Vlachy, Jan},
Title = {DERIVATIVES AND BUCKETSHOPS: A FORGOTTEN HISTORY OF CZECH FINANCIAL
   MARKETS BETWEEN THE WORLD WARS},
