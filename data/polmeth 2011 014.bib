@article{ WOS:000293106000004,
Author = {Schaltegger, Christoph A. and Somogyi, Frank and Sturm, Jan-Egbert},
Title = {Tax competition and income sorting: Evidence from the Zurich
   metropolitan area},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2011},
Volume = {27},
Number = {3},
Pages = {455-470},
Month = {SEP},
Abstract = {We provide empirical evidence on the influence of income taxes on the
   choice of residence of taxpayers at the local government level. The fact
   that Swiss communities can independently set tax multipliers, thereby
   shifting the progressive tax scheme that is fixed at the cantonal
   (state) level, enables us to study the effect of differences in income
   taxation on individuals' choice of location within an economically and
   culturally homogeneous region. Using panel IV regressions covering the
   years 1991-2003 and 171 communities in the Swiss canton of Zurich and
   spatial error regressions for the 171 communities in 2003, we find
   substantial evidence of sorting according to income. (C) 2011 Elsevier
   B.V. All rights reserved.},
