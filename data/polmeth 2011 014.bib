@article{ WOS:000289606500010,
Author = {Daniele, Vittorio and Marani, Ugo},
Title = {Organized crime, the quality of local institutions and FDI in Italy: A
   panel data analysis},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2011},
Volume = {27},
Number = {1},
Pages = {132-142},
Month = {MAR},
Abstract = {This paper examines the impact of crime on foreign direct investment
   (FDI) inflows in the Italian provinces. The incidence of organized crime
   is measured by the number of complaints regarding criminal offences of
   different kinds traditionally related to the mafia organizations. The
   results show how the correlation between organized crime and FDI is both
   negative and significant, even when an indicator of financial incentives
   for investment is included in the regressions. Our analysis shows that
   crime represents a deterrent for foreign investors, suggesting that high
   levels of (certain) crimes may be perceived as a signal of a local
   socio-institutional environment unfavourable for FDI. (C) 2010 Elsevier
   B.V. All rights reserved.},
