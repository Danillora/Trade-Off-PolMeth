   Jeanneney, Sylviane Guillaumont; Hua, Ping, Univ Auvergne, Clermont Univ, CNRS, Ctr Etud \& Rech Dev Int, F-63000 Clermont Ferrand, France.},
ISSN = {0373-2630},
EISSN = {2105-2883},
Keywords = {China; labour productivity; real exchange rate; renminbi revaluation},
Keywords-Plus = {REAL EXCHANGE-RATE; ECONOMIC-GROWTH; PANEL-DATA; PRIVATIZATION;
