@article{ WOS:000296069000002,
Author = {Zd'arek, Vaclav},
Title = {AN EMPIRICAL ANALYSIS OF THE PROCESS OF PRICE CONVERGENCE OF THE CZECH
   REPUBLIC AND NEW EU MEMBER STATES TOWARDS THE EURO AREA},
