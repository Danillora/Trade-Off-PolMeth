@article{ WOS:000290769900007,
Author = {Lindstaedt, Rene and Vander Wielen, Ryan J.},
Title = {Timely shirking: time-dependent monitoring and its effects on
   legislative behavior in the U.S. Senate},
Journal = {PUBLIC CHOICE},
Year = {2011},
Volume = {148},
Number = {1-2},
Pages = {119-148},
Month = {JUL},
Abstract = {We examine legislative shirking under conditions of imperfect
   constituent monitoring. Our core argument states that variation in
   information costs and constituent memory leads to systematic variation
   in constituent monitoring over time. Assuming that legislators are
   responsive to multiple cues, we expect to observe time-dependent
   legislative shirking. We develop a theoretical model of legislative
   behavior with time-dependent monitoring, which we test empirically using
   Senate roll call data. Our findings show that a substantial number of
   senators engage in systematic, time-dependent shirking, and that
   senators consistently move toward extreme positions when monitoring is
   low and toward moderate positions when it is high.},
