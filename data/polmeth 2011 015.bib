@incollection{ WOS:000293363500002,
Author = {Schillmeier, Michael and Pohler, Wiebke},
Editor = {Benson, M and Munro, R},
Title = {The Danube and ways of imagining Europe},
Booktitle = {SOCIOLOGICAL ROUTES AND POLITICAL ROOTS},
Series = {Sociological Review Monographs},
Year = {2011},
Pages = {25-43},
Abstract = {This paper is concerned with the River Danube as a European object that
   articulates forms of relations which, by linking human and non-humans in
   highly specific ways, create actor-networks that dispute common societal
   divisions and (b)orders. By discussing the contested social relevance of
   the River Danube, this paper visualizes different ways of imagining
   Europe. Following some of the controversies concerning the River Danube,
   we outline a process oriented, transnational research agenda for a
   specific European object with the conceptual explication of human and
   non-human relations at its core.},
