@article{ WOS:000289654700010,
Author = {Bolanos de Miguel, Aitor Manuel},
Title = {HISTORIOGRAPHY AND POSTMODERNISM: F.R. ANKERSMIT'S THEORY OF
   REPRESENTATION},
Journal = {HISTORIA Y POLITICA},
Year = {2011},
Number = {25},
Pages = {271-308},
Month = {JAN-JUN},
Abstract = {This article analyses F. R. Ankersmir.s aesthetic theory of historical
   representation. After some preliminary thoughts on the discipline, I
   examine the work of Arthur C. Danto and Ernst H. Gombrich, both of which
   were an important influence for Ankersmit. Then I present the
   rapprochement between historiography, literature and aesthetics proposed
   by Ankersmit, alongside with some of the criticism it aroused, namely by
   Perez Zagorin. The article concludes with a general assessment of the
   essential core of postmodernist historiography.},
