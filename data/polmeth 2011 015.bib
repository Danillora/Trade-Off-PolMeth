@article{ WOS:000304782200008,
Author = {Martin Sanchez, Maria},
Title = {HISTORICAL APPROACH TO THE LEGAL AND SOCIAL TREATMENT GIVEN TO
   HOMOSEXUALITY IN EUROPE)},
Journal = {ESTUDIOS CONSTITUCIONALES},
Year = {2011},
Volume = {9},
Number = {1},
Pages = {245-275},
Abstract = {Homosexuality is not a recent phenomenon, but it has been present in all
   societies and civilizations along History, being treated in many
   different ways depending on diverse factors. The experience reveals that
   the different attitudes towards homosexuality, from its acceptance or
   veneration to its criminal conviction, respond to each society's level
   of tolerance, without forgetting other circumstances. The evolution of
   homosexuality throughout History has currently achieved, not only its
   legal protection from discrimination in the highest European Organisms,
   such as the European Court of Human Rights (ECHR), but also the
   acknowledgement of same sex marriages in some countries, such as Spain,
   Holland or Belgium.},
