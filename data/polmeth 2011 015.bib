@article{ WOS:000291707400004,
Author = {Fleurbaey, Marc},
Title = {Willingness-to-pay and the equivalence approach},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2011},
Volume = {121},
Number = {1},
Pages = {35-58},
Month = {JAN-FEB},
Abstract = {This paper describes the evaluation of individual situations in terms of
   equivalent incomes (or money-metric utilities) computed from ordinary
   income by adding or subtracting various terms of willingness-to-pay. It
   discusses the origin of the approach and its connection with social
   choice theory, fair allocation theory, and philosophical principles. It
   shows how this has been generalized into a broader ``equivalence
   approach{''}, and argues that the criticisms that have been raised
   against this methodology are not compelling. It also examines the
   challenges to be addressed for empirical applications of the approach.},
