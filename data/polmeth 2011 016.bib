@article{ WOS:000289998200002,
Author = {Weirich, Paul},
Title = {Exclusion from the social contract},
Journal = {POLITICS PHILOSOPHY \& ECONOMICS},
Year = {2011},
Volume = {10},
Number = {2, SI},
Pages = {148-169},
Month = {MAY},
Abstract = {Does rational bargaining yield a social contract that is efficient and
   so inclusive? A core allocation, that is, an allocation that gives each
   coalition at least as much as it can get on its own, is efficient.
   However, some coalitional games lack a core allocation, so rationality
   does not require one in those games. Does rationality therefore permit
   exclusion from the social contract? I replace realization of a core
   allocation with another type of equilibrium achievable in every
   coalitional game. Fully rational agents coordinate the pursuit of
   incentives so that equilibria of this type are efficient. They adopt a
   social contract that is efficient and inclusive.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Weirich, P (Corresponding Author), Univ Missouri, Dept Philosophy, Columbia, MO 65211 USA.
   Univ Missouri, Dept Philosophy, Columbia, MO 65211 USA.},
DOI = {10.1177/1470594X10387262},
ISSN = {1470-594X},
EISSN = {1741-3060},
Keywords = {bargaining; contractarianism; the core; efficiency; equilibrium;
   exclusion; rationality},
Keywords-Plus = {COASE THEOREM; JUSTICE},
Research-Areas = {Social Sciences - Other Topics; Government \& Law},
Web-of-Science-Categories  = {Ethics; Political Science},
Author-Email = {weirichp@missouri.edu},
