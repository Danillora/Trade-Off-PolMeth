@article{ WOS:000296272200003,
Author = {Flores, Alejandro Quiroz and Smith, Alastair},
Title = {LEADER SURVIVAL AND CABINET CHANGE},
Journal = {ECONOMICS \& POLITICS},
Year = {2011},
Volume = {23},
Number = {3},
Pages = {345-366},
Month = {NOV},
Abstract = {Political leaders face both internal and external threats to their
   tenure as leaders. To retain office leaders need mass support, for
   instance to win elections. However, they also need to deal with
   potential internal party rivals. Using a game theoretic model, we
   examine how the incentives created by these competing pressures affect
   the retention of ministers across different political systems. Since
   non-democratic leaders face relatively little threat from the masses,
   their concern is to reduce internal party risk. Therefore, they remove
   high performing ministers and retain mediocre and poor performers. As it
   becomes easier for the masses to replace the party in power, leaders
   must tradeoff internal and external threats. Retaining competent leaders
   improves party performance but generates an internal party rival.},
