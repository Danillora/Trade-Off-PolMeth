@article{ WOS:000296779900002,
Author = {Crawford, Michael J.},
Title = {Noble Status and Royal Duplicity in the Crown of Castile, 1454-1504},
Journal = {EUROPEAN HISTORY QUARTERLY},
Year = {2011},
Volume = {41},
Number = {4},
Pages = {586-608},
Month = {OCT},
Abstract = {This article examines the seemingly opposing actions of Castile's late
   medieval monarchs, who both authorized the noble status of hidalgo for
   supporters and developed rules and legal procedures to limit the
   proliferation of this status. Addressing both royal policy and disputes
   over status in the territory of Seville during the second half of the
   fifteenth century, it demonstrates how local authorities strategically
   employed laws and appeals to royal courts to resist the recognition of
   those who claimed hidalgo status and to deny them related privileges.
   The actions of these monarchs and their subjects reveal the nature and
   limits of royal absolutism in matters of social status in the late
   fifteenth century and the degree to which the development of state
   judicial apparatus affected the local recognition of status and the
   growth of state power.},
