@article{ WOS:000293273500002,
Author = {Hamnett, Brian},
Title = {Spain and Portugal and the Loss of their Continental American
   Territories in the 1820s: An Examination of the Issues},
Journal = {EUROPEAN HISTORY QUARTERLY},
Year = {2011},
Volume = {41},
Number = {3, SI},
Pages = {397-412},
Month = {JUL},
Abstract = {The least studied aspect of Ibero-American Independence is the impact on
   Spain and Portugal. Many complex issues were involved, including the
   nature of the economic impact, the degree of political readjustment, and
   the international position of the former metropoles. The 1820s exposed
   the contradictions in Iberian responses to the loss of continental
   American territories. Spanish merchants turned towards the domestic and
   Caribbean markets, and Portuguese policy shifted away from Brazil to the
   southern-African territories. Although the monarchy survived in both
   Spain and Portugal, each experienced dynastic convulsions of differing
   dimensions. The survival of empire beyond continental America
   complicated the development of European identities and of specifically
   Spanish and Portuguese national sentiment.},
