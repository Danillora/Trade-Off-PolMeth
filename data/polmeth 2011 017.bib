@article{ WOS:000289654700012,
Author = {Delgado, Ander},
Title = {SYMPATHY WITH YOU AS A NATION. THE RELATIONSHIP BETWEEN BASQUE
   AND WELSH NATIONALISM UNTIL THE SPANISH CIVIL WAR},
Journal = {HISTORIA Y POLITICA},
Year = {2011},
Number = {25},
Pages = {331-355},
Month = {JAN-JUN},
Abstract = {This article focuses on the debate of the influence of the international
   context on the development of nationalist movements. With that purpose,
   the relations between Basque and Welsh nationalism in the period prior
   to the end of the Spanish Civil War are analyzed. This study concludes
   that mutual relations do not occur automatically as a result of sharing
   common interests, but respond to a complex and slow process. However,
   the above mentioned aspect is an element to be taken into consideration
   because it illustrates, although in a tenuous way, the role of political
   programs and vindications of outside referents in each of the studied
   movements.},
