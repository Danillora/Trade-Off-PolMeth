@article{ WOS:000294688100007,
Author = {Kselman, Daniel and Niou, Emerson},
Title = {Protest voting in plurality elections: a theory of voter signaling},
Journal = {PUBLIC CHOICE},
Year = {2011},
Volume = {148},
Number = {3-4},
Pages = {395-418},
Month = {SEP},
Abstract = {This paper develops a model of protest voting in which unsatisfied
   voters may abandon their most-preferred candidate even though he or she
   has a good chance of winning, in the hope that this signal of
   disaffection will lead to downstream improvements in that candidate's
   performance. We use a spatial model to identify voters whose ideological
   profile makes protest voting an option, and an expected utility model to
   identify the conditions under which potential protest voters will in
   fact use their vote as a signaling device. Aggregate-level data provide
   suggestive evidence in the argument's favor.},
