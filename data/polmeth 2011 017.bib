@article{ WOS:000289606500011,
Author = {Bjerg, Christina and Bjornskov, Christian and Holm, Anne},
Title = {Growth, debt burdens and alleviating effects of foreign aid in least
   developed countries},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2011},
Volume = {27},
Number = {1},
Pages = {143-153},
Month = {MAR},
Abstract = {We explore the potential growth effects of foreign aid when in
   conjunction with severe debt problems. When used to finance debt
   repayments, aid may not lead to Dutch Disease while it still alleviates
   an economic problem. A set of empirical estimates shows that although
   inflows of foreign aid in general are not associated with growth in a
   sample of 38 Least Developed Countries, an interaction term with the
   level of external debt is significant. We take this as suggestive
   evidence of an alleviating effect of aid in these countries and offer
   some tentative thoughts on the implications for future aid policies. (C)
   2010 Elsevier B.V. All rights reserved.},
