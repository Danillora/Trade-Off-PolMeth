@article{ WOS:000293545800006,
Author = {Morgan, Glenn and Helleiner, Eric and Seabrooke, Leonard and Erturk,
   Ismail and Froud, Julie and Leaver, Adam and Williams, Karel},
Title = {Reforming the global financial architecture},
Journal = {SOCIO-ECONOMIC REVIEW},
