@article{ WOS:000294922500011,
Author = {Whang, Taehee},
Title = {Playing to the Home Crowd? Symbolic Use of Economic Sanctions in the
   United States},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
Year = {2011},
Volume = {55},
Number = {3},
Pages = {787-801},
Month = {SEP},
Abstract = {Why do we observe economic sanctions despite strong doubts regarding
   their effectiveness? While the symbolic use of sanctions is advanced as
   an alternative to the instrumental use explanation, no one has assessed
   this alternative explanation empirically. I investigate the symbolic use
   of sanctions for domestic political gain in the United States, assessing
   in particular the effect of sanctions imposition on US presidential
   approval ratings. Findings suggest that policymakers benefit from
   imposing sanctions through increased domestic support. This domestic
   political gain can present policymakers with an incentive to use
   sanctions as a low-cost way of displaying strong leadership during
   international conflicts.},
