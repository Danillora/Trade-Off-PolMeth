@article{ WOS:000289616700001,
Author = {Murdie, Amanda and Bhasin, Tavishi},
Title = {Aiding and Abetting: Human Rights INGOs and Domestic Protest},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2011},
Volume = {55},
Number = {2},
Pages = {163-191},
Month = {APR},
Abstract = {This article studies the effects of human rights international
   nongovernmental organizations (INGOs) on domestic antigovernment
   protest. Unlike mainstream scholarship, the authors argue that human
   rights INGOs are not simply the magic bullet in orchestrating nonviolent
   protests; different types of human rights INGO activity have varying
   effects on protest. Moreover, some human rights INGO activities may lead
   to higher levels of violent protest. The empirical tests use new data on
   the activities of over 400 human rights INGOs and domestic nonviolent
   and violent protest globally from 1991 to 2004. The authors find that
   increases in human rights INGO activities reflecting a greater
   commitment to the domestic population are associated with higher levels
   of both violent and nonviolent protest.},
