@article{ WOS:000289800200003,
Author = {Homolar, Alexandra},
Title = {How to Last Alone at the Top: US Strategic Planning for the Unipolar Era},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2011},
Volume = {34},
Number = {2},
Pages = {189-217},
Abstract = {This article investigates how key actors within the US defence policy
   community realigned their interests to forge a new consensus on the
   redirection of US defence strategy following the `peace shock' they
   faced with the collapse of bipolarity. This consensus centred on the
   idea that achieving US security in the `age of uncertainty' demanded
   overwhelming US military power, which was widely interpreted as
   necessitating military capabilities to fight multiple major theatre wars
   simultaneously against regional `Third World' adversaries. This helped
   to preserve many of the principal pillars of US Cold War defence policy
   through deflecting calls for more radical organisational changes and
   deeper cuts to defence budgets.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXFORDSHIRE, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Homolar, A (Corresponding Author), Univ Warwick, Dept Polit \& Int Studies, Coventry CV4 7AL, W Midlands, England.
   Univ Warwick, Dept Polit \& Int Studies, Coventry CV4 7AL, W Midlands, England.},
DOI = {10.1080/01402390.2011.559024},
Article-Number = {PII 936613902},
ISSN = {0140-2390},
Keywords = {US military power; Post-Cold War US defence policy; Major theatre wars},
