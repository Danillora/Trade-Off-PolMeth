@article{ WOS:000289331300006,
Author = {Szwed, Robert},
Title = {Printmedia poll reporting in Poland: Poll as news in Polish
   parliamentary campaigns, 1991-2007},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
