@article{ WOS:000293745500004,
Author = {Bernardino-Costa, Joaze},
Title = {Destabilizing the National Hegemonic Narrative The Decolonized Thought
   of Brazil's Domestic Workers' Unions},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2011},
Volume = {38},
Number = {5},
Pages = {33-45},
Month = {SEP},
Abstract = {Theories of decolonization (principally those of Dussel, Quijano, and
   Mignolo), hold that knowledge is produced not from a neutral and
   universal position but from the different positions of actors within a
   hierarchical structure of power. Analysis of the thought of Brazil's
   unionized domestic workers as a subaltern voice reveals that these
   workers' political activism produces a form of knowledge that
   articulates class, race, and gender and allows us to problematize the
   national hegemonic narrative, destabilizing the hegemonic cultural
   meanings structured by the myths of racial democracy and the good
   master.},
