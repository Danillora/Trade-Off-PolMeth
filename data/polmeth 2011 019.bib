@article{ WOS:000287072700007,
Author = {Oomen, Barbara},
Title = {Between Rights Talk and Bible Speak: The Implementation of Equal
   Treatment Legislation in Orthodox Reformed Communities in The
   Netherlands},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2011},
Volume = {33},
Number = {1},
Pages = {175-200},
Month = {FEB},
Abstract = {This article discusses the responses to a number of recent court cases
   concerning the equal treatment of women and homosexuals amongst Dutch
   orthodox reformed in the semi-public sphere (e.g. political parties,
   schools). In doing so, this article applies and refines legal
   anthropological theories on the realization of rights in a context of
   cultural and religious diversity. It also specifically addresses the
   responses to court cases launched ``from the outside{''} in the context
   of a western country with a tradition of code law. These cases have an
   adverse effect on discussions of equal treatment within the communities
   concerned.},
