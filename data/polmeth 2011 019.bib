@article{ WOS:000218297100002,
Author = {Box, Zira and Saz, Ismael},
Title = {Spanish Fascism as a Political Religion (1931-1941)},
Journal = {POLITICS RELIGION \& IDEOLOGY},
Year = {2011},
Volume = {12},
Number = {4},
Pages = {371-389},
Abstract = {This article focuses on the controversial argument that Spanish Fascism,
   organized in the political party Falange Espanola (FE y de las JONS),
   can be interpreted as a form of political religion, understood in the
   sense of Emilio Gentile. The central thesis is that Spanish Fascism was
   a political religion during the Republican period, that is, from the
   time of its consolidation during the early 1930s to the beginning of the
   Civil War. However - and this is the main analytical challenge - it was
   also one after July 1936, in spite of the fact that the formation of
   Franco's Movimiento Nacional imposed no small transformations upon it.
   Even if those transformations were linked to the increasingly prominent
   role played by the Catholic Church and traditional religion, the
   argument and conclusion of these pages is that an analytical tool such
   as that of political religion is always useful in dealing with Spanish
   Fascism and the Francoist dictatorship.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
