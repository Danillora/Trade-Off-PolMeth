@article{ WOS:000298364300006,
Author = {Avci, Gamze},
Title = {The Nationalist Movement Party's Euroscepticism: Party Ideology Meets
   Strategy},
Journal = {SOUTH EUROPEAN SOCIETY AND POLITICS},
Year = {2011},
Volume = {16},
Number = {3, SI},
Pages = {435-447},
Abstract = {This article explores what conditions or what specific issues lead to
   shifts in the positioning of the Nationalist Movement Party (Milliyetci
   Hareket Partisi, MHP) towards Turkey's European Union (EU) membership
   goal. The article first examines the MHP's background and
   characteristics and compares it with similar parties in Western Europe.
   Subsequently, the MHP's EU stance during 1999-2002 and in the post-2002
   period is discussed. The article concludes that ideology matters for the
   MHP but that shifts in degrees of Euroscepticisim during particular
   periods can be explained by strategic considerations of the MHP, which
   respond to the party's electoral gains or losses, its position in the
   party political spectrum and whether it is in government, in opposition
   or outside parliament.},
