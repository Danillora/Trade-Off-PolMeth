@article{ WOS:000296038900004,
Author = {Bader, Max},
Title = {Hegemonic political parties in post-Soviet Eurasia: Towards party-based
   authoritarianism?},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2011},
Volume = {44},
Number = {3},
Pages = {189-197},
Month = {SEP},
Abstract = {Hegemonic parties in authoritarian regimes can fulfill important
   purposes for those regimes and thereby contribute to their survival.
   Along with the consolidation of authoritarian regimes, hegemonic
   political parties have emerged in some post-Soviet states, raising
   questions about the role that these parties play in the survival of the
   regimes. This article asks which of the purposes that are frequently
   ascribed to ruling authoritarian parties are fulfilled by United Russia,
   the Yeni Azerbaijan Party, and Nur Otan of Kazakhstan, the hegemonic
   parties of the three strongest consolidated authoritarian regimes with a
   hegemonic party in the former Soviet Union. It is argued that despite
   the increasing prominence of the hegemonic parties, full-fledged
   party-based authoritarianism has not yet been established in Russia,
   Azerbaijan, and Kazakhstan. (C) 2011 Published by Elsevier Ltd on behalf
   of The Regents of the University of California.},
