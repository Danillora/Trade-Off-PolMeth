@article{ WOS:000295092200005,
Author = {Alba, Carlos R. and Navarro, Carmen},
Title = {ADMINISTRATIVE TRADITION AND REFORMS IN SPAIN: ADAPTATION VERSUS
   INNOVATION},
Journal = {PUBLIC ADMINISTRATION},
Year = {2011},
Volume = {89},
Number = {3},
Pages = {783-800},
Abstract = {The main purpose of this paper is understanding public administration
   formation and change in Spain. Its development before and since the
   years of Franco from 1939 to 1975, is noticeably path dependent in
   nature. As this paper will show, the foundations were laid before the
   Franco regime. Then came a period of political domination of the
   bureaucracy and the emergence of a key structural element of Spanish
   administration, the special corps. The paper covers the period of
   political transition, democratic consolidation, and the many processes
   of administrative reforms. These include the reforms of the Spanish
   civil service as well as the deep processes of reform.},
