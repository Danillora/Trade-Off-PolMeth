@article{ WOS:000299982000002,
Author = {Neumann, Iver B.},
Title = {The New Norwegian `We': The Diaspora Factor in Norwegian Foreign Policy},
Journal = {INTERNASJONAL POLITIKK},
Year = {2011},
Volume = {69},
Number = {4},
Pages = {569+},
Abstract = {The New Norwegian `We': The Diaspora Factor in Norwegian Foreign Policy
   The article gives a nutshell sketch of historic Norwegian diasporas.
   They are, first, small groups in Norway, notably Jews and Roma.
   Secondly, they are large groups of Norwegians and people who consider
   themselves as descending from Norwegians, first and foremost in the US,
   but also in Canada, Argentina and elsewhere in the new world. Today's
   diasporas are, first, colonies of Norwegians abroad, primarily in Spain,
   but also in Thailand and elsewhere. Politically, the most important
   issue would be diasporas within Norway itself. The article suggests a
   classification of foreign policy-related strategies that may be followed
   by these groups, ranging from op-ed writing via demonstrations, lobbying
   and party-political activity to violent action. We lack research on all
   these. The article also introduces the other articles that make up this
   section of the journal.},
