@article{ WOS:000292564300005,
Author = {Crew, Jr., Robert E. and Lewis, Christopher},
Title = {Verbal Style, Gubernatorial Strategies, and Legislative Success},
Journal = {POLITICAL PSYCHOLOGY},
Year = {2011},
Volume = {32},
Number = {4},
Pages = {623-642},
Month = {AUG},
Abstract = {This article examines the relationship between a governor's verbal style
   and his success in achieving his legislative goals. We add a measure of
   verbal style developed by Roderick Hart to a traditional model used by
   scholars of political chief executives to explain legislative success.
   We apply this model to the State of the State addresses of six governors
   of Florida who served between 1966 and 2006. Our findings reflect the
