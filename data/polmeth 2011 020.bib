@article{ WOS:000291811700003,
Author = {Cheung, Tai Ming},
Title = {The Chinese Defense Economy's Long March from Imitation to Innovation},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2011},
Volume = {34},
Number = {3, SI},
Pages = {325-354},
Abstract = {China's defense economy has been vigorously developing a comprehensive
   set of innovation capabilities that will eventually allow it to join the
   world's top tier of military technological powers. China's target is to
   catch up by 2020. Although this maybe possible in a few select areas,
   the defense economy as a whole will likely require another decade or
   more to successfully master the ability to produce major innovations of
   a radical nature. This paper analyzes the key areas in the Chinese
   defense economy's gradual but accelerating shift from imitation to
   indigenous innovation.},
