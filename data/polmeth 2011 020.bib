@article{ WOS:000293082600003,
Author = {Veit, Alex and Barolsky, Vanessa and Pillay, Suren},
Title = {Violence and Violence Research in Africa South of the Sahara},
Journal = {INTERNATIONAL JOURNAL OF CONFLICT AND VIOLENCE},
Year = {2011},
Volume = {5},
Number = {1},
Pages = {14-31},
Abstract = {This review presents the major lines of investigation regarding violence
   in Africa since the Cold War. After a historical introduction to the
   development of violent phenomena and their political contexts, diverse
   issues such as civil war, democratization, vigilantism, and the role of
   youth are assessed. It is argued that recent research has produced
   important insights by re-focusing on violent phenomena beyond the state.
   Yet despite the increasing number of non-state violent actors active on
   the African continent, to speak of a ``privatization{''} of violence may
   be premature.},
Publisher = {INST INTERDISCIPLINARY RES},
Address = {BIELEFELD UNIV, UNIVERSITATSSTR 25, BIELEFELD, 33615, GERMANY},
Type = {Article},
Language = {English},
Affiliation = {Veit, A (Corresponding Author), Univ Bremen, Inst Intercultural \& Int Studies, D-2800 Bremen 33, Germany.
