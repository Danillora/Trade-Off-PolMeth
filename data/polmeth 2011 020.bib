@article{ WOS:000299742500011,
Author = {Valladao De Carvalho, Maria Izabel},
Title = {Institutions and interest groups: European Union in Doha Round
   agriculture negotiation (2001-2005)},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2011},
Volume = {54},
Number = {2},
Pages = {212-229},
Abstract = {This article deals with the influence of the agriculture interest groups
   in the positions adopted by EU during the Doha Round. The proposals
   defended by the two actors were compared and analyzed. The text argues
   that the distinctions of positions found can be explained by the
   institutional rules of the decision-making process of EU Common Trade
   Policy. The conclusion highlights that the pressures affected the
   proposals defended by EU in Doha Round negotiations, but they did not
   prevent advancements in this context.},
