@article{ WOS:000299963600003,
Author = {Alonso, Rogelio},
Title = {Why Do Terrorists Stop? Analyzing Why ETA Members Abandon or Continue
   with Terrorism},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2011},
Volume = {34},
Number = {9},
Pages = {696-716},
Abstract = {This article analyzes the factors that have motivated members of Euskadi
   Ta Askatasuna (ETA) to abandon terrorism and the factors that have
   motivated other members of the terrorist organization to maintain their
   activism. The article examines the process of disengagement of an
   important faction of ETA and that of other terrorists throughout the
   group's protracted campaign in order to explain the reasons behind the
   decision to give up violence. The article concludes with an analysis of
   the variables that should be considered in order to encourage the
   collective disengagement of the group that has become the most enduring
   terrorist organization in Europe.},
