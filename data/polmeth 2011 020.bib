@article{ WOS:000287072700002,
Author = {Sabatello, Maya},
Title = {Advancing Transgender Family Rights through Science: A Proposal for an
   Alternative Framework},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2011},
Volume = {33},
Number = {1},
Pages = {43-75},
Month = {FEB},
Abstract = {This article examines the impact of the scientific revolution on the
   family rights of transgender individuals from an international human
   rights perspective. It reviews how the European Court of Human Rights
   has responded to quandaries arising in this context and observes the
   inequalities that surface at the intersection between law, gender
   identities, culture, and science with regard to transgender family
   relations. To close the gaps, the article suggests pursuing family
