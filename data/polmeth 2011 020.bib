@article{ WOS:000296956100005,
Author = {Shalini, Ramessur Taruna and Boopen, Seetanah},
Title = {On the link between urban infrastructure and poverty: evidence from
   Africa using GMM estimates},
Journal = {POLICY AND POLITICS},
Year = {2011},
Volume = {39},
Number = {2},
Pages = {221-238},
Month = {APR},
Abstract = {The role of infrastructure, in spite of the high level of investment
   that national governments and international donor agencies are putting
   into infrastructure development, has largely been ignored in the
   assessment of urban poverty in Sub-Saharan Africa. This article presents
   an investigation of the impact of transport infrastructure on the urban
   poor in a sample of 15 Sub-Saharan African countries over the period
   1980-2006. The results from both the fixed effect and dynamic panel
   models reveal that transport infrastructure is indeed an efficient tool
   in fighting urban poverty and that poverty is essentially a dynamic
   phenomenon.},
