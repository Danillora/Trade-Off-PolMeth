@article{ WOS:000299217500011,
Author = {Peter, Mateja},
Title = {The politics of self-defence: beyond a legal understanding of
   international norms},
Journal = {CAMBRIDGE REVIEW OF INTERNATIONAL AFFAIRS},
Year = {2011},
Volume = {24},
Number = {2},
Pages = {245-264},
Abstract = {This contribution considers an alternative approach to the research on
   the use of force, a field that has been largely dominated by
   international legal scholars. I argue for application of an evolutionary
   approach to international (legal) norms, one that complements current
   legal approaches and moves the discussion of norms beyond their
   dichotomous legal/illegal understanding. This kind of research
   highlights the role of politics in international law and allows us to
   determine factors influencing dissemination of international norms.
   Through a study of seven post-Second World War cases, I trace the
   development of both narrow and expanded notion of preemptive
   self-defence and conclude that while ascertaining legality of specific
   actions is vital for understanding established international law, one
   cannot talk about radical breaks in development of the law on the use of
   force. The expanded concept is in fact a product of the evolution of
   (societal) norms on self-defence and an accumulation of previous
   successful challenges in metanorms on the use of force.},
