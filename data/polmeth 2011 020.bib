@article{ WOS:000211372300004,
Author = {Tudoroiu, Theodor},
Title = {The Neo-Communist Regime of Present-Day China},
Journal = {JOURNAL OF CHINESE POLITICAL SCIENCE},
Year = {2011},
Volume = {16},
Number = {4},
Pages = {407-429},
Month = {DEC},
Abstract = {The first goal of this article is to define the neo-communist regime as
   a specific type of undemocratic post-communist construct. Three case
   studies analyzing the regimes led by Zhan Videnov in Bulgaria, Ion
   Iliescu in Romania, and Alyaksandr Lukashenka in Belarus are used to
   identify its main characteristics. The second goal is to show that the
   present-day Chinese regime falls into this category. As such, it does
   not represent an intermediate or transitional phase. This `hard'
   neo-communist regime is the final stage of the Chinese post-communist
   transition. In the foreseeable future, it will most likely preserve its
   present characteristics.},
