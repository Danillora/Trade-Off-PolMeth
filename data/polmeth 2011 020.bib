@article{ WOS:000292327600003,
Author = {Morris, Justin},
Title = {How Great is Britain? Power, Responsibility and Britain's Future Global
   Role},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
Year = {2011},
Volume = {13},
Number = {3},
Pages = {326-347},
Month = {AUG},
Abstract = {Hedley Bull argued that for a state to be classed as a great power it
   must be in the first rank in terms of military strength but also
   recognised by others to have, and conceived by its own leaders and
   peoples to have, certain special rights and duties. Adopting this
   approach, this article argues that Britain's great power credentials are
   far stronger than commonly appreciated and that, while the term is no
   longer in vogue, within government the idea that Britain is a great
   power remains an influential factor in determining British foreign and
   defence policy.},
