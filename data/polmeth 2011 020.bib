@article{ WOS:000299116000010,
Author = {Weber, Annette},
Title = {Women Without Arms: Gendered Fighter Constructions in Eritrea and
   Southern Sudan},
Journal = {INTERNATIONAL JOURNAL OF CONFLICT AND VIOLENCE},
Year = {2011},
Volume = {5},
Number = {2},
Pages = {358-370},
Abstract = {An analysis of gendered fighter constructions in the liberation
   movements in Eritrea and southern Sudan (EPLF and SPLA/M), examining the
   question of female access to the sphere of masculine fighter constructs
   and the relevance of this for influence in peacetime affairs. Empirical
   research in both countries, in particular interviews with participants,
   reveals that what keeps women out of the sphere of legitimized violence
   is not some ``inherent peacefulness,{''} but the exclusivist construct
   of the masculine fighter, which is supported by society. This makes it
   hard for women to participate in war, and especially to gain full
   fighter status. An intrinsic link is found between fighter status and
   access to power in post-conflict state-building from which women, being
   unable to gain full fighter status, are largely excluded.},
