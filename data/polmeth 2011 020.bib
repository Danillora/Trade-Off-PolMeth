@article{ WOS:000289331300009,
Author = {Smith, Scott M. and Bryson, Phillip J. and Cornia, Gary C.},
Title = {The view from city hall: Local perceptions of intergovernmental fiscal
   relations in the Czech Republic},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2011},
Volume = {44},
Number = {1},
Pages = {99-110},
Month = {MAR},
Abstract = {This article analyzes perceptions of inter-governmental fiscal relations
   as held by local officials of the Czech Republic. The field study probes
   local perceptions of progress toward fiscal decentralization in the
   Czech Republic. A statistical analysis is based on a scaling of cities
   according to size and according to a generalized, multi-part measure of
   fiscal autonomy. This measure is effective in discriminating high and
   low autonomy cities by size, types of expenditures, and funding sources.
   Cities of diverse sizes are divided into groups reflecting perceptions
   of greater or lesser autonomy. The implications of the findings are
   discussed. (C) 2011 Published by Elsevier Ltd on behalf of The Regents
   of the University of California.},
