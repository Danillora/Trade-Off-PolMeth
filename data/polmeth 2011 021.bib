@article{ WOS:000297310000004,
Author = {Hill, Lisa},
Title = {Increasing Turnout Using Compulsory Voting},
Journal = {POLITICS},
Year = {2011},
Volume = {31},
Number = {1},
Pages = {27-36},
Month = {FEB},
Abstract = {In a recent issue of Politics Ben Saunders argued that the use of
   compulsory voting to increase turnout is misguided: lower turnout - even
   if unequal across social groups - is not necessarily undemocratic and
   may even serve democratic values. While accepting Saunders' normative
   premises that democratic decisions should be made by those affected by
   them and that distortions should be eliminated from the electoral
   process, I show that when implementing such ideals against empirical
   trends in voter behaviour and government responsiveness, compulsory
   voting is vindicated rather than invalidated.},
