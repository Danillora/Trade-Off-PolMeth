@article{ WOS:000293309300009,
Author = {Flacks, Simon},
Title = {Drug Control, Human Rights, and the Right to the Highest Attainable
   Standard of Health: A Reply to Saul Takahashi},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2011},
Volume = {33},
Number = {3},
Pages = {856-877},
Month = {AUG},
Abstract = {A recent article in this journal challenged claims that a human rights
   framework should be applied to drug control. This article questions the
   author's assertions and reframes them in the context of socio-legal drug
   scholarship, aiming to build on the discourse concerning human rights
   and drug use. It is submitted that a rights-based approach is a
   necessary, indeed obligatory, ethical and legal framework through which
   to address drug use and that international human rights law provides the
   proper scope for determining where interferences with individual human
   rights might be justified on certain, limited grounds.},
