@article{ WOS:000299674400005,
Author = {Winton, Harold R.},
Title = {An Imperfect Jewel: Military Theory and the Military Profession},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2011},
Volume = {34},
Number = {6},
Pages = {853-877},
Abstract = {This article explores a perennial theme in the literature of strategic
   studies: the relationship between military theory and the military
   profession. It begins with a conceptual analysis of this relationship.
   It then investigates what military theorists themselves have had to say
   about the utility of their craft. It concludes by assessing the actual
   influence of military theory on selected individuals and institutions.
   The individuals are George S. Patton, Jr., and Ulysses S. Grant. The
   institutions are the United States Army and the United States Air Force
   in the late twentieth century. The fundamental finding is suggested in
   the title: military theory can indeed be quite useful in the maturation
   of military commanders and in the development of martial institutions,
   but it is not always necessary and by no means perfect. It should thus
   be studied assiduously but used with caution.},
