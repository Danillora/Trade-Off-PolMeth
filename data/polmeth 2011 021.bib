@article{ WOS:000289256700005,
Author = {Dyson, Tom},
Title = {Defence Policy under the Labour Government: Operational Dynamism and
   Strategic Inertia},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
