@article{ WOS:000288551800009,
Author = {Pollitt, Christopher},
Title = {NOT ODIOUS BUT ONEROUS: COMPARATIVE PUBLIC ADMINISTRATION},
Journal = {PUBLIC ADMINISTRATION},
