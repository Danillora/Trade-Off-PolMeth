@article{ WOS:000299742800008,
Author = {Ekmekci, Faruk},
Title = {Terrorism as war by other means: national security and state support for
   terrorism},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2011},
Volume = {54},
Number = {1},
Pages = {125-141},
Abstract = {The conventional approach in the discipline of International Relations
   is to treat terrorist organizations as ``non-state{''} actors of
   international relations. However, this approach is problematic due to
   the fact that most terrorist organizations are backed or exploited by
   some states. In this article, I take issue with the non-stateness of
   terrorist organizations and seek to answer the question of why so many
   states, at times, support terrorist organizations. I argue that in the
   face of rising threats to national security in an age of devastating
   wars, modern nation states tend to provide support to foreign terrorist
   organizations that work against their present and imminent enemies. I
   elaborate on my argument studying three cases of state support for
   terrorism: Iranian support for Hamas, Syrian support for the PKK, and
   American support for the MEK. The analyses suggest that, for many
   states, terror is nothing but war by other means.},
