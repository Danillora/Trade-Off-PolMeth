@article{ WOS:000288418700005,
Author = {White, Stuart},
Title = {Basic income versus basic capital: can we resolve the disagreement?},
Journal = {POLICY AND POLITICS},
Year = {2011},
Volume = {39},
Number = {1, SI},
Pages = {67-81},
Month = {JAN},
Abstract = {Some philosophers argue that a just society must provide all citizens
   with an unconditional resource endowment. But there is disagreement
   about whether this should take the form of a basic income or a capital
   grant. This article considers whether either of two of the types of
   argument used to defend the idea of a citizen's endowment (entitlement
   and freedom-based arguments) give reason to prefer one or the other. It
   argues that entitlement arguments give little reason either way; that
   different freedom-based arguments possibly give conflicting
   recommendations; and that a reasonable balance of freedom-based
   considerations might favour a hybrid of basic income and basic capital.},
