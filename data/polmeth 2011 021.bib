@article{ WOS:000289504900005,
Author = {da Conceicao-Heldt, Eugenia},
Title = {Variation in EU member states' preferences and the Commission's
   discretion in the Doha Round},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2011},
Volume = {18},
Number = {3},
Pages = {403-419},
Abstract = {During the period leading up to the 2005 WTO Hong Kong ministerial
   meeting and the 2006 Geneva informal meeting, European Union member
   states became even more strongly opposed to any further concessions on
   agricultural issues in the Doha Trade Round. Despite this opposition,
   the European Commission made a further offer which included concessions
   on agricultural issues. Based on data collected from Agence Europe and
   interviews with officials from the European Commission and the Council
   of Ministers, this contribution shows that preference heterogeneity with
   two camps of nearly equal size, a vague mandate and conflicting messages
   from principals all give the agent more discretion at the international
   level.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
