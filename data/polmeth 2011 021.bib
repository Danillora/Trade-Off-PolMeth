@article{ WOS:000294922500009,
Author = {Choi, Seung-Whan},
Title = {Re-Evaluating Capitalist and Democratic Peace Models},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
Year = {2011},
Volume = {55},
Number = {3},
Pages = {759-769},
Month = {SEP},
Abstract = {After replicating Oneal and Russett's (International Studies Quarterly,
   41, 1997, 267; Journal of Peace Research, 36, 1999, 423) democratic
   peace model, Gartzke's (American Journal of Political Science, 51, 2007,
   180) study contends that ``capitalism, and not democracy, leads to
   peace. Additional research is needed to corroborate, extend, and even
   refute the findings reported here.'' In response to this open
   invitation, this study re-evaluates Gartzke's capitalist peace model
