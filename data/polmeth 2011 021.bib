ISSN = {1369-5258},
EISSN = {1469-3569},
Keywords = {World Bank; International Development Association; foreign aid; debt;
   multilateral development institutions},
Keywords-Plus = {AID-ALLOCATION; MODELS; POLITICS; SELECTIVITY; ESTIMATOR; GROWTH; DEBT},
