@article{ WOS:000288274400007,
Author = {Mullins, Sam},
Title = {Islamist Terrorism and Australia: An Empirical Examination of the
   oHome-Growno Threat},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2011},
Volume = {23},
Number = {2},
Pages = {254-285},
Abstract = {Australian interests have been considered viable targets for Islamist
   terrorists since at least 2001, and Australians have suffered from
   attacks in Bali in 2002 and 2005, and Jakarta in 2004 and 2009.
   Moreover, Australian citizens have been involved in militant Islamist
   networks since the late 1980s, and similar to other Western countries in
   recent years there have been examples of ohome-growno plots to carry out
   domestic terrorist attacks. This article seeks to clarify the nature of
   the contemporary security threat within Australia by analysing the
   involvement of Australian citizens and residents in Islamist terrorism,
   both at home and abroad. The results build upon previous research
   findings revealing that while the profile of Australian jihadis is
