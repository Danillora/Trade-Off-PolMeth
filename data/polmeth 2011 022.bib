@article{ WOS:000299675100001,
Author = {May, Stephen},
Title = {Language Rights: The ``Cinderella{''} Human Right},
Journal = {JOURNAL OF HUMAN RIGHTS},
Year = {2011},
Volume = {10},
Number = {3},
Pages = {265-289},
Abstract = {In the last 60 years, we have seen the growing development and
   articulation of human rights, particularly within international law and
   within and across supranational organizations. However, in that period,
   the right to maintain one's language(s), without discrimination, remains
   peculiarly underrepresented and/or problematized as a key human right.
   This is primarily because the recognition of language rights presupposes
   recognizing the importance of wider group memberships and social
   contexts; conceptions that ostensibly militate against the primacy of
   individual rights in the post-Second World War era. Drawing on
   theoretical debates in political theory and international law, as well
   as the substantive empirical example of Catalonia, this article argues
   that language rights can and should be recognized as an important human
   right.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
