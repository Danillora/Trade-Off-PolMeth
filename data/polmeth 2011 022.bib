@article{ WOS:000292327600008,
Author = {Bishop, Matthew Louis},
Title = {Slaying the `Westmonster' in the Caribbean? Constitutional Reform in St
   Vincent and the Grenadines},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
Year = {2011},
Volume = {13},
Number = {3},
Pages = {420-437},
Month = {AUG},
Abstract = {The model of governance bequeathed by Britain to the Caribbean has long
   been praised for ensuring that vibrant democratic practice has generally
   prevailed since the era of decolonisation. Yet it has also become
   increasingly clear that the Westminster system of politics, as it is
   applied to the region's tiny polities, has come to exhibit serious
   deficiencies and is in need of reform. The current government of St
   Vincent and the Grenadines has recently made a serious attempt to
   address the issue, engaging in an expansive process of public
   consultation and constitutional reform which was ultimately rejected by
   the populace in a referendum held in November 2009. This article seeks
   to analyse both the form and content of the constitutional reform
   process, situating it within broader debates about the Westminster Model
   itself.},
