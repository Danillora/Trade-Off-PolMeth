@article{ WOS:000421275000003,
Author = {Massif, Justin and Morin, David},
Title = {Francophonie and peace operations. Towards a geocultural appropriation},
Journal = {ETUDES INTERNATIONALES},
Year = {2011},
Volume = {42},
Number = {3},
Pages = {313-336},
Abstract = {This paper critically examines the purposes and objectives of the
   Organisation internationale de la Francophonie (OIF). While analysts
   debate whether the OIF is a hegemonic or counter-hegemonic instrument of
   statecraft, we argue that it is an institution founded upon the
   principle of geopolitical ownership of a distinctive geocultural space.
   Its prime task, with regards to conflict resolution, is to help transfer
   responsibilities and capabilities from Northern to Southern member
   states. From this perspective, we contend that the OIF has progressively
   taken charge of a distinct and necessary geocultural role with regards
   to international security.},
