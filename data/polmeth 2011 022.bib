@article{ WOS:000290489600012,
Author = {Su, Fubing and Ran, Tao and Sun, Xin and Liu, Mingxing},
Title = {Clans, Electoral Procedures and Voter Turnout: Evidence from Villagers'
   Committee Elections in Transitional China},
Journal = {POLITICAL STUDIES},
Year = {2011},
Volume = {59},
Number = {2},
Pages = {432-457},
Month = {JUN},
Abstract = {Villagers' committee elections in Chinese rural areas highlight the
   promise as well as the challenge of democratization in a one-party
   state. Why are there huge regional variations in terms of voter turnout
   rate? Based on a large survey conducted in 2005, we argue that electoral
   procedures have played a crucial role in signaling the democratic intent
   of local governments. Villagers were more responsive to more meaningful
   elections with higher-quality electoral procedures. In addition,
   competitive politics has reactivated traditional social ties and
   networks in village lives, which have mobilized villagers to voting
   stations. The results are robust to endogeneity tests and other model
   specifications. These empirical findings highlight electoral dynamics
   that are common in many transitional democracies. They also imply some
   major challenges facing China's rural democracy.},
Publisher = {SAGE PUBLICATIONS LTD},
