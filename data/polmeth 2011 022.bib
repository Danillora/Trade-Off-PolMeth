@article{ WOS:000296956100003,
Author = {Elcock, Howard},
Title = {Citizenship and the public interest throughout the ages: pointers to a
   new politics?},
Journal = {POLICY AND POLITICS},
Year = {2011},
Volume = {39},
Number = {2},
Pages = {187-202},
Month = {APR},
Abstract = {Political philosophers have for centuries postulated the existence of a
   public interest beyond the sum of individual citizens' interests. Three
   definitions are transcendental, individualistic and instrumental.
   Transcendental definitions were related to failed ideologies. The
   individualistic definition has dominated political discourse for the
   last 30 years. However, it has now been found wanting because it is too
   permissive of greed and corruption. An instrumental definition must
   include accountability, legality, integrity and responsiveness and
   recognise the role of citizens as the community's governors. Only then
   can a start be made in recovering public allegiance for governments and
   the political process.},
