@article{ WOS:000293745500008,
Author = {Navarrete, J. Manuel and Aragon-Durand, Fernando},
Title = {Garment Maquiladoras in the South A Closer Look at the Mayan Maquiladora
   Workforce},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2011},
Volume = {38},
Number = {5},
Pages = {93-109},
Month = {SEP},
Abstract = {The aggregated economic impact of textile maquiladoras in rural Yucatan
   has recently been studied, but little is known about the characteristics
   of the maquila workforce there. A case study of a garment maquiladora in
   Motul in 2003 focused on these characteristics and provides insight into
   the region's maquiladora operations. In contrast to northern urban
   maquiladoras, the Motul garment maquiladora has attracted mainly young
   workers, primarily men. Similar to its northern urban counterparts, it
   is characterized by low salaries, high turnover, and limited use of
   Mexican inputs.},
