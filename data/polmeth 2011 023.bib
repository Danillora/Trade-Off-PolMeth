@article{ WOS:000299465500002,
Author = {Crone, Manni and Harrow, Martin},
Title = {Homegrown Terrorism in the West},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2011},
Volume = {23},
Number = {4},
Pages = {521-536},
Abstract = {The London bombings in 2005 led to the perception that the terrorist
   threat had changed from external to internal. This became conceptualized
   shortly after as ``homegrown terrorism.{''} This article deals with the
   meaning and scope of this phenomenon. We begin by tracing an ambiguity
   in the term ``homegrown,{''} which is both about belonging in the West
   and autonomy from terrorist groups abroad. A quantitative study of
   Islamist terrorism in the West since 1989 reveals an increase in both
   internal and autonomous terrorism since 2003 and that most plots are now
   internal-but not autonomous. Finally we suggest that an increase in
   autonomous terrorism is a transitory phenomenon.},
