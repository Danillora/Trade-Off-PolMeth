@article{ WOS:000294931700002,
Author = {Garcia-Lacalle, Javier and Martin, Emilio and Royo, Sonia},
Title = {Strengthening of Citizen Role in the Public Healthcare Systems in Europe},
Journal = {REVISTA DEL CLAD REFORMA Y DEMOCRACIA},
Year = {2011},
Number = {50},
Pages = {45+},
Month = {JUN},
Abstract = {The provision of healthcare services is one of the main pillars of the
   welfare State and one of the public services to which citizens are more
   sensitive. In recent years, many European countries have reformed their
   healthcare systems to focus them on citizens and patients. These reforms
   try to improve healthcare services, to empower citizens and to enhance
   citizens' trust in governments.
   This paper evaluates the degree of orientation towards the patients of
   the public healthcare systems of European countries and some factors
   that influence it. The results indicate that the orientation towards
   patients is related, in addition to the expenditure devoted to
   healthcare, to the public administration style and to the organization
   of the public healthcare system.},
