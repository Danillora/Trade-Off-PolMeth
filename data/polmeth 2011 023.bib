@article{ WOS:000295098700003,
Author = {Landwehr, Claudia and Boehm, Katharina},
Title = {Delegation and Institutional Design in Health-Care Rationing},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2011},
Volume = {24},
Number = {4},
Pages = {665-688},
Month = {OCT},
Abstract = {The delegation of decision-making powers to nonmajoritarian, independent
   agencies has become a significant phenomenon in more and more policy
   areas. One of these is the health-care sector, where decisions on the
   range of services covered within public systems have, in most developed
   countries, been delegated to specialized bodies. This article offers an
   analytical framework that seeks to grasp the empirical variety and
   complexity of delegative processes and appointed institutions. The
   framework is used to describe decision-making processes and institutions
   in six countries: Austria, Germany, Norway, Sweden, New Zealand, and the
   United Kingdom. We find that, although constrained by preexisting
   institutional structures and traditions, delegators enjoy a considerable
   degree of discretion in their institutional design choices and engage in
   strategic design and redesign of appointed bodies.},
