@article{ WOS:000292101100006,
Author = {Ferlie, Ewan and Fitzgerald, Louise and McGivern, Gerry and Dopson, Sue
   and Bennett, Chris},
Title = {PUBLIC POLICY NETWORKS AND `WICKED PROBLEMS': A NASCENT SOLUTION?},
Journal = {PUBLIC ADMINISTRATION},
Year = {2011},
Volume = {89},
Number = {2},
Pages = {307-324},
Abstract = {The last two decades have seen a shift in public services organizations
   from hierarchies to networks. Network forms are seen as particularly
   suited to handling `wicked problems'. We make an assessment of the
   nature and impact of this shift. Using recent evidence from the United
   Kingdom (UK) National Health Service (NHS), we explore the nature and
   functioning of eight different public policy networks. We are also
   interested in whether there has been a radical transition - or not -
   from hierarchical to network forms.},
