@article{ WOS:000299675100005,
Author = {Root, Rebecca K.},
Title = {The Fujimori Effect},
Journal = {JOURNAL OF HUMAN RIGHTS},
Year = {2011},
Volume = {10},
Number = {3},
Pages = {358-375},
Abstract = {This article examines how efforts to hold Alberto Fujimori, president of
   Peru from 1990 to 2000, accountable for human rights violations and to
   build safeguards against a recurrence of those violations have set
   important precedents for international human rights law and practice.
   Among the key developments his crimes have provoked are new protections
   for democracy as a human right via the Inter-American Democratic
   Charter; international legal precedents banning the use of amnesties for
   human rights crimes and the use of military courts to try alleged
   terrorists; and the extradition of a democratically elected former head
   of state for human rights violations and his conviction for those crimes
   by the courts of his own country.},
