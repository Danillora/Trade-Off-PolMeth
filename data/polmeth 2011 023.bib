@article{ WOS:000285415200007,
Author = {Goldman, Ogen},
Title = {The Globalization of Terror Attacks},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2011},
Volume = {23},
Number = {1},
Pages = {31-59},
Abstract = {This study raises two questions: have terror attacks been globalized and
   if so, is the phenomenon somehow linked with globalization. Two opposite
   hypotheses are tested by trend analysis for the years 1968-2007, using
   several statistical tools. The principal conclusions are that terror
   attacks have not become globalized and even display signs of
   localization since the 1990s. Furthermore, globalization is not
   significantly connected, positively or negatively, with transnational
   terror attack trends.},
