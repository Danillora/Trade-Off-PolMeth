@article{ WOS:000296956100004,
Author = {Acheson, Nick},
Title = {A case of avoiding `political mumbo jumbo': do collective identities
   within ethnically diverse voluntary associations spill over to other
   contexts? Reflections on Northern Ireland's experience},
Journal = {POLICY AND POLITICS},
Year = {2011},
Volume = {39},
Number = {2},
Pages = {203-220},
Month = {APR},
Abstract = {This article explores the role of voluntary associations in recent
   policies that address ethnic conflict where they are portrayed both as a
   source of division but also as part of a policy solution. This role in
   policy relies on the assumption that bridging social capital in
   associations that bring diverse people together can spill over to other
   contexts. Reporting recent research in Northern Ireland, the article
   challenges this view both theoretically and empirically and finds that
   an important limiting factor is the degree of recognition that
   collective identities gain outside the specific settings in which they
   are generated.},
