@article{ WOS:000289164400007,
Author = {Walker, Richard M. and Damanpour, Fariborz and Devece, Carlos A.},
Title = {Management Innovation and Organizational Performance: The Mediating
   Effect of Performance Management},
Journal = {JOURNAL OF PUBLIC ADMINISTRATION RESEARCH AND THEORY},
Year = {2011},
Volume = {21},
Number = {2},
Pages = {367-386},
Month = {APR},
Abstract = {Management innovations (MIs) are widely adopted, but their influences on
   organizational performance are little researched in public settings.
   Positing that the MI-performance relationship is complex and is
   conditional over other characteristics of the organization, we examine
   the influence of MI on organizational performance both directly and
   indirectly through performance management (PM). PM is an important
   organizational process characteristic arising from public management
   reform and in itself influences performance. We test our hypotheses
   using structural equation models with data from three sources in English
   local governments. The findings indicate that the impact of MI on
   performance is not direct; it is mediated by PM. We also find that PM
   positively affects organizational performance. In conclusion, we discuss
   the implications of these findings for research and practice.},
Publisher = {OXFORD UNIV PRESS},
