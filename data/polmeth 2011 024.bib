@article{ WOS:000297538100001,
Author = {Villoria, Manuel},
Title = {More Freedom or More Happiness? The Good Government of the 21st Century},
Journal = {REVISTA DEL CLAD REFORMA Y DEMOCRACIA},
Year = {2011},
Number = {51},
Pages = {5+},
Month = {OCT},
Abstract = {The question ``what should a good government do in the 21st century?{''}
   is the key driver of this investigation. According to normative
   political theory it has been found two possible answers: to promote
   freedom or to promote happiness. Epistemologically, it is assumed the
   idea that there are several values which may be equally correct and
   fundamental, and yet in conflict with each other (value pluralism). That
   is the reason for which the ideas from the deontological ethics and from
   the utilitarianism are used in order to answer the question.
   Finally, two concepts of good government are offered: strictly speaking
   and in the broadest sense. Strictly speaking it could be the combination
   of certain ideas from deontological and utilitarian ethics. In the
   broadest sense, good government is the result of the sum of good
   society, good institutions and strictly speaking good government.},
