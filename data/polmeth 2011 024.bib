@article{ WOS:000285193300004,
Author = {Nabatchi, Tina and Goerdel, Holly T. and Peffer, Shelly},
Title = {Public Administration in Dark Times: Some Questions for the Future of
   the Field},
Journal = {JOURNAL OF PUBLIC ADMINISTRATION RESEARCH AND THEORY},
Year = {2011},
Volume = {21},
Number = {1},
Pages = {I29-I43},
Month = {JAN},
Abstract = {This essay identifies two problems that impede the ability of public
   administration to govern effectively in dark times. First, public
   administration has failed to adequately acknowledge itself as an arbiter
   of political conflict and as a discipline responsible for shaping
   societal affairs. Second, the field is entrenched in a bureaucratic
   pathology that limits its capacity to address complex policy problems.
   We argue that these issues show a clear need for the reinvigoration of
   democratic ethos as the foundation for public administration. Building
   on the ideas of some Minnowbrook III working groups, we pose questions
   to help begin discussions about both democratic ethos and the ability of
   public administration to govern in dark times.},
