@article{ WOS:000421270900002,
Author = {Massie, Justin},
Title = {Canada, France and the Transformation of the Atlantic Alliance:
   Converging International Security Policies},
Journal = {ETUDES INTERNATIONALES},
Year = {2011},
Volume = {42},
Number = {1},
Pages = {25-46},
Abstract = {Canada has supported each of NATO's major initiatives since its
   inception. The current transformation of the Alliance, including the
   reintegration of France into its integrated military command structures,
   will have an undeniable impact on the future of Canada's international
   security policy. To assess this impact, the article first highlights the
   convergence of France's and Canada's Atlanticist policies, which its
   labels inclusive institutional balancing strategies. Faced with the
   dilemma of a global and expeditionary NATO or a defensive alliance and
   European pillar, the article concludes that Canada can overcome the
   alleged dichotomy of its choice by putting forward a policy of national
   and regional ownership of conflict resolution processes.},
