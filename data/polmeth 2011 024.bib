@article{ WOS:000286483000003,
Author = {Jarvis, Darryl S. L.},
Title = {Race for the money: international financial centres in Asia},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2011},
Volume = {14},
Number = {1},
Pages = {60-95},
Month = {JAN},
Abstract = {Asia's emergence as a key player in the global economy is witnessing
   intense competition within the region to become Asia's next great
   international financial centre (IFC). Hong Kong, Singapore and Shanghai,
   among others, are vying for primacy, attempting to attract dense
   clusters of financial services firms and reap the lucrative rewards
   associated with this. This paper explores this emerging competition. It
   does so from the perspective of attempting to map the parameters
   necessary to become an IFC, particularly the institutional, political
   and spatial contexts that facilitate the concentration of international
   financial services. Why and how financial clustering occurs and the
   factors that determine the location of financial centres is an important
   public policy concern, both for established centres eager to maintain
   their competitive position as well as emerging economies keen to
   identify the policy levers necessary to support financial sector growth.
