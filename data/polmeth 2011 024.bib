   Gerring, John; Van Gorp, Johan, Boston Univ, Dept Polit Sci, Boston, MA 02215 USA.
   Ziblatt, Daniel, Harvard Univ, Cambridge, MA 02138 USA.},
DOI = {10.1017/S0043887111000104},
ISSN = {0043-8871},
EISSN = {1086-3338},
Keywords-Plus = {COLONIAL RULE; DECENTRALIZATION; CENTRALIZATION; POLITICS; QUALITY;
