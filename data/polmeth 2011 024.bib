@article{ WOS:000288469800004,
Author = {Forsby, Andreas Boje},
Title = {Power, Identity, and Security Politics: A New Theoretical Trinity?},
Journal = {INTERNASJONAL POLITIKK},
Year = {2011},
Volume = {69},
Number = {1},
Pages = {59+},
Abstract = {For more than three decades, and despite numerous critics, the study of
   security politics among states has been dominated by structural realism
   and its power-permeated conceptualization of the international system.
   Within a constructivist framework, this article demonstrates how the
   identity concept can be theorized into a system-structural counterpart
   to the realist model, thereby not only filling out a conspicuous gap in
   the existing constructivist literature, but also paving the way for
   theoretically coupling power and identity in a simple, consistent and
   fruitful manner. The result is a state-centric model of systemic power
   and identity structures, the interplay of which creates a more or less
   stable main pattern of conflict that temporarily conditions the security
   behaviour of states in the international system. The explanatory power
   of the model is briefly illustrated in light of the overall patterns of
   state security behaviour in the post-Cold War era.},
