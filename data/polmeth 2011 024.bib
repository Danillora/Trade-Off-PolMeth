@article{ WOS:000287706500003,
Author = {Minkler, Lanse},
Title = {On the Cost of Economic Rights in the United States},
Journal = {JOURNAL OF HUMAN RIGHTS},
Year = {2011},
Volume = {10},
Number = {1},
Pages = {34-54},
Abstract = {I estimate that respecting economic rights in the United States would
   have added \$123 billion to the federal budget in 2006 and \$308 billion
   in 2009. In 2006, the additional benefit to crime reduction would have
   been \$3 billion, while the additional benefits to states would have
   totaled \$30 billion. The federal net cost estimates are qualitatively
   similar to those of Harvey (1989), despite our different assumptions.
   Importantly, I assume that a government employment program would add a
   new federal bureaucracy with 2.3 million administrative workers. A
   government employment program would have increased the federal budget by
   4.6\% in 2006, but if the cost of civil and political rights can be
   represented by total government outlays on defense and protection, then
   the additional cost of protecting economic rights would have been only
