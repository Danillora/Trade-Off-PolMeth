@article{ WOS:000296668600003,
Author = {Copus, Colin},
Title = {Elected Mayors in English Local Government: Mayoral Leadership and
   Creating a new Political Dynamic},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2011},
Volume = {9},
Number = {4},
Pages = {335-351},
Month = {OCT},
Abstract = {The introduction of directly elected mayors into English local
   government was intended to bring about a new form of local political
   leadership, revitalise local politics and re-engage citizens with local
   democracy. Yet, English elected mayors were not given any new powers to
   distinguish them, or their councils, from the rest of local government
   or powers that would enable them to change the dynamic of local
