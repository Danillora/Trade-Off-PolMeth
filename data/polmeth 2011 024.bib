@article{ WOS:000287710900005,
Author = {Brands, Hal},
Title = {Inside the Iraqi State Records: Saddam Hussein, `Irangate', and the
   United States},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2011},
Volume = {34},
Number = {1},
Pages = {95-118},
Abstract = {This article uses captured Iraqi state records to examine Saddam
   Hussein's reaction to US arms to sales to Iran during the Iran-Iraq War
   (the Iran/Contra scandal). These records show that `Irangate' marked a
   decisive departure in Saddam's relations with the United States.
   Irangate reinforced Saddam's preexisting suspicions of US policy,
   convincing him that Washington was a strategic enemy that could not be
   trusted. Saddam concealed his anger to preserve a working relationship
   with the Reagan administration, but this episode nevertheless cemented
   his negative views of the United States and forged a legacy of hostility
   and mistrust that would inform his strategic calculus for years to come.},
