@article{ WOS:000212537800005,
Author = {Yoon, Mi Yung},
Title = {Factors hindering `larger' representation of women in parliament: the
   case of Seychelles},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2011},
Volume = {49},
Number = {1},
Pages = {98-114},
Abstract = {Female legislative representation in some sub-Saharan African countries
   surpasses that of most other countries on the Inter-Parliamentary
   Union's 187 country list by exceeding 20 per cent. While almost all of
   those African countries achieved such level of representation by
   implementing gender quotas, Seychelles achieved that level without
   gender quotas. Its female parliamentary representation, however, is far
   short of the 50 per cent target set for 2015 by the African Union. This
   study, therefore, examines the factors hindering `larger' representation
   of women in the Seychellois parliament. It concludes that the dual
   burden of women, the longstanding traditional beliefs about women's
   role, the lukewarm commitment of political parties to nominate women,
   the authoritarian nature of Seychellois politics, and parliamentarians'
   financial contributions expected by constituents are the key variables
   hindering `larger' female legislative representation in Seychelles.},
