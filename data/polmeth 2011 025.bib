@article{ WOS:000291273900008,
Author = {Aydinli, Ersel and Ozcan, Nihat Ali},
Title = {The Conflict Resolution and Counterterrorism Dilemma: Turkey Faces its
   Kurdish Question},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2011},
Volume = {23},
Number = {3},
Pages = {438-457},
Abstract = {This article considers the relationship between two processesconflict
   resolution and counterterrorismwhich conceptually share many common
   points, yet in practice do not necessarily proceed together easily
   towards a common goal. Considering particular cases of ethnic conflict
   in which terrorist factions exist, the article argues that while neither
   conflict resolution nor counterterrorism alone can adequately address
   the problem, simultaneously conducting both must keep in mind the
   processes' inherent differences and avoid excessive prioritizing of one
   over the other. By exploring recent Turkish governmental initiatives to
   address the Kurdish question, the article attempts to provide an outline
   for how to successfully cope with the two processes simultaneously.},
