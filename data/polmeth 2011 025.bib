@article{ WOS:000285415200001,
Author = {Etzioni, Amitai},
Title = {Terrorists: A Distinct Species},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2011},
Volume = {23},
Number = {1},
Pages = {1-12},
Abstract = {This article first lays out arguments that explain why terrorists should
   not be treated as criminals or as soldiers. It suggests that they are
   best characterized as a third, distinct species. Although their basic
   rights should be honored, e.g., they should not be tortured, on all
   other matters they should be subject to distinct authorities and
   procedures. For instance, instead of being held indefinitely without
   review, they should be held until a review board (somewhat like a parole
   board in prison) determines that they are no longer a danger to society.},
