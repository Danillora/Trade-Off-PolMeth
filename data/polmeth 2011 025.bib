@article{ WOS:000290990600004,
Author = {Borras, Susana and Peters, B. Guy},
Title = {The Lisbon Strategy's empowerment of core executives: centralizing and
   politicizing EU national co-ordination},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2011},
Volume = {18},
Number = {4, SI},
Pages = {525-545},
Abstract = {This paper studies the effects of the Lisbon Strategy on the way in
   which national executives co-ordinate EU policy at the domestic level.
   Comparing seven countries (Denmark, the United Kingdom {[}UK], Austria,
   Slovenia, Spain, France and Poland) it finds evidence that the Lisbon
   Strategy has been advancing (further) centralization and politicization
   in national patterns of EU policy co-ordination, empowering core
   executives. The Lisbon Strategy's ideational elements ('grand' goals and
   politically visible targets) as well as organizational requirements
   (Spring Council, national programming and annual reports) are factors
   behind this phenomenon. These results have implications for the
   literature on Europeanization, international politics, and governance
   studies in what is eminently an empirical research agenda about how far
   and how changes in international governance architectures affect the
   redistribution of power within national executives.},
