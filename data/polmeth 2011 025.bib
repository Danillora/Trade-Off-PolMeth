@article{ WOS:000289331300001,
Author = {Ash, Konstantin},
Title = {A game-theoretic model for protest in the context of post-communism},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2011},
Volume = {44},
Number = {1},
Pages = {1-15},
Month = {MAR},
Abstract = {In recent years, large-scale protests have forced several incumbent
   governments in former Soviet countries from power. Scholarly
   examinations of these events have lacked a cohesive explanation of the
   reasons for the success of certain movements and the failure of others.
   This study uses prior research on the dynamics of protest to formulate a
   game-theoretic model for why protest takes place and how its eventual
   outcome comes about. The model is tested through logistic regression
   analyses of monthly protest data. The statistical analysis shows that
   elections, prior protests and government transgressions increase the
   likelihood of anti-government protests. (C) 2011 Published by Elsevier
   Ltd on behalf of The Regents of the University of California.},
