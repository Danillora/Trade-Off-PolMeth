@article{ WOS:000291811700005,
Author = {Tyroler-Cooper, Samm and Peet, Alison},
Title = {The Chinese Aviation Industry: Techno-Hybrid Patterns of Development in
   the C919 Program},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2011},
Volume = {34},
Number = {3, SI},
Pages = {383-404},
Abstract = {The C919 program is representative of China's broader approach to
   advancing its aviation industry: dependence on foreign partnerships
   alongside investment in indigenous development. We find that this
   approach most closely fits the techno-hybrid model of technological
   development. This combination of techno-globalism and techno-nationalism
