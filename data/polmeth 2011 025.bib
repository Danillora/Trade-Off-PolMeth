@article{ WOS:000293545800004,
Author = {Oesch, Daniel and Rodriguez Menes, Jorge},
Title = {Upgrading or polarization? Occupational change in Britain, Germany,
   Spain and Switzerland, 1990-2008},
Journal = {SOCIO-ECONOMIC REVIEW},
