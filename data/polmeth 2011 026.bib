@article{ WOS:000294456000001,
Author = {Pinto, Sanjay and Macdonald, Kate and Marshall, Shelley},
Title = {Rethinking Global Market Governance: Crisis and Reinvention?},
Journal = {POLITICS \& SOCIETY},
