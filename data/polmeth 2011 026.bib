@article{ WOS:000295939500003,
Author = {Duvanova, Dinissa S.},
Title = {Firm Lobbying versus Sectoral Organization: The Analysis of
   Business-State Relations in Post-Communist Russia},
Journal = {POST-SOVIET AFFAIRS},
Year = {2011},
Volume = {27},
Number = {4},
Pages = {387-409},
Month = {OCT-DEC},
Abstract = {In the past two decades, Russia has experienced a tremendous growth in
   the number of industry associations, but different sectors of the
   economy have had differential success in developing formal organizations
   to pursue common interests collectively. Using data from two major
   surveys to focus on institutional channels of representation, this
   article analyzes the number of and membership levels in industry
   associations representing different sectors of the Russian economy. The
   effects of single-firm lobbying, organizational fragmentation, and
   consolidated representation are considered. The possible impacts of
   Russia's formal sectoral associations on the overall patterns of
   business-state relations and economic policymaking are also addressed.},
