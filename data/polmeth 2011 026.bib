@article{ WOS:000299221200003,
Author = {Sierra, Oscar B. Pardo},
Title = {Shaping the Neighbourhood? The EU's Impact on Georgia},
Journal = {EUROPE-ASIA STUDIES},
Year = {2011},
Volume = {63},
Number = {8},
Pages = {1377-1398},
Abstract = {Drawing on the literature of EU governance in its neighbourhood, this
   article investigates the EU's impact on Georgia by examining the
   following policies regarding EU-Georgia relations during the period
   2004-2008: single market access; energy security; and foreign and
   security policy. It argues that policy-level incentives and the overall
   process of deepening relations with the EU are necessary conditions for
   the EU to make an impact. However, the EU-promoted rules and norms are
   key conditions for EU impact and for understanding whether they are
   adopted or not. The case of Georgia shows the limits of the EU's
   normative power. Geopolitical pressures and alternative models of
   development present a challenge for EU impact.},
