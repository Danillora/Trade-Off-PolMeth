@article{ WOS:000293019900004,
Author = {Haslam, Emily},
Title = {Subjects and Objects: International Criminal Law and the
   Institutionalization of Civil Society},
Journal = {INTERNATIONAL JOURNAL OF TRANSITIONAL JUSTICE},
Year = {2011},
Volume = {5},
Number = {2},
Pages = {221-240},
Month = {JUL},
Abstract = {This article argues that international criminal law distinguishes
   between `civil society as subject' (expert international nongovernmental
   organizations and transnational networks of activists voluntarily
   entering into the international criminal justice arena) and `civil
   society as object' (objects of the purportedly transformative processes
   of international criminal law). Although the International Criminal
   Court (ICC) has adopted a different approach towards civil society than
   that of the ad hoc Tribunals, it too struggles to provide `civil society
   as object' with a meaningful opportunity to participate, as exemplified
   by continuing difficulties with the operation of victim participation.
   And yet, just as `civil society as subject' has benefited from the
   institutionalization of civil society at the ICC, so has the ICC
   benefited from the mobilization of the expertise of `civil society as
   subject.' The danger is that other voices are thereby excluded to the
   detriment of justice.},
