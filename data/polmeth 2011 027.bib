@article{ WOS:000292748300003,
Author = {Forest, Benjamin and Johnson, Juliet},
Title = {Monumental Politics: Regime Type and Public Memory in Post-Communist
   States},
Journal = {POST-SOVIET AFFAIRS},
Year = {2011},
Volume = {27},
Number = {3},
Pages = {269-288},
Month = {JUL-SEP},
Abstract = {This article looks at collective memory formation-the study of
   monuments, memory, and public space-through a political science lens. An
   explicit theoretical focus on power relations in ``monumental
   politics{''} and a methodological approach featuring large-N comparative
   analysis are combined to examine the process of monument creation,
   destruction, and alteration. Using a new database on monuments in 26
   post-communist states over a 25-year period, patterns of monument
   transformation are identified, and official and private efforts to
   transform monuments are systematically compared across different regime
   types. The complex implications of private provision and alteration of
   monuments is also discussed.},
