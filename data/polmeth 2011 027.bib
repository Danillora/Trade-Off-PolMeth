@article{ WOS:000299220900004,
Author = {Tokluoglu, Ceylan},
Title = {The Political Discourse of the Azerbaijani Elite on the Nagorno-Karabakh
   Conflict (1991-2009)},
Journal = {EUROPE-ASIA STUDIES},
Year = {2011},
Volume = {63},
Number = {7},
Pages = {1223-1252},
Abstract = {This article attempts to analyse the political discourse of some of the
   Azerbaijani elite on the Nagorno-Karabakh conflict through the lens of
   the views of leading members of Azerbaijani society. The information is
   based on personal interviews conducted with some influential members of
   the Azerbaijani political society in Baku in December 2001 and April
   2009. The focus of the study was to analyse how Azerbaijanis (re)
   construct their ideas about Armenian identity and community. The
   Azerbaijani narration of the Nagorno-Karabakh conflict reveals how they
   have isolated themselves from their neighbours and from Western powers
   and how they legitimise their isolation.},
