@article{ WOS:000299433000003,
Author = {Onis, Ziya and Guven, Ali Burak},
Title = {Global Crisis, National Responses: The Political Economy of Turkish
   Exceptionalism},
Journal = {NEW POLITICAL ECONOMY},
Year = {2011},
Volume = {16},
Number = {5},
Pages = {585-608},
Abstract = {With its dilatory and piecemeal fiscal activism and uncharacteristic
   aversion to IMF assistance, the Turkish government's response to the
   global economic crisis of 2008-9 diverged considerably from prevalent
   trends in other major emerging market countries. Underlying this
   intriguing pattern were Turkey's pre-existing policy and macroeconomic
   constraints, cognitive lapses on the part of policymakers, and the
   conjunctural dynamics of domestic politics. The interplay of these
   factors progressively narrowed the policy space for vigorous action,
   leading to a motley combination of reactive initiatives that neither
   offered sufficient protection to vulnerable social groups nor promised
   sustainable growth in the long run despite rapid short-term recovery.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
