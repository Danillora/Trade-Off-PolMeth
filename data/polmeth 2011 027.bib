@article{ WOS:000300011500001,
Author = {David, Assaf and Nanes, Stefanie},
Title = {The Women's Quota in Jordan's Municipal Councils: International and
   Domestic Dimensions},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2011},
Volume = {32},
Number = {4},
Pages = {275-304},
Abstract = {Quotas are products of interactions between international and local
   factors and should be analyzed through those interactions. This article
   examines the processes of passage and implementation of a gender quota
   on Jordan's municipal councils and the public discourse accompanying
   those processes. Preliminary results show the quota encouraged many
   women to enter the political arena, but public discourse in Jordan does
   not look favorably on international intervention in women's issues.
   Although it is too soon to determine the quota's outcome, the process by
   which the public sphere comes to understand the quota will shape its
   long-term effects in significant ways.},
