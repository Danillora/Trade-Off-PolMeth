@article{ WOS:000291220200002,
Author = {Eichler, Stefan},
Title = {What Can Currency Crisis Models Tell Us about the Risk of Withdrawal
   from the EMU? Evidence from ADR Data},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2011},
Volume = {49},
Number = {4},
Pages = {719-739},
Month = {JUL},
Abstract = {We study whether ADR (American depositary receipt) investors perceive
   the risk that countries such as Greece, Ireland, Italy, Portugal or
   Spain could leave the eurozone to address financial problems produced by
   the sub-prime crisis. Using daily data, we analyse the impact of
   vulnerability measures related to currency crisis theories on ADR
   returns. We find that ADR returns fall when yield spreads of sovereign
   bonds or CDSs (credit default swaps) rise (i.e. when debt crisis risk
   increases); when banks' CDS premiums rise or stock returns fall (i.e.
   when banking crisis risk increases); or when the euro's overvaluation
   increases (i.e. when the risk of competitive devaluation increases).},
