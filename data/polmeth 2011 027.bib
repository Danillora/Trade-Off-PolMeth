@article{ WOS:000294345700004,
Author = {Moser, Christoph and Sturm, Jan-Egbert},
Title = {Explaining IMF lending decisions after the Cold War},
Journal = {REVIEW OF INTERNATIONAL ORGANIZATIONS},
