@article{ WOS:000292717700009,
Author = {Hutchins, Rachel D.},
Title = {Heroes and the renegotiation of national identity in American history
   textbooks: representations of George Washington and Abraham Lincoln,
   1982-2003},
Journal = {NATIONS AND NATIONALISM},
Year = {2011},
Volume = {17},
Number = {3},
Pages = {649-668},
Month = {JUL},
Abstract = {American history textbooks for the USA's public schools act as
   quasi-official loci for the renegotiation of national identity and are,
   as such, subject to much controversy. The choice of heroes and the way
   in which textbooks depict them display the interplay between competing
   visions of popular ethno-history and scholarly historiography. This
   article examines contemporary renegotiation of the national narrative
   through an analysis of the evolving representation of the USA's two most
   prominent traditional national heroes - George Washington and Abraham
   Lincoln - in history textbooks for elementary-school students published
   from the early 1980s to 2003. This period marks the development of the
   multiculturalist movement and its subsequent conservative backlash, with
   debates intensifying in the wake of the events of 11 September 2001.},
