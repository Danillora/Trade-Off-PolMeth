@article{ WOS:000290411400002,
Author = {Petroni, Suzanne},
Title = {Historical and Current Influences on United States International Family
   Planning Policy},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2011},
Volume = {32},
Number = {1},
Pages = {28-51},
Abstract = {The United States has played a leading role in the international
   population arena for more than four decades. In the early years, support
   among US policymakers for family planning assistance to developing
   countries was strong, bipartisan, and centered on slowing the growth of
   these countries' populations. Since the early 1980s, US assistance has
   been highly politicized and increasingly partisan. Some blame this shift
   on feminists, who sought a role for government based on supporting
   individual freedom and women's health, rather than achieving demographic
   goals. This article suggests that the political shift resulted in large
   part from the growing political influence of the Religious Right in the
   United States.},
