@article{ WOS:000212623000003,
Author = {Buhari-Gulmez, Didem},
Title = {Ombudsmanship and Turkey's Europeanization in `World Society'},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2011},
Volume = {19},
Number = {4, SI},
Pages = {475-487},
Abstract = {This article investigates Europeanization in a candidate country from a
   macro-sociological perspective. From the point of view of sociology,
   extant Europeanization studies treat culture only in a reductionist
   manner. In particular, `sender-receiver' models overlook the domestic
   perceptions that translate, vernacularize or reframe exogenous
   conditionality. Furthermore, `goodness of fit' approaches `bringing
   domestic agency back in' misleadingly reinforce the image that the
   domestic and the external spheres are easily separated. By studying the
   ombudsmanship reform in Turkey, the article concludes that: (1) the
   content of Europeanization is not purely European; (2) the role for
   domestic norm entrepreneurs in leading cultural change is variable and
   constrained; and (3) domestic motivations for Europeanization include
   exogenous factors trespassing upon EU conditionality, that is, global
   culture/world society.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
