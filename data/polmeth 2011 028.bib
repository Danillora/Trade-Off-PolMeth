@article{ WOS:000296353700005,
Author = {Selb, Peter and Munzert, Simon},
Title = {Estimating Constituency Preferences from Sparse Survey Data Using
   Auxiliary Geographic Information},
Journal = {POLITICAL ANALYSIS},
Year = {2011},
Volume = {19},
