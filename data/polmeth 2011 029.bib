@article{ WOS:000288667600010,
Author = {Hunt, Diana},
Title = {The UfM and Development Prospects in the Mediterranean: Making a Real
   Difference?},
Journal = {MEDITERRANEAN POLITICS},
Year = {2011},
Volume = {16},
Number = {1},
Pages = {171-192},
Abstract = {Can the UfM make a distinctive contribution to development in the
   Mediterranean region? How innovative are its priority project themes? In
   an attempt to answer these questions, this contribution examines the
   relationship between the priority project themes of the Union for the
   Mediterranean and the substance of the Euro-Mediterranean Partnership
   (EMP) and the European Neighbourhood Policy (ENP) which preceded it. It
   explores the extent to which, by establishing the six priority project
   themes, the UfM has broken with the previous focus of the economic
   reform programmes and projects promoted by the EMP and the ENP. It then
   reviews the potential contribution of the themes to economic development
   and examines selected factors which may influence project implementation
   and goal attainment, including funding access and the impact of the
   UfM's institutional innovations on the effective pursuit of project
   themes. The conclusion is that there is scope for the UfM to enhance
   development in the Mediterranean region but outcomes so far have been
   limited and future outcomes depend on a range of variables including
   geo-political tensions, national priorities of partner states and
   funding availability.},
