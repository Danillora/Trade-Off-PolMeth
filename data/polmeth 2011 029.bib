@article{ WOS:000291664700006,
Author = {Montaquila, Jill M. and Hsu, Valerie and Brick, J. Michael},
Title = {USING A ``MATCH RATE{''} MODEL TO PREDICT AREAS WHERE USPS-BASED ADDRESS
   LISTS MAY BE USED IN PLACE OF TRADITIONAL LISTING},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2011},
Volume = {75},
Number = {2},
Pages = {317-335},
Month = {SUM},
Abstract = {For decades, traditional listing has been the standard method for
   constructing address-based sampling (ABS) frames. Address lists compiled
   by listers generally are believed to provide good coverage of
   residential units in the sampled area. Nonetheless, the time and
   resources associated with creating these address lists have led survey
   practitioners to consider the use of the U.S. Postal Service (USPS)
   Computerized Delivery Sequence File listings as an alternative. Previous
   studies have yielded an abundance of information about the utility of
   ABS frames. In our recent investigation (Montaquila et al. 2009), we
   found coverage of the USPS-based address lists to be generally good in
   urban areas but inadequate in some rural areas, and also poorer in
   high-population-growth areas. Resources, time, and effort could be
   conserved if we are able to use the USPS list as a sampling frame in
   areas where it provides adequate coverage. In this article, we develop a
   ``match rate{''} model that uses multiple regression to predict the
   match rate (the proportion of traditionally listed addresses that would
   have been obtained from a USPS list) based on characteristics of the
   geographic areas of interest. We discuss the feasibility of using USPS
   lists for household surveys, drawing on results from our previous work
   and the match rate modeling. We describe an approach for setting
   threshold values of predicted match rate to determine when to use USPS
   lists in lieu of traditional listing. Finally, we assess the model's
   external validity based on the application of the model to an
   independent dataset.},
