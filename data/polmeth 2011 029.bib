@article{ WOS:000296630500001,
Author = {Link, Michael W. and Lai, Jennie W.},
Title = {CELL-PHONE-ONLY HOUSEHOLDS AND PROBLEMS OF DIFFERENTIAL NONRESPONSE
   USING AN ADDRESS-BASED SAMPLING DESIGN},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2011},
Volume = {75},
Number = {4},
Pages = {613-635},
Month = {WIN},
Abstract = {Identifying and contacting cell-phone-only (CPO) households in an
   efficient and cost-effective manner is critical for many studies to
   ensure the representativeness of final survey estimates. With the advent
   of address-based sampling (ABS)-that is, the sampling of addresses from
   a database with near universal coverage of residential
   households-researchers have an alternative to cell-phone frame sampling
   for identifying and surveying these households. Using a large ABS
   mixed-mode study, we explore three key questions. First, how effective
   is an ABS design in identifying CPO households? Second, is there
   significant differential response across the ``matched'' (address and
