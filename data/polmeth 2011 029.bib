@article{ WOS:000298192700008,
Author = {Brick, J. Michael},
Title = {The Future of Survey Sampling},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2011},
Volume = {75},
Number = {5, SI},
Pages = {872-888},
Abstract = {The twentieth century saw a dramatic change in the way information was
   generated as probability sampling replaced full enumeration. We examine
   some key events of the past and issues being addressed today to gain
   perspective on the requirements that might steer changes in the
   direction of survey sampling in the future. The potential for online Web
   panels and other methods that do not use probability sampling are
   considered. Finally, we conclude with some thoughts on how the future of
   survey sampling might be shaped by unpredictable factors.},
