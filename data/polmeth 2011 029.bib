@article{ WOS:000288268800010,
Author = {Bowler, Shaun and Donovan, Todd},
Title = {Electoral Competition and the Voter},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2011},
Volume = {75},
Number = {1},
Pages = {151-164},
Month = {APR},
Abstract = {This article examines how electoral competition, in the form of
   district-level campaign expenditures, affects voters' opinions about
   elections. We direct our attention at how voters perceive competition,
   and at how electoral competition affects how people perceive elections.
   Although people generally overestimate the competitiveness of U.S. House
   races, we demonstrate that perceptions of competition are connected to
   actual levels of campaign activity. We also find that electoral
   competition may have contradictory democratic effects. District-level
   spending is associated with greater attention to news about the local
   campaign, but also with greater dissatisfaction with election choices.},
