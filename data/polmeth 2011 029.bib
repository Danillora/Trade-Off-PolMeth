@article{ WOS:000292717700002,
Author = {Sheppard, Randal},
Title = {Nationalism, economic crisis and `realistic revolution' in 1980s Mexico},
Journal = {NATIONS AND NATIONALISM},
Year = {2011},
Volume = {17},
Number = {3},
Pages = {500-519},
Month = {JUL},
Abstract = {This article examines how the Mexican state drew upon nationalist
   discourse for legitimacy following the 1982 debt crisis. The analytical
   framework situates Mexico within the context of Latin American
   nationalism and explores the structural and conjunctural factors that
   contributed to the endurance and effectiveness of Mexican revolutionary
   nationalism as a hegemonic nationalist discourse. Historical
   commemorations during the Miguel de la Madrid administration (1982-88)
   are then examined to show how the state evoked nationalist motifs as it
   dealt with economic crisis, pressure from the USA, domestic political
   opposition and the implementation of neoliberal reforms. The relative
   effectiveness of sometimes counterintuitive appeals to nationalist
   legitimacy is found to be neither wholly `rational' nor `irrational', in
   this case having its basis in a history of elite and popular negotiation
   through the revolutionary nationalist framework, the continuity of the
   post-revolutionary Partido de la Revolucion Institutional (PRI) state
   model and the lack of a viable competing paradigm.},
