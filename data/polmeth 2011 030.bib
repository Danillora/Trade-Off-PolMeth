@article{ WOS:000217777700007,
Author = {Motyl, Alexander J.},
Title = {The paradoxes of Paul Robert Magocsi: the case for Rusyns and the
   logical necessity of Ukrainians},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2011},
Volume = {39},
Number = {1},
Pages = {105-109},
Abstract = {How is it possible for a Rusyn nation-builder to have contributed to the
   historiography of Ukraine to such a significant degree that one might
   suspect that Paul Robert Magocsi is really a Ukrainian nation-builder?
   Like all social-science puzzles, this one dissolves upon closer
   inspection. Magocsi resembles a Ukrainian nation-builder - or perhaps
   even is a Ukrainian nation-builder malgre soi - precisely because he is
   a Rusyn nation-builder. That is so because all nation-builders are
   always builders of at least two nations, their own and the others.},
