@article{ WOS:000217786900006,
Author = {Stebelsky, Ihor},
Title = {Putting Ukraine on the map: the contribution of Stepan Rudnyts'kyi to
   Ukrainian nation-building},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2011},
Volume = {39},
Number = {4},
Pages = {587-613},
Abstract = {This paper examines the contribution of the founder of modern Ukrainian
   geography, Stepan Rudnyts'kyi, to Ukrainian nation-building. It
   demonstrates how Rudnyts'kyi put Ukraine on the mental map of the
   Ukrainian public before the declaration of Ukraine's independence in
   1918. This is done by analyzing his key publications and showing how he
   formed a vision of Ukraine and delineated its territory to influence the
   perceptions of the Ukrainian public on the eve of the struggle for
   Ukraine's independence. Rudnyts'kyi's contribution is also viewed within
   the context of competition from rival modern nation-building projects in
   Eastern Europe, most notably Polish and Russian. The developments are
   also examined within Miroslav Hroch's periodization of national
   movements. Rudnyts'kyi played an important role in stage B (patriotic
   agitation) in Ukrainian national revival.},
