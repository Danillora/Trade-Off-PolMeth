@article{ WOS:000215510700005,
Author = {Hernandez i Marti, Gil-Manuel and Albert Rodrigo, Maria},
Title = {The general dynamics of cultural policy in the Valencian Country:
   positions, discourses and practices of Valencian cultural actors},
Journal = {REVISTA DE INVESTIGACIONES POLITICAS Y SOCIOLOGICAS},
Year = {2012},
Volume = {11},
Number = {3},
Pages = {89-114},
Abstract = {The present article, arises from the description of the configurating
   process of automic Valencian cultural system. This system is based on
   two basic components: the structure of the map and ruled and funding
   relationships that are established between the main actors.
   Subsequently, and in order to draw the relationships of communication
   and the influence between cultural actors established in Valencia, our
   work focuses on the analysis that have come through, after in-depth
   interviews of positions, speeches and practices that support and
   implement cultural agents, focusing especially on those collaborative
   and conflictive relationships. Thus, we can make a general
   characterization of the dynamics of Valencian cultural policy.},
