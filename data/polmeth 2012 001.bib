@article{ WOS:000306389000003,
Author = {Fluegel-Martinsen, Oliver},
Title = {Critique of Capitalism. Lessons from History of Political Thought},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2012},
Volume = {41},
Number = {2},
Pages = {161-176},
Abstract = {One can distinguish two ways of a critique of capitalism: a structural
   critique on the one hand, and a critique by means of virtue ethics on
   the other. Going back to Aristotle, the long tradition of central themes
   of ethical critique of capitalism becomes evident. The other mode of
   capitalism critique goes beyond certain individual behaviour and focuses
   on certain structural characteristics of capitalism. The article argues
   that this second, structural mode of critique is superior to the ethical
   one, because it doesn't take into account only misled individual
   behaviour, but also structural undesirable developments. In explicating
   this, I refer to Ferguson and Hegel who have given important impulses to
   it in the history of ideas.},
