@article{ WOS:000421298500002,
Author = {Mantovani, Rafael},
Title = {Fashion against tyranny: lucubration of 19th century Argentinian
   costumbrists},
Journal = {TEMAS Y DEBATES},
Year = {2012},
Number = {23},
Pages = {39-54},
Month = {JAN-JUN},
Abstract = {Fashion is an issue which is totally unimportant nowadays in the
   analysis of politics. But it does not mean it has always been like this.
   There have already been studies on subjects which nowadays are ignored
   or, even, considered biased. For instance, phrenology, the study of how
   climate and geography influence man's personality and expose it through
   his physical traits, is considered absolutely racist. But vestments,
   even if they had not been assorted by a negative value judgment, are now
   insignificant. For costumbristas, scholars who study customs and
   externality of human being, the manner of speaking, the gait, the way
   one dresses, fashion is a vital subject for society development. To
   demonstrate how this thought was in Argentina in the 19th century is
   what this essay aims.},
