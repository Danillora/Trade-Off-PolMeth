@article{ WOS:000304651000006,
Author = {Harrison, Brian F. and Michelson, Melissa R.},
Title = {Not That There's Anything Wrong with That: The Effect of Personalized
   Appeals on Marriage Equality Campaigns},
Journal = {POLITICAL BEHAVIOR},
Year = {2012},
Volume = {34},
Number = {2},
Pages = {325-344},
Month = {JUN},
Abstract = {An increasingly predominant strategy used by organizations seeking to
   increase support for gay marriage is to personalize the issue by
   focusing on individuals in the LGBT community. However, competing
   theoretical traditions (e.g., Allport's contact theory, group threat,
   implicit bias) raise questions about whether this strategy has the
   desired effect. This paper presents results from an original field
   experiment conducted in coordination with a marriage equality
   organization. Callers who self-identified as a member of the LGBT
   community were less effective in soliciting donations compared to
   callers who did not self-identify, suggesting that personalization has a
   negative effect on persuasion efforts. The findings cut against the
   grain of the Allport (The nature of prejudice, 1954) hypothesis and have
   important implications for social advocacy organizations in terms of
   rhetorical and message strategy.},
