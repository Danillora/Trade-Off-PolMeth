@article{ WOS:000310680800014,
Author = {Arceneaux, Kevin and Kousser, Thad and Mullin, Megan},
Title = {Get Out the Vote-by-Mail? A Randomized Field Experiment Testing the
   Effect of Mobilization in Traditional and Vote-by-Mail Precincts},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2012},
Volume = {65},
Number = {4},
Pages = {882-894},
Month = {DEC},
Abstract = {This study extends previous field experimental research on turnout by
   considering how institutional context moderates the effect of
   mobilization. Taking advantage of a setting in which some registrants
