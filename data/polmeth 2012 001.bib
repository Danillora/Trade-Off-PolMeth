@article{ WOS:000309727400030,
Author = {Smith, Keith},
Title = {Why Just Two Parties? A Voting Game to Illustrate Duverger's Law},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2012},
Volume = {45},
Number = {4},
Pages = {759-764},
Month = {OCT},
Abstract = {With the 2012 presidential elections approaching, student interest in
   (and frustration with) the US two-party system is likely to grow.
   Although many explanations exist for this phenomenon, their number and
   complexity often overwhelm students. This article describes an iterative
   game, designed for a single class session, that illustrates one of the
   major institutional explanations for the US two-party system-Duverger's
   Law. The game can also spur discussion of some of the major criticisms
   of the Law.},
