@article{ WOS:000309294000022,
Author = {Plischke, Thomas and Bergmann, Michael},
Title = {Decision-making Processes of Late-deciders at the 2009 German Federal
   Election},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2012},
Number = {45},
Pages = {489+},
Abstract = {Thomas Plischke / Michael Bergmann, Decision-making Processes of
   Late-deciders at the 2009 German Federal Election, pp. 489-513.
   The share of late deciding voters is rising steeply in Germany. With
   regard to the 2009 German federal election this article explores how
   late-deciders reach their final voting decisions. Using longitudinal
   data we can show that the bulk of late-deciders, especially those being
   politically involved, already had decided on a pre-selection of eligible
   parties at the very beginning of the election campaign, and, therefore,
   no longer was susceptible to other parties' persuasive communications.
   The commonality of those parties considered eligible for late-decider;
   was the similarity of their policy positions. Ideological or policy
   considerations thus played a major role in preselecting political
   parties, whereas decision-making during the election campaign with
   regard to the ultimately chosen party was guided by evaluations such as
   those towards candidates which were easier to compare.},
