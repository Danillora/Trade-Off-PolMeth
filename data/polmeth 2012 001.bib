@article{ WOS:000308301700006,
Author = {Dincsahin, Sakir},
Title = {A Symptomatic Analysis of the Justice and Development Party's Populism
   in Turkey, 2007-2010},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2012},
Volume = {47},
Number = {4},
Pages = {618-640},
Month = {OCT},
Abstract = {This article focuses on the populist strategy of the Turkish Justice and
   Development Party between the 2007 presidential election, when Turkish
   politics experienced an impasse, and the 2010 referendum over the
   constitutional amendments. As a means of analysing populism, the
   symptomatic approach is preferred over other theoretical perspectives,
   including empiricism and historicism. An analysis of the discourse
