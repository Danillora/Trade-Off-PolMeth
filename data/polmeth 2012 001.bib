@article{ WOS:000306092700011,
Author = {Vincenti, Luc},
Title = {The relation to the revolution: the ember and the ashes},
Journal = {PENSEE},
Year = {2012},
Number = {370},
Pages = {119+},
Month = {APR-JUN},
Abstract = {Luc Vincenti, The relation to the revolution: the ember and the ashes
   Rousseau depicts the necessary degeneration of human societies. Is it
   pessimistic, or does his reading of history, punctuated by contingent
   breaks, allow us to hope for a better society? We must understand the
   revolutionary break in relation to the necessity of historical
   degeneration, in which it inscribes itself in order to bring it to an
   end. Rousseau maintains the possibility of a revival of the State, but
   it must be understood from the perspective of dialectical thought, first
   developed in relation to literature. The application of this dialectic
   to politics allows us to understand how the political leader has to,
   unlike the despot, join the freedom with the strength of laws.},
