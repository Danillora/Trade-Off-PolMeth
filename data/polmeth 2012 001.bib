@article{ WOS:000421238800015,
Author = {Garcia Alvarez, Manuel and Garcia Lopez, Ruben},
Title = {Some remarks on a constitutional reform concerning the Spanish
   ombudsman. Another step towards social justice},
Journal = {TEORIA Y REALIDAD CONSTITUCIONAL},
Year = {2012},
Volume = {29},
Pages = {373-387},
Abstract = {The constitutional regulation of the Spanish ombudsman, as well as the
   functions performed by the latter has generally been appraised in a
   positive way. This not withstanding should a Constitutional reform be
   undertaken, having in mind the strengthening of the Welfare (social)
   state, it may be advisable to reinforce the ombudsman as a warrant of
   social, economic rights. With this end in view, we suggest to undertake
   some changes in article 54 of our Constitution. These changes should
   concern the functions which it performs, its proceedings and its
   election. And last but not least, the necessary cooperation by
   authorities and civil servants, as well as the aftermath in case they
   break their duty to provide the ombudsman with the information required
   by it.},
