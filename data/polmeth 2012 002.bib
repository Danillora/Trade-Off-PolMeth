@article{ WOS:000304827300003,
Author = {Kedar, Orit},
Title = {Voter Choice and Parliamentary Politics: An Emerging Research Agenda},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2012},
Volume = {42},
Number = {3},
Pages = {537-553},
Month = {JUL},
Abstract = {This article offers organizing principles to an emerging research agenda
   that analyses how parliamentary politics affects voter considerations.
   It uses the process by which votes are turned into policy as a unifying
   framework: every step in the process poses incentives for voters and
   encourages different types of strategic behaviour by voters. The
   standard version of strategic voting commonly found in analyses of voter
   choice is about the step familiar from the Anglo-American model - the
   allocation of seats based on votes - yet insights about voter behaviour
   originated from that model have been inadvertently reified and assumed
   to apply universally. The article identifies a set of empirical
   implications about the likelihood of voters employing policy-oriented
   strategies under different circumstances.},
