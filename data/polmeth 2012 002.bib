@article{ WOS:000215838800003,
Author = {Wagemann, Claudius},
Title = {What's new in the Comparative Method?: QCA and fuzzy sets analysis},
Journal = {REVISTA MEXICANA DE ANALISIS POLITICO Y ADMINISTRACION PUBLICA},
Year = {2012},
Volume = {1},
Number = {1},
Pages = {51-75},
Month = {JAN-JUN},
Abstract = {Over the last 25 years, Qualitative Comparative Analysis (QCA) has found
   much attention within social science methodology. It is based on set
   relations and aims at the discovery of sufficient and necessary
   conditions and their INUS and SUIN derivates. As such, it is possible
   with QCA to model causal complexity. This article first presents QCA as
   a technique. It then proposes three aspect of causal complexity which
   can be managed with a QCA design: equifinality, conjunctural causation
   and asymmetry. It moves on to discuss what is new about QCA and defines
   as the most prominent novelty QCA's ability to deal with systematic
   comparison; to overcome the need of dichotomization through the use of
   fuzzy sets; and the configurational view on cases. Finally, a list of
   why QCA can be really called a ``qualitative{''} method (despite its
   formalization) and an outlook on the most important challenges for the
   future are introduced.},
