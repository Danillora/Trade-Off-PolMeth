@article{ WOS:000309141400003,
Author = {Ferrell, Jason},
Title = {Isaiah Berlin as Essayist},
Journal = {POLITICAL THEORY},
Year = {2012},
Volume = {40},
Number = {5},
Pages = {602-628},
Month = {OCT},
Abstract = {One of the largest contemporary debates in political theory revolves
   around the question of how pluralists can justify their political
   commitments. Isaiah Berlin, one of the first to face this problem, was a
   self-proclaimed liberal, whose political writings have led to
   controversy. In this essay, I take up the issue of how Berlin's use of
   the essay genre contributes to his defense of liberalism given his
   pluralist beliefs. I argue that while his reliance upon the essay
   generates particular interpretive problems, it need not undermine his
   commitment to liberalism. Rather, Berlin's defense of liberalism
   involves a rhetorical attempt to discredit alternatives to liberalism
   while drawing attention to suggestive ties between history, political
   judgment, and liberalism.},
