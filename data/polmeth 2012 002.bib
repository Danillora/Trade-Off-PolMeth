@article{ WOS:000218110500004,
Author = {Mura, Andrea},
Title = {A genealogical inquiry into early Islamism: the discourse of Hasan
   al-Banna},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2012},
Volume = {17},
Number = {1},
Pages = {61-85},
Abstract = {This article inquires into the ideological vision of Hasan al-Banna
   (1906-1949), one of the most influential figures of Islamist thought. By
   assuming a discourse theory perspective, I argue that al-Banna's
   Islamist discourse was genealogically caught between a traditional
   pan-Islamic vocation and modern ways of articulating political
   discourse, such as nationalism and Arab nationalism. Following the
   traumatic encounter between tradition and modernity that colonialism
   enacted, al-Banna increasingly integrated and valourized modern national
   `signifiers', downplaying early universalistic ethos. This denoted a
   growing reliance on the language of modernity over the language of
   tradition, though such reliance was instrumental to al-Banna's
   anti-imperialist political project, entailing the very preservation of
   tradition as a moderator principle in the appropriation of modernity.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
