@article{ WOS:000210293300005,
Author = {Lopez de Lizaga, Jose Luis},
Title = {Reason or Revolution? The Republicanism of Kant and Georg Forster},
Journal = {FORO INTERNO-ANUARIO DE TEORIA POLITICA},
Year = {2012},
Volume = {12},
Pages = {81-106},
Abstract = {This article examines the life and political thought of German explorer,
   scientist and revolutionary, Georg Forster (1754-1794), one of the few
   members of the German Enlightenment to unambiguously support the cause
   of the French Revolution. Comparing Forster's republicanism to Immanuel
   Kant's (1724-1804), one finds that the political differences between the
   two authors are based on differing conceptions of the power that reason
   exerts over human will, and of its function in politics. Kant's
   republican and reformist autocracy is opposed to Forster's democratic
   republicanism, and to his unconditional defense of revolution, even in
   the case of a totalitarian revolutionary State.},
