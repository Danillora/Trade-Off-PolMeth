@article{ WOS:000300742700005,
Author = {Termeer, Catrien and Biesbroek, Robbert and Van den Brink, Margo},
Title = {Institutions for adaptation to climate change: comparing national
   adaptation strategies in europe},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2012},
Volume = {11},
Number = {1},
Pages = {41-53},
Month = {MAR},
Abstract = {In addition to reducing greenhouse gas emissions, societies worldwide
   have to cope with the potential impacts of climate change. The central
   question of this paper is to what extent our historically grown
   institutions enable actors to cope with the new challenges of climate
   adaptation. We present six qualities of governance institutions that are
   crucial to allow for, and encourage adaptation, and apply them to the
   National Adaptation Strategies of the Netherlands, the United Kingdom,
   Finland and Sweden. We conclude that although the governance
   institutions involved seem to have the basic qualities required, they
   face five institutional weaknesses, causing tensions on the long term:
   (1) lack of openness towards learning and variety; (2) strong one-sided
   reliance on scientific experts; (3) tension between top-down policy
   development and bottom-up implementation; (4) distrust in the
   problem-solving capacity of civil society; and (5) wickedness of
   reserving funding for long-term action.},
