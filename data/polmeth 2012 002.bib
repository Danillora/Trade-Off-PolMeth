@article{ WOS:000300864600004,
Author = {Grant, Wyn},
Title = {Forty years studying British politics: The decline of Anglo-America},
Journal = {BRITISH POLITICS},
Year = {2012},
Volume = {7},
Number = {1, SI},
Pages = {30-42},
Month = {APR},
Abstract = {The still present belief some 40 years ago that British politics was
   both exceptional and superior has been replaced by more theoretically
   sophisticated analyses based on a wider and more rigorously deployed
   range of research techniques, although historical analysis appropriately
   remains important. The American influence on the study of British
   politics has declined, but the European Union dimension has not been
   fully integrated. The study of interest groups has been in some respects
   a fading paradigm, but important questions related to democratic health
   have still to be addressed. Public administration has been supplanted by
   public policy, but economic policy remains under-studied. A key
   challenge for the future is the study of the management of expectations.
   British Politics (2012) 7, 30-42. doi:10.1057/bp.2011.32; published
