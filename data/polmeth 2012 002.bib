@article{ WOS:000310680800005,
Author = {Hinojosa, Magda and Franceschet, Susan},
Title = {Separate but Not Equal: The Effects of Municipal Electoral Reform on
   Female Representation in Chile},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2012},
Volume = {65},
Number = {4},
Pages = {758-770},
Month = {DEC},
Abstract = {Chile altered its electoral rules for municipal elections in 2002. The
   new rules separated mayoral and council member elections, and while the
   latter continue to use proportional representation, mayors are now
   chosen based on plurality. These changes serve as a natural test of the
   effects of electoral rules on women's political representation. The
   Chilean case demonstrates (1) that electoral reform is gendered, (2)
   that women's representation is shaped by both formal and informal rules,
   and (3) that electoral rules have different effects on mayoral and
   council member positions, demonstrating that the ``glass ceiling{''} for
   executive positions is harder to crack.},
Publisher = {SAGE PUBLICATIONS INC},
