@article{ WOS:000311254900004,
Author = {Tichit, Christine},
Title = {The Onset of Social Class Tastes among Children of Migrants: Competing
   Patterns of Tastes and Eating Habits},
Journal = {POLITIX},
Year = {2012},
Volume = {25},
Number = {99},
Pages = {51+},
Abstract = {The Onset of Social Class Tastes among Children of Migrants: Competing
   Patterns of Tastes and Eating Habits
   Christine TICHIT
   The aim of this paper is to explore the social dimension of food
   acculturation in situation of migration, emphasizing the perspective
   from children rather than from adults as usually collected. Based on a
   research in schools, the field device combines observation times and
   declarative collection by questionnaire and by focus group, sparing both
   individual responses and also interactivity between pupils. The
   presentation of some issues discussed in class shows that biological and
   social ages are combining differently depending on social class origin
   of children. The results about children's food preferences, are also
   translating the social class tastes of migrants families, which are
   clearly underpinned by a logic of distinction more social than cultural.},
