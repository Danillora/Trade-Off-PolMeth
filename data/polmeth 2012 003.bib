ISSN = {0092-5853},
EISSN = {1540-5907},
Keywords-Plus = {GENDER-DIFFERENCES; PERSONALITY; CHOICE; EXPLANATION; PROPENSITY;
   FRAMEWORK; SCALE; DEVIL},
