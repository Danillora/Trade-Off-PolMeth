@article{ WOS:000210638600005,
Author = {Watkins, Evan},
Title = {History and Epistemology: Just Don't Order the Pickles},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2012},
Volume = {24},
Number = {3},
Pages = {360-368},
Month = {JUL},
Abstract = {I argue that David Ruccio and Jack Amariglio's Postmodern Moments in
   Modern Economics has a particular pedagogical importance for those of us
   who teach in the humanities, for three different reasons. Ruccio and
   Amariglio understand our disciplines a lot better than most of us
   understand the discipline of economics, and can act as very effective
   translators. Second, their emphasis on everyday economic discourse
   intersects very usefully with cultural studies work on the everyday.
   Finally, and most important, they offer a crucial lesson in the
   contribution of Marxist analysis. In contrast to the tendency in the
   humanities to consider the recognition of historically specific and
   socially constructed discourse as a point of arrival in critical
   discussion, they find it a point of departure. This is where you begin
   the work of analysis, not where you end.},
