@article{ WOS:000442719400007,
Author = {Peregudov, S. P.},
Title = {Monitoring Democracy: Building New Relations Between State and Society},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2012},
Number = {6},
Pages = {55+},
Abstract = {Among new approaches to rethinking the nature of relations between
   society and state institutions the concept of monitoring democracy is of
   prior importance: it does not limit itself to the political space, but
   penetrates into different spheres of social life. The author analyzes
   the monitoring democracy concept and pays special attention to its
   practical outcomes. An important part of the article is dedicated to the
   analysis of contradictory, but mostly positive trends that permit to
   assert the implementation of some traits of monitoring democracy in
   contemporary social and political life in Russia. A full-fledged
   implementation of this model could lead to qualitative changes in the
   overall relations between state and society.},
