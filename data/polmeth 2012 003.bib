@article{ WOS:000421676100002,
Author = {Velandia Montes, Rafael},
Title = {Sexual delinquency and penal populism in Colombia},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
Year = {2012},
Number = {11},
Pages = {19-32},
Month = {MAR},
Abstract = {Nowadays there is an overuse of criminal law as a state mechanism to
   confront social conflicts of diverse nature. Changes on legislation are
   being made without taking into account its necessity and efficacy and
   such reforms mean to satisfy particular interests and not to resolve
   social conflicts. Politicians are main actors in this growing process,
   although not the only ones, who in seek of political prestige in order
   to obtain electoral benefits continuously propose legal modifications
   that are broadening the social area subject to criminal law. This
   overuse of criminal law has negative consequences such as more work
   overload to a currently overburdened system and an increase in the bad
   reputation of at the present discredited branch of justice. This
   legislative tendency, named as penal populism, will be studied in regard
   to its existence in Colombia and sexual delinquency.},
