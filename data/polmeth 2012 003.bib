@article{ WOS:000214679900009,
Author = {Kauda, Toshihide},
Title = {A critique of Marx's two basic laws: The general law of capitalist
   accumulation and the law of the tendency of the rate of profit to fall},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2012},
Volume = {2},
Number = {3},
Pages = {387-403},
Abstract = {Marx's two basic laws are founded on the organic composition of capital
   becoming higher due to the capitalistic accumulation necessarily
   accompanying the rise of labor productivity. I think this concept is
   consistent through the whole of Capital, but do not think Marx's
   analysis is sufficient. Above all, he does not bring into the laws the
   diminution of the values of the means of production caused by the same
   movement. We try to take it into account in the laws. First, we present
   original formulas for the two laws. Second, we analyze the main factors
   which affect the total social organic composition theoretically and
   actually. This is an inquiry into the social reproduction process using
   schematic analysis employing my technique. We conclude Marx's two laws
   are not supported.},
