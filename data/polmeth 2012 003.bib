@article{ WOS:000309294000008,
Author = {Wuest, Andreas M.},
Title = {Permanent or Temporary? The Impact of Immigration Background on Turnout
   and Party Choice at the 2009 German Federal Election},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2012},
Number = {45},
Pages = {157+},
Abstract = {Andreas M. Wust, Permanent or Temporary? The Impact of Immigration
   Background on Turnout and Party Choice at the 2009 German Federal
   Election, pp. 157-178.
   Taking the example of Germany's 2009 general election the paper explores
   the impact of immigration background on electoral participation and
   party choice. The influence of the respective migration system,
   experiences with the political regime in the country of origin, contexts
   of reception and degree of integration as well as changes from the first
   to the second generation are systematically tested. While the electoral
   participation gap between immigrants and the native population proves to
   be mainly temporary, party choice is rather permanently shaped by the
   reception of an immigrant group in the country and by the potential of
   being discriminated against.},
Publisher = {SPRINGER},
Address = {ONE NEW YORK PLAZA, SUITE 4600, NEW YORK, NY, UNITED STATES},
Type = {Article},
Language = {German},
Affiliation = {Wüst, AM (Corresponding Author), Univ Mannheim, MZES, A 5,6, D-68131 Mannheim, Germany.
