@article{ WOS:000306092700009,
Author = {Lopez Yanez, Aina D.},
Title = {Philosophy of the history and theory of society},
Journal = {PENSEE},
Year = {2012},
Number = {370},
Pages = {99+},
Month = {APR-JUN},
Abstract = {Aina D. Lopez Yanez, Philosophy of the history and theory of society
   The Rousseau's Philosophy of History, a subject runs parallel to his
   social theory. This link is founded on the conviction that history is
   determined by the influence of anthropological limits that impeded Man's
   integral socialization. Rousseau identified several differences between
   social formations which can't be described only as historical period
   distinctions but also as differences of consequences over the moral
   conditions of mankind. Following this theory, Rousseau made a conception
   of human sociability, a particular theory of community life and moreover
   a representation of life in a rather complex society. The merit of his
   general theory about social phenomenon lies likewise in the ease with
   which it can be applied for relating concepts to those introduced by the
   great classical sociologists.},
