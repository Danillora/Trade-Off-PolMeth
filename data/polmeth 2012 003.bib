@article{ WOS:000212884600004,
Author = {Walker, Kathryn},
Title = {A troubled reconciliation: a critical assessment of Tan's Liberal
   Cosmopolitanism},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2012},
Volume = {15},
Number = {1},
Pages = {63-77},
Abstract = {Kok-Chor Tan argues for a conception of Liberal Cosmopolitanism that
   seeks to reconcile ideals of global justice and national partiality. I
   provide two objections to his luck egalitarian model of global justice:
   first, it fails to provide adequate space for legitimate cultural
   variation with respect to the understanding of and valuing of natural
   resources; and second, that its account of ideas of collective
   responsibility is restricted to a point at which it becomes
   unrecognizable and inefficacious. I conclude with some reflections on
   the implications of my analysis for the more general question of whether
   or how national partiality and global justice can be reconciled.},
