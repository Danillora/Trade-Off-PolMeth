@article{ WOS:000210010100009,
Author = {Lopez Arevalo, Jorge Alberto and Pelaez Herreros, Oscar},
Title = {Analysis of economic convergence within Chiapas municipalities, regions
   and apparent inconsistencies},
Journal = {AMERICA LATINA HOY-REVISTA DE CIENCIAS SOCIALES},
Year = {2012},
Volume = {60},
Pages = {183-206},
Month = {APR},
Abstract = {The data of annual per capita income estimated by the United Nations
   Development Programme (UNDP) for the municipal human development index
   in Mexico shows that,in Chiapas, during the period 2000-2005, there was
   a strong convergence at the municipal level while there was no regional
   convergence. To explain this apparent inconsistency, the article
   ``Regression towards mediocrity in hereditary stature{''}, published by
   Galton in 1886, is examined, concluding that the clustering level of the
   data (municipal or regional) determines the convergence analysis
   results, because the grouping of municipalities in regions causes loss
   of information that can generate phenomena of (R) regression towards the
   mean{''} or just the opposite, as is the case. In Chiapas, the municipal
   convergence would have led to convergence within the regions but not
   between regions.},
