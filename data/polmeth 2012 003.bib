@article{ WOS:000317531800006,
Author = {Gerschewski, Johannes and Merkel, Wolfgang and Schmotz, Alexander and
   Stefes, Christoph H. and Tanneberg, Dag},
Title = {Why Do Dictatorships Survive?},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2012},
Number = {47},
Pages = {106+},
Abstract = {Johannes Gerschewski, Wolfgang Merkel, Alexander Schmotz, Christoph H.
   Stefes and Dag Tanneberg, Why Do Dictatorships Survive?, pp. 106-131.
   Why do dictatorships survive? To answer this question, we propose a
   theoretical model that identifies repression, legitimation, and
   cooptation as the central pillars of autocratic regimes. We thereby
   combine the findings of older and newer studies on autocratic rule. The
   better autocratic regimes are able to institutionalize these three
   functions, the more stable they are. In the short run, repression,
   legitimation, and cooptation are thereby complementary and can support
   each other. In the long run, however, a weakening of one pillar is
   likely to destabilize the other pillars as well. This process culminates
   in a critical juncture during which the defining feature of a regime -
   the rules that structure the access to power are called into question.},
