@article{ WOS:000212010100007,
Author = {Smith-Cannoy, Heather},
Title = {Defending Democracy? Assessing the OAS's 2002 Diplomatic Intervention in
   Haiti},
Journal = {CIVIL WARS},
Year = {2012},
Volume = {14},
Number = {3, SI},
Pages = {431-450},
Abstract = {Inter-governmental organizations (IGOs) have vastly different capacities
   for intervening in conflicts in their member states. The Organization of
   American States (OAS), a regional IGO in Latin America, has the capacity
   to intervene diplomatically to defend democracy in the hemisphere. This
   article examines how well the OAS applied these diplomatic tools in
   defense of democracy in Haiti following the 2000 political crisis. I
   argue that the OAS achieved some success in Haiti, particularly the
   Electoral Technical Program. However, the bulk of the evidence suggests
   that the OAS missed key opportunities to invoke their strongest
   diplomatic tools in defense of democracy in Haiti.},
