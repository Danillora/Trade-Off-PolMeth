@article{ WOS:000307111900007,
Author = {Peksen, Dursun},
Title = {Does Foreign Military Intervention Help Human Rights?},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2012},
Volume = {65},
Number = {3},
Pages = {558-571},
Month = {SEP},
Abstract = {This article examines the effect of foreign armed intervention on human
   rights conditions in target countries. It is argued that military
   intervention contributes to the rise of state repression by enhancing
   the state's coercive power and encouraging more repressive behavior,
   especially when it is supportive or neutral toward the target
   government. Results from bivariate probit models estimated on
   time-series cross-section data show that supportive and neutral
   interventions increase the likelihood of extrajudicial killing,
   disappearance, political imprisonment, and torture. Hostile
   interventions increase only the probability of political imprisonment.
   The involvement of an intergovernmental organization or a liberal
   democracy as an intervener is unlikely to make any major difference in
   the suggested negative impact of intervention.},
Publisher = {SAGE PUBLICATIONS INC},
