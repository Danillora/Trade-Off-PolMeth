@article{ WOS:000307111900001,
Author = {Tepe, Sultan},
Title = {Moderation of Religious Parties: Electoral Constraints, Ideological
   Commitments, and the Democratic Capacities of Religious Parties in
   Israel and Turkey},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2012},
Volume = {65},
Number = {3},
Pages = {467-485},
Month = {SEP},
Abstract = {Whether religious parties' inclusion in electoral competition moderates
   or polarizes their positions remains an enigma as deductive accounts
   yield contradictory results. This analysis questions the institution-
   and ideology-centered approaches to party change and shows that
   dichotomizing religious parties as moderate or extreme and moderation as
   a monolithic process obscures religious parties' role in democracy. When
   scholars view moderation as consisting of behavioral and ideological
   dimensions and examine it through an inductive analysis of Israel and
   Turkey's religious parties, several modes of moderation emerge with
   different democratic outcomes. While some bolster procedural democracy,
   others thwart the expansion of liberal democracy.},
