@article{ WOS:000212641600003,
Author = {Horrigmo, Aase Marthe Johansen},
Title = {Why Study the Spread of Culture-led Development Strategies?},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2012},
Volume = {22},
Number = {5},
Pages = {553-575},
Abstract = {Culture, entertainment and amenities as urban and regional development
   policies have received much attention over the last decade. In the wake
   of recent work on the creative class, culture-led development policies
   have been applied in cities and regions across the Western world. This
   article uses quantitative data to examine whether these policies have
   gained the support of Scandinavian local and regional politicians and,
   if so, why and how. Drawing on policy diffusion and policy transfer
   theory, the article examines from where politicians are influenced. Can
   the sources of influence be characterized as (1) horizontal, (2)
   top-down or (3) bottom-up? The main finding is that the top-down
   perspective identifies significant national pressure on local and
   regional policy-makers, leading to the conclusion that regional and
   local policy-making in this policy area depends on national policies.},
