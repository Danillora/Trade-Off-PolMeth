@article{ WOS:000301305300007,
Author = {Tverdova, Yuliya V.},
Title = {The Formation of Economic Perceptions in Post-Communist Countries of
   East Central Europe},
Journal = {POLITICAL BEHAVIOR},
Year = {2012},
Volume = {34},
Number = {1},
Pages = {137-158},
Month = {MAR},
Abstract = {In the early transition phase in post-communist Europe, citizens'
   perceptions of the national economy appeared more favorable than
   objective economic indicators would suggest. With triple and quadruple
   digit annual inflation rates and a severe economic contraction, there
   was a substantial portion of the population in these countries who still
   thought that the national economy had been and would be improving. Thus,
   sociotropic economic perceptions in the wake of the democratic
   transformation appeared to be disconnected from the real economic
   situation. The purpose of this paper is threefold. First, it explores
   the link between objective economic indicators and public evaluations of
   the economy. Second, it investigates the microfoundations of economic
   perceptions. And finally, it tests a well-established proposition that
   political sophisticates are more accurate in their perceptions than
   their less informed counterparts. The findings of the study can be
   generalized to any political and economic system undergoing transition.},
