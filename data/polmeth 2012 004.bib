@article{ WOS:000302884100002,
Author = {Marshall, Stephen},
Title = {Taking Liberty behind God's Back: Mastery as the Central Problem of
   Slavery},
Journal = {POLITY},
Year = {2012},
Volume = {44},
Number = {2},
Pages = {155-181},
Month = {APR},
Abstract = {Slavery is universally condemned by scholars and politicians alike.
   However, by foregrounding the depredations suffered by slaves, scholarly
   and public condemnations of slavery elide the moral and political
   problems of mastery This article revisits the problem of mastery in
   light of the writings of the novelist Edward P Jones, the philosopher
   Friedrich Nietzsche, and the theologian St. Augustine. The article
   argues that mastery is a primary, and not a tangential, problem of
   slavery The article further contends that theories about a slave's
   repudiation of mastery provide crucial conceptual and normative
   resources for diagnosing and combating the temptation of mastery within
   post-slavery liberal democracies. Polity (2012) 44, 155-181.
   doi:10.1057/pol.2012.3; published online 12 March 2012},
