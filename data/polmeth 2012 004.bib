@article{ WOS:000210664400006,
Author = {Whitford, Andrew},
Title = {Signing Statements as Bargaining Outcomes: Evidence from the
   Administration of George W. Bush},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2012},
Volume = {42},
Number = {2},
Pages = {343-362},
Month = {JUN},
Abstract = {Recent studies have documented how presidents issue signing statements.
   A president might try to bend policy closer to his own position through
