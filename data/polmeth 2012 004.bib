@article{ WOS:000317531800014,
Author = {Wischermann, Joerg},
Title = {Civil Societies as Supporters of Authoritarian Regimes. The Case of
   Vietnam},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2012},
Number = {47},
Pages = {324+},
Abstract = {Jorg Wischermann, Civil Societies as Supporters of Authoritarian
   Regimes. The Case of Vietnam, pp. 324-353. Civil society and civil
   society organizations (CSOs) are not per se drivers of democratization
   processes in authoritarian regimes. Empirical findings laid out in this
   article indicate that in all types of Vietnamese CSOs authoritarian
   dispositions and practices are prevalent at least as far as internal
   decision-making processes are concerned. From the empirical findings and
   in a heuristical way it might be concluded that Vietnamese Civic
   Organizations support authoritarian rule though the extent of such
   support varies region-wise and in terms of types of organizations.},
