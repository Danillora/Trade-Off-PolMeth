@article{ WOS:000306092700008,
Author = {Vernes, Paule-Monique},
Title = {Did Jean-Jacques died a timely death?},
Journal = {PENSEE},
Year = {2012},
Number = {370},
Pages = {89+},
Month = {APR-JUN},
Abstract = {Paule-Monique Vernes, Did Jean-Jacques died a timely death ?
   It would be false to say that Robespierre, one of the earliest followers
   of Rousseau, found in Rousseau's work a posteriori justifications for
   his own ideas. However, the sovereignty of the people, the principle of
   virtue, the Supreme Being, the Revolutionary feast days, as conceived by
   the Jacobins, seem foreign to the logic and complexity of Rousseau's
   work. To account for this misinterpretation, one must return to the
   philosophy of history, as far as it concerns Rousseau, and to the
   concept of political revolution which leaves little doubt as to the
   benefits one could draw from its fulfillment. If, as Desmoulins said,
   Robespierre was a Rousseau in power, then Jean-Jacques died a timely
   death.},
