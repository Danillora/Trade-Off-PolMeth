@article{ WOS:000303127700007,
Author = {Ladrech, Robert},
Title = {Party Change and Europeanisation: Elements of an Integrated Approach},
Journal = {WEST EUROPEAN POLITICS},
Year = {2012},
Volume = {35},
Number = {3},
Pages = {574-588},
Abstract = {The literature on party organisational change emphasises environmental
   factors as well as internal circumstances. The literature on
   Europeanisation and political parties privileges the EU as a key
   environmental factor in terms of change. This article combines insights
   from the party organisation and Europeanisation literatures in order to
   more precisely conceptualise the EU as a stimulus and therefore causal
   factor in party organisational change. Two types of party change are
   analysed, new party positions (MEPs and specialised EU party staff) and
   party leaderships' responses to internal dissent over the EU. Linking a
   specific form of EU stimulus to a particular party goal helps to explain
   certain types of change, while inter-governmental bargaining may produce
   uncertainty for domestic political actors thereby inducing defensive
   reactions.},
