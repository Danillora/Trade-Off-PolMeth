@article{ WOS:000317531800016,
Author = {Bank, Andre and Richter, Thomas},
Title = {Authoritarian Monarchies in the Middle East. Conditions for Survival and
   Breakdown after 1945},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2012},
Number = {47},
Pages = {384+},
Abstract = {Andre Bank und Thomas Richter, Authoritarian Monarchies in the Middle
   East. Conditions for Survival and Breakdown after 1945, pp. 384-417.
   Neither in authoritarianism studies nor in the Middle East-specific
   literature on monarchies does there exist a convincing explanation for
   the survival as well as the breakdown of authoritarian monarchies. Using
   Fuzzy-Set QCA, we compare 13 current and former monarchies in the Middle
   East after 1945. We thereby concentrate on the four most prominent
   explanatory factors derived from previous research on authoritarian
   monarchies, namely external support, rent income, family participation
   in political decision-making as well as legitimation qua religion and
   state-building. Our findings point to the existence of different
   equifinal trajectories for the survival and the breakdown of
   authoritarian monarchies, which however both specifically combine
   external and internal conditions.},
