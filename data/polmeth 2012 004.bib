@article{ WOS:000309684700015,
Author = {Rich, Jessica A. J. and Gomez, Eduardo J.},
Title = {Centralizing Decentralized Governance in Brazil},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2012},
Volume = {42},
Number = {4},
Pages = {636-661},
Month = {FAL},
Abstract = {Contrary to the once-popular notion of the central bureaucracy withering
   away as an outcome of decentralization, scholars have shown that in
   cases of local policy success, national bureaucrats have instead
   redefined their roles, strengthening their focus on monitoring and
   accountability. Yet building national capacity for effective oversight
   presents a challenge within a context of strong subnational autonomy
   such as Brazil. Comparing the dynamics of decentralization across two
   areas of health policy, AIDS and tuberculosis, this article presents one
   strategy utilized by federal bureaucrats to increase their regulatory
   capacity: seeking resources located outside the formal political arena.
   Specifically, national bureaucrats utilize international resources to
   mobilize local civic groups as policy watchdogs, thus increasing the
   accountability of subnational politicians both to the center and to the
   public.},
