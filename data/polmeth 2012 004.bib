@article{ WOS:000217015700012,
Author = {Matthiasdottir, Sigriour},
Title = {The New Feminist Movement in the Nordic Countries in the 1970s},
Journal = {ICELANDIC REVIEW OF POLITICS \& ADMINISTRATION},
