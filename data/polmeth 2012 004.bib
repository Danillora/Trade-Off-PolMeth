@article{ WOS:000304500900006,
Author = {Alfaro-Redondo, Ronald and Gomez-Campos, Steffan},
Title = {Costa Rica: Political reconfiguration under divided government},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2012},
Volume = {32},
Number = {1},
Pages = {109-128},
Abstract = {Three main features characterize socio-political events in Costa Rica
   during 2010 and 2011. First, in the political system, internal divisions
