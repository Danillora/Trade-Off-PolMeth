@article{ WOS:000308224100003,
Author = {Hazama, Yasushi},
Title = {Hegemonic preservation or horizontal accountability: constitutional
   review in Turkey},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2012},
Volume = {33},
Number = {4},
Pages = {421-440},
Month = {SEP},
Abstract = {Does constitutional review in emerging democracies tend to enhance
   horizontal accountability or to preserve state-elite hegemony? The
   results of a quantitative analysis of abstract constitutional review in
   Turkey during the 1984-2007 period show: (1) that the Constitutional
   Court was no more likely to accept unconstitutionality claims by
   state-elite parties than by non-state-elite parties; and (2) that the
   Constitutional Court was more likely to accept unconstitutionality
   claims of executive transgressions than those of state-principles
   violations. In sum, the findings largely point to the Constitutional
   Court's preference for horizontal accountability over hegemonic
   preservation.},
