@article{ WOS:000309727400019,
Author = {Sebold, Karen and Limbocker, Scott and Dowdle, Andrew and Stewart,
   Patrick},
Title = {The Political Geography of Campaign Finance: Contributions to 2008
   Republican Presidential Candidates},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2012},
Volume = {45},
Number = {4},
Pages = {688-693},
Month = {OCT},
Abstract = {In fundraising, potential candidates who do not collect sizable amounts
   of ``early money{''} may be effectively eliminated even before the start
   of the Iowa Caucus. This winnowing raises concern about the impact money
   has on narrowing the field of candidates from whom voters can choose. To
   better grasp patterns of successful fundraising, we explore where
   candidates obtain funds during the preprimary and primary periods. We
   use individual contributions data from the Federal Election Commission
   during the preprimary and primary periods of the 2008 Republican
