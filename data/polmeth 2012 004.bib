@article{ WOS:000218532300005,
Author = {Janeliunas, Tomas},
Title = {The Lithuanian OSCE chairmanship 2011: Ambitions and results},
Journal = {SECURITY AND HUMAN RIGHTS},
Year = {2012},
Volume = {23},
Number = {2},
Pages = {115+},
Abstract = {The goal of this article is to discuss and evaluate the importance of
   Lithuania's OSCE Chairmanship in 2011, the achievements of the political
   objectives and the results of the chairmanship for Lithuanian national
   interests, and foreign and security policy. The article raises questions
   as to what motivates national states to seek the chairmanship of the
   OSCE, how they used to formulate the agendas of the chairmanship and
   what obligations have to be assumed by chairing the OSCE. It is argued
   that the motivation for Lithuania's OSCE Chairmanship has evolved from
   early efforts to enhance national interests (based on political
   objectives) to an obligation to be efficient in fulfilling the formal
   functions of the OSCE (based on functional goals).},
