@article{ WOS:000309593500003,
Author = {Wollebaek, Dag and Lundasen, Susanne Wallman and Traegardh, Lars},
Title = {Three Forms of Interpersonal Trust: Evidence from Swedish Municipalities},
Journal = {SCANDINAVIAN POLITICAL STUDIES},
Year = {2012},
Volume = {35},
Number = {4},
Pages = {319-346},
Month = {DEC},
Abstract = {Social trust is usually treated as a dichotomy between particularized
   and generalized trust. In this article it is argued that a third
   distinct form, community trust, is neither particularized nor
   generalized and bounded in space rather than persons. Factor analysis of
   survey data from 33 Swedish municipalities (N = 6,453) distinguishes
   between particularized, generalized and community trust. Furthermore,
   regression analyses show that the three trust forms have partly distinct
   antecedents and linked to different types of behaviours. While
   generalized trust best predicts leaps of faith in relation to strangers,
   community trust is the only trust form significantly predicting taking
   part in local problem solving. Finally, multilevel analysis shows that
   community trust is the trust form most vulnerable to changes with
   respect to ethnic diversity and socioeconomic equality.},
