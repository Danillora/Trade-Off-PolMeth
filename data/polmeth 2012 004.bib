@article{ WOS:000218532300007,
Author = {Zieba, Marta},
Title = {To what extent is lustration an effective mechanism of transitional
   justice and democratic consolidation? The case of Polish Lustration Law},
Journal = {SECURITY AND HUMAN RIGHTS},
Year = {2012},
Volume = {23},
Number = {2},
Pages = {147+},
Abstract = {Poland was the last Eastern European country to carry out lustration
   and, unlike many other post-communist states, focused on discontinuing
   past values through the revelation of truth. This article aims to assess
   whether the three aims of lustration: The provision of factual records
   about the past, the establishment of minimal justice and strengthening
   democratic processes, have been satisfied in the years following the
   Round-table talks. Through an analysis of recent legislative
   developments, the argument concludes that the inherently temporary
   nature of the process and its late implementation have left it
   vulnerable to becoming a retributive, rather than stabilizing instrument
   of transitional justice.},
