@article{ WOS:000219453000002,
Author = {Ketola, Markus},
Title = {The Everyday Politics of the European Public Sphere: Moving Beyond EU
   Policy Perspectives},
Journal = {JOURNAL OF CIVIL SOCIETY},
Year = {2012},
Volume = {8},
Number = {3, SI},
Pages = {213-228},
Abstract = {While in the academic debate the idea of a European public sphere is
   seen from both consensual and conflictual perspectives, in the field of
   European Union (EU) policymaking, it tends to assume the profile of a
   neutral and all-inclusive social space. By focusing on the street level,
   this article suggests that such a view is problematic because it fails
   to resonate with people's everyday experiences. The article contrasts EU
   policy on civil society engagement and immigration with examples of
   civil society reactions to immigration in two corners of Europe-on the
   Italian island of Lampedusa and in the city of Athens, Greece. In order
   for the concept of a European public sphere to garner meaning outside
   the EU policy process, it needs to be anchored in the everyday politics
   of social space in Europe. This means embracing an `ethnographic
   approach' that is sensitive to difference, diversity, and conflict, and
   to the daily (micro) political struggles that are played out in the
   public sphere.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
