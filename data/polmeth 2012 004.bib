@article{ WOS:000307336400001,
Author = {Dussauge-Laguna, Mauricio I.},
Title = {On The Past and Future of Policy Transfer Research: Benson and Jordan
   Revisited},
Journal = {POLITICAL STUDIES REVIEW},
Year = {2012},
Volume = {10},
Number = {3},
Pages = {313-324},
Month = {SEP},
Abstract = {David Benson and Andrew Jordan's article What Have We Learned from
   Policy Transfer Research? Dolowitz and Marsh Revisited provides a timely
   and interesting assessment on the state of the art. However, it is also
   a debatable one for at least two reasons: because it more or less
   assumes that Dolowitz and Marsh's works have provided the intellectual
   foundations of the policy transfer approach; and because Benson and
   Jordan use a rather restrictive understanding of what counts as policy
   transfer studies. This article will challenge these assumptions and will
   argue that we can find conceptualisations and questions about policy
   transfer in earlier works by other scholars; that Dolowitz and Marsh's
   definition of policy transfer is widely used, but has been subject to
   more criticisms than Benson and Jordan acknowledge; that a thorough
   assessment of our knowledge about policy transfer requires the use of a
