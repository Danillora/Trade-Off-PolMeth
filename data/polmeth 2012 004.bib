@article{ WOS:000307539900001,
Author = {Nishikawa, Misa},
Title = {Electoral and party system effects on ruling party durability},
Journal = {PARTY POLITICS},
Year = {2012},
Volume = {18},
Number = {5},
Pages = {633-652},
Month = {SEP},
Abstract = {Political scientists have not paid sufficient attention to the driving
   forces of ruling party stability, although other areas of political
   stability, such as democratic stability, leadership stability and
   cabinet stability have been studied extensively. This research fills a
   significant gap. It focuses on electoral rules and political party
   systems to explain ruling party durability. It demonstrates the
   following: (1) ruling parties' hazard rates under the
   first-past-the-post systems are initially lower than those under
   proportional representation rules, but this tendency reverses over time,
   and (2) ruling parties' hazard rates under two-party systems are
   initially lower than those under multiparty systems, but this too
   reverses.},
