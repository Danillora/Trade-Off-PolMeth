@article{ WOS:000309294000016,
Author = {Wagner, Aiko and Wessels, Bernhard},
Title = {Chancellor Candidates - How Do they Impact Vote Choice?},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2012},
Number = {45},
Pages = {345+},
Abstract = {Aiko Wagner / Bernhard Wessels, Chancellor Candidates How Do they Impact
   Vote Choice?, pp. 345-370.
   Candidate-centered politics plays a relevant role in public and
   scientific discourse. It is said that chancellor candidates can decide
   elections. On the basis of this debate we empirically test a theoretical
   model of vote choice with three steps of decision-making: generalization
   of evaluations, preference formation and decision process. Results for
   the federal elections from 1998 to 2009 show that information processing
