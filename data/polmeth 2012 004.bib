@article{ WOS:000446948100011,
Author = {Zulal, Shwan},
Title = {Survival Strategies and Diplomatic Tools: The Kurdistan Region's Foreign
   Policy Outlook},
Journal = {INSIGHT TURKEY},
Year = {2012},
Volume = {14},
Number = {3},
Pages = {141-158},
Month = {SUM},
Abstract = {The Kurds were late to the idea of nationalism in the 20th century, and
   when the borders were drawn in the region they became the largest
   stateless nation in the world, divided mainly between Turkey, Iran, Iraq
   and Syria. In an unlikely period when hope was fading, a Kurdistan
   regional government in Iraq was born as the former Iraqi regime was
   weakened after the first Gulf War and the subsequent no-fly zone. Two
   decades on, the region has become more assertive and been making many
   new friends, largely because of its newfound wealth, its influence in
   post-Saddam Iraq, and its stability when compared with the rest of Iraq.
   Oil has been a curse for the Kurds and Iraq as a whole, but now the
   Kurds appear to have found a way to use its resources for economic
   development, ensuring that the Kurdistan region remains stable and can
   establish itself as a self-governing and influential entity.},
