@article{ WOS:000421472200005,
Author = {Deluchey, Jean-Francois},
Title = {Organized civil society and the government administration of interests:
   the example of joint councils},
Journal = {REVISTA ESTUDOS POLITICOS},
Year = {2012},
Number = {5},
Pages = {77-101},
Abstract = {This article addresses the issue of civil society participation in new
   institutions of Brazilian democracy, such as the Joint Councils. Taking
   into account the legitimacy and representativeness of the members of
   such Councils, this study identifies some internal political asymmetry
   and analyzes the conditions of the (dis) qualification of the discourse
   of civil society members. The author assumes that, despite the fact that
   these Councils present themselves as innovative policy arenas capable of
   consolidating democratic debate, they are more likely to become spaces
   of control and disqualification of civil society paricipation, through
   the logics of neoliberal governmentality, which aims to confine the
   debate in the sphere of interests and deny politics.},
