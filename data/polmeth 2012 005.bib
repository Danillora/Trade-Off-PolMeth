@article{ WOS:000309626400005,
Author = {Polak, Jan Tobias},
Title = {Youth Movements in West Africa - the Ambivalent Political Proximity of
   the Malian Youth Movement},
Journal = {SWS-RUNDSCHAU},
Year = {2012},
Volume = {52},
Number = {3},
Pages = {291-311},
Abstract = {In response to the revolutions in Northern Africa, hope has risen that
   youth movements in African societies south of the Sahara will also
   contribute to political change. Based on an empirical research about
   local environmental politics in Mali, the article tries to find an
   answer to the question, how youth movements act politically on an
   everyday basis and how this political action is viewed by them. The
   article shows that local youth organisations have an intense
   relationship with the relevant local political representations. But with
   this proximity to political decision makers comes the
   instrumentalisation of youth by politicians. Therefore, their
   possibilities to criticize more fundamentally the local political system
   are limited.},
