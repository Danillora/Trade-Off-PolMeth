@article{ WOS:000439310700009,
Author = {Ozbebek Tunc, Aysegul and Akdemir Omur, Gokce and Duren, A. Zeynep},
Title = {ENVIRONMENTAL AWARENESS},
Journal = {ISTANBUL UNIVERSITY JOURNAL OF FACULTY OF POLITICAL SCIENCES-SIYASAL
   BILGILER FAKULTESI DERGISI},
Year = {2012},
Number = {47},
Pages = {227-246},
Month = {OCT},
Abstract = {21st century global knowledge world requires not only rethinking on
   economics, politics, social things, technology and increasing importance
   of knowledge but also rethinking on ecology and handling it as a part of
   globalization trend. Environment issues recently have became the topic
   that discussed, looked for solutions and concerned with all people.
   Improving environmental awareness of people and taking precautions is
   assumed that vital step on solving these issues. In this respect,
   creating environmental awareness and protection on people especially
   students have taken place in academic studies. The aim of this study is
   to prove the relationship between environmental awareness and personal
   precaution of university students. In this context, the model of study
   is shaped knowing environmental issues as antecedent, environmental
   awareness as necessity and improving personal precaution as conclusion.
   Survey results attending 353 university students showed that university
   students have information about and aware of environmental issues, but
   they do not behave cautious to the environment.},
Publisher = {ISTANBUL UNIV},
Address = {VETERINER FAKULTESI, ISTANBUL, 00000, TURKEY},
Type = {Article},
