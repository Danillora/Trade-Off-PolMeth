@article{ WOS:000420544400007,
Author = {Dlabac, Oliver and Schaub, Hans-Peter},
Title = {A dual measurement concept for liberal and radical quality of democracy},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2012},
Volume = {6},
Number = {1},
Pages = {161-184},
Month = {SEP},
Abstract = {Since existing measures of democracy are based solely on a liberal
   conceptualization of representative democracy, they overlook another
   fundamental tradition: that of radical democracy, which strives for
   direct participation of all citizens in the formation of the public
   opinion and in political decision-making. The quality of democracy of
   political systems with pronounced radical democratic institutions and
   culture has therefore not been well understood so far. Drawing from
   classical liberal and radical views on what democratic institutions can
   or should accomplish, the authors construct two empirically measurable
   models of democracy. With a particular interest in assessing democracy
   in the Swiss cantons, they present a dual measurement concept capable of
   differentiating democratic systems according to their degree of both
   liberal and radical democracy.},
Publisher = {SPRINGER VIEWEG-SPRINGER FACHMEDIEN WIESBADEN GMBH},
