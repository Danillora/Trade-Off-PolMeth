@article{ WOS:000210638600007,
Author = {Wayne, Mike},
Title = {Kant's Philosophy of the Aesthetic and the Philosophy of Praxis},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2012},
Volume = {24},
Number = {3},
Pages = {386-402},
Month = {JUL},
Abstract = {This essay seeks to reconstruct the terms for a more productive
   engagement with Kant than is typical within contemporary academic
   cultural Marxism, which sees him as the cornerstone of a bourgeois model
   of the aesthetic. The essay argues that, in the Critique of Judgment,
   the aesthetic stands in as a substitute for the missing realm of human
   praxis. This argument is developed in relation to Kant's concept of
   reflective judgment that is in turn related to a methodological shift
   toward inductive and analogical procedures that help Kant overcome the
   dualisms of the first two Critiques. This reassessment of Kant's
   aesthetic is further clarified by comparing it with and offering a
   critique of Terry Eagleton's assessment of the Kantian aesthetic as
   synonymous with ideology.},
