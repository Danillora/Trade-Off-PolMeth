@article{ WOS:000314190400005,
Author = {Otero Hermida, Paula and Bouzas Lorenzo, Ramon},
Title = {Structure, resources and development of equality policies at local level},
Journal = {REVISTA DE ESTUDIOS POLITICOS},
Year = {2012},
Number = {158},
Pages = {139-163},
Month = {OCT-DEC},
Abstract = {This article provides an overview of equality policies at the local
   level, based on a study carried out in Spanish municipalities with
   between 25,000 and 100,000 inhabitants. Special attention is given to
   the impact of variables such as the political composition of municipal
   governing bodies, the gender of the highest political authority and how
   the size of the municipality influences the development of gender
   policies. Our findings show that though efforts in this area have
   achieved greater institutionalization of gender equality policies due to
   the establishment of gender-related normative and administrative tools
   and the increased structural capacity of municipal administrations,
   these do not guarantee consistently transversal or broad implementation
   of equality plans in local governmental actions. However, and in line
   with prior studies, a positive correlation was found between the
   ideological profile of the governing political party, progress in
   equality plans and greater permeability in female access to executive
   positions.},
