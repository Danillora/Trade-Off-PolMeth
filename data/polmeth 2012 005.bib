@article{ WOS:000421244600005,
Author = {Bryza, Matthew J.},
Title = {TURKEY'S DRAMATIC SHIFT TOWARD IRAQI KURDISTAN: POLITICS BEFORE PEACE
   PIPELINES},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2012},
Volume = {11},
Number = {2},
Pages = {53-61},
Month = {SUM},
Abstract = {Ankara's dramatic and positive shift toward Erbil is driven primarily by
   its aim to elevate its credentials as an energy transit hub, but is also
   in line with the AKP's policies towards the domestic Kurdish issue.
   Regardless of the position of Washington, Ankara and Erbil now perceive
   their political interests as synchronizing with their longstanding
   economic interests. Yet Turkish and KRG authorities must still need to
   take a series of bold political decisions - such as either to
   accommodate or confront Iraq's federal government in Baghdad. Moreover,
   if Syria breaks apart and an autonomous Kurdish region that favors
   pan-Kurdish unity emerges, Ankara may rethink its political decision to
   hold Iraqi Kurds close as economic partners.},
