
@article{ WOS:000457923400003,
Author = {Balalaeva, Dina},
Title = {POLITICAL INSTITUTIONS: BLOCKING OR PROMOTING INNOVATIONS?},
Journal = {POLITEIA-JOURNAL OF POLITICAL THEORY POLITICAL PHILOSOPHY AND SOCIOLOGY
   OF POLITICS},
Year = {2012},
Volume = {65},
Number = {2},
Pages = {44+},
Abstract = {On the basis of the analysis of the innovation production process
   through the prism of institutional design and rational choice theories
   D.Balalaeva demonstrates that if the established institutional design
   aiming at protecting the status quo stays intact, the innovational
   system of Russia will not develop any further. Breaking through this
   stalemate necessitates socio-economic changes that will exert influence
   upon the initial distribution of power in the society and as a result
   will alter elitist preferences. According to the author, this process
   might be accelerated through the collective action undertaken by the
   civil society and, in particular, by the Russian scientists, aiming at
   the protection of intellectual property rights and improvement of
   science status within the society.},
