@article{ WOS:000211573700013,
Author = {Davies, Sara E.},
Title = {Nowhere to hide: informal disease surveillance networks tracing state
   behaviour},
Journal = {GLOBAL CHANGE PEACE \& SECURITY},
Year = {2012},
Volume = {24},
Number = {1},
Pages = {95-107},
Abstract = {Since the revisions to the International Health Regulations (IHR) in
   2005, much attention has turned to how states, particularly developing
   states, will address core capacity requirements attached to the revised
   IHR. Primarily, how will states strengthen their capacity to identify
   and verify public health emergencies of international concern (PHEIC)?
   Another important but under-examined aspect of the revised IHR is the
   empowerment of the World Health Organization (WHO) to act upon
   non-governmental reports of disease outbreaks. The revised IHR
   potentially marks a new chapter in the powers of `disease intelligence'
   and how the WHO may press states to verify an outbreak event. This
   article seeks to understand whether internet surveillance response
   programs (ISRPs) are effective in `naming and shaming' states into
   reporting disease outbreaks.},
