@article{ WOS:000306118700005,
Author = {Kay, John},
Title = {THE MAP IS NOT THE TERRITORY: MODELS, SCIENTISTS, AND THE STATE OF
   MODERN MACROECONOMICS},
Journal = {CRITICAL REVIEW},
Year = {2012},
Volume = {24},
Number = {1},
Pages = {87-99},
Abstract = {Policy makers and economists alike failed to predict the financial
   crisis of 2008. Their failure is due not only to the difficulties in
   predicting events in a complex world, but to the self-referential
   character of modern macroeconomics. Instead of seeking new empirical
   insights about economic behavior, macroeconomists have become creators
   of computer games-content to develop models that are internally
   consistent but have no necessary connection to the real world. Economic
   modeling aspires to be scientific in its deductive consistency and
   rigor. However, even the natural sciences require a good dose of
   induction, which, in turn, entails varying one's method of study based
   on the phenomenon one is studying.},
