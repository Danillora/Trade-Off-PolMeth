@article{ WOS:000308876800001,
Author = {Tresch, Anke},
Title = {The (Partisan) Role of the Press in Direct Democratic Campaigns:
   Evidence from a Swiss Vote on European Integration},
Journal = {SWISS POLITICAL SCIENCE REVIEW},
Year = {2012},
Volume = {18},
Number = {3},
Pages = {287-304},
Month = {SEP},
Abstract = {This article analyzes the role of the press in direct democratic
   campaigns. The paper argues the press has a dual role: On news pages,
   newspapers ought to inform citizens about the issue positions and frames
   of the pro and con camps in a balanced way. In editorials, newspapers
   act as political advocates that promote their own issue frames and try
   to shape public opinion through voting recommendations. Comparing the
   issue positions and frames in editorials and news reports in the run-up
   to the vote on the popular initiative Yes to Europe in Switzerland, this
   article shows that newspapers give similar visibility to the pro and con
   camps regardless of the papers own editorial position. However, some
   newspapers favor issue frames that are in line with their editorial
   perspectives. In conclusion, newspapers are more similar in news report
   content than in editorial views.},
