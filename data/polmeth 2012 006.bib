@article{ WOS:000314174300003,
Author = {Vauchez, Antoine},
Title = {Keeping the dream alive: the European Court of Justice and the
   transnational fabric of integrationist jurisprudence},
Journal = {EUROPEAN POLITICAL SCIENCE REVIEW},
Year = {2012},
Volume = {4},
Number = {1},
Pages = {51-71},
Month = {MAR},
Abstract = {How does the European Court of Justice (ECJ) firmly maintain a now
   45-year-old consistent integrationist jurisprudence when exerting
   virtually no control over the recruitment of its members (a selection
   left to national governments)? Rather than considering such judicial
   consistency over time as a `given', the paper questions the social
   fabric of judicial preferences. On the basis of a variety of
   commemorative materials produced within the Court (Festschriften,
   tributes, eulogies, and jubilees) and never studied so far, the paper
   stresses the manner in which these rituals are home to social processes
   of aggregation (into one unique judicial family), demarcation (from the
   political realm), and self-identification (to roles of so-called
   `founding father', `current spokesmen', or `would-be judges'), thereby
   enabling transnational role transmission within international courts
   such as the ECJ.},
