@article{ WOS:000446944900010,
Author = {Sinkaya, Bayram},
Title = {Rationalization of Turkey-Iran Relations: Prospects and Limits},
Journal = {INSIGHT TURKEY},
Year = {2012},
Volume = {14},
Number = {2},
Pages = {137-156},
Month = {SPR},
Abstract = {This article examines Turkish-Iranian relations in the 2000s, when the
   two countries initiated an unprecedented rapprochement. It argues that
   modification of foreign policy paradigms in Turkey and Iran led to the
   rationalization of bilateral relations that paved the way for
   improvement of economic and political ties between the two states. In
   addition to the rationalization, a supportive regional context helped
   them expand their relations. However, structural differences prevent the
   Turkish-Iranian rapprochement from turning into a strategic partnership.
   Moreover, restructuring of the regional context and rise of the specter
   of a conflictual relationship, which is still alive, threaten the future
   of Turkish-Iranian relations.},
