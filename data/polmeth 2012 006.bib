@article{ WOS:000209003800007,
Author = {Dupuy, Claire},
Title = {The race in the mid-regions. Competition and regional political of
   education in France and in Germany},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2012},
Volume = {45},
Number = {4},
Pages = {881-907},
Month = {DEC},
Abstract = {The regionalization of public policy is one of the most remarkable
   transformations European states have undergone since the 1970s. Through
   an examination of this particular development, the article explores
   theories of interregional competition in two very different cases:
   education policy in France, a decentralized unitary state where regional
   governments are entrusted with limited policy competences in this field,
   and Germany, a federal state where regions have exclusive responsibility
   over secondary education. The paper shows that, in both cases,
   interregional competition lends itself neither to a race to the bottom
   nor a race to the top, but rather to a race to the middle. Regional
   governments aim, in fact, to demonstrate that they adopt similar
   policies to other regions so as to avoid being blamed by both the
   electorate and the central state.},
Publisher = {CAMBRIDGE UNIV PRESS},
