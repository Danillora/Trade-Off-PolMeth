@article{ WOS:000300885700006,
Author = {Smith, Alastair and Bueno De Mesquita, Bruce},
Title = {Contingent Prize Allocation and Pivotal Voting},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2012},
Volume = {42},
Number = {2},
Pages = {371-392},
Month = {APR},
Abstract = {Parties can elitcit widespread electoral support by making the
   distribution of prizes or rewards to groups of voters contingent upon
   electoral support. In addition to altering which party wins, a voter's
   choice also influences the distribution of prizes. This latter factor,
   referred to in this article as prize pivotalness, tends to be the
   dominant influence in vote choice. The desire to win prizes can induce
   voters to coalesce into a highly supportive group, even if they dislike
   the party's policies. Characterizing voting equilibria in this framework
   explains the rationale for the support of patronage parties, variance in
   voter turnout and the endogenous political polarization of groups in
   both established and new democracies.},
