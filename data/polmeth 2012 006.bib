@article{ WOS:000308240200003,
Author = {Nahaboo, Zaki},
Title = {Subverting orientalism: political subjectivity in Edmund Burke's India
   and liberal multiculturalism},
Journal = {CITIZENSHIP STUDIES},
Year = {2012},
Volume = {16},
Number = {5-6, SI},
Pages = {587-603},
Abstract = {There has recently been extensive debate on the relation between
   multiculturalism and orientalism. Yet the question which elides this is
   how orientalist visions of political subjectivity find themselves
   articulated through multiculturalism. To address this question, this
   article explains whether liberal-pluralist multiculturalism poses a
   challenge to political orientalism. Historically, Edmund Burke's
   writings on the French Revolution and speeches on India illustrate the
   emergence of the orientalised political subject. The traces of this in
   the present can be seen through the relationship between British
   multiculturalism and the undocumented migrant. By identifying the traces
   of the orientalised political subject within multiculturalist practice,
   this article reveals unsettling connections between multiculturalism,
   orientalism and political subjectivity.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
