@article{ WOS:000212012200001,
Author = {Green, Elliott},
Title = {The Political Demography of Conflict in Modern Africa},
Journal = {CIVIL WARS},
Year = {2012},
Volume = {14},
Number = {4},
Pages = {477-498},
Abstract = {Sub-Saharan Africa has shifted from having a low population density and
   no population growth in the 19th century to an extremely high population
   growth today. I argue here that an important cause behind contemporary
   civil conflict has been this rapid demographic shift. Specifically, I
   show that low population densities in Africa historically contributed to
   communal land rights and the creation of large states. In the
   postcolonial era, however, these two variables have combined with high
   population growth rates, low levels of urbanization, and rural-rural
   migration flows to produce large amounts of `sons of the soil' conflict
   over land. Evidence from contemporary civil wars in Sudan and the
   Democratic Republic of Congo supports my theory.},
