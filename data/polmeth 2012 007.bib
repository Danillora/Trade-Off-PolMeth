   Baylor Univ, Dept Polit Sci, Waco, TX 76798 USA.},
DOI = {10.1177/1532673X11416920},
ISSN = {1532-673X},
EISSN = {1552-3373},
Keywords = {economic inequality; political inequality; political representation;
   public opinion; state politics; public policy},
Keywords-Plus = {PUBLIC-OPINION; DYADIC REPRESENTATION; UNITED-STATES; INSTITUTIONS;
   DYNAMICS},
Research-Areas = {Government \& Law},
Web-of-Science-Categories  = {Political Science},
Author-Email = {Patrick\_J\_Flavin@baylor.edu},
