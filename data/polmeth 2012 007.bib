@article{ WOS:000297637600003,
Author = {Warwick, Paul V.},
Title = {Representation as a median mandate? A response to Best, Budge and
   McDonald},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
Year = {2012},
Volume = {51},
Number = {1},
Pages = {57-63},
Month = {JAN},
Abstract = {The preceding article by Best, Budge and McDonald acknowledges much of
   the substance of the alternative bilateralist interpretation of
   democratic governance I advocated and attempts to re-focus the median
   mandate approach towards a longer-term, and potentially more productive,
   understanding of the opinion-policy relationship. Both are welcome
   developments. Despite taking these steps, however, the authors choose to
   allow the fate of the median mandate thesis to rest ultimately on an
   attempt to re-establish the short-term one-to-one relationship that I
   challenged. In this brief note, I argue that this not only undercuts the
   more positive initiatives noted above, but also is based on a flawed
   understanding of how the short-term relationship should be
   operationalised and tested.},
