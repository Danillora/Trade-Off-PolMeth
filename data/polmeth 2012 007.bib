@article{ WOS:000210639700003,
Author = {Danby, Colin},
Title = {Postwar Norm},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2012},
Volume = {24},
Number = {4},
Pages = {499-515},
Month = {OCT},
Abstract = {Economic and population data standardized after World War II embedded
   the modernist ideology that society unites the opposed spheres of a
   competitive and calculating economy and loving, self-sacrificing
   households. The fundamental concept is complementarity, the
   union-in-difference of two opposed principles. This difference is
   commonly troped through gender (men compete and women care), generating
   an obvious heteronormativity, but the heteronorm goes deeper, into the
   very ways output and population become intelligible. This has
   implications even for radical social and theoretical interventions that
   conceive ``economy'' as a separate opposed sphere.},
