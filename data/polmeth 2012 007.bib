@article{ WOS:000212886500009,
Author = {Maffettone, Sebastiano},
Title = {Global justification and local legitimation},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2012},
Volume = {15},
Number = {2},
Pages = {239-257},
Abstract = {This paper distinguishes between the concepts of justification and
   legitimation with a view to offering a normative standard for global
   justice compatible with cultural pluralism. According to this
   distinction, justification is presented as an idealized, substantive and
   top-down enterprise rooted in the moral and metaphysical substrate of a
   specific culture. On the other hand, legitimation has a procedural and
   factual connotation and derives its strength from the success of some
   culturally independent but historically situated practice (bottom-up
   approach). Building on this distinction, I argue that justification and
   legitimation should be made complementary for the sake of crosscultural
   dialogue.},
