@article{ WOS:000317531800015,
Author = {Basedau, Matthias and Elischer, Sebastian},
Title = {Return to the Barracks? Authoritarian Rule in Sub-saharan Africa},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2012},
Number = {47},
Pages = {354+},
Abstract = {Matthias Basedau und Sebastian Elischer, Return to the Barracks?
   Authoritarian Rule in Sub-saharan Africa, pp. 354-383. Despite a decline
   in military interventions in sub-Saharan Africa, there are sufficient
   indications of a connection between the influence of the military and
   the stabilization of autocratic rule. Due to a scholarly neglect of the
   study of authoritarianism in Africa, this connection has not yet been
   analyzed systematically. With the help of several indicators this
   article examines to what extent autocratic rule in Africa constitute
   military rule or/and to what extent autocratic rule is dependent on the
   support of the military. Results indicate that the military is an
   important pillar of autocratic rule in Africa and that African armies
   are essential for the stabilization of military rule.},
