@article{ WOS:000449436400007,
Author = {Yannielli, Joseph L.},
Title = {The nationalist international: Or what American history can teach us
   about the fascist revolution},
Journal = {EUROPEAN JOURNAL OF POLITICAL THEORY},
Year = {2012},
Volume = {11},
Number = {4, SI},
Pages = {438-458},
Month = {OCT},
Abstract = {In challenging Marxist theorists to confront the radical rebirth at the
   core of the fascist revolution, Roger Griffin has carried fascist
   studies to a new and valuable plateau. Likewise, David D. Roberts's
   elaboration of Griffin's model offers a provocative and fruitful avenue
   to rethink fascist political culture. This article seeks to advance the
   dialogue to the next level by considering what an international approach
   can add to these primarily nationalist interpretations of generic
   fascism. Drawing on examples from the history of the United States, I
   argue that fascism is a fundamentally cosmopolitan process and that it
   needs to be placed on a broader continuum with the histories of slavery,
   racism and nationalism.},
