@article{ WOS:000215060800003,
Author = {Salton, Herman T.},
Title = {France's Other Enlightenment: Laicite, Politics and the Role of Religion
   in French Law},
Journal = {JOURNAL OF POLITICS AND LAW},
Year = {2012},
Volume = {5},
Number = {4},
Pages = {30-41},
Month = {DEC},
Abstract = {Although the separation of Church and State was a traumatic event for
   France, this article argues that its legal product-the Law of 1905-was
   and remains respectful of religious freedom. By considering the relevant
   literature and by reviewing the key laws and judicial decisions on the
   matter, this article also suggests that, for France, laicite represented
   `the other Enlightenment', that is to say, a moment when national
   identity was reawakened, if not formed anew. The piece concludes that
   the reason for this lies in the fact that, in 1905, France's Founding
   Fathers had the courage to distance the country's bloody religious past
   from its legal system, thus producing a statute (the Law of Separation)
   and a concept (laicite) that fostered tolerance rather than conflict.},
