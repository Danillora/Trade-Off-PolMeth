@article{ WOS:000213900200012,
Author = {Martin, Elaine},
Title = {`I' for iconoclasm: graphic novels and the (re) presentation of
   terrorism},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2012},
Volume = {5},
Number = {3},
Pages = {469-481},
Abstract = {This article considers a `new-old' media - that is, a relatively newly
   created medium with deep historical roots - that has gained increasing
   popularity in recent years: a subgenre of the comic book, most often
   referred to as the graphic novel. Presented here as an antidote to
   dominant interpretations of political violence ranging from the state
   terrorism of the Holocaust to the events of 9/11, the article briefly
   traces the history of graphic novels and details their growing
   popularity before describing and analysing representations of terrorism,
   both written and visual, in eight paradigmatic works that purvey
   variously victim, survivor and perpetrator perspectives.},
