@article{ WOS:000211649100004,
Author = {Drahokoupil, Jan and Domonkos, Stefan},
Title = {Averting the funding-gap crisis: East European pension reforms since
   2008},
Journal = {GLOBAL SOCIAL POLICY},
Year = {2012},
Volume = {12},
Number = {3, SI},
Pages = {283-299},
Month = {DEC},
Abstract = {This article analyses pension reforms in Central and East European
   countries in the aftermath of the 2008 financial crisis. The crisis
   revealed unresolved problems in the implementation of previous reforms,
   namely the financing of the transition costs. In their attempts to solve
