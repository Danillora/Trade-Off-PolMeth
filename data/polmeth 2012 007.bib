@article{ WOS:000211993800006,
Author = {Deane, Darren},
Title = {Architectural education as an abbreviated sphere of national
   collaboration: re-examining the Bauhaus},
Journal = {NATIONAL IDENTITIES},
Year = {2012},
Volume = {14},
Number = {3},
Pages = {273-285},
Abstract = {In Nations and Nationalism Gellner spoke for the capacity of
   institutions to substitute for weakened or withering nations, and it is
   in this sense that the Bauhaus (1919 - 1933), a German institute of
   architectural education, is theorised here as an abbreviated state of
   renewed collaboration. The paper posits the view that the contribution
   of architecture to discussions of nationalism, based on its capacity to
   elucidate latent aspects of the lifeworld, is its account of the spatial
   and material state of the nation. Institutions are viewed as
   middle-ground operations that rehearse the national spirit within an
   abbreviated setting for redemptive praxis.},
