@article{ WOS:000310680800010,
Author = {Smelcer, Susan Navarro and Steigerwalt, Amy and Vining, Jr., Richard L.},
Title = {Bias and the Bar: Evaluating the ABA Ratings of Federal Judicial
   Nominees},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2012},
Volume = {65},
Number = {4},
Pages = {827-840},
Month = {DEC},
Abstract = {The vetting of potential federal judges by the Standing Committee on
   Federal Judiciary of the American Bar Association (ABA) is politically
   controversial. Conservatives allege the Standing Committee is biased
   against Republican nominees. The ABA and its defenders argue the ABA
   rates nominees objectively based on their qualifications. The authors
   investigate whether accusations of liberal bias have merit. They analyze
   all individuals nominated to the U.S. Courts of Appeals from 1977 to
   2008. Using genetic matching methods and ordered logit models, the
   authors find evidence of bias against Republican nominees in the ABA's
   ratings. They conclude by discussing the implications of these results.},
