@article{ WOS:000215409500002,
Author = {Ramos Torre, Ramon},
Title = {Sociology of evil and theodicy in Durkheim's Elementary Forms of
   Religious Life},
Journal = {POLITICA Y SOCIEDAD},
Year = {2012},
Volume = {49},
Number = {2},
Pages = {223-240},
Abstract = {In his analysis of piaculiar rites in The Elementary Forms of Religious
   Life, Durkheim outlines a sociology of evil of general interest. This
   sociology conceives evil as a social fact and explains the rites of
   mourning and atonement, the belief in malignant powers and the ritual
   purge of evil. Durkheim relates evil with the ambiguity of the sacred,
   causing serious tensions in his theoretical system. He also addresses
   the problem of theodicy for which he proposes a lay solution in the form
   of a functional based sociodicy.},
