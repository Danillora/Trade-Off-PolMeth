@incollection{ WOS:000325621300008,
Author = {Rasmussen, Claire E.},
Editor = {Sarat, A},
Title = {IMAGINING OTHERNESS: THE POLITICAL NOVEL AND ANIMAL RIGHTS},
Booktitle = {SPECIAL ISSUE: THE LEGACY OF STUART SCHEINGOLD},
Series = {Studies in Law Politics and Society},
Year = {2012},
Volume = {59},
Pages = {155-177},
Abstract = {Scheingold's The Politics of Rights and The Political Novel while having
   different objects of study at the center of their analyses, both concern
   themselves with the difficulties in producing meaningful social change
   on a late modern political terrain. His critiques of rights-claiming are
   echoed in debates over the practical and philosophical difficulties
   incorporating animals into contemporary legal regimes. This chapter
   considers insights from Scheingold's two texts arguing that his insights
   into the legal imaginary in the latter text anticipates the critique of
   animal rights while his emphasis on the fictional imaginary in the
   former text can also be found in contemporary texts that suggest animals
   can help us rethink political agency.},
