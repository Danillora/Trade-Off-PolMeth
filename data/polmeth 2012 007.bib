@article{ WOS:000422312700005,
Author = {Lopez Roa, Juan Carlos},
Title = {The right to public space},
Journal = {PROVINCIA},
Year = {2012},
Number = {27},
Pages = {105-136},
Month = {JAN-JUN},
Abstract = {The study and promotion of the emerging human rights has become
   increasingly relevant in recent years, especially after the 2004,
   Barcelona Universal Forum of Cultures. These rights, have turned into an
   essential tool for social legislation for any government. Within this
   category, one can find the right to public space as a fundamental part
   of the right to the city. The absence and disregard for this collective
   right in the legal ordering of Venezuela has favored social exclusion,
   limited participation, the loss of community identity and communication,
   segregation, the lack of monumentality and beauty, privatization,
   fragmentation and insecurity, reduced mobility and accessibility,
   limited spaces for sports and recreation. Without public spaces, there
   is no democracy nor quality of life, no equality nor solidarity. The
   pedagogy of otherness is perverted. Thus, the importance in guaranteeing
   effectively this right. It is necessary for the condition and expression
   of citizenship and therefore, of great relevance for the healthy
   existence of cities presently and in the future. The opposite could mean
   the death of the city.},
