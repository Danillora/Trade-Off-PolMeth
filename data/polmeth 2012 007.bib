@article{ WOS:000305439600002,
Author = {Bertossi, Christophe},
Title = {French republicanism and the problem of normative density},
Journal = {COMPARATIVE EUROPEAN POLITICS},
Year = {2012},
Volume = {10},
Number = {3, SI},
Pages = {248-265},
Month = {JUL},
Abstract = {French republicanism is usually understood as the legacy of the French
   Revolution and the Third Republic, a unique example of colorblind
   assimilation and universal citizenship: the model for understanding
   immigrant integration in contemporary France. This article challenges
   this usual conception of the French model, and highlights the extent to
   which this approach is not successful in conceptualizing the evolution
   and content of how France's politics and policies have defined `French
   republicanism' over the last three decades. To do so, the article
   emphasizes first the problems of conceiving a national model such as
   France's as a dense system of values a philosophy of integration or a
   national idiom of citizenship. Second, through the analysis of public
   discourses and official reports between 1983 and 2012, the article
   identifies strong variations in the French public narratives, revealing
   the existence of no less than four different and contradictory schemas
   throughout the period: nationality-based integration in the 1980s,
   antidiscrimination in the 1990s, laicite in the 2000s and assimilation
   based on cultural order in the 2010s. Comparative European Politics
   (2012) 10, 248-265. doi:10.1057/cep.2012.6},
