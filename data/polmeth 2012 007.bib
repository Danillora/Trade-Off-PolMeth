@article{ WOS:000312694100002,
Author = {Rosar, Ulrich and Klein, Markus and Beckers, Tilo},
Title = {Magic Mayors: Predicting Electoral Success from Candidates' Physical
   Attractiveness under the Conditions of a Presidential Electoral System},
Journal = {GERMAN POLITICS},
Year = {2012},
Volume = {21},
Number = {4},
Pages = {372-391},
Abstract = {A range of empirical studies has shown that candidates' physical
   attractiveness can substantially influence the outcome of political
   elections. This applies to different countries, different electoral
   systems, and different levels of political systems, and equally affects
   simple direct or list candidates and front-runners. However, no previous
   investigation using actual election results has been made into whether
   candidates' attractiveness also has an effect under the conditions of a
   presidential electoral system. Theoretical reasons can be formulated
   that suggest attractiveness is ineffective under these circumstances. In
   order to clarify this point empirically, we analysed the 2009 North
   Rhine-Westphalia mayoral elections. Yet the results of the analyses
   clearly show that candidates' attractiveness has a substantial
   influence. Taking into account earlier findings, the influence of
   physical attractiveness in political elections appears to be resistant,
   to a large degree, to varying constraints.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
