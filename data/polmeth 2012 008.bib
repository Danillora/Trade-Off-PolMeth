@article{ WOS:000210638600003,
Author = {Kayatekin, Serap},
Title = {Walking in the Graveyard that Is Economics: Comments on Postmodern
   Moments in Modern Economics},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2012},
Volume = {24},
Number = {3},
Pages = {344-354},
Month = {JUL},
Abstract = {In their seminal book Postmodern Moments in Modern Economics, David
   Ruccio and Jack Amariglio destabilize the hegemony of economics by
   challenging its modernist claims of a unified science, uncovering and
   analyzing in depth instances of postmodernity buried under this claim.
   While the book accomplishes this task admirably, it also raises a whole
   set of aporias that are of equal importance. In this article I argue
   that, while it is imperative to point to the plurality of views within
   economics, it is also crucial to reflect on the uneven context of any
   possible `conversation' between the different points of view. I also
   argue that, while it is indeed destabilizing to note the different views
   of core concepts within the discourse, this effort should be accompanied
   with an analysis of the context of power relations, those conditions of
   existence that render these conceptualizations and their transformations
   possible.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
