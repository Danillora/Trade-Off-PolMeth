@article{ WOS:000214443000005,
Author = {Carreras, Miguel},
Title = {Party Systems in Latin America after the Third Wave: A Critical
   Re-assessment},
Journal = {JOURNAL OF POLITICS IN LATIN AMERICA},
Year = {2012},
Volume = {4},
Number = {1},
Pages = {135-153},
Abstract = {This essay calls for a more nuanced analysis of the evolution of party
   systems in Latin America. I contend that the general impression that
   party systems are collapsing in Latin America and that processes of
   partisan and electoral dealignment are affecting most countries in the
   region is incorrect. I also argue that the process of moderation and
   de-ideologization of the main political parties in many Latin American
   party systems often facilitates processes of democratic consolidation.
   Finally, I discuss the positive impact recent transformations of Latin
   American party systems had on political representation in the region, by
   showing that formerly excluded groups - especially indigenous groups -
   have been integrated into the political system.},
