@article{ WOS:000311369100003,
Author = {Gallagher, Michael},
Title = {The Political Role of the President of Ireland},
Journal = {IRISH POLITICAL STUDIES},
Year = {2012},
Volume = {27},
Number = {4, SI},
Pages = {522-538},
Abstract = {The formal constitutional powers of the President of Ireland define the
   framework within which the office holder can act, but they tell only
   part of the story. The main powers of the President are the right to
   refer bills to the Supreme Court for a decision on their
   constitutionality and, in certain circumstances, to refuse a prime
   ministerial request for a dissolution of parliament. Within the
   parameters of the constitution presidents might choose either a
   maximalist or a minimalist interpretation of the role, and in practice
   the latter has prevailed, perhaps due in some measure to path
   dependency. It is difficult to argue that any aspects of government
   policy would have been different had any presidential election produced
   a different outcome, though on occasions presidents have wielded `soft
   power' in a way that has had political repercussions. The article also
   addresses the question of what would happen if a president were to `go
   rogue' by refusing to abide by the letter of the constitution,
   highlighting the difficulty of removing a president from office.},
