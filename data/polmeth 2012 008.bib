@article{ WOS:000308298100004,
Author = {Esaiasson, Peter and Gilljam, Mikael and Persson, Mikael},
Title = {Which decision-making arrangements generate the strongest legitimacy
   beliefs? Evidence from a randomised field experiment},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
Year = {2012},
Volume = {51},
Number = {6},
Pages = {785-808},
Month = {OCT},
Abstract = {How can democracies satisfy citizens' demands for legitimate decision
   making? This article reports findings from a randomised field experiment
   designed to mimic decision making in large-scale democracies. Natural
   collectives of individuals with a shared history and future (high school
   classes) were studied. They were asked to make a decision about how to
   spend a sum of money under arrangements imposed by the researchers and
   distributed randomly across classes. Within this setting, empirical
   support for three ideas about legitimacy enhancing decision-making
   arrangements is tested: participatory constitution-making; personal
