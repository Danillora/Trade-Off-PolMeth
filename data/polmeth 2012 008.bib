@article{ WOS:000210529800002,
Author = {Major, Mark},
Title = {Objective but Not Impartial: Human Events, Barry Goldwater, and
   the Development of the ``Liberal Media{''} in the Conservative
   Counter-Sphere},
Journal = {NEW POLITICAL SCIENCE},
Year = {2012},
Volume = {34},
Number = {4},
Pages = {455-468},
Abstract = {This article contends that we have little understanding of why the idea
   of the ``liberal media{''} became an article of faith among
   conservatives. This study looks to the past by examining the
   construction of the ``liberal media{''} in conservative thought to
   understand the present state of right-wing populism. The objective is
   not to determine whether the news media has a liberal bias, rather it is
   to understand the origins and development of this political and
   rhetorical project. It is argued that the liberal media critique was
   developed in the ``conservative counter-sphere,{''} a public sphere for
   right-wing activists and thinkers. Based on a content analysis of the
   conservative publication Human Events from 1957-1965, this study finds
   that the presidential campaign of 1964, and the political actors and
   issues surrounding it, played a critical role in solidifying the
   rightwing critique of the liberal media.},
