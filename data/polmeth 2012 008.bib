@article{ WOS:000421244600012,
Author = {Stein, Aaron and Udum, Sebnem},
Title = {A COMPLICATED DECISION: WHY TURKEY IS NOT LIKELY TO FOLLOW IN IRAN'S
   NUCLEAR FOOTSTEPS},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2012},
Volume = {11},
Number = {2},
Pages = {141-148},
Month = {SUM},
Abstract = {The regional security dilemma stemming from Iran's nuclear program has
   led many to speculate about whether or not Turkey will opt to pursue a
   nuclear weapons capability. Since 2006, Turkey has been named as one of
   the states likely to seek out nuclear weapons if Iran were to decide to
   develop its own nuclear arsenal. These assumptions, however, do not
   account for Ankara's long held and consistent policies on
   nonproliferation and nuclear weapons. This paper explores Turkey's
   approach to nuclear issues and identifies the numerous constraints a
   Turkish leader would face when making the decision to pursue for a
   nuclear weapon.},
