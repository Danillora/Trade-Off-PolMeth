@article{ WOS:000310547300004,
Author = {Batto, Nathan F. and Kim, Henry A.},
Title = {Coordinative Advantages of State Resources under SNTV: The Case of
   Taiwan},
Journal = {JAPANESE JOURNAL OF POLITICAL SCIENCE},
Year = {2012},
Volume = {13},
Number = {3},
Pages = {355-377},
Month = {SEP},
Abstract = {Success in SNTV requires not merely winning but also coordinating votes.
   Governing parties often reap coordination advantage thanks to their
   control of the state and its resources. Since governing parties in
   authoritarian states enjoy greater control over the state and its
   resources, we argue that they should also enjoy magnified coordinative
   advantages in SNTV election. Of course, authoritarian regimes often use
   state resources to win more votes; we argue that, in SNTV, in addition
   to winning more votes, those votes can also be distributed more
   effectively. We demonstrate our claims using election data from both
   local and national SNTV elections in Taiwan from 1954 to 2005.},
Publisher = {CAMBRIDGE UNIV PRESS},
