@article{ WOS:000211588700011,
Author = {Cole, Phillip},
Title = {TAKING MORAL EQUALITY SERIOUSLY: EGALITARIANISM AND IMMIGRATION CONTROLS},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
Year = {2012},
Volume = {8},
Number = {1-2},
Pages = {121-134},
Month = {APR},
Abstract = {In this paper I re-state the egalitarian argument against the morality
   of immigration controls: such limits violate the central ethical
   commitment to moral equality. This means that immigration controls fail
   a fundamental moral test and represent the ethical failure of the
   liberal project of moral equality. I set this re-statement against
   recent arguments about what moral equality means, specifically
   Christopher Heath Wellman's use of Elizabeth Anderson's notion of
   relational equality. Wellman believes that Anderson's ideas seriously
   damage the egalitarian argument, but I argue that this is a misreading
   of her account. I conclude that any liberal attempt to morally justify
   immigration controls must fail through committing the basic logical
   error of `begging the question'.},
Publisher = {SAGE PUBLICATIONS LTD},
