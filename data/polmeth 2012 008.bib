@incollection{ WOS:000308093500009,
Author = {Preston, P. W.},
Book-Author = {Preston, PW},
Title = {England: Available Images, Imagined Futures},
Booktitle = {ENGLAND AFTER THE GREAT RECESSION: TRACKING THE POLITICAL AND CULTURAL
   CONSEQUENCES OF THE CRISIS},
Year = {2012},
Pages = {156-200},
Abstract = {Elites must read and react to enfolding change; elites must respond
   routinely to these demands; elites must plot a route to the future; and
   they must mobilize their population. These requirements point to complex
   practical political processes, international exchanges and domestic
   exchanges. The centre of gravity of elite actions is domestic but the
   international context is always present. Schematically, two elements:
   political policy-making - that is, determining the line of advance in
   economic, social and political matters; and political-cultural
   mobilization/legitimation - that is, explaining and legitimating elite
   actions via creating an official ideology or equivalent. The two
   elements are intermingled: thus an image of the polity-in-the-future
   must be constructed; part pragmatic (the policy-making) and part
   interpretive (ways of explaining and legitimating selected courses of
   action). Various resources are available: elite ideological
   predilections; the inchoate wishes of the masses; plus the demands of
   shifting international contexts. All these can be invoked to shape a
   route to the future. Yet outturns are contingent: for the United Kingdom
   polity the debacle of 2008-10 presents a challenge (as a response is
   required) and an opportunity (crucial relationships are presently in
   flux). And here is a possibility for reform; a chance to re-animate an
   idea of England in the context of a developing Europe.},
Publisher = {PALGRAVE},
