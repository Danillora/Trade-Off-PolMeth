@article{ WOS:000306727600006,
Author = {Barrault, Lorenzo},
Title = {The Resistances of the School Map: The Limits of Political Voluntarism
   in Policy Making},
Journal = {POLITIX},
Year = {2012},
Volume = {25},
Number = {98},
Pages = {109+},
Abstract = {This article deals with the resistances of the school map to political
   reforms, approaching more generally the conditions of change and the
   elected members' limits of voluntarism (national and local) in the lead
   of policies. An analysis of the debates linked to the decentralization
   of 2004 and to the reform of 2007, as well as the study of the concrete
   elaboration of the map in three departments (from ethnographic
   observations, from interviews and from archives) allow to grasp the
   conditions of maintaining of the division into sectors of secondary
   schools. The reform of 2007 constitutes a symbolic policy for which the
   governors have paradoxically no resources of displayed ambitions. Since
   2004, the school map rests with the general councils, and those who
   attempt to change the sectors are limited by technical constraints, by
   the effects of inertia of the cartographic tool, and by the necessity to
   negotiate diverse institutional supports to legitimate their
   intervention. So the resistances of the school map underline the
   limitation of the possibilities of political change in policy making.},
