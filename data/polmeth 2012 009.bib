@article{ WOS:000299888200014,
Author = {Kettell, Steven},
Title = {Has Political Science Ignored Religion?},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2012},
Volume = {45},
Number = {1},
Pages = {93-100},
Month = {JAN},
Abstract = {A common complaint from political scientists involved in the study of
   religion is that religious issues have been largely overlooked by
   political science. Through a content analysis of leading political
   science and sociology journals from 2000 to 2010, this article considers
   the extent of this claim. The results show that political science
   publications involving religious topics have been significantly fewer
   than those engaging with subjects typically regarded as being more
   central to the discipline, and markedly less numerous than religious
   articles in leading sociology publications. Where political science
   publications have engaged with religious issues, these articles have
   also focused on a limited number of subject areas and been concentrated
   in specific disciplinary subfields. The proportion of articles covering
   religion has shown no real increase since the turn of the century. These
   findings underpin calls for political scientists to take religious
   issues more seriously.},
