@article{ WOS:000301461900006,
Author = {Ridout, Travis N. and Fowler, Erika Franklin},
Title = {Explaining Perceptions of Advertising Tone},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2012},
Volume = {65},
Number = {1},
Pages = {62-75},
Month = {MAR},
Abstract = {The authors investigate whether the news media and the tone of actual
   ads aired during a political campaign influence people's perceptions of
   campaign ad tone. Using data on the content of political advertising,
   local television news coverage, and local newspaper coverage in nine
   races in five midwestern states in 2006, the authors discover that
   perceptions of ad tone respond to both exposure to advertising and
   exposure to local news media. Both positive and negative advertising
   drive tone perceptions, and the impact of ad coverage depends not on its
   volume or mentions of tone but on whether that coverage is framed
   strategically or not.},
