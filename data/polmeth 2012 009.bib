@article{ WOS:000309293700005,
Author = {Ziai, Aram},
Title = {Postcolonial Studies and Political Science: Complementary Deficits,
   State of the Art, and Research Perspectives},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2012},
Volume = {53},
Number = {2},
Pages = {291-322},
Abstract = {The article deals with the relationship between postcolonial studies and
   political science. As political science has hitherto largely ignored
   postcolonial inquiries and perspectives, and postcolonial studies on the
   other hand have treated political institutions and processes often in a
   rather superficial manner, the article argues that connecting these
   fields will prove to be productive and insightful. Based on a survey on
   the state of the art of political science informed by postcolonialism, a
   research programme is sketched which operationalises the concepts of
   orientalism and othering, subalternity and representation/articulation,
   hybridity and provincialising Europe.},
