@article{ WOS:000309724400013,
Author = {Maranto, Robert and Woessner, Matthew},
Title = {Diversifying the Academy: How Conservative Academics Can Thrive in
   Liberal Academia},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2012},
Volume = {45},
Number = {3},
Pages = {469-474},
Month = {JUL},
Abstract = {Researchers have long recognized that higher education is dominated by
   professors whose politics are well to the left of the American political
   center. The cause and implications of this ideological imbalance have
   been intensely debated since the 1960s. Although critics of higher
   education, such as David Horowitz, argue that the political imbalance in
   academia is largely the result of ideological discrimination, emerging
   research on the views, values, and experiences of the professoriate
   tells a more complex story. Despite the relatively small numbers in the
   academy, the findings suggest that many conservative scholars can
   succeed in a predominantly liberal environment. Drawing on the latest
   research, as well as their own personal experience, the authors outline
   steps that conservative faculty can take to avoid needless political
   conflict and work happily in a profession largely dominated by the Left.},
