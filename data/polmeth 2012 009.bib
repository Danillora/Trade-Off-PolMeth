@article{ WOS:000310695000007,
Author = {Magleby, David B. and Nelson, Candice},
Title = {Independent Leaners as Policy Partisans: An Examination of Party
   Identification and Policy Views},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
Year = {2012},
Volume = {10},
Number = {3},
Month = {OCT},
Abstract = {Previous research has shown that Independents who lean towards the
   Democratic or Republican parties exhibit voting behavior similar to
   outright partisans. Less attention has been paid to the policy positions
   of Independent leaners. In this article we compare the policy positions
   of Independent leaners, Strong and not very strong Democrats and
   Republicans, and Pure Independents on five policy issues: the 2010
   Affordable Care Act, the 2009 stimulus bill, the war in Iraq, abortion,
   and same sex marriage. On all five issues, Independent leaners are
   policy partisans; their positions are almost identical to the positions
   of outright partisans, and quite different from the views of Pure
   Independents. Analyses which consider Independents as one monolithic
   block are just as erroneous on policy issues as they are on political
   behavior.},
