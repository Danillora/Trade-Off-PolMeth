@article{ WOS:000304761900003,
Author = {Fierke, K. M.},
Title = {The Warden's Dilemma: Self-Sacrifice and Compromise in Asymmetric
   Interactions},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2012},
Volume = {47},
Number = {3, SI},
Pages = {321-341},
Month = {JUL},
Abstract = {Many of the violent conflicts of the post-Cold War period have involved
   peoples who have historically been victims of interstate politics.
   Compromise is highly problematic in contexts of this kind, given that
   sovereign powers tend to attach the label terrorism to acts of
   resistance and the resistance tends to claim an experience of injustice.
   Given a situation where compromise is seen by actors on both sides to be
   impossible, how would anything other than a rotten compromise be
