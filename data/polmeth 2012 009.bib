@article{ WOS:000301180100004,
Author = {Byland, Karin and Varone, Frederic},
Title = {Judicialization of public policies in Switzerland: analysis of
   administrative judicial review at the Federal Supreme Court},
Journal = {SWISS POLITICAL SCIENCE REVIEW},
Year = {2012},
Volume = {18},
Number = {1},
Pages = {78-100},
Month = {MAR},
Abstract = {The judicialization of public policies has not attracted much attention
   from political scientists in Switzerland. This paper aims to fill this
   gap by analyzing the evolution of 79' 752 administrative appeals to the
   Federal Supreme Court from 1990 to 2010. The empirical study shows that
   administrative litigation grows by 45\% over this period, principally in
   welfare policies. However there are significant variations amongst
   policy sectors as well as between the cantons of origin of the appeals.
   These differences can be explained by the influence of economic factors
   (e. g. economic growth), institutional factors (e. g. organization of
   judicial authorities at the cantonal level) and factors related to the
   actors of the judicial system (e. g. density of lawyers per canton). The
   conclusion of this exploratory analysis identifies the next research
   steps to analyze thoroughly the scope of the judicialization process and
   its impacts on public policies.},
Publisher = {WILEY},
