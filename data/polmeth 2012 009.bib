@article{ WOS:000446466200002,
Author = {Scarf, Charlotte},
Title = {Using ICT to Strengthen the Voices of the `Poor' Without Asking Who Will
   Listen},
Journal = {INTERNATIONAL JOURNAL OF E-POLITICS},
Year = {2012},
Volume = {3},
Number = {3},
Pages = {21-39},
Month = {JUL-SEP},
Abstract = {This paper uses a case study of the `Open Knowledge Network' to explore
   the political value of `information and communication technology for
   development' (ICT4D) projects that promote the creation and exchange of
   `local content' in poor communities. These initiatives are
   distinguishable from the vast majority of digital content initiatives
   that aggregate and adapt `global content, `which project implementers
   consider relevant to the needs of target beneficiaries. They are guided
   by the assumption that active participation in the Information Society
   is a crucial component of human development, which is closely tied to
   citizenship and political agency. From this starting point, ICTs are
   seen as political vehicles for strengthening the voices of the poor;
   rather than positioning them as passive recipients of mediated messages
   from above. This paper argues that the political value of these projects
   will be limited if they focus too strongly on generating local content
   without addressing the question of demand.},
