DOI = {10.1111/j.1478-9302.2011.00245.x},
ISSN = {1478-9299},
EISSN = {1478-9302},
Keywords = {democracy; deliberative democracy; Estlund; utopophobia; ideal theory},
Research-Areas = {Government \& Law},
Web-of-Science-Categories  = {Political Science},
Author-Email = {patrick.tomlin@ccc.ox.ac.uk},
Affiliations = {University of Reading},
Cited-References = {Cohen G.A., 2008, Rescuing Justice and Equality, DOI {[}10.4159/9780674262713, DOI 10.4159/9780674262713, DOI 10.4159/9780674029651].
