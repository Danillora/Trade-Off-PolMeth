@article{ WOS:000308705200004,
Author = {Zingher, Joshua N. and Thomas, M. Steen},
Title = {Patterns of Immigrant Political Behaviour in Australia: An Analysis of
   Immigrant Voting in Ethnic Context},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2012},
Volume = {47},
Number = {3},
Pages = {377-397},
Abstract = {Immigration is becoming an increasingly important issue in virtually
   every Western democracy. However, immigrants' participation in politics
   varies greatly from country to country. This article identifies and
   explains the two key determinants of this variation. We establish that
   ethnicity along with traditional socioeconomic factors are the two
   primary forces that determine immigrant political behaviour. We theorise
   that immigrants' ethnic differences from the native population, along
   indicators such as language and residential segregation, increase
   information costs and create barriers to participation in politics as
   well as influencing partisanship. To empirically evaluate our claims, we
   analyse data from the Australian Election Study from 1993 to 2010. The
   results of the analysis in this article provide strong empirical support
   for our hypotheses.},
