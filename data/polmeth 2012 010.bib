@article{ WOS:000299379800004,
Author = {Dilts, Andrew},
Title = {To Kill a Thief: Punishment, Proportionality, and Criminal Subjectivity
   in Locke's Second Treatise},
Journal = {POLITICAL THEORY},
Year = {2012},
Volume = {40},
Number = {1},
Pages = {58-83},
Month = {FEB},
Abstract = {This essay argues that the thief, a liminal figure that haunts the
   boundary of political membership and the border between the law of
   reason and the law of beasts, drives Locke's accounts of the foundation
   of the commonwealth and the right to rebellion in the Second Treatise of
   Government. Locke's political theory is best read through punishment as
   a theory of subject formation, which relies on an unstable concept of
   proportionality to produce this liminal figure in order to secure the
   member as a ``stable{''} political subject.},
