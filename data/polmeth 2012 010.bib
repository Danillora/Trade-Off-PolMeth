@article{ WOS:000304176900003,
Author = {Bates, David},
Title = {`making politics matter': political education in a `knowledge-exchange'
   context},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2012},
Volume = {11},
Number = {2},
Pages = {164-174},
Month = {JUN},
Abstract = {This article seeks through constructive criticism to consider how the
   `knowledge exchange' agenda might be harnessed for the progressive goal
   of political education. The article discusses `Making Politics Matter',
   an initiative that seeks to encourage active learning and teaching of
   politics, facilitate the development of social network capital among
   students, and to reach out to the wider community, with the aim of
