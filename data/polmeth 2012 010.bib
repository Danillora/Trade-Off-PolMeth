@article{ WOS:000317531800005,
Author = {Moller, Jorgen and Muno, Wolfgang and Skaaning, Svend-Erik},
Title = {Transformation of the International System and Autocracy},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2012},
Number = {47},
Pages = {86+},
Abstract = {The association between structural factors and political regime type has
   received much attention in recent decades. However, cross-temporal
   changes in these relationships have remained largely unexplored. In this
   chapter, we examine several propositions about differences in the
   relationship between structural conditions and autocracy between the
   Cold War and post-Cold War periods. First, we propose that due to the
   advent of Western liberal hegemony following the end of the Cold War
   structural factors generally account less for the presence of
   autocracies today than was the case as recently as the 1980s. Moreover,
   this decrease in the explanatory power of structural conditions should
   be an abrupt one, driven by the changes in the structure of the
   international system around 1989-4991, rather than a more gradual one
   continuing into the late 1990s and the early part of this century.
   However, we argue that at a lower level of generality, Western liberal
   hegemony is likely to have had an asymmetrical impact on the structural
   conditions of autocracy. More particularly, while it is expected to have
   diminished the effect of neighbor diffusion, modernization, ethnic
   fractionalization, and British colonial heritage, we expect that the
   impact of large-scale oil extraction and Islam have largely been
   resistant to changes in the international environment. A quantitative
   analysis of 167 countries from 1973 to 2007 lends empirical backing to
   both the general proposition about the decreasing impact of structural
   factors and the more particular qualification about the nexus between
   structural conditions and international dynamics.},
