@article{ WOS:000422109000002,
Author = {Rose, Deidre},
Title = {AIDS Rumours, Vulnerability, and the Banana Wars: A View From Dominica},
Journal = {NEW PROPOSALS-JOURNAL OF MARXISM AND INTERDISCIPLINARY INQUIRY},
Year = {2012},
Volume = {5},
Number = {2},
Pages = {8-19},
Month = {MAY},
Abstract = {This article examines rumours about HIV/AIDS in relation to the recent
   shift in Dominica's economic status from an agricultural producer to a
   service provider. This shift has been the direct result of neo-liberal
   economic policies and decisions made by the international economic
   community. The virtual destruction of Dominica's agricultural sector has
   forced the country to develop its tourist sector. The increased reliance
   on tourism and emigration as sources of income has led to anxieties
   about contact and contagion; specifically, people have become acutely
   aware of the social and health consequences that would result from this
