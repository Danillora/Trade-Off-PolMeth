@article{ WOS:000218114300003,
Author = {Skradol, Natalia},
Title = {Funny enemies: the `humorous performative' in the Stalinist master
   narrative},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2012},
Volume = {17},
Number = {3},
Pages = {281-299},
Abstract = {The article introduces the category of `humorous performativity' as a
   characteristic feature of Stalinist public, judicial and political
   discourse. Taking the lead from Judith Butler's analysis of discursive
   performativity, the article suggests ways in which the application of
   the notion of `humorous performativity' can deepen our understanding of
   the mechanics of Stalinist terror. At the same time, these reflections
   are an attempt to draw the attention of scholars to the importance of
   humour as a performative force in the structuring of an ideology-a topic
   that remains virtually unexplored.},
