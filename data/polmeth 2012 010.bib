@article{ WOS:000218770400002,
Author = {Cooper, Laurence D.},
Title = {Every Man a Socrates? Tocqueville and the Conceit of Modernity},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2012},
Volume = {1},
Number = {2},
Pages = {208-235},
Month = {FAL},
Abstract = {Tocqueville provides a compelling portrait of the American and indeed
   the democratic mind that still rings true in many respects. The
   principal features of this portrait constitute a coherent if problematic
   creed, which I call popular Socratism. This creed embraces intellectual
   independence, the vindication of self-interest, and a commitment to
   knowing what is useful to know. Such principles should add up to
   intellectual rigor and vigor and above all to freedom of mind, as they
   did in Socrates. Yet somehow they do not. Why not? The answer is
   complex, but its core can be simply (if strangely) expressed: unlike
   original Socratism, popular Socratism entails the forgetting of
   eternity. What if anything can be done? Tocqueville looks to religion
   and classical learning to counter the worst failings of popular
   Socratism. I follow him in this but conclude by considering what he does
