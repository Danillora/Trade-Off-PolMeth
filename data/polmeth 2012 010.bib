@article{ WOS:000209060400007,
Author = {Neef, Christian},
Title = {Platitudes for Tula A Misdiagnosis of Reporting on Russia},
Journal = {OSTEUROPA},
Year = {2012},
Volume = {62},
Number = {1},
Pages = {101+},
Month = {JAN},
Abstract = {M. Gathmann and S. Scholl recently criticized the media coverage of
   Russia. Journalists, they wrote, are fixated on Moscow and pursue
   ``old-style Kremlin astrology{''}. Those who wish to provide adequate
   reporting on Russia should head out to the provinces, it was said. Such
   allegations are unfounded, such claims politically naive. A subjective
   lack of ability on the part of colleagues is not the problem. It is,
   rather, the near total absence of transparency in Russian political life
