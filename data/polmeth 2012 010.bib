@article{ WOS:000217316100003,
Author = {Belanger, Eric and Lewis-Beck, Michael S. and Nadeau, Richard},
Title = {Choosing the dependent variable: Sarkozy's forecasting lesson},
Journal = {FRENCH POLITICS},
Year = {2012},
Volume = {10},
Number = {4},
Pages = {364-368},
Month = {DEC},
Abstract = {This article revisits our forecast of the 2012 French presidential
   election. Although our forecast was generally accurate in seeing the
   return of the left to the presidency, its point estimate error proved to
   be large (9.2 points). This article suggests that this forecast error
   may come from a wrong choice of dependent variable. New empirical tests
   of our French `proxy model' of forecasting indicate that the dependent
   variable to use should continue to be total left vote share, but
   measured on the second ballot rather than the first.},
Publisher = {PALGRAVE MACMILLAN LTD},
