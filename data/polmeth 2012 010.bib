@article{ WOS:000310926900003,
Author = {Lievens, Matthias},
Title = {Ideology critique and the political: Towards a Schmittian perspective on
   ideology},
Journal = {CONTEMPORARY POLITICAL THEORY},
Year = {2012},
Volume = {11},
Number = {4},
Pages = {381-396},
Month = {NOV},
Abstract = {The notion of ideology and its critique have taken a remarkable
   about-turn in recent decades. While in classical Marxism, ideology used
   to be understood in terms of a distorted representation of real social
   divisions, recent authors such as Claude Lefort and Ernesto Laclau have
   argued that there is no standpoint outside language or representation,
   and they consider those representations as ideological that remain blind
   to their own political effects. However, a dimension that was crucial in
   the classical Marxist tradition has been downplayed in their work,
   namely the importance of ideology critique for the process of
   emancipation or of a particular subject becoming political. To
   reintroduce this dimension in the recent debate on ideology, this
   article takes recourse to a selective and creative reading of Carl
   Schmitt. Schmitt, it is argued, understands ideology critique as a
   crucial precondition for the process through which a subject becomes
   political. From a Schmittian perspective, ideology critique aims at
   unmasking depoliticisation and thus at making visible an intersubjective
   space of conflict, which is the inevitable condition for every
   democratic or emancipatory struggle. Contemporary Political Theory
   (2012) 11, 381-396. doi:10.1057/cpt.2011.36 published online 29 January
   2012},
