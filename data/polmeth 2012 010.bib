@article{ WOS:000307609600002,
Author = {Dominguez, Roberto and Royo, Sebastian},
Title = {the study of the european integration process in the united states},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2012},
Volume = {11},
Number = {3},
Pages = {285-297},
Month = {SEP},
Abstract = {This article explores the institutional development of European studies
   in the United States. We show the extent to which the study of European
   integration reflects the deep roots that sustain the vitality and
   strength of the transatlantic relationship since the end of World War
   II. Outside Europe, the United States is still the country where studies
   on European integration have the most prominent place in the research
