@article{ WOS:000215510700003,
Author = {Sanz Hernandez, Alexia},
Title = {Cultural policy in Aragon, institutional asymmetrical multipolarization
   and structural atomization},
Journal = {REVISTA DE INVESTIGACIONES POLITICAS Y SOCIOLOGICAS},
Year = {2012},
Volume = {11},
Number = {3},
Pages = {39-64},
Abstract = {Cultural action of public policy in Aragon, discursively inspired in
   prevailing approaches in the rest of Spain, has some territorial
   peculiarities but also emphasizing that institutionalization and
   complexity characteristic of Spanish cultural policies of the past
   thirty years.
   This distinctive materialization especially sits in interactional modes
   among different actors (public, private or voluntary sector) whose most
   notable feature is the tendency to the multipolarization and structural
   asymmetries of cultural institutions. Functional differentiation is the
   answer to the complexity, resulted in creation of networks, systems or
   structures that nevertheless have not prevented the excessive
   fragmentation of the cultural field.},
