@article{ WOS:000215057800018,
Author = {Talaat, Wan Izatul Asma Wan and Tahir, Norhayati Mohd and Husain, Mohd
   Lokman},
Title = {The Existing Legislative, Administrative and Policy Framework for the
   Mangrove Biodiversity Management \& Conservation in Malaysia},
Journal = {JOURNAL OF POLITICS AND LAW},
Year = {2012},
Volume = {5},
Number = {1},
Pages = {180-188},
Month = {MAR},
Abstract = {The pertinent roles of mangroves have been clearly recognised
   particularly after the 2004 tsunami. Lots of interests have been created
   on the importance of sustainable management of mangrove biodiversity,
   which plays an important role to the environment as well as in the
   socio-economic growth in coastal zones. As the world's fifth largest,
   Malaysian mangroves are facing threats from anthropogenic activities
   such as deforestation, aquaculture, pollution run off and land
   development. A signatory to both the Convention of Biological Diversity
   and UNCLOS, Malaysia has to develop national strategies, plans and
   programmes by taking legislative, administrative and policy ( LAP)
   measures for the conservation and sustainable use of mangrove
   biodiversity outlined by these two conventions. Sustainable management
   of mangrove biodiversity requires proper and effective LAP framework
   including clear allotment of jurisdictional boundaries between the
   various departments involved. The existing LAP framework in the
   management of mangrove biodiversity was examined where three problems
   were found to hinder the sustainable management of mangrove biodiversity
   in Malaysia namely the unclear policies, segmented laws and overlapping
   administrative jurisdictions. These problems collectively contribute to
   the insufficiency of the existing LAP framework to provide for the
   sustainable management of mangrove biodiversity in Malaysia.},
