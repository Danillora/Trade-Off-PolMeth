@article{ WOS:000307174300002,
Author = {Roy, Jason and Singh, Shane},
Title = {Canadian and American Voting Strategies: Does Institutional
   Socialization Matter?},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2012},
Volume = {45},
Number = {2},
Pages = {289-312},
Month = {JUN},
Abstract = {This paper uses data from an online voting experiment to examine the
   impact of institutional socialization on the vote decision process. More
   specifically, we examine how Canadian and American voters differ in
   their vote decision processes in two- and four-party elections. Our
   expectation is that Canadian voters, who are more familiar with
   multiparty electoral context, will adjust to the increased complexity of
   the four-party competition by engaging in a more detailed decision
   process. Alternatively, we expect US voters, who are less familiar with
   multiparty competitions, will not undertake such an adjustment, perhaps
   even engaging in a less detailed vote calculus under more complex
   conditions. Results lend support to our expectations, offering insight
   into how institutional design and socialization can affect voter
   decision processes.},
