@article{ WOS:000420544400009,
Author = {Traunmueller, Richard},
Title = {Measuring church-state-relations: a comparative analysis of new indices},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
