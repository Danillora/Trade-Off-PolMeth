@article{ WOS:000322967700007,
Author = {Mayer, William G.},
Title = {How the Romney Campaign Blew it},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
Year = {2012},
Volume = {10},
Number = {4},
Pages = {40-50},
Month = {DEC},
Abstract = {Political scientists have generally been skeptical that presidential
   campaigns have a significant effect on the final outcome of a
   presidential election. But 2012, this article argues, was an exception.
   By all the indicators that political scientists traditionally look to,
   2012 had long shaped up to be a very close election. That Mitt Romney
   lost was due largely to the remarkably inept quality of his campaign,
   which largely avoided any serious discussion of the issues, even when
   such discussion would have worked to his advantage.},
