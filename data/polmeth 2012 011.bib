@article{ WOS:000421244600002,
Author = {Albert, II, Prince},
Title = {THE MEDITERRANEAN: A SYMBOL OF THE WOUNDS AND THE HOPES OF OUR EARTH},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2012},
Volume = {11},
Number = {2},
Pages = {17-28},
Month = {SUM},
Abstract = {The Mediterranean has always been at the heart of major upheavals of
   history because of its geographical position at the crossroads of
   Europe, Africa, and Asia. At the present time, it epitomizes most of the
   dangers affecting our planet. In the Mediterranean, every landscape
