@article{ WOS:000309932300013,
Author = {Baccini, Leonardo and Urpelainen, Johannes},
Title = {Strategic Side Payments: Preferential Trading Agreements, Economic
   Reform, and Foreign Aid},
Journal = {JOURNAL OF POLITICS},
Year = {2012},
Volume = {74},
Number = {4},
Pages = {932-949},
Month = {OCT},
Abstract = {We propose that major powers give foreign aid to developing countries to
   facilitate politically costly economic reforms that preferential trading
   agreements prescribe. Democratic developing countries (1) need
   adjustment assistance more than autocracies and (2) can credibly commit
   to using fungible revenue to compensate the domestic losers, so a side
   payment for deeper reforms should only be available for democracies. A
   quantitative test lends support to the theory. Fully democratic
   developing countries that form a preferential trading agreement with the
   European Union or the United States obtain a large increase in foreign
   aid in the short run. These results imply that donors have used foreign
   aid to strengthen the effect of preferential trading agreements on
   economic reforms.},
