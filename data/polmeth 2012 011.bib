@article{ WOS:000217072900004,
Author = {Boer, Roland},
Title = {TOWARDS AN AKAIROLOGICAL POLITICS: REREADING NEGRI ON THE BIBLICAL BOOK
   OF JOB},
Journal = {SOCIALIST STUDIES},
Year = {2012},
Volume = {8},
Number = {2},
Pages = {42-59},
Month = {FAL},
Abstract = {This essay engages with an unfamiliar Antonio Negri, one who engages in
   biblical interpretation in The Labor of Job (2009). The analysis focuses
   on two key themes: kairos and measure/immeasure. Concerning kairos I
   critique Negri's relatively conventional approach - creative and
   opportune time - by identifying its inescapable moral and class
   associations with ruling ideology in ancient Greece, where it
   designates, through its basic sense of measure, the right time and right
   place. In response, I pursue an akairological position, one that draws
   upon Negri's complex treatment of measure and immeasure. While Negri
   seeks a reshaped and creative measure, I suggest we tarry with
   immeasure, for it overlaps with what is opposed to kairos. The article
   closes by asking why Negri should be interested in the Bible. The
   answer: he is able to do so, as his studies of Spinoza show, through a
   radical relativising of the absolute truth claims of theology.},
