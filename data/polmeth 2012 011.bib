@article{ WOS:000307270300007,
Author = {Stadelmann-Steffen, Isabelle and Vatter, Adrian},
Title = {Does Satisfaction with Democracy Really Increase Happiness? Direct
   Democracy and Individual Satisfaction in Switzerland},
Journal = {POLITICAL BEHAVIOR},
Year = {2012},
Volume = {34},
Number = {3},
Pages = {535-559},
Month = {SEP},
Abstract = {This paper takes the influential ``direct democracy makes people
   happy{''}-research as a starting point and asks whether direct democracy
   impacts individual satisfaction. Unlike former studies we distinguish
   two aspects of individual satisfaction, namely satisfaction with life
   ({''}happiness{''}) and with how democracy works. Based on multilevel
   analysis of the 26 Swiss cantons we show that the theoretical assumption
   on which the happiness hypothesis is based has to be questioned, as
   there is very little evidence for a robust relationship between
   satisfaction with democracy and life satisfaction. Furthermore, we do
   not find a substantive positive effect of direct democracy on happiness.
