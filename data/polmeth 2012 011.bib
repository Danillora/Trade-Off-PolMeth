@article{ WOS:000442719000003,
Author = {De Vreede, E.},
Title = {Culture, Civilization and Identity},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2012},
Number = {5},
Pages = {17+},
Abstract = {In the 1st part of the article the author realizes semantic
   differentiation of these sociological categories. To relate the gist in
   brief, culture is interpreted as the totality of behavioral practices
   being acquired and changing within a certain group - practices providing
   communication within it and its contraposition to the surrounding world.
   Civilization is that very culture seen by the group through the
   rose-colored spectacles, in an ideal presentation. Identity is the
   internalization of an individual's civilization. The 2nd part of the
   article presents in a generalized manner a typical modern West European
   plural society, i.e. composed of groups of different cultural origin.
   The final part stresses the meaning of Intercultural Dialogue as the
   main solution to problems originating in cultural differences.},
