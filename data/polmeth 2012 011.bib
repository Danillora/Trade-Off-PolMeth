@article{ WOS:000420656000006,
Author = {Lobo, D. Erick and Osorio, D. Efren},
Title = {QUALITY OF DEMOCRACY: THE VENEZUELAN CASE},
Journal = {REVISTA ANDINA DE ESTUDIOS POLITICOS},
Year = {2012},
Volume = {2},
Number = {1},
Pages = {153-165},
Abstract = {The United Nations Program for Development, in the Human Development
   Report 2002, notes that many governments democratically elected tend to
   hold their authority by non-democratic methods, for example, by
   modifying the national Constitution in their favor or intervening in
   electoral processes and subtracting independence to the Legislative or
   Judiciary. Many of these facts have occurred repeatedly in Venezuela.
   Therefore, the purpose of this paper is to define, empirically, the
   quality of democracy in Venezuela, trying to identify the conditions for
   the existence of a clear political regime, the existence of substantive
   access to the State, the rule of law and certain form of organization of
   Society.},
Publisher = {UNIV NACL MAYOR SAN MARCOS, INST ESTUDIOS POLITICOS ANDINOS},
Address = {UNIV NACL MAYOR SAN MARCOS, INST ESTUDIOS POLITICOS ANDINOS, LIMA,
   00000, PERU},
Type = {Article},
