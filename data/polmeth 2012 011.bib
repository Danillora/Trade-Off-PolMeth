@article{ WOS:000420778300004,
Author = {Valle, Roberto},
Title = {Cults of Personalities and Scenarios of Power. Theories and techniques
   of dictatorship in Russia},
Journal = {STORIA DEL PENSIERO POLITICO},
Year = {2012},
Volume = {1},
Number = {1},
Pages = {71-104},
Month = {JAN-APR},
Abstract = {The catastrophic development of Russian history seems to have gone
   through the ``authoritarian way{''}. The idea of ``authoritarian way{''}
   of development of Russia seems to derive by the propensity of Russian
   political culture toward autocracy. In retracing the long journey of the
   authoritarian way in Russia, the metamorphosis and the pseudomorphosis
   of dictatorship could be placed in three scenarios of power that have
   taken place in Russian history: the one dominated by the figure of the
   Car' as anointed dictator; the framework of Soviet power based on the
   ``dictatorship of the proletariat{''} and the ``cult of personality{''};
   the post-Soviet landscape characterized by the ``vertical of power{''}
   and the ``dictatorship of law{''}.},
