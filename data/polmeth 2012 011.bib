@article{ WOS:000305837800004,
Author = {Botting, Eileen Hunt and Kronewitter, Sean},
Title = {Westernization and Women's Rights: Non-Western European Responses to
   Mill's Subjection of Women, 1869-1908},
Journal = {POLITICAL THEORY},
Year = {2012},
Volume = {40},
Number = {4},
Pages = {466-496},
Month = {AUG},
Abstract = {The publication in 1869 of Mill's Subjection of Women gave rise to
   philosophical and political responses beyond Western Europe on the
   relationship between Westernization and women's rights in developing,
   colonial, and post-colonial countries. Through the first comparative
   study of the Subjection of Women alongside the forewords to six of its
   earliest non-Western European editions, we explore how this book
   provoked local intellectuals in Russia, Chile, and India to engage its
   liberal utilitarian, imperial, Orientalist, and feminist ideas. By
   showing how Mill's Western European biases and instrumental reasoning
   establish problematic rhetorical models for women's rights arguments, we
   are able to explore the ethical dimensions of women's rights issues in
   the context of cultural and political imperialism. Most importantly,
   this reception history illustrates how cross-cultural and culturally
   sensitive dialogue on women's rights can push us beyond Western bias and
   imperialism in advocating for the end of women's subjection around the
   globe.},
