@article{ WOS:000309548500006,
Author = {Lindeboom, Gert-Jan},
Title = {Public priorities in government's hands: Corresponding policy agendas in
   the Netherlands?},
Journal = {ACTA POLITICA},
Year = {2012},
Volume = {47},
Number = {4},
Pages = {443-467},
Month = {OCT},
Abstract = {An overwhelming body of literature concentrates on whether governments
   and representatives share congruent opinions with the general public.
   Though these are important questions of representation, this article
   proposes not to consider actual policy positions, but study whether
   governments share similar issue priorities with citizens. I argue that
   this is an important feature of the link between government and citizens
   in a representative democracy and should in fact precede the study of
   policy positions. This study investigates the correspondence of issue
   priorities in the Netherlands between 1971 and 2008. Despite recent
   concerns about the manifestation of a gap between citizens and political
   elites, it seems that issue priorities of the government move hand in
   hand in public priorities. On the other hand, new political lines of
   conflict give some cause for concern. Acta Politica (2012) 47, 443-467.
   doi:10.1057/ap.2012.14; published online 22 June 2012},
