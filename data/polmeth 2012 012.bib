@article{ WOS:000449589500004,
Author = {Herrera, Carlos Miguel},
Title = {Some remarks on the notion of social counter-powers},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2012},
Volume = {105},
Pages = {81-99},
Month = {JUL-DEC},
Abstract = {The present article explain the idea of social counter-powers in
   Constitutional Law, particularly in the light of social protest
   movements that have become widespread in the past two years in several
   countries, under the name of ``indignados{''} (outraged). The notion of
   social counter-powers appears as specific and distinct one, principally
   from other like-minded as the ``right to resistance{''} or ``civil
   disobedience{''}, but also of the traditional ``check and balance{''}.
   Above all, this notion of social counter-powers opens new perspectives
   for building a new Constitutional Law.},
