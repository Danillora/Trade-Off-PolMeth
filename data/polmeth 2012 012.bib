@article{ WOS:000308980300002,
Author = {Wagner, Corina and Konzelmann, Laura and Rattinger, Hans},
Title = {Is Germany Going Bananas? Life Cycle and Cohort Effects on Party
   Performance in Germany from 1953 to 2049},
Journal = {GERMAN POLITICS},
Year = {2012},
Volume = {21},
Number = {3},
Pages = {274-295},
Abstract = {In the public debate concerning the consequences of demographic change
   in Germany it is often argued that the rising share of older voters will
   help the CDU/CSU to consolidate its power. This argumentation only
   applies if the age effect is assumed to be dominant. However, diverse
   socialisation backgrounds, captured by the cohort effect, also have to
   be taken into account. Based on the Reprasentative Wahlstatistik
   (Representative Electoral Statistic, RES) and Population Forecasts, the
   consequences of demographic shifts for federal elections since 1953 are
   estimated, as well as for future elections. First, age, cohort and
   period effects on vote choice for previous elections are calculated by
   using cohort analysis. Second, these effects are applied to the future
   age distribution. The results show that the `greying' electorate is
   expected to favour the CDU/CSU and the FDP, while especially the SPD is
   likely to suffer electoral losses due to demographic shifts.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
