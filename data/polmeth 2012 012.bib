@article{ WOS:000306727600004,
Author = {Aebischer, Sylvie},
Title = {Reinventing School and Administration: A Pedagogical and Managerial Law
   Analyzed Through its Producers},
Journal = {POLITIX},
Year = {2012},
Volume = {25},
Number = {98},
Pages = {57+},
Abstract = {A managerial discourse in French educational policies became established
   since the 1980s. The ``Jospin{''} law for education of 1989 is a key
   moment of this evolution, even if it is mostly seen as a pedagogical
   text. This paper studies the reform of 1989 through a scene whish is
   rarely analyzed in political science, the drawing up of the law inside
   the cabinet and among the minister's advisers. The managerial accent of
   this pedagogical text can be explained by its producers and their way of
   working. Analyzing the career of the minister's advisers, this paper
   shows the links between their social origins, professional
   socialization, their relation to the school system and their attachment
   to the ``state reform{''}.},
