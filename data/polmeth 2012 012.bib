@article{ WOS:000218532300004,
Author = {Homan, Kees},
Title = {CEBRI-Clingendael Conference in Brasilia on 29 May 2012, challenges in
   implementing the Responsibility to Protect: Undertaking coercive and
   non-coercive action},
Journal = {SECURITY AND HUMAN RIGHTS},
Year = {2012},
Volume = {23},
Number = {2},
Pages = {105+},
Abstract = {The UN members recognized for the first time in 2005 in the Final
   Declaration of the UN World Summit, the responsibility of a state to
   protect its own people and, if it fails to do so, for the international
   community to act. UN Security Council Resolution 1973, adopted on 17
   March 2011, was the first fully application of the responsibility to
   protect, including military measures. However, the military mission
   triggered more and more criticism, as mission creep became manifest.
   As a reaction to this mission creep Brazil presented on 9 November 2011
   a paper with a new set of principles and procedures on the theme of
   `responsibility while protecting'. Its two proposals are firstly a set
   of criteria (including last resort, proportionality, and balance of
   consequences) to be taken into account before the Security Council
   mandates any use of military force. Secondly Brazil proposes a
   monitoring-and-review mechanism to ensure that such mandates'
   implementation is seriously debated. However, most importantly is that
   the nature of sovereignty itself has changed: legitimate governments are
   defined not only by their control of a territory and a population but
   also by how they exercise that control. If they fail in that obligation,
   the international community has the responsibility to protect those
   citizens.},
