@article{ WOS:000303448800015,
Author = {Owens, Michael Leo and Yuen, Amy},
Title = {The Distributive Politics of ``Compassion in Action{''}: Federal
   Funding, Faith-Based Organizations, and Electoral Advantage},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2012},
Volume = {65},
Number = {2},
Pages = {422-442},
Month = {JUN},
Abstract = {Incorporating race into tactical spending for electoral gain, this
   article revisits the relative effects of vote production and vote
   retention on distributive politics. It investigates whether a
   ``compassion strategy{''} to influence the electoral behavior of voters
   while being responsive to need-based social welfare demands affected
   federal discretionary grants to faith-based organizations (FBOs) during
   the administration of George W. Bush. The findings suggest that federal
   domestic social welfare funding of FBOs may have involved a combination
   of the tactical use of grants for both electoral purposes (i.e., vote
   production and vote retention) and the reduction of need among the
   states.},
