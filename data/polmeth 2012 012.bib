@article{ WOS:000210245200009,
Author = {Alberto Baquero-Hernandez, Ricardo},
Title = {Characterizing E-Government in China},
Journal = {DESAFIOS},
Year = {2012},
Volume = {24},
Number = {2},
Pages = {233-257},
Month = {JUL-DEC},
Abstract = {Although E-Government (E-G) is one public policy in China, it still has
   a long way to go in terms of diffusion across the country and
   interactive participation from citizens. It constitutes a double sided
   sword since it enhances the features that China needs to show to the
   world in international competitiveness and status, but also poses
   challenges to the Communist rule inasmuch as it has to be controlled.
   Therefore, E-G is both a political and a managerial issue: it is
   sensitive to the Chinese Communist Party but also leads to economical
   and administrative prosperity and efficiency. Some of the keys would be
   the development of the middle class, further penetration of the
   Internet, and more education about virtual tools in government issues
   and services.},
