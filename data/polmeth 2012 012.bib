@article{ WOS:000302133300002,
Author = {Garry, John},
Title = {Consociationalism, regional integration and vote choice: Northern
   Ireland and the 2009 European Parliament election - A research note},
Journal = {ACTA POLITICA},
Year = {2012},
Volume = {47},
Number = {2},
Pages = {113-127},
Month = {APR},
Abstract = {Consociational institutional arrangements in deeply divided societies
   are often criticised for cementing the underlying conflict cleavage,
   encouraging the continued dominance of conflict-based party competition
   and voter behaviour and prohibiting the emergence of `normal' (that is,
   non-conflict-based) dimensions of political competition. However,
   drawing on evidence from a post-election survey at the 2009 Northern
   Ireland election to the European Parliament, I find that EU issues
   determined intra-bloc vote choice (at least in the nationalist
   community). This suggests that there is potential for regional
   integration projects, such as the EU, to contribute to the
   `normalisation' of politics in a consociational system by acting as the
   source of an externally generated dimension of political competition.
   Acta Politica (2012) 47, 113-127. doi:10.1057/ap.2011.26; published
   online 21 October 2011},
