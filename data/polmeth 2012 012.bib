@article{ WOS:000317531800008,
Author = {Backes, Uwe},
Title = {Four Fundamental Types of Autocracy and their Legitimation Strategies},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2012},
Number = {47},
Pages = {157+},
Abstract = {Uwe Backes, Four Fundamental Types of Autocracy and their Legitimation
   Strategies, pp. 157-175. Autocracies can be subdivided into
   authoritarian and totalitarian regimes; following Juan J. Linz, this is
   how it is represented in most standard works and textbooks of political
   science. Increasingly, and for good reasons, this abridged
   differentiation is criticized and, in light of the loss of importance of
   totalitarian regimes on the global scale as well as the diversity of
   ``authoritarianisms{''}, hardly any longer appears to be of importance
   to research. Hence, other typologies take their place in growing
   measure. This contribution pleads for a return to Juan J. Linz, a new,
   unabridged systematization of his types, and, thusly, the
   differentiation of fundamental types of autocracy on the basis of power
   legitimation, not merely for present-day analysis but also suitable for
   sweeping historic considerations. After developing the typology, the
   attempt is made to explain its relevance based on the discussion of
   characteristic legitimation strategies.},
