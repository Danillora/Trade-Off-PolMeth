@article{ WOS:000421676700004,
Author = {Moreno Torres, Aurora},
Title = {Democratic security and militarization in Colombia: beyond the armed
   conflict},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
Year = {2012},
Number = {12},
Pages = {41-56},
Month = {DEC},
Abstract = {This study analyzes the previous decade's militarization process in
   Colombia, where a policy of ``security{''} exacerbated violence,
   displacement, and degradation of human rights- a process justified by
   the need to defeat insurgencies in military confrontations. The
   amplified use of force, however, did not offer greater safety, nor has
   it led to the end of insurgencies; instead, it has accentuated different
   types of violence and normalized human rights violations, up to and
   including the loss of civil and political liberties. The article lays
   out key features that were developed under the aegis of state security
   policy.},
