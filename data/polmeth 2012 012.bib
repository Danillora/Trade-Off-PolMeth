@article{ WOS:000309294000003,
Author = {Debus, Marc},
Title = {The Impact of Structural and Attitudinal Factors on Voting Behavior at
   German Federal Elections over Time: West Germany, 1976-2009},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2012},
Number = {45},
Pages = {40+},
Abstract = {Marc Debus, The Impact of Structural and Attitudinal Factors on Voting
   Behavior at German Federal Elections over Time: West Germany, 1976-2009,
   pp. 40-62.
   This article tests two alternative hypotheses: the individualization
   hypothesis of political behavior which states that the impact of
   long-term factors like the belonging to a social group on voting
   behavior is declining while attitudinal factors are becoming more
   relevant for the decision-making of voters over time, and the hypothesis
   that importance of predictors varies according to the thematic emphases
   of election campaigns. The analysis reveals that belongingness as well
   as ties to specific social groups are still of importance for voting
   behavior in Germany, while there are at best scarce indications of an
   increasing importance of attitudes for vote choices. Moreover, there is
   no evidence that the thematic profiles of election campaigns are
   relevant for the varying impact of attitudinal predictors of voting
   behavior.},
