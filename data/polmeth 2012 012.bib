@article{ WOS:000420546700005,
Author = {Westle, Bettina},
Title = {``Post-democracies?{''} - Perceptions of political parties' position on
   the ideological left-right scale: Great Britain, France and Germany},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2012},
Volume = {6},
Number = {2},
Pages = {255-301},
Month = {DEC},
Abstract = {One of the main contentious issues in the debate on post-democracy is
   the claim that the major political parties have converged ideologically
   up to a point where they are indistinguishable due to the fact that the
   left parties have moved to the right. This supposedly fosters negative
   consequences such as political dissatisfaction, decreasing voter turnout
   and an increasing interest in right-wing populist parties. On the basis
   of the European Elections Studies from 1989 to 2009, the article shows
   that only the citizens in Great Britain (especially the partisans of the
   Labour party) perceive a considerable convergence of the political
   parties' positions. However, the convergence is also due to a movement
   toward the left by the Conservative party. The majority of citizens in
   Germany, France and Great Britain is in ideological accord with
   ``their{''} major parties even when they perceive only a small
   difference to the rivalling party. When parties are considered to have
   no clear profile, this does not systematically contribute to
   dissatisfaction with democracy. However, it discourages (albeit to a
   small extent) participation in elections and encourages a tendency
   toward extreme parties on the right as well as the left wing of the
   ideological spectrum.},
