@article{ WOS:000311712700002,
Author = {Landman, Todd},
Title = {Assessing the quality of democracy: the international IDEA framework},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2012},
Volume = {11},
Number = {4},
Pages = {456-468},
Month = {DEC},
Abstract = {This articles sets out the main principles, mediating values, and
   pillars of the democracy assessment framework developed by the
   International Institute for Democracy and Electoral Assistance
   (International IDEA). It discusses the tradition of democracy
