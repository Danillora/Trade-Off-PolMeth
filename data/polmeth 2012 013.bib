@article{ WOS:000446175900001,
Author = {Tumova, Jana},
Title = {THE WAYS OF AMERICAN UNILATERALISM: CORE AND DEVELOPMENT OF THE CONCEPT},
Journal = {POLITICKE VEDY},
Year = {2012},
Volume = {15},
Number = {2},
Pages = {8-35},
Abstract = {The study explores the roots and the specifics of the American
   unilateralism. It also focuses on the essence of the American
   unilateralism during the Cold War and indicates the changes that the
   system has undergone in the post-Cold War era. By analyzing the
   development of the concept it proves that the hints of American
   unilateralism can to be observed since the 18th Century. The concrete
   observations of the American unilateralism were carried out in the case
   study with the focus on the US voting patterns in the UN Security
   Council from 1945 with the simultaneous applications of the public goods
   theory. This case study proves that the American unilateralism is the
   present pattern of the American international politics making for (at
   least) more than 50 years.},
