@article{ WOS:000309294000014,
Author = {Thurner, Paul W. and Mauerer, Ingrid and Binder, Martin},
Title = {Party-Specific Issue Voting at the 2002, 2005 and 2009 German Federal
   Elections},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2012},
Number = {45},
Pages = {302+},
Abstract = {Paul W Thurner / Ingrid Mauerer / Martin Binder, Party-Specific Issue
   Voting at the 2002, 2005 and 2009 German Federal Elections, pp. 302-320.
   Do voters take into account position issues, i.e. issues where parties,
   with varying degrees of emphasis and ambivalence, offer different
   positions? This article contributes to the understanding of issue voting
   by highlighting the so far neglected question whether such issue voting
   effects vary over the parties under consideration. We provide new
   hypotheses on the conditionality of party-specific issue voting shedding
   new light on recent discussions. Taking advantage of the flexibility of
   conditional logit models, we test these hypotheses for the German
   federal elections of 2002, 2005 and 2009. We expect a relatively higher
   issue reactivity for polar parties, for small parties and for opposition
   parties. According to our estimations, the null hypothesis of identical
   issue reactions with regard to all parties has to be clearly refuted.
   The main result is that especially polar parties induce issue voting in
   the sense of the spatial theory of voting.},
