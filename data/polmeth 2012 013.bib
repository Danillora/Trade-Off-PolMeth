@article{ WOS:000310712500005,
Author = {Wiss, Tobias},
Title = {Pension Privatization in Bismarckian Countries: The Role of Social
   Partners as Veto Players},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2012},
Volume = {53},
Number = {3},
Pages = {467-492},
Abstract = {The article analyses the role of social partners within the process of
   changing pension systems in four countries: Germany, Austria, Belgium,
   and Italy. So far the research has focused on partisan actors and public
   pensions, whereas this contribution extends the analysis to non-state
   actors (social partners) and to non-state social policy (occupational
   pensions). Only the extension and improvement of the theories concerning
   veto players and veto points allow for the inclusion of the
   political-parliamentary arena and the collective arena in order to
   capture the overall power and role of social partners. The main result
   is, that far-reaching pension reforms in Bismarckian countries with
   multiple veto possibilities can be observed if governments win social
   partners over with package deals and if social partners can expect new
   future (veto) power.},
