@article{ WOS:000308705200007,
Author = {Pringle, Helen},
Title = {Compulsory Voting in Australia: What is Compulsory?},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2012},
Volume = {47},
Number = {3},
Pages = {427-440},
Abstract = {Many academic and popular writers have argued that `compulsory voting'
   is an illusion in Australia because it is not compulsory to fill out the
   ballot-paper. This is the orthodox assumption. I argue that this view is
   mistaken, and that the duty to vote clearly includes the marking of the
   paper. Whether and how the requirement is able to be enforced is a
   completely different question from whether it is indeed a requirement.},
