@article{ WOS:000300208500005,
Author = {Faber, Gerrit and Gerritse, Michiel},
Title = {Foreign determinants of local institutions: Spatial dependence and
   openness},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2012},
Volume = {28},
Number = {1},
Pages = {54-63},
Month = {MAR},
Abstract = {There are both empirical and theoretical arguments for the thesis that
   foreign factors have an impact on domestic institutional quality. Yet
   the literature is divided over whether exposure (openness to the world
   economy) or the kind of institutions in surrounding countries (relative
   location) determines the quality of local institutions. This paper
   confronts these hypotheses empirically, addressing the issues of strong
   cross-sectional dependence and the endogeneity of openness. In a
   107-country cross-section, both trade openness and relative location
   have a positive impact on local institutions. The institutional quality
   of neighbouring countries is not found to be statistically significant
   when considering openness to foreign direct investments instead of
   trade, but the statistical performance of that model is poorer. (C) 2011
   Elsevier B.V. All rights reserved.},
