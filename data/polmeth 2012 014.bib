@article{ WOS:000217365600007,
Author = {Sen, Ronojoy},
Title = {Identifying Criminals and Crorepatis in Indian Politics: An Analysis of
   Two Supreme Court Rulings},
Journal = {ELECTION LAW JOURNAL},
Year = {2012},
Volume = {11},
Number = {2},
Pages = {216-225},
Month = {JUN},
Abstract = {While there is overwhelming support for democracy in India and voter
   turnout is higher than in mostWestern democracies, there seems to be
   dangerously low levels of trust in political parties and politicians.
   There are at least two facts about Indian politics that merit mention.
   First, the number of members of Parliament (MPs) in the Lok Sabha or
   Lower House (which is directly elected by the people in a first past the
   post system) with criminal records is striking. Second, a significant
   feature of Indian politics is the number of extremely wealthy-or
   crorepati (multi-millionaire) to use common parlance-MPs in Parliament.
   The criminal records of MPs and their financial worth are available in
   the public domain thanks to two landmark Supreme Court judgments in 2002
   and 2003. In this essay I examine these two judgments and the reasoning
   behind the Court's efforts to inject some transparency into the
   political system. I argue that while the courts and commissions have had
   a positive impact in regulating elections in India, it is nowhere enough
   to restore confidence in politicians and stem electoral corruption. The
   political class and elected representatives have to be a part of the
   process and only sustained pressure, not just from courts and
   institutions like the Election Commission (EC), but also from voters can
   effect real change.},
