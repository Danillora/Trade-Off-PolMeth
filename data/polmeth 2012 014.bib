@article{ WOS:000297819200017,
Author = {Katsimi, Margarita and Sarantides, Vassilis},
Title = {Do elections affect the composition of fiscal policy in developed,
   established democracies?},
Journal = {PUBLIC CHOICE},
Year = {2012},
Volume = {151},
Number = {1-2},
Pages = {325-362},
Month = {APR},
Abstract = {This paper investigates the impact of elections on the level and
   composition of fiscal instruments using a sample of 19 high-income OECD
   democracies during the period 1972-1999. We find that elections shift
   public spending towards current expenditures at the cost of public
   investment. Although we find no evidence for an electoral cycle for
   government deficit and overall expenditures, we find a negative effect
   of elections on revenue attributed to a fall in direct taxation. Our
   results apply for predetermined electoral periods while endogenous
   elections seem to increase deficit and leave the composition of fiscal
   policy unaffected.},
