@article{ WOS:000310498000005,
Author = {Deloche, Regis},
Title = {On the absence of priority fights within economics, cheap talk, and the
   Walras-Jevons correspondence},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2012},
Volume = {122},
Number = {4},
Pages = {585-598},
Month = {JUL-AUG},
Abstract = {According to Merton {[}1957], the history of science is punctuated by
   disputes over priority of discovery, but there are no such priority
   fights in economics. Hands {[}2006] explains this phenomenon by assuming
   that economists have no sense of collective moral outrage. We provide a
   counterexample to this thesis by developing a game-theoretic analysis of
   the Walras-Jevons correspondence. We show that one-way pre-play
   communication helped them to coordinate on a pure strategy Nash
   equilibrium of a ``battle of the sexes game{''}. Walras recognized
   Jevons as the inventor of the ``theory of exchange{''}. It was good for
   the promotion of mathematical economics.},
