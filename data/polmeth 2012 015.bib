@article{ WOS:000307280000014,
Author = {Veiga, Linda Goncalves},
Title = {Determinants of the assignment of EU funds to Portuguese municipalities},
Journal = {PUBLIC CHOICE},
Year = {2012},
Volume = {153},
Number = {1-2},
Pages = {215-233},
Month = {OCT},
Abstract = {The paper examines the determinants of the assignment of EU funds to
   Portuguese municipalities using a large and unexplored dataset covering
   all (278) mainland municipalities over 15 years. Empirical results
   reveal that besides normative objectives, political motivations also
   influence the distribution of funds by the national government across
   municipalities. Grants to municipalities increase during local election
   years, and more funds are transferred to municipalities where
   legislative elections have been closely contested and where the ruling
   national party had been supported by voters.},
