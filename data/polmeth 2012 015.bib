@article{ WOS:000306830300002,
Author = {Habel, Philip D.},
Title = {Following the Opinion Leaders? The Dynamics of Influence Among Media
   Opinion, the Public, and Politicians},
Journal = {POLITICAL COMMUNICATION},
Year = {2012},
Volume = {29},
Number = {3},
Pages = {257-277},
Abstract = {Media elites strive to shape the policy preferences of their audience
   through the publication of their opinions. Scholars, however, have not
   fully distilled whether the opinions communicated by media elites are
   successful in moving the public or politicians toward their preferred
   policy position, or whether media is responsive to these actors. This
   article offers a means of assessing media influence. I provide measures
   of the policy preferences of two leading newspaper editorial pages, the
   New York Times and the Wall Street Journal, and employ these scales in a
   dynamic time series analysis. I find that the announced positions of the
   media have minimal influence. Rather, I find evidence of a movable
   media, where media opinion shifts in response to changes in the policy
   positions of politicians.},
