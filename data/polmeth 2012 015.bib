@article{ WOS:000309911700004,
Author = {Azemar, Celine and Darby, Julia and Desbordes, Rodolphe and Wooton, Ian},
Title = {Market Familiarity and the Location of South and North MNEs},
Journal = {ECONOMICS \& POLITICS},
Year = {2012},
Volume = {24},
Number = {3},
Pages = {307-345},
Month = {NOV},
Abstract = {We use a systematic empirical analysis of the determinants of
   South-South (SS) and North-South (NS) foreign direct investment (FDI) as
   a canvas to explore how multinational enterprises (MNEs) location
   decisions are shaped by better acquaintance with a foreign market
   resulting from bilateral ties, experience of international expansion,
   and knowledge of how to deal with poor governance. We find that these
   various aspects of market familiarity, which can interact together, are
   important to explain and differentiate the location behaviors of South
   MNEs (S-MNEs) and North MNEs (N-MNEs) in developing countries.},
