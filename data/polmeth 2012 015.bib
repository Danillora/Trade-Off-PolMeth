@article{ WOS:000301711800004,
Author = {Cole, Creagh McLean},
Title = {Libertarian Traditions: FA Hayek and John Anderson},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2012},
Volume = {58},
Number = {1},
Pages = {51-66},
Month = {MAR},
Abstract = {An examination of John Anderson's work in relation to other political
   theorists of his time would help to provide a more adequate assessment
   of one of the greats of Australian intellectual and cultural life.
   Comparisons between Anderson's libertarian strain and F.A. Hayek's have
   been noted but there has been no real attempt to draw out the real
   differences arising from their substantive theoretical positions.
   Anderson and Hayek rejected any notion of a single, complete ethical
   code; they despaired at the demoralising effects of a culture of
   dependence and irresponsibility; they rejected the ameliorative
   liberalism represented by T.H. Green and Bernard Bosanquet. But they
   disagreed centrally in their assessments of those practices and
   movements which act as sites of resistance to the levelling standards
   and values of commercial and consumerist institutions.},
