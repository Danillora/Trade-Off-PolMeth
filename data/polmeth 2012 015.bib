@article{ WOS:000214103000011,
Author = {Hinduja, Sameer},
Title = {The Heterogeneous Engineering of Music Piracy: Applying Actor- Network
   Theory to Internet-Based Wrongdoing},
Journal = {POLICY AND INTERNET},
Year = {2012},
Volume = {4},
Number = {3-4},
Pages = {229-248},
Month = {DEC},
Abstract = {Actor-network theory, as developed within the sociology of science and
   technology, views any phenomenon as the product or effect of a patterned
   network of heterogeneous elements. This paper applies such a
   paradigmatic perspective to the phenomenon of music piracy, in an
   attempt to underscore the relevance of studying and analyzing each of
   the components (and their interrelationships) that make up the reality
   of that behavior. Heterogeneity implies not only the social but also the
   technical, economic, political, organizational, informational,
   contextual, and perceptual. Each of these components merit attention and
   response if progress is to be made in understanding and responding to
   digital piracy and possibly other forms of Internet-based wrongdoing.},
