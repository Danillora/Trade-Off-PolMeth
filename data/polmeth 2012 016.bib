@article{ WOS:000297817800015,
Author = {Suh, Jaekwon},
Title = {Corporate governance under proportional electoral systems},
Journal = {PUBLIC CHOICE},
Year = {2012},
Volume = {150},
Number = {3-4},
Pages = {671-690},
Month = {MAR},
Abstract = {This paper provides a political explanation for the form of corporate
   governance captured by the degree of minority shareholder protection
   (MSP). Using a multi-stage game model of three-party competition under
   proportional electoral systems, I demonstrate that the electoral
   threshold-the minimum percentage of votes a party must receive to get at
   least one seat in parliamentary PR systems-is inversely related to the
   degree of MSP. This finding represents an important modification to
   formal models in the political economic literature that tends to omit
   the political process.},
