@article{ WOS:000304637400005,
Author = {Padovano, Fabio},
Title = {The drivers of interregional policy choices: Evidence from Italy},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2012},
Volume = {28},
Number = {3},
Pages = {324-340},
Month = {SEP},
Abstract = {Theoretical developments in political economy analyses of
   intergovernmental transfers identify several factors previously ignored
   in the empirical literature and emphasize the importance of
   institutional details. Combined, these two developments create a degrees
   of freedom problem and make cross country samples less meaningful. This
   paper circumvents these problems by examining an Italian regional panel
   that minimizes the set of institutional conditioning factors and is
   large enough to examine a comprehensive set of explanatory variables.
   The estimates confirm the relevance of most political determinants and
   of standard economic and socio-demographic determinants of interregional
   redistribution. Differences in type of expenditures and
   intergovernmental relations are also considered. (C) 2012 Elsevier B.V.
