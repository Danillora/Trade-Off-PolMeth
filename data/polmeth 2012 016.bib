@article{ WOS:000300208500006,
Author = {Goel, Rajeev K. and Nelson, Michael A. and Naretta, Michael A.},
Title = {The internet as an indicator of corruption awareness},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2012},
Volume = {28},
Number = {1},
Pages = {64-75},
Month = {MAR},
Abstract = {We apply a standard specification of the causes of corruption to a large
   sample of countries to investigate the effect of internet awareness
   about corruption on prevalence and perceptions of corruption. The main
   hypothesis is that greater corruption awareness acts as a corruption
   deterrent. A unique data set of internet searches on Google and Yahoo is
   compiled using alternate variations of ``corruption{''}, ``bribery{''}
   and ``country name{''} keywords to capture internet corruption
   awareness. Results show that internet hits about corruption per capita
   correlate negatively with corruption perceptions and corruption
   incidence. This finding generally holds for different specifications and
   other robustness checks. Published by Elsevier B.V.},
