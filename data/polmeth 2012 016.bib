@article{ WOS:000302845700004,
Author = {Martorell, Miguel},
Title = {THE REASON IN JOSE ANTONIO'S WORDS. THE ARRIBA'S TEAM AND THE
   SPANISH ECONOMIC POLICY IN THE 50'S},
Journal = {HISTORIA Y POLITICA},
Year = {2012},
Number = {27},
Pages = {83-111},
Month = {JAN-JUN},
Abstract = {In 1953 a group of young economists, led by Juan Velarde Fuertes, took
   control of the economy section of the Falangist newspaper Arriba. From
   the pages of this newspaper, and from other media in the Movimiento's
