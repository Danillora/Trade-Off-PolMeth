@article{ WOS:000297867100005,
Author = {Adres, Eitan and Vanhuysse, Pieter and Vashdi, Dana R.},
Title = {The Individual's Level of Globalism and Citizen Commitment to the State:
   The Tendency to Evade Military Service in Israel},
