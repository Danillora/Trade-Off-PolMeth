@article{ WOS:000213005900002,
Author = {Thakur, Dhanaraj},
Title = {Diversity in the Online Deliberations of NGOs in the Caribbean},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2012},
Volume = {9},
Number = {1, SI},
Pages = {16-30},
Abstract = {Deliberative democracy has been promoted as a way of improving
   legitimacy and political equality. This article seeks to understand how
   deliberation takes place within the intersection of two unique spaces:
   regional civil society groups in the Caribbean and communication in
   online forums. Specifically, I explore how various forms of participant
   diversity influence deliberation. Using both content analysis and
   interview data, I argue that occupational and country diversity
   contribute to the use of reasoned arguments. The results suggest that
   online deliberative spaces could be useful in international public
   policy arenas.},
