@article{ WOS:000312531900006,
Author = {Juriansz, John and Opeskin, Brian},
Title = {Electoral Redistribution in Australia: Accommodating 150 Years of
   Demographic Change},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2012},
Volume = {58},
Number = {4},
Pages = {557-579},
Month = {DEC},
Abstract = {Electoral redistribution (redistricting) is a process that has the
   potential to advance the principle of one vote, one value in the face of
   dynamic human populations. Using the Australian federal electoral system
   as a case study, this article examines the impact of changes in the
   size, composition and spatial distribution of the population on
   electoral boundary delimitation over the past 110 years, and analyses
   the likely impact of future population change over the next forty years.
   The article concludes that the Australian electoral system has moved
   progressively towards greater equality of voting power encapsulated by
   the one vote, one value principle. However, the capacity to achieve even
   greater equality through electoral redistribution is constrained by
   constitutional and pragmatic considerations.},
