@article{ WOS:000213008300004,
Author = {Hepburn, Paul},
Title = {Local Governance and the Online Networked Public Sphere-A Case Study},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2012},
Volume = {9},
Number = {4},
Pages = {370-387},
Abstract = {What are the prospects for the Web enhancing the local governance
   process by providing a networked public sphere? This article addresses
   this question by researching online local civic activity associated with
   a case study of a UK local government referendum. Drawing upon social
   network analysis theory, this research first of all applies relational
   hyperlink analysis to map and structurally analyze the hyperlinked
   network. Second, it draws upon a network ethnography approach to
   identify and then interview key actors associated with this network. The
   results suggest a need for policy intervention to counter the distinct
   homophily effects and ``politics as usual{''} scenario uncovered in the
   local online network.},
