@article{ WOS:000310680000006,
Author = {Fording, Richard C. and Smith, Joseph L.},
Title = {Barack Obama's ``Fight{''} to End Poverty: Rhetoric and Reality},
Journal = {SOCIAL SCIENCE QUARTERLY},
