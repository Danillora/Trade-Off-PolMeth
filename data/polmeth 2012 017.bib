@article{ WOS:000213007200001,
Author = {Hoffman, Lindsay H.},
Title = {Participation or Communication? An Explication of Political Activity in
   the Internet Age},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2012},
Volume = {9},
Number = {3},
Pages = {217-233},
Abstract = {This study clarifies the differences between political participation and
   political communication online. Although many studies have examined the
   nature and effects of online activity, none has effectively
   distinguished between the two types of behavior. This lack of clarity
   has arguably led to conflicting findings and confusion about what
   demarks a truly participatory act online. Using Pew 2008 data, online
   political behaviors are defined and examined. Results suggest that
   online communication and participation do appear to be different
   constructs, and while online participation predicts voting, online
   communication does not. Implications for conceptualizing these behaviors
   and directions for future research are discussed.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
