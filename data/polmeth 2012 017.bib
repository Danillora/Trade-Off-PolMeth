@article{ WOS:000307129400004,
Author = {Chvalkovska, Jana and Jansky, Petr and Skuhrovec, Jiri},
Title = {BEARER SHARES IN PAPER FORM AND PUBLIC PROCUREMENT},
Journal = {POLITICKA EKONOMIE},
Year = {2012},
Volume = {60},
Number = {3},
Pages = {349-361},
Abstract = {This article examines for the first time the relationship between
   transparency of ownership structure and (i) profits of firms winning
   public procurement contracts and (ii) competition for the contracts and
   savings of the public authority. These characteristics are compared for
   two groups of public procurement contract winners that differ by their
   ownership structure. The detailed data from the Czech Republic are used
   and the firms with bearer shares in paper form serve as the less
   transparent firms. The results show that less transparent firms have
   significantly higher profit margins in terms of tens of percentage
   points and participate in less competitive public procurement contracts
   and result into lower savings for the public authority. This could be an
   indirect indicator of conflict of interests or corruption. The article
   concludes with policy recommendations.},
