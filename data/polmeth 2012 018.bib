@article{ WOS:000298789300006,
Author = {Le, Thanh},
Title = {R\&D Spillovers through Student Flows, Institutions, and Economic
   Growth: What can we Learn from African Countries?},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2012},
Volume = {59},
Number = {1},
Pages = {115-130},
Month = {FEB},
Abstract = {Using modern panel cointegration estimation techniques, this paper
   examines whether tertiary student flows can effectively transmit
   technological knowledge from industrialized countries to African
   countries. The results obtained lend strong support to this hypothesis.
   In addition, this paper extends the analysis to include institutional
   variables such as the ease of doing business, legal origins, and
   religious majority to see if institutional characteristics have any
   impact on the way knowledge diffusion affecting total factor
   productivity (TFP). However, it is not clear that institutional
   differences are important factors that influence the degree of R\&D
   spillovers and, hence, the TFP of African countries.},
