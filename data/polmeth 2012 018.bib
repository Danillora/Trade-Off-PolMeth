@article{ WOS:000301317400002,
Author = {Fumagalli, Eileen and Narciso, Gaia},
Title = {Political institutions, voter turnout, and policy outcomes},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2012},
Volume = {28},
Number = {2},
Pages = {162-173},
Month = {JUN},
Abstract = {This paper tests whether constitutions directly affect economic
   outcomes. By introducing citizens' political participation as the
   driving force connecting institutions to policy outcomes, we empirically
   show that voter turnout is the channel through which forms of government
   affect economic policies. We provide evidence of the existence of two
   relationships. First, presidential regimes appear to be associated with
   lower voter participation in national elections. Second, higher voter
   participation induces an increase in government expenditure, total
   revenues, welfare state spending, and budget deficit. We conclude that
   forms of government affect policy outcomes only through voter turnout.
