@article{ WOS:000302845700009,
Author = {Aviles, Juan},
Title = {ANARCHIST AND JIHADI TERRORISM: A COMPARATIVE ANALYSIS},
Journal = {HISTORIA Y POLITICA},
Year = {2012},
Number = {27},
Pages = {227-249},
Month = {JAN-JUN},
Abstract = {Anarchist terrorism of bygone and jihadi terrorism of our days show
   obvious differences but also some similarities. This essay analyses them
   focusing on four aspects: the concept of terrorism as propaganda, which
   was pioneered by anarchists and has inspired the spectacular attacks by
   Al Qaeda; the similarities and differences between anarchist and jihadi
   ideologies, with special reference to the religious traits of anarchism
   and the anarchisant traits of jihadism; the use by both of the concept
   of martyrdom; and finally that decentralized type of organization which
   was peculiar of anarchism and may become common in the new phase of the
   global terrorist jihad.},
