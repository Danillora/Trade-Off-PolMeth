@article{ WOS:000297817200005,
Author = {Schulte, Elisabeth},
Title = {Communication in committees: who should listen?},
Journal = {PUBLIC CHOICE},
Year = {2012},
Volume = {150},
Number = {1-2},
Pages = {97-117},
Month = {JAN},
Abstract = {In a parsimonious model of a collective decision problem with partially
   conflicting interests, we show that restricting communication may
   enhance decision quality. If disclosed information is observed by
   decision makers with different preferences, individuals may
   strategically withhold information. In this case, a committee member's
   silence arouses the other members' suspicions. As a consequence, in case
   of residual uncertainty, individual votes may react less to information
   that is publicly observed.},
