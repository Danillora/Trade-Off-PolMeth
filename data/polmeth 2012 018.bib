@article{ WOS:000297818800001,
Author = {Casey, Peter Colum and Wierman, Mark J. and Gibilisco, Michael B. and
   Mordeson, John N. and Clark, Terry D.},
Title = {Assessing policy stability in Iraq: a fuzzy approach to modeling
   preferences},
Journal = {PUBLIC CHOICE},
Year = {2012},
Volume = {151},
Number = {3-4},
Pages = {409-423},
Month = {JUN},
Abstract = {The first Council of Representatives elected under the new Iraqi
   Constitution was unable to pass legislation required to achieve the
   political benchmarks set by the government. We argue that the exercise
   of a qualified veto by the three-member Presidency Council essentially
   required near unanimity among the nine parties of the governing
   coalition. Given the policy positions of these parties, unanimity was
   not possible. Our analysis makes use of a fuzzy veto players model. The
   placement of the government parties along a single dimension based on
   fuzzy preference measures derived from party text data reveals no common
   area of agreement.},
