@article{ WOS:000302432000006,
Author = {Dreher, Axel and Gassebner, Martin},
Title = {Do IMF and World Bank Programs Induce Government Crises? An Empirical
   Analysis},
Journal = {INTERNATIONAL ORGANIZATION},
Year = {2012},
Volume = {66},
Number = {2},
Pages = {329-358},
Month = {SPR},
Abstract = {We examine whether and under what circumstances World Bank and
   International Monetary Fund (IMF) programs affect the likelihood of
   major government crises. We find that crises are, on average, more
   likely as a consequence of World Bank programs. We also find that
   governments face an increasing risk of entering a crisis when they
   remain under an IMF or World Bank arrangement once the economy's
   performance improves. The international financial institution's (IFI)
   scapegoat function thus seems to lose its value when the need for
   financial support is less urgent. While the probability of a crisis
   increases when a government turns to the IFIs, programs inherited by
   preceding governments do not affect the probability of a crisis. This is
   in line with two interpretations. First, the conclusion of IFI programs
   can signal the government's incompetence, and second, governments that
   inherit programs might be less likely to implement program conditions
   agreed to by their predecessors.},
