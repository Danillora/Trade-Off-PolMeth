@article{ WOS:000303629300002,
Author = {Zhang, Feng},
Title = {Rethinking China's grand strategy: Beijing's evolving national interests
   and strategic ideas in the reform era},
Journal = {INTERNATIONAL POLITICS},
Year = {2012},
Volume = {49},
Number = {3},
Pages = {318-345},
Month = {MAY},
Abstract = {The question of China's grand strategy is of great importance for
   understanding the international impact of China's rise. Both Western and
   Chinese scholars dispute whether China has developed a coherent grand
   strategy in the reform era. The main reason for the controversy seems to
   lie as much in theoretical and methodological assumptions about defining
   and analyzing grand strategy as in empirical validity. This article
   contributes to the debate by adopting a novel theoretical approach to
   analyzing grand strategy by seeing it as the conjunction of national
   interests and strategic ideas. It examines China's evolving national
   interests and strategic ideas in the reform period in order to clarify
   the exploratory, evolutionary and adaptive nature of policy change.
   China cannot be said to have developed a premeditated grand strategy
   during this period. Even though one may still be able to rationalize
   elements of China's foreign policies into a grand strategy, it comes at
   the cost of missing their changing nature. International Politics (2012)
   49, 318-345. doi:10.1057/ip.2012.5; published online 16 March 2012},
