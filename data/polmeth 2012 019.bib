@article{ WOS:000301786100008,
Author = {Braun, Caelesta},
Title = {The Captive or the Broker? Explaining Public Agency-Interest Group
   Interactions},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2012},
Volume = {25},
Number = {2},
Pages = {291-314},
Month = {APR},
Abstract = {The literature offers individually valid yet collectively inconsistent
   hypotheses concerning the nature of public agencies' responsiveness to
   interest groups. This article analyzes the nature of this responsiveness
   by examining the brokerage potential of public agencies among interest
   groups. Such a brokerage potential is hypothesized to follow from
   agencies' preferences for policy goods as well as the tendency of
   interest groups to seek access to public agencies. It combines analyses
   of agencies' demand for policy goods with interest groups preferences
   for seeking access to specific policy venues. The analyses are based on
   survey data of national civil servants and interest groups in the United
   Kingdom and in the Netherlands. The findings suggest that both strategic
   preferences as well as organizational routines positively correlate with
   a brokerage potential while interaction patterns within and with the
   organizational environment of public agencies can constrain their
   brokerage potential in several distinct ways.},
