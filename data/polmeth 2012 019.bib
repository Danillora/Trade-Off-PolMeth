@article{ WOS:000307851300018,
Author = {Simao, Licinia},
Title = {The problematic role of EU democracy promotion in Armenia, Azerbaijan
   and Nagorno-Karabakh},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2012},
Volume = {45},
Number = {1-2},
Pages = {193-200},
Month = {MAR-JUN},
Abstract = {This article looks at the interdependences between the democratisation
   processes in Armenia, Azerbaijan and Nagorno-Karabakh and the management
   of the Karabakh conflict, focussing on the EU's democracy promotion
   policies. The article argues that the EU's normative foreign policy in
   the South Caucasus has been limited by the permanence of the protracted
   conflicts, in two interrelated ways. First, by not addressing the
   conflicts the EU focused on long-term goals but failed to provide
   short-term incentives towards peace. Second, by allowing only a limited
   involvement in the protracted conflicts, especially inside Karabakh, the
   EU was perceived as a reluctant partner, undermining its normative
   credentials. (C) 2012 The Regents of the University of California.
   Published by Elsevier Ltd. All rights reserved.},
