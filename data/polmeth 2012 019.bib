@article{ WOS:000311698000005,
Author = {Kim, Junmo and Kwon, Yong-Soo},
Title = {Economic development, the evolution of foreign labor and immigration
   policy, and the shift to multiculturalism in South Korea},
Journal = {PHILIPPINE POLITICAL SCIENCE JOURNAL},
Year = {2012},
Volume = {33},
Number = {2},
Pages = {178-201},
Abstract = {Since South Korea's economic transformation from a labor to a capital
   economy, it has had to grapple with issues like labor shortages and
   demographic changes such as low birth rates and a rapidly aging
   population that threaten long-term growth. In order to sustain its
   development, the country has increasingly turned to foreign labor and
   selective immigration as countermeasures for its economic and
   demographic problems. The state manages the influx of foreigners under a
   framework of ``multiculturalism{''} that professes openness towards
   becoming a ``multicultural society{''} despite resistance rooted in
   ethno-nationalism and a history of homogeneity. Despite criticism on its
   shortcomings and the failure of multiculturalism elsewhere,
   multiculturalism has provided the South Korean state with a convenient
   and flexible, if ambiguous, framework under which a diverse range of
   policies and programs for improving the lives of foreigners and
   immigrants could be adopted while simultaneously dealing with its
   economic and demographic concerns.},
