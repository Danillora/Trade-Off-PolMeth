@article{ WOS:000309731300003,
Author = {Babb, Florence E.},
Title = {Theorizing Gender, Race, and Cultural Tourism in Latin America A View
   from Peru and Mexico},
Journal = {LATIN AMERICAN PERSPECTIVES},
