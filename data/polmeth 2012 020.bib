@article{ WOS:000214260900001,
Author = {Parsons, Wayne},
Title = {Keynes and the utility of policy relevant knowledge},
Journal = {CRITICAL POLICY STUDIES},
Year = {2012},
Volume = {6},
Number = {3},
Pages = {223-242},
Abstract = {This article explores Keynes' approach to the utility of policy relevant
   knowledge as making an important contribution to the theory and practice
   of `evidence-based policy'. It argues that although Keynes believed that
   policymaking should be better informed by the weight of evidence, he
   also appreciated the profound limitations of rationality when human
   beings make decisions in conditions of uncertainty and the dangers of
   using mathematics rather than `ordinary discourse'. His treatment of
   decision-making in The General Theory is shown to have considerable
   relevance for: the development of a critical perspective on the
   relationship between knowledge production and utilization; the
   application of economics to the policy process; the position of
   economics as a policy science; and understanding the economic and
   financial crisis of the early twenty-first century.},
