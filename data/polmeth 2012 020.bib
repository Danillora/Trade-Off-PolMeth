@article{ WOS:000303568400005,
Author = {Driver, Stephen and Hensby, Alexander and Sibthorpe, Joanne},
Title = {The shock of the new? Democratic narratives and political agency},
Journal = {POLICY STUDIES},
Year = {2012},
Volume = {33},
Number = {2, SI},
Pages = {159-172},
Abstract = {Political parties were at the heart of the traditional narrative of
   British democracy. But parties as agents of political mobilisation are
   in decline. By contrast, membership of political pressure groups and
   social movement organisations has grown considerably. This shift in
   political activism is considered by some, but by no means all, to offer
   a radical alternative narrative of democratic participation. This
   article examines the organisational changes taking place behind this
   shift; and explores the extent to which more traditional models of
   political agency can be reformed in ways that supports and sustains the
   political activism at the core of a healthy democratic society.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
