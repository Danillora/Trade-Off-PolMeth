@article{ WOS:000309623900002,
Author = {Tallberg, Jonas},
Title = {The EU and regional security. Integration, security and associated
   non-members},
Journal = {INTERNASJONAL POLITIKK},
Year = {2012},
Volume = {70},
Number = {3},
Pages = {287+},
Abstract = {The EU and regional security. Integration, security and associated
   non-members
   This article analyses the relationship between EU security, integration
   and associated members using insights from security studies and the
   literature on Europeanization. While much of the recent literature
   emphasizes either the EU's insignificance as a security actor or its
   importance as a normative and global actor, I investigate its role as a
   security actor in its own region, arguing that the EU is primarily a
   regional security actor. I make two general claims: (1) it is the
   development of common rules and values in various policy areas that
   constitutes the basis for the EU as a security actor; and (2) it is the
   successful projection of these rules and values beyond EU borders that
   will determine the impact of the EU as a security actor. The aim is
   therefore to show how the EU promotes security and stability through the
   externalization of rules and values through various processes,
   association agreements and neighbourhood policies.},
