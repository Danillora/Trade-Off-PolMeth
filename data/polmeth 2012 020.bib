@article{ WOS:000307485800001,
Author = {Dean, Hartley},
Title = {Welcome relief or indecent subsidy? The implications of wage top-up
   schemes},
Journal = {POLICY AND POLITICS},
Year = {2012},
Volume = {40},
Number = {3},
Pages = {305-321},
Month = {JUL},
Abstract = {A key policy response to the downward pressure on wages of the
   lowest-paid workers in the developed economies of the capitalist world
   has been the introduction of means-tested cash transfer schemes by which
   to top up low wages. Findings from a study of the experiences of the
   beneficiaries of a particular scheme (the United Kingdom's Working Tax
   Credit) suggest that, although schemes may serve to relieve the poverty
   of low-paid workers and their families, the extent to which they promote
   the accessibility of `decent work' is ambiguous.},
