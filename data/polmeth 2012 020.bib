@article{ WOS:000302366500004,
Author = {Cunningham, Kathleen Gallagher and Bakke, Kristin M. and Seymour, Lee J.
   M.},
Title = {Shirts Today, Skins Tomorrow: Dual Contests and the Effects of
   Fragmentation in Self-Determination Disputes},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2012},
Volume = {56},
Number = {1, SI},
Pages = {67-93},
Month = {FEB},
Abstract = {While theoretical models of conflict often treat actors as unitary, most
   self-determination groups are fragmented into a number of competing
   internal factions. This article presents a framework for understanding
   the ``dual contests{''} that self-determination groups engage in-the
   first with their host state and the second between co-ethnic factions
   within groups. Using a new data set of the number of factions within a
   sample of self-determination groups from 1960 to 2008, the authors find
   that competition between co-ethnic factions is a key determinant of
   their conflict behavior. More competing factions are associated with
   higher instances of violence against the state as well as more factional
   fighting and attacks on co-ethnic civilians. More factions using
   violence increases the chances that other factions will do so, and the
   entry of a new faction prompts violence from existing factions in a
   within-group contest for political relevance. These findings have
   implications for both theory and policy.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Cunningham, KG (Corresponding Author), Univ Maryland, Dept Govt \& Polit, 3140 Tydings Hall, College Pk, MD 20742 USA.
   Cunningham, Kathleen Gallagher, Univ Maryland, Dept Govt \& Polit, College Pk, MD 20742 USA.
   Cunningham, Kathleen Gallagher, Peace Res Inst Oslo, Oslo, Norway.
   Bakke, Kristin M., UCL, Sch Publ Policy, London, England.
   Seymour, Lee J. M., Leiden Univ, Inst Polit Sci, Leiden, Netherlands.},
DOI = {10.1177/0022002711429697},
ISSN = {0022-0027},
