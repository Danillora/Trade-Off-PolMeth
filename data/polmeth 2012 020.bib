@article{ WOS:000307231700005,
Author = {Lewis, Stephen E.},
Title = {Indigenista Dreams Meet Sober Realities The Slow Demise of
   Federal Indian Policy in Chiapas, Mexico, 1951-1970},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2012},
Volume = {39},
Number = {5},
Pages = {63-79},
Month = {SEP},
Abstract = {In 1951, Mexico's National Indigenist Institute (Instituto Nacional
   Indigenista-INI) opened its pilot coordinating center in highland
   Chiapas. For several years, Mexico's top indigenistas directed the
   center and tested several innovative projects aimed at promoting
   economic development, modernization, and cultural assimilation among the
   Tzeltal and Tzotzil Maya. They also tried to protect the indigenous from
   their worst non-Indian exploiters. After a period of initial fanfare,
   this important center slid into decline, hampered by inadequate budgets,
   compromised programs in education, health, and agriculture, and a
   demoralized staff. Its decadence had implications for the dozens of
   centers built by the INI in subsequent years. Crippled by local
   opposition, an outdated theoretical underpinning, and tepid political
   support, this coordinating center-and the INI in general-also suffered
   from close association with the Ministry of Public Education and other
   institutions of Mexico's corporatist state.},
