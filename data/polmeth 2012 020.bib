@article{ WOS:000303781400004,
Author = {Dicklitch, Susan and Yost, Berwood and Dougan, Bryan M.},
Title = {Building a Barometer of Gay Rights (BGR): A Case Study of Uganda and the
   Persecution of Homosexuals},
Journal = {HUMAN RIGHTS QUARTERLY},
