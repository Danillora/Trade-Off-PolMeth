@article{ WOS:000313717200008,
Author = {Mario Battaglino, Jorge},
Title = {The coexistence of peace and conflict in South America: toward a new
   conceptualization of types of peace},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2012},
Volume = {55},
Number = {2},
Pages = {131-151},
Abstract = {South America's predominant democratic regimes and its increasing
   interdependence on regional trade have not precluded the emergence of
   militarized crises between Colombia and Venezuela or the revival of
   boundary claims between Chile and Peru. This way, how can we
   characterize a zone that, in spite of its flourishing democracy and
   dense economic ties, remain involved in territorial disputes for whose
   resolution the use of force has not yet been discarded? This article
   contends that existing classifications of zones of peace are not
   adequate to explain this unusual coexistence. Thus, its main purpose is
   to develop a new analytical category of regional peace for assessing
   this phenomenon: the hybrid peace. It aims to research the evolution of
   security systems in South America during the previous century and build
   a new, threefold classification of peace zones: negative peace zones,
   hybrid peace zones, and positive peace zones.},
