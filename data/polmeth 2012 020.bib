@article{ WOS:000305914100001,
Author = {Barzashka, Ivanka and Oelrich, Ivan},
Title = {Iran and nuclear ambiguity},
Journal = {CAMBRIDGE REVIEW OF INTERNATIONAL AFFAIRS},
Year = {2012},
Volume = {25},
Number = {1},
Pages = {1-26},
Abstract = {A comparison between Iran's current nuclear efforts and those of the
   pro-Western regime of Shah Reza Pahlavi shows that Iranian ambitions for
   a full-fledged civilian nuclear programme have remained relatively
   constant for nearly half a century. Today, fuel cycle technology
   provides Iran with a latent nuclear weapon's potential. However, US
   concerns about an Iranian bomb, which began in the early 1970s and
   aggravated after the Iranian Revolution, long predate Teheran's uranium
   enrichment programme. Thus, Iran is a specific case of the general
   problem presented by the inherent potential of nuclear technology to
   both civilian and military ends. Approaches to dealing with a long-term,
   ambiguous, latent nuclear weapon threat, whether Iranian or other, are
   suggested.},
