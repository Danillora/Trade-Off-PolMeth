@article{ WOS:000302791900006,
Author = {Gilley, Bruce},
Title = {Authoritarian environmentalism and China's response to climate change},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2012},
Volume = {21},
Number = {2},
Pages = {287-307},
Abstract = {Authoritarian environmentalism is a non-participatory approach to public
   policy-making and implementation in the face of severe environmental
   challenges. Using the case of China's climate change policy, the
   meaning, causes, and consequences of authoritarian environmentalism are
   explored. A key finding is that authoritarian environmentalism is more
   effective in producing policy outputs than outcomes. Theoretical and
   policy implications follow.},
