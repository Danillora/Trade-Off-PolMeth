DOI = {10.1111/j.1467-9299.2011.01996.x},
ISSN = {0033-3298},
EISSN = {1467-9299},
Keywords-Plus = {PUBLIC-PARTICIPATION; ACTIVE CITIZENSHIP; GOVERNMENT; COMMUNITY;
