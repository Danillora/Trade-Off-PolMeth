@article{ WOS:000310591500006,
Author = {Kaunert, Christian and Leonard, Sarah},
Title = {The development of the EU asylum policy: venue-shopping in perspective},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2012},
Volume = {19},
Number = {9},
Pages = {1396-1413},
Abstract = {The development of the EU asylum and migration policy is often explained
   as the result of `venue-shopping', that is, the move by policy-makers to
   an EU policy venue in order to avoid national constraints. This article
   demonstrates that, contrary to what would have been expected on the
   basis of this widespread view, EU co-operation on asylum matters has
   actually led to a rise in the legal standards applicable to
   asylum-seekers and refugees. This outcome can be mainly explained by
   broader changes that have gradually affected the EU `system of venues'
   and have thereby decreased the likelihood of more restrictive measures
   being adopted in the EU asylum policy venue. This has important
   implications for the EU governance of asylum and migration in general.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Kaunert, C (Corresponding Author), Univ Salford, Sch Humanities Languages \& Social Sci, Crescent House, Salford M5 4WT, Lancs, England.
   Kaunert, Christian; Leonard, Sarah, Univ Salford, Manchester, England.
   Kaunert, Christian, European Univ Inst, Robert Schuman Ctr Adv Studies, Florence, Italy.
