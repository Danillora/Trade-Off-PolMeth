@article{ WOS:000306995900006,
Author = {Davenport, Christian and Inman, Molly},
Title = {The State of State Repression Research Since the 1990s},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2012},
Volume = {24},
Number = {4, SI},
Pages = {619-634},
Abstract = {Researchers have been exploring government repressive behavior for
   decades, but the greatest improvements have come in the last two. For
   example, greater theoretical specification has allowed us to determine a
   great deal about what repression is and why it occurs, while greater
   methodological sophistication has allowed us to test these theories
   rigorously. Despite or rather because of these advancements, however, we
   know comparatively little about what impact repression has on other
   social, economic, and political phenomena. This article reviews our
   general knowledge of the topic and attempts to improve our understanding
   of how repressive action influences behavioral challenges to
   governments.},
