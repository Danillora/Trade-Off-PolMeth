@article{ WOS:000305469700003,
Author = {Dahl, Elizabeth S.},
Title = {Oil and Water? The Philosophical Commitments of International Peace
   Studies and Conflict Resolution},
Journal = {INTERNATIONAL STUDIES REVIEW},
