@article{ WOS:000300336700008,
Author = {Murray, Kyle},
Title = {Christian `Renewalism' and the production of global free market hegemony},
Journal = {INTERNATIONAL POLITICS},
Year = {2012},
Volume = {49},
Number = {2, SI},
Pages = {260-276},
Month = {MAR},
Abstract = {Charismatic and Pentecostal elements of global Christianity serve key
   roles in the production of free market hegemony within and between
   states, societies and markets across the world. While many of the
   institutions of these Christian social forces are fiercely
   decentralised, this popular global movement has converged on key
   elements of a shared conception of the world which links core,
   semi-peripheral and peripheral societies across national boundaries and
   class distinctions. These activities highlight the necessity of linking
   the emergent narratives on `global religious economy' with the larger
   narratives of Critical IPE. It is argued here that Gramsci provides us
   with useful conceptual tools with which we may contextualise specific
   transnational religious movements and social forces within the framework
   of globalisation and the production of neoliberal hegemony within and
   between specific states, societies and markets. International Politics
   (2012) 49, 260-276. doi:10.1057/ip.2011.41; published online 13 January
   2012},
