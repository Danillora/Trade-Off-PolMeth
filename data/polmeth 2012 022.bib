@article{ WOS:000301793500006,
Author = {Ruedig, Wolfgang},
Title = {The perennial success of the German Greens},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2012},
Volume = {21},
Number = {1},
Pages = {108-130},
Abstract = {The German Greens achieved a record result in the federal elections of
   2009. Using both qualitative and quantitative methods, it is argued that
   this excellent result was not an isolated phenomenon but is in line with
   a long-term growth of Green electoral support that has a strong
   generational basis. A `feminisation' and `greying' of Green voters is
   also apparent. Despite the party's effort to emphasise economic and
   social issues in its campaigning, the chief factors explaining Green
   voting remain environmental concern and opposition to nuclear energy.},
