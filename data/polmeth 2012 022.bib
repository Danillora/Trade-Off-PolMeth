@article{ WOS:000436098600002,
Author = {Hernandez Rodriguez, Rogelio and Pansters, Wil G.},
Title = {Democracy in Mexico and the Return of the PRI},
Journal = {FORO INTERNACIONAL},
Year = {2012},
Volume = {52},
Number = {4},
Pages = {755+},
Month = {OCT-DEC},
Abstract = {In 2012 the Mexican democratic system confronted a new kind of
   alternation of governments. One of the principal characteristics of this
   challenge was the electoral recovery of the PRI, which seemed certain to
   win, representing for some a return to authoritarian rule. The essay
   considers this possibility in the light of analysis of the institutional
   maturity of political changes (electoral, in competency and
   participation, relations between branches of power and between the
   federation and the states), and concludes that such a return to the past
   is unlikely. However, this conclusion does not mean that democracy is
   becoming entrenched, given the resistance offered by numerous informal
   practices such as clientelism, corporatism, corruption and the lack of
   effective rule of law.},
Publisher = {COLEGIO MEXICO, A C, DEPT PUBLICACIONES},
