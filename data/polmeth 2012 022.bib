@article{ WOS:000307910000004,
Author = {Aboussi, Mourad and Garcia-Quero, Fernando},
Title = {An Institutional Approach to the Immigrant Entrepreneurship},
Journal = {REVISTA DEL CLAD REFORMA Y DEMOCRACIA},
Year = {2012},
Number = {53},
Pages = {97+},
Month = {JUN},
Abstract = {This article presents the complex relationship between immigration and
   entrepreneurship in a specific geographical context, the region of
   Andalusia (Spain). From an institutional approach, we analyze the
   results of an empirical study, carried out with a qualitative
   methodology, in order to explain the variables that affect the decision
   of an immigrant entrepreneur to open a business; and the form, evolution
   and success of the business. Latest progresses in immigrant
   entrepreneurship literature are shown as well.
   The main conclusion of this document is the need to incorporate in the
   migration policy, actions that contribute to improving the institutional
   framework where immigrant entrepreneurship takes place. This is crucial
   because immigrant entrepreneurship has a great potential to improve the
   quality of life of immigrants and to produce a positive effect on
   society and the host economy.},
