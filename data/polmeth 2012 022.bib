@article{ WOS:000214260900003,
Author = {Pinto, Laura Elizabeth},
Title = {Hidden privatization in education policy as `quick fixes' by `hired
   guns': contracting curriculum policy in Ontario},
Journal = {CRITICAL POLICY STUDIES},
Year = {2012},
Volume = {6},
Number = {3},
Pages = {261-281},
Abstract = {Despite critiques of privatization in education, little investigation
   has been conducted surrounding how it occurs in education policy
   production. This article describes and analyzes the privatization of
   curriculum policy production in Ontario during the 1990s based on
   archival documents and interviews with 16 policy actors. When analyzed
   in light of democratic ideals, this privatization resulted in elitism,
   and a politicized and fragmented process that reified a culture of
   control. Though privatization allowed the government to complete a vast
   amount of policy work in a relatively short period of time, findings
   reveal a shortfall of democracy.},
