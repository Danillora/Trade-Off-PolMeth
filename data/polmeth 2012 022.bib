@article{ WOS:000306605000001,
Author = {Kaplan, Danny and Rosenmann, Amir},
Title = {Unit Social Cohesion in the Israeli Military as a Case Study of ``Don't
   Ask, Don't Tell{''}},
Journal = {POLITICAL PSYCHOLOGY},
