@article{ WOS:000313085100004,
Author = {Tunander, Ola},
Title = {Contemporary history, international relations and the problem with
   sources},
Journal = {INTERNASJONAL POLITIKK},
Year = {2012},
Volume = {70},
Number = {4},
Pages = {475+},
Abstract = {Contemporary history, international relations and the problem with
   sources Writing about contemporary history or international relations
   presupposes relevant empirical knowledge. However, ``formative
   events{''} that define a ruling discourse can be interpreted falsely in
   real time. This article presents two examples: First, the Cold War
   submarine activity in Swedish waters, which at the time was described as
   the first Soviet military political initiative against a Western
   European state since the Berlin crisis of 1960-1961, is now viewed as
   having been a US-UK deception operation. Second, during the Cold War,
   European terrorism was described as an anarchist/leftwing threat, but we
   now know that Eastern and Western state agencies were deeply involved in
   these operations. In these examples, contemporary history appears as a
   parallel ``simulacrum{''} bearing no similarity to the authentic events.
   Secret documents may turn history upside-down, but may not be
   dedassified until perhaps 70 years later, as if the scholar had to wait
   70 years to write about international relations. However, this article
   also presents senior officials who say that much of the world of secrets
   exists only as oral information and, accordingly, has to be collected
   before the documents are declassified. A combination of oral and written
   sources is often necessary. From a methodological point of view, a
   triangulation of documents, oral information and technical evidence may
   provide us with a road forward.},
