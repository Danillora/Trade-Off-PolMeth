@article{ WOS:000300599600008,
Author = {Cowell, Richard and Downe, James and Martin, Steve and Chen, Alex},
Title = {Public confidence and public services: it matters what you measure},
Journal = {POLICY AND POLITICS},
Year = {2012},
Volume = {40},
Number = {1},
Pages = {123-143},
Month = {JAN},
Abstract = {Governments around the world are concerned about declining public
   confidence in democratic institutions. It has been widely assumed that
   improving the performance of public services will help address this
   problem. Policy makers in the United Kingdom (UK) have therefore been
   puzzled to discover that public confidence in local government continued
   to decline at a time when local services seemed to be improving.The
   reason for this apparent paradox is that public confidence is influenced
   by a wide array of factors only some of which are captured by official
   measures. Since different data can lead to quite different conclusions,
   it matters what is measured.},
