@article{ WOS:000303577500002,
Author = {Kleinmann, Scott Matthew},
Title = {Radicalization of Homegrown Sunni Militants in the United States:
   Comparing Converts and Non-Converts},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2012},
Volume = {35},
Number = {4},
Pages = {278-297},
Abstract = {Are the mechanisms and processes that lead to radicalization different
   for Muslim converts and non-converts in the United States? While many
   scholars attempt to explain why people violently radicalize, the
   theories are diverse and most treat converts and non-converts the same.
   This study answers this question by categorizing the many radicalization
