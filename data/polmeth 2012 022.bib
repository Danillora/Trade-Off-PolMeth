@article{ WOS:000436093100006,
Author = {Ornelas, Raul},
Title = {The Capitalist Crisis: the End of U.S. Hegemony? A Study from the Point
   of View of Competition between Transnational Companies},
Journal = {FORO INTERNACIONAL},
Year = {2012},
Volume = {52},
Number = {1},
Pages = {192+},
Month = {JAN-MAR},
Abstract = {The recent crisis has struck a deep blow to the U.S. economy and
   businesses, entailing spectacular bankruptcies and a relative withdrawal
   on the part of the world's leading power. However, analysis of
   competition reveals a complementary process of concentration of property
   and economic power without precedent in recent history. The
   financialization of the world economy established relationships of
   interdependence between the principal bourgeoisies, together with a
   considerable advantage held by U.S.-based transnational companies in
   strategic sectors of the world economy. As such, it is too early to
   declare an end to U.S. hegemony, at least in the sphere of competition
   between businesses.},
