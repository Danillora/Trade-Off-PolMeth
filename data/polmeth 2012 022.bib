@article{ WOS:000435280900007,
Author = {van Willigen, Niels},
Title = {International administration and institutional autonomy in Bosnia and
   Herzegovina and Kosovo},
Journal = {EAST EUROPEAN POLITICS},
Year = {2012},
Volume = {28},
Number = {4},
Pages = {429-451},
Abstract = {Bosnia and Herzegovina and Kosovo have been under international
   administration since 1995 and 1999, respectively. The international
   administrations were aimed at building autonomous political institutions
   which are able to peacefully resolve political conflicts. Instead,
   institutions were created which heavily depended on the international
   administrations for their functioning. The absence of autonomous
   domestic political institutions can be explained by the absence of
   conflict resolution. The incompatibility (or contents) of the conflict
   was incorporated in the post-war order, as a result of which the
   international administrations received extensive powers to impose peace.
   The international administrations' authority enabled them to build
   political institutions. At the same time, the absence of conflict
   resolution impeded domestic institutions to become autonomous in the
   sense of being independent and having a capacity to act. As a result,
   domestic actors relied on the international administration to take
   fundamental political decisions, leading to institutional dependence on
   the international authority.},
