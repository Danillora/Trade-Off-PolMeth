@incollection{ WOS:000357753700029,
Author = {Scarabelli, Diego},
Editor = {Majer, M and Ondrejcsak, R and Tarasovic, V},
Title = {CLEANING THE BALKAN ROUTE: ELEMENTS OF POLICE COOPERATION IN SERBIA},
Booktitle = {PANORAMA OF GLOBAL SECURITY ENVIRONMENT - 2012},
Year = {2012},
Pages = {401+},
Abstract = {The Serbian territory is one of the parts of the infamous `Balkan
   Route,' which is the main itinerary for the smuggling of heroin,
   dominating land and sea consignments of this drug. In that sense,
   fostering police cooperation in the Western Balkans is an important
   component in tackling transnational organised crime, but also for
   establishing good neighbourly relations. This paper examines the police
   cooperation of the Republic of Serbia with high-income active partners,
   the Nordic states, and states in Southeast Europe. The article concludes
   by stating that Serbia has lately been very proactive in police
   cooperation, but still lacks a strategic goal and needs to pursue more
   multilateral cooperation.},
Publisher = {CENAA-CENTRE EUROPEAN \& NORTH ATLANTIC AFFAIRS},
Address = {HAANOVA 10, PO BOX 209, BRATISLAVA, 851 04, SLOVAKIA},
Type = {Article; Book Chapter},
