@article{ WOS:000306665700003,
Author = {Svensson, My},
Title = {The Socialist Housing Projects as Non-Places in Post-2000 Polish
   Literary and Cinematic Narratives},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
