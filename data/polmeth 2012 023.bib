@article{ WOS:000435277500004,
Author = {Sakwa, Richard},
Title = {Modernisation, neo-modernisation, and comparative democratisation in
   Russia},
Journal = {EAST EUROPEAN POLITICS},
Year = {2012},
Volume = {28},
Number = {1},
Pages = {43-57},
Abstract = {There has long been a debate over whether development is a prerequisite
   for democracy, and by the same token, whether democracy is a
   precondition for development. This debate is part of the larger
   literature examining problems of `transition', a term which is at best
   no more than a code word for the processes shaping accelerated and
   conscious transformation of a society from one type of social order to
   another. For some three decades, the field of comparative
   democratisation has focused attention on the mechanics of political
   transition and the creation of new democracies, accompanied by analysis
   of the reasons for `failed transitions'. While linear versions of
   modernisation theory have been discredited, the creation of capitalist
   democracies on the western model has encountered resistance. The two
   versions of the neo-modemisation paradigm (critical and civilisational)
   help us to examine the `transition' dynamics of developing societies as
   well as providing a framework to critique existing theories.},
