@article{ WOS:000307240100003,
Author = {Zahariadis, Nikolaos},
Title = {Complexity, coupling and policy effectiveness: the European response to
   the Greek sovereign debt crisis},
Journal = {JOURNAL OF PUBLIC POLICY},
Year = {2012},
Volume = {32},
Number = {2},
Pages = {99-116},
Month = {AUG},
Abstract = {What is the impact of Greece's fiscal meltdown on the effectiveness of
   Europe's response? Using Perrow's normal accidents theory, I argue that
   efforts to reduce the likelihood of a Greek default activated
   conflicting centripetal and centrifugal modes of governance. Greater
   centralisation in decision-making at the European Union level improves
   policy effectiveness because it addresses problems of contagion but it
   simultaneously raises the risk of overall failure by increasing
   diagnosis, coordination and compliance costs. Three episodes are
   explored: the first bailout in May 2010, the mid-term fiscal strategy in
   June-July 2011 and the second bailout in February 2012. Implications are
   drawn for theories of delegation, intergovernmentalism and the future of
   EU crisis management.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {EDINBURGH BLDG, SHAFTESBURY RD, CB2 8RU CAMBRIDGE, ENGLAND},
