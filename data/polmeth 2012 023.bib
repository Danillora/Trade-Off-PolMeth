@article{ WOS:000302021900005,
Author = {Williams, Jackson},
Title = {Geographic variations in health care utilization: effects of social
   capital and self-interest, and implications for US Medicare policy},
Journal = {SOCIO-ECONOMIC REVIEW},
Year = {2012},
Volume = {10},
Number = {2},
Pages = {317-342},
Month = {APR},
Abstract = {This paper applies social capital analysis to the geographic variations
   in health care utilization documented by the Dartmouth Atlas project.
   The paper proposes a typology of social capital effects on behaviour
   relevant to health policy, including a dimension of interpersonal
   transactions in which strict adherence to a duty to others may be
   overcome by a measure of selfishness. The paper examines the concept of
   physician-induced demand through this lens, hypothesizing that regions
   with low social capital will have a higher intensity of health care
   resource utilization. A series of regressions employing indicators of
   intensity is presented to illustrate a likely link between social
   capital and utilization. The paper concludes that, while other effects
   of social capital may explain geographic variations, there appears to be
   enough circumstantial evidence of opportunistic behaviour by physicians
   to justify consideration of more rigorous physician payment policies in
   the US Medicare programme.},
