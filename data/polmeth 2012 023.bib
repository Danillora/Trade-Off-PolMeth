@article{ WOS:000302296100001,
Author = {McEwen, Nicola and Swenden, Wilfried and Bolleyer, Nicole},
Title = {Introduction: Political Opposition in a Multi-level Context},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
Year = {2012},
Volume = {14},
Number = {2, SI},
Pages = {187-197},
Month = {MAY},
Abstract = {This short introduction sets out the rationale for the special issue. It
   introduces the concepts of intergovernmental relations (IGR) and party
   political incongruence which are central to the analyses contained in
   the issue. It considers the nature and form of intergovernmental
   relations in the early years of devolution, under conditions of
   predominant party congruence in the composition of the central and
   sub-state governments. It then develops the hypothesised relationship
   between party political incongruence and intergovernmental relations,
   focusing on the nature and structure of IGR. It introduces the key
   questions to be addressed in the issue and each of the subsequent
   contributions which explore this relationship in greater depth.},
