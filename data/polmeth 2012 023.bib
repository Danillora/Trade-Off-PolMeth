@article{ WOS:000421277800002,
Author = {Lemay-Hebert, Nicolas},
Title = {Multi-ethnicity or ghettoization? : international State building
   and partition of Kosovo against the controversial project of wall in
   Mitrovica},
Journal = {ETUDES INTERNATIONALES},
Year = {2012},
Volume = {43},
Number = {1},
Pages = {27-47},
Month = {MAR},
Abstract = {More than twenty years after the fall of the Berlin wall, a number of
   cities live with walls or fences which impose spatial and political
   divisions. This paper intends to focus on the partition of the city of
   Mitrovica ( Kosovo), which dates back to the NATO intervention in 1999.
   The partition of this city is emblematic of the division of Kosovo in
   two distinct communities. The project of building a wall within
   Mitrovica, proposed by the NATO in 2001 encompasses the paradox of the
   entire intervention in Kosovo: the international goal of promoting a
   multiethnic Kosovo came to be seen as a policy of ghettoization of the
   various communities, contributing to the identity crisis that NATO
   originally wanted to subdue.},
