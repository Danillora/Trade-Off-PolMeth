@article{ WOS:000421278900007,
Author = {Brice, Benjamin},
Title = {The future of the war in the world of commerce Raymond Aron in the face
   of pessimistic and optimistic story philosophies},
Journal = {ETUDES INTERNATIONALES},
Year = {2012},
Volume = {43},
Number = {3},
Pages = {421-438},
Month = {SEP},
Abstract = {The West, and especially Europe, often understands globalization as a
   new process that will eventually challenge the domination of traditional
   politics. This paper will attempt to question the idea of a gradual
   disappearance of war in human affairs, using Raymond Aron's thoughts on
   the subject. In the first part we will see how the concepts of
   ``proces{''} (process) and ``drame{''} (drama) provide a method to
   analyze historical developments. Then, in a second part, we will apply
   such concepts to three general causes of conflict (namely passions,
   economic interests and principles) to show that pacifistic hopes must be
   refrained. Even the pacification in Europe does not mean an end of war.},
