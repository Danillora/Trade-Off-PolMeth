@article{ WOS:000302092300009,
Author = {Scharpf, Fritz W.},
Title = {Perpetual momentum: directed and unconstrained?},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2012},
Volume = {19},
Number = {1, SI},
Pages = {127-139},
Abstract = {At the most general level, the perpetual momentum of `integration
   through law' is driven by the substantive dynamics of legal doctrines
   extending the protection of individual interests and by procedural
   conditions facilitating the use of European law to challenge the
   institutional regimes of EU member states. Given the supremacy and
   direct effect of European law, and the decision rules of EU policy
   making, this momentum could not be halted through political or judicial
   intervention.},
