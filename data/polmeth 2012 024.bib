@article{ WOS:000300435800002,
Author = {Ferreira, Mariana Leal and Devine, Dominique},
Title = {Theater of the Oppressed as a Rhizome Acting for the Rights of
   Indigenous Peoples Today},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2012},
Volume = {39},
Number = {2, 1},
Pages = {11-26},
Month = {MAR},
Abstract = {The spread of Augusto Boal's Theater of the Oppressed across the
   Americas and the rest of the world can be understood in terms of Deleuze
   and Guattari's metaphor of the rhizome, whose nomadic habit of growth
   and propagation mirrors the power of Theater of the Oppressed to
   reproduce itself in more than 70 countries worldwide. The Theater of the
   Oppressed rhizome is now deeply rooted in academia and has sprouted in
   classrooms and in the streets, bringing together students, scholars,
   administrators, policy makers, and community activists in the pursuit of
   social justice and human rights. An examination of its use as a
   pedagogical tool calls attention to its potential for creating a world
   in which human rights are appreciated and protected. Its use is
   particularly timely today given the worldwide attention to the rights of
   the indigenous peoples represented by the adoption of the United Nations
   Declaration on the Rights of Indigenous Peoples in 2007.},
