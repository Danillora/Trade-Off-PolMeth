@article{ WOS:000314018800004,
Author = {Maurer, John H.},
Title = {`Winston has gone mad': Churchill, the British Admiralty, and the Rise
   of Japanese Naval Power},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2012},
Volume = {35},
Number = {6},
Pages = {775-798},
Abstract = {As Chancellor of the Exchequer during the late 1920s, Winston Churchill
   was at the center of British strategic decision making about how to
   respond to the naval challenge posed by Japan's rise as a rival sea
   power. Churchill downplayed the likelihood of war with Japan. The
   leadership of the Royal Navy disagreed: they saw Japan as a dangerous
   threat to the security of the British Empire. Examining this dispute
   between Churchill and the Admiralty highlights the awkward political,
   economic, and strategic tradeoffs confronting British leaders between
   the world wars.},
