@article{ WOS:000306557100004,
Author = {Bas, Muhammet A. and Coe, Andrew J.},
Title = {Arms Diffusion and War},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2012},
Volume = {56},
Number = {4},
Pages = {651-674},
Month = {AUG},
Abstract = {The authors present a model of the relationship between the spread of
   new military technologies and the occurrence of war. A new technology
   could shift the balance of power, causing anticipatory war as one side
   tries to prevent the other from obtaining it. When one side already has
   it, war is more likely when the shift in power is large, likely, and
   durable. When neither side has it, war is more likely when the expected
   shift is asymmetric (e.g., one side is more likely to get it) and when
   the two sides fear that a war will occur once one of them has it. The
   authors illustrate the model with historical examples from the spread of
   firearms (the Musket Wars in precolonial New Zealand) and of nuclear
   weapons (the end of US nuclear monopoly and the 1967 Six-Day War). A
   broader implication is that major power competition can unintentionally
   cause wars elsewhere.},
