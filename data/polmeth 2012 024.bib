@article{ WOS:000302600400002,
Author = {Ford, Matthew Charles},
Title = {Finding the Target, Fixing the Method: Methodological Tensions in
   Insurgent Identification},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2012},
Volume = {35},
Number = {2},
Pages = {113-134},
Abstract = {This article is concerned with exploring the recent observations of
   Lieutenant-General Lamb who stated that there was no simple binary
   between counterintelligence (COIN) and counterterrorism (CT).
   Specifically, the article will use the intelligence-gathering,
   assessment, and target identification processes and methods used on
   operations in Iraq and Afghanistan to examine this further. What makes
   this an interesting exercise is that the effectiveness of a COIN/CT
   intervention totally depends on whether an insurgent has been properly
   identified. If the wrong person has been targeted then kinetic,
   influence, or policing activities are at best exploratory and at worst
   wasteful or even positively harmful. Thus, by investigating the
   intelligence model that frames the way adversaries and communities are
   identified, it becomes possible to understand the limitations in the
   processes and methods used. At the same time this approach makes it
   possible to cast light on how and to what extent various techniques
   drawn from COIN and CT work together in Overseas Contingency Operations.},
