@article{ WOS:000302296100005,
Author = {Jones, Richard Wyn and Royles, Elin},
Title = {Wales in the World: Intergovernmental Relations and Sub-state Diplomacy},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
Year = {2012},
Volume = {14},
Number = {2, SI},
Pages = {250-269},
Month = {MAY},
Abstract = {This article explores intergovernmental relations between the devolved
   Welsh Assembly Government and the central UK government through the
   prism of two case studies focusing on examples of Welsh sub-state
   diplomacy, the first being international activity aimed at promoting
   trade and investment and the second the Wales for Africa programme. The
   article focuses in particular on the implications for WalesUK relations
   of partial party incongruence brought about by the formation of the
   LabourPlaid Cymru coalition government in Cardiff in the summer of 2007.
   The authors also examine the early indications of the impact of full
   party incongruence following the formation of the ConservativeLiberal
   Democrat coalition government in London in June 2010.},
