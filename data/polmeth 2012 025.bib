@article{ WOS:000313380700013,
Author = {Sakwa, Richard},
Title = {Looking for a greater Europe: From mutual dependence to an international
   regime},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2012},
Volume = {45},
Number = {3-4, SI},
Pages = {315-325},
Month = {SEP-DEC},
Abstract = {On a range of structural and ideological issues Russia and Europe are
   increasingly mutually dependent, as in the energy sector, as well as in
   security matters and a number of other issues, but this has not
   generated `interdependence' - the term popular from the 1970s to denote
   a qualitative transformation of the relationship, as described by
   Keohane and Nye and others. The paper asks why this has been the case,
   providing historical and theoretical context, but above all it looks at
   some of the ideas now advanced that could transcend the present impasse.
   An `international regime' is emerging in response to Russian aspirations
   for some sort of `greater European' community, the European Union's
   complex pattern of `external governance', and the need to integrate
   third parties (notably Turkey) into an extended network of relations
   that falls short of full-scale accession but gives form to aspirations
   for pan-European unity. (C) 2012 The Regents of the University of
   California. Published by Elsevier Ltd. All rights reserved.},
