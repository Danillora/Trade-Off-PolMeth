@article{ WOS:000305975600003,
Author = {Ewert, Joachim Winfried},
Title = {A force for good? Markets, cellars and labour in the South African wine
   industry after apartheid},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2012},
Volume = {39},
Number = {132, SI},
Pages = {225-242},
Abstract = {This paper argues that on balance, deregulation and the exposure to
   overseas markets has been beneficial to the South African wine industry,
   resulting in strong growth, increased employment and much improved
   international competitiveness. However, the paper also draws attention
   to the fact that not everybody has benefited in equal measure from the
   process of upgrading, modernisation and internationalisation. Since the
   start of the transition in the mid 1990s, a number of growers have left
   the industry and for many workers employment has become less secure.},
