@article{ WOS:000212715200001,
Author = {Desrosiers, Marie-Eve},
Title = {Reframing Frame Analysis: Key Contributions to Conflict Studies},
Journal = {ETHNOPOLITICS},
Year = {2012},
Volume = {11},
Number = {1},
Pages = {1-23},
Abstract = {This article focuses on framing theory, an approach studying strategic
   communication, and its contribution to conflict studies. The article's
   aims are twofold: to demonstrate how framing bridges the divide between
   instrumental and social-psychological approaches in conflict studies and
   to illustrate how it is accordingly better able to shed light on the
   mobilization of ethnic violence. Drawing on the work of American framing
   theorists, the article shows that framing addresses how actors
   purposefully develop interpretations of an issue, event or change to
   achieve specific goals, but does so by recognizing the very real impacts
   social structures have on framers' ability to frame. As a result,
   framing is better able to shed light on communication aimed at ethnic
   mobilization and conflict. Framing analysts propose a sophisticated
   understanding of the strategies and vocabularies used to stimulate what
   they call consensus mobilization and action mobilization. The paper
   illustrates how a framework built around these framing insights can
   highlight some of the micro-processes behind the mobilization of ethnic
   solidarity and the creation of a will to act in instances of conflict.},
