@article{ WOS:000409466600004,
Author = {Brown, Dana and Knudsen, Jette Steen},
Title = {Managing corporate responsibility globally and locally: Lessons from a
   CR leader},
Journal = {BUSINESS AND POLITICS},
Year = {2012},
Volume = {14},
Number = {3, SI},
Month = {OCT},
Abstract = {Corporate Responsibility (CR) is today an essential component of
   corporate global strategy. CR can bolster the institutional context for
   market expansion fill institutional voids or facilitate market entry as
   a component of non- market strategy. Yet, in fulfilling these functions,
   CR may need to be highly sensitive to local contexts. How can
   transnational firms organize CR so as to maximize efficiencies from
   globalization and to minimize the fragmentation of corporate
   organizational cultures? provide a framework for analyzing the way that
   corporations coordinate global and local functions. We build on this
   framework in a case study of Novo Nordisk and its approach to
   determining global and local CR policies and procedures with regard to
   its China and US subsidiaries. Our findings suggest that it is important
   for companies to define a common set of organizational norms. In
   addition, CR need to be sensitive to local institutional contexts, but
   learning from subsidiary experience is important and lends itself to
   standardization and replication of initiatives across market contexts.},
