@article{ WOS:000214261500002,
Author = {Skilling, Peter},
Title = {Immigration policy in New Zealand: divergent narratives, shared
   assumptions and national identity},
Journal = {CRITICAL POLICY STUDIES},
Year = {2012},
Volume = {6},
Number = {4},
Pages = {363-378},
Abstract = {Policies can be analyzed as stories about who and what is valued within
   society. Immigration policy (and immigration policy narratives) offer
   particularly clear renderings of such stories. This article critically
   adopts the approach of narrative policy analysis to explore the core
   assumptions that are shared by apparently opposed policy narratives of
   immigration policy in contemporary New Zealand. It theorizes these
   shared assumptions as a policy (pre) meta-narrative that is logically
   related to discourses of national identity, and that effects specific
   silences and exclusions. These public silences, it is argued, serve to
   damagingly limit and distort the field of policy debate.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
