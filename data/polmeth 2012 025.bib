@article{ WOS:000302092300004,
Author = {Kelemen, R. Daniel},
Title = {The political foundations of judicial independence in the European Union},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2012},
Volume = {19},
Number = {1, SI},
Pages = {43-58},
Abstract = {This contribution explores the political foundations of judicial
   independence in the European Union and asks to what degree the ECJ is
   insulated against court curbing mechanisms that might threaten judicial
   independence. The contribution reviews the major court curbing
   mechanisms identified in the literature on American law and politics and
   assesses the extent to which these mechanisms might be applied in
   restraining the ECJ. The evidence and analysis suggests that the ECJ is
   remarkably well insulated against each of the major court curbing
   mechanisms identified in the literature.},
