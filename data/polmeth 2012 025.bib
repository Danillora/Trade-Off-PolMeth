@article{ WOS:000309731300008,
Author = {Bell, Elizabeth R.},
Title = {``Pirates of Our Spirituality{''} The 2012 Apocalypse and the Value of
   Heritage in Guatemala},
Journal = {LATIN AMERICAN PERSPECTIVES},
