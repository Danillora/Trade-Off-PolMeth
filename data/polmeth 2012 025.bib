@article{ WOS:000303596100007,
Author = {Clements, Ben},
Title = {Public Opinion in Britain towards Military Action in Libya: A
   Micro-level Analysis},
Journal = {POLITICS},
Year = {2012},
Volume = {32},
Number = {2},
Pages = {109-119},
Month = {JUN},
Abstract = {This article undertakes a micro-level analysis of public opinion towards
   military action in Libya. It examines the effect of sociological factors
   and political attitudes. It finds evidence of a gender gap in support
   for military action. The effects of partisanship are mediated by
   attention to politics. Politically attentive Conservative and Liberal
   Democrat partisans are more supportive of military action. Public
   opinion is also affected by ideological beliefs, economic perceptions
   and pre-existing foreign policy attitudes. The findings contribute to
   research on public opinion and military intervention and to scholarly
   understanding of foreign policy attitudes in Britain.},
Publisher = {WILEY-BLACKWELL},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
