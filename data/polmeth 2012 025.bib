@article{ WOS:000302633700006,
Author = {Sundberg, Ralph and Eck, Kristine and Kreutz, Joakim},
Title = {Introducing the UCDP Non-State Conflict Dataset},
Journal = {JOURNAL OF PEACE RESEARCH},
