@article{ WOS:000302272900003,
Author = {Steine, Bjorn Arne},
Title = {Between National Ambitions, Intellectual Cooperation and American
   Philanthropy: The Attempts to establish International Studies as a
   Scholarly Discipline in Scandinavia before World War II},
Journal = {INTERNASJONAL POLITIKK},
Year = {2012},
Volume = {70},
Number = {1},
Pages = {35+},
Abstract = {This analysis of attempts to establish international studies in
   Scandinavia before World War II demonstrates how closely integrated
   these efforts were with similar ongoing work outside Scandinavia. The
   initiatives in Scandinavia were the result of contact between national
   actors and, in particular, the Rockefeller Foundation and the
   International Studies Conference, who played the part of initiators and
   actively pushed the work forward. However, the article stresses that
   there were also national developments rooted in local initiatives.
   Establishment of the Nobel Institute is particularly important. In the
   process leading up to the founding of the institute in 1904,
   well-developed ideas about the scientific study of international
   relations were expressed. Thus, the article supports recent research
   stressing continuities in the history of the discipline from the late
   1800s and into the interwar years.},
