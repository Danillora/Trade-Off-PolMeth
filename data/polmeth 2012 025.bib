@article{ WOS:000306557100003,
Author = {Faria, Joao Ricardo and Arce, Daniel},
Title = {A Vintage Model of Terrorist Organizations},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2012},
Volume = {56},
Number = {4},
Pages = {629-650},
Month = {AUG},
Abstract = {A dynamic model of a terrorist organization is presented with the
   defining feature being that a succession of operatives is recruited at
   different points in time. Consequently, a government's counterterror
   policy must be tailored according to the vintage structure of the
   terrorist group that it faces. This implies that past history of
   counterterror policy and attacks matter for the formulation of current
   and future policy. The authors present the necessary steps to formulate
   and solve a vintage model, and to deal with the delay differential
   equations that naturally arise from the vintage structure. The resulting
   analysis captures the implications of a diverse set of phenomena such as
   Internet recruiting, training delays for logistically complex plots, age
   distribution of operatives, and the sensitivity of government
   impatience/cabinet composition to terrorist events for the inner
   dynamics of terrorist organizations and counterterror policy. Directions
   for future research are also suggested.},
