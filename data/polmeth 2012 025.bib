@article{ WOS:000305180200005,
Author = {Holmes, Ronald D.},
Title = {The Philippines in 2011},
Journal = {PHILIPPINE POLITICAL SCIENCE JOURNAL},
Year = {2012},
Volume = {33},
Number = {1},
Pages = {81-97},
Abstract = {Twenty months in office, the Aquino administration has zealously pursued
   one of its avowed goals-to fight corruption and prepare the ground for
   transparent, accountable and participatory governance. Events in 2011
   manifest the premium put by President Aquino on this goal, specifically
   securing the accountability of former President Arroyo as well as the
   incumbent Chief Justice of the Supreme Court through cases filed versus
   the former and an impeachment case filed against the latter. However,
   the administration has yet to show equal zeal in acting on issues
   involving asset redistribution as well as crucial political reforms, two
   key challenges that are expected to hound the administration during the
   remainder of its six-year term.},
