@article{ WOS:000302481700006,
Author = {Bryceson, Deborah Fahy},
Title = {Discovery and denial: Social science theory and interdisciplinarity in
   African Studies},
Journal = {AFRICAN AFFAIRS},
Year = {2012},
Volume = {111},
Number = {443},
Pages = {281-302},
Month = {APR},
Abstract = {This article discusses the ebb and flow of theoretical ideas in African
   Studies, specifically the interface between African Studies and
   Development Studies. It explores the epistemological nature of
   interdisciplinarity in African Studies, interrogating when and how
   theoretical insight may contribute to an understanding of material
   reality and welfare improvement in some circumstances, and miss the mark
   by a wide margin in other cases. The purpose of this exercise is to
   stimulate reflection on the contribution of African Studies to
   continental and global intellectual and material change, juxtaposing
   African Studies theory and its role as an applied field of study. This
   necessitates consideration of Africa's position in the interplay of
   world politics and the power of agenda-setting international
   institutions, notably the World Bank.},
