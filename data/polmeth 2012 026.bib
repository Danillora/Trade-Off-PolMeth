@article{ WOS:000311694100004,
Author = {de la Calle, Luis and Roussias, Nasos},
Title = {How Do Spanish Independents Vote? Ideology vs. Performance},
Journal = {SOUTH EUROPEAN SOCIETY AND POLITICS},
Year = {2012},
Volume = {17},
Number = {3, SI},
Pages = {411-425},
Abstract = {We claim that independent voters use performance as their main
   heuristic: if the incumbent fares well, independents reward it; if it
   fares poorly, they switch to the main opposition party. To make the
   switch easier, independents place themselves in the centre of the
   ideological dimension. Using Spanish data, our results confirm that
   independents rely more on performance than partisan voters, net of
   ideological proximity. Two implications also hold: voters ideologically
   equidistant between the Partido Socialista Obrero Espanol and Partido
   Popular are overwhelmingly independents, valuing the incumbent's
   performance more than non-equidistant voters; and independents rarely
   switch to smaller parties when the incumbent's performance is bad.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXFORDSHIRE, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {de la Calle, L (Corresponding Author), Juan March Inst, Ctr Adv Study Social Sci, Madrid, Spain.
   de la Calle, Luis, Juan March Inst, Ctr Adv Study Social Sci, Madrid, Spain.
   Roussias, Nasos, Univ Sheffield, Dept Polit, Sheffield S10 2TN, S Yorkshire, England.},
DOI = {10.1080/13608746.2012.701894},
