@article{ WOS:000306876500003,
Author = {Chowdhury, Mohammad Shahjahan and Al-Hossienie, Chowdhury Abdullah},
Title = {Quest for an Effective and Efficient Local Government: Local Government
   in Japan and Policy Implication for Local Government in Bangladesh},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2012},
Volume = {10},
Number = {3},
Pages = {247-264},
Month = {JUL},
Abstract = {The aim of this paper is to identify the actors and factors which are
   important for effective and efficient functioning of local government.
   With a view to presenting the features of local government Japan has
   been selected as case for the study, in terms of legal authority,
   political parties, negotiation in budget process, inter-transfer of
