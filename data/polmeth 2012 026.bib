@article{ WOS:000218393300004,
Author = {Jones, David Martin and Smith, Michael L. R.},
Title = {Ideology, Networks and Political Religion: Structure and Agency in
   Jemaah Islamiah's Small World},
Journal = {POLITICS RELIGION \& IDEOLOGY},
Year = {2012},
Volume = {13},
Number = {4, SI},
Pages = {473-493},
Abstract = {It has become orthodoxy in terrorism studies that the survival or
   durability of the organization or network is central to the terrorist
   mission. From this perspective, its ideology or political religion is
   subordinated to the core purpose of the group, which is understood to be
   survival for its own sake. Through a case study that examines the
   evolution, development and degradation of the Southeast Asian jihadist
   organization Jemaah Islamiah, this paper tests such a hypothesis. It
   finds, in contrast to organizational and network theory, that the group
   mission is not particularly flexible and that the crucial explanatory
   variable that affecting both the recourse to violence and attempts to
   counter it, lies in the ideological agenda that the theory dismisses.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
