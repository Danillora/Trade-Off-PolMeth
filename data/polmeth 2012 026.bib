@article{ WOS:000212576700007,
Author = {Ackren, Maria and Lindstrom, Bjarne},
Title = {Autonomy development, irredentism and secessionism in a Nordic context},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2012},
Volume = {50},
Number = {4},
Pages = {494-511},
Abstract = {This contribution focuses on the three Nordic autonomous islands: the
   angstrom land Islands, the Faroe Islands and Greenland, and their path
   towards autonomy and the development of their own party systems. It
   focuses on the way mobilisation patterns have developed within the three
   jurisdictions and how the relationships towards their metropolitan
   states (Finland and Denmark, respectively) have evolved during a period
   of administrative and political devolution. The angstrom land Islands
   have experienced a slower process towards increasing autonomy than the
   Danish islands, which are now both heading towards independence. The
   reasons behind this are considered in the light of historical factors,
   the different models of autonomy and the existence of irredentist and/or
   secessionist movements.},
