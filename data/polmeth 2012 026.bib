@article{ WOS:000299279400008,
Author = {Tudoroiu, Theodor},
Title = {The European Union, Russia, and the Future of the Transnistrian Frozen
   Conflict},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
Year = {2012},
Volume = {26},
Number = {1},
Pages = {135-161},
Month = {FEB},
Abstract = {In recent years, increased European Union interest in its eastern
   ``neighborhood{''} has been hailed as a possible solution of the
   Transnistrian frozen conflict. The fall of the communist authoritarian
   regime of Chisinau and the internal crisis of the Smirnov regime in
   Tiraspol also modified the conditions of the nineteen-year conflict.
   However, the European involvement in Moldova is perceived by the Kremlin
   as an intrusion in its own domaine reserve. Moreover, the 2008 war in
   South Ossetia illustrates Russia's return to the early 1990s policy of
   overt instrumentalization of the post-Soviet frozen conflicts. This
   volatile situation is analyzed in order to predict the future evolution
   of the Transnistrian conflict in the larger context of the developing
   regional rivalry between Brussels and Moscow.},
