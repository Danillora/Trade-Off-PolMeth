@article{ WOS:000309353400008,
Author = {Binningsbo, Helga Malmin and Loyle, Cyanne E. and Gates, Scott and
   Elster, Jon},
Title = {Armed conflict and post-conflict justice, 1946-2006: A dataset},
Journal = {JOURNAL OF PEACE RESEARCH},
