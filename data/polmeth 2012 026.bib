@article{ WOS:000307851300004,
Author = {Laruelle, Marlene},
Title = {The paradigm of nationalism in Kyrgyzstan. Evolving narrative, the
   sovereignty issue, and political agenda},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
