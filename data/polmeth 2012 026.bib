@article{ WOS:000435279200005,
Author = {Atwal, Maya and Bacon, Edwin},
Title = {The youth movement Nashi: contentious politics, civil society, and party
   politics},
Journal = {EAST EUROPEAN POLITICS},
Year = {2012},
Volume = {28},
Number = {3, SI},
Pages = {256-266},
Abstract = {The youth movement Nashi was established in Russia with the support of
   the Putin regime in 2005. The success of anti-regime demonstrators in
   Ukraine's `Orange Revolution' in 2004 had been noted in Moscow, and
   Nashi's role was to serve as a pro-regime force to be mobilised against
   the opposition. Its focus was the contentious politics of the street.
   Nashi represents an interesting theoretical case from the perspective of
   contentious politics and its relationship with civil society and formal
   party politics. Nashi's role has developed to include facilitating young
   people's engagement with party politics and business. Its early
   centralised control has been ameliorated somewhat by a reorganisation
   focused on local action. Nonetheless, Nashi exists with state support.
   Its continued role in contentious politics in support of the Putin
   regime, for example, countering opposition demonstrations in Moscow in
   December 2011, makes its identification as a component of democratic
   civil society problematic.},
