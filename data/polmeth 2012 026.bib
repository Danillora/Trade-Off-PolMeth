@article{ WOS:000309427400002,
Author = {Bas, Muhammet A.},
Title = {Democratic Inefficiency? Regime Type and Suboptimal Choices in
   International Politics},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2012},
Volume = {56},
Number = {5},
Pages = {799-824},
Month = {OCT},
Abstract = {This article examines the relationship between regime type and decision
   makers' tendency to make suboptimal choices in international crises. To
   test hypotheses on the optimality of democratic foreign policy, the
   author uses a novel statistical measure of suboptimality in foreign
   policy behavior. This estimator builds on Signorino's statistical
   strategic models to allow for actor-level variation in deviations from
   optimal behavior in a strategic setting. An analysis of the
   international disputes from 1919 to 1999 shows that democratic leaders
   have a greater tendency to choose policies not optimal for their
   citizens than do nondemocratic leaders.},
