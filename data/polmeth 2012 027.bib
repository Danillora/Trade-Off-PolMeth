@article{ WOS:000298184700004,
Author = {Lucas, Jeffrey W. and Baxter, Amy R.},
Title = {Power, Influence, and Diversity in Organizations},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2012},
Volume = {639},
Pages = {49-70},
Month = {JAN},
Abstract = {This article summarizes literatures on power, status, and influence in
   sociology's group processes tradition and applies them to issues of
   diversity in organizations. Power-defined as the ability to impose one's
   will even against resistance from others-results primarily from position
   in a social structure. Influence-defined as compelling behavior change
   without threat of punishment or promise of reward-results largely from
   the respect and esteem in which one is held by others. Research
   identifies status as a foundation of influence differences in groups and
   indicates that members of disadvantaged status groups, such as women and
   minorities, will have decreased influence and face challenges in
   acquiring and using power. The literature also suggests solutions to
   these challenges, including self-presentation strategies of group
   motivation and institutional arrangements that support women and
   minority group members in powerful leadership positions.},
