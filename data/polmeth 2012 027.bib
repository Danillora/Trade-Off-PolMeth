@incollection{ WOS:000306699000018,
Author = {Liu Jinyuan},
Editor = {Tuo, C},
Title = {ANTI-GLOBALIZATION MOVEMENT AND ITS RESTRICTIONS ON GLOBALIZATION},
Booktitle = {CHINESE PERSPECTIVES ON GLOBALIZATION AND AUTONOMY},
Series = {Issues in Contemporary Chinese Thought and Culture},
Year = {2012},
Volume = {3},
Pages = {347-358},
Abstract = {Anti-globalization movement is an inevitable product of the development
   of globalization. It mainly consists of two basic forms: the street
   demonstrations against globalization and World Social Forum with
   anti-globalization as its leading target. Essentially, the
   anti-globalization movement is not against the process of globalization
   itself, but against the great influences and various negative results of
   neoliberal globalization. By bringing great challenges and pressures
   upon the elites in different countries and international economic
   institutions that promote globalization, anti-globalization movement is
   playing a positive restricting role on the present world globalization
   and its further development, and is helpful to make globalization
   humanistic, just, equal and sustainable.
   Anti-globalization movement is an inevitable product of the development
   of globalization. Since the mid-1990s, especially ``the Seattle
   Battle{''}(1) at the end of 20th century, anti-globalization movement
   has emerged and become popular in the world very quickly. In fact, the
   rise and development of the anti-globalization movement which aims at
   globalization are increasingly affecting and changing the present
   globalization process. Here, on the basis of a brief retrospection of
   the anti-globalization movement, the paper will analyze its restrictions
   on globalization from the essence of the movement so as to help the
   reader understand the anti-globalization movement.},
