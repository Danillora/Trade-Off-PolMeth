@article{ WOS:000301044200006,
Author = {Subotic, Jelena},
Title = {The Transformation of International Transitional Justice Advocacy},
Journal = {INTERNATIONAL JOURNAL OF TRANSITIONAL JUSTICE},
Year = {2012},
Volume = {6},
Number = {1},
Pages = {106-125},
Month = {MAR},
Abstract = {The scope and structure of international transitional justice (TJ)
   advocacy have changed significantly in two principal ways over the past
   two decades. First, international TJ advocacy has greatly expanded its
   purpose. While it used to be aimed primarily at discretionary mechanisms
   for societies to deal with past violence, it is now increasingly
   legalized and directed at broader issues, such as postconflict peace
   accords, and even longer-term goals, such as instituting democracy, rule
   of law and a culture of human rights. This legalization of TJ advocacy
   has had a profound impact on the acceptance of TJ as a norm or standard
   practice for state behavior after conflict. Second, international TJ
   advocacy has become much more professionalized and specialized. The
   professionalization trend has led to increasing calls for tangible
   indicators of the effectiveness of TJ projects, which, in turn, have led
   to increasing templatization of `best practices' around which most
   professional international TJ nongovernmental organizations (NGOs)
   converge. After presenting evidence of this important transformation,
   the article discusses its consequences for the future development of the
   TJ field.},
