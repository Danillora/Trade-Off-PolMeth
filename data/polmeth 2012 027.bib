@article{ WOS:000310607800004,
Author = {Kosterina, Irina},
Title = {Young Married Women in the Russian Countryside: Women's Networks,
   Communication and Power},
Journal = {EUROPE-ASIA STUDIES},
Year = {2012},
Volume = {64},
Number = {10, SI},
Pages = {1870-1892},
Abstract = {This essay explores the experiences of recently married young women
   living in rural Russia in the village of Karsun in the Ul'yanovsk
   Oblast'. It analyses the connections between women in rural communities
   looking at social networks and the power relations inherent within them.
   The ways in which forms of power among and between women (as well as
   between women and men) impact on young women's agency are critically
   appraised through an analysis of what this essay terms forms of `women's
   power'. The essay also shows how women's discussions of their
   experiences reflect the problems that the perceived physical and
   emotional absences of men present for younger married women and their
   attempts to develop forms of autonomy.},
