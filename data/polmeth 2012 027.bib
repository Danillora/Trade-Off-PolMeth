@article{ WOS:000210795300004,
Author = {Rezabek, Randy},
Title = {TICOM: The Last Great Secret of World War II},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2012},
Volume = {27},
Number = {4},
Pages = {513-530},
Abstract = {Recent releases from the National Security Agency reveal details of
   TICOM, the mysterious 1945 operation targeting Germany's cryptologic
   secrets. Often mentioned by such authors as Kahn, Bamford, Parrish and
   Aldrich, for the first time the public has access to this information.
   This article provides a review in greater depth than has been previously
   covered in the open literature of the history of the TICOM operation,
   and its resulting intelligence, including the Germans' efforts against
   Soviet communications. In addition, some comments are provided on why
   TICOM has remained classified long after other similar information from
   World War II was declassified, and why locating TICOM documents is still
   difficult.},
