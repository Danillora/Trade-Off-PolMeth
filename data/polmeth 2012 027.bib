@article{ WOS:000214200700007,
Author = {Mangadan-Konath, Nabeel},
Title = {HIV-Related Border Restrictions: Time to Address Gaps in Policy and
   Practice in India and South Korea},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2012},
Volume = {4},
Number = {2},
Pages = {273-279},
Month = {JUL},
Abstract = {In the early years of HIV, when fear and panic took a lead in the
   absence of scientific evidence, countries imposed restrictions for
   foreigners with HIV. However, evidence made it clear in HIV's first
   decade that border restrictions for international travellers living with
   HIV were an ineffective, costly, and impractical public health strategy.
   Thanks to efforts from different corners, the number of countries with
   such restrictions has come down remarkably. Advocacy efforts led by the
   Joint United Nations Programme on HIV/AIDS ( UNAIDS) are ongoing with
   countries where foreigners still face HIV-related restrictions for
   entry, stay or residence.
   The issue has been reportedly settled in India and South Korea in 2010.
   Both these countries are thus considered to be in the `solved' category
   of countries. However, this policy and practice note unearths some gaps
   that exist in translating this policy-level change into practice.
   Governments in India and South Korea, other national governments, civil
   society at large, and international human rights organizations have
   their roles to play in bridging these gaps. The note further explains
   the importance of tying up these loose ends in human rights protection
   urgently, and suggests ways to accomplish it.},
Publisher = {OXFORD UNIV PRESS},
Address = {GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND},
