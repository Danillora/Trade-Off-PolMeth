@article{ WOS:000305826100012,
Author = {Buendia, Felipe Cala},
Title = {Truth in the Time of Fear: Adios, Ayacucho's Poetics of Memory
   and the Peruvian Transitional Justice Process},
Journal = {INTERNATIONAL JOURNAL OF TRANSITIONAL JUSTICE},
Year = {2012},
Volume = {6},
Number = {2},
Pages = {344-354},
Month = {JUL},
Abstract = {The 1986 short novel, Adios, Ayacucho (Farewell, Ayacucho), by Peruvian
   writer Julio Ortega, can be read as a set of aspirational claims
   regarding the, at the time, nascent transitional justice discourse.
   Analyzed against the findings of the Peruvian Truth and Reconciliation
   Commission about the same events it portrays, Ortega's novel opens a
   productive discussion about the way memory - or, rather, memories -
   should be constructed in the wake of the armed conflict. As the
   disjointed narration of a fragmented body's failed quest for
   whole(some)ness, the text advocates for a decentralized process of
   memory building. If transitional justice is to attain its fundamental
   aim of reinstating and enhancing citizenship in the wake of massive
