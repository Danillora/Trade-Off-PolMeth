@article{ WOS:000308133900003,
Author = {Hansen, Stig Jarle},
Title = {The Dynamics of Somali Piracy},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2012},
Volume = {35},
Number = {7-8, SI},
Pages = {523-530},
Abstract = {By drawing upon information gathered from interviews with pirates as
   well as statistics this article explores the major traits of
   contemporary piracy in Somalia and the how they developed. Criminal
   maritime groups in the country have traditionally been the product of
   the actions and decisions of individual actors, although the current
   wave of activity is more the result of the weakening of political
   institutions. In all cases syndicates are driven by profit
   considerations and have largely shunned connections with political
   entities-although this latter factor appears to have changed since the
   end of 2010.},
