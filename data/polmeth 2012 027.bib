@article{ WOS:000306784900002,
Author = {Hawkins, Benjamin},
Title = {Nation, Separation and Threat: An Analysis of British Media Discourses
   on the European Union Treaty Reform Process},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
