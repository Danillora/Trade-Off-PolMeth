@article{ WOS:000307558000002,
Author = {Wegren, Stephen K.},
Title = {The Impact of WTO Accession on Russia's Agriculture},
Journal = {POST-SOVIET AFFAIRS},
Year = {2012},
Volume = {28},
Number = {3},
Pages = {296-318},
Month = {JUL-SEP},
Abstract = {The article analyzes the impact of WTO accession on Russia's
   agricultural sector. Three aspects of WTO rules that will affect Russian
   agriculture are examined: (1) financial support from the Russian
   government, (2) market access for foreign imports, and (3) market access
   for Russia's food exports. Addressing the question of positive and
   negative impacts on Russia's agricultural sector, the article asks if a
   pessimistic view that sees primarily negative consequences as a result
   of accession is justified. Claims made by skeptics who are dismissive of
   the impact WTO entry will have on Russian agriculture are also
   considered.},
