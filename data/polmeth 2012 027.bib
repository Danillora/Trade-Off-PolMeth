@article{ WOS:000214200700006,
Author = {Sharma, Mandira and Massage, Ingrid and McDonald, Kathryn},
Title = {Lawyers' Intervention at Pretrial Stage Helps to Prevent Torture,
   Illegal Detention and Other Human Rights Violations: Experiences of
   Advocacy Forum-Nepal},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2012},
Volume = {4},
Number = {2},
Pages = {253-272},
Month = {JUL},
Abstract = {This practice note aims to summarize the proven success of a holistic
   strategy for the prevention of torture developed by Advocacy Forum-Nepal
   (AF). Through systematic visits to government detention facilities to
   monitor the status of detainees, giving detainees (especially those from
   poor, marginalized backgrounds) access to a lawyer and protecting their
   rights in detention centres, AF aimed to reduce the widespread torture
   and other ill-treatment in pretrial detention facilities and increase
   access to justice for all detainees. Detailed statistical analysis of
   findings during detention visits were shared with stakeholders of the
   criminal justice system in intra-and inter-sectoral consultation
   meetings to discuss and address problems existing in detention centres
   and end the practice of torture in detention. The findings were reported
   to national and international human rights organizations and expert
   bodies who issued urgent appeals addressed to the government of Nepal to
   prevent torture. In addition, detailed reviews of laws and policies were
   conducted, which in turn fed into advocacy at international, national
   and local levels. This model has had measurable impact; and is set out
   here in the hope it will assist other non-governmental organizations
   (NGOs) and human rights practitioners around the world to devise their
   own effective torture prevention strategy.},
