@article{ WOS:000307952000003,
Author = {Knedlik, Tobias and Von Schweinitz, Gregor},
Title = {Macroeconomic Imbalances as Indicators for Debt Crises in Europe},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2012},
Volume = {50},
Number = {5},
Pages = {726-745},
Month = {SEP},
Abstract = {European authorities and scholars published proposals on which
   indicators of macroeconomic imbalances might be used to uncover risks
   for the sustainability of public debt in the European Union. In this
   article the ability of four proposed sets of indicators to send early
   warnings of debt crises is tested using a signals approach for the study
   of indicators and the construction of composite indicators. It is found
   that a broad composite indicator has the highest predictive power. This
   fact still holds true if equal weights are used for the construction of
   the composite indicator in order to reflect the uncertainty about the
   origin of future crises.},
