@article{ WOS:000308636200001,
Author = {Thorsten, Marie},
Title = {Graphic ``Heart of Darkness{''}: Two Visions of Current Affairs Comics},
Journal = {INTERNATIONAL POLITICAL SOCIOLOGY},
