@article{ WOS:000299035900002,
Author = {Kenny, Michael},
Title = {The Political Theory of Recognition: The Case of the White Working
   Class'},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
Year = {2012},
Volume = {14},
Number = {1},
Pages = {19-38},
Month = {FEB},
Abstract = {This article explores the normative dimensions of a growing political
   focus upon the grievances of the white working class. It asks whether
   these can be considered as forms of recognition claim, rather than
   framed simply as expressions of resentment. I assess the character of
   three different forms of recognition that are invoked in the rhetoric
   which this group has attractedrecognition as (i) liberal-egalitarian
   restitution; (ii) political inclusion; and (iii) cultural politics. And
   I observe the difficulties and dangers of pursuing the third of these
   strategies, while pointing to the potential significance for liberals of
   the first two. I conclude by alluding to the difficulties that the
   political theory of recognition faces from the proliferation of claims
   to recognition among groups in contemporary democratic society.},
