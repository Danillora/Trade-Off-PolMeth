@incollection{ WOS:000306699000009,
Author = {He Zengke},
Editor = {Tuo, C},
Title = {CHALLENGES OF GLOBALIZATION AND RESPONSES OF STATE POWER},
Booktitle = {CHINESE PERSPECTIVES ON GLOBALIZATION AND AUTONOMY},
