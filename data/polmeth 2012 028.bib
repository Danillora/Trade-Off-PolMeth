@article{ WOS:000214200000006,
Author = {Petrasek, David},
Title = {A Road Less Travelled: International Human Rights Advocacy and Armed
   Groups},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2012},
Volume = {4},
Number = {1},
Pages = {128-140},
Month = {MAR},
Abstract = {This note discusses the inadequate attention given to armed groups in
   international human rights advocacy efforts. Although the human rights
   violations committed by armed groups are denounced and reported on,
   there is too little further effort taken to actually bring about a
   change in the behaviour of armed groups. The first part of this note
   develops further the point that the response has been inadequate, and
   the second part suggests some of the underlying reasons preventing a
   more robust engagement by human rights advocates with the problem of
   human rights violations by armed group. A third section suggests a
   number of techniques that may help human rights organizations approach
   the problem of armed groups.},
