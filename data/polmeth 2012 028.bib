@article{ WOS:000309429000010,
Author = {Moore, Cerwyn},
Title = {Suicide Bombing: Chechnya, the North Caucasus and Martyrdom},
Journal = {EUROPE-ASIA STUDIES},
Year = {2012},
Volume = {64},
Number = {9, SI},
Pages = {1780-1807},
Abstract = {This essay analyses Chechen-related suicide attacks, locating them
   within the historical and political context of the anti-Russian
   insurgency in the North Caucasus and the different factions of the
   anti-Russian armed resistance movement in the period between the first
   and second Russo-Chechen wars. The core of the essay is an analysis of
   the different character of two waves of suicide operations, (2000-2002)
   and (2002-2004). The first wave was linked to nominally Islamist groups,
   whereas the second set of attacks were linked to Operation Boomerang
   devised by Shamil Basaev. Finally, the essay considers other attacks
   that do not fit into either of these two waves of terrorism.},
