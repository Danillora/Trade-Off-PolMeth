@article{ WOS:000305208000003,
Author = {Stanojevic, Miroslav},
Title = {The Rise and Decline of Slovenian Corporatism: Local and European
   Factors},
Journal = {EUROPE-ASIA STUDIES},
Year = {2012},
Volume = {64},
Number = {5},
Pages = {857-877},
Abstract = {In the 1990s a regulative pattern that strongly mirrored the structure
   and basic functions of post-war European corporatism was formed and
   stabilised in Slovenia. The system enabled the country's relatively fast
   and smooth inclusion in the European monetary system. However, its
   former rise and recent tendency towards disorganisation clearly overlap
   with the qualitatively different phases of Europeanisation. At first
   glance, this overlap supports the thesis that there has been a decline
   of corporatist pacts in the post-EMU period, suggesting that the decline
   is caused by the more or less successful internalisation of EMU demands
   and pressures during the accommodation process. In the Slovenian case,
   this interpretation is basically misleading. It is true that the decline
   of corporatism in Slovenia was connected with EMU pressures, but the
   primary source of its disorganisation lay in its specific
   micro-foundations.},
