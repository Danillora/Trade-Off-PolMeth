@article{ WOS:000309426100015,
Author = {Wang, Ming and Gabay, Itay and Shah, Dhavan V.},
Title = {The Civic Consequences of ``Going Negative{''}: Attack Ads and
   Adolescents' Knowledge, Consumption, and Participation},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2012},
Volume = {644},
Pages = {256-271},
Month = {NOV},
Abstract = {This study explores whether negative political advertising has any
   impact on adolescents. Two datasets are merged for this inquiry: (1)
   content-coded ad-buy data on the placement of campaign messages on a
   market-by-market and program-by-program basis and (2) national survey
   data of parent-child dyads collected immediately after the 2008
   presidential election. The authors' analysis finds that the negativity
   of political advertising to which adolescents were exposed predicted
   human-interest candidate knowledge, but not policy-relevant candidate
   knowledge. In addition, the negativity of political advertising exposure
   suppressed political consumerism among adolescents, but had no effect on
   their levels of political participation. This study shows that political
   campaigns can affect adolescents' knowledge and participation in
   unconventional and potentially deleterious ways.},
