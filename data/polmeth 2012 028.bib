@incollection{ WOS:000306699000006,
Author = {Tuo, Cai},
Editor = {Tuo, C},
Title = {GLOBAL GOVERNANCE: A CHINESE PERSPECTIVE AND PRACTICE},
Booktitle = {CHINESE PERSPECTIVES ON GLOBALIZATION AND AUTONOMY},
Series = {Issues in Contemporary Chinese Thought and Culture},
Year = {2012},
Volume = {3},
Pages = {49-78},
Abstract = {China needs to adopt a theoretical perspective that is different from
   that of the prevailing western theory of global governance. Such a
   theory with a Chinese perspective may be termed ``global governance at
   the national level{''}. It refers to global governance 1) transformed
   into transnational cooperation carried out at the domestic level, 2)
   exclusively focused upon the management of global issues and 3) deeply
   rooted in the effort to foster a civil society within China and develop
   grassroots democracy. China is practising such a type of global
   governance and providing the world with fresh and helpful experience and
   lessons: 1) a multi-actor, multi-departmental cooperation system is the
   basic form and effective mechanism for global governance; 2) while
   government still plays a leading role in global governance, the notions
   of power and management of government agencies and officials are
   undergoing constant change; 3) although global awareness and global
   values are clearly discernible in global governance, they are far from
   adequate for a global transformation; and 4) the evolvement of civic
   virtue and civil society organisations is a great asset created by
   global governance.},
Publisher = {E J BRILL},
Address = {PO BOX 9000, NL-2300 PA LEIDEN, NETHERLANDS},
