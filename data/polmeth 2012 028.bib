@article{ WOS:000302181100009,
Author = {Martinez, Jr., Ramiro and Stowel, Jacob I.},
Title = {Extending Immigration and Crime Studies: National Implications and Local
   Settings},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2012},
Volume = {641},
Pages = {174-191},
Month = {MAY},
Abstract = {One of American society's enduring debates centers on the immigration
   and violent crime relationship. This classic debate is revisited using
   data for individual homicide incidents and census-tract-level homicides
   in Miami, Florida, and San Antonio, Texas, in the 1980s and 1990s,
   respectively. The article starts with these two comparative cases
   because they mirror the immigration influx, Latino growth, and homicide
   decline seen throughout the country since 1980. These findings are also
   replicated in an analysis of the immigration and crime influx across the
   nation using U.S. counties in 2000. In sum, results from comparative
   cases, different time points, homicide motivations, and
   individual/community/national levels-and even controlling for Latino
   regional concentration-are reported. The findings were clear and
   unequivocal: more immigrants did not mean more homicide, and that
   outcome held across time and place.},
