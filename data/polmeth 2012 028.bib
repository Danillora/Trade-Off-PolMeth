@article{ WOS:000210791400005,
Author = {Fischer, Benjamin B.},
Title = {Anglo-American Intelligence and the Soviet War Scare: The Untold Story},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2012},
Volume = {27},
Number = {1},
Pages = {75-92},
Abstract = {During the Soviet war scare of the 1980s, British intelligence shared
   vital information from KGB officer Oleg Gordievsky with its American
   partners. The US intelligence community, however, was suspicious of the
   message and the messenger, dismissing Soviet `war talk' as
   disinformation. Some officials even believed that the British had
   tweaked their reports to influence US policy. President Ronald Reagan,
   however, on the advice of Prime Minister Margaret Thatcher, listened to
   Gordievsky rather than his intelligence advisors. The war scare had a
   profound influence on Reagan's thinking about nuclear war, Kremlin
   fears, and Soviet-American relations that led him to seek a new detente
   with Moscow and the end of the Cold War through diplomacy rather than
   confrontation.
   Subsequent events and post-Cold War revelations vindicated Gordievsky.
   Reagan sought his advice on the eve of his first summit meeting with
   Mikhail Gorbachev and later expressed his gratitude during a private
   meeting in the Oval Office.},
