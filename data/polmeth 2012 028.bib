@article{ WOS:000306784400010,
Author = {Van Rie, Tim and Marx, Ive},
Title = {The European Union at Work? The European Employment Strategy from Crisis
   to Crisis},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
