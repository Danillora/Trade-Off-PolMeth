@article{ WOS:000210797200001,
Author = {Omand, David y and Bartlett, Jamie and Miller, Carl},
Title = {Introducing Social Media Intelligence (SOCMINT)},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2012},
Volume = {27},
Number = {6},
Pages = {801-823},
Abstract = {We introduce the latest member of the intelligence family. Joining
   IMINT, HUMINT, SIGINT and others is `SOCMINT' - social media
   intelligence. In an age of ubiquitous social media it is the
   responsibility of the security community to admit SOCMINT into the
   national intelligence framework, but only when two important tests are
   passed. First, that it rests on solid methodological bedrock of
   collection, evidence, verification, understanding and application.
   Second, that the moral hazard it entails can be legitimately managed.
   This article offers a framework for how this can be done.},
