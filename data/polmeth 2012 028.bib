@article{ WOS:000303256400002,
Author = {Peterson, V. Spike},
Title = {Rethinking Theory INEQUALITIES, INFORMALIZATION AND FEMINIST
   QUANDARIES},
Journal = {INTERNATIONAL FEMINIST JOURNAL OF POLITICS},
Year = {2012},
Volume = {14},
Number = {1},
Pages = {5-35},
Abstract = {`Capitalist racist patriarchy' is how Zillah Eisenstein ( 1998)
   characterizes global inequalities and the hierarchies of `difference'
   they constitute. This article assumes that feminist theory aims not only
   to `empower women' but to advance critical analyses of intersecting
   structural hierarchies; that this entails not only a critique of
   patriarchy but its complex conjunction with capitalism and racism; and
   that such critique requires rethinking theory. Through a critical lens
   on devalued ('feminized') informal work worldwide, the article explores
   how positivist, modernist and masculinist commitments variously operate
   in prevailing theories of informality - including those of feminists
   with the effect of impeding both intersectional analyses and more
   adequate critiques of capitalist racist patriarchy.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
