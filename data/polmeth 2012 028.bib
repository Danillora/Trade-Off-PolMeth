@article{ WOS:000306994800001,
Author = {Bacon, Edwin},
Title = {Writing Russia's Future: Paradigms, Drivers, and Scenarios},
Journal = {EUROPE-ASIA STUDIES},
Year = {2012},
Volume = {64},
Number = {7},
Pages = {1165-1189},
Abstract = {The development of prediction and forecasting in the social sciences
   over the past century and more is closely linked with developments in
   Russia. The Soviet collapse undermined confidence in predictive
   capabilities, and scenario planning emerged as the dominant
   future-oriented methodology in area studies, including the study of
   Russia. Scenarists anticipate multiple futures rather than predicting
   one. The approach is too rarely critiqued. Building on an account of
   Russia-related forecasting in the twentieth century, analysis of two
   decades of scenarios reveals uniform accounts which downplay the
   insights of experts and of social science theory alike.},
