@article{ WOS:000302011800002,
Author = {Schatz, Edward and Maltseva, Elena},
Title = {Kazakhstan's Authoritarian ``Persuasion{''}},
Journal = {POST-SOVIET AFFAIRS},
Year = {2012},
Volume = {28},
Number = {1},
Pages = {45-65},
Month = {JAN-MAR},
Abstract = {Using post-Soviet Kazakhstan as a conceptual point of departure, this
   article considers the role that proactive framing and persuasion play in
   ensuring regime survival in soft authoritarian contexts. Drawing on
   interviews, opinion polls, news media, and the scholarly literature, the
   authors use three examples-Kazakhstan's OSCE bid, the global financial
   crisis, and ``Rakhatgate{''}-that highlight the regime's varying
   proportions of persuasive and coercive efforts. The ways a soft
   authoritarian leader responds to potentially threatening events are
   examined. Non-material sources of regime durability are analyzed as
   essentials for understanding authoritarian regime dynamics and, by
   implication, for developing a full theory of regime change.},
