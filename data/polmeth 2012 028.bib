@article{ WOS:000305208000006,
Author = {Klinke, Ian},
Title = {Postmodern Geopolitics? The European Union Eyes Russia},
Journal = {EUROPE-ASIA STUDIES},
Year = {2012},
Volume = {64},
Number = {5},
Pages = {929-947},
Abstract = {The discourse on EU-Russia relations amongst practitioners, think-tank
   experts, journalists and academics has congealed around a
   postmodern-modern binary. It is frequently argued that whereas Russia is
   caught up in a `modern' framework of fixed territory, national identity
   and traditional geopolitics, the European Union is driven by a
   `postmodern' spatial mindset that transcends these `backward' values.
   This article argues that the EU's supposed postmodern geopolitics
   remains enmeshed in a very modern temporality-a consciousness of time
   that valorises the present over the past. It also detects a problematic
   disillusion with the postmodern and questions its implicit normativity.},
