@article{ WOS:000303820900003,
Author = {Zimin, Dmitry},
Title = {How Can Foreign Companies Influence Russia's Economic Course? The Cases
   of Finnish Firms Fortum and Neste},
Journal = {POST-SOVIET AFFAIRS},
