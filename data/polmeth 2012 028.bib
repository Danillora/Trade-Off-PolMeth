@incollection{ WOS:000306699000017,
Author = {Ruan Zongze},
Editor = {Tuo, C},
Title = {AN ANALYSIS OF ANTI-GLOBALIZATION MOVEMENTS FROM THE PERSPECTIVE OF
   ``DEMOCRATIC DEFICIT{''}},
Booktitle = {CHINESE PERSPECTIVES ON GLOBALIZATION AND AUTONOMY},
Series = {Issues in Contemporary Chinese Thought and Culture},
Year = {2012},
Volume = {3},
Pages = {329-345},
Abstract = {World politics and economic relations have undergone profound and
   drastic adjustments since 1990s while an eye-catching phenomenon
   occurred: globalization and increasing anti-globalization movements.
   Since the World Trade Organization suffered the first large-scale
   protest by anti-globalization people in Seattle in 1999, more than 10
   similar ``anti-globalization{''} protests have been staged with
   increasing scale and better organizing. Though such protests were once
   lessened after the 9/11 terrorist attacks, they were up again during the
   following World Economic Forum held in New York at the end of 2001 and
   the European Union Summit held in Spain in February, 2002. Various signs
   showed that Western countries' ``anti-globalization front{''} was
   ubiquitous. These protest activities used to be spontaneous and
   orderless, but now they are well organized and planned, evolving toward
   another ``globalization of anti-globalization movements{''}. In the
   foreseeable future, these protests will remain closely related to
   globalization and have a significant influence on the world. Because the
   central stage of globalization is in developed countries, the driving
   force behind anti-globalization movements also comes from Western
   countries. Therefore, this article is to analyze the rising
   anti-globalization movements from the perspective of ``democratic
   deficit{''}.},
