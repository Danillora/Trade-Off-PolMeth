@article{ WOS:000306373300002,
Author = {Brandt, Patrick T. and Sandler, Todd},
Title = {A Bayesian Poisson Vector Autoregression Model},
Journal = {POLITICAL ANALYSIS},
Year = {2012},
Volume = {20},
Number = {3},
Pages = {292-315},
Month = {SUM},
Abstract = {Multivariate count models are rare in political science despite the
   presence of many count time series. This article develops a new Bayesian
   Poisson vector autoregression model that can characterize endogenous
   dynamic counts with no restrictions on the contemporaneous correlations.
   Impulse responses, decomposition of the forecast errors, and dynamic
   multiplier methods for the effects of exogenous covariate shocks are
   illustrated for the model. Two full illustrations of the model, its
   interpretations, and results are presented. The first example is a
   dynamic model that reanalyzes the patterns and predictors of superpower
   rivalry events. The second example applies the model to analyze the
   dynamics of transnational terrorist targeting decisions between 1968 and
   2008. The latter example's results have direct implications for
   contemporary policy about terrorists' targeting that are both novel and
   innovative in the study of terrorism.},
