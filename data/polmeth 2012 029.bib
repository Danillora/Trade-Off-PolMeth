@article{ WOS:000302660600002,
Author = {Underdal, Arild},
Title = {Strategies in international regime negotiations: reflecting background
   conditions or shaping outcomes?},
Journal = {INTERNATIONAL ENVIRONMENTAL AGREEMENTS-POLITICS LAW AND ECONOMICS},
Year = {2012},
Volume = {12},
