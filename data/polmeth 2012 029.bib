@article{ WOS:000308978300012,
Author = {Sokolova, Natalia},
Title = {Co-opting Transmedia Consumers: User Content as Entertainment or `Free
   Labour'? The Cases of STALKER and Metro 2033},
Journal = {EUROPE-ASIA STUDIES},
Year = {2012},
Volume = {64},
Number = {8, SI},
Pages = {1565-1583},
Abstract = {The expansion of new media and the advent of Web 2.0 has resulted in
   dramatic changes in consumers' roles, turning them into `prosumers':
   co-participants in cultural production. This essay considers the new
   situation through analysis of transmedia phenomena produced
   simultaneously by companies and consumers' activity. The analysis of two
   Russian transmedial projects: S.T.A.L.K.E.R. and Metro 2033, reveals a
   paradox in this phenomenon testifying to the mass expansion and boom in
   creative practices (rooted, to a great extent, in Soviet cultural
   traditions) and to their total commodification at the same time.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
