@article{ WOS:000212629200007,
Author = {Tudoroiu, Theodor},
Title = {Red Seas: A Study in Revolutionary Contagion},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2012},
Volume = {20},
Number = {3},
Pages = {337-357},
Abstract = {This article tries to identify the key elements that determine the
   success or failure of revolutionary contagion processes. Using three
   case studies of mutinies that took place in the first half of the
   twentieth century aboard fleets operating in the Black Sea and the
   Eastern Mediterranean, it concludes that the result of the revolutionary
   contagion depends mainly on the quality of the ideological and
   organizational effort undertaken by the primary revolution and its
   affiliated revolutionaries in the target society. A successful process
   of revolutionary contagion, however, is a necessary but not sufficient
   condition for the victory of the revolution itself. The 2011 Libyan
   rebellion is used to test these findings.},
