@article{ WOS:000301068300007,
Author = {Chamberlain, Adam},
Title = {A Time-Series Analysis of External Efficacy},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2012},
Volume = {76},
Number = {1},
Pages = {117-130},
Month = {SPR},
Abstract = {The literature on political efficacy has long grappled with this
   question: what is the difference between internal and external political
   efficacy, and what are the best ways to measure these concepts? Though a
   quality measure of internal efficacy has been devised, external
   efficacy, as measured by the National Elections Studies (NES), is still
   questioned. In this study, a lagged dependent variable model is used to
   test the measure from an aggregate and dynamic perspective. The results
   show that aggregate external efficacy is not currently responsive to
   changes in the political environment. This leads to the conclusion that
   the measure of external efficacy supported by the NES needs to be
   studied more thoroughly.},
