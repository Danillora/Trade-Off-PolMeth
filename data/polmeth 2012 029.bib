@article{ WOS:000311383400006,
Author = {Meseguer, Covadonga and Javier Aparicio, Francisco},
Title = {Migration and Distributive Politics: The Political Economy of Mexico's
   3x1 Program},
Journal = {LATIN AMERICAN POLITICS AND SOCIETY},
Year = {2012},
Volume = {54},
Number = {4},
Pages = {147-178},
Month = {WIN},
Abstract = {Mexico's 3 x 1 Program for Migrants is a matching grant scheme that
   seeks to direct the money sent by migrant organizations abroad to the
   provision of public and social infrastructure and to productive projects
   in migrants' communities of origin. To this end, the municipal, state,
   and federal administrations match the amount sent by hometown
   associations by 3 to 1. This article explores the impact on the
   operation of the 3 x 1 of a particular facet of Mexican political life:
   its recent democratization and the increasing political fragmentation at
   the municipal level. The study finds a lower provision of public
   projects in jurisdictions where a high number of political parties
   compete. This finding casts doubt on the claim that policy interventions
   such as the 3 x 1 Program actually improve local public goods provision
   at the local level under increasing political competition.},
