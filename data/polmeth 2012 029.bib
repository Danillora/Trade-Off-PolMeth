@article{ WOS:000311254600001,
Author = {Koesel, Karrie J. and Bunce, Valerie J.},
Title = {Putin, Popular Protests, and Political Trajectories in Russia: A
   Comparative Perspective},
Journal = {POST-SOVIET AFFAIRS},
Year = {2012},
Volume = {28},
Number = {4},
Pages = {403-423},
Month = {OCT-DEC},
Abstract = {During December 2011, large-scale, anti-regime protests erupted across
   Russia. What do the Russian uprisings reveal about popular challenges to
   authoritarian rule and the political trajectory of contemporary Russian
   authoritarianism? This study identifies two models of citizen
   mobilization against authoritarian leaders, one focusing on street-based
   protests and the other using elections as the key arena for launching
   popular challenges. Three factors encouraging both models are analyzed:
   declining public support for the regime; growing evidence that leaders
   are no longer invincible; and changing political opportunities for the
   opposition. The extent to which these conditions are present in Russia
   and what this ledger suggests about plausible political trajectories
   there are evaluated.},
