@article{ WOS:000298087400006,
Author = {Nagle, John and Clancy, Mary-Alice C.},
Title = {Constructing a shared public identity in ethno nationally divided
   societies: comparing consociational and transformationist perspectives},
Journal = {NATIONS AND NATIONALISM},
Year = {2012},
Volume = {18},
Number = {1},
Pages = {78-97},
Month = {JAN},
Abstract = {. In order to bolster sustainable peace building in violently divided
   societies, a normative suggestion is that efforts should be made to
   construct a shared public identity that overarches ethnic divisions. A
   number of different centripetal/transformationist processes are
   identified as engineering a shared identity in comparison to
   consociational arrangements, which are accused of institutionalising
   ethnic differences and perpetuating conflict. These transformationist
   approaches essentially rest on the premise that because ethnicity is
   constructed it can be reconstructed into new, shared forms. Looking at
