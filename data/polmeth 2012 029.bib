@article{ WOS:000306994800006,
Author = {Henry, Laura A. and Sundstrom, Lisa McIntosh},
Title = {Russia's Climate Policy: International Bargaining and Domestic
   Modernisation},
Journal = {EUROPE-ASIA STUDIES},
Year = {2012},
Volume = {64},
Number = {7},
Pages = {1297-1322},
Abstract = {This article accounts for the gap between Russia's weak initial
   implementation of the Kyoto Protocol and its more active engagement in
   climate policy during the Medvedev presidency. We examine the
   intersection of climate policy and broader efforts to modernise Russia's
   economy, drawing attention to synergies between domestic and
   international politics. We argue that international factors alone do not
   explain the change in climate policy as they have remained relatively
   constant. Instead, greater attention toward climate policy results from
   efforts to introduce new technologies and increase energy efficiency,
   spurred by the recent financial crisis and a shift in domestic policy
   priorities associated with the Medvedev presidency.},
