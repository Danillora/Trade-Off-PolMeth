@article{ WOS:000300584900004,
Author = {Ladly, Sarah Davidson},
Title = {Border carbon adjustments, WTO-law and the principle of common but
   differentiated responsibilities},
Journal = {INTERNATIONAL ENVIRONMENTAL AGREEMENTS-POLITICS LAW AND ECONOMICS},
