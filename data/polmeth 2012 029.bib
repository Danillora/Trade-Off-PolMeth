@article{ WOS:000302693100001,
Author = {Negoita, Marian and Block, Fred},
Title = {Networks and Public Policies in the Global South: The Chilean Case and
   the Future of the Developmental Network State},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
