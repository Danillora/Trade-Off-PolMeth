@article{ WOS:000208958800002,
Author = {Marston, Hunter},
Title = {Bauxite Mining in Vietnam's Central Highlands: An Arena for Expanding
   Civil Society?},
Journal = {CONTEMPORARY SOUTHEAST ASIA},
