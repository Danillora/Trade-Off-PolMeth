@article{ WOS:000309684100010,
Author = {Shaw, Greg M. and Gaffey, Laura},
Title = {THE POLLS-TRENDS AMERICAN PUBLIC OPINION ON ECONOMIC INEQUALITY, TAXES,
   AND MOBILITY: 1990-2011},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2012},
Volume = {76},
Number = {3},
Pages = {576-596},
Month = {FAL},
Abstract = {As the Great Recession has unfolded in the United States, Occupiers, Tea
   Partiers, and even the Congressional Budget Office have brought
   discussion of economic inequality and government intervention to the
   forefront. An examination of polls focused on inequality, taxes, and
   mobility conducted between 1990 and 2011 reveals that American public
   opinion has remained fairly stable on these issues, despite changing
   political and economic conditions. Though subtle responses to events are
   evident, such as slightly greater support for income taxes, there have
   been no dramatic shifts of public opinion on these issues. Economic
   inequality, the government's role in redistribution, and taxation
   policies will likely remain divisive political issues in coming years in
   light of no decisive evolution of public opinion on how to address
   growing economic inequality.},
