
@article{ WOS:000298087400010,
Author = {Miller-Idrissn, Cynthia and Rothenbergnn, Bess},
Title = {Ambivalence, pride and shame: conceptualisations of German nationhood},
Journal = {NATIONS AND NATIONALISM},
Year = {2012},
Volume = {18},
Number = {1},
Pages = {132-155},
Month = {JAN},
Abstract = {. This article examines complex everyday expressions and understandings
   of nationhood in Germany, focusing on citizens' articulations of
   national pride and their relationship with the nation. Through an
   analysis of ninety semi-structured interviews with ordinary Germans
   conducted between 2000 and 2002, we argue that the prevailing,
   elite-centred approach to studying nationhood has not adequately
   captured the complex relationships that individuals have to the nation.
   We examine how individuals actively process and interpret nationhood in
   ways that reveal ambivalence, confusion and contradictory emotions
   towards the nation. Such individual variation is not neatly captured by
   official, elite, public or institutional presentations of the nation. We
   argue for further research on everyday understandings of nationhood and
   on ordinary people's views on national pride and national identity.},
