@article{ WOS:000304807100004,
Author = {Ames, Barry and Garcia-Sanchez, Miguel and Smith, Amy Erica},
Title = {Keeping Up with the Souzas: Social Influence and Electoral Change in a
   Weak Party System, Brazil 2002-2006},
Journal = {LATIN AMERICAN POLITICS AND SOCIETY},
Year = {2012},
Volume = {54},
Number = {2},
Pages = {51-78},
Month = {SUM},
Abstract = {Despite weak partisanship and considerable political change in the wake
   of the 2002 election, three-quarters of Brazilian voters supported a
   presidential candidate in 2006 from the same party they had backed in
   2002. This article assesses the factors causing both electoral stability
   and electoral change with a transition model, a model testing whether
   the effects of respondents' evaluative criteria depend on their initial
   vote choices. Social contextpersonal discussion networks, neighborhood
   influences, and the interactions of social networks and municipal
   contextis the major force promoting stability and change, while the
   impact of partisanship is limited to a small share of voters.},
