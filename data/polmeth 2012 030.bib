@article{ WOS:000311670600007,
Author = {Kam, Cindy D. and Simas, Elizabeth N.},
Title = {Risk Attitudes, Candidate Characteristics, and Vote Choice},
Journal = {PUBLIC OPINION QUARTERLY},
