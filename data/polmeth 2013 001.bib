@article{ WOS:000215603700005,
Author = {Mera Sotelo, Adriana Victoria},
Title = {Experimenting with conflict sensitivity in a peacebuilding laboratory},
Journal = {CIUDAD PAZ-ANDO},
Year = {2013},
Volume = {6},
Number = {1},
Pages = {59-78},
Abstract = {By analyzing the context where the Program of the Second Peace
   Laboratory as developed, and focusing on the Alto Patia Region, this
   paper describes the process of closure of this program and explores two
   cases of projects implemented under this agreement through a conflict
   sensitivity approach. With these elements we construct a study that aims
   to evaluate the program and identify lessons learned from experiences of
   the project. We end with a series of recommendations, from the viewpoint
   of sensitivity to conflict and peacebuilding, in order to optimize the
   impact reached, and to facilitate the exit strategy of the European
   Union and the National Government with aims to contributing to peace
   building in Colombia.},
