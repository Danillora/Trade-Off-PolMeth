@article{ WOS:000210193400001,
Author = {Doran, Evan and Loefgren, Hans},
Title = {Drug promotion in Australia: Policy contestation and the tightening of
   regulation},
Journal = {AUSTRALIAN REVIEW OF PUBLIC AFFAIRS},
Year = {2013},
Volume = {11},
Number = {2},
Pages = {19-41},
Month = {APR},
Abstract = {This paper describes developments in Australia's regulation of
   prescription drug marketing and promotion. We show that the
   pharmaceutical industry has proved less capable of shaping the
   regulation of promotion than other areas of pharmaceutical policy.
   Public health advocates have effectively highlighted the negative impact
   of promotion on quality use of medicines. While consumers have long been
   assumed to be in need of protection from drug promotion, it is now
   accepted that marketing to medical professionals should also be more
   closely controlled. Government has responded by tightening such
   regulation but has stopped short of ending industry self-regulation.},
