@article{ WOS:000319171600005,
Author = {Closa, Carlos},
Title = {National Higher Courts and the Ratification of EU Treaties},
Journal = {WEST EUROPEAN POLITICS},
Year = {2013},
Volume = {36},
Number = {1},
Pages = {97-121},
Month = {JAN 1},
Abstract = {Courts in a given institutional context are aware of the implementation
   costs of their decisions. Contrary to some interpretations of Higher
   Courts' reasoning, these have decided, in most cases, that EU treaties
   are compatible with national constitutional law. In cases on the
   constitutionality of EU treaties, a negative decision on a given treaty
   offers only two possible implementation options: either the treaty is
   re-negotiated or the constitution is modified. The most rigid
   constitutional revision procedures involve citizens either by means of a
   referendum and/or an intervening election. Therefore, and despite a
   significant number of appeals, Higher Courts have not ruled that EU
   reform treaties require constitutional revision for their ratification
   when citizens intervene in constitutional amendment procedures},
