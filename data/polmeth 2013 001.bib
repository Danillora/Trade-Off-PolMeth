@article{ WOS:000319128500002,
Author = {Hess, Steve},
Title = {From the Arab Spring to the Chinese Winter: The institutional sources of
   authoritarian vulnerability and resilience in Egypt, Tunisia, and China},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
