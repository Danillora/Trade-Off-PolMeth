@article{ WOS:000447052900008,
Author = {Tezcur, Gunes Murat},
Title = {Prospects for Resolution of the Kurdish Question: A Realist Perspective},
Journal = {INSIGHT TURKEY},
Year = {2013},
Volume = {15},
Number = {2},
Pages = {69-84},
Month = {SPR},
Abstract = {The developments in early 2013 generated expectations that the almost
   three decades old armed conflict between the Turkish state and PKK would
   eventually come to an end. This article adopts a skeptical position and
   identifies two principal factors that make a peaceful settlement a
   distant possibility. First, the current military situation is a
   stalemate that is not ripe for peace. The costs of the conflict remain
   highly tolerable for both sides. Next, huge differences separate what
   the Turkish government is willing to deliver and what the Kurdish
   insurgency is willing to accept for disarmament. In particular, the PKK
   has no incentive to accept disarmament and demobilization given current
   geopolitical dynamics conducive to Kurdish self-rule.},
