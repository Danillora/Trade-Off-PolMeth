@article{ WOS:000316992200019,
Author = {Hill, Walter W.},
Title = {Should More Polls Be Interpreted as Too Close to Call?},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2013},
Volume = {46},
Number = {2},
Pages = {329-332},
Month = {APR},
Abstract = {During political campaigns the spread between the popularity of the
   candidates is a common metric capturing the state of the horse-race
   feature of the campaign. One candidate is said to be ahead of another by
   an indicated number of percentage points. If the difference is less than
   the margin of error, the race is considered too close to call. In
   two-person races, however, the spread corresponds to a much smaller
   confidence level than is usually reported because the two numbers used
   to compute the spread are not independent. The size of the confidence
   interval that is typically reported is incorrect by a factor of two.
   Therefore, some spreads that are reported as decisive are races too
   close to call.},
