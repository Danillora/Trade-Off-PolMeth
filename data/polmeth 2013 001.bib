@article{ WOS:000214865000006,
Author = {Baker, Stephanie Alice and Rowe, David},
Title = {The power of popular publicity: new social media and the affective
   dynamics of the sport racism scandal},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2013},
Volume = {6},
Number = {3, SI},
Pages = {441-460},
Abstract = {Sociologists have tended to take insufficient account of the importance
   of emotions to the social power of the institution of media,
   particularly as altered by the emergence of social media in the current
   media ecology. This paper compensates for this neglect by means of a
   brief illustrative case study of the effect of social media on the
   public reception of the 2011 Sepp Blatter racism scandal and of other
   `race-related' scandals in the UK. In proposing media scandals' wider
   sociological significance regarding the dynamic, multi-accented
   relationships between emotions and power, it analyses how England's
   prevailing climate of `postcolonial guilt' was reinforced and conveyed
   through social media networks.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
