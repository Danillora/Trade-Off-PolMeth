@article{ WOS:000323535800003,
Author = {Carlos Castillo, Juan and Madero-Cabib, Ignacio and Salamovich, Alan},
Title = {Partisan Cleavages and Changes in Distributive Preferences in Chile},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2013},
Volume = {33},
Number = {2},
Pages = {469-488},
Abstract = {Beliefs about how goods and resources should be distributed in society
   constitute a central element in the identification with political
   parties. In this sense, the preference for a more or less active role of
   the state in redistribution is expected to be related with different
   party identifications and with the left-right continuum. The present
   article challenges this assumption, proposing that processes of
   ideological destructuration have led to that party identification does
   not constitute a current political cleavage in Chile. The data to be
   analyzed correspond to the International Social Survey Programme survey
   implemented in Chile in 1999 and 2009. Results indicate there are scarce
   differences in distributional preferences by the identification with
   political parties.},
