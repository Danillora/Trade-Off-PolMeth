@article{ WOS:000214864700006,
Author = {Sepos, Angelos},
Title = {Imperial power Europe? The EU's relations with the ACP countries},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2013},
Volume = {6},
Number = {2},
Pages = {261-287},
Abstract = {By drawing from theories of imperialism and power, this paper examines
   the agential and structural forms of the EU's power projection in the
   world with a particular focus on the African, Caribbean and Pacific
   Countries. As evident in the areas of trade, agriculture, energy and
   security, the EU operates within a hierarchical, centre-periphery
   relationship with these countries and projects forms of power such as
   coercion, mobilization of bias, manipulation, exploitation but also
   attraction, features that are all associated with imperialism. This has
   broader implications on how we understand the EU as a polity as well as
   how power is embedded in the concept of imperialism.},
