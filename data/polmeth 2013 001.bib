@article{ WOS:000447063200011,
Author = {Karagol, Erdal Tanas},
Title = {The Turkish Economy During the Justice and Development Party Decade},
Journal = {INSIGHT TURKEY},
Year = {2013},
Volume = {15},
Number = {4},
Pages = {115-129},
Month = {FAL},
Abstract = {During the 1990s, political uncertainties in Turkey had negative effects
   that left the economy vulnerable to public and foreign debt due to high
   inflation, high budget deficit and high current account deficit.
   Coalition governments failed to address these problems. Following its
   rise to power in 2002, the AK Party developed a new perspective for the
   economy, politics and foreign policy collectively referred as the New
   Turkey. The government emphasized fiscal discipline, structural
   transformation and privatization. During this period, Turkey rapidly
   recovered from the negative effects of the 2001 financial crisis and
   reached a steady growth rate. The country also survived the 2008 global
   crisis with minimum damage. The government seeks to meet its targets for
   the centennial of the Republic's establishment.},
