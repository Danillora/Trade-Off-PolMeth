@article{ WOS:000317059700001,
Author = {Cammack, Daniela},
Title = {Aristotle on the Virtue of the Multitude},
Journal = {POLITICAL THEORY},
Year = {2013},
Volume = {41},
Number = {2},
Pages = {175-202},
Month = {APR},
Abstract = {It is generally believed that one argument advanced by Aristotle in
   favor of the political authority of the multitude is that large groups
   can make better decisions by pooling their knowledge than individuals or
   small groups can make alone. This is supported by two analogies, one
   apparently involving a ``potluck dinner{''} and the other aesthetic
   judgment. This article suggests that that interpretation of Aristotle's
   argument is implausible given the historical context and several
   features of the text. It argues that Aristotle's support for the rule of
   the multitude rested not on its superior knowledge but rather on his
   belief that the virtue of individuals can be aggregated and even
   amplified when they act collectively. This significantly alters our
   understanding of Aristotle's political thought and presents a powerful
   alternative to the epistemic defenses of mass political activity popular
   today.},
