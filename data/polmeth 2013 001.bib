@article{ WOS:000330449200004,
Author = {Sapir, Eliyahu V. and Sullivan, Jonathan and Veen, Tim},
Title = {Scale Matters: Addressing the Limited Robustness of Findings on Negative
   Advertising},
Journal = {JAPANESE JOURNAL OF POLITICAL SCIENCE},
Year = {2013},
Volume = {14},
Number = {4},
Pages = {521-541},
Month = {DEC},
Abstract = {Negative campaign advertising is a major component of the electoral
   landscape, and has received much attention in the literature. In many
   studies, political scientists have tried to explain why some campaign
   ads contain more negative messages than others and to identify the
   determinants of this form of campaign behavior. In recent years, a
   number of studies have acknowledged the differences between alternative
   measures of negativity, but, in most cases, it is assumed that since
   these measures are highly correlated, they are unidimensional and
   essentially interchangeable. In this article, we argue that much of the
   debate in the literature over negative campaigning is a result of
