@article{ WOS:000218585000001,
Author = {Dorff, Cassy and Ward, Michael D.},
Title = {Networks, Dyads, and the Social Relations Model},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2013},
Volume = {1},
Number = {2},
Pages = {159-178},
Month = {DEC},
Abstract = {Quantitative international relations scholarship has focused on analysis
   of the so-called dyad. Few studies have given serious thought to the
   definition of a dyad, or to the implications that flow from such a
   conceptualization. This article argues that the current approach to
   dyadic analysis is necessarily incomplete and at times induces
   incoherent pictures of the ebb and flow of interactions among actors in
   global politics and economics. It presents the Social Relations Model as
   a systematic way of analyzing the dependencies inherent in dyadic data.
   The study uses this model to examine militarized interstate disputes
   from 1816 to 2001, the trade network of 2000 and reciprocity between
   enemies in the treatment of prisoners of war.},
