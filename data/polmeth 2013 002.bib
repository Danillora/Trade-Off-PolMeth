@article{ WOS:000319171600008,
Author = {Macklin, Graham},
Title = {Transnational Networking on the Far Right: The Case of Britain and
   Germany},
Journal = {WEST EUROPEAN POLITICS},
Year = {2013},
Volume = {36},
Number = {1},
Pages = {176-198},
Month = {JAN 1},
Abstract = {This article examines an aspect of transnational activity that is often
   neglected, namely the transnational activism of the far right. It uses a
   case study of the British National Party (BNP) and the
   Nationaldemokratische Partei Deutschlands (NPD German National
   Democratic Party) in order to explore how its leaders and activists
   share ideas and information, policy and praxis, and how this is employed
   in the development of a strategic master frame' that they believe will
   allow them to overcome the limits of purely national activity in a
   globalised world. It explores the evolution of the links between the two
   parties, their nature and indeed the limits of such transnational
   activism upon national frames' in order to present a historical overview
   of the diffusion of ideology and strategy within the contemporary
   European far right.},
