@article{ WOS:000338803600007,
Author = {Spurny, Matej},
Title = {Reliability and the Border: The Discourse of the Czech Borderlands,
   1945-49},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2013},
Volume = {42},
Number = {1},
Pages = {83-94},
Abstract = {Immediately after the World War II, the new borderlands population in
   Czech Lands was confronted with the notion of a community of ``reliable
   citizens{''} capable of protecting the frontier and, consequently, the
   security of the nation and the state. Its logical counterpart was the
   ``cleansing{''} and removal of all unwanted and ``unreliable{''}
   inhabitants. This was not only state policy: Even more radical demands
   for resettlement where articulated by the local Czech-speaking
   population, especially by the new settlers in the borderlands. In the
   first two post-war years, these demands were directed mostly against the
   remaining Germans and other non-Slav ethnic groups. With time, however,
   it also affected other citizens and social groups. The rhetoric of
   recomposing society and eliminating ``alien{''} elements remained
   similar after the Communist takeover in February 1948. This aspect of
   the Communist dictatorship was not an import, but rather an answer to a
   demand for ``purification{''}, which had been articulated by a large
   part of the society.},
Publisher = {FACULTAS VERLAGS \& BUCHHANDELS AG},
Address = {STOLBERGGASSEE 26, VIENNA, A-1050, AUSTRIA},
Type = {Article},
Language = {English},
Affiliation = {Spurny, M (Corresponding Author), Charles Univ Prague, Inst Contemporary Hist, Prague, Czech Republic.
   Charles Univ Prague, Inst Contemporary Hist, Prague, Czech Republic.},
ISSN = {1615-5548},
Keywords = {borderlands; communist dictatorship; ethnicity},
Research-Areas = {Government \& Law},
Web-of-Science-Categories  = {Political Science},
Author-Email = {matej.spurny@seznam.cz},
