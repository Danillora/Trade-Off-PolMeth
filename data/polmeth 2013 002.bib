@article{ WOS:000213001700001,
Author = {Hankivsky, Olena},
Title = {Gender Mainstreaming: A Five-Country Examination},
Journal = {POLITICS \& POLICY},
Year = {2013},
Volume = {41},
Number = {5},
Pages = {629-655},
Month = {OCT},
Abstract = {Although gender mainstreaming (GM) has been the international norm for
   working toward gender equality in policies and practices since the
   Fourth World Conference on Women in Beijing 1995, its impact has been
   uneven. The lack of substantive results has led to debate surrounding
   GM's capacity for engendering meaningful policy change. This article
   synthesizes the input of key GM stakeholders (within government,
   academia, and nongovernmental organizations) across Canada, Australia,
   Sweden, the United Kingdom, and Ukraine. It discusses national
   approaches to mainstreaming gender, identifies key factors inhibiting
   andlor promoting GM, and proposes how current strategies can be
   modified, strengthened and/ or replaced by alternative approaches.
   Central to the analysis is the question as to whether GM in current or
   expanded versions has the potential to addresses the wide variety of
   diversities among nation state populations.},
