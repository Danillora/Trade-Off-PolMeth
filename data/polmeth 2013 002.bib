@article{ WOS:000212441800003,
Author = {Stump, Jacob L.},
Title = {Exploring Politics and Government With Popular Culture: Justifications,
   Methods, Potentials, and Challenges in Introductory Political Science
   Courses},
Journal = {JOURNAL OF POLITICAL SCIENCE EDUCATION},
Year = {2013},
Volume = {9},
Number = {3},
Pages = {292-307},
Abstract = {For an introductory-level course (Politics and Government) mostly
   composed of nonpolitical science majors, I use a combination of
   nontraditional texts, including novels and films, along with primary
   documents. This article briefly justifies the value of nontraditional
   texts for exploring politics and government. It then sketches out the
   nontraditional texts used in the course and how they relate to politics
   and government. I also describe some methods I use in combining
   nontraditional texts with primary documents to highlight and explore
   important concepts and presentday political problems relevant for an
   introductory-level politics and government class. I close with a brief
   discussion of two problems I have encountered in employing such an
   approach.},
