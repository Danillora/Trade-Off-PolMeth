@article{ WOS:000330464600006,
Author = {Aligica, Paul Dragos and Tarko, Vlad},
Title = {Co-Production, Polycentricity, and Value Heterogeneity: The Ostroms'
   Public Choice Institutionalism Revisited},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
