@article{ WOS:000341680800005,
Author = {Laczo, Ferenc},
Title = {The Guiding Principle of Liberalism Boon and Bale of the Hungarian
   Intellectuals},
Journal = {OSTEUROPA},
Year = {2013},
Volume = {63},
Number = {4},
Pages = {29+},
Month = {APR},
Abstract = {Five former dissidents serve as example's for the broad spectrum of
   liberal intellectuals in Hungary in the early 1990s: Istvan Eorsi, Janos
   Kis, Gyorgy Konrad, Miklos Szabo, and Gaspar Miklos Tamas. Their goal
