@article{ WOS:000215063200012,
Author = {Lashaki, Abdollah Baei and Goudarzi, Masoumeh Rad and Amraei, Davood},
Title = {The Roots of Tension in South Caucasus: The Case of Iran-Azerbaijan
   Relationship},
