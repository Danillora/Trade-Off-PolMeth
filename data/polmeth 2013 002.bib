@article{ WOS:000436659600003,
Author = {Wong, John},
Title = {China's Economy: Rebounded in 2012, Gearing up for Rebalancing in 2013
   and Beyond},
Journal = {EAST ASIAN POLICY},
Year = {2013},
Volume = {5},
Number = {1},
Pages = {32-41},
Month = {JAN-MAR},
Abstract = {The Chinese economy experienced serious slowdown for the greater part of
   2012 as a result of export slump. It rebounded in Quarter 4 and ended
   2012 with around 7.8\% along with 2.6\% inflation. The slowdown signals
   the end of China's era of double-digit rates of growth and precipitated
   government efforts to step up macroeconomic rebalancing by boosting
   domestic demand-led growth, a departure from the past investment-driven
   growth. The year 2012 marks a critical shift in China's growth pattern.},
Publisher = {WORLD SCIENTIFIC PUBL CO PTE LTD},
