@article{ WOS:000318424100005,
Author = {Kalmoe, Nathan P.},
Title = {From Fistfights to Firefights: Trait Aggression and Support for State
   Violence},
Journal = {POLITICAL BEHAVIOR},
Year = {2013},
Volume = {35},
Number = {2},
Pages = {311-330},
Month = {JUN},
Abstract = {Aggression is a fundamental component of human behavior, yet is mostly
   absent from scholarship on mass political behavior. This study proposes
   and tests a theory of state violence attitudes in which citizens develop
   preferences from aggressive personality traits. In an original
   nationally-representative survey, trait aggression strongly predicts
   support for violent state policies, as does its subcomponent trait
   anger, rivaling the power of partisanship. More provocatively, the
   well-documented gender gap in state violence attitudes replicated here
   is not attributable to sex differences in aggressive personality. This
   work builds on recent advances in political personality research and
   highlights the important role of aggression in political behavior.},
