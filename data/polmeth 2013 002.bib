@article{ WOS:000420781700004,
Author = {La Mattina, Giulia},
Title = {``Civilization against civilized barbarism{''} : Slavic nationalisms in
   the Austrian thinking between Restoration and Revolution},
Journal = {STORIA DEL PENSIERO POLITICO},
Year = {2013},
Volume = {2},
Number = {2},
Pages = {259-282},
Month = {MAY-AUG},
Abstract = {This essay aims to describe the Austrian thinking over the rise of
   Slavic nationalisms during the Vormarz. Its purpose is to smooth the
   idea of a substantial indifference to national issues by the German
   population of the empire and to emphasize instead the elements of
   continuity in the political Austrian thought between Restoration and the
   1848 Revolution. The apologetic writings and the documentation of the
   movement Young Austria show that, as early as in Vormarz, the comparison
   with the Slavic revival promoted a wide rethinking on the idea of
   Austria and its role. The practical implications of this reflection will
   be verified only in 1848.},
