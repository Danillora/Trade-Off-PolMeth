@article{ WOS:000329983000001,
Author = {Howe, Stephen},
Title = {The Cruiser and the Colonist: Conor Cruise O'Brien's Writings on
   Colonialism},
Journal = {IRISH POLITICAL STUDIES},
Year = {2013},
Volume = {28},
Number = {4},
Pages = {487-514},
Month = {DEC 1},
Abstract = {Almost all commentary published thus far on Conor Cruise O'Brien's
   career and ideas has focused on his engagement with Irish affairs. Yet a
   great deal of his life and work was concerned with the politics of the
   late-colonial and postcolonial worlds, and with questions of imperialism
   and colonialism. This article surveys and analyses that involvement and
   those writings, exploring the major recurring themes in O'Brien's Third
   World' writings. Assessing the much-debated question of whether his
   views exhibited a clear, lifelong consistency as he himself claimed, or
   rather a sharp break from radical to conservative views, it finds
   (perhaps unsurprisingly) strong elements of both.},
