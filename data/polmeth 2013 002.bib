@article{ WOS:000217318800004,
Author = {Grossmana, Emiliano and Francoisb, Abel},
Title = {Who are the ministers of the Fifth Republic?},
Journal = {FRENCH POLITICS},
Year = {2013},
Volume = {11},
Number = {3},
Pages = {272-283},
Month = {SEP},
Abstract = {This article presents a descriptive analysis of French ministers since
   the beginning of the Fifth Republic. We find that ministers are
   generally appointed aged about 50 years; that women's representation has
   increased, but is still less than 30 per cent in the most recent
   cabinets, and that women often hold more junior positions; that the
   proportion of ministers who were members of parliament varies, but
   rarely falls below 60 per cent; that the average minister serves around
   15 months; and that ministerial tenure ends mainly due to collective
   reasons (80 per cent), that is, the prime minister's resignation or the
   end of the legislature, rather than reasons relating to the minister
   individually.},
