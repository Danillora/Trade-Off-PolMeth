@article{ WOS:000436661600010,
Author = {Lau Siu-kai},
Title = {The Middle Class and Politics in Hong Kong since the Handover},
Journal = {EAST ASIAN POLICY},
Year = {2013},
Volume = {5},
Number = {4},
Pages = {107-116},
Month = {OCT-DEC},
Abstract = {Since the handover, Hong Kong's middle-class has transformed from a
   self-confident and complacent social group to a group stricken by
   anxiety and discontent. These changes have brought about middle-class
   radicalism, ``poisoned{''} Hong Kong's political atmosphere, deepened
   government-people conflict, and aggravated social and political
   friction. As the middle class still has vested interests in the existing
   institutional arrangements, middle-class instability is not likely to
   result in political turmoil in post-handover Hong Kong.},
