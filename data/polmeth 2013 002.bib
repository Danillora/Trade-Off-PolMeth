@article{ WOS:000217018100025,
Author = {Gunnarsdottir, Sigrun and Jonsdottir, Birna Gerdur},
Title = {Servant leadership and research in Iceland},
Journal = {ICELANDIC REVIEW OF POLITICS \& ADMINISTRATION},
