@article{ WOS:000324569300005,
Author = {Linhart, Eric},
Title = {Does an Appropriate Coalition Theory Exist for Germany? An Overview of
   Recent Office- and Policy-Oriented Coalition Theories},
Journal = {GERMAN POLITICS},
Year = {2013},
Volume = {22},
Number = {3},
Pages = {288-313},
Month = {SEP 1},
Abstract = {Recent coalition theories assume that parties are both office and policy
   motivated and thus use utility functions that combine both motivations.
   Although such utility functions may be adequate in general, a discussion
   on the theories' assumptions in detail is lacking. Such a discussion is
   necessary in order to find out which theory is in accordance with the
