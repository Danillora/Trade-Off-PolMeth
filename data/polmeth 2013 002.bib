@article{ WOS:000420158600004,
Author = {Nupen, Charles},
Title = {Mediation and conflict resolution in South and Southern Africa: A
   personal account of the past 30 years},
Journal = {AFRICAN JOURNAL ON CONFLICT RESOLUTION},
Year = {2013},
Volume = {13},
Number = {3},
Pages = {85-114},
Abstract = {This article is a personal account of my involvement with mediation and
   facilitation over the past 30 years.
   It does not purport to be a comprehensive or systematic account of the
   growing impact of these processes both in preventing and resolving
   conflict in our country and beyond. It therefore does not focus on
   organisations outside my own spheres of involvement, like the African
   Centre for the Constructive Resolution of Disputes and the Centre for
   Conflict Resolution, whose important contributions I readily
   acknowledge.
   The insights and experiences reflected in the piece nevertheless point
   to an ever widening application of the processes across a range of
   disciplines and tell something of the rich contribution they have made
   to making South Africa a better, and safer, place.},
