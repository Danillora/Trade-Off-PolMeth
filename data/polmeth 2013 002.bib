@article{ WOS:000410251500002,
Author = {Gaines, Brian J. and Taagepera, Rein},
Title = {How to Operationalize Two-Partyness},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2013},
Volume = {23},
Number = {4},
Pages = {387-404},
Abstract = {Because of the prominence of Duverger's law, a great deal of work has
   focused on assessing whether or not given election outcomes can be
   regarded as exhibiting two-party competition. The most common metric for
   assessment is whether the effective number of parties is close to two.
   We introduce two statistics that better measure conformity to
   two-partyness, briefly survey their logical properties, and then
