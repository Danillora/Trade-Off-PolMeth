@article{ WOS:000434545700002,
Author = {Tomba, Massimiliano},
Title = {Accumulation and time: Marx's historiography from the Grundrisse to
   Capital},
Journal = {CAPITAL AND CLASS},
Year = {2013},
Volume = {37},
Number = {3},
Pages = {355-372},
Month = {OCT},
Abstract = {This article investigates the way in which Marx combines different
   historiographical approaches in order to draft the possibility for both
   new forms of social relationship and a new anthropology. The classic
   unilinear and historicist paradigm and its idea of pre-modern,
   pre-capitalist and pre-political social forms does not help in
   understanding the interaction of different temporalities and the
   combination of formal and real subsumptions. The representation of
   ongoing accumulation requires different temporalities, which conflict
   and intertwine with one another.},
