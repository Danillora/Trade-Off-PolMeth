@article{ WOS:000212749400005,
Author = {Giebler, Heiko and Wessels, Bernhard},
Title = {Campaign Foci in European Parliamentary Elections: Determinants and
   Consequences},
Journal = {JOURNAL OF POLITICAL MARKETING},
Year = {2013},
Volume = {12},
Number = {1},
Pages = {53-76},
Abstract = {Personalization of politics is a major topic in discussions about
   elections and electoral behavior. Does the personalization of politics
   imply an individualization of campaigning as well? Up to now, this
   question has not been sufficiently dealt with. On basis of the 2009
   European Election Candidate Study carried out in all 27 member countries
   of the EU, three main research questions are addressed: (1) Are there
   significant differences with regard to candidate-and party-centered
   campaign strategies between countries or party families? (2) Which
   factors account for such differences? (3) Do preferences for a campaign
   strategy have consequences for campaign efforts of candidates and the
   means they use? Results show that the dominant feature is party-centered
   campaigning, but close to one-fifth of the candidates prefer
   candidate-centered campaigning. A number of hypotheses are tested with a
   multi-level model showing that candidates react to electoral and
   institutional incentives. It is a strategic choice to go for a more
   personalized campaign. This choice has consequences for campaign efforts
   and means used.},
