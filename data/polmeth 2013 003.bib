@article{ WOS:000317059500003,
Author = {Esquith, Stephen L.},
Title = {Reframing the Responsibilities of Bystanders through Film},
Journal = {POLITICAL THEORY},
Year = {2013},
Volume = {41},
Number = {1},
Pages = {33-60},
Month = {FEB},
Abstract = {Political responsibilities for systemic mass violence have been
   subordinated to the moral guilt and legal liability of perpetrators and
   collaborators, while the role of the bystander has been narrowly
   construed in terms of charitable rescue or negligence. This dominant
   victim-perpetrator framework ignores the complex political dimensions of
   bystander responsibilities for systemic mass violence, especially those
   responsibilities that stem from the benefits that bystanders receive.
   The films of Claude Lanzmann, Rithy Panh, and Yael Hersonski contain
