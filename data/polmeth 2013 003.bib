@article{ WOS:000457928400003,
Author = {Bliakher, Leonid},
Title = {ARE SPONTANEOUS ORDER AND POLICE STATE RECONCILABLE? State versus local
   community in small towns of the Russian Far East},
Journal = {POLITEIA-JOURNAL OF POLITICAL THEORY POLITICAL PHILOSOPHY AND SOCIOLOGY
   OF POLITICS},
Year = {2013},
Volume = {69},
Number = {2},
Pages = {50+},
Abstract = {The article discusses versions of spontaneous order that emerged in the
   small towns of the Far East against the background of the state control
   weakening, types of confrontation arising from the ``return{''} of the
   dirigiste state as well as the conditions under which such confrontation
   might end up with the interests reconciliation. It is shown that
   although state and local communities interests are rarely the same, such
   a situation does not necessarily produce conflict. The decisive factor
   here is an institutional opportunity to articulate community specialty
   and interests that in its turn hinges upon the availability of a legal
   and legitimate ``negotiator{''} as well as space for negotiations.
   However, the long-term alignment of interests is hindered by
   inarticulate interests of the state itself. Such circumstances make it
   extremely difficult to avoid the perception of the state version of the
   public weal as a form of structural violence.},
Publisher = {ZHURNAL POLITICHESKOI FILOSOFII \& SOTSIOLOGII POLITIKI POLITIYA},
