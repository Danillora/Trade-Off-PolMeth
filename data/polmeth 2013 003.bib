@article{ WOS:000338803600008,
Author = {Bayer, Joasef},
Title = {Emerging Anti-pluralism in New Democracies - the Case of Hungary},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2013},
Volume = {42},
Number = {1},
Pages = {95-110},
Abstract = {The signs of a definite anti-pluralist turn in the politics of some
   newly democratized countries in Europe are growing. The paper describes
   the present Hungarian political situation since the 2010 election. Due
   to a landslide electoral victory of the Hungarian Civic Union - Fidesz,
   the new government felt free to rearrange the whole constitutional and
   political system. Earlier institutions of control over the government
   have been restrained. The declared need for a ``strong state{''} in a
   crisis situation seemed to justify a resort to some authoritarian
   tendencies of the Hungarian past. The paper analyzes the political and
   ideological features that endanger the guaranties of rights and the
   achieved political pluralism. It deals with changes of the constitution
