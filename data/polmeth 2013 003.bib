@article{ WOS:000210672500007,
Author = {Hitt, Matthew},
Title = {Presidential Success in Supreme Court Appointments: Informational
   Effects and Institutional Constraints},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2013},
Volume = {43},
Number = {4, SI},
Pages = {792-813},
Month = {DEC},
Abstract = {Spatial models of Supreme Court appointments assume that the president
   knows the preferences of nominees and is constrained only by the
   ideology of the Senate. However, nominees vary in the amount of
   available information that can be used to determine their preferences. I
   find that justices who offered more information in the form of relevant
   professional experience at the time of nomination are more congruent
   with their appointing president. Institutional factors, such as
   polarization between the Senate and president, exert less influence on
   congruence. The president is, however, constrained from appointing
   highly experienced justices if the Senate and president are distant
   ideologically.},
