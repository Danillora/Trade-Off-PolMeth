@article{ WOS:000323584300008,
Author = {Zizumbo Colunga, Daniel},
Title = {Security Satisfaction and Citizens' Democratic Preferences: The
   Hard-Line Approach and Mexico},
Journal = {POLITICA Y GOBIERNO},
Year = {2013},
Number = {SI},
Pages = {187-219},
Abstract = {When will people prefer ``iron fist{''} governments to a government that
   acts with everyone's consent in mind? Democracies can and do implement
   policies that vary across a hard-line to soft-line continuum. I
   hypothesize that: a) citizens are more likely to prefer an authoritarian
   government when they are more satisfied with the outcomes yield by a
   hard-line policy and b) the effect of these outcomes on preferences will
   be accentuated for those that feel insecure. Data from Mexico collected
   by the Americas Barometer brings strong support for the first and
   moderate support for the second hypothesis. Evidence from more than 3
   000 face to face interviews collected by the Americas Barometer in
   Mexico during the years 2008 and 2010 shows that the phenomenon
   presented is not exclusive to the year 2010 but is particular to the
   government's hardline security policy.},
Publisher = {CENTRO DE INVESTIGACION Y DOCENCIA ECONOMICAS},
