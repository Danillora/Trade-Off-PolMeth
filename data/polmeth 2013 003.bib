@article{ WOS:000215512700013,
Author = {Gomez Martin, Carmen},
Title = {The contemporary Saharawi literature and its development in the Spanish
   migratory context},
Journal = {REVISTA DE INVESTIGACIONES POLITICAS Y SOCIOLOGICAS},
Year = {2013},
Volume = {12},
Number = {2},
Pages = {229-241},
Abstract = {Sahrawi contemporary literature cannot be understood in isolation from
   its insertion and evolution context. Such context is marked by the
   tragic experience of the war against Morocco and the exile in Algeria;
   the long period of studies of the Sahrawi youth in Cuba; and the later
   migration process to Spain. These experiences contribute to the
   development of a rich Sahrawi literature where diverse cultures coexist
   in perfect harmony: Arab, Caribbean and European. In this text, the
   influence exerted by literature and migration in the vital experience of
   the Sahrawi writers related to the literary group known as La Generacio
   de la Amistad (The Friendship Generation) is analysed. Currently, this
   group is regarded as the main reference of Sahrawi contemporary
   literature in Spanish language.},
