@article{ WOS:000445535900002,
Author = {Eibl, Otto and Gregor, Milos and Mackova, Alena},
Title = {Issues and Tonality of Vaclav Klaus' Public Speeches},
Journal = {POLITOLOGICKY CASOPIS-CZECH JOURNAL OF POLITICAL SCIENCE},
Year = {2013},
Number = {4},
Pages = {392-418},
Abstract = {Research on political speeches is often done in countries with strong
   presidential powers, typically in the USA. However, political speeches
   by presidents are also a critical method of influencing public opinion
   in the Czech Republic. Therefore this article is focused on the analysis
   of speeches of the former President of the Czech Republic, Vaclav Klaus.
   Our research is based on a combination of quantitative and qualitative
   content analysis of Klaus' speeches (thematic units are used as the unit
   of analysis) found on his website (www.klaus.cz) in the period from 1995
   to the end of his presidency, March 2013 (N = 470).
   We aim to identify the changing structure of political issues in Klaus'
   speeches and changes in Klaus' position on those issues, and to uncover
   any characteristic features of his speeches (the use of personal
   pronouns, the role of the speaker, the tone of the speech, etc.).},
