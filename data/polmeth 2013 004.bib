@article{ WOS:000439312600002,
Author = {Hursoy, Siret},
Title = {CYPRUS PROBLEM FROM THE PERSPECTIVE OF CRITICAL THEORY},
Journal = {ISTANBUL UNIVERSITY JOURNAL OF FACULTY OF POLITICAL SCIENCES-SIYASAL
   BILGILER FAKULTESI DERGISI},
Year = {2013},
Number = {48},
Pages = {23-41},
Month = {MAR},
Abstract = {Grounding itself on the realities in the Cyprus conflict, this article
   aims to provide an analytical study of a possible new structure for the
   solution of the problems in the island from a critical theory point of
   view. This involves a discussion, from a critical theory perpective, of
   the recent political transformation and change in the island, the
   removal of the obstacles for the substitution of national sovereignty
   with that of social freedoms, and a re-unification of the Turkish and
   the Greek communities of the island on a comprehensive basis. Finally,
   the proposed restructuring of inter-communal politics on a rational
   basis will be analysed through the critical theory notions of autonomy,
   security, and identity.},
