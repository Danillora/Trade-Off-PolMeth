@article{ WOS:000341680900011,
Author = {Klessmann, Christoph},
Title = {Communism in Half of the Country The Rise, Characteristics, and Decline
   of the GDR},
Journal = {OSTEUROPA},
Year = {2013},
Volume = {63},
Number = {5-6},
Pages = {175+},
Month = {MAY-JUN},
Abstract = {A constitutive feature of communism in the German Democratic Republic
   (GDR) was the lack of a national identity. The East-West conflict
   covered up the problem, but did not solve it. Even after their
   international recognition as a second German state, the GDR never
   overcame the crushing competition from the capitalist Federal Republic.
   Initially, communism in eastern Germany drew its legitimacy from the
   liberation from fascism and the hope for a just socialist model for
   society and the economy. But the repressive course at home and the
   country's role as western outpost of the Soviet empire deeply shaped the
   structure of East German communism. Society and politics were highly
   militarised, while the ruling communist party was more dependent on
   Moscow than communist parties elsewhere in the Soviet bloc.},
