@article{ WOS:000324012400028,
Author = {Foucault, Martial and Nadeau, Richard and Lewis-Beck, Michael S.},
Title = {Patrimonial voting: Refining the measures},
Journal = {ELECTORAL STUDIES},
Year = {2013},
Volume = {32},
Number = {3},
Pages = {557-562},
Month = {SEP},
Abstract = {Several studies have shown the importance of patrimony on voting for the
   right in French, British, and American national elections. However,
   these studies have only taken into account the diversity of patrimony
   and not their value. We propose to fill this gap in the literature with
   the ``Mode de vie des Francais{''} dataset that contains information on
   the savings and patrimony of French voters and was collected before the
   May 2007 presidential election. The results show that including measures
   that take into account the value of survey respondents' patrimony does
   not change the conclusions of previous studies that have demonstrated
   the existence of a strong relationship between holding a risky patrimony
   and support for the right. (C) 2013 Elsevier Ltd. All rights reserved.},
