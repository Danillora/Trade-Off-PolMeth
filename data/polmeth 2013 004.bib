@article{ WOS:000218294200010,
Author = {Gilmore, Margaret},
Title = {THE IMPLICATIONS OF NORTH AFRICAN TERORISM FOR THE UK COUNTER-TERORISM
   EFFORT},
Journal = {RUSI JOURNAL},
Year = {2013},
Volume = {158},
Number = {2},
Pages = {80-85},
Abstract = {The attack on the Tigantourine gas facility near In Amenas in Algeria
   highlights the shifting nature of the terrorist threat to the national
   security of the UK and its interests abroad. It is a sign that the
   threat, while still very strong in parts of Pakistan and Afghanistan, is
   growing in North Africa. However, this does not necessarily mean that
   North Africa will be where the most significant threat can be found in
   future, or where extra resources should be allocated now. Using current
   interviews and historical evidence, Margaret Gilmore investigates where
   the counter-terrorism challenges of the next five years might come from.},
