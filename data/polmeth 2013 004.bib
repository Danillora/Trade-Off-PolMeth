@article{ WOS:000330079600011,
Author = {Fabiani, Jean-Louis},
Title = {About Michel de FORNEL and Albert OGIEN, dir. Bourdieu, theoricien de
   la pratique, Paris, Editions de l'EHESS, coll. ``Raisons
   pratiques{''}, vol. 21, y. 2011, pg. 284},
Journal = {POLITIX},
Year = {2013},
Volume = {26},
Number = {103},
Pages = {223+},
Abstract = {The collected essays devoted to Pierre Bourdieu's theory of practice
   break away from the sterile tradition that forces readers to be
   unconditionally for or radically against the sociologist's work. The
   authors are philosophers, sociologists, anthropologists and linguists.
   Their relationship with Bourdieu's endeavor may vary and the book is
   constructed from diverse perspectives. If there is a stress on
   pragmatism, linguistics and phenomenology, the central issue is to know
   what should be added to Bourdieu's theory, or what should be removed to
   develop a full theory of action. The volume is divided in four parts,
   Theory, Politics, Language and Action, in a both plural and cohesive
   questioning. The book is undoubtedly an inescapable reference on a
   classical work.},
