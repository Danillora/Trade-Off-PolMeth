@article{ WOS:000324898800002,
Author = {Herrera, Remy},
Title = {Where is the ``Socialism of the 21st century{''} heading?},
Journal = {PENSEE},
Year = {2013},
Number = {375},
Pages = {9+},
Month = {JUL-SEP},
Abstract = {The revolutionary advances that occurred in Latin America and the
   Caribbean for several years are extremely important for all of us, in
   the South and in the North, who are still fighting for a socialist
   world. This paper concentrates on the case of Venezuela after the death
   of the leader of the Bolivarian Revolution, Hugo Chavez Elias. It
   examines successively the progresses which have been achieved thanks to
   this revolution, then the progressive evolution of Hugo Chavez's ideas
   in the course of the revolutionary process itself. It also analyzes the
   content and future of the ``Socialism of the 21st century{''}, as well
   as the challenges to be met by the Venezuelan people in struggle.
   Finally, it discusses the international impact of this revolution and
   the links it has with the regional integration and the construction of a
   multipolar world.},
