@article{ WOS:000323431100003,
Author = {Hinshelwood, Brad},
Title = {The Carolinian Context of John Locke's Theory of Slavery},
Journal = {POLITICAL THEORY},
Year = {2013},
Volume = {41},
Number = {4},
Pages = {562-590},
Month = {AUG},
Abstract = {The debate over Locke's theory of slavery has focused on his involvement
   with the Royal African Company and other institutions of African
   slavery, as well as his rhetorical use of slavery in opposing
   absolutism. This overlooks Locke's deep involvement with the Carolina
   colony, and in particular that colony's Indian slave trade, which was
   largely justified in just-war terms. Evidence of Locke's participation
   in the 1682 revisions to the Fundamental Constitutions of Carolina,
   which removed the infamous absolute power and authority clause, and his
   knowledge of colonial affairs, when examined alongside textual evidence
   of the timing of the composition of Locke's theory of slavery, indicates
   a significant connection between the colony and Locke's work. In light
