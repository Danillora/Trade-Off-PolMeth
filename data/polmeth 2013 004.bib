@article{ WOS:000330745000005,
Author = {Muntschick, Johannes},
Title = {Regionalism and External Influence: Does the EU Interfere with Regional
   Market Integration in Southern Africa?},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2013},
Volume = {54},
Number = {4},
Pages = {686-713},
Abstract = {This article addresses the questions, why the rather successful process
   of regional market integration in the Southern African Development
   Community (SADC) is stalling at present and why the existence of the
   centennial Southern African Customs Union (SACU) is likewise at risk
   although the economic and political circumstances within the region have
   not changed much in recent times. Applying a situation-structural
   approach to the analysis of regional integration that takes the
   influence of external actors explicitly into account, the article
   demonstrates that the European Union has an unexpectedly interfering
   effect on regionalism in the South because its Economic Partnership
   Agreements can be made co-responsible for the failure of SADC's
   scheduled customs union project and an undermining of the SACU.},
