@article{ WOS:000211996800003,
Author = {Gibson, Marion},
Title = {Wicker men and Straw dogs: internal colonialism in Celtic novels
   and films 1968-1978},
Journal = {NATIONAL IDENTITIES},
Year = {2013},
Volume = {15},
Number = {2},
Pages = {139-156},
Abstract = {This article explores depictions of Celtic barbarism and paganism in
   novels and films 1968-1978 to consider how Hechter's concept ``internal
   colonialism'' relates to their portrayals of ``Celts'' and their
   ``colonisers''. Whilst the fictions portray Celts as threatening to the
   British nation state and its American allies, the non-Celtic colonial
   powers are ultimately blamed for the violent horror in each text. The
   article focuses on depictions relating to Cornwall. This case study
   shows how much ``a product of its time'' Hechter's theory was, and
   examines the interplay between academia and popular culture in producing
   theories of nationalism.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
