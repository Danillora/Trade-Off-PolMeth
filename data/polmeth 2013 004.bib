@article{ WOS:000447050200010,
Author = {Aktay, Yasin},
Title = {The ``Ends{''} of Islamism: Rethinking the Meaning of Islam and the
   Political},
Journal = {INSIGHT TURKEY},
Year = {2013},
Volume = {15},
Number = {1},
Pages = {111-125},
Month = {WIN},
Abstract = {There have been almost regular attempts to declare the ``end of
   Islamism{''} or ``the end of political Islam.{''} Since the late 1980s,
   many observers have been quick to pronounce the end of Islamism with
   every new political development concerning the Muslim world. In
   countries where authoritarian regimes repressed the Islamists with
