@article{ WOS:000316973400005,
Author = {Vanhoutte, Bram and Hooghe, Marc},
Title = {The influence of social structure, networks and community on party
   choice in the Flemish region of Belgium: A multilevel analysis},
Journal = {ACTA POLITICA},
Year = {2013},
Volume = {48},
Number = {2},
Pages = {209-236},
Month = {APR},
Abstract = {The impact of social position, networks and context on party choice is
   tested in a multilevel model of voting choice for a multi-party system,
   Flanders (Belgium), using the 2009 Social Cohesion Indicators Flanders
   survey data. Social position is shown to be a robust indicator of party
   choice for Christian-Democrats, Greens and extreme right partisans. The
   contribution of network indicators is modest, except for the extreme
   right party, where voters are more socially isolated. Although community
   has an impact on most preferences, a vote for the extreme right does not
   seem to be related to the community context any more. The analysis
   demonstrates that, although varying by party, voting choice still
   depends on structural features of social position, network and context.
   These structural determinants proved to be important not just for
   traditional parties, but also for more recent parties like the Greens or
   the extreme right. Acta Politica (2013) 48, 209-236.
   doi:10.1057/ap.2012.28; published online 23 November 2012},
