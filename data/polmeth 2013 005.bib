@article{ WOS:000212644300007,
Author = {Swenden, Wilfried},
Title = {Conclusion: The Future of Belgian Federalism-Between Reform and
   Swansong? 2007-11: A Critical Juncture in the Transformation of the
   Belgian State?},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2013},
Volume = {23},
Number = {3, SI},
Pages = {369-382},
Abstract = {This article summarizes the main arguments of the special issue. It also
   probes into the deficiencies of the Belgian federal system and argues
   that the most recent crisis of Belgian federalism (2007-11) is not the
   result of rising socio-economic differences between Flanders and
   Wallonia, but the consequence of building a bipolar federation on to a
   split party system. The article concludes by identifying some potential
   roadmaps (federal reform, confederalism, and the referendum) for the
   future.},
