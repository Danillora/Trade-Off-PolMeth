@article{ WOS:000323244200008,
Author = {Bloemraad, Irene and Schoenwaelder, Karen},
Title = {Immigrant and Ethnic Minority Representation in Europe: Conceptual
   Challenges and Theoretical Approaches},
Journal = {WEST EUROPEAN POLITICS},
Year = {2013},
Volume = {36},
Number = {3},
Pages = {564-579},
Month = {MAY 1},
Abstract = {This article introduces the symposium on the representation of
   immigrant-origin and ethnic minorities in Europe. It argues for the
   importance of research on this topic, noting the large, established
   populations of immigrant-origin citizens and their descendants across
   Western European countries and these minorities' underrepresentation in
   elected bodies. Current research gaps concern both empirical knowledge
   and the theoretical conceptualisation of immigrant and ethnic minority
   political involvement. The article argues that existing research on
   representation needs to be extended to suit the cases of immigrants and
   ethnic minorities. It ends by providing a brief overview of each
   contribution to the symposium.},
