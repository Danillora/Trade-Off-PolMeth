@article{ WOS:000323584300002,
Author = {Aguilar, Rosario},
Title = {The Tones of Democratic Challenges: Skin Color and Race in Mexico},
Journal = {POLITICA Y GOBIERNO},
Year = {2013},
Number = {SI},
Pages = {25-57},
Abstract = {In this paper I analyze the effect of skin tone and racial identity on
   Mexicans' political attitudes. I also test different intergroup conflict
   theories to assess which one explains Mexico's situation better if we
   look at skin tone and racial identification as independent variables. In
   order to test my hypotheses I use the Americas Barometer data from
   Mexico 2010 that includes a measure of respondents' skin tone. The
   results show that people's skin tone not only affects their political
   attitudes but their income as well, once controlling for education, age
   and gender. Moreover, the theory of social identity explains better the
   Mexican case than other theories. The conclusions show that Mexico's
   democracy faces the challenge of political inclusion of all its citizens
   regardless of their skin tone or racial identification.},
Publisher = {CENTRO DE INVESTIGACION Y DOCENCIA ECONOMICAS},
