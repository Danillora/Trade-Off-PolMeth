
@article{ WOS:000341681300010,
Author = {Bossina, Zlata},
Title = {Reluctantly Political Russian in Berlin: queer plus art = Quarteera},
Journal = {OSTEUROPA},
Year = {2013},
Volume = {63},
Number = {10},
Pages = {117+},
Month = {OCT},
Abstract = {In Berlin, a group of Russian-speaking lesbians, gays, bisexuals, and
   transgender individuals have joined forces in the association Quarteera.
   Quarteera offers capacity development and uses art and words to fight
   homophobia in the diaspora and in Russia. On a weekly basis, Quarteera
   explains the situation of the queer population in Russia and the
   post-Soviet space. Zlata Bossina introduces Quarteera, analyses the
   purpose of homophobic mobilisation, depicts what it means for those
   affected, and dares to take a pessimistic and an optimistic look into
   the future.},
