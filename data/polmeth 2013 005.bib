@article{ WOS:000420236400005,
Author = {Luparello, Velia and Martin, Fausto},
Title = {Perspectives and results: American Trotskyism and postwar Europe
   (1943-1947)},
Journal = {IZQUIERDAS},
Year = {2013},
Volume = {16},
Pages = {95-114},
Month = {AUG},
Abstract = {This paper summarizes the polemics within the American Trotskyist
   movement between 1942 and 1945, about the situation in post-war Europe
   and the political line that Trotskyist organizations should adopt in
   such situation. The leaders of the SWP, following literally the analysis
   contained in Trotsky's Transitional Program of 1938, thought there were
   no possibilities for the stabilization of democratic regimes in post-war
   Europe. This narrow ``catastrophist{''} view was questioned by Felix
   Morrow, Albert Goldman y Jean Van Heijenoort, who confronted the party
   leadership on this matter.},
