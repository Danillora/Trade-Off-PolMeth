@article{ WOS:000421473900005,
Author = {Bianchi, Bernardo},
Title = {Philosophies for a second nature: strategies of finite},
Journal = {REVISTA ESTUDOS POLITICOS},
Year = {2013},
Number = {7},
Pages = {86-107},
Abstract = {The article discusses both (i) the affinities between Vauvenargues and
   Spinoza, and (ii) the way Laurent Bove examines the relation. Despite
   all the efforts made by Vauvenargues to mislead his readers and,
   especially, the censorship, Bove's endeavor consists in reconstructing
   Vauvenargues concealed metaphysical premises. As we explore
   Vauvenargues' philosophy, in which Pascal plays a major role, it becomes
   clear the conceptual twists made by the provencal writer in favor of a
   kind of philosophy where the finite, man's ontological condition, is
   valued - and that is a sort of spinozism.},
