@article{ WOS:000213000400003,
Author = {Heo, Inhye},
Title = {The Political Economy of Policy Gridlock in South Korea: The Case of the
   Lee Myung-bak Government's Green Growth Policy},
Journal = {POLITICS \& POLICY},
Year = {2013},
Volume = {41},
Number = {4},
Pages = {509-535},
Month = {AUG},
Abstract = {This article analyzes the occurrence, development, results, and special
   features of policy gridlock in South Korea's green growth (GG) policy
   from a political-economic perspective. Analysis is based on the claim
   that the relationships that the government has formed with other
   relevant policy actors-including the National Assembly, market players,
   and civil society-underpin the critical political-economic causes of the
   gridlock. Research results show that the characteristics of the
   relationships that the government has formed with those actors, as well
   as the complex interactions among them (such as intervening in other
   actor-government relationships and pursuing either conflict or
   cooperation with one another), have functioned as direct causes of
