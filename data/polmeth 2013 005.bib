@article{ WOS:000448115800001,
Author = {Casey, Walter T.},
Title = {RE-EVALUATING GENDER ROLES AND REVOLUTIONS: CASES OF UNEXPECTED
   UTILITIES},
Journal = {JOURNAL OF COMPARATIVE POLITICS},
Year = {2013},
Volume = {6},
Number = {1},
Pages = {4-29},
Month = {JAN},
Abstract = {Women have been a contributing factor of modern revolutions since at
   least the American Revolution against Great Britain. Yet too often most
   descriptions and analyses of revolution relegate women to a supporting
   role, or make no mention of women's involvement at all. This work
   differs from prior efforts in that it will explore one possible
   explanation for the successes of three revolutions based upon the levels
   of women's support for those revolutions. An analysis of the three cases
   (Ireland, Russia, and Nicaragua) suggests a series of hypotheses about
   women's participation in revolution and its importance to revolutions'
   success.},
