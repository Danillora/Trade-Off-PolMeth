@article{ WOS:000325934200007,
Author = {Nokken, Timothy P.},
Title = {Uncertainty and Roll-Call Voting in Lame-Duck Sessions of the US House,
   1969-2010},
Journal = {LEGISLATIVE STUDIES QUARTERLY},
Year = {2013},
Volume = {38},
Number = {4},
Pages = {571-591},
Month = {NOV},
Abstract = {Lame-duck sessions of Congress have become increasingly common of late.
   Such sessions are marked by higher levels of ideological and
   participatory shirking among departing members, creating a more
   uncertain legislative environment. I investigate the consequences of
   such shirking on coalition formation and roll-call behavior. I analyze
   House roll-call votes held in the 12 congresses that convened lame-duck
   sessions from 1969 to 2010 (91st to 111th Congresses) to assess how
   roll-call behavior changes across sessions. I find subtle but
   statistically significant changes across sessions consistent with claims
   regarding greater uncertainty in roll-call voting in lame-duck sessions.},
