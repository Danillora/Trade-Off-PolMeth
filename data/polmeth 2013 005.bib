@article{ WOS:000325762900001,
Author = {Gaffney, John and Lahel, Amarjit},
Title = {Political Performance and Leadership Persona: The UK Labour Party
   Conference of 2012},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2013},
Volume = {48},
