@article{ WOS:000218117500005,
Author = {Hansson, Sven Ove},
Title = {John Stuart Mill's political self-identifications},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2013},
Volume = {18},
Number = {3},
Pages = {348-357},
Abstract = {Mill's choice of political self-identifications was thoughtful and
   enduring. He described himself as a radical and an advanced liberal but,
   perhaps surprisingly, avoided the designation of a liberal simpliciter.
   He also called himself a democrat, but often added reservations that
   referred to his dissension from some common views among contemporary
   democrats. Beginning in 1848 he regarded himself as a socialist, but he
   was careful to distinguish his views from those of some others who
   called themselves socialists. A study of how he described his own
   political views can contribute to our understanding of how he related to
   the political ideologies and movements of his own time.},
