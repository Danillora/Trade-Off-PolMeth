@article{ WOS:000217969100008,
Author = {Cuesta, Micaela},
Title = {Variations on happiness. Hannah Arendt, G.W.F. Hegel, Walter Benjamin},
Journal = {ANACRONISMO E IRRUPCION},
Year = {2013},
Volume = {3},
Number = {4},
Pages = {179-208},
Month = {MAY-NOV},
Abstract = {This paper aims to reflect on happiness on three dimensions. The first
   one refers to Hannah Arendt's developments, and focuses on the
   historical pathway that goes from a notion of happiness centered on its
   public nature, towards a notion restricted to the individual interest
   and retracted to the private sphere. The oblivion into which the first
   of these meanings has fallen, pushes us to interrogate and to ponder the
   hypothesis that Hegel's philosophy of history would have contributed to
   its overlapping, by making of happiness the forbidden object of his
