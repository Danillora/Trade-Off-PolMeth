@article{ WOS:000317695900001,
Author = {Croissant, Aurel and Wurster, Stefan},
Title = {Performance and persistence of autocracies in comparison: introducing
   issues and perspectives},
Journal = {CONTEMPORARY POLITICS},
Year = {2013},
Volume = {19},
Number = {1, SI},
Pages = {1-18},
Month = {MAR 1},
Abstract = {Authoritarianism research has evolved into one of the fastest growing
   areas in comparative politics and political economy. However, the newly
   awakened interest in autocratic regimes goes hand in hand with a lack of
   systematic research on the results of the political and substantive
   policy performance of variants of autocratic regimes. In this article we
   introduce the individual contributions to this special issue and
   summarize their findings with regard to three core research questions:
   What are the differences between autocracies and democracies, as well as
   between different forms of authoritarian regimes, with regard to their
   outcome performance in selected policy fields? Does policy performance
   matter for the persistence of authoritarian rule? How can we
   conceptualize different types of autocratic regimes and do differences
   in the availability of performance data matter for the results of
   empirical studies comparing democracies and autocracies or different
   types of non-democratic regimes?},
