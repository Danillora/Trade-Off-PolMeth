@article{ WOS:000215415400003,
Author = {Zurolo, Anna and Garzillo, Francesco},
Title = {Bodies, Gender and Violence: construction(s) and deconstruction(s)},
Journal = {POLITICA Y SOCIEDAD},
Year = {2013},
Volume = {50},
Number = {3},
Pages = {803-815},
Abstract = {The paper aims to develop a reflection on gender violence from an
   interdisciplinary perspective. Moving from tasks and positions of
   philosophical and psychoanalytic matrix, the authors bring into
   discussion the notion of gender based violence as a monolithic entity.
   After drawing a short theoretical trajectory on gender, sex and
   identity, the different meanings of the violence as social phenomenon
   are highlighted from a critical reflection on interpretative paradigms
   so far suggested. The main reference will be the difficulty associated
   with concepts such as heterosexual norm, required identity, performative
   gender, exclusionary effects associated with it, in order to analyze the
   rhetoric that act as guidance/ordering in the speeches on gender based
   violence.},
