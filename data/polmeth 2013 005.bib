@article{ WOS:000320358800006,
Author = {Zolleis, Udo and Wertheimer, Carina},
Title = {Is the CSU Still a Volkspartei?},
Journal = {GERMAN POLITICS},
Year = {2013},
Volume = {22},
Number = {1-2, SI},
Pages = {97-113},
Month = {JUN 1},
Abstract = {One of the peculiarities of the territorial dimension of Christian
   Democracy in Germany is the fact that it is represented by not one, but
   two parties: the CDU and the Bavarian CSU. This article focuses on the
   CSU and discusses the pressures for transformation that it has been
   subjected to, by focusing on the question of whether it can still be
   considered as a Volkspartei. The article disaggregates this term around
   the notions of a majority party (Smith), office-holding party
   (Frohlich), membership party (Wiesendahl), cross-class party (Nipperdey)
   and Kirchheimer's catch-all party. The paper concludes that the CSU
   remains a Volkspartei, albeit with some qualifications.},
