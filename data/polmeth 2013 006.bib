@article{ WOS:000313067000024,
Author = {Jones, David A.},
Title = {The Polarizing Effect of a Partisan Workplace},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2013},
Volume = {46},
Number = {1},
Pages = {67-73},
Month = {JAN},
Abstract = {The workplace provides an exceptional outlet for citizens to encounter
   different viewpoints and discuss politics with people with whom they may
   disagree. In Washington, DC, however, many people work in partisan
   environments where like-minded coworkers rarely encounter alternate
   points of view. Group polarization theory suggests that these
   environments push individuals to the partisan and ideological extremes.
   This study tests this hypothesis by comparing opinion change among
   interns who worked in a partisan workplace with that of interns who were
   employed in less-partisan environments. The results suggest that
   partisan workplace environments foster opinion polarization, especially
   among Republicans.},
