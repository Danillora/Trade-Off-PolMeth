@article{ WOS:000218562200005,
Author = {Carey, Jr., Tony E.},
Title = {The dimensionality of black nationalism and African-American political
   participation},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2013},
Volume = {1},
Number = {1},
Pages = {66-84},
Abstract = {This article examines the dimensionality of black nationalism and its
   relationship to African-American political participation, focusing
   specifically on blacks' electoral and non-electoral involvement. An
   analysis of the 1993-94 National Black Politics Study confirms earlier
   findings that reveal two key strains of black nationalist thought,
   namely community and separatist nationalism. Most importantly, the
   analysis demonstrates that both versions drive distinct forms of
   political activity. Community nationalism increases electoral and
   non-electoral involvement among African Americans, while separatist
   nationalists tend to abstain from voting.},
