@article{ WOS:000320863700010,
Author = {Clemens, Austin C. and Crespin, Michael H. and Finocchiaro, Charles J.},
Title = {Research Note Pork-Barrelling in Australian Politics: A
   Reconsideration of Leigh},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2013},
Volume = {48},
Number = {2},
Pages = {221-232},
Month = {JUN 1},
Abstract = {This research note replicates Leigh's (2008) analysis of pork-barrelling
   in Australian politics and tests the validity of his findings using
   geographic techniques. Distributive politics is integral to nearly all
   theories of legislative politics because it is fundamental to the
   connection between legislators and constituents, and can be used to
   facilitate coalition-building. This note addresses an aspect of
   pork-barrelling that has been overlooked in most previous scholarship.
   It argues that distributive politics is, at its core, a geographic,
   constituency-centred process. It is therefore essential to reconsider
   the conventional models and take geography into account in both
   substantive and statistical terms. To that end, we replicate several of
   Leigh's results using a relatively new technique, Geographically
   Weighted Regression, and reveal substantial spatial heterogeneity in the
   coefficients. Our findings highlight the importance of political
   geography and have important implications for studies of legislative
   politics and elections.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
