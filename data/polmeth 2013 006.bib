@article{ WOS:000218117500002,
Author = {Finlayson, Lorna},
Title = {Preachers and propagandists: political theory and the status quo},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2013},
Volume = {18},
Number = {3},
Pages = {281-298},
Abstract = {This paper begins from an examination of a very short and now largely
   forgotten article by John Plamenatz, entitled `The Use of Political
   Theory' (1960). Section 1 outlines Plamenatz's vision of the role of
   political theory, whilst Section 2 relates this vision to some themes of
   contemporary political philosophy: in particular, the `realist' critique
   of mainstream liberalism. Without unequivocally endorsing `realism'-in
   any case an extremely ill-defined category-I suggest that its central
   complaint pinpoints deficiencies in contemporary liberal theory which
   are especially clearly manifested in Plamenatz's earlier account of its
   `use': in particular, a tendency both to idealize and to idealize away
   from the political status quo. Finally, Section 3 explores how these
   deficiencies might be remedied by a closer attention to the Marxist
   notion of ideology: a notion which, I argue, is accorded insufficient
   credit by Plamenatz as well as by many contemporary political theorists.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
