@article{ WOS:000212019400004,
Author = {Mahmoud, Dimah I.},
Title = {Sudan's Comprehensive Peace Agreement Amidst the Clash of Agendas:
   Attempts, Failures and Lessons Unlearned},
Journal = {CIVIL WARS},
Year = {2013},
Volume = {15},
Number = {2},
Pages = {157-172},
Abstract = {The euphoria of celebrating South Sudan's independence and the
   completion of the Comprehensive Peace Agreement, signed in 2005, must
   not undermine the significance of assessing and more importantly
   acknowledging the shortcomings of securing Sudan's stability and unity
   as a result of its exclusive nature. By all means, the responsibility
   was primarily that of the government of Sudan to ensure the country's
   territorial integrity. However, the regional and international mediators
   are not free of blame. By analysing the objectives and outcomes of the
   peace process, while considering the conflicting agendas of all involved
   parties, this article underlines the failures that led to Sudan's
   secession and left the country with yet another conflict: Darfur.},
