@article{ WOS:000325761200002,
Author = {van Kessel, Stijn},
Title = {A Matter of Supply and Demand: The Electoral Performance of Populist
   Parties in Three European Countries},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2013},
Volume = {48},
Number = {2},
Pages = {175-199},
Month = {APR},
Abstract = {This article assesses the electoral performance of populist parties in
   three European countries: the Netherlands, Poland and the United
   Kingdom. In explaining the electoral performance of the populist parties
   in the three countries, the article considers the agency of political
   parties in particular. More specifically, it examines the responsiveness
   of established parties and the credibility of the populist parties.
   Whereas the agency of populist parties, or other radical outsiders, has
   often been overlooked in previous comparative studies, this article
   argues that the credibility of the populist parties themselves plays a
   crucial role in understanding their electoral success and failure.},
