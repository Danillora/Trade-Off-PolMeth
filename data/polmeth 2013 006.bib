@article{ WOS:000325925200008,
Author = {Jamieson, Kathleen Hall},
Title = {Messages, Micro-targeting, and New Media Technologies},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
Year = {2013},
Volume = {11},
Number = {3},
Pages = {429-435},
Month = {OCT},
Abstract = {This article argues that new media technologies are likely to elicit
   changes in the content, tone, and potential electoral impact of those
   campaign messages micro-targeted through them, with a resulting increase
   in the level of unaccountable, deceptive, pseudonymous campaigning.
   Access to data-mined information will increase the likelihood that the
   candidate with the larger war-chest will gain an advantage by changing
   the composition of the electorate. In a world of micro-targeted
   messaging, reporters have greater difficulty holding sponsors
   accountable and policing deception.},
