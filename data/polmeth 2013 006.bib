@article{ WOS:000323484800004,
Author = {Goelzhauser, Greg and Vouvalis, Nicole},
Title = {State Coordinating Institutions and Agenda Setting on the US Supreme
   Court},
Journal = {AMERICAN POLITICS RESEARCH},
Year = {2013},
Volume = {41},
Number = {5},
Pages = {819-838},
Month = {SEP},
Abstract = {What determines state success when petitioning the U.S. Supreme Court
   for review? We suggest that states can improve the likelihood of
   securing Supreme Court review by coordinating litigation efforts. This
   coordination occurs in two ways. First, some states coordinate their
   appellate litigation efforts internally through the creation of state
   solicitors general offices. Second, external coordination occurs when
   states join amicus briefs at the agenda setting stage urging the Supreme
   Court to grant review in state-filed cases. Using new data on all
   state-filed certiorari petitions from the 2001-2009 terms, we find that
   internal and external coordination is associated with an increased
   likelihood of the Supreme Court granting review in state-filed cases.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Goelzhauser, G (Corresponding Author), Utah State Univ, Old Main 320, Logan, UT 84322 USA.
   Goelzhauser, Greg; Vouvalis, Nicole, Utah State Univ, Logan, UT 84322 USA.},
