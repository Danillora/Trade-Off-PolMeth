@article{ WOS:000218582000008,
Author = {Price, Melynda J.},
Title = {Are we imprisoned in narratives of progress? The work of Hanes Walton
   and its lessons for an Afrofuturistic black politics},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2013},
Volume = {1},
Number = {4},
Pages = {577-588},
Abstract = {Hanes Walton was a prolific scholar and a fine human being. In this
   article, I use his work to engage theories of Afrofuturism and explore
   their combined import for the future of black politics. I argue that
   Afrofuturism's concern with how past systems of oppression collude with
   ideas of post-raciality offer a caution to those studying the politics
   of black people. The most significant way this intersection impacts the
   field is in limiting both the subjects we study and our methods. Using
   the broad span of Hanes' works, I show how he represents a model of how
   to produce scholarship that is empirically sound and responsive to the
   concerns of the black community for years to come. I end with a
   discussion of Hanes' impact beyond the page and how, thanks to his
   guidance, I continue to do ``home work{''}.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
