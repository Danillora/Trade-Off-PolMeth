@article{ WOS:000449589700004,
Author = {Fiuza, Cesar and Poli, Luciana Costa},
Title = {Private autonomy and intervention in the Rule of Law: the
   (im)possibility of gay marriage},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2013},
Volume = {106},
Pages = {95-131},
Month = {JAN-JUN},
Abstract = {This work analizes the principle of private autonomy in the affective
   relations context. It approaches the legal fundaments of the
   contemporary family, which is essencialy based in cooperation and takes
   on new characteristics. In this context, the human dignities' promotion
   will be acomplished in the core of the family by the full
   acknowledgement of the individual choices, be it in the constitution way
   of the familiar unity by the recognition of the homosexual union rights.
   The private autonomy is associated with the concepts of freedom,
   autonomy and volition to aloud the legal protection to the homosexual
   family. Through this point of view, we'll analise the marriage as
   proposed in the Civil Code, verifying if the legal discipline matches
   with the constitutional principology. The marriage will be analized in
   the contemporary society's historical and social context to demonstrate
   that its means of procreation, heritage protection and of
   indissolubility caracteristichs change and is becoming an environmet to
   safeguard plurarity, human dignity promotion, equality preservation and
   the non-discrimination. We will sustain the possibility of homosexual
   marriage achievement by the implementation of the principles above
   mentioned. We sustain the the most appropriate mean of promoting
   homosexual and heterosexual couples equality is, precisely, aloud them
   to safeguard the same rights. We advocate that the mariage concession
   must be done by the contitucional comand force of equality,
   independently of considerations on the gender.},
