@article{ WOS:000439068600010,
Author = {Modrzejewski, Arkadiusz},
Title = {CRISIS OF TRUTH IN THE GLOBALIZED WORLD OF MASS MEDIA},
Journal = {EUROPEAN JOURNAL OF TRANSFORMATION STUDIES},
Year = {2013},
Volume = {1},
Number = {1},
Pages = {108-120},
Abstract = {This article is dedicated to the epistemological and ethical issue of
   truth in the globalized and commercialized mass media world. We are
   eyewitnesses that postmodern civilization means the crisis of classical
   understanding of truth. It has serious ethical and pragmatic
   consequences. Human beings can feel lost in this reality. Nothing is
   sure and everything is flexible. Medial teaching could be a proper way
   for preparing people, especially the youth to live in these conditions.},
Publisher = {EUROPE OUR HOUSE},
Address = {EUROPE OUR HOUSE, TBILISI, 00000, REP OF GEORGIA},
Type = {Article},
Language = {English},
