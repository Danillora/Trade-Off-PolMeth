@article{ WOS:000218116700001,
Author = {Hackell, Melissa},
Title = {Taxpayer citizenship and neoliberal hegemony in New Zealand},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2013},
Volume = {18},
Number = {2},
Pages = {129-149},
Abstract = {Drawing on what Norval describes as Laclau's `grammar of political
   analysis', this article explores the neoliberalization of citizenship in
   New Zealand. The analysis focuses on how, during the 1990s, pre-existing
   moral standards of social justice, popular cultural ideals of fairness
   and equality of opportunity policy discourse were re-articulated to the
   neoliberal discourse of the citizen as taxpayer. Furthermore, this
   article illustrates the paradoxical role of contestation in distilling
   this consensus by exploring how the strategies of New Zealand's major
   Centre Left and Centre Right Parties to define themselves as distinct
   from each other in moral terms consolidated a new `inclusive middle'
   with Left and Right dialects.},
