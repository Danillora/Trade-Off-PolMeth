@article{ WOS:000326879200018,
Author = {Fortunato, David and Koenig, Thomas and Proksch, Sven-Oliver},
Title = {Government Agenda-Setting and Bicameral Conflict Resolution},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2013},
Volume = {66},
Number = {4},
Pages = {938-951},
Month = {DEC},
Abstract = {We examine the extent to which governments consider the role of
   bicameral conflict resolution procedures in legislative agenda-setting.
   We argue that governments may use these institutions to promote policy
   change in the event of bicameral conflict, especially when facing
   uncertainty over bicameral policy preferences. We test our arguments
   using comprehensive original data on forty years of German legislation
   and find that bicameral conflict resolution committees play a more
   sophisticated role in governmental policy making than previously
   suspected.},
