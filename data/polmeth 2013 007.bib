@article{ WOS:000320835200005,
Author = {Yang, Jae-jin},
Title = {Parochial Welfare Politics and the Small Welfare State in South Korea},
Journal = {COMPARATIVE POLITICS},
Year = {2013},
Volume = {45},
Number = {4},
Pages = {457+},
Month = {JUL},
Abstract = {Why is the Korean welfare state underdeveloped? From the
   institutionalist point of view, it is evident that existing institutions
   influence the policy preferences of key actors of welfare politics:
   organized labor and employers on the demand side and politicians on the
   supply side. Distributive demands of organized labor have been satisfied
   by affluent big business (or chaebol) through an implicit or explicit
   cross-class alliance with parochial enterprise unions, and the nation's
   single-member-district electoral rules have induced politicians to sell
   geographically targeted benefits rather than promising social welfare
   for all beyond their district. As a result, despite the nation's
   successful economic growth and democratization, neither the supply nor
   the demand side has been conducive to significant welfare state
   expansion in Korea.},
