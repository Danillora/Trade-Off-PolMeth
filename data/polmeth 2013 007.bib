Language = {English},
Affiliation = {Zechmeister, EJ (Corresponding Author), Vanderbilt Univ, Dept Polit Sci, PMB 0505, Nashville, TN 37203 USA.
   Zechmeister, Elizabeth J.; Corral, Margarita, Vanderbilt Univ, LAPOP, Nashville, TN 37203 USA.
   Corral, Margarita, Vanderbilt Univ, Dept Polit Sci, Nashville, TN 37203 USA.},
DOI = {10.1177/0010414012463880},
ISSN = {0010-4140},
