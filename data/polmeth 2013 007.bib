@article{ WOS:000214686300011,
Author = {Asadi, Muhammed},
Title = {Frozen Lives: Notes on the US Homeless},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2013},
Volume = {3},
Number = {2},
Pages = {245-257},
Abstract = {These field notes and commentary are part of the project that culminated
   in a paper titled ``How does It Feel to Be a Problem? The Diasporic
   Identity of the Homeless.{''} The project was an exploratory research
   into the homeless identity, where besides the in-depth interviews with
   homeless people chosen from homeless shelters across Southern Illinois,
   I conducted ethnographic field research through passive and
   semi-participatory observation of various shelters across Southern
