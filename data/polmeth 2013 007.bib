@article{ WOS:000328329400012,
Author = {Esselment, Anna Lennox},
Title = {A Little Help from My Friends: The Partisan Factor and Intergovernmental
   Negotiations in Canada},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2013},
Volume = {43},
Number = {4},
Pages = {701-727},
Month = {FAL},
Abstract = {This article takes a new approach to the study of federal-provincial
   relations by arguing that in the conduct of intergovernmental relations
   in Canada, whether on major constitutional issues or the mundane,
   ordinary intergovernmental negotiations, partisanship has an effect. An
   examination of the Meech Lake Accord constitutional negotiations
   (1987-1990) and the Child Care Agreements (2004-2005) reveals that where
   traditional factors fail to provide a reason for conflict or cooperation
   between governments, the partisan variable offers valuable explanatory
   power. The impact is found in the process of federal-state bargaining
   and not in the substance of agreements themselves-in other words,
   partisanship can influence how an agreement is reached and whether it is
   kept.},
