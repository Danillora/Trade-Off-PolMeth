@article{ WOS:000217073600007,
Author = {Mcmichael, Christopher},
Title = {`CLEARLY BLOWN AWAY BY THE END OF THE MORNING'S DRAMA': SPECTACLE,
   PACIFICATION AND THE 2010 WORLD CUP, SOUTH AFRICA},
Journal = {SOCIALIST STUDIES},
Year = {2013},
Volume = {9},
Number = {2, SI},
