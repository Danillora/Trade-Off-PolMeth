@article{ WOS:000328106200001,
Author = {Abels, Gabriele},
Title = {Adapting to Lisbon: Reforming the Role of German Landesparlamente
   in EU Affairs},
Journal = {GERMAN POLITICS},
Year = {2013},
Volume = {22},
Number = {4},
Pages = {353-378},
Month = {DEC 1},
Abstract = {In Europeanisation studies subnational parliaments are so far treated as
   a quantite negligeable - despite the fact that their key functions are
   strongly affected by European integration and they are perceived as
   losers of European integration. Given the provisions of the Lisbon
   Treaty, which acknowledges a role for subnational parliaments on
   subsidiarity issues, and given the concept of parliamentary
   responsibility for integration' introduced by the German
   Bundesverfassungsgericht, this represents a substantial gap. These
   recent changes in the institutional environment have led to a wave of
   reforms in the German Landesparlamente concerning their EU-related
   functions. The key strategy is strengthening the control function
   vis-a-vis the Lander governments. The high degree of convergence in
   terms of control strategies and instruments is explained by the fact
   that these reforms follow by and large the path adopted in the aftermath
   of the Maastricht Treaty, but with some interesting modifications.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
