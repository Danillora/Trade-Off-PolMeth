@article{ WOS:000435232700005,
Author = {Bicchi, Federica and Voltolini, Benedetta},
Title = {EU Democracy Assistance in the Mediterranean: What Relationship with the
   Arab Uprisings?},
Journal = {DEMOCRACY \& SECURITY},
Year = {2013},
Volume = {9},
Number = {1-2},
Pages = {80-99},
Abstract = {This article analyzes the European Union (EU) policy for democracy
   assistance toward the Southern Mediterranean countries and tracks
   changes in the last decade, with a special emphasis on the most recent
   period. It shows that the EU policy, which goes under the acronym of
   EIDHR, has evolved, but predominantly in response to internal dynamics
   rather than to developments in Arab countries. The EU has increasingly
   provided assistance to local actors on the ground in non-member
   countries and has differentiated its action in authoritarian countries
   from countries in transition. When it comes to implementing its own
   policy, however, the EU is less able to promote democracy than human
   rights, and most of the funds go to support projects centered on
   relatively uncontroversial rights such as women's and children's.},
