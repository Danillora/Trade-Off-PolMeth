@article{ WOS:000330464600013,
Author = {Booth, W. James},
Title = {Maitres Chez Nous: Some Questions about Culture and Continuity A
   Response to Alan Patten's ``Rethinking Culture: The Social Lineage
   Account{''}},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2013},
Volume = {107},
Number = {4},
Pages = {866-874},
Month = {NOV},
Abstract = {Alan Patten's social lineage account of cultural continuity is the most
   recent effort to provide multicultural theory with a non-essentialist
   concept of culture, its continuity and loss that meets broadly liberal
   normative desiderata. In this essay, I argue that it too fails to offer
   an alternative to essentialism, to meet standard liberal normative
   stipulations, and to construct a theory of continuity sufficient to
   underpin the present claims of involuntarily incorporated communities.
   That result is theoretically interesting for it shows the deep
   intractability of the problems at the core of liberal multiculturalism.},
