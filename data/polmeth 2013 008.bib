   Asdisardottir, Regina; Steinporsson, Runolfur Smari, Haskola Islands, Vidskiptafraedideild, Reykjavik, Iceland.},
DOI = {10.13177/irpa.a.2013.9.1.11},
ISSN = {1670-6803},
EISSN = {1670-679X},
