@article{ WOS:000214352100005,
Author = {Ahmadi, Hamid},
Title = {Iran and the Arab Spring: Why Haven't Iranians Followed the Arabs in
   Waging Revolution?},
Journal = {ASIAN POLITICS \& POLICY},
Year = {2013},
Volume = {5},
Number = {3},
Pages = {407-420},
Month = {JUL},
Abstract = {Since the beginning of the Arab Spring, the question of Iran's own
   political situation and its relationship to the Arab movements has
   become a controversial issue. This article examines the Iranian ruling
   elites' as well as oppositionist views of the Arab Spring, explaining
   why Iran has not followed its Arab counterparts in engaging a
   revolutionary and violent confrontation with the Islamic Republic
   regime. A main argument is that there is no agreement among the Iranians
   regarding the nature of the Arab Spring, thus situational and
   nationalistic considerations make Iranians hesitant to take a radical
   and violent move against the regime. Further, it is argued that in
   contrast to the expectations of the ruling elite, the empowerment of
   Arab Islamists brings about unexpected challenges, rather than
   advantages, for the Islamic Republic in Iran.},
