@article{ WOS:000322968100003,
Author = {Harris, Richard A.},
Title = {Does ``Too Big Too Fail{''} Signal the Triumph of Business Power?},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
Year = {2013},
Volume = {11},
Number = {1},
Pages = {17-32},
Month = {MAY},
Abstract = {This essay explores the concern that in contemporary American Politics,
   business exercises outsized and historically unprecedented influence.
   Focusing on the largest sector of our economy, the banking industry, the
   analysis tracks the systematic, purposeful, and bipartisan deregulation
   of the financial services industry from the Carter Administration
   thorough the 2008 financial collapse. The claim of business hegemony is
   examined from an historical/institutional perspective and placed in the
   context of political science theory, and the conclusion is that while
   business, represented in this case by the ``too big to fail{''} banks,
   enjoys significant organizational and structural advantages, business
   power remains both variable and contingent rather than fixed and
   hegemonic.},
