@article{ WOS:000443271700003,
Author = {Mancini, Paolo},
Title = {FROM MEDIA EFFECTS TO SYSTEM THEORY: THE JOURNEY OF COMPARATIVE RESEARCH
   IN POLITICAL COMMUNICATION},
