@article{ WOS:000323584300009,
Author = {Merolla, Jennifer L. and Mezini, Evis and Zechmeister, Elizabeth J.},
Title = {Crime, Economic Crisis, and Support for Democracy in Mexico},
Journal = {POLITICA Y GOBIERNO},
Year = {2013},
Number = {SI},
Pages = {221-251},
Abstract = {We argue that conditions of threat (compared to times of well-being) can
   induce individuals to place less value on democratic forms of
   governance. We test our argument in the context of Mexican politics and
   with respect to two types of threat that are arguably the most pressing
   issues today: crime and economic. Our data come from a national
   experiment implemented in the spring of 2009 and a national survey, the
   AmericasBarometer, conducted in 2010. The experimental evidence supports
   our contention that threats can cause democratic decay among the mass
   public; the survey evidence provides a window into the generalizability
