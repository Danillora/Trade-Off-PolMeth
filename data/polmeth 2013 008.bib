@article{ WOS:000210641200010,
Author = {Cassano, Graham},
Title = {Labor and Meconnaissance in the Culture Industry},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2013},
Volume = {25},
Number = {1},
Pages = {104-108},
Month = {JAN},
Abstract = {In her case study of a union in the midst of a strike, Unions and Class
   Transformation: The Case of the Broadway Musicians (2009), Cathy Mulder
   argues that only renewed attention to the radical critique of
   exploitation can salvage a beleaguered movement. While she sees the
   possibility for a new basis of union activity in the struggles of
   Broadway musicians, her text makes an equally compelling case that a
   number of social and cultural processes continue to produce an active
   set of professional phantasies which interfere with labor struggles and
   class transformation. Mulder never resolves these contradictions, but
   her recognition of the unique position of the culture worker, and the
   forms of vocational meconnaissance that such a position often entails,
   provides important spurs to further research. More substantially, her
   work provides a virtual Marxian cartography of class exploitation within
   one work force.},
