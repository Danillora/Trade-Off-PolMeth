@article{ WOS:000329013500026,
Author = {Linos, Elizabeth},
Title = {Do conditional cash transfer programs shift votes? Evidence from the
   Honduran PRAF},
Journal = {ELECTORAL STUDIES},
Year = {2013},
Volume = {32},
Number = {4},
Pages = {864-874},
Month = {DEC},
Abstract = {How do national social programs influence local voting? This study
   utilizes the experimental set up of a conditional cash transfer program
   to show that small, targeted cash transfers can have large electoral
   effects. The Honduran PRAF program allocated an average of \$18 per
   capita per year to poor households within municipalities that were
   randomly assigned to receive the program. Although the program was
   administered at the national level, the program increased an incumbent
   mayor's re-election probabilities by 39\%, without significantly
   influencing voting behavior in presidential elections. Moreover, the
   evidence suggests that transferring cash to poor households were more
   effective at increasing political support than interventions providing
   public goods for poor villages. (C) 2013 Elsevier Ltd. All rights
   reserved.},
Publisher = {ELSEVIER SCI LTD},
