@article{ WOS:000215173100003,
Author = {Szentannai, Andreea},
Title = {ROMANIA. THE USES OF MEDIATION TO SOLVE PUBLIC DISPUTES},
Journal = {CONFLICT STUDIES QUARTERLY},
Year = {2013},
Number = {2},
Pages = {40-49},
Month = {JAN},
Abstract = {While working on my dissertation to reveal to what extent mediation is
   suitable as an alternative dispute resolution method when it comes to
   community conflicts, the analysis of the conflict chosen as a case study
   has also shown the necessity of having a mechanism which can facilitate
   the access to mediation.
   A newly formed community around a long-existing chicken farm has shaken
   the grounds of the social relationship between the members of the
   community, the local administration and the management of the farm. The
   goal of the present paper is to identify the features of a mechanism
   which can, in reality, be implemented so that people and institutions
   will prefer mediation as a first mean to solve their conflicts, rather
   than the traditional way of going to court. Sketching such a mechanism
   will eventually make it possible to imagine a viable approach.
   The particular case study is somehow specific to a certain reality of
   the Romanian life, nevertheless some aspects can be found in other
   countries as well. Therefore the mechanism which will be envisaged in
   this paper aims to serve, first of all, the specific needs of this
   nation.},
