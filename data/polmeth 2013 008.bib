@article{ WOS:000319850500008,
Author = {Mateos, Araceli and Penades, Alberto},
Title = {Spain: Crisis and cutouts},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2013},
Volume = {33},
Number = {1},
Pages = {161-184},
Abstract = {The economic crisis in Spain since 2008 has translated in 2012 into
   crises of social and political representation, and a drop in the quality
   of democracy. The public policies designed by the government of Mariano
   Rajoy, during his first year in office and under European tutelage, have
   been characterized by austerity measures and budget cuts. These policies
   have increased unemployment, with the consequent social mobilizations
   and severe public criticism regarding the performance of the political
   system. This dissatisfaction also entailed a distancing by the citizenry
   from mainstream political parties and a questioning of the viability of
   the functioning of a decentralized model of State in autonomous
   communities.},
