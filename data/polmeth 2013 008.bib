@article{ WOS:000421019200009,
Author = {Martinez, Rafael and Ade, Isabel and Duran, Marien and Diaz, Antonio},
Title = {Experiences of the spanish military participation in international
   missions (2000-2012)},
Journal = {REVISTA ESPANOLA DE CIENCIA POLITICA-RECP},
Year = {2013},
Number = {32},
Pages = {205-223},
Month = {JUL},
Abstract = {This study, based on, one hundred in-depth interviews, analyzes the
   experiences of the Spanish Armed Forces in international operations. We
   explore their interactions with other actors in the theater of
   operations, their training and education prior to the mission, their
   accommodation and understanding of the rules of engagement, motivations,
   perceptions and experiences, personal and group performances, their
   degree of fulfillment of mission objectives and the psychological and/or
   family problems that participation in such a mission might engender. The
   result of this case study is part of an international project called
   Lessons learned on Asymmetric Warfare and sponsored by RC no 1 of ISA
   and the ERGOMAS Working Group Military Profession.},
