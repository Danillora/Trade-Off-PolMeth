@article{ WOS:000314713000001,
Author = {Palm, Trineke},
Title = {Embedded in Social Cleavages: An Explanation of the Variation in Timing
   of Women's Suffrage},
Journal = {SCANDINAVIAN POLITICAL STUDIES},
