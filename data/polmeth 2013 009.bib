@article{ WOS:000213001700005,
Author = {Musgrave, Meagan},
Title = {The Illinois Underground Storage Tank Fund: Tanks for Nothing},
Journal = {POLITICS \& POLICY},
Year = {2013},
Volume = {41},
Number = {5},
Pages = {765-787},
Month = {OCT},
Abstract = {At the expense of those who use petroleum in Illinois, the Underground
   Storage Tank (UST) Fund provides tank owners and operators with the
   financial support they need to remediate leaking UST sites. Little has
   been written on the sociodemographic factors that influence UST site
   remediation because most analysts characterize ``open{''} tank sites
   (those in need of remediation) using only descriptive statistics. I
   examine how socioeconomic variables and the passage of time influence
   the distribution of open sites within Illinois and find that only time
   contributes to the likelihood that a site will remain open. This result
   suggests that reducing the reimbursement timeline for tank owners and
   operators should be considered as an effective strategy for increasing
   remediation rates, both within Illinois and throughout the United
   States.},
