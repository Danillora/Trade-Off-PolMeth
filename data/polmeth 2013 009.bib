Author = {Gonzalez-Ferrer, Amparo and Morales, Laura},
Title = {Do citizenship regimes shape political incorporation? evidence from four
   european cities},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2013},
Volume = {12},
Number = {4},
Pages = {455-466},
Month = {DEC},
Abstract = {We examine how different formal citizenship regimes fare in achieving
   the political inclusion of immigrants and their children by looking at
   voting turnout in local elections. We analyse survey data from four
   European cities that grant voting rights to foreigners but are located
   in countries with contrasting rules for access to formal citizenship.
   Local voting gaps are larger where citizenship rules are more
   restrictive, which suggests that citizenship regimes have long-lasting
   effects on political inclusion.},
