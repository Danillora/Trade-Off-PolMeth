@article{ WOS:000215840900003,
Author = {Moncagatta, Paolo and Safranoff, Ana},
Title = {Who supports the Latin American `populist' left?},
Journal = {REVISTA MEXICANA DE ANALISIS POLITICO Y ADMINISTRACION PUBLICA},
Year = {2013},
Volume = {2},
Number = {1},
Pages = {29-48},
Month = {JAN-JUN},
Abstract = {At the beginning of the XXI st century, Latin America is being largely
   governed by leftist governments. Several authors have differentiated
   between a `pragmatic' left, on the one hand, and a `populist' left, on
   the other. Using data from the AmericasBarometer surveys from 2012, we
   assess the socio-demographic profiles of supporters of the `populist'
   left in four nations: Argentina, Bolivia, Ecuador and Venezuela. By
   means of logistic regression models, we analyze the intention to vote
   for the candidate or party of the current President for citizens
   belonging to different levels of education, wealth, and other
   socio-demographic variables. The results suggest that not all
   governments classified as belonging to the Latin American `populist'
   left receive the support of the same kind of individuals.},
