@article{ WOS:000420782600008,
Author = {Pozzi, Regina},
Title = {Suffrage and ``capacite{''}. A survey on a recurrent category in the
   French political debate (1815-1848)},
Journal = {STORIA DEL PENSIERO POLITICO},
Year = {2013},
Volume = {2},
Number = {3},
Pages = {483-504},
Month = {SEP-DEC},
Abstract = {The aim of this article is to examine the debate that took place in
   France on the category of ``capacite{''} applied to election laws before
   1848. It is particularly focused on doctrinaire liberalism and on
   Guizot, who was the leading figure of this political current. The Author
   argues that the category of ``capacite{''}, according to which the
   requisite for suffrage was individual worship and not census, didn't
   become a subject matter of legislation not so much for the growing
   social blindness of the July Monarchy as for the difficulty to find some
   shared defining criteria.},
