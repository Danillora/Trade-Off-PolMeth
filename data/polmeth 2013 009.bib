@article{ WOS:000212644300002,
Author = {Beyers, Jan and Bursens, Peter},
Title = {How Europe Shapes the Nature of the Belgian Federation: Differentiated
   EU Impact Triggers Both Co-operation and Decentralization},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2013},
Volume = {23},
Number = {3, SI},
Pages = {271-291},
Abstract = {This paper disentangles the impact of various dimensions of European
   integration on different aspects of the Belgian federal polity. We
   discern two opposite trends. While the institutional embeddedness of
   Belgium as an EU member state results in domestic centralizing
   tendencies and co-operative political practices, economic integration
   stimulates political pressures for regional autonomy, contributing to
   further divergence and hollowing out of the federal level. We conclude
   by arguing that the EU clearly affects territorial politics in Belgium,
   but, as the European causes are multidimensional, that also the effects,
   albeit substantial, will be diverse.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
