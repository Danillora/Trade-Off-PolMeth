@article{ WOS:000212891700001,
Author = {Gheaus, Anca},
Title = {Care drain: who should provide for the children left behind?},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2013},
Volume = {16},
Number = {1},
Pages = {1-23},
Abstract = {Care brings the traditional problem of carers' choice between paid work
   and family at a new level. Taking care drain from Romania as a case
   study, I analyse the consequences of parents' migration within a
   normative framework committed to meeting the needs of vulnerable
   individuals. The temporary migration of parents who cannot take their
   children with them involves moral harm, particularly the frustration of
   children's developmental and emotional needs. I use recent feminist work
   on justice and care in the economy to address the question whose
   responsibility it is to fill the void of care created by temporary
   migration. I argue that the moral issues raised by care drain are also
   issues of social justice and therefore call for rectification by the
   states involved.},
