@article{ WOS:000214995100001,
Author = {Marfleet, Philip},
Title = {MUBARAK'S EGYPT - NEXUS OF CRIMINALITY},
Journal = {STATE CRIME},
Year = {2013},
Volume = {2},
Number = {2},
Pages = {112-134},
Month = {FAL},
Abstract = {Employing a Marxist framework, this article examines how neo- liberal
   agendas for development pursued by successive regimes in Egypt have been
   associated with violence, fraud and corruption. Encouraged by
   international financial institutions and Western governments, the
   Egyptian state became a means of channelling public resources into
   private hands, using complex relations of privilege among officials and
   oligarchs. The article examines a growing conviction among Egypt's
   people that the presidency and the ruling party represented a criminal
   enterprise. Addressing notions of ``crony capitalism{''}, and the idea
   that the revolution of 2011 punished an aberrant political leader, it
   argues that bodies such as the International Monetary Fund and the World
   Bank have been party to consolidation of corrupt networks and
   development of an authoritarian state acting with impunity vis- a- vis
   the mass of Egypt's people. The article raises pressing questions about
   global responsibilities for state crime manifested at the local level.},
