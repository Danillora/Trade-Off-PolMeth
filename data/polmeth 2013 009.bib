@article{ WOS:000218294200006,
Author = {Mumfor, Andrew},
Title = {PROXY WARFARE AND THE FUTURE OF CONFLICT},
Journal = {RUSI JOURNAL},
Year = {2013},
Volume = {158},
Number = {2},
Pages = {40-46},
Abstract = {The contemporary dynamics of proxy warfare will make it a significant
   feature of the character of conflict in the future. Andrew Mumford
   identifies four major changes in the nature of modern warfare and argues
   that they point to a potential increase in the engagement of proxy
   strategies by states: the decreased public and political appetite in the
   West for large-scale counter-insurgency `quagmires' against a backdrop
   of a global recession; the rise in prominence and importance of Private
   Military Companies (PMCs) to contemporary war-fighting; the increasing
   use of cyberspace as a platform from which to indirectly wage war; and
   the ascent of China as a superpower.},
