@article{ WOS:000325518800005,
Author = {McKnight, David and Hobbs, Mitchell},
Title = {Public Contest through the Popular Media: The Mining Industry's
   Advertising War against the Australian Labor Government},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2013},
Volume = {48},
Number = {3},
Pages = {307-319},
Month = {SEP 1},
Abstract = {This article explores the mining tax ad war of 2010', which contributed
   to the removal of a first-term Labour prime minister and shaped
   Australia's Minerals Resource Rent Tax. In particular, it examines the
   uses, ethics and consequences of advocacy advertising, which is an
   under-explored aspect of communication power. The article identifies
   advocacy advertising as an increasingly prevalent technique used by
   corporations and lobby groups to influence public policy in Australia.
   In conclusion, the article focuses on the regulatory environment for
   such campaigns and the areas of future research that might help to
   safeguard democratic practices.},
