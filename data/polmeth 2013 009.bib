@article{ WOS:000314847200001,
Author = {Bolsen, Toby},
Title = {A Light Bulb Goes On: Norms, Rhetoric, and Actions for the Public Good},
Journal = {POLITICAL BEHAVIOR},
Year = {2013},
Volume = {35},
Number = {1},
Pages = {1-20},
Month = {MAR},
Abstract = {This paper explores whether invoking social norms, in the context of a
   persuasive appeal, affects individuals' willingness to take action for
   the public good. The framework I develop brings together a host of
   factors treated as distinct in past work, including attitudes, rhetoric,
   and social norms. I test predictions from this framework in an
   experiment that focuses on a particularly important behavior-actions
   regarding the consumption of energy. I find that highlighting norms in
   the context of an appeal for energy conservation increases the
   importance individuals' associate with these actions, intentions to
   conserve energy, and actual behavior on a light-bulb purchasing
   decision. The findings have implications more generally for
   understanding when individuals take actions that promote the public
   good.},
