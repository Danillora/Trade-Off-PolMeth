@article{ WOS:000218302100004,
Author = {Ince, Matt},
Title = {DEFEATING COLOMBIA'S OLDEST INSURGENCY PROSPECTS FOR PEACE AND
   RECONCILIATION WITH THE FARC},
Journal = {RUSI JOURNAL},
Year = {2013},
Volume = {158},
Number = {1},
Pages = {20-27},
Abstract = {The decision to enter into peace negotiations with the FARC represents
   the latest in a series of attempts by Colombia's Santos administration
   to prioritise political over military activities in seeking an end to
   the country's internal conflict. Yet despite favourable political
   conditions, a number of major challenges remain which could prevent a
   meaningful peace settlement from coming to fruition. Matt Ince argues
   that the single largest obstacle in this regard is the potential for the
   talks to cause fragmentation within the FARC, an outcome that could
   impede its leaders in negotiating a settlement viewed as legitimate by
   the entire organisation, while opening the door to the emergence of FARC
   splinter groups in the period ahead.},
