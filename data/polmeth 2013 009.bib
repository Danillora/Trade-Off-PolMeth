@article{ WOS:000211651200002,
Author = {Deacon, Bob and Stubbs, Paul},
Title = {Global social policy studies: Conceptual and analytical reflections},
Journal = {GLOBAL SOCIAL POLICY},
Year = {2013},
Volume = {13},
Number = {1, SI},
Pages = {5-23},
Month = {APR},
Abstract = {This article reviews the conceptual and analytical contribution of one
   strand of `global social policy studies' since the mid-1990s. It
   outlines some of the strengths and weaknesses of the core conceptual
   basis of the approach acknowledging that the theoretical aspects have
   remained more implicit than explicit in many core texts. The article
   advances the case for using the `Agency, Structure, Institution and
   Discourse' (ASID) approach as a framework within which to advance the
   analysis of the formation and transformation of `global social policy'.
   The recent development of the ILO and UN policy on advancing social
   protection floors is then used to illustrate how the ASID approach might
   be applied to an actual global social policy change.},
