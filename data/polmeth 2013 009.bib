@article{ WOS:000210672500004,
Author = {Jacobs, Lawrence},
Title = {Lord Bryce's Curse: The Costs of Presidential Heroism and the Hope of
   Deliberative Incrementalism},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2013},
Volume = {43},
