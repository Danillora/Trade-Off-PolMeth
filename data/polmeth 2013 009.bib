@article{ WOS:000421314100002,
Author = {Duran, Rafael},
Title = {Political corruption and mobilization of elections in the
   gerontoinmigrantes Europeans in the municipal elections of 2007},
Journal = {REVISTA DE INVESTIGACIONES POLITICAS Y SOCIOLOGICAS},
Year = {2013},
Volume = {12},
Number = {3},
Pages = {13-39},
Abstract = {Political corruption is a key theoretical issue regarding the quality of
   democracy. And it was a main theme from the empirical point of view
   during the Spanish local elections held on May 2007. This paper focuses
   upon the electoral behaviour of foreign European retirees on that
   occasion and aims to know whether and to what extent corruption
   mobilized those citizens. Data confirm that there is a relationship
   between public charges of political corruption and electoral
   mobilization, but this is not enough as to affect election results. The
   paper analyses MIRES3i survey results and basic data provided by both
   the electoral census and the population census.},
