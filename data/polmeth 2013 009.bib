@article{ WOS:000212643600003,
Author = {Isumonah, V. Adefemi and Egwaikhide, Festus O.},
Title = {Federal Presence in Higher Institutions in Nigeria and the North/South
   Dichotomy},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2013},
Volume = {23},
Number = {2},
Pages = {169-188},
Abstract = {Which region gains more from Nigeria's wealth is a subject of heated
   debate between northerners and southerners. While the former believe
   that the southern region gets more because it plays host to most of the
   economic activities of Nigeria, the latter believe that the northern
   region has through its many years of control of federal power. Given
   limited space, this study uses the regional distribution of the benefits
   of student enrolments in federal universities to provide some insights.
   The reason for the choice of university education is that it combines
   two features of being a benefit and key to certain other benefits the
   federal government dispenses. The conclusion from the data analysed is
   that the greater beneficiary of federal expenditures on education is
   southern Nigeria, which also likely has greater access to other benefits
   that come with educational qualifications. It follows that the extent of
   federal government's involvement in the provision of subsidized
   university education is the extent of advantage the south enjoys over
   the north.},
