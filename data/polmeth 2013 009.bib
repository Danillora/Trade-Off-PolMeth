@article{ WOS:000421677100004,
Author = {Ponton Cevallos, Daniel},
Title = {Why do not decriminalize drugs? The reasons for the validity of a failed
   policy},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
Year = {2013},
Number = {13},
Pages = {41-55},
Month = {JUN},
Abstract = {This paper analyzes current proposals for the decriminalization and
   legalization of psychotropic drugs emphatically discussed by authorities
   and public opinion. I argue that despite the importance of political
   powers openly discussing and questioning the worldwide prohibition on
   drugs, the variety of proposals for drug decriminalization on offer face
   technical inconsistencies that prove self-defeating. Instead, the topic
   of prohibitionism must be located as a major global and geopolitical
   problem, making such a paradigm shift impossible to overturn in the
   short, medium and long term.},
