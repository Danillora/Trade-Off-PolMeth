@article{ WOS:000209506100004,
Author = {Ludwig, Gundula},
Title = {Feminist Considerations on Postdemocracy and the Depolitization of the
   Social.},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2013},
Volume = {54},
Number = {3},
Pages = {461-484},
Abstract = {From a feminist perspective the text argues that Crouch's diagnosis of a
   `postdemocracy' remains conceptually limited because it denies the
   interwovenness of democracy and social relations. Furthermore, the
   article argues that in the current `postdemocracy' restrictions of the
   political become visible that have always been constitutive for modern,
   western democracies exactly because of an intrinsic depoliticization of
   the social, the social relations and living circumstances of the
   political subjects. This depolitization of the social can remain
   invisible not at least due to powerful gendering mechanisms. Against
   this background, the article ends by proposing to re-think democracy by
   taking the social as starting point for re-thinking the political.},
