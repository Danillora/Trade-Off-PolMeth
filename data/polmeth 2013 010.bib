@article{ WOS:000213003200004,
Author = {Lavery, Lesley},
Title = {Gender Bias in the Media? An Examination of Local Television News
   Coverage of Male and Female House Candidates},
Journal = {POLITICS \& POLICY},
Year = {2013},
Volume = {41},
Number = {6},
Pages = {877-910},
Month = {DEC},
Abstract = {Several decades of scholarship suggest that by covering male and female
   candidates differently, the news media may influence the success of
   female candidates for higher office. I employ a content analysis to
   assess gender differences in the local television news coverage of 172
   U.S. House candidates in the nation's top 50 media markets in 2002. The
   results of the study suggest that female candidates for the U.S. House
   were covered with the same frequency as male candidates, and received
   equitable issue-based and personal coverage.},
