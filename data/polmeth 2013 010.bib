@article{ WOS:000421014500002,
Author = {Nobbe, Philipp},
Title = {Electoral System Choice in Transition Countries. The Importance of
   Driving Forces},
Journal = {REVISTA ESPANOLA DE CIENCIA POLITICA-RECP},
Year = {2013},
Number = {31},
Pages = {41-62},
Month = {MAR},
Abstract = {This research paper examines the relationship between the type of
   transition and electoral system choice. The analysis is based upon a
   country sample of 51 third-wave democracies. The theoretical arguments
   lead to the hypothesis that countries whose transitions to democracy are
   driven both by the ruling elites and civic forces apply more permissive
   electoral systems afterwards. Whenever there is one dominant actor
   during the transition process, a less permissive electoral system
   becomes more likely. It is also assumed that countries that have a
   democratic past tend to rely on the institutional design of that period.
   The results of a multiple regression analysis confirm the hypotheses and
   point out that political actors behave strategically and according to
   their utility function, especially in transition processes, which are
   characterized by great uncertainty. Ideology does not matter and
   information from the past is used to predict future developments.},
