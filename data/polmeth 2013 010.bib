@article{ WOS:000320358800010,
Author = {Wiliarty, Sarah Elise},
Title = {Gender as a Modernising Force in the German CDU},
Journal = {GERMAN POLITICS},
Year = {2013},
Volume = {22},
Number = {1-2, SI},
Pages = {172-190},
Month = {JUN 1},
Abstract = {In the past twenty years, the CDU's perspective on gender issues has
   changed radically. Although the CDU has not become a feminist party, it
   has significantly modified its former stances on gender issues mostly in
   a progressive direction. The presence of female party chair and
   Chancellor Angela Merkel is the most obvious change, but the party has
   shifted its position in many other areas as well. The CDU adopted a
   gender quorum' in the mid-1990s, which has been partially successful at
   increasing women's representation within the party. Under the Grand
   Coalition the CDU-led government backed reforms to the parental leave
   law, linking payments to prior salary and explicitly encouraging fathers
   to take time off. The Grand Coalition also significantly increased child
   care for children under 3. Merkel did not initiate the party's
   modernisation, and in many cases these changes date back to the 1980s.
   Merkel has accelerated the pace of change, however. The CDU's reforms in
   gendered issue areas are quite popular with female voters and the party
   is gaining ground with women of all age groups.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
