
@article{ WOS:000217261400007,
Author = {Polonska-Kimunguyi, Eva and Kimunguyi, Patrick},
Title = {Communicating the European Union to Australia: The EU Information
   Strategy and Its Reception Down Under},
Journal = {BALTIC JOURNAL OF EUROPEAN STUDIES},
Year = {2013},
Volume = {3},
