@article{ WOS:000316709400015,
Author = {Smith, Timothy Hallam},
Title = {Are you sitting comfortably? Estimating incumbency advantage in the UK:
   1983-2010-A research note},
Journal = {ELECTORAL STUDIES},
Year = {2013},
Volume = {32},
Number = {1},
Pages = {167-173},
Month = {MAR},
Abstract = {This note adapts two models commonly used to estimate the incumbency
   advantage that US members of Congress enjoy - the `slurge' and the
   Gelman-King Index - to provide comparable estimates for UK MPs. The
   results show that Liberal Democrats enjoy extremely large such
   advantages on a par with those of US Congressmen of between 5\% and 15\%
   of the vote. Labour and the Conservatives have incumbency advantages at
   around 2\% and 1\% respectively. The note estimates that effects could
   have changed the outcome in as many as 25 seats in some elections, and
   that they cost the Conservatives the chance to govern alone after the
   2010 election. (C) 2012 Elsevier Ltd. All rights reserved.},
