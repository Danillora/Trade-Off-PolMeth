@article{ WOS:000212995600004,
Author = {Correa-Cabrera, Guadalupe},
Title = {Security, Migration, and the Economy in the Texas-Tamaulipas Border
   Region: The ``Real{''} Effects of Mexico's Drug War},
Journal = {POLITICS \& POLICY},
Year = {2013},
Volume = {41},
Number = {1},
Pages = {65-82},
Month = {FEB},
Abstract = {This article analyzes the effects of Mexico's drug war on security,
   migration, and the economy on the eastern U.S.-Mexico border between the
   state of Texas and the Mexican state of Tamaulipas. Both sides of the
   border are analyzed and compared simultaneously. The article shows that
   the extreme violence that Mexico is suffering has benefited U.S. border
   towns while having a negative impact on Tamaulipas. The positive effects
   of Mexico's violent spiral on U.S. border security and the U.S. border
   economy are evident, notwithstanding American public discourse and the
   reporting of U.S. media alleging the opposite.},
