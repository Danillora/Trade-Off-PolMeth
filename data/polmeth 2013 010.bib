@article{ WOS:000218942500002,
Author = {Moreno-Brid, Juan Carlos},
Title = {Industrial Policy: A Missing Link in Mexico's Quest for Export-led
   Growth},
Journal = {LATIN AMERICAN POLICY},
Year = {2013},
