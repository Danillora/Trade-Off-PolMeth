@article{ WOS:000316709400016,
Author = {Meyer, Thomas M. and Jenny, Marcelo},
Title = {Measuring error for adjacent policy position estimates: Dealing with
   uncertainty using CMP data},
Journal = {ELECTORAL STUDIES},
Year = {2013},
Volume = {32},
Number = {1},
Pages = {174-185},
Month = {MAR},
Abstract = {Careful users of CMP party position data should take the uncertainty of
   position estimates into account. We compare and evaluate two current
   approaches that provide error estimates for party positions. Researchers
   of the CMP group identify measurement error in quantitative content
   analysis as the cause of uncertainty about position estimates, whereas a
   second approach by Benoit et al. (2009) attributes the uncertainty of
   position estimates to a stochastic generation of election programs. We
   illustrate the commonalities and differences of these approaches and
   provide two empirical applications, the identification of the left-right
   order of parties and of policy shifts by parties, using CMP data for 25
   countries. Despite conceptual differences, results in these applications
   are surprisingly similar. (C) 2012 Elsevier Ltd. All rights reserved.},
