@article{ WOS:000322755300005,
Author = {Fortin-Rittberger, Jessica and Eder, Christina},
Title = {Towards a Gender-Equal Bundestag? The Impact of Electoral Rules on
   Women's Representation},
Journal = {WEST EUROPEAN POLITICS},
Year = {2013},
Volume = {36},
Number = {5},
Pages = {969-985},
Month = {SEP 1},
Abstract = {Cross-sectional research designs show that proportional representation
   (PR) tends to produce more equitable outcomes in terms of gender
   representation. While existing research provides valuable insights into
   the covariates of gender representation, such cross-sectional designs
   fail to provide a definitive test of the influence of electoral rules
   both over time and in the case of mixed systems. Addressing this gap,
   this article proposes a longitudinal research design using the case of
   Germany, because half the Bundestag's seats are allotted through
   majoritarian first-past-the-post methods, while the other half are
   allotted through proportional methods. The main findings point towards
   heavy interaction between the two electoral tiers that becomes visible
   in party quotas, double nominations and increased competitiveness of
   female candidates.},
