@article{ WOS:000326990300004,
Author = {Savigny, Heather},
Title = {The (political) idea of a university: political science and
   neoliberalism in english higher education},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2013},
Volume = {12},
Number = {4},
Pages = {432-439},
Month = {DEC},
Abstract = {This article is concerned primarily with the relationship between
   academic ideas and the `real world' of politics. Disciplinary histories
   often assume a one-way influence of ideas, that of the academy into
