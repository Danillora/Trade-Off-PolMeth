@article{ WOS:000312642900009,
Author = {Mahon, Rianne and Brennan, Deborah},
Title = {Federalism and the ``New Politics{''} of Welfare Development: Childcare
   and Parental Leave in Australia and Canada},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2013},
Volume = {43},
Number = {1, SI},
Pages = {90-108},
Month = {WIN},
Abstract = {How does federal state architecture affect the design of welfare? We
   explore the development of childcare and parental leave in Canada and
   Australia to address this question. Both countries are considered
   liberal welfare regimes, but their federal institutions operate in quite
   different ways, providing an opportunity for comparative analysis. We
   consider the ways in which federal institutions have affected
   mobilization around childcare and parental leave and have helped to
   shape policy outcomes. There is a plausible connection between the
   institutional configuration of each federation and policy design. It is
   not definitive, but interacts with variables such as the nature and
   scale of federal fiscal and policy capacity, the gendered assumptions
   embedded in the structures of the welfare state, political party
   strategies, and feminist mobilization.},
