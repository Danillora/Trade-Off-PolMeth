@article{ WOS:000323399900007,
Author = {Le Ru, Veronique},
Title = {The sciences of l'Encyclopaedia and d'Alembert},
Journal = {PENSEE},
Year = {2013},
Number = {374},
Pages = {63+},
Month = {APR-JUN},
Abstract = {The purpose of my paper is to show that d'Alembert, by his reputation
   and his status as a scientist took control of the classification and
   organization of sciences in the Encyclopaedia's hierarchical system.
   This is well proved by the ``Illustrated System of Knowledge{''} and the
   ``Preliminary Discourse{''} in the Encyclopaedia's first volume. My
   first point is to explain the main lines of d'Alembert's epistemology.
   The second point is to make obvious there was a real discussion between
   the two directors, by writing, reading texts and answering each other.
   When d'Alembert decided to resign, Diderot could at last take the wheel
   and give utterance to his opinion that experimental science prevailed
   over mathematical science.},
