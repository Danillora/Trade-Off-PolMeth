@article{ WOS:000329013500009,
Author = {Crisp, Brian F. and Olivella, Santiago and Malecki, Michael and Sher,
   Mindy},
Title = {Vote-earning strategies in flexible list systems: Seats at the price of
   unity},
Journal = {ELECTORAL STUDIES},
Year = {2013},
Volume = {32},
Number = {4},
Pages = {658-669},
Month = {DEC},
Abstract = {In theory, flexible list systems are a compromise between closed-list
   and open-list proportional representation. A party's list of candidates
   can be reordered by voters if the number of votes cast for an individual
   candidate exceeds some quota. Because these barriers to reordering are
   rarely overcome, these systems are often characterized as basically
   closed-list systems. Paradoxically, in many cases, candidates are
   increasingly earning individual-level preference votes. Using data from
   Slovakia, we show that incumbents cultivate personal reputations because
   parties reward preference vote earning candidates with better
   pre-election list positions in the future. ironically, the party's
   vote-earning strategy comes at a price, as incumbents use voting against
   the party on the chamber floor to generate the reputations that garner
   preference votes. (C) 2013 Elsevier Ltd. All rights reserved.},
Publisher = {ELSEVIER SCI LTD},
