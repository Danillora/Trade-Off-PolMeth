@article{ WOS:000421314100006,
Author = {Casado Neira, David and Costoya Bello, Gemma},
Title = {Victims and citizens. The themes of childhood in the Spanish press},
Journal = {REVISTA DE INVESTIGACIONES POLITICAS Y SOCIOLOGICAS},
Year = {2013},
Volume = {12},
Number = {3},
Pages = {103-121},
Abstract = {Children are active consumer and target of the media, but they also
   appear as subject of the news. The presence of themes related to
   childhood in the media leads us to consider what representations of the
   children are made in the media. We would like to present in this paper a
   comparative study on printed media, an analysis of the representation of
   children in two state-run newspapers, each of them with different
   political editorial lines (El Pais and El Mundo). The results show that
   there is not a clear difference between the editorial line of both
   newspapers, as well as the topics concerning children. These lead to a
   representation of children as vulnerable subjects (proto-victims) and as
   destabilizers of the social order.},
