@article{ WOS:000420547300001,
Author = {Wenzelburger, Georg},
Title = {The politics of law and order - An empirical exploration from a
   comparative public policy perspective},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2013},
Volume = {7},
Number = {1},
Pages = {1-25},
Month = {MAR},
Abstract = {Researchers in comparative public policy have dedicated little to no
   attention to the politics of Law and Order. This fact motivated us to
   present a portrait of this field of research and to show how it can be
   studied from the perspective of comparative politics in order to
   elaborate existing theories and to increase research findings in this
   policy field. In order to do so, we combine various theoretical
   approaches from sociology, criminology and comparative public policy. In
   addition, we conduct an empirical analysis which shows that the politics
   of Law and Order vary considerably cross-nationally and over time. We
   conclude with prospects for future political scientific analyses in the
   field of politics of Law and Order.},
