@article{ WOS:000210641200012,
Author = {Mulder, Catherine},
Title = {Unions' Resistance to Capital and the Potential for Class Transformation},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2013},
Volume = {25},
Number = {1},
Pages = {114-120},
Month = {JAN},
Abstract = {This is a response to the comments made by David Brennan, Graham
   Cassano, and Vincent Lyon-Callo on my book Unions and Class
   Transformation: The Case of the Broadway Musicians. Following the book's
   publication, I remain critical and continue to think about unions' roles
   in combating capitalism. Here I address some of my new/revised thoughts
   thanks to the insightful comments by the symposium's participants. In
   the book I argued that the musicians' union could facilitate a class
   transformation from capitalism to communism for the Broadway musicians.
   However, given the union's own internal capitalist class structure and
   the exclusivity of the ``club'' of Broadway musicians, there may be more
   intense barriers to class transformation than I previously believed.
   Nonetheless, though barriers abound, the book remains a model for unions
   to improve not only the Broadway musicians' work environment, but others
   as well.},
