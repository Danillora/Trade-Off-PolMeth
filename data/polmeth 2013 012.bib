
@article{ WOS:000321109300003,
Author = {Rigby, Elizabeth and Wright, Gerald C.},
Title = {Political Parties and Representation of the Poor in the American States},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
Year = {2013},
Volume = {57},
Number = {3},
Pages = {552-565},
Month = {JUL},
Abstract = {Motivated by recent work suggesting that low-income citizens are
   virtually ignored in the American policymaking process, this article
   asks whether a similar bias shapes the policy positions adopted by
   political parties much earlier in the policymaking process. While the
   normative hope is that parties serve as linkage institutions enhancing
   representation of those with fewer resources to organize, the
   resource-dependent campaign environment in which parties operate
   provides incentives to appeal to citizens with the greatest resources.
   Using newly developed measures of state party positions, we examine
   whether low-income preferences get incorporated in parties' campaign
   appeals at this early stage in the policymaking processfinding little
   evidence that they do. This differential responsiveness was most
   pronounced for Democratic parties in states with greater income
   inequality; it was least evident for Republicans' social policy
   platforms. We discuss the implications of these findings for
   representation in this era of growing economic inequality.},
