@article{ WOS:000325183600009,
Author = {Rossiter, David and Johnston, Ron and Pattie, Charles},
Title = {Representing People and Representing Places: Community, Continuity and
   the Current Redistribution of Parliamentary Constituencies in the UK},
Journal = {PARLIAMENTARY AFFAIRS},
