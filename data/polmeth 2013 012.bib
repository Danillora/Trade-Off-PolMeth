@article{ WOS:000320967500006,
Author = {Uribe Otalora, Ainhoa},
Title = {Gender quotas and their application in Spain: the effects of the
   Equality Act (LO 3/2007) on the State Parliament and Regional
   Parliaments},
Journal = {REVISTA DE ESTUDIOS POLITICOS},
Year = {2013},
Number = {160},
Pages = {159-197},
Month = {APR-JUN},
Abstract = {The present article aims at delving into the impact of the Spanish
   gender quota, imposed by the Equality Act, named ``Ley Organica 3/2007,
   de 22 de marzo, para la igualdad efectiva entre mujeres y hombres{''},
   five years after its approval. Hence, the article tries to verify the
   law,s consequences in the National Parliament (in the Deputy Congress
   and the Senate) and in the Territorial Parliaments, from a political
   perspective. Therefore, the article examines the behaviour of the main
   political parties, as well as the elements of the Spanish electoral
   system and the design of the gender quota. The target is to assess if
   there is or not an improvement of the Spanish women empowerment in the
   legislative power due to the Act of Equality.},
