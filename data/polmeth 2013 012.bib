@article{ WOS:000209506100005,
Author = {Joerke, Dirk},
Title = {Re-democratization of Post-democracy by Alternative Procedures of
   Participation?},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2013},
Volume = {54},
Number = {3},
Pages = {485-505},
Abstract = {Since some years political scientists observe a growing political
   dissatisfaction. On closer inspection one sees that mainly socially
   disadvantaged citizens have retreated from the democratic process. This
   applies not only to the participation in elections and membership in
   political parties, but to a greater degree also to alternative forms of
   political participation such as town meetings and referenda. Against
   this background, the article is looking for possibilities of a
   reintegration of socially disadvantaged citizens into the democratic
   process. Following considerations made by Young and McCormick, who
   highlight an ``aristocratic effect{''} of formal democratic procedures,
   a suspensive veto-right for socially disadvantaged citizens is proposed
   at the end.},
