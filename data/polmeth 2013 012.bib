@article{ WOS:000218279200004,
Author = {Ince, Matt},
Title = {FILLING THE FARC-SHAPED VOID POTENTIAL INSECURITY IN POST-CONFLICT
   COLOMBIA},
Journal = {RUSI JOURNAL},
Year = {2013},
Volume = {158},
Number = {5},
Pages = {26-34},
Abstract = {If ongoing peace talks with the FARC succeed, it is unclear what will
   become of the guerrilla group's illicit finance networks; who would
   replace the FARC in the drugtrafficking chain should they demobilise;
   and what would happen to their business interests in other illegal
   sectors. Against this backdrop, Matt Ince assesses whether a potential
   future peace deal with Colombia's oldest insurgent group could open the
   door to new sources of violence and insecurity in Colombia in the period
   ahead, as rival criminal organisations compete with one another to gain
   control over the FARC's current portfolio of illegal activities.},
