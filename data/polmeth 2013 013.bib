   Temple Univ, Philadelphia, PA 19122 USA.},
DOI = {10.1111/ssqu.12014},
ISSN = {0038-4941},
Research-Areas = {Government \& Law; Sociology},
Web-of-Science-Categories  = {Political Science; Sociology},
Author-Email = {mhooper@temple.edu},
Affiliations = {Pennsylvania Commonwealth System of Higher Education (PCSHE); Temple
