@article{ WOS:000218572300001,
Author = {Stout, Christopher and Tate, Katherine},
Title = {The 2008 presidential election, political efficacy, and group
   empowerment},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2013},
Volume = {1},
Number = {2},
Pages = {143-163},
Abstract = {Primarily due to lack of data, previous cross-sectional studies on
   descriptive representation have not been able to address the temporal
   criticisms about the effects of descriptive representation on Black and
   female political attitudes. We address this shortcoming in the
   literature using the growth curve models and the 2008-2009 American
   National Election Study Panel Survey to assess whether bids for the
   White House by Obama and Palin changed levels of internal and external
   efficacy among Blacks and women. Moreover, we examine internal
   differences within these groups to better understand which factors
   interact with race and gender to produce the largest empowerment
   effects. While still very much a contentious issue, we find that
   descriptive group representation for numerically underrepresented social
   groups, and particularly for Black women, has dynamic and transformative
   effects on political efficacy. Conversely, we find mixed evidence that
   women were empowered by Sarah Palin's candidacy.},
