@incollection{ WOS:000318476300004,
Author = {Neagu, Maria-Ionela},
Book-Author = {Neagu, MI},
Title = {Symbolic Power and Argumentation},
Booktitle = {DECODING POLITICAL DISCOURSE: CONCEPTUAL METAPHORS AND ARGUMENTATION},
Series = {Palgrave Pivot},
Year = {2013},
Pages = {73-109},
Abstract = {Decoding the conceptual metaphors embedded in the premises of a
   practical argument highlights the ways in which thinking and speaking
   metaphorically correlates with social action and practice. The empirical
   investigation takes a different turn in this chapter. It is argued that
   agents perceive the world according to their habitus, their instilled
   dispositions, and internalized values which impact upon their use of
   language. The range of argumentation schemes that speakers exploit shed
   some light upon the strategies deployed to defend or to refute a certain
   standpoint. The reconstruction and evaluation of the argument for ending
   the war in Iraq clearly accounts for Lakoff's (2009: 67) idea that
   `framing comes before policy.'},
