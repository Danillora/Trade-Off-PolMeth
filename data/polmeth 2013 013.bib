@article{ WOS:000327029800008,
Author = {Praino, Rodrigo and Stockemer, Daniel and Moscardelli, Vincent G.},
Title = {The Lingering Effect of Scandals in Congressional Elections: Incumbents,
   Challengers, and Voters},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2013},
Volume = {94},
Number = {4},
Pages = {1045-1061},
Month = {DEC},
Abstract = {ObjectiveWe have two goals. First, we investigate both the short- and
   long-term electoral impact of involvement in scandals on reelection
   margins of incumbents in U.S. congressional elections. Second, we
   evaluate the impact of scandals on district-level turnout.
   MethodsWe model the impact of involvement in a political scandal on
   incumbents' electoral margins in the election cycle in which the scandal
   comes to light, as well as in future election cycles. We also model the
   impact of scandal on district-level turnout.
   ResultsInvolvement in a scandal exerts not only an immediate, negative
   effect on incumbents' margins, but one that also lingers beyond the
   initial reelection cycle. Elections involving incumbents embroiled in
   scandals experience a small boost in turnout.
   ConclusionIn tandem, these results implicate the mobilization of
   previous nonvoters intent on throwing the bum out as one mechanism
   through which incumbent vote share is depressed in scandal elections.},
