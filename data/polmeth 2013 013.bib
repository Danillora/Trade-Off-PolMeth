@article{ WOS:000212031800001,
Author = {Liu, Wei and Li, Wenzhao},
Title = {Public Administration in China-Evolution and Current Challenge},
Journal = {ASIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2013},
Volume = {21},
Number = {1},
Pages = {1-20},
Abstract = {Taking an ecological approach, this article examines the resumption,
   formation, and development of public administration (PA) as a discipline
   in China. Through content analysis and process tracing, we argue that
   the social contexts during China's transition period and existing
   knowledge, both endogenous and exogenous, are major forces shaping the
   nature of the discipline and pushing its development. We also point out
   that Chinese PA now is facing challenges, calling for deeper
   localization and closer integration into global scholarship.},
