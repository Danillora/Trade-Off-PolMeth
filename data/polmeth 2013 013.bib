@article{ WOS:000449420300002,
Author = {Walzer, Michael},
Title = {Should we reclaim political utopianism?},
Journal = {EUROPEAN JOURNAL OF POLITICAL THEORY},
Year = {2013},
Volume = {12},
Number = {1},
Pages = {24-30},
Month = {JAN},
Abstract = {Isaiah Berlin welcomed the decline of political utopianism and was eager
   to help it along. But there are reasons to think that occasional moments
   of utopian aspiration and enthusiasm are necessary to sustain even
   moderate liberal regimes. In all such regimes there are `natural'
   tendencies towards authoritarianism and hierarchy, which have to be
   resisted. And it is hard to image a successful resistance that isn't
   fuelled by something very like political utopianism.},
