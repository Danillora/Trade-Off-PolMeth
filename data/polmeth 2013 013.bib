@article{ WOS:000325760800004,
Author = {Fleckenstein, Timo},
Title = {Learning to Depart from a Policy Path: Institutional Change and the
   Reform of German Labour Market Policy},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2013},
Volume = {48},
Number = {1},
Pages = {55+},
Month = {JAN},
Abstract = {While new institutionalism with its path-dependence theory has proved to
   be an especially powerful device for explaining the stability and
   inertia of public policies, its focus on the stickiness of institutions
   has contributed to conceptual deficits in grasping and explaining
   actually occurring policy change which have attracted much criticism.
   With reference to the critical case of German labour market reforms,
   policy learning is identified as a key mechanism in the paradigmatic
   transformation of social policy. Pursuing the argument that learning
   does not happen in a vacuum and is institutionally embedded, policy
   learning is conceptually enriched with insights from new
   institutionalism to develop an institutional account of learning. Such
   an approach to policy learning and a stronger emphasis on ideas address
   the stability bias in new institutionalism and its path-dependence
   theory by accounting for knowledge-based institutional change.},
