@article{ WOS:000321897600004,
Author = {Pickering, Andrew C. and Rockey, James},
Title = {Ideology and the size of US state government},
Journal = {PUBLIC CHOICE},
Year = {2013},
Volume = {156},
Number = {3-4},
Pages = {443-465},
Month = {SEP},
Abstract = {This paper theorizes that the impact of ideology on the size of US state
   governments increases with state income. This idea is tested using
   state-level ideology data derived from the voting behavior of state
   congressional representatives. Empirically the interaction of ideology
   and mean income is a key determinant of state government size. At 1960s
   levels of income the impact of ideology is negligible. At 1997 levels of
   income a one standard-deviation move towards the left of the ideology
   spectrum increases state government size by about half a standard
   deviation. Estimated income elasticities differentiated by state and
   time are found to be increasing with ideology and diminishing with
   income, as predicted by the theory.},
