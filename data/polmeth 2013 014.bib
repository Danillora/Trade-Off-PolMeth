@article{ WOS:000314725200010,
Author = {Cotterill, Sarah and John, Peter and Richardson, Liz},
Title = {The Impact of a Pledge Request and the Promise of Publicity: A
   Randomized Controlled Trial of Charitable Donations},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2013},
Volume = {94},
Number = {1},
Pages = {200-216},
Month = {MAR},
Abstract = {Objective This study investigates whether asking people to make a pledge
   causes them to donate to a charitable cause and whether the promise of
   public recognition increases the effectiveness of the request. Method A
   randomized controlled trial in Manchester, United Kingdom, where
   households were sent letters asking them to donate a book for school
   libraries in South Africa. Results People who are asked to make a pledge
   and offered local public recognition are more likely to make a book
   donation than the control group. The combination of requesting a pledge
   and offering publicity raises book donations from 7.3 percent to 8.9
   percent of households, an effect size of 22 percent. Asking people to
   pledge alone, without the promise of publicity has no statistically
   significant impact on giving. Conclusion Combining a pledge request and
   the promise of local publicity increases individual charitable
   donations.},
