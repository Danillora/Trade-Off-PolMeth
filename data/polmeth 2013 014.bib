@article{ WOS:000328720200006,
Author = {Schuster, Philipp and Schmitt, Carina and Traub, Stefan},
Title = {The retreat of the state from entrepreneurial activities: A convergence
   analysis for OECD countries, 1980-2007},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2013},
Volume = {32},
Pages = {95-112},
Month = {DEC},
Abstract = {In this paper, we measure the extent and dissemination of the retreat of
   the state from entrepreneurial activities in 20 OECD countries during
   the 1980-2007 period using a three-dimensional concept and a new
   dataset, the REST database. We consider privatization, deregulation in
   network-based services, and the cutback of subsidies. Though the
   empirical analysis confirms a clear trend towards diminishing state
   influence in all three dimensions, the analysis of both sigma and
   conditional beta convergence indicates convergence only for
   privatization (measured by public employment) and for subsidization,
   indicating that OECD countries have become similar and have converged to
   a common equilibrium with respect to these dimensions. (C) 2013 Elsevier
   B.V. All rights reserved.},
