@article{ WOS:000320872000007,
Author = {Jacobs, Carly M. and Theiss-Morse, Elizabeth},
Title = {Belonging in a ``Christian Nation{''}: The Explicit and Implicit
   Associations between Religion and National Group Membership},
Journal = {POLITICS AND RELIGION},
Year = {2013},
Volume = {6},
Number = {2},
Pages = {373-401},
Month = {JUN},
Abstract = {If many consider the United States to be a Christian nation, how does
   this affect individuals who are American citizens but not Christian? We
   test two major hypotheses: (1) Americans consider Christians to be more
   fully American than non-Christians. We examine whether Americans
   explicitly and implicitly connect being Christian with being a true
   American; and (2) Christian Americans are more likely to be patriotic
   and set exclusive boundaries on the national group than non-Christian
   Americans. Among non-Christians, however, those who want to be fully
   accepted as American will be more patriotic and set more exclusive
   boundaries to emulate prototypical Americans than non-Christians who
   place less emphasis on national group membership. We test these
   hypotheses using data from a survey and from an Implicit Association
   Test. We find that Americans in general associate being Christian with
   being a true American. For Christians, this is true both explicitly and
   implicitly. For non-Christians, only the implicit measure uncovers an
   association. We also found that non-Christians exhibit significantly
   more pro-national group behaviors when they desire being prototypical
   than when they do not.},
