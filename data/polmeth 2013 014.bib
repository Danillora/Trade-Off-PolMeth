@article{ WOS:000318305600010,
Author = {Klugman, Joshua and Condran, Gretchen and Wray, Matt},
Title = {The Role of Medicolegal Systems in Producing Geographic Variation in
   Suicide Rates},
Journal = {SOCIAL SCIENCE QUARTERLY},
