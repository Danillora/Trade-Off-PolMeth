@article{ WOS:000328650200003,
Author = {Antonello, Alessandro},
Title = {Australia, the International Geophysical Year and the 1959 Antarctic
   Treaty},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2013},
Volume = {59},
Number = {4},
Pages = {532-546},
Month = {DEC},
Abstract = {Australia's experience of the International Geophysical Year (IGY) in
   Antarctica (1957-8) tempers the dominant narrative of the IGY as a
   transformative event in Antarctic affairs. This article argues that the
   IGY was not a great rupture in Australia's relationship with Antarctica.
   Rather, because of a long cultural attachment to and continuing
