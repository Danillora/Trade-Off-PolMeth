@article{ WOS:000314531800005,
Author = {Abbott, Andrew and Jones, Philip},
Title = {Procyclical government spending: a public choice analysis},
Journal = {PUBLIC CHOICE},
Year = {2013},
Volume = {154},
Number = {3-4},
Pages = {243-258},
Month = {MAR},
Abstract = {Procyclical government spending occurs when government expenditures
   increase at a faster rate than income in an economic upturn but fall at
   a faster rate in a recession. Voracity effects occur when competition
   for increased spending proves more effective as national income
   increases. Public choice theory can be applied to describe the
   distribution of fiscal power across different tiers of government to
   shed insight into competition for intergovernmental transfers.
   Politicians have electoral incentives to press for intergovernmental
   transfers but they also have electoral incentives to signal their
   ability to manage the economy. With this mix of incentives, the
   prediction is that intergovernmental transfers will be procyclical and
   that sub-central government spending will be more procyclical than
   central government spending. Public choice analysis of pressure for
   increased public spending predicts a specific pattern of cyclical
   government spending. This pattern can be observed when analyzing
   government expenditures in 20 OECD countries between 1995 and 2006.},
Publisher = {SPRINGER},
Address = {VAN GODEWIJCKSTRAAT 30, 3311 GZ DORDRECHT, NETHERLANDS},
Type = {Article},
Language = {English},
Affiliation = {Jones, P (Corresponding Author), Univ Hull, Sch Business, Cottingham Rd, Kingston Upon Hull HU6 7RX, N Humberside, England.
   Abbott, Andrew, Univ Hull, Sch Business, Kingston Upon Hull HU6 7RX, N Humberside, England.
   Jones, Philip, Univ Bath, Dept Econ, Bath BA2 7AY, Avon, England.},
DOI = {10.1007/s11127-011-9816-9},
ISSN = {0048-5829},
EISSN = {1573-7101},
Keywords = {Business cycles; Fiscal policy; Voracity effects; Volatility},
