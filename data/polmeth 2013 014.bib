@article{ WOS:000315425400003,
Author = {Aleksynska, Mariya and Havrylchyk, Olena},
Title = {FDI from the south: The role of institutional distance and natural
   resources},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2013},
Volume = {29},
Pages = {38-53},
Month = {MAR},
Abstract = {Using a novel dataset of bilateral FDI flows, we analyze location
   choices of investors from emerging economies, with an emphasis on
   institutions and natural resources. We show that FDI from the South has
   a more regional aspect than investment from the North. Institutional
   distance has an asymmetric effect on FDI depending on whether investors
   choose countries with better or worse institutions. In the latter case,
   large institutional distance discourages FDI inflows, but this deterring
   effect is diminished for destination countries with substantial
   resources. We also find a complementary relationship between capital
   flows from the North and the South in developing recipient countries,
   which we attribute to different FDI patterns of these investors. (C)
   2012 Elsevier B.V. All rights reserved.},
Publisher = {ELSEVIER SCIENCE INC},
