@article{ WOS:000217436600008,
Author = {Carroll, David J. and Davis-Roberts, Avery},
Title = {The Carter Center and Election Observation: An Obligations-Based
   Approach for Assessing Elections},
Journal = {ELECTION LAW JOURNAL},
Year = {2013},
Volume = {12},
Number = {1},
Pages = {87-93},
Month = {MAR},
Abstract = {This article provides an overview of recent efforts by the Carter Center
   to establish an analytical framework for election observation
   assessments based in public international law. The authors argue that by
   basing the framework on international legal commitments that states have
   accepted freely, election assessments can be more transparent,
   objective, and acceptable to host countries. The authors also argue that
   an obligations based approach to election assessment provides a
   promising avenue for fostering consensus on the elements of democratic
   elections, as well as the assessment criteria used by observer groups.
   This article includes details of the practical tools used by Carter
   Center observation missions including the Database of Obligations for
   Democratic Elections, and forthcoming publications such as the Center's
   methodology handbook.},
