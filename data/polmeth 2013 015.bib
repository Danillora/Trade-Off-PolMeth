@article{ WOS:000320743100001,
Author = {Comsa, Mircea and Postelnicu, Camil},
Title = {Measuring Social Desirability Effects on Self-Reported Turnout Using the
   Item Count Technique},
Journal = {INTERNATIONAL JOURNAL OF PUBLIC OPINION RESEARCH},
Year = {2013},
Volume = {25},
Number = {2},
Pages = {153-172},
Month = {SUM},
Abstract = {In this article, the authors offer a comprehensive review of the item
   count technique (ICT) and a detailed application of the method in voter
   turnout drawing on a three-wave national panel survey. They critically
   discuss the methodological and practical problems occurring throughout
   the implementation of ICT design in a survey research. Furthermore, the
   authors use ICT to estimate the social desirability bias in
   self-reported turnout and to evaluate the effectiveness of using ICT to
   reduce turnout over-reporting and to estimate the real turnout. Results
   align to the expected direction: By using ICT, the estimation of
   self-reported turnout is reduced by about 10\%. Moreover, social
   desirability bias varies with respondents' socio-demographic
   characteristics.},
