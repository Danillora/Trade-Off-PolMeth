@article{ WOS:000213010500003,
Author = {Edgerly, Stephanie and Vraga, Emily K. and Dalrymple, Kajsa E. and
   Macafee, Timothy and Fung, Timothy K. F.},
Title = {Directing the Dialogue: The Relationship Between YouTube Videos and the
   Comments They Spur},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2013},
Volume = {10},
Number = {3},
Pages = {276-292},
Abstract = {This study performs a content analysis of 207 YouTube videos and 45,759
   comments about the Proposition 8 campaign in California. Specifically,
   we examine how a video's tone and focus are related to comment features.
   We find consistent support for the flow of information from topics
   mentioned in the video to topics addressed in commentary, as well as
   uptake of an uncivil tone from the video to the comments. Implications
   are discussed for promoting higher quality online information exchanges
   and the democratic merits of social media.},
