@article{ WOS:000315629800005,
Author = {Wonneberger, Anke and Schoenbach, Klaus and van Meurs, Lex},
Title = {Dimensionality of TV-news Exposure: Mapping News Viewing Behavior with
   People-Meter Data},
Journal = {INTERNATIONAL JOURNAL OF PUBLIC OPINION RESEARCH},
Year = {2013},
Volume = {25},
Number = {1},
Pages = {87-107},
Month = {SPR},
Abstract = {The goal of this study is to explore different facets of exposure to
   television news-from the regularity of exposure via viewing durations to
   deliberate news selection and avoidance behavior. Using Dutch
   people-meter data on the individual viewer level, nine different
   exposure measures are compared. Our results suggest that exposure to
   news entails a three-dimensional construct consisting of the amount of
   watching the news, modes of program selection, and the share of viewing
   news. In addition, the ability of different aspects of news exposure to
   be predicted by relevant viewer characteristics is assessed. The amount
   of exposure is most strongly and most consistently related to those
   characteristics. Implications for public opinion research are discussed.},
