@article{ WOS:000317703500004,
Author = {Desmarchelier, Benoit and Djellal, Faridah and Gallouj, Faiz},
Title = {Does tertiarization foster the production of municipal waste? The
   discussion of a paradox on the basis of a multi-agent model},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2013},
Volume = {123},
Number = {1},
Pages = {83-126},
Month = {JAN-FEB},
Abstract = {This article proposes an explanation for the paradox of the polluting
   tertiarization. We are particularly interested in the growth of
   municipal wastes which seems to follow the tertiarization process in
   developed economies. Relying on recent evolutionary works proposing a
   microfoundation of structural change based on the agents'adaptative
   behaviors, we build a multi-agent system and we evaluate its relevance
   regarding stylized facts in the field of municipal wastes production.
   Finally, we use the model to compare the efficiency of eco-taxes and
   regulatory policies regarding the reduction of municipal wastes.},
Publisher = {EDITIONS DALLOZ},
Address = {31-35 RUE FROIDEVAUX, PARIS CEDEX 14, 75685, FRANCE},
Type = {Article},
Language = {French},
Affiliation = {Desmarchelier, B (Corresponding Author), Lab Clerse, F-59655 Villeneuve Dascq, France.
   Desmarchelier, Benoit; Djellal, Faridah; Gallouj, Faiz, Lab Clerse, F-59655 Villeneuve Dascq, France.},
DOI = {10.3917/redp.231.0083},
ISSN = {0373-2630},
EISSN = {2105-2883},
Keywords = {waste production; tertiarization process; multi-agents system},
