@article{ WOS:000315425400001,
Author = {Rogers, Douglas B. and Smith, Adam C. and Wilson, Bart J.},
Title = {Violence, access, and competition in the market for protection},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2013},
Volume = {29},
Pages = {1-17},
Month = {MAR},
Abstract = {We conduct a laboratory experiment to examine the performance of a
   market for protection. As the central feature of our treatment
   comparisons, we vary the access that ``peasants{''} have to
   violence-empowered ``elites{''}. The focus of the experiment is to
   observe how elites enforce and operate their protective services to
   peasants, and to observe the degree to which elites engage in
   wealth-destroying violence in competition amongst each other for
