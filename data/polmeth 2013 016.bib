@article{ WOS:000317522500003,
Author = {Murdie, Amanda},
Title = {The Bad, the Good, and the Ugly: The Curvilinear Effects of
   Civil-Military Conflict on International Crisis Outcome},
Journal = {ARMED FORCES \& SOCIETY},
