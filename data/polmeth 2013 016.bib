@article{ WOS:000341581200003,
Author = {Samuel, Chris},
Title = {Symbolic Violence and Collective Identity: Pierre Bourdieu and the
   Ethics of Resistance},
Journal = {SOCIAL MOVEMENT STUDIES},
Year = {2013},
Volume = {12},
Number = {4},
Pages = {397-413},
Abstract = {Social movement researchers have only recently begun to make use of the
   rich explanatory leverage offered by Pierre Bourdieu's theory of
   practice. The normative implications of his framework remain almost
   entirely undeveloped. This paper seeks to redress these shortcomings by
   applying central concepts from Bourdieu's work-symbolic violence and his
   realist philosophy of science-to social movement analysis. Using North
   American lesbian, gay, bisexual, transgender and queer collective
   identities to illustrate this application, the paper advocates shifting
   analysis of collective identities from models based on negotiation to
   one based on domination and advocates treatment of social movement
   organizations as sites of justice amenable to normative evaluation and
   critique.},
