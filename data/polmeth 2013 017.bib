@article{ WOS:000322651300004,
Author = {Hirsch, Boris and Koenig, Marion and Moeller, Joachim},
Title = {Is There a Gap in the Gap? Regional Differences in the Gender Pay Gap},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2013},
Volume = {60},
Number = {4},
Pages = {412-439},
Month = {SEP},
Abstract = {In this paper, we investigate regional differences in the gender pay gap
   both theoretically and empirically. Within a spatial model of
   monopsonistic competition, we show that more densely populated labour
   markets are more competitive and constrain employers' ability to
   discriminate against women. Utilizing a large administrative data set
   for western Germany and a flexible semi-parametric propensity score
   matching approach, we find that the unexplained gender pay gap for young
   workers is substantially lower in large metropolitan than in rural
   areas. This regional gap in the gap of roughly 10 percentage points
   remained surprisingly constant over the entire observation period of 30
   years.},
Publisher = {WILEY-BLACKWELL},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
