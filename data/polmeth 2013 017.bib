@article{ WOS:000313000800005,
Author = {Weber, Enzo and Wolters, Juergen},
Title = {Risk and Policy Shocks on the US Term Structure},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2013},
Volume = {60},
Number = {1},
Pages = {101-119},
Month = {FEB},
Abstract = {We document two stylized facts of US short-term and long-term interest
   rate data seemingly incompatible with the expectations hypothesis: low
   contemporaneous cross-correlation and relatively slow adjustment to
   long-run relationships. We explain these features in a small structural
   model with three types of randomness: While a persistent monetary policy
   shock implies immediate identical reactions through the term structure,
   both a transitory policy shock and an autocorrelated risk premium allow
   for sustained deviations. Indeed, we find important impacts and
   persistence of risk premia and considerable contribution of transitory
   policy shocks to short rates. Results of standard expectations
   hypothesis tests can be rationalized.},
