@article{ WOS:000321897600007,
Author = {Gibson, John and Kim, Bonggeun and Stillman, Steven and Boe-Gibson, Geua},
Title = {Time to vote?},
Journal = {PUBLIC CHOICE},
Year = {2013},
Volume = {156},
Number = {3-4},
Pages = {517-536},
Month = {SEP},
Abstract = {Despite the centrality of voting costs to the paradox of voting, little
   effort has been made to measure these costs accurately, outside of a few
   spatially limited case studies. In this paper, we apply Geographic
   Information Systems (GIS) tools to validated national election survey
   data from New Zealand. We calculate distance and travel time by road
   from the place of residence to the nearest polling place and combine our
   time estimate with imputed wages for all sample members. Using this new
   measure of the opportunity cost of voting to predict turnout at the
   individual level, we find that small increases in the opportunity costs
   of time can have large effects in reducing voter turnout.},
