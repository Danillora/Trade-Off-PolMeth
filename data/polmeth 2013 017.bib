@article{ WOS:000315629800006,
Author = {Le, Kien T. and Brick, J. Michael and Diop, Abdoulaye and Alemadi,
   Darwish},
Title = {Within-Household Sampling Conditioning on Household Size},
Journal = {INTERNATIONAL JOURNAL OF PUBLIC OPINION RESEARCH},
Year = {2013},
Volume = {25},
Number = {1},
Pages = {108-118},
Month = {SPR},
Abstract = {We review existing methods for sampling an adult from a household and
   highlight the strengths and weaknesses of these methods with respect to
   issues relevant for sampling in the Middle East and developing
   countries. We then develop a new, flexible within-household sampling
   scheme that takes full advantage of the fact that household size must be
   obtained to produce unbiased estimates. The proposed method randomly
   selects an adult conditional on the number of adults in the household.
   The method was applied in a national sample in Qatar and it proved it
   was effective in a country where large household sizes are common and
   the information required for other sampling methods is generally not
   known.},
