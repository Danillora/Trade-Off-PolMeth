   Simeant, Johanna, Univ Paris 01, CESSP, F-75231 Paris 5, France.},
DOI = {10.1080/14742837.2012.714125},
ISSN = {1474-2837},
EISSN = {1474-2829},
Keywords = {Activism; career; internationalisation; Africa; NGOs; World Social
   Forums},
