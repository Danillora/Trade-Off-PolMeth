@article{ WOS:000320872000005,
Author = {Monsma, Stephen V. and Smidt, Corwin E.},
Title = {Faith-Based Interventions for At-Risk Latino Youths: A Study of Outcomes},
Journal = {POLITICS AND RELIGION},
