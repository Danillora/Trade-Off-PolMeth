@article{ WOS:000314531800007,
Author = {McGuirk, Eoin F.},
Title = {The illusory leader: natural resources, taxation and accountability},
Journal = {PUBLIC CHOICE},
Year = {2013},
Volume = {154},
Number = {3-4},
Pages = {285-313},
Month = {MAR},
Abstract = {This paper proposes and tests a mechanism through which natural
   resources can affect democracy. I posit that, in the presence of high
   natural resource rents, leaders lower the burden of taxation on citizens
   in order to reduce the demand for democratic accountability. The theory
   is corroborated using micro-level data from public opinion surveys
   across 15 sub-Saharan countries, in addition to country-level data on
   natural resource rents. Results are robust to a range of alternative
   specifications. A supplementary analysis reveals that, consistent with
   the two-period model proposed, the effects are more acute closer to
   national elections.},
