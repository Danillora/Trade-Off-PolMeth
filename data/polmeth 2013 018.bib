@article{ WOS:000321900900004,
Author = {Dowling, Andrew},
Title = {The Leading Role of the Party: Catalan Communism and the Franco Regime,
   1939-1975},
Journal = {EUROPEAN HISTORY QUARTERLY},
Year = {2013},
Volume = {43},
Number = {3},
Pages = {489-507},
Month = {JUL},
Abstract = {Prior to the Spanish Civil War, the Catalan national movement had been
   regarded with deep suspicion by organized labour, and society had been
   torn by social and political conflict. The rise to dominance of Catalan
   communism and the impact this movement had on the Catalan opposition to
   the Franco regime enabled the over-coming of pre-war divisions.
   Furthermore, the defence of Catalan national identity became a key
   component in the left's strategy of opposition to the dictatorship. Thus
   a new correlation of forces, as well as demographic change, ensured that
   the opposition in Catalonia was unified before any other territory in
   Spain. This article concerns itself with the structural changes in
   Catalan left political culture that determined a distinctive outcome to
   late Francoism.},
