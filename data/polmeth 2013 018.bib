@article{ WOS:000320443600010,
Author = {Lott, Jr., John R.},
Title = {ABA ratings: what do they really measure?},
Journal = {PUBLIC CHOICE},
Year = {2013},
Volume = {156},
Number = {1-2},
Pages = {139-161},
Month = {JUL},
Abstract = {This study reveals the ABA as systematically giving lower ratings to
   Republican circuit court nominees, although no similar bias appears to
   exist for district court nominees. The data show how important it is to
   separate the evidence for circuit and district court nominees. This
   study fits in with my previous research showing that it is the brightest
   nominees who face the most difficult time getting confirmed and that the
   most difficult confirmations involve circuit court nominees. One
   difference with earlier findings is that the biases seem to be hidden
   and are worse than looking at the averages across administrations. For
   example, the oldest Republican nominees, who will not be on the court
   for long, receive the highest ratings. The reverse is true for
   Democratic nominees. Similarly, the research explains why Republican
   nominees tend to get their lowest evaluations from the ABA when the
   Democrats control the Senate.},
