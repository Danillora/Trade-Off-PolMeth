@article{ WOS:000316271400004,
Author = {Mann, Jatinder},
Title = {``Leavening British Traditions{''}: Integration Policy in Australia,
   1962-1972},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2013},
Volume = {59},
Number = {1},
Pages = {47-62},
Month = {MAR},
Abstract = {During the period 196272 integration replaced assimilation as official
   government policy in dealing with migrants in Australia. Migrants were
   now encouraged to incorporate themselves into the dominant Anglo-Celtic
   society but also to retain elements of their own culture. The policy
   emerged in response to the unravelling of Britishness and the
   incremental dismantling of the White Australia policy as the twin
   pillars of Australian national identity. The new nationalism, which
   stressed a more independent and home grown Australian image, arose as a
   possible replacement to British race patriotism towards the end of this
   period. At the same time whiteness was also broken down.},
