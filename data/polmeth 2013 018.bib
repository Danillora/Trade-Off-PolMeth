@article{ WOS:000339532600011,
Author = {Goma, Daniel},
Title = {SOCIALISM AND THE ARMY: THE ESTABLISHMENT OF THE ``BURMESE WAY TO
   SOCIALISM{''} AND THE CONSOLIDATION OF THE SOCIALIST MILITARY REGIME IN
   BURMA (1962-1974)},
Journal = {HISTORIA Y POLITICA},
Year = {2013},
Number = {30},
Pages = {279-303},
Month = {JUL-DEC},
Abstract = {From the analysis of primary sources available, combined with foreign
   literature, this article examines the first stage of the so-called
   ``Burmese Way to Socialism{''}, a utopian, revolutionary and
   militaristic experiment developed in Burma between 1962 and 1988, which
   ended in a resounding failure. This first stage was marked by the
   left-wing military regime's desire to promote this socialist ideology
   with Burmese characteristics and, as we shall see, by the attempt of the
   regime to legitimize its authority by strengthening its political wing,
   the Burma Socialist Programme Party, as the only authorized party.},
