@article{ WOS:000315425400007,
Author = {Voia, Marcel-Cristian and Ferris, J. Stephen},
Title = {Do business cycle peaks predict election calls in Canada?},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2013},
Volume = {29},
Pages = {102-118},
Month = {MAR},
Abstract = {This paper examines the empirical regularity that in Canada business
   cycle peaks and federal elections have tended to arise together over the
   long post-Confederation time period following 1867. We argue that rather
   than being simultaneous, the two events are related sequentially and
   that causality can be identified properly if the selection issue
   associated with observed events is addressed carefully. Our results
   suggest that business cycle peaks lead federal elections rather than the
   other way around. Such a finding reinforces the hypothesis of strategic
   election timing for such countries and is insightful in helping to
   explain why the presence of a political business cycle is harder to
   establish for parliamentary governments where the date of the next
   election is under the control of the incumbent governing party than in
   democratic systems where governing durations and election dates are
   fixed. (C) 2012 Elsevier B.V. All rights reserved.},
Publisher = {ELSEVIER SCIENCE INC},
Address = {360 PARK AVE SOUTH, NEW YORK, NY 10010-1710 USA},
