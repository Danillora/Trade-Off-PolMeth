@article{ WOS:000328581600002,
Author = {Wincott, Daniel},
Title = {THE (GOLDEN) AGE OF THE WELFARE STATE: INTERROGATING A CONVENTIONAL
   WISDOM},
Journal = {PUBLIC ADMINISTRATION},
Year = {2013},
Volume = {91},
Number = {4, SI},
Pages = {806-822},
Month = {DEC},
Abstract = {The idea of a (golden) age pervades most academic analysis and debate
   about the welfare state. This article interrogates this largely
   unquestioned epochal image of welfare history which is, in effect, a
   shared conventional wisdom. That vast and often disputatious scholarly
   literatures should share a conventional epochal axiom is remarkable. So
   is the absence of academic debate focused on this idea, which is both
   pervasive and vague. Perhaps paradoxically, our easy recognition of the
   (golden) age of the welfare state may contribute to the lack of
   precision in its use: taken-for-granted concepts are often sloppily
   deployed.},
