@incollection{ WOS:000415725400007,
Author = {Schmidt, Vivien A.},
Editor = {Telo, M},
Title = {Comparative Institutionalisms},
Booktitle = {GLOBALISATION, MULTILATERALISM, EUROPE: TOWARDS A BETTER GLOBAL
   GOVERNANCE?},
Series = {Globalisation Europe Multilateralism},
Year = {2013},
Pages = {109-123},
Abstract = {Political science in recent years has been divided along methodological
   lines. In comparative politics but also increasingly in international
   relations, these divisions take the form of different `new'
   institutionalist approaches. This chapter discusses all four of these
   neo-institutionalisms in an effort to demonstrate the importance of
   methodological pluralism. The three long-standing neo-institutionalisms
   include rational choice institutionalism, focused on rationalist
   interests and institutional incentives; historical institutionalism,
   concerned with institutional path-dependent rules and regularities; and
   sociological institutionalism, centred on cultural norms and
   institutional frames. The most recent neo-institutionalism, discursive
   institutionalism, concentrates on the substantive content of ideas and
   the interactive processes of discourse in institutional context. It
   claims to be better able to explain the dynamics of change than the
   three other neo-institutionalisms, which tend to be better at explaining
   continuity but have lately also been concerned with `endogenizing'
   change.},
