@article{ WOS:000320747200001,
Author = {Gurgul, Henryk and Lach, Lukasz},
Title = {Political instability and economic growth: Evidence from two decades of
   transition in CEE},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
