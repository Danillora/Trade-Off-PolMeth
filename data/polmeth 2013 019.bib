@article{ WOS:000317725900006,
Author = {Ahlqvist, Toni},
Title = {Potential Governmentality and the State Transformation in Finland},
Journal = {GEOPOLITICS},
Year = {2013},
Volume = {18},
Number = {2, SI},
Pages = {328-342},
Month = {APR 1},
Abstract = {This essay opens a perspective of potential governmentality on state
   transformation. It is argued that potential governmentality is a process
   of building a visionary script of an emergent governmental form linked
   to the creation of a new politico-economic path in a state. The essay
   discusses the idea of potential governmentality as a heuristic,
   elaborates the idea of state vigour as a generic form of governmentality
   and presents a case study in the context of the Finnish state. The essay
   is an attempt to chart a particular form of state transformation in the
   context of diffusing geoeconomic calculation.},
