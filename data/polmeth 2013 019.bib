@article{ WOS:000320975600003,
Author = {Clarke, Nick and Cochrane, Allan},
Title = {Geographies and politics of localism: The localism of the United
   Kingdom's coalition government},
Journal = {POLITICAL GEOGRAPHY},
Year = {2013},
Volume = {34},
Pages = {10-23},
Month = {MAY},
Abstract = {There has always been a localist element to British politics. But
   recently, a particular version of localism has been moved to the
   foreground by the 2011 Localism Act. This paper identifies various uses
   and meanings of localism, maps their geographical assumptions and
   effects, and critiques their politics. It does this using the localism
   of the United Kingdom's Coalition Government as a case study of localism
   in practice. The rationalities, mentalities, programmes, and
   technologies of this localism are established from Ministerial speeches
   and press releases, along with Parliamentary Acts, Bills, White Papers,
   Green Papers, and Statements - all published between May 2010 when the
   Coalition Government was formed, and November 2011 when the Localism Act
   became law. We argue that localism may be conceptualised as spatial
   liberalism, is never straightforwardly local, and can be anti-political.
   (c) 2013 Elsevier Ltd. All rights reserved.},
Publisher = {ELSEVIER SCI LTD},
