@article{ WOS:000320556200007,
Author = {Diehl, Paul F. and Wallensteen, Peter and Sarkees, Meredith Reid and
   Geller, Daniel S.},
Title = {The Forum: Reflections and Reassessments on the Early Work and Ideas of
   J. David Singer},
Journal = {INTERNATIONAL STUDIES REVIEW},
Year = {2013},
Volume = {15},
Number = {2},
Pages = {259-284},
Month = {JUN},
Abstract = {The four essays in this forum consider the influence of J. David
   Singer's initial views and analyses dating from the 1960s and 1970s on
   topics that continue to hold great importance today. Three essays
   consider the implications of these early works and decisions on the
   subsequent study of war, as well as their continuing wisdom (or not) in
   light of subsequent research. The fourth essay examines Singer's policy
   prescriptions in light of subsequent decisions and choices. In some
   cases, the ideas continue to have resonance with contemporary conflict
   research and policy issues, but in other instances there are grounds for
   reevaluation and the adoption of new research and data collection
   strategies or policy perspectives.},
