@article{ WOS:000329005700002,
Author = {Moraski, Bryon J.},
Title = {Constructing courts after communism: Reevaluating the effect of
   electoral uncertainty},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2013},
Volume = {46},
Number = {4},
Pages = {433-443},
Month = {DEC},
Abstract = {A prominent view in political science is that electoral uncertainty
   leads institutional designers to prefer independent and powerful courts.
   Yet few scholars have examined the design of constitutional courts
   systematically across Eastern Europe and those who have employed the
   results of elections held after constitutions were adopted to estimate
   the actors' perceptions of the balance of power prior to the court's
   design. This work reevaluates the effects of electoral uncertainty in
   post-communist Europe using more appropriate data and fuzzy-set
   qualitative comparative analysis to outline the different causal
   configurations linking electoral uncertainty to the initial judicial
   empowerment. (C) 2013 The Regents of the University of California.
   Published by Elsevier Ltd. All rights reserved.},
