@article{ WOS:000321055800006,
Author = {Swedberg, Richard},
Title = {The financial crisis in the US 20082009: losing and restoring confidence},
Journal = {SOCIO-ECONOMIC REVIEW},
Year = {2013},
Volume = {11},
Number = {3},
Pages = {501-523},
Month = {JUL},
Abstract = {Confidence is an understudied topic in modern economics as well as in
   modern sociology, but vital to an understanding of financial markets and
   financial crises. In this article, I suggest what a theory of confidence
   may look like that is centered around signs. With the help of this
   theoretical approach, to which I add some ideas by Walter Bagehot, I
   explore the financial crisis of 20082009 in the USA, how confidence was
   lost during the fall of 2008 in connection with the bankruptcy of Lehman
   Brothers, and especially how it was then restored in the spring of 2009.
   I conclude that the processes of loosing and restoring confidence
   differ.},
