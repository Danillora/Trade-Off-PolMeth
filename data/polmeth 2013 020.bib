@article{ WOS:000317270200007,
Author = {Broome, Andre},
Title = {The politics of IMF-EU co-operation: institutional change from the
   Maastricht Treaty to the launch of the euro},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2013},
Volume = {20},
Number = {4},
Pages = {589-605},
Month = {APR 1},
Abstract = {How do regional changes affect the process of global governance? This
   article addresses this question by examining how the International
   Monetary Fund (IMF) responded to the challenges presented by Economic
   and Monetary Union (EMU) between the signing of the Maastricht Treaty in
   1992 and the launch of the euro in 1999. Based on primary research from
   the IMF archives, the article illustrates how the IMF's efforts to
   reconfigure its relationship with European institutions evolved
   gradually through a logic of incremental change, despite initial
   opposition from member states. The article concludes that bureaucratic
   actors within international organizations will take advantage of
   informal avenues for promoting a new agenda when this fits with shared
   conceptions of an organization's mandate. The exercise of informal
   influence by advocates for change within an international organization
   can limit the options available to states in formal decision-making
   processes, even when these options cut across state preferences.},
