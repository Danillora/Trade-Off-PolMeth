@article{ WOS:000323761000004,
Author = {Persson, Mikael and Solevid, Maria and Ohrvall, Richard},
Title = {Voter Turnout and Political Equality: Testing the `Law of Dispersion' in
   a Swedish Natural Experiment},
Journal = {POLITICS},
Year = {2013},
Volume = {33},
Number = {3},
Pages = {172-184},
Month = {OCT},
Abstract = {According to the law of dispersion', the level of inequality in
   political participation is higher when voter turnout is low. We
   empirically test this hypothesis by evaluating levels of voter turnout
   in the 2010 Swedish election to the Vastra Gotaland county council and
   in the 2011 re-election for the same county council. The re-election
   voter turnout was reduced by almost half, from 80.6 per cent to 44.1 per
   cent. Our results support the law of dispersion: the level of inequality
   in participation substantially increased between young and old, rich and
   poor, low and high educated and politically interested and uninterested.},
