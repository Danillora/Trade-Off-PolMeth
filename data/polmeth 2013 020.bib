@article{ WOS:000211781400009,
Author = {Hammond, Grant T.},
Title = {Reflections on the Legacy of John Boyd},
Journal = {CONTEMPORARY SECURITY POLICY},
Year = {2013},
Volume = {34},
Number = {3},
Pages = {600-602},
Abstract = {Much of what exists in print about Boyd are not his own ideas but those
   of interpreters from numerous perspectives, both civilian and military.
   Since most commentators never sat through his briefings, separating Boyd
   from his interpreters complicates things greatly. Boyd was a synthesizer
   who roamed through military history, philosophy and science to question
   and inspire. He wanted people to be creative, think differently and
   better, and find new ways to solve problems. Though we may bemoan his
   shortcomings, his accomplishments cannot be separated from his
   eclecticism. Osinga and Hasik show the way toward a more conventional
   and reproducible legacy. Rigorous exegesis and testing of Boyd's work is
   appropriate and necessary for its future influence.},
