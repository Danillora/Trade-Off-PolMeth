@article{ WOS:000323135700004,
Author = {Shipley, Tyler},
Title = {The New Canadian Imperialism and the Military Coup in Honduras},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2013},
Volume = {40},
Number = {5},
Pages = {44-61},
Month = {SEP},
Abstract = {Canada's key role in supporting the 2009 coup d'etat in Honduras is
   emblematic of a new Canadian imperialist positioning in world affairs. A
   close reading of Canadian state responses to the coup and the repression
   that followed points to the apparent contradiction in its being an
   outspoken supporter of a violent and antidemocratic regime despite
   rhetorical commitments to peace and security. The contradiction can be
   explained in terms of the extent of Canadian economic investment in
   Honduras and an understanding of Canada as a secondary imperial power
   committed to imposing and maintaining conditions in Honduras that will
   protect that investment. The promotion of hyper-neoliberalism in
   Honduras, then, is representative of Canada's larger projects for the
   Global South in a context in which Canadian capital is aggressively
   expanding its reach.},
