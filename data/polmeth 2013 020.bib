@article{ WOS:000212580400003,
Author = {Akuffo, Edward Ansah},
Title = {Beyond apartheid: moral identity, FIPAs, and NEPAD in Canada-South
   Africa relations},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
