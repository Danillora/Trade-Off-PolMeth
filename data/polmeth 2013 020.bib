@article{ WOS:000330447600004,
Author = {Trager, Robert F.},
Title = {How the scope of a demand conveys resolve},
Journal = {INTERNATIONAL THEORY},
Year = {2013},
Volume = {5},
Number = {3},
Pages = {414-445},
Month = {NOV},
Abstract = {How does the scope of costless threats convey information about resolve
   to adversaries? Analysis of a model similar to Fearon demonstrates that
   higher demands increase perceptions of a state's resolve to fight for
   more favorable outcomes when bargaining is such that both sides share in
