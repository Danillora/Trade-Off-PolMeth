@article{ WOS:000327371300006,
Author = {Oksanen, Markku and Kumpula, Anne},
Title = {Transparency in conservation: rare species, secret files, and democracy},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2013},
Volume = {22},
