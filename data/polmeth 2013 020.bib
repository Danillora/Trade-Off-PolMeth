@article{ WOS:000319217900004,
Author = {Koch, Philippe},
Title = {Overestimating the Shift from Government to Governance: Evidence from
   Swiss Metropolitan Areas},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2013},
Volume = {26},
Number = {3},
Pages = {397-423},
Month = {JUL},
Abstract = {There is an oft-stated claim in political science research that public
   sector reforms in Western democracies are consistent with a shift from
   government to governance. This article challenges that claim. Based on a
   comparative analysis of governance reforms in Swiss metropolitan areas,
   the article reveals that what seems to be a shift to governance may turn
   out to be a path to government, however, including a change either from
   the local to the metropolitan level or from general-purpose authorities
   to task-specific jurisdictions. Further, the article analyzes the
   political dynamics behind governance reforms. It shows that exogenous
   pressures put existing governance schemes under strain. Its impact,
   however, is mediated by the initial institutional setting and the
   narratives responding to case-specific political dilemmas.},
