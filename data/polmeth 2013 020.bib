@article{ WOS:000320911800005,
Author = {Yeros, Paris},
Title = {The rise and fall of trade unionism in Zimbabwe, Part I: 1990-1995},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2013},
Volume = {40},
Number = {136},
Pages = {219-232},
Month = {JUN 1},
Abstract = {This article is the first of a two-part study on the evolution of the
   Zimbabwe Congress of Trade Unions (ZCTU) in the 1990s. This first part
   covers the period 1990-1995, when the labour centre was in its most
   radical mode. This is demonstrated by tracing its interventions in the
   public debate, its mobilisation and democratisation campaign, and its
   escalating strike action. It is argued, however, that the weaknesses of
   the ZCTU, especially its lack of organic roots outside the formal sector
   and its dependence on foreign donors, set the stage for a significant
   change in its ideology and strategy.},
