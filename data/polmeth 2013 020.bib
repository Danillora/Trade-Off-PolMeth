@article{ WOS:000327468800004,
Author = {Contreras, Antonio P.},
Title = {Everyday university politics in the Philippines: a tale of two
   universities},
Journal = {PHILIPPINE POLITICAL SCIENCE JOURNAL},
Year = {2013},
Volume = {34},
Number = {2},
Pages = {170-187},
Month = {DEC 1},
Abstract = {The experience of the two universities presented in this article
   provides evidence to the proliferation of contestations for power in
   modern universities, including those in the Philippines. This can be
   analyzed in the context of ordinary resistance and everyday politics
   where architectures of power deployed by university administrators tend
   to both constrain and enable political action. This occurs even as human
   agents offer their transgressions by deploying forms of localized
   resistance that may appear petty and mundane but has the power to
   reinforce the self-identification of its bearers. Indeed, this is
   enabled by the fact that the university is now normalized into simply
   another place of work, and where knowledge is no longer produced for
   knowledge's sake only, but as a collateral benefit in a political
   economy of symbols, narratives, images, and commodities. Thus, this
   article shows that in late capitalism the university has become just
   another place for performativity and simulacra.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
