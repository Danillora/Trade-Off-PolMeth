@article{ WOS:000321886100003,
Author = {Simi, Pete and Bubolz, Bryan F. and Hardman, Ann},
Title = {Military Experience, Identity Discrepancies, and Far Right Terrorism: An
   Exploratory Analysis},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2013},
Volume = {36},
Number = {8},
Pages = {654-671},
Month = {AUG 1},
Abstract = {The relationship between far right extremism and military involvement is
   a complex problem and the dynamics that explain this relationship are
   not well understood. The present article addresses this gap by
   discussing the relationship between military experience, identity
   discrepancies, and far right terrorism. This article proposes that
   identity discrepancies occur when individuals experience involuntary
   role exits from the military or when individuals perceive that personal
   achievements earned while enlisted are unrecognized or unappreciated.
   Identity discrepancies may facilitate a change in behavior toward far
   right extremism and eventually terrorism. Case study examples are
   provided to illustrate this relationship.},
