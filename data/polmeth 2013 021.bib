@article{ WOS:000312009100005,
Author = {Ruffinelli, Jorge},
Title = {Uruguay 2008 The Year of the Political Documentary},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2013},
Volume = {40},
Number = {1},
Pages = {60-72},
Month = {JAN},
Abstract = {The year 2008 was prolific in terms of documentary film production in
   Uruguay. This type of Uruguayan cinema, which dates to the 1960s, was
   interrupted by the military dictatorship (1973-1984), and on the return
   of democracy it was very slow to recover. In 2008, however, it showed
   great vitality as political expression and great diversity in the
