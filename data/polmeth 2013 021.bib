@article{ WOS:000320960400003,
Author = {Reitberger, Magnus},
Title = {License to kill: is legitimate authority a requirement for just war?},
Journal = {INTERNATIONAL THEORY},
Year = {2013},
Volume = {5},
Number = {1},
Pages = {64-93},
Month = {MAR},
Abstract = {In traditional just war theory, legitimate authority is regarded as a
   necessary requirement for war to be just. This article challenges this
   requirement by arguing that a right to wage war can be derived from the
   right to self-defense and the justifiability of exercising political
