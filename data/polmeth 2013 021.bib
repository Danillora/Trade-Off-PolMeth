@article{ WOS:000323473400004,
Author = {Danforth, Benjamin and Stephens, John D.},
Title = {Measuring social citizenship: achievements and future challenges},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2013},
Volume = {20},
Number = {9},
Pages = {1285-1298},
Month = {OCT 1},
Abstract = {Significant advances have been made in the development of measures of
   social citizenship, but this progress is not without its challenges and
   limitations. Drawing on new comparative analyses of two prominent
   datasets - the Comparative Welfare States Dataset (CWED) and Social
   Citizenship Indicator Program (SCIP) - this study highlights several
   important inconsistencies found in existing data on social citizenship
   and identifies their underlying causes. This synthesis shows that much
   of the variation detected between corresponding CWED and SCIP measures
   stems from justifiable differences in basic operationalization, data
   selection, and measurement calculation. Based on our own pooled
   time-series analysis of the CWED and SCIP data, we find that some of the
   discrepancies between their measures have been overstated. Moving on to
   a broader perspective, we discuss the main advantages and shortcomings
   of each dataset and outline several directions for future data
   collection, with special attention paid to social services and new
   social risks.},
