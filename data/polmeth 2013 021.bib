@article{ WOS:000316633200002,
Author = {Vasquez, John A. and Kang, Choong-Nam},
Title = {How and why the Cold War became a long peace: Some statistical insights},
Journal = {COOPERATION AND CONFLICT},
Year = {2013},
Volume = {48},
Number = {1},
Pages = {28-50},
Month = {MAR},
Abstract = {A data-based analysis of how and why the long peace of the Cold War
   occurred. The analysis tests a proposition originally suggested by
   Senese and Vasquez (2008) that alliance polarization played an important
   role in producing the long peace, because in the 1816-1945 and post-Cold
   War periods, both sides having outside alliances was positively
   associated with the escalation of a militarized interstate dispute (MID)
   to war and in the Cold War it was negatively related. New hypotheses are
   derived to test this claim. Tests of two hypotheses support the claim
   that alliance polarization played a key role in the long peace, even
   when controlling for the presence of nuclear weapons.},
