@incollection{ WOS:000327149700004,
Author = {Rana, Kishan S.},
Book-Author = {Rana, KS},
Title = {The New and the Old Change is the only constant. The more things change,
   the more they are the same},
Booktitle = {CONTEMPORARY EMBASSY: PATHS TO DIPLOMATIC EXCELLENCE},
Series = {Palgrave Pivot},
Year = {2013},
Pages = {32-52},
Abstract = {Although staff at a few large embassies number in the hundreds, the
   average embassy is rather small; its offcials have to multitask. We
   examine some of the ways embassy work has changed in recent years,
   including the increased diversity of diplomatic work, its
   omni-directional nature, and it's public and inclusive nature. The three
   principal components of diplomacy-political, economic and public - are
   examined in detail, citing examples from countries ranging from Nepal to
   the United Kingdom and the United States. We also consider the kind of
   official who should handle the work of economic diplomacy, the notion of
   education as a `tradable good', as well as the rising importance of
   consular and diaspora work. The application of technology has
   transformed diplomacy in concrete ways. Sub-state actors have gained in
   value, and embassies increasingly work outside the capital of the
   receiving country.},
