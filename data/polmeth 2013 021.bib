@article{ WOS:000329087500001,
Author = {Schmidt, Soren},
Title = {The Role of the Military for Democratization during the Arab Spring -
   the cases of Tunisia, Egypt and Syria},
Journal = {INTERNASJONAL POLITIKK},
Year = {2013},
Volume = {71},
Number = {4},
Pages = {479+},
Abstract = {Popular uprising against authoritarian regimes is not enough to allow
   transition towards democracy to begin. The military has the final word
   on whether such an uprising will be successful or the uprising will be
   repressed in order to preserve the incumbent dictatorship. The military
   supported the uprising in Tunisia while it sided with the regime and
   repressed the uprising in Syria. In Egypt did the military refrain from
   supporting the regime and took over power themselves. Why did the
   military act so differently in the three cases? The article propose a
   theoretical explanation which combines variables influencing whether the
   military has the potential to support a popular uprising or not; and if
   there is a political situation, which allows for this potential to be
   exploited. The model of explanation is then tested against the cases of
   Tunisia, Egypt and Syria during The Arab Spring.},
Publisher = {NORSK UTENRIKSPOLITISK INST},
Address = {GRONLANDSLEIRET 25 P.O. BOX 8159 DEPT., N-0033 OSLO 1, NORWAY},
Type = {Article},
Language = {Norwegian},
Affiliation = {Schmidt, S (Corresponding Author), Aalborg Univ, Aalborg, Denmark.
   Schmidt, Soren, Aalborg Univ, Aalborg, Denmark.},
ISSN = {0020-577X},
EISSN = {1891-1757},
