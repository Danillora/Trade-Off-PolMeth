@article{ WOS:000344139900008,
Author = {Monaghan, Elizabeth},
Title = {Making the Environment Present: Political Representation, Democracy and
   Civil Society Organisations in EU Climate Change Politics},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2013},
Volume = {35},
Number = {5, SI},
Pages = {601-618},
Abstract = {The climate change issue has attracted the attention of civil society
   organisations in the European Union but standard theories of groups as
   interest aggregators fail to offer a convincing explanation of their
   characteristics and contribution. However as the concept of
   representation has been transformed - from a static view pertaining
   mostly to legislatures, to a dynamic view of a creative act involving
   all political actors - it has opened up the possibility of a new
   explanatory framework for understanding how groups make environmental
   issues present. This is in direct contrast to the idea of representation
   and the role of groups outlined in the Lisbon Treaty.},
