@article{ WOS:000316297200002,
Author = {Rosenberg, Justin},
Title = {Kenneth Waltz and Leon Trotsky: Anarchy in the mirror of uneven and
   combined development},
Journal = {INTERNATIONAL POLITICS},
Year = {2013},
Volume = {50},
Number = {2},
Pages = {183-230},
Month = {MAR},
Abstract = {Waltz's neorealist theory has been charged with falsely separating
   geopolitical from social and economic processes. Yet Waltz's critics
   themselves have failed to show how sociological and geopolitical
   phenomena can be explained in a unified international theory. Such a
   theory, says Waltz, would have to pass three tests. It must delimit a
   field of specifically international phenomena. It must identify
   structured (and hence theorizable) effects within this field. And it
   must furnish `a brilliant intuition', which reveals the causal relations
   that explain these effects. This article argues that the idea of `uneven
   and combined development' (U\&CD) can pass these tests. The article
   delimits `the international' as those phenomena arising from the
   interactive multiplicity of societies. Next, it uses Gerschenkron's
   theory of backwardness to identify internationally structured effects
   arising from societal multiplicity. And finally, by considering the
   debate on the First World War, it explores how the causal mechanisms
   identified by U\&CD can be used to construct a unified sociological and
   geopolitical explanation. International Politics (2013) 50, 183-230.
   doi:10.1057/ip.2013.6},
