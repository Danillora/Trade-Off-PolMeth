@incollection{ WOS:000327149700008,
Author = {Rana, Kishan S.},
Book-Author = {Rana, KS},
Title = {The Future Can We Do Better?},
Booktitle = {CONTEMPORARY EMBASSY: PATHS TO DIPLOMATIC EXCELLENCE},
Series = {Palgrave Pivot},
Year = {2013},
Pages = {117-141},
Abstract = {Diplomacy exists in practice. Savvy countries have used technology to
   `embed' the embassy in the foreign ministry; elsewhere too, the embassy
   has gained in importance in external policy management. A counter trend
   favors direct exchanges among leaders and among foreign ministry
   officials, leaving embassies out of the loop. Gender issues affect the
   functioning of foreign ministries and embassies. Countries are concerned
   with the capacity of their diplomatic networks, and their efficacy.
   Training is a determinant of capacity. What constitutes an ideal
   embassy? We posit this in terms of embassies that are `enlightened,
   empowered, and efficient', elaborating these characteristics. The
   chapter concludes with assessing future trends: radical change is
   unlikely; but we are likely to see a focus on improved performance,
   better mutual learning among foreign ministries, and improved
   appreciation of the value embassies deliver as cost-effective promoters
   of national interests.},
