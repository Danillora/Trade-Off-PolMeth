@incollection{ WOS:000316786300004,
Author = {Mukhimer, Tariq},
Book-Author = {Mukhimer, T},
Title = {The Rise of Hamas as a Non-State Actor Resembling a Government:
   Background and Operational Setting},
Booktitle = {HAMAS RULE IN GAZA: HUMAN RIGHTS UNDER CONSTRAINT},
Series = {Palgrave Pivot},
Year = {2013},
Pages = {25-55},
Abstract = {This chapter introduces a thorough empirical background of the conflict
   between Hamas and Fatah and the rise of Hamas as a non-state actor
   resembling a government; at the same time the chapter provides a
   detailed analysis of the operational setting of the Hamas authority in
   Gaza. The chapter argues that the case of the Hamas authority as a
   non-state actor impacting Gaza's public sphere is unique, noting the
   parameters under which the Hamas authority has been operating since
   2007, including: the continued Israeli occupation of Gaza; the
   diplomatic boycott and tightened closure imposed by the international
   community and Israel (both on the authority itself as an organization
   and on the population of the Gaza Strip generally), and the political
   conflict between the Hamas authority in Gaza and the Palestinian
   Authority (PA) in Ramallah.},
