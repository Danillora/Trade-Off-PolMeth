@article{ WOS:000212719100001,
Author = {Guha, Sohini},
Title = {From Ethnic to Multiethnic: The Transformation of the Bahujan Samaj
   Party in North India},
Journal = {ETHNOPOLITICS},
Year = {2013},
Volume = {12},
Number = {1},
Pages = {1-29},
Abstract = {The Bahujan Samaj Party (BSP) has succeeded in expanding its
   constituency beyond its core low caste base and attracting voters from
   upper and middle castes since the late 1990s. Building on a case study
   of the BSP in the North Indian state of Uttar Pradesh, this paper argues
   that the BSP's multiethnic transformation, whereby the party went from
   being a low caste party to one mobilizing constituencies across caste
   divides, throws new light on the mechanisms facilitating such
   transformatory attempts. The literature predicts that the success of
   multiethnic transformations will be determined by either the character
   of sociological divides, or of intra-party organization; but neither of
   these factors has counted for much in the BSP's case. Here, it is the
   core base's willingness to swap patronage and co-ethnic representation
   for programmatic gains, and the BSP's recourse to asymmetric
   representation whereby it has delivered different bundles of benefits to
   its old and new voters, that have allowed the party to attract new
   constituencies while retaining the old.},
