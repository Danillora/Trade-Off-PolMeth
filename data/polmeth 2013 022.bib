@incollection{ WOS:000401979700004,
Author = {Liu Taoxiong and Hu Angang},
Editor = {Junsheng, L and Bo, C and Na, H},
Title = {DEFENSE EXPENDITURE AND ECONOMIC GROWTH UNDER EXTERNAL PREDATION},
Booktitle = {COOPERATION FOR A PEACEFUL AND SUSTAINABLE WORLD, PT 2},
Series = {Contributions to Conflict Management Peace Economics and Development},
Year = {2013},
Volume = {20},
Pages = {39-63},
Abstract = {This chapter develops a growth model of a country under a Hobbesian
   environment with international conflicts in which national defense is
   the only way to prevent external predation. The long run growth path is
   determined by the equilibrium of a dynamic game with three players: the
   external predator, the government, and the family. The equilibrium
   growth path has three phases: submissive equilibrium, tolerant
   equilibrium, and full-protected equilibrium. Different defense
   strategies result in different growth prospects, and sustainable growth
   will endogenously induce adjustment of defense strategies.},
