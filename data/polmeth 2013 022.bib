@article{ WOS:000316962800011,
Author = {Andrews, Rhys and Boyne, George and O' Toole, Larry and Meier, Kenneth
   and Walker, Richard},
Title = {MANAGING MIGRATION? EU ENLARGEMENT, LOCAL GOVERNMENT CAPACITY AND
   PERFORMANCE IN ENGLAND},
Journal = {PUBLIC ADMINISTRATION},
Year = {2013},
Volume = {91},
Number = {1, SI},
Pages = {174-194},
Abstract = {This study explores how English local governments managed high levels of
   worker migration from Eastern Europe in the wake of the enlargement of
   the European Union in 2004. Theories of local government capacity
   suggest that strong central administration and vibrant community
   organizational life can increase the likelihood that public service
   performance will be maintained despite sudden shifts in the external
   environment. We explore the interactive effects of immigration and
   administrative and community capacity on English local government
   performance, using multivariate statistical techniques. The results
   suggest that local government capacity moderated the negative impact on
   performance of migration from Eastern Europe.},
