@article{ WOS:000315864200002,
Author = {Karagiannis, Emmanuel and McCauley, Clark},
Title = {The Emerging Red-Green Alliance: Where Political Islam Meets the Radical
   Left},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2013},
Volume = {25},
Number = {2},
Pages = {167-182},
Month = {APR 1},
Abstract = {No matter how unlikely it may seem, radical Leftists and Islamists have
   come closer in recent years. Drawing on substantial ideological
   interchange, and operating at both state and non-state levels, the two
   movements are building a Common Front against the United States and its
   allies. In this article, we use framing theory to examine the
   contemporary convergence of political Islam and the radical Left. Both
   radical Leftists and Islamists have utilized the master frame of
   anti-globalization/anti-capitalism and the master frame of
   anti-colonialism/anti-imperialism to elicit support from the widest
   possible range of people. The emerging Red-Green alliance presents a
   complex challenge that will require careful attention from U.S. and
   European policymakers.},
