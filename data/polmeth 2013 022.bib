@article{ WOS:000329920900005,
Author = {Aydin-Duzgit, Senem},
Title = {European security and the accession of Turkey: Identity and foreign
   policy in the European Commission},
Journal = {COOPERATION AND CONFLICT},
Year = {2013},
Volume = {48},
Number = {4},
Pages = {522-541},
Month = {DEC},
Abstract = {Following a poststructuralist theorising of identity in international
   relations, which argues that identity is relationally and discursively
   constructed through foreign policy, this article attempts to analyse the
   way in which the European Commission discursively constructs European
   identit(ies) in its relations with Turkey around the theme of security'.
   The study utilises the methodological tools of critical discourse
   analysis in analysing the speeches and field interviews conducted with
   European Commission officials, in examining the way in which they
   construct Europe' in relation to the security implications of Turkish
   accession. The article's findings challenge the argument that Europe is
   moving beyond the modern state to resemble a postmodern order, and
   problematises the designation of the Commission as a cosmopolitan' actor
   in EU enlargement policy.},
