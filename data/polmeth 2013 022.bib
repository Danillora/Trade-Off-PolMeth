@article{ WOS:000322684300025,
Author = {Cedilnik, Marko and Ramsak, Rok},
Title = {A Local Perspective to the Authorised Economic Operator (AEO) Concept},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2013},
Volume = {11},
Number = {3},
Pages = {673-685},
Month = {JUL},
Abstract = {This paper explains the concept the Authorised Economic Operator (AEO)
   and discusses why it is important for the local authorities to be
   familiar with the concept. Based on the quantitative survey carried out
   in this research we found that the ownership of the AEO certificate
   accelerates the road-based flow of goods and that it also positively
   affects the operation of economic operators within the scope of
   international trade. We also established that the infrastructure at
   border crossings does not enable the complete utilisation of all
   benefits offered by the ownership of the AEO certificate. This research
   of the AEO customs control program finds that the positive effects of
   AEO are reflected in the local environment as well and that the
   maximisation of the effects of the AEO requires the participation of the
   local government (municipalities, other local communities). This is
   especially the case in the activities associated with the planning of
   cross border traffic infrastructure and in the provision of information
   on the significance of time and security of supply chain functions.},
