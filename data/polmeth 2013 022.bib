@article{ WOS:000322105200002,
Author = {Toke, David},
Title = {Climate change and the nuclear securitisation of UK energy policy},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2013},
Volume = {22},
Number = {4, SI},
Pages = {553-570},
Month = {JUL 1},
Abstract = {The relationship between energy security, neoliberalism and climate
   change in the development of UK energy policy from 2003 to 2011 is
   discussed, with a particular focus on nuclear power. A securitisation'
   discourse has been associated with the promotion of nuclear interests,
   although the government policy storylines have been associated as much
   with changing party political priorities as with justifying instrumental
   policy objectives. However, the extent to which neoliberal electricity
   arrangements will be overridden in the wake of the securitising agenda
   is uncertain, due to the embedded nature of neoliberal practices.},
