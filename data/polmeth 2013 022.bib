@article{ WOS:000211781400008,
Author = {Hasik, James},
Title = {Beyond the Briefing: Theoretical and Practical Problems in the Works and
   Legacy of John Boyd},
Journal = {CONTEMPORARY SECURITY POLICY},
Year = {2013},
Volume = {34},
Number = {3},
Pages = {583-599},
Abstract = {More celebrated than analysed, John Boyd's contribution to strategic
   theory remains largely unevaluated. While his influence in air warfare
   and aircraft design is widely accepted, his role as a strategist -
   especially the widely applied OODA (Observe-Orient-Decide-Act) Loop - is
   more controversial. His ideas were originally presented in briefings
   more than in his own writings, and rely on exposition by others. One
   result of this complicated legacy is that his work is advocated more
   than tested. Among existing studies, the most rigorous explain his
   approach to strategy and its application, but the task of testing Boyd's
   novelty and validity as a strategic theorist through rigorous social
   science has only just begun. Comparative analysis suggests that Boyd's
   strategic theory - including the OODA Loop - is highly ambiguous, as are
   claims about his uniqueness. Tested against the broader literatures on
   nuclear pre-emption or counterinsurgency, his novelty and usefulness
   seem limited. Boyd's strategic thought needs investigating in three
   principle areas: epistemological validity, theoretical applicability,
   and the quality and limits of his evidence. Until these areas are better
   explored, Boyd's strategic thought should be taught in any strategic
   studies programme, business school, or war college, though perhaps not
   in the doctrinaire way his strongest adherents might want. Boyd is an
   important figure in the development of the military thought. Students
   who so far only know his name and the OODA concept would benefit greatly
   from engaging with, applying, and criticizing his ideas.},
