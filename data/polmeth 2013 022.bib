@article{ WOS:000331339400006,
Author = {Alarcon-Leiva, Jorge and Castro-Paredes, Moyra and Donoso-Diaz,
   Sebastian},
Title = {Centralism and Decentralized Aspirations in the Organization of the
   Chilean School System: 1980-2011},
Journal = {REVISTA DEL CLAD REFORMA Y DEMOCRACIA},
Year = {2013},
Number = {57},
Pages = {157+},
Month = {OCT},
Abstract = {The text describes the tension between centralization-decentralization
   in Chile, in order to maintain that the process of nation formation,
   having its origin in local identities, will end being concurrent with
   the consolidation of centralism. Given this context, the education
   system has been dependent on this centralizing tendency to the extent
   that it has been affected by the political, social and economic
   concentration, which has had an impact on its ability to respond to the
   development required by the different regions of the country.
   In this sense, the article. addresses the proposed problem from three
   perspectives. First, it reviews the presence of decentralizing trends in
   national political history, emphasizing the gradual establishment of
   centralism. Second, it analyzes the question of centralism within the
   framework of the changes in the Chilean educational system from the
   beginning of the civil-military dictatorship, suggesting the
   relationship between municipalization and privatization. It considers
   the effect of centralization on the education system in terms of its
   operating conditions, the heterogeneity of its territory and parts of
   its background relating to the characteristics of the educational
   offerings for the community. Finally, this paper concludes with a debate
   on the proposed hypothesis.
   The document seeks to demonstrate the priority of centralism in the
   organization of the country; the article relates this to the
   characteristics of the national education system and argues that the
   policy of educational decentralization of Chile is deconcentration and
   privatization.},
