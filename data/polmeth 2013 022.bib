@article{ WOS:000321811400004,
Author = {Dudley, Geoff},
Title = {Why do ideas succeed and fail over time? The role of narratives in
   policy windows and the case of the London congestion charge},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2013},
Volume = {20},
Number = {8},
Pages = {1139-1156},
Month = {SEP 1},
Abstract = {The dynamics by which policy ideas rise and fall can be linked to the
   intimate interactions between discrete physical time dimensions, such as
   policy windows, and distinctive perceptions of time, particularly the
   connections made between past, present and future in the form of
   narratives. Kingdon describes how an idea whose time has come' can be
   carried forward in the key policy change instrument of a policy window,
   but much remains to be understood about the dynamics of windows,
   including why they apparently open for a relatively short time period,
   and the significance of the legacy left by windows in terms of their
   ability to influence later events. The article seeks a greater
   understanding of these dynamics through an examination of the
   potentially vital role played by the construction of narratives. The
   case study is taken from the politically sensitive area of urban road
   pricing, in the form of the London Congestion Charge.},
