@article{ WOS:000212581500004,
Author = {Christopher, A. J.},
Title = {The commonwealth censuses: partial insights into issues of identity},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2013},
Volume = {51},
Number = {3},
Pages = {326-342},
Abstract = {Population censuses gather considerable quantities of information on
   virtually every person within the state. This is converted into
   statistical tabulations for the use of the government and private
   individuals. Although most of the questions asked are directed towards
   specific demographic and economic goals, others are concerned with more
   contentious issues of identity, most notably ethnicity, religion and
   language. No standard procedures or classifications of these indicators
   have been evolved within the Commonwealth, and virtually every country
   has created its own systems. Thus a series of country specific sets of
   questions and responses provide significant insights into the identity
   of the populations of Commonwealth countries, but also require careful
   interpretation.},
