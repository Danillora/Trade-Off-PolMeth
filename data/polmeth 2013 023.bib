@article{ WOS:000327295100005,
Author = {Mullins, Sam},
Title = {``Global Jihad{''}: The Canadian Experience},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2013},
Volume = {25},
Number = {5},
Pages = {734-776},
Month = {NOV 1},
Abstract = {This study aims to address the relative lack of research examining the
   Canadian experience of terrorism relating to the Global Salafi Jihad.
   The fundamental research question was What have people living in, or
   from, Canada been doing to support or advance violent jihad either at
   home or abroad? Data were collected on individuals active from the 1980s
   through to the end of 2011 in an effort to be as exhaustive as possible.
   They were analysed according to three broad categories: background
   variables; operational variables; and investigations and outcomes. The
   sample was further divided into two and results compared according to
   whether individuals began offending before or after September 11, 2001,
   in order to assess change over time. The article begins with a brief
   history of terrorism in Canada, followed by the methodology, analysis of
   variables, and summary and conclusions. Cases included and excluded from
   the analysis are listed in Appendices A and B respectively.},
