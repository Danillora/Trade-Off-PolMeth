@article{ WOS:000322955500009,
Author = {Phuc Thi Tran and Vieira, Alena Vysotskaya G. and Ferreira-Pereira,
   Laura C.},
Title = {Vietnam's strategic hedging vis-a-vis China: the roles of the European
   Union and Russia},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2013},
Volume = {56},
Number = {1},
Pages = {163-182},
Abstract = {Against the backdrop of China's assertive policies in the South China
   Sea, the present study evaluates how Vietnam has sought to mitigate the
   increasingly unequal regional power distribution vis-a-vis China. It
   argues that Vietnam tends to cope with China mainly by engaging itself
   in hedging strategies on the basis of diversified and strong
   relationships with different players. Appraising the roles of Russia and
   the European Union (EU), the study analyzes the pay-offs of Vietnam's
   military hedging with Russia and its economic hedging with the EU.},
