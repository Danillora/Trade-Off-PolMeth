@article{ WOS:000322686000004,
Author = {Sandal, Nukhet A.},
Title = {Public Theologies of Human Rights and Citizenship: The Case of Turkey's
   Christians},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2013},
Volume = {35},
Number = {3},
Pages = {631-650},
Month = {AUG},
Abstract = {Human rights violations against religious or ethnic minorities cannot be
   treated independently of the state-society relations in respective
   countries. With a focus on the Christian citizens of the Turkish
   Republic, this article investigates the limits posed by theological
   underpinnings of citizenship to the rights and freedoms of religious
