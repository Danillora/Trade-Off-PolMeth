@article{ WOS:000316624500004,
Author = {Boumans, Jelle W. and Boomgaarden, Hajo G. and Vliegenthart, Rens},
Title = {Media Personalisation in Context: A Cross-National Comparison between
   the UK and the Netherlands, 1992-2007},
Journal = {POLITICAL STUDIES},
Year = {2013},
