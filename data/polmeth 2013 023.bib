@article{ WOS:000326860400001,
Author = {Rudner, Martin},
Title = {Al Qaeda's Twenty-Year Strategic Plan: The Current Phase of Global
   Terror},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2013},
Volume = {36},
Number = {12},
Pages = {953-980},
Month = {DEC 1},
Abstract = {This study examines Al Qaeda's organizational dynamics, doctrinal
   precepts, strategic objectives, operational priorities, and tactical
   vectors in the context of its Twenty-Year Strategic Plan. This
   seven-stage Strategic Plan is addressed with a particular focus on its
   current phase (Stage 5), 2013-2016, involving the mobilization of Muslim
   forces for the Declaration of the Caliphate. In this regard, particular
   attention is paid to the Al Qaeda engagement in militant jihadist
   campaigns across the Middle East and other Muslim domains. The study
   concludes with an overall assessment of global threats and ongoing risks
   arising from the current phase of Al Qaeda's Twenty-Year Strategic Plan.},
