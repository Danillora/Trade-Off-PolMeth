@article{ WOS:000326482800002,
Author = {de Waardt, Mijke},
Title = {Are Peruvian Victims Being Mocked?: Politicization of Victimhood and
   Victims' Motivations for Reparations},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2013},
Volume = {35},
Number = {4},
Pages = {830-849},
Month = {NOV},
Abstract = {Research on the (promised) effects of transitional justice efforts on
   victims of civil conflicts remains rare. This article seeks to advance
   the field of research in two ways. First, this article focuses on how
   Peruvian victimhood became politicized as a consequence of a promised
   transitional justice mechanism: the Peruvian reparation program. Second,
   by highlighting the diverse motivations of members of grassroots
   victims' organizations, it brings to the fore important lessons on the
   successes and challenges of this transitional justice mechanism.},
