@article{ WOS:000323939100006,
Author = {Ferrero, Mario},
Title = {The Cult of Martyrs},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2013},
Volume = {57},
Number = {5},
Pages = {881-904},
Month = {OCT},
Abstract = {This article suggests a rational explanation for extreme voluntary
   sacrifice in situations in which the state of the world when the
   decision must be made is observable only by the agent. Such explanation
   is the cult of martyrs, heroes, and saints. This cult may get out of
   control and fuel fanaticism, or excessive sacrifice from the standpoint
   of the sponsoring organization. A survey of the historical evidence of
   Christian martyrdom strongly suggests that martyrs were driven by the
   expectation of a cult in this world, not by otherworldly rewards. In
