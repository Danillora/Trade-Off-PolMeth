@article{ WOS:000317430000008,
Author = {Herzog, Lawrence A.},
Title = {Barra da Tijuca The Political Economy of a Global Suburb in Rio de
   Janeiro, Brazil},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2013},
Volume = {40},
Number = {2},
Pages = {118-134},
Month = {MAR},
Abstract = {In Rio de Janeiro, Brazil's second-largest metropolis, an important new
   form of early-twenty-first-century peripheral development that deserves
   critical inquiry has emerged-the ``global suburb.{''} In Brazil, these
   suburbs take the form of gated high-rise compounds providing upper- and
   middle-upper-income residents enclaves of security amidst what they
   perceive as the chaos and uncertainty of cities overrun by favelas,
   crime, and gang-related conflict. The Barra da Tijuca suburban enclave
   in Rio de Janeiro offers a poignant case study of a development project
   that was initially conceived as an innovative urban design plan for a
   modernist ``new town{''} but, with little opposition from government
   officials and planners, was subsequently absorbed by private developers
   and turned into an elite, U.S.-style suburb.},
