@article{ WOS:000344139600004,
Author = {Salgado, Rosa Sanchez and Parthenay, Kevin},
Title = {Fostering Regional Democracy Through Civic Organisations: Comparing EU
   Mechanisms in Europe and Central America},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2013},
Volume = {35},
Number = {2},
Pages = {151-168},
Abstract = {This article compares the effects of the mechanisms of the European
   Union (EU) on the promotion of regional civic organisations, especially
   funding opportunities, in two regional settings (Central America and
   Europe). It is argued that the EU is exporting a specific model of
   relationships between regional institutions and civic organisations,
   giving priority to the promotion of regional integration, democracy and
   social inclusion. This article shows that EU funds have significantly
   contributed to the shaping of the civil society landscape in both
   regions, especially enhancing the participation of excluded groups in
   the policy process at the regional level. It is also important to
   determine if the EU is acting as an internal or external promoter of
   civic organisations. Even if the economic, social and cultural context
   is not neglected, the differential impact is primarily related to
   different types of European requirements.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
