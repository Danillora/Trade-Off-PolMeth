@article{ WOS:000320299800003,
Author = {Bhattacharya, Srobana},
Title = {Strategic Interaction Between Rebels and the State: A Study of the
   Maoist Conflict in Nepal},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
