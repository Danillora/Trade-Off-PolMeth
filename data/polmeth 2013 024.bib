@incollection{ WOS:000332286100003,
Author = {McKeown, Shelley},
Book-Author = {McKeown, S},
Title = {Current Situation},
Booktitle = {IDENTITY, SEGREGATION AND PEACE-BUILDING IN NORTHERN IRELAND: A SOCIAL
   PSYCHOLOGICAL PERSPECTIVE},
Series = {Palgrave Pivot},
Year = {2013},
Pages = {14-23},
Abstract = {Although it has been is years since the signing of the Agreement, the
   legacy of the troubles is that Northern Ireland continues to be a highly
   segregated society. At the same time the demographical landscape is
   changing; with the 2011 census highlighting that Northern Ireland is now
   made up of minorities, as no one religious group constitutes more than
   so per cent of the population. Further, despite recent statistics
   suggesting that the situation is improving, at least through a reduction
   in levels of political violence, negative attitudes and perceptions
   remain. Chapter 2, therefore, based on a series of national surveys and
   census data focuses on the current situation in Northern Ireland. It
   considers demographics and levels of segregation, as well as, attitudes
   towards mixing and inter-group contact.},
