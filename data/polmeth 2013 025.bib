   Brewer, John D.; Hayes, Bernadette C., Univ Aberdeen, Dept Sociol, Aberdeen AB24 3QY, Scotland.},
DOI = {10.1111/j.1467-9248.2012.00973.x},
ISSN = {0032-3217},
