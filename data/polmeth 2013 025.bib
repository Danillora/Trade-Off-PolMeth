@article{ WOS:000314436200004,
Author = {Bishop, Matthew Louis and Heron, Tony and Payne, Anthony},
Title = {Caribbean development alternatives and the CARIFORUM-European Union
   economic partnership agreement},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
