@article{ WOS:000319551900005,
Author = {Ackrill, Robert and Kay, Adrian and Zahariadis, Nikolaos},
Title = {Ambiguity, multiple streams, and EU policy},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2013},
Volume = {20},
Number = {6, SI},
Pages = {871-887},
Month = {JUN 1},
Abstract = {The multiple streams framework draws insight from interactions between
   agency and institutions to explore the impact of context, time and
   meaning on policy change and to assess the institutional and issue
   complexities permeating the European Union (EU) policy process. The
   authors specify the assumptions and structure of the framework and
   review studies that have adapted it to reflect more fully EU
   decision-making processes. The nature of policy entrepreneurship and
   policy windows are assessed to identify areas of improvement. Finally,
   the authors sketch out a research agenda that refines the logic of
   political manipulation which permeates the lens and the institutional
   complexity which frames the EU policy process.},
