@article{ WOS:000327838500002,
Author = {Scott, David},
Title = {India's Aspirations and Strategy for the Indian Ocean - Securing the
   Waves?},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2013},
Volume = {36},
Number = {4},
Pages = {484-511},
Month = {AUG 1},
Abstract = {India has increasingly high aspirations in the Indian Ocean, as
   enunciated by politicians, naval figures and the wider elite. These
   aspirations, its strategic discourse, are of pre-eminence and
   leadership. India's maritime strategy for such a self-confessed
   diplomatic, constabulary and benign role is primarily naval-focused; a
   sixfold strategy of increasing its naval spending, strengthening its
   infrastructure, increasing its naval capabilities, active maritime
   diplomacy, exercising in the Indian Ocean and keeping open the choke
   points. Through such strategy, and soft balancing with the United
   States, India hopes to secure its own position against a perceived
   growing Chinese challenge in the Indian Ocean.},
