@article{ WOS:000314436200005,
Author = {Lovec, Marko and Erjavec, Emil},
Title = {The Common Agricultural Policy Health Check: time to check the health of
   the theory of the reform?},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2013},
Volume = {16},
Number = {1},
Pages = {111-137},
Month = {JAN},
Abstract = {In 2008, the ministers of agriculture of European Union member states
   made a political agreement on the Common Agricultural Policy reform,
   also known as the Health Check. The reform coincided with three things:
   the ongoing Doha round of the World Trade Organization negotiations;
   political pressures to limit the costs of the policy financed from the
   common budget; and various `new' policy issues. Rational institutional
   and constructivist approaches, which are often viewed as theoretical
   alternatives with each explaining some aspects of the reform, have
   employed simplified and narrow abstractions in conceptualising the role
   of these policy contexts. In order to identify the mechanisms
   facilitating the Health Check, a critical realist approach is proposed
   here, arguing that the relationship among the trade negotiations, budget
   bargaining, new issues and the policy reform can be explained by
   theoretically endorsing the asymmetrical development of the agricultural
   production factors and of production relations. A qualitative analysis
   is used to determine which of these three approaches seems to be better
   able to explain the empirical evidence. Journal of International
   Relations and Development (2013) 16, 111-137. doi:10.1057/jird.2012.14;
   published online 25 May 2012},
