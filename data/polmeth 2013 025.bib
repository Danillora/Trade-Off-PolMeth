Type = {Article},
Language = {English},
Affiliation = {Shelley-Egan, C (Corresponding Author), Univ Twente, Dept Publ Adm, Law \& Regulat Grp, POB 217, NL-7500 AE Enschede, Netherlands.
   Shelley-Egan, Clare, Univ Twente, Dept Publ Adm, Law \& Regulat Grp, NL-7500 AE Enschede, Netherlands.
   Davies, Sarah R., Univ Copenhagen, Dept Media Cognit \& Commun, Copenhagen, Denmark.},
DOI = {10.1111/ropr.12032},
ISSN = {1541-132X},
