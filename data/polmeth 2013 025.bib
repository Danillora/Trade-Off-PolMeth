@article{ WOS:000327468800003,
Author = {De Castro, Renato Cruz},
Title = {China and Japan in maritime Southeast Asia: extending their
   geo-strategic rivalry by competing for friends},
Journal = {PHILIPPINE POLITICAL SCIENCE JOURNAL},
Year = {2013},
Volume = {34},
Number = {2},
Pages = {150-169},
Month = {DEC 1},
Abstract = {This article examines the expansion of the geo-strategic rivalry between
   China and Japan into maritime Southeast Asia. This rivalry is reflected
   concretely in their policies, in their positions, and actions on the
   South China Sea dispute. Likewise, the article analyzes this rivalry on
   maritime security in Southeast Asia and then concludes that this
   increasing hostility not only strains the relations between these two
   East Asian powers but might also threaten the survival of ASEAN as a
   loose association of small Southeast Asian powers.},
