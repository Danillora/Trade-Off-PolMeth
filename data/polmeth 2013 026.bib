@incollection{ WOS:000415725400008,
Author = {Maffettone, Sebastiano},
Editor = {Telo, M},
Title = {Normative Approaches to Global Justice},
Booktitle = {GLOBALISATION, MULTILATERALISM, EUROPE: TOWARDS A BETTER GLOBAL
   GOVERNANCE?},
Series = {Globalisation Europe Multilateralism},
Year = {2013},
Pages = {125-143},
Abstract = {Normative theories of global justice(2) can be divided according to two
   criteria: i) relational or non-relational and ii) statist or
   cosmopolitan. Relational authors can either be statist or cosmopolitan.
   Non-relational authors are usually cosmopolitan albeit in a different
   way than relational cosmopolitans. The limits of statists (such as
   Blake, Nagel, Julius, Sangiovanni(3)) are shown further, starting with
   cosmopolitan objections to the statists. The cosmopolitans can be
   relational or non-relational: the difference is that relational
   cosmopolitans (Beitz, Pogge, Cohen and Sabel(4)) expand the basic
   structure from the domestic to the global level whereas non-relational
   cosmopolitans (Caney, Buchanan(5)) use moral arguments in order to
   bypass the institutional arguments. Statist arguments are adopted to
   criticise cosmopolitanism. Cosmopolitanism is criticised for two kinds
   of reasons: i) institutional reasons and ii) moral reasons. The former
   mainly concern relational cosmopolitans, while the latter relate to
   non-relational cosmopolitans. In doing so, one can play a cosmopolitan
   in combination with statist features and vice versa. This is not because
   either cosmopolitans or statists are right. The intention is rather to
   avoid the pitfalls of both statism and cosmopolitanism by
   {[}GRAPHICS]
   presenting a fresh start under the label of liberal internationalism. In
   the next step, relational institutional support for liberal
   internationalism is provided: normative regionalism. Finally, a moral
   support for liberal internationalism is established: a non-relational
   humanitarian duty of justice. This duty of justice is not egalitarian
   but sufficientarian, that is, more focused on absolute rather than on
   relative deprivations.},
