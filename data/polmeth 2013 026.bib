@article{ WOS:000326396500005,
Author = {Bar-Joseph, Uri},
Title = {Forecasting a Hurricane: Israeli and American Estimations of the
   Khomeini Revolution},
Journal = {JOURNAL OF STRATEGIC STUDIES},
