@article{ WOS:000326482800004,
Author = {Nathan, Laurie},
Title = {The Disbanding of the SADC Tribunal: A Cautionary Tale},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2013},
Volume = {35},
Number = {4},
Pages = {870-892},
Month = {NOV},
Abstract = {In 2011 the heads of state of the Southern African Development Community
   (SADC) disbanded the SADC Tribunal after the regional court held that
   the Zimbabwean government's land seizures violated the rule of law. The
   disbandment reflects SADC's hierarchy of values, in terms of which the
   organization's formal commitment to human rights and a regional legal
   order is subordinate to the political imperatives of regime solidarity
   and respect for sovereignty. The Tribunal saga demonstrates that the
   jurisdiction of regional courts derives not simply from their official
   mandates but from an interplay between domestic and regional law and
   politics.},
