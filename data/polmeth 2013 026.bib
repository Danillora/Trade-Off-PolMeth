@article{ WOS:000327276200003,
Author = {Duckitt, John and Bizumic, Boris},
Title = {Multidimensionality of Right-Wing Authoritarian Attitudes:
   Authoritarianism-Conservatism-Traditionalism},
Journal = {POLITICAL PSYCHOLOGY},
Year = {2013},
Volume = {34},
