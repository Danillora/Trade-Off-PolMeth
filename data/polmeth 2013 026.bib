@article{ WOS:000435282100005,
Author = {Cisar, Ondrej},
Title = {The diffusion of public interest mobilisation: a historical sociology
   perspective on advocates without members in the post-communist Czech
   Republic},
Journal = {EAST EUROPEAN POLITICS},
Year = {2013},
Volume = {29},
Number = {1},
Pages = {69-82},
Abstract = {The paper proposes an explanation for the boom in advocacy organisations
   in the Czech Republic after the collapse of the Communist regime in
   1989. There has been a major increase in the number of professional
   advocacy organisations, accompanied by an influx of
   ``post-materialist{''} issues. In the first half of 1990s, American and
   European state and non-state actors promoted these issues. Since the end
   of 1990s, European Union funds have taken over as the most important
   source of funding. Foreign funding has thus created conditions conducive
   to the emergence and development of advocacy-based activism in the Czech
   Republic. Moreover, advocacy organisations are presented in the paper as
   the locus of a new type of politically oriented activism, which is no
   longer based on mobilisation, but rather on transactions.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
