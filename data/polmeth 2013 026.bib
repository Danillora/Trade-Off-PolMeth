@article{ WOS:000211707200004,
Author = {Hansel, Mischa},
Title = {The Decline of Arms Control: Media Coverage and Elite Opinion in the
   United States},
Journal = {CONTEMPORARY SECURITY POLICY},
Year = {2013},
Volume = {34},
Number = {1},
Pages = {64-93},
Abstract = {Is arms control becoming irrelevant? This analysis offers a new way of
   testing the decline of arms control. Rather than focusing on foreign
   policy outputs and international outcomes, this analysis examines
   perceptions of security policy decision makers and opinion leaders
   through their references to arms control. Salience and framing analysis
   are used to evaluate the marginalization of arms control in political
   communication. Empirically, the article examines selected American media
   coverage, research institute publications, and the Congressional Record.
   This research confirms a dwindling number of references to the concept
   of arms control over the last two decades. This trend is not explained
   by the emergence of new technological issues, nor does it follow the
   chronological pattern of actual arms control successes and failures.
   Rather it hints at a more fundamental transformation of political
   thought.},
