Language = {English},
Affiliation = {Atkinson, C (Corresponding Author), Univ Bradford, Sch Management, Bradford BD7 1DP, W Yorkshire, England.
   Atkinson, Carol, Univ Bradford, Sch Management, Bradford BD7 1DP, W Yorkshire, England.
   Lucas, Rosemary, Manchester Metropolitan Univ, Sch Business, Manchester M15 6BH, Lancs, England.},
DOI = {10.1111/j.1467-9299.2012.02040.x},
ISSN = {0033-3298},
EISSN = {1467-9299},
Keywords-Plus = {STREET-LEVEL BUREAUCRACY; CRITICAL REALISM; SERVICE SECTOR;
