@article{ WOS:000218623500014,
Author = {Newman, Edward},
Title = {The violence of statebuilding in historical perspective: implications
   for peacebuilding},
Journal = {PEACEBUILDING},
Year = {2013},
Volume = {1},
Number = {1},
Pages = {141-157},
Abstract = {In historical perspective statebuilding has often been violent because
   it threatens the interests of groups which are on the outside of the
   process and it encounters outlying resistance which must be subjugated.
   The consolidation of national political projects is a related process
   that has often been accompanied by significant armed conflict as groups
   with vying political visions compete for control of the agenda. Yet, in
   the twenty-first century some scholars and most policy analysts
   interested in peacebuilding portray peacebuilding and statebuilding as
   complementary or even mutually dependent. International peacebuilding
   activities in post-conflict and conflict-prone societies - undertaken by
   international organisations and individual donor states focus upon the
   creation or recreation of state institutions as a conciliatory process
   and as the key to peace and stability. This article explores this
   apparent paradox and considers if there has been a historical
   transformation in the relationship between statebuilding and peace, and
   what implications historical statebuilding experience holds for
   international peacebuilding activities.},
