@article{ WOS:000326474400008,
Author = {Maria Kowalczyk, Anna},
Title = {Indigenous Peoples and Modernity Mapuche Mobilizations in Chile},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2013},
Volume = {40},
Number = {4},
Pages = {121-135},
Month = {JUL},
Abstract = {The twenty-first century has ushered in a new era for the numerous
   social and political movements of Mapuche indigenous groups in Chile.
   Apart from distancing themselves from state institutionality and
   demanding autonomy, they have tended to reject alliances with the
   traditional left, considering it alien to indigenous subjectivities.
   Study of these various new wave movements paying particular attention to
   the relationship between indigenous and modern subjectivities suggests
