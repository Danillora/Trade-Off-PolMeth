@article{ WOS:000218629000006,
Author = {Ojendal, Joakim and Ou, Sivhuoch},
Title = {From friction to hybridity in Cambodia: 20 years of unfinished
   peacebuilding},
Journal = {PEACEBUILDING},
Year = {2013},
Volume = {1},
Number = {3, SI},
Pages = {365-380},
Abstract = {Peacebuilding is rarely delivering to expectations. Instead, high-strung
   ambitions tend to mix with local dynamics triggering complex processes.
   In particular, the liberal peace approach has been criticised for
   operating with simplified world-views, mismatching with realities. In
   this article, we take a long-term perspective and assess the outcome of
   peacebuilding efforts in Cambodia two decades after the massive
   UNoperation was concluded. We apply the notion of `friction' between
   interveners and intervened in order to illuminate the `hybridity' that
   has, we claim, been the key outcome of both peace and democratisation
   attempts. We trace several key development processes in some detail in
   order to illuminate how the particular kind of `friction' that came with
   this particular kind of peacebuilding. We conclude that although
   progressive development and deepened peace have emerged, inevitably a
   hybrid society has been created with remaining deep-seated tensions and
   severe questions on progressive long-term development.},
