@article{ WOS:000314735600002,
Author = {Bielefeldt, Heiner},
Title = {Misperceptions of Freedom of Religion or Belief},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2013},
Volume = {35},
Number = {1},
Pages = {33-68},
Month = {FEB},
Abstract = {Freedom of religion or belief has been recognized as an international
   human right; however, some recent conceptualizations may blur its status
   as a human right by undermining the principles of universalism, freedom,
   and equality. This happens under different auspices, such as: combating
   defamation of religions, preserving a state imposed interreligious
   harmony, or promoting ideological versions of state secularism. In
   addition, some question the interrelatedness of freedom of religion and
   other human rights. By discussing typical misperceptions, this article
   aims to rebuild a consensus on the significance of freedom of religion
   or belief as a universal human right.},
