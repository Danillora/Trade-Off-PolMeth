@article{ WOS:000329983700003,
Author = {Badcock, Sarah},
Title = {From Villains to Victims: Experiencing Illness in Siberian Exile},
Journal = {EUROPE-ASIA STUDIES},
Year = {2013},
Volume = {65},
Number = {9, SI},
Pages = {1716-1736},
Month = {NOV 1},
Abstract = {This essay presents the subjective experience of life and sickness for
   the punished in late Imperial Siberia, and the distinctions the punished
   made between legitimate and illegitimate forms of punishment. The essay
   also explores state policies towards the sick punished, and explores how
   different levels of the Tsarist administration and local Siberian
   society dealt with the challenge of sick and decrepit exiles. It argues
   that conditions in Siberian prisons were, in general, worse than those
   in European Russian prisons in the post-1906 period, and that the
   experience of exile in eastern Siberia placed it among the most
   difficult locations for exile. Though neither the state nor the punished
   regarded illness as an integral part of their punishment, the prevalence
   of illness and disease compounded the cruelty of sentences.},
