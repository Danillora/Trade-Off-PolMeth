@article{ WOS:000318304900006,
Author = {Stein, Robert and Buzcu-Guven, Birnur and Duenas-Osorio, Leonardo and
   Subramanian, Devika and Kahle, David},
Title = {How Risk Perceptions Influence Evacuations from Hurricanes and
   Compliance with Government Directives},
Journal = {POLICY STUDIES JOURNAL},
Year = {2013},
Volume = {41},
Number = {2},
Pages = {319-342},
Month = {MAY},
Abstract = {In this study we present evidence supporting the view that people's
   perceived risk of hurricane-related hazards can be reduced to a single
   seriousness score that spans different hurricane-induced risk types and
   that compliant behavior with official advisories is strongly dependent
   on whether one perceives a high risk with respect to any type of
   hurricane-related hazards. Our analysis suggests that people are less
   sensitive to risk type than they are to the general seriousness of the
   risks. Using this single seriousness score, representing a composite
   risk measure, emergency managers can be informed about the severity of
   the public's risk perceptions to impending hurricane hazards and might
   better craft their public directives in ways that minimize disruptive
   evacuations and achieve greater compliance with government directives.},
