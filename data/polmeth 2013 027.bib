@article{ WOS:000316815400002,
Author = {Schneider, Christina J. and Urpelainen, Johannes},
Title = {Distributional Conflict Between Powerful States and International Treaty
   Ratification},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
