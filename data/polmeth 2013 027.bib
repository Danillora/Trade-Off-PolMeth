@article{ WOS:000214270300001,
Author = {Rear, David and Jones, Alan},
Title = {Discursive struggle and contested signifiers in the arenas of education
   policy and work skills in Japan},
Journal = {CRITICAL POLICY STUDIES},
Year = {2013},
Volume = {7},
Number = {4},
Pages = {375-394},
Abstract = {This article critically examines discourses of education policy and work
   skills in twentyfirst century Japan. Conducting a close analysis of
   texts produced by the Ministry of Education (MEXT) and the Japan
   Business Federation, it combines analytical constructs from the
   Discourse Theory of Laclau and Mouffe with those from Critical Discourse
   Analysis (CDA) to identify two distinct discourses currently engaged in
   a struggle for hegemony: a discourse of individual-centered
   neoliberalism and a discourse of group-centered moral conservatism. This
   struggle can be described at the level of key signifiers, whose meanings
   are tacitly contested by the two discourses. In employing these
   antagonistic discourses within single texts and even single clauses,
   policymakers produce a hybridized but ultimately incoherent discourse
   that attempts to uphold the prevailing social order by harnessing the
   individuality of Japanese citizens into a vision of patriotism and
   national solidarity.},
