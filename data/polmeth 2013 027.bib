@article{ WOS:000214201900005,
Author = {Oomen, Barbara},
Title = {Waving with Treaties? The Politics of Implementing Human Rights
   Education in the Netherlands},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2013},
Volume = {5},
Number = {2},
Pages = {291-317},
Month = {JUL},
Abstract = {Given the fact that many countries did not take any formal steps to
   implement the policies in the field of human rights education agreed
   upon worldwide in the 1990s, this article seeks to give an example of
   such a country, and to explain its reluctance. Taking an actor-oriented
   approach to human rights implementation, and partly building on personal
   experience, it seeks to explain why formal reforms did not take place by
   looking into the wide variety of actors involved in the process and
   their positions on formal human rights education. As a second part of
   the explanation, it discusses five different dynamics of rights
   implementation and their effectiveness from a civil society perspective:
   waving with treaties, putting the issue on the political agenda,
   vernacularization, mobilizing a broad support base, and litigation.
   Considered together, this discussion of the actors involved in realizing
   human rights education in the Netherlands and the strategies that they
   employed adds not only to an understanding of the difficulties in
   realizing this right in a given setting, but also to theories of human
   rights implementation in general.},
