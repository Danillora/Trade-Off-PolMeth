@article{ WOS:000326713100002,
Author = {Wolf, Joan B.},
Title = {The Politics of Dissent},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2013},
Volume = {34},
Number = {4, SI},
Pages = {306-316},
Month = {OCT 1},
Abstract = {I respond here to criticism of my book, Is Breast Best? Taking on the
   Breastfeeding Experts and the New High Stakes of Motherhood, that has
   appeared in popular media and academic debate. I argue that the
   frequently visceral reaction to the book affirms several of its central
   arguments: that both scientists and the public often misunderstand the
   limits of observational research; that they fail to appreciate that all
   choices, including those about risk, involve trade-offs; and that an
   ethic of total motherhood, which requires mothers to do whatever it
   takes to eliminate virtually any childhood risk, no matter how small or
   likely the risk or what the cost is for them in the process, frames
   public discourse about breastfeeding, families, and care work more
   broadly defined.},
