@article{ WOS:000320691700002,
Author = {Stachowitsch, Saskia},
Title = {Professional Soldier, Weak Victim, Patriotic Heroine: GENDER IDEOLOGIES
   IN DEBATES ON WOMEN'S MILITARY INTEGRATION IN THE US},
Journal = {INTERNATIONAL FEMINIST JOURNAL OF POLITICS},
Year = {2013},
Volume = {15},
Number = {2},
Pages = {157-176},
Month = {JUN 1},
Abstract = {Discourses on military gender integration in the US are shaped by
   various groups of social actors who promote different gender ideologies
   according to their varying interests in the process of military
   restructuring. Different branches of the Armed Forces, politicians, NGOs
   and lobbyists argue for or against integration by utilizing different
   images of military masculinity and femininity. Depending on social and
   military conditions, female soldiers are portrayed as efficient
   professionals, patriotic heroines, victims in need of protection or
   sexualized intruders in a male domain. The aim of this paper is to
   identify the strategic and political contexts that influence these
   ideologies, emphasizing the importance of structural change in military
   and civilian labor markets, patterns of gender-specific labor division
   and political power relations. For this purpose, media reporting on
   military gender issues in The New York Times (NYT) and The Washington
   Post (WP) from 1990 to 2010 is examined in a discourse-analytical case
   study.},
