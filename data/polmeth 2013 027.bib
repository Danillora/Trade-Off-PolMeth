@article{ WOS:000316334700010,
Author = {Mugge, Daniel},
Title = {THE POLITICAL ECONOMY OF EUROPEANIZED FINANCIAL REGULATION},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
