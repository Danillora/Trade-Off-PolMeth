@article{ WOS:000315692100005,
Author = {Gill, Graeme},
Title = {Political Symbolism and the Fall of the USSR},
Journal = {EUROPE-ASIA STUDIES},
Year = {2013},
Volume = {65},
Number = {2, SI},
Pages = {244-263},
Month = {MAR 1},
Abstract = {When Gorbachev came to power he inherited not simply a system in crisis,
   but a structure of symbols that was embedded within the broader
   political system and which was also in crisis. Given the ideocratic
   nature of the Soviet system, any change to that system would require
   symbolic change as well. This essay charts how Gorbachev sought to come
   to grips with this problem, investing some symbols with new meaning
   while rejecting others. It shows how ultimately he was unable to produce
   a new, coherent narrative.},
