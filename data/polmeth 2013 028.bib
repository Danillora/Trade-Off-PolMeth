@article{ WOS:000320018800003,
Author = {Buzogany, Aron},
Title = {Selective Adoption of EU Environmental Norms in Ukraine. Convergence
   a la Carte},
Journal = {EUROPE-ASIA STUDIES},
Year = {2013},
Volume = {65},
Number = {4, SI},
Pages = {609-630},
Month = {JUN 1},
Abstract = {While the EU's policies towards non-member states are often discussed
   within frameworks of high politics', one of the most important features
   of the European Neighbourhood Policy is its emphasis on the low
   politics' of sectoral dialogue in functionally differentiated policy
