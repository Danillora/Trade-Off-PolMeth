@article{ WOS:000327929100008,
Author = {Casier, Tom},
Title = {The EU-Russia Strategic Partnership: Challenging the Normative Argument},
Journal = {EUROPE-ASIA STUDIES},
Year = {2013},
Volume = {65},
Number = {7},
Pages = {1377-1395},
Month = {SEP 1},
Abstract = {Russia-EU relations have often been presented in terms of a normative
   gap, with the EU appearing as a normative and Russia as a non-normative
   actor. This article critically analyses this normative argument' which
   sees this gap as the cause of tensions. Pleading for a less dichotomous
   approach to norms and interests, it challenges the normative argument on
   the basis of the assumed congruence between the norm-driven input and
   norm-promoting output of European foreign policy. As an alternative, the
   article explores how the normative agenda in Eastern Europe serves
   instrumental purposes. Selective norm promotion has the potential to
   change the hierarchy of identities among post-Soviet states.},
