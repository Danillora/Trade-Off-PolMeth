@article{ WOS:000214201900004,
Author = {Pantazidou, Maro},
Title = {De-Constructing Marginality with Displaced People: Learning Rights from
   an Actor-Oriented Perspective},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2013},
Volume = {5},
Number = {2},
Pages = {267-290},
Month = {JUL},
Abstract = {In this article, I reflect on the lessons learned from a one year
   action-research process with displaced people in London to advocate for
   an actor-oriented approach to human rights education. I suggest that
   when it comes to supporting marginalized people to claim their rights,
   we need not to look for a transferable set of skills and knowledge but
   to nurture a rights consciousness - a sense of entitlement to one's
   rights - through transformative learning methodologies. The article
   discusses both the content and pedagogy of such methodologies, paying
   special attention to critical reflection and affective learning. I
   conclude that for us as human rights educators to engage in
   transformative learning for human rights we need to approach rights as
   rooted in the relationships, contextual realities, and perspectives of
   the people we work for and with, and I suggest that the way we
   understand rights incurs a set of implications for the way we `teach'
   and claim them.},
