@article{ WOS:000214201600007,
Author = {Williams, Carmel and Brian, Garry},
Title = {Using a Rights-Based Approach to Avoid Harming Health Systems: A Case
   Study from Papua New Guinea},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2013},
Volume = {5},
Number = {1},
Pages = {177-194},
Month = {MAR},
Abstract = {Impact assessments of proposed health programmes in developing countries
   have not been conducted routinely. But with recent evidence that
   well-intentioned health programmes can do harm to health systems, it is
   timely that such assessments become a part of programme design. This
   policy and practice note applies a rights-based health system impact
   assessment to a proposed eye health intervention in Papua New Guinea
   (PNG). The planned stand-alone intervention was based on an Indian model
   of eye health service. The impact of the planned activities on each of
   the World Health Organization (WHO)-defined six blocks of PNG's health
   system was assessed using 22 indicators from a rights-based tool. The
   authors found that all six blocks would have been weakened had this
   programme of activities been implemented. PNG did not have the
   facilities, human resources, information or supply systems, management
   capacity or finances to accommodate and sustain the scale of this
   intervention. Had the intervention proceeded as planned, resources from
   basic health services would have been redirected to eye health, away
   from core services, likely causing a regression in the realization of
   health rights. Use of this tool affords an opportunity to assess and
   improve health programme proposals before raising expectations,
   attracting political and funding support, and, on implementation,
   causing harm to health systems.},
