@article{ WOS:000329349100004,
Author = {Creswell, Michael H. and Kollmer, Dieter H.},
Title = {Power, Preferences, or Ideas? Explaining West Germany's Armaments
   Strategy, 1955-1972},
Journal = {JOURNAL OF COLD WAR STUDIES},
Year = {2013},
Volume = {15},
Number = {4},
Pages = {55-103},
Month = {OCT},
Abstract = {Which major theory of international relationsneorealism, liberalism, or
   constructivismbest explains states' weapons procurement strategies? This
   article addresses that question by examining the case of the Federal
   Republic of Germany (FRG) during the Cold War. Through an in-depth
   analysis of the FRG's armaments strategy from the time the country was
   admitted into the North Atlantic Treaty Organization in 1955 until the
   early 1970s, the article demonstrates that although ideas and external
   security concerns did play a role, economic concerns won out more often
   than not. IR scholars must therefore account for a variety of factors
   when attempting to explain the FRG's weapons procurement strategy during
   this period. Neorealism, liberalism, and constructivism are useful in
   thinking about West Germany's purchases of weapons, but none of the
   three theories is adequate on its own.},
