@article{ WOS:000330290100002,
Author = {Zenteno, Rene and Giorguli, Silvia E. and Gutierrez, Edith},
Title = {Mexican Adolescent Migration to the United States and Transitions to
   Adulthood},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2013},
Volume = {648},
Number = {1},
Pages = {18-37},
Month = {JUL},
Abstract = {This article contributes to our understanding of how the motivation to
   migrate varies depending on the stage in the life course, particularly
   during the youth-to-adult transition. Using data from the Mexican
   Migration Project, we estimate discrete-time-hazard models of the
   probabilities of a first migration, using individual, household,
   community, and macroeconomic variables during and after adolescence for
   both men and women. We show that the determinants of migration are
   different for adolescents than they are for adults. While
   migration-related social capital has proved to be an important factor in
   increasing and perpetuating migration, we find that its effect is even
   stronger for teenagers than for other age groups. We also shed light on
   how adolescent migration is influenced by other major markers of the
   transition to adulthood, such as education, labor force experience, and
   family formation.},
