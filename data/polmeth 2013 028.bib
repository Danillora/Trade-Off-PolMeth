@article{ WOS:000319740400003,
Author = {Toal (Gearoid O Tuathail), Gerard and O'Loughlin, John},
Title = {Inside South Ossetia: a survey of attitudes in a de facto state},
Journal = {POST-SOVIET AFFAIRS},
Year = {2013},
Volume = {29},
Number = {2},
Pages = {136-172},
Month = {MAR 1},
Abstract = {South Ossetia was the main site of the August 2008 war between Georgian
   military forces, local South Ossetian forces, and the Russian military.
   Soon thereafter, the Russian Federation recognized the territory as a
   state, the South Ossetian Republic. This article reviews the contending
   scripts used to understand South Ossetia and the basis of its claim to
   be a state. Presenting the results of a public opinion survey of
   Ossetians living in the territory in late 2010, we discuss the trust in
   local institutions and leadership, ethnic Ossetian attitudes toward
   other groups, return and property, as well as relations with Russia and
   Georgia.},
