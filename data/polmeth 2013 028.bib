@article{ WOS:000329983900006,
Author = {Bloom, Stephen and Petrova, Vladislava},
Title = {National Subversion of Supranational Goals: `Pork-Barrel' Politics and
   EU Regional Aid},
Journal = {EUROPE-ASIA STUDIES},
Year = {2013},
Volume = {65},
Number = {8},
Pages = {1599-1620},
Month = {OCT 1},
Abstract = {The structural funds were designed to give the European Commission
   supranational control over where regional aid was spent. In this
   article, we argue that domestic pork-barrel' politics determine which
   regions are awarded regional aid in the new member states of Eastern
   Europe. Support for our argument comes from a dataset that includes
   1,688 European Regional Development Fund grants to Latvia's 118 regions
   and 1,533 pre-accession Phare awards to Bulgaria's 264 municipalities.
   In both Bulgaria and Latvia, we find that regional aid projects went to
   wealthier not poorer localities, and to those that supported coalition
   parties in the previous election.},
