@article{ WOS:000320018800005,
Author = {Dimitrova, Antoaneta and Dragneva, Rilka},
Title = {Shaping Convergence with the EU in Foreign Policy and State Aid in
   Post-Orange Ukraine: Weak External Incentives, Powerful Veto Players},
Journal = {EUROPE-ASIA STUDIES},
Year = {2013},
Volume = {65},
Number = {4, SI},
Pages = {658-681},
Month = {JUN 1},
Abstract = {This essay analyses convergence with EU rules in Ukraine in two policy
   areasforeign and security policy and state aid regulation. Comparing the
   two, we find different levels of convergence, somewhat higher in foreign
   policy (but slowing down after 2010) than in state aid law regulation.
   We explain this by analysing the presence and actions of oligarchs as
   veto players that have had an extensive influence on policy in the
   Ukrainian political system in recent years. In policy areas where
   convergence with EU rules negatively affects the interests of oligarchs
   and their political allies, we see only limited convergence with EU
   legislation and policies.},
