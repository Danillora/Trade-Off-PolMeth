@article{ WOS:000329349100002,
Author = {Gasiorowski, Mark J.},
Title = {The CIA's TPBEDAMN Operation and the 1953 Coup in Iran},
Journal = {JOURNAL OF COLD WAR STUDIES},
Year = {2013},
Volume = {15},
Number = {4},
Pages = {4-24},
Month = {OCT},
Abstract = {This article gives an overview of TPBEDAMN, a large covert operation the
   U.S. Central Intelligence Agency (CIA) carried out in Iran for several
   years in the early 1950s. TPBEDAMN was a psychological warfare operation
   intended to undermine the influence of the Soviet Union and its Iranian
   ally, the Communist Tudeh Party, through covert propaganda and political
   action activities. When U.S. officials decided in early 1953 to
   overthrow Iran's prime minister, Mohammad Mosaddeq, they relied heavily
   on TPBEDAMN's large network of Iranian agents and subagents to plan and
   implement a coup d'etat. The overview of TPBEDAMN presented here helps
   to clarify both the nature of the organizational apparatus the CIA used
   against Mosaddeq and the broader context within which the coup occurred,
   especially the intense Cold War climate that prevailed and the
   prevalence of psychological warfare in this era.},
