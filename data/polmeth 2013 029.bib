@article{ WOS:000312485100009,
Author = {Duff, Andrew},
Title = {On Dealing with Euroscepticism},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2013},
Volume = {51},
Number = {1, SI},
Pages = {140-152},
Month = {JAN},
Abstract = {The article examines if and, if so, why and how political parties
   opposing the European Union have thrived. It looks at the role of
   national parliaments and national media in failing to engage
   convincingly with the European dimension of politics, and asks why the
   forces of European federalism have been so relatively ineffective. By
   analyzing the tensions between national and European political parties
   especially in the conduct of elections to the European Parliament, the
   article traces the rise of the nationalist forces within the EP and
   assesses their impact on the legislative and other work of the
   institution. It concludes by proposing measures to tackle the
   nationalist challenge to European unification, including more
   differentiated forms of integration, more direct democracy, the
   development of federal political parties and the constitutional
   settlement of a federal union.},
