@article{ WOS:000322945900013,
Author = {Voinea, Liviu},
Title = {Revisiting crisis generators in Romania and other new EU member states},
Journal = {REVIEW OF INTERNATIONAL POLITICAL ECONOMY},
Year = {2013},
Volume = {20},
Number = {4},
Pages = {979-1008},
Month = {AUG 1},
Abstract = {This article argues that the causes of the crisis in Romania and other
   new EU member states were different from those affecting Western Europe
   or the US. The main argument of this paper is that bad domestic
   monetary, fiscal and regulatory policies were the real crisis
   generators, while exogenous crisis mechanisms, including contagion from
   the global financial crisis, were just the trigger. Romania, and other
   new EU member states, has been facing the crisis of a consumption-led
   development model, not just a financial crisis imported from more
   developed economies. This paper brings insights into the impact of the
   flat-tax regime on consumption and the impact of speculative capital
   inflows disguised as foreign direct investments.},
