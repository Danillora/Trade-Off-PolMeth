Author = {Renz, Bettina and Sullivan, Jonathan},
Title = {Electronic Resources and the Study of Political Elites in Russia},
Journal = {EUROPE-ASIA STUDIES},
Year = {2013},
Volume = {65},
Number = {10},
Pages = {1898-1911},
Month = {DEC 1},
Abstract = {Developments in the information society in Russia have led to the
   creation of an abundance of online primary texts by political actors and
   institutions. This article surveys a large selection of official
   electronic texts with the aim of encouraging more extensive use of
   online sources and easing researchers' navigation of Russian
   politicians' use of the internet. While the authors do not claim that
   online data can replace the insights gained from elite interviews, the
   article suggests that online texts could be used more extensively as a
   valuable addition to existing methods applied to the study of Russian
   political elites.},
