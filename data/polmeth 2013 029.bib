@article{ WOS:000326234000004,
Author = {Freeman, Richard B.},
Title = {Failing the Test? The Flexible US Job Market in the Great Recession},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2013},
Volume = {650},
Number = {1},
Pages = {76-97},
Month = {NOV},
Abstract = {The Great Recession tested the ability of the great U.S. jobs machine to
   limit the severity of unemployment in a major economic downturn and to
   restore full employment quickly afterward. In the crisis the American
   labor market failed to live up to expectations. The level and duration
   of unemployment increased substantially in the downturn, and the growth
   of jobs was slow and anemic in the recovery. This article documents
   these failures and their consequences for workers. The U.S. performance
   in the Great Recession contravenes conventional views of the virtues of
   market-driven flexibility compared to institution-driven labor
   adjustments and the notion that weak labor institutions and greater
   market flexibility offer the best road to economic success in a modern
   capitalist economy.},
