@article{ WOS:000320364800002,
Author = {Loytomaki, Stiina},
Title = {The Law and Collective Memory of Colonialism: France and the Case of
   `Belated' Transitional Justice},
Journal = {INTERNATIONAL JOURNAL OF TRANSITIONAL JUSTICE},
Year = {2013},
Volume = {7},
Number = {2},
Pages = {205-223},
Month = {JUL},
Abstract = {This article examines the role of law in a number of disputes concerning
   collective memory and the legacy of colonialism in France, and addresses
   the implications of this intersection of law, memory and politics
   concerning colonialism for French national identity. It is argued that
   the trials and other legal interventions analysed in this article are a
   means by which contested versions of the colonial past claim publicity
   and legitimacy. The article illustrates the state's use of law to
   maintain a particular narrative of French colonialism, increasingly
   contested in the present by postcolonial memory politics, and analyses
   the role and impact of collective memory, as constructed in the legal
   realm, in how nations address the past. Through this French case study,
   the article argues that the law has particular relevance as an
   instrument of history and memory politics in `belated' transitional
   justice settings.},
