@article{ WOS:000326713100003,
Author = {Bartick, Melissa},
Title = {Breastfeeding and Health: A Review of the Evidence},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2013},
Volume = {34},
Number = {4, SI},
Pages = {317-329},
Month = {OCT 1},
Abstract = {Some commentators have claimed that breastfeeding benefits are
   overstated or that breastfeeding is not the cause of the improved health
   outcomes. In 2007, the Agency for Healthcare Research and Quality (AHRQ)
   issued a definitive report summarizing the body of evidence on various
   maternal and infant health outcomes in developed countries. The weight
   of this and subsequent evidence supports an association between
   breastfeeding and health outcomes in mothers and infants. While
   confounding factors exist, this association is likely to be causal for
   many or most of these conditions based on the Bradford Hill criteria for
   causation. Policy discussions need to consider the substantial maternal
   health effects of breastfeeding.},
