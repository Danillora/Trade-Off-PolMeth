@article{ WOS:000314156400008,
Author = {Muzaka, Valbona},
Title = {Contradictions, frames and reproductions: The emergence of the WIPO
   Development Agenda},
Journal = {REVIEW OF INTERNATIONAL POLITICAL ECONOMY},
