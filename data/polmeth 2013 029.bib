@article{ WOS:000210808200006,
Author = {Kahn, Martin},
Title = {British Intelligence on Soviet War Potential in 1939: A Revised Picture
   and Some Implications (a Contribution to the ` Unending Debate')},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2013},
Volume = {28},
Number = {5},
Pages = {717-747},
Abstract = {The British government tried to create a peace front against German
   aggression in 1939 and the inclusion of the Soviet Union was
   controversial. This article investigates British intelligence on Soviet
   war potential in 1939 with the aim of comparing intelligence to reality,
   placing intelligence in context by following the British government
   discussion, commenting on earlier research about British intelligence on
   Soviet war potential and trying to find explanations for the fact that
   intelligence was far from reality. British assessments were of
   importance and earlier research is amended.},
