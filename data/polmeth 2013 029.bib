@article{ WOS:000318586900006,
Author = {Moran, Christopher},
Title = {Ian Fleming and the Public Profile of the CIA},
Journal = {JOURNAL OF COLD WAR STUDIES},
Year = {2013},
Volume = {15},
Number = {1},
Pages = {119+},
Month = {WIN},
Abstract = {This article represents the first major analysis of the appearance of
   the U.S. Central Intelligence Agency (CIA) in the James Bond novels of
   British spy fiction writer Ian Fleming. The article shows that Fleming
   was remarkably influential during the early Cold War in establishing the
   public profile of the CIA. The novels, which include Manifold references
   to the agency and its staff, were published at a time when the CIA kept
   out of the public limelight and when other cultural forms, including
   Hollywood, refrained from making too much fanfare about intelligence
   matters. Drawing on recently declassified material, including the papers
   of fabled CIA Director Allen Dulles, the article demonstrates that the
   agency took a keen interest in Bond, even drawing inspiration from his
   adventures and the novels' depictions of technology.},
Publisher = {MIT PRESS},
