@article{ WOS:000315692100004,
Author = {Vanhala-Aniszewski, Marjatta and Siilin, Lea},
Title = {The Representation of Mikhail Gorbachev in the Twenty-first Century
   Russian Media},
Journal = {EUROPE-ASIA STUDIES},
Year = {2013},
Volume = {65},
Number = {2, SI},
Pages = {221-243},
Month = {MAR 1},
Abstract = {The aim of this study is to investigate the representation of Mikhail
   Gorbachev in contemporary Russian media discourse. Attention is paid to
   Gorbachev's social roles and activities as well as his personality, as
   presented in Russian news texts. The empirical data were collected over
   the period from 2000 to 2009 from seven major Russian newspapers.
   According to these data, a dual relationship to Gorbachev exists: in the
   West he is an honoured politician with a high profile, whereas in Russia
   the attitude towards him is ambivalent. In most texts he is represented
   as a once important political actor.},
