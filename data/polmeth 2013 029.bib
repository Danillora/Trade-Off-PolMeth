@article{ WOS:000327841100005,
Author = {Kay, Rebecca},
Title = {`She's Like a Daughter to Me': Insights into Care, Work and Kinship from
   Rural Russia},
Journal = {EUROPE-ASIA STUDIES},
Year = {2013},
Volume = {65},
Number = {6},
Pages = {1136-1153},
Month = {AUG 1},
Abstract = {This article draws on ethnographic research into a state-funded homecare
   service in rural Russia. The article discusses intersections between
   care, work and kinship in the relationships between homecare workers and
   their elderly wards and explores the ways in which references to
   kinship, as a means of authenticating paid care and explaining its
   emotional content, reinforce public and private oppositions while doing
   little to relieve the tensions and conflicts of care work. The
   discussion brings together detailed empirical insights into local
   ideologies and practices as a way of generating new theoretical
   perspectives, which will be of relevance beyond the particular context
   of study.},
