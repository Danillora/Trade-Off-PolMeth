@article{ WOS:000212638300003,
Author = {Jevsnik, Mojca Vah and Gombac, Jure},
Title = {Transnationalism, Education and Slovenian Diaspora: Exploring Online
   Educational Resources for Slovenian Emigrants and Their Descendants},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2013},
Volume = {21},
Number = {4},
Pages = {513-525},
Abstract = {The article addresses the use of online communication technologies for
   educational purposes by Slovenian emigrants and their descendants.
   Technologies such as web-classes, forums, blogs and social networks
   enable them to maintain transnational links with the homeland,
   especially in terms of learning the language, and exploring, preserving
   and negotiating cultural practices. The article builds on the extensive
   content analysis of web pages, portals and social network sites where
   they interact to discuss the variety of themes connected with Slovenian
   culture. It is argued that shared meanings are not necessarily
   communicated and negotiated within the frame of a single language, and
   that learning about cultural heritage and contemporary lifestyles
   through discussions on social networks is an efficient and popular mode
   of informal learning.},
