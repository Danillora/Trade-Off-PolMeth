@article{ WOS:000326187900008,
Author = {Bishop, Bradford H.},
Title = {DROUGHT AND ENVIRONMENTAL OPINION A STUDY OF ATTITUDES TOWARD WATER
   POLICY},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2013},
Volume = {77},
Number = {3},
Pages = {798-810},
Month = {FAL},
Abstract = {Previous research into place effects has provided mixed evidence about
   the effect of geography on public opinion. Much of the work finding a
   relationship is susceptible to methodological criticisms of spuriousness
   or endogeneity. In this paper, I leverage a unique research design to
   examine the influence of residential setting on environmental attitudes
   regarding water use. The findings indicate that local drought conditions
   increase individuals level of concern about the nations water supply. In
   addition, drought conditions are related to public attitudes toward
   water use regulation, with those living in drought-afflicted counties
   more likely to support government regulation. This study provides a firm
   foundation for research attempting to demonstrate that local conditions
   have a causal effect on public opinion.},
Publisher = {OXFORD UNIV PRESS},
