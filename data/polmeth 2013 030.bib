@article{ WOS:000325776100003,
Author = {Almquist, Zack W. and Butts, Carter T.},
Title = {Dynamic Network Logistic Regression: A Logistic Choice Analysis of
   Inter- and Intra-Group Blog Citation Dynamics in the 2004 US
   Presidential Election},
Journal = {POLITICAL ANALYSIS},
