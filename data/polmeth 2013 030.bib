@article{ WOS:000316655400001,
Author = {Hoffmann, Anja and Koenig, Christoph},
Title = {Scratching the Democratic Facade: Framing Strategies of the 20 February
   Movement},
Journal = {MEDITERRANEAN POLITICS},
Year = {2013},
Volume = {18},
Number = {1},
Pages = {1-22},
Month = {MAR 1},
Abstract = {This article tentatively assesses the role of the 20 February Movement
   in current Moroccan politics. Drawing on social movement theory, the
   analysis focuses on the ways in which the movement has attempted to
   challenge the legitimacy of the system in place. The article first
   provides a thorough description of the movement's characteristics.
   Subsequently, the frame analysis in the main part studies how the
   movement has sought to deconstruct the self-portrayal of the incumbent
   regime. It argues that the relevance of the challenge mounted by the 20
   February Movement derives from its transgressions of red lines formerly
   delimiting political discourses in the kingdom.},
