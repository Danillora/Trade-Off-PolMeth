@article{ WOS:000329778800004,
Author = {Mermier, Franck},
Title = {The Frontiers of Beirut: Some Anthropological Observations},
Journal = {MEDITERRANEAN POLITICS},
Year = {2013},
Volume = {18},
Number = {3},
Pages = {376-393},
Month = {NOV 1},
Abstract = {In Beirut, the relationship to urban space is configured by forms of
   violence, fear and power, which are connected to the political and
   sectarian dimension of urban territories. This dimension conveys to
   these spaces a geopolitical status which combines local and external
   stakes. The struggle to impose identity markers and symbolic
   appropriation of spaces turns Beirut into a disputed multi-dimensional
   geopolitical site. This article deals with the different forms of
   representation and the marking of frontiers in Beirut, whether on the
   level of symbolic perceptions or on that of the social usage of space.},
