@article{ WOS:000314715000003,
Author = {Giraudy, Agustina},
Title = {Varieties of Subnational Undemocratic Regimes: Evidence from Argentina
   and Mexico},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
