@article{ WOS:000318877800002,
Author = {Ahmed, Amel},
Title = {The Existential Threat: Varieties of Socialism and the Origins of
   Electoral Systems in Early Democracies},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
