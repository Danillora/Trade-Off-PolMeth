@article{ WOS:000322304900007,
Author = {Ozzano, Luca and Giorgi, Alberta},
Title = {The Debate on the Crucifix in Public Spaces in Twenty-First Century
   Italy},
Journal = {MEDITERRANEAN POLITICS},
Year = {2013},
Volume = {18},
Number = {2, SI},
Pages = {259-275},
Month = {JUL 1},
Abstract = {Although the presence of the crucifix in public classrooms and other
   public offices is an ancient Italian tradition, it was never a political
   issue until recent times. In the early 2000s, some court cases and other
   events (first at the national and later also at the European level)
   turned the public display of the crucifix into a major issue in the
   national political debate. This article analyses the frames used by
   social and political actors in the different phases of this debate, in
   order to understand its evolution and its connection to the broader
   discussion on values in the public sphere developed in Italy in recent
   times.},
