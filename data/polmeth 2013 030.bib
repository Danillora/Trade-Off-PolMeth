@article{ WOS:000316284400004,
Author = {Fricke, Christine},
Title = {Protocol, politics and popular culture: the independence jubilee in
   Gabon},
Journal = {NATIONS AND NATIONALISM},
Year = {2013},
Volume = {19},
Number = {2},
Pages = {238-256},
Month = {APR},
Abstract = {National days are powerful moments of commemoration that aim at renewing
   the citizens' bonds to the nation and the state. In order to be
   successful, public rituals need to draw large audiences, and their
   ceremonial design therefore has to be adapted to suit the masses,
   employing elements of popular culture and everyday forms of nationhood.
   Despite drawing its significance from the declaration of independence in
   1960, however, Gabon's independence jubilee was less concerned with
   history and commemoration than with celebrating the state and the nation
   in the present. The ceremonial design of Gabon's jubilee featured
   intensive preparations, official ceremonies, popular festivities and
   symbolic politics. In this article, I look at why history and
   commemoration played such an unimportant role during the celebrations
   and how Gabon's jubilee organisers included official as well as popular
   forms of nationhood to assure the population's participation.},
