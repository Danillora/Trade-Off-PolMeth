@article{ WOS:000217818200008,
Author = {Cummings, Sally N.},
Title = {Leaving Lenin: elites, official ideology and monuments in the Kyrgyz
   Republic},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2013},
Volume = {41},
Number = {4, SI},
Pages = {606-621},
Abstract = {Many Lenin monuments remain in cities around the former Soviet republics
   and a few national or regional authorities have decreed it against the
   law to deface or remove them. The Lenin monument in Bishkek, capital
   city of the Kyrgyz Republic, is an example of both policies. On two main
   counts, however, the fate of this particular bronze statue of Vladimir
   Ilyich Lenin has been unusual. Only in the Kyrgyz case was the country's
   central Lenin monument left untouched for over a decade after the
   collapse of communism, a decree for its preservation as a national
   treasure being put in force as late as 2000. And, when, in 2003, the
   government after all decided to remove the monument, it was then
   relocated only some 100 yards from its original location. These twin
   issues of timing and new spatial framing offer a window on the
   relationship between state ideology and politics in the Kyrgyz Republic.
   I propose to use an official ideology approach to understand the Kyrgyz
   ruling elite's ideological relationship to the Lenin monument after the
   collapse of communism.},
