@article{ WOS:000320863800008,
Author = {Bel, Germa and Fageda, Xavier and Mur, Melania},
Title = {Why Do Municipalities Cooperate to Provide Local Public Services? An
   Empirical Analysis},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2013},
Volume = {39},
Number = {3, SI},
Pages = {435-454},
Month = {JUN 1},
Abstract = {Intermunicipal cooperation is being increasingly adopted in various
   countries as a part of local service delivery reforms. This article
   draws on survey data from Spain's municipalities to examine the reasons
   underpinning the decisions of local governments to engage in
   intermunicipal cooperation and privatisation. Our empirical analysis
   indicates that small municipalities prefer to rely on cooperation for
   reducing costs, while their larger counterparts prefer to privatise the
