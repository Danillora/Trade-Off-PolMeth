@incollection{ WOS:000422855100011,
Author = {Harrison, Ann},
Book-Author = {Lannon, J
   Halpin, EF},
Title = {Counting the Unknown Victims of Political Violence: The Work of the
   Human Rights Data Analysis Group},
Booktitle = {HUMAN RIGHTS AND INFORMATION COMMUNICATION TECHNOLOGIES: TRENDS AND
   CONSEQUENCES OF USE},
Year = {2013},
Pages = {139-156},
Abstract = {The Benetech Human Rights Data Analysis Group (HRDAG)
   (http://www.hrdag.org/) analyzes the patterns and magnitude of
   large-scale human rights violations. Together with local partners, HRDAG
   collects and preserves human rights data and helps NGOs and other human
   rights organizations accurately interpret quantitative findings. HRDAG
   statisticians, programmers, and data analysts develop methodologies to
   determine how many of those killed and disappeared have never been
