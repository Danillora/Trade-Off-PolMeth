@article{ WOS:000217819200008,
Author = {Kamusella, Tomasz},
Title = {Germanization, Polonization, and Russification in the partitioned lands
   of Poland-Lithuania},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2013},
Volume = {41},
Number = {5},
Pages = {815-838},
Abstract = {Two main myths constitute the founding basis of popular Polish ethnic
   nationalism: first, that Poland-Lithuania was an early Poland, and
   second, that the partitioning powers at all times unwaveringly pursued
   policies of Germanization and Russification. In the former case, the
   myth appropriates a common past today shared by Belarus, Latvia,
   Lithuania, Poland, and Ukraine. In the latter case, Polonization is
   written out of the picture entirely, as also are variations and changes
   in the polices of Germanization and Russification. Taken together, the
   two myths to a large degree obscure (and even falsify) the past, making
   comprehension of it difficult, if not impossible. This article seeks to
   disentangle the knots of anachronisms that underlie the Polish national
   master narrative, in order to present a clearer picture of the interplay
   between the policies of Germanization, Polonization, and Russification
   as they unfolded in the lands of the partitioned Poland-Lithuania during
   the long nineteenth century.},
