@article{ WOS:000215070200005,
Author = {Hassani, Hassan},
Title = {Pre-freedom},
Journal = {JOURNAL OF POLITICS AND LAW},
Year = {2014},
Volume = {7},
Number = {3},
Pages = {41-46},
Month = {SEP},
Abstract = {Although some concepts like negative and positive freedom are certainly
   considerable in the area of freedom; this paper argues to highlight what
   the researcher has called `pre-freedom'. For this purpose, it tries to
   clarify the concept of pre-freedom as a condition in which a person is
   related to other ones by primary ties. It is observed under the light of
   the fact that s/he does not conceive itself as a person who is being
   instructed his/her own social role. In other words, pre-freedom is the
   condition in which a person is not able to choose. It only imitates its
   leader and practice sheepish obedience of authorities. This person can,
   thus, be employed and exploited through three major techniques, namely
   the dependent identity, the doll of authorities, and the fragile
   security that the researcher attempts to explain them respectively based
   on various German thinkers such as Erich Fromm, Hannah Arendt, and
   Herbert Marcuse.},
