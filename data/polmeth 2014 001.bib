@article{ WOS:000211579500004,
Author = {Poe, Danielle},
Title = {Responding to violence and injustice using nonviolence: Martin Luther
   King Jr., Leymah Gbowee, and Dorothy Stang},
Journal = {GLOBAL CHANGE PEACE \& SECURITY},
Year = {2014},
Volume = {26},
Number = {2},
Pages = {177-193},
Abstract = {This paper addresses the idea that in order to effectively address human
   rights violations, military intervention is necessary. It advocates the
   perspective that nonviolent direct action is an effective way to address
   human rights abuses. By analyzing the work of Martin Luther King, Jr. in
   the US during the 1960s, Leymah Gbowee in Liberia in the 1990s, and
   Dorothy Stang in Brazil through 2005, the article demonstrates that
   their actions required great courage and imagination and that nonviolent
   activism is effective in a variety of historical contexts and global
   contexts.},
