@article{ WOS:000218228600006,
Author = {Boeke, Sergei and Tisseron, Antonin},
Title = {MALI'S LONG ROAD AHEAD},
Journal = {RUSI JOURNAL},
Year = {2014},
Volume = {159},
Number = {5},
Pages = {32-40},
Abstract = {Operation Serval, the eighteen-month mission mounted by France to oust
   jihadists from northern Mali, came to an end in August 2014. However,
   despite the transition into a broader, region-wide operation to counter
   terrorism, both UN and French troops remain focused on securing Mali and
   targeting jihadists still operating within the country's borders. Sergei
   Boeke and Antonin Tisseron warn against the temptation to treat the
   jihadists as a homogenous group with common goals and modi operandi,
   instead highlighting the multiple fractures within the myriad, smaller
   factions that must be addressed if stability is to be restored to Mali.},
