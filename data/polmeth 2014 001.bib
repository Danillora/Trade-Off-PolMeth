@article{ WOS:000337751700003,
Author = {Williams, Laron K. and Brule, David J.},
Title = {Predictably Unpredictable: The Effects of Conflict Involvement on the
   Error Variance of Vote Models},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2014},
Volume = {44},
Number = {2},
Pages = {287-299},
Month = {APR},
Abstract = {International conflict has profoundly influenced election outcomes in
   some cases, and in other cases has had a minimal impact. This article
   develops a theory that the increased salience of foreign policy issues
   following periods of international hostilities increases the variance of
   government parties' vote shares. In elections following conflict, the
   ability to accurately predict election outcomes using traditional
   economic voting models is reduced. The article provides evidence from
   advanced democracies in the post-WorldWar II era that being involved in
   international disputes increases the predictive error of vote shares.
   More substantively, vote choice models should model the role of
   exogenous shocks such as international conflict in order to avoid making
   misleading inferences. The study concludes by discussing the meaningful
   implications for various theories of voting behavior and international
   conflict.},
Publisher = {CAMBRIDGE UNIV PRESS},
