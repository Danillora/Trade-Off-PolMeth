@article{ WOS:000439071200011,
Author = {Modrzejewski, Arkadiusz},
Title = {The New Empire of Lies? Essay on Lack of Truth in International
   Relations},
Journal = {EUROPEAN JOURNAL OF TRANSFORMATION STUDIES},
Year = {2014},
Volume = {2},
Number = {1},
Pages = {111-121},
Abstract = {The following paper is a quite freely written essay from a normativistic
   position. Its aim is not to present the results of academic research but
   to present the opinions on threats resulting from the departure from the
   principle of truth in international relations. Current international
   situation, especially the conflict in Ukraine, is a good example. The
   author believes that establishing real and permanent international peace
   may be realised only when the principle of truth finds its due place in
   international relations. In present-day world there is a dominating
   mentality resulting from so-called realistic thinking, which in fact is
   the source of political cynicism. Universal truth has yielded to
   particular reasons and interests.},
