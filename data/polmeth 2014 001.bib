@article{ WOS:000210675000004,
Author = {Finnegan, Cara and Mixon, Anita},
Title = {Art Controversy in the Obama White House: Performing Tensions of Race in
   the Visual Politics of the Presidency},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2014},
Volume = {44},
Number = {2, SI},
Pages = {244-266},
Month = {JUN},
Abstract = {In 2009, two works by African American artists selected for display in
   the White House produced controversy among critics of the president. Our
   analysis explores how media discussion involving a Charles Alston bust
   of Dr. Martin Luther King, Jr., and an abstract painting by Alma Thomas
   unearthed cultural tensions involving the practice of imitation, the
   value of presence, and the role of Black art and artists. Such tensions
   shaped the debate about the art works chosen by the Obamas and raised
   the question of how to define and place a Black president in the first
   year of his first term.},
