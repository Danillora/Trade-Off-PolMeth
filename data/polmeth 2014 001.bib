@article{ WOS:000331294000004,
Author = {Holbrook, Thomas M. and Weinschenk, Aaron C.},
Title = {Campaigns, Mobilization, and Turnout in Mayoral Elections},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2014},
Volume = {67},
Number = {1},
Pages = {42-55},
Month = {MAR},
Abstract = {Research on local turnout has focused on institutions, with little
   attention devoted to examining the impact of campaigns. Using an
   original data set containing information from 144 large U.S. cities and
   340 separate mayoral elections over time, our contributions to the
   scholarship in this field are manifold: we focus the literature more
   squarely on the impact of campaigns by examining the role of campaign
