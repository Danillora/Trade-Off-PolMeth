@article{ WOS:000210678300008,
Author = {Brewer, Paul and Hoffman, Lindsay and Harrington, Ruby and Jones, Philip
   and Lambe, Jennifer},
Title = {Polls and Elections Public Perceptions Regarding the Authenticity
   of the 2012 Presidential Candidates},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2014},
Volume = {44},
Number = {4},
Pages = {742-757},
Month = {DEC},
Abstract = {Public perceptions of candidates' personality traits play important
   roles in shaping vote choice. Previous accounts point to authenticity as
   one key trait, but little research has systematically investigated
   perceptions regarding candidate authenticity. This study uses data from
   a telephone survey to show that political predispositions (trust,
   external efficacy, interest, partisanship, and ideology), and television
   news use (broadcast and cable) predicted perceptions of candidate
   authenticity in the context of the 2012 presidential campaign. A
   question-wording experiment also showed that perceptions regarding the
   authenticity of political messages varied across source (Obama or
   Romney), substance (working for ``the middle class{''}or ``job
   creators{''}), and the receiver's partisanship},
