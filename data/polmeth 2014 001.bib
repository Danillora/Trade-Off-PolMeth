@article{ WOS:000420556900003,
Author = {Schell, Toni Sebastian},
Title = {Attitudes towards the EU in times of crisis: a comparison of the
   determinants of two dimensions of attitudes before and at the start of
   the euro crisis},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2014},
Volume = {8},
Number = {2},
Pages = {35-54},
Month = {NOV},
Abstract = {This article analyses two dimensions of attitudes towards the EU: an
   instrumental and a strengthening dimension. The focus is mainly on the
   economic attitudes and developments before and at the start of the euro
   crisis, and how these attitudes and developments affect both dimensions.
   The results are based primarily on multilevel models, using
   Eurobarometer data and country-specific context factors. Contrary to
   expectations, results show no increasing influence of economic factors.
   Only net contribution to the EU budget proves to be a significant
   predictor of instrumental attitudes towards the EU.},
