@article{ WOS:000348688100009,
Author = {Reisinger, Heidi and Gol'c, Aleksandr},
Title = {Hybrid War in Ukraine Russia's Intervention and the Lessons for the NATO},
Journal = {OSTEUROPA},
Year = {2014},
Volume = {64},
Number = {9-10},
Pages = {119+},
Month = {SEP-OCT},
Abstract = {Russia's annexation of the Crimea and the war in eastern Ukraine feed on
   the perception held by the political elite around President Vladimir
   Putin that democratization in Ukraine also threatens the authoritarian,
   order in Moscow. The hybrid war with which Russia answered is nothing
   new. It involves an effective mix of military and non-military means,
   conventional and irregular methods including cyber-attacks and
   propaganda. For a multinational organization such as NATO, which is
   based on the principle of consensus, a quick and appropriate response is
   difficult.},
