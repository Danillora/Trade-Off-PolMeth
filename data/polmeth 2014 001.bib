@article{ WOS:000218232800012,
Author = {Kronenbitter, Guenther},
Title = {THE AUSTRO-HUNGARIAN EXPERIENCE OF COALITION WARFARE, 1914-18},
Journal = {RUSI JOURNAL},
Year = {2014},
Volume = {159},
Number = {4},
Pages = {76-82},
Abstract = {While the Anglo-French experience of coalition warfare during the First
   World War has been the subject of many English-language volumes,
   Austria-Hungary's relationship with Germany - its senior partner within
   the Triple Alliance - has been underexplored. In this article, Gunther
   Kronenbitter analyses the uneasy dynamics of this alliance, tracing it
   through the two countries' wilfully blinkered, thirty-year preparations
   for a war that eventually came in August 1914, before exploring their
   increasingly fraught and inadequate efforts to co-ordinate their
   campaigns and resources - a process marked by resentment and, more
   importantly, a failure to take a unified strategic approach.},
