@article{ WOS:000342823900003,
Author = {Hyvonen, Ari-Elmeri},
Title = {Tentative Lessons of Experience: Arendt, Essayism, and ``The Social{''}
   Reconsidered},
Journal = {POLITICAL THEORY},
Year = {2014},
Volume = {42},
Number = {5},
Pages = {569-589},
Month = {OCT},
Abstract = {The article addresses the role of the essay in Hannah Arendt's
   theorizing. By paying attention to Arendt's style, we are better able to
   draw the full conclusions from the well-known fact that she was not a
   system-builder but took her bearings from concrete experiences. First,
   the concept of experiencetoo often taken at face valueis explicated. It
   is argued that Arendt's understanding of the concept cannot be reduced
   to her personal experiences, but must be read against the background of
   the common world. A crucial dimension in the incorporation of
   experiences into the Arendtian political theory is the essayistic style
   of her writings. The essay, as an experimental and tentative platform,
   provides an indispensable angle to Arendt's workeven though often
   overshadowed by classical or narrative readings. This will be
   demonstrated in relation to the puzzlement surrounding the controversial
   concept of the social.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
