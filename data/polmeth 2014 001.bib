@article{ WOS:000435234000003,
Author = {Weinberg, Leonard and Martin, Susanne},
Title = {Coming Together or Growing Apart? Western Europe and the United States},
Journal = {DEMOCRACY \& SECURITY},
Year = {2014},
Volume = {10},
Number = {2},
Pages = {156-173},
Abstract = {Observers on both sides of the Atlantic believe that the United States
   and the major countries of Western Europe are growing apart in important
   ways. The view adopted here is quite the opposite. Instead, the
   assertion here is that the New World is coming to resemble the Old
   World, both in regard to the behavior of their fragmented political
   elites and the resentment-laden attitudes of their mass publics. What
   was once labeled ``American exceptionalism{''} by prominent analysts was
   largely a product of the peculiar conditions of the postwar era
   (1945-1965), conditions that no longer depict current realities.},
