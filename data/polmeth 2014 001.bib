@article{ WOS:000213904400002,
Author = {Ives-Allison, Nicole D.},
Title = {Visual rehumanisation: torture and terror in In the Name of the
   Father and Fifty Dead Men Walking},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2014},
Volume = {7},
Number = {2},
Pages = {205-222},
Abstract = {This article explores the use of torture by both state and non-state
   actors during the Northern Irish Troubles as visualised in two films,
   Jim Sheridan's In the Name of the Father (1998) and Kari Skogland's
   Fifty Dead Men Walking (2009). Using the concept of
   ``acknowledgement{''} as developed by Stanley Cohen, this article argues
   that the visualisation of torture not only draws the issue out from the
   shadows, but challenges the dehumanisation process which can facilitate
   passive acceptance of the use of torture where there exists a threat to
   the integrity of the political and moral community.},
