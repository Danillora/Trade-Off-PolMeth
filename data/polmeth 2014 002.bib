@article{ WOS:000338005700008,
Author = {Dalton, Russell J.},
Title = {Interpreting Partisan Dealignment in Germany},
Journal = {GERMAN POLITICS},
Year = {2014},
Volume = {23},
Number = {1-2},
Pages = {134-144},
Abstract = {Partisan ties in Germany have been weakening over the past three
   decades, which is changing the landscape of electoral politics. In
   contrast to a recent article by Dassonneville et al. in this journal,
   this article argues that a generational decline in partisanship is
   contributing to this dealignment trend, and virtually all of the new
   independents are more sophisticated apartisans who are politically
   engaged even though they lack party ties. These findings are based on
   the 1972-2009 time series of surveys by the Forschungsgruppe Wahlen. The
   results point to a more sophisticated German electorate that will inject
   more fluidity into electoral politics and empower more Germans to make
   reasonable electoral choices.},
