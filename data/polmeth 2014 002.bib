@article{ WOS:000420267900002,
Author = {Schmiedecke, Natalia Ayo},
Title = {The political commitment of the Chilean New Song Movement and the
   musical construction of a historical becoming during the Popular Unity},
Journal = {IZQUIERDAS},
Year = {2014},
Volume = {20},
Pages = {27-51},
Month = {SEP},
Abstract = {This paper aims to capture the dynamics of the tumultuous and hopeful
   years of the Popular Unity government through a bias still little
   explored by historiography: the relations between revolutionary utopia
   and popular music. Focusing on the ideological foundations that
   supported the revolutionary perspective in the speech of musicians
   linked to the Chilean New Song movement, we will analyze the way in
   which the temporal experience was experienced and represented in their
   records, relating it to the political project of the Chilean way to
   socialism.},
