@article{ WOS:000347140800004,
Author = {Geay, Kevin},
Title = {``Our Lord first served{''}. An Ethnography of a Traditionalist
   Catholics Moral Crusade},
Journal = {POLITIX},
Year = {2014},
Volume = {27},
Number = {106},
Pages = {59+},
Abstract = {Traditionalist activists of Civitas Institute are talked about but
   remain sociologically unknown. Where do they come from, why do they
   express their radical involvment in that organization? How are they
   attracted by a kind of commitment that may marginalize them from young
   adults' social groups? The participant observation survey introduced
   here describes the early enrolment of traditionalists in the blue thread
   of counter-revolutionary memory. Yet young activists' conformity to such
   a dictated role has a cost for Civitas. The traditionalist institute is
   strained under the wish to widen its audience beyond its historical
   breeding ground and its inability to compromise with the esoteric
   practices that specify and unify it.},
