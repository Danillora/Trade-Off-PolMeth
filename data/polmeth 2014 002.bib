@article{ WOS:000212000100001,
Author = {Lee, Jerry},
Title = {Legacies of Japanese colonialism in the rhetorical constitution of South
   Korean national identity},
Journal = {NATIONAL IDENTITIES},
Year = {2014},
Volume = {16},
Number = {1},
Pages = {1-13},
Abstract = {Despite Korea's independence from Japan in 1945, collective memories of
   Japanese colonialism continue to shape South Korean national identity
   today. This article extends Charland's theory of constitutive rhetoric
   and reconsiders Billig's theory of banal nationalism to analyse key
   discourses of national memory in South Korea, including presidential
   speeches delivered on the Liberation Day of Korea (Gwang-bokjeol) and
   exhibits at the Independence Hall (Dongnip Ginyeomgwan). This article
   examines how South Korean nationalistic discourses today, by fostering a
   sustained antagonism towards the Japanese, constitute a national
   identity in an inextricable relationship to the Japanese Other.},
