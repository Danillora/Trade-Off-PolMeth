@article{ WOS:000218542700007,
Author = {Hill, William H.},
Title = {The OSCE and the Moldova-Transdniestria Conflict: Lessons in Mediation
   and Conflict Management},
Journal = {SECURITY AND HUMAN RIGHTS},
Year = {2014},
Volume = {24},
Number = {3-4},
Pages = {287-297},
Abstract = {The OSCE has been involved in attempts to resolve the Transdniestrian
   conflict since 1992, even before the conference became a formal
   organization. As is the case with other conflicts, the OSCE has often
   been criticized in Moldova for failure to resolve the conflict. Such
   criticism, however, often glosses over the positive impact OSCE missions
   have had on mediation processes, the Moldova-Transdniestria conflict in
