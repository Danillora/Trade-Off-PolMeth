@article{ WOS:000420558800007,
Author = {Wenzelburger, Georg and Zohlnhoefer, Reimut},
Title = {Welfare state change, replacement rates and the problem of validity: a
   cautionary note},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2014},
Volume = {8},
Number = {3-4},
Pages = {307-328},
Month = {DEC},
Abstract = {Analyzing welfare state changes based on standardized and individualized
   welfare entitlements data is fashionable. By using these data, scholars
   hope to avoid many of the issues previous macro-quantitative studies
   were faced with-especially the fact that cuts in welfare state
   generosity did not necessarily reduce social spending. However, the
   article casts some doubt on this optimistic expectation. Based on a
   comparison between the two major data sets in the field-Scruggs' CWED (1
   and 2) and Korpi/Palme's SCIP as well as the most recent statements of
   the compilers of these data sets, we draw three important conclusions
   for the field of comparative welfare state research. First, the data
   sets treat tax clawbacks differently, they vary in the extent they take
   federal lower-level social benefits into account and handle means-tested
   programs in a different manner, all of which leads to validity issues.
   Second, based on the recent debate in the scientific community, we
   discuss in how far there are conceptual differences between the data
   sets that may be able to explain these differences. Third, we include
   the latest, updated version of Scruggs' data set (CWED 2) that has been
   published only very recently and show that some of the errors in the
   original version have been corrected.},
