@article{ WOS:000218598500001,
Author = {Walsh, Shannon Drysdale and Xydias, Christina},
Title = {Women's organizing and intersectional policy-making in comparative
   perspective: evidence from Guatemala and Germany},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2014},
Volume = {2},
Number = {4},
Pages = {549-572},
Abstract = {Despite worldwide increases in the proportion of women in legislatures,
   advances in levels of democracy, and rising levels of economic and human
   development, the preferences and priorities of the most marginalized
   women are still typically neglected in public policy. We identify a type
   of substantive representation that we call intersectional policy-making,
   which addresses the specific sources of disadvantage that deter the
   social mobility and even fundamental well-being of marginalized
   subgroups of women. We conduct a cross-national paired comparison of
   Guatemala and Germany as contrasting cases with unexpected outcomes.
   Intersectional policies emerged in Guatemala but were limited and even
   misguided in Germany. We find that typical explanations for variation in
   the emergence of policies benefiting women as a larger group do not
   explain variation in intersectional policies. Instead, we advance the
   novel argument that civil society organizations composed of marginalized
   subgroups of women must engage in grassroots advocacy as well as work in
   coordination with key allies within the state to advance intersectional
   policies. Among other things, this coordination facilitates the
   placement of marginalized women's preferences and priorities on the
   policy agenda.},
