@article{ WOS:000217972400007,
Author = {Ferras, Graciela},
Title = {Hipolito Yrigoyen, Apostle of the Nation},
Journal = {ANACRONISMO E IRRUPCION},
Year = {2014},
Volume = {4},
Number = {7},
Pages = {127-148},
Month = {NOV},
Abstract = {The purpose of this article is to dwell on the relationship between
   politics and religion, in the dawn of Argentine democracy. Hence, it
   focuses on the study of the language of religion and the mystical
   symbolism found in the construction of Hipolito Yrigoyen's political
   leadership, and in the conception of a political party as a civic
   religion. Though the Radical Party was originally founded on impersonal
   principles - according to its Charter - Hipolito Yrigoyen conceived it
   as a movement, as the Nation itself, rather than as a political party.
   This idea was linked to the conception of political activity as
   apostolate. Yrigoyen, thus, arises as a restoration hero sent by
   Providence.},
