@article{ WOS:000212754100007,
Author = {Yli-Kaitala, Kirsi},
Title = {Revolution 2.0 in Egypt: Pushing for Change, Foreign Influences on a
   Popular Revolt},
Journal = {JOURNAL OF POLITICAL MARKETING},
Year = {2014},
Volume = {13},
Number = {1-2, SI},
Pages = {127-151},
Abstract = {This article presents a case study of a movement for democratic change
   in Egypt. In particular, it reviews the uses of social media that helped
   to facilitate the revolution that led to the overthrow of Hosni
   Mubarak's regime in February 2011. It looks at the movement in Egypt and
   the uses of social media from the perspective of public diplomacy,
   particularly in the context of U.S. efforts to drive public opinion in
   the Middle East. It reviews how the groups and individuals considered
   instrumental in the events that led to Mubarak's resignation are
   connected to the actors in U.S. ``democracy promotion.{''} The article
   further discusses how social media, by dint of its characteristics,
   conveniently lends itself to democracy promotion. It highlights the
   risks involved in an instrumental approach to democracy promotion that
   treats the tools of social media as means to effect immediate policy
   gains country by country.},
