@article{ WOS:000217598800002,
Author = {Lanza, Andrea},
Title = {Fraternite and solidarite in 1848. Traces of a
   Sociological Approach},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
Year = {2014},
Volume = {26},
Number = {51},
Pages = {17-39},
Abstract = {The article focuses on the social-lexical history of the terms
   fraternite and solidarite in France during the first half of the 19th
   century. In particular, the author highlights how, in 1848, fratenite is
   both used in the motto of the Republic and associated with the defeated
   socialist republicanism. Then, the author focuses on its synonymous,
   solidarite, and he reconstructs the different contexts of use of this
   term (fiscal, legal, religious, economic and political contexts), in
   order to show that its use in the social context accompanies the
   formation of the first sociological approach with a
   republican-saint-simonian matrix.},
