@article{ WOS:000219905100006,
Author = {Borrego Zabala, Bartolome},
Title = {The Necessary Adaptation of Taxation to The New Trends in E-Business},
Journal = {IDP-INTERNET LAW AND POLITICS},
Year = {2014},
Number = {18},
Pages = {51-59},
Month = {JUN},
Abstract = {The new models of digital business and the complexity of certain
   e-commerce transactions make the Internet both the ideal place for
   diverting profits to territories that impose less or no taxation and the
   means for, on occasions, committing tax evasion. This makes taxing this
   type of transactions and fraud control by tax authorities extremely
   difficult. The rapid evolution of the new technologies frequently
   catches governments out, which can get left behind with regard to
   complex transactions that take place on a daily basis in the virtual
   world and require the continual amending and adapting of regulations to
   adjust to the new reality brought about by the digital economy.},
