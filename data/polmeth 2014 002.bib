@article{ WOS:000421255900003,
Author = {Breedlove, Philip M.},
Title = {WHAT IS NEXT FOR NATO?},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2014},
Volume = {12},
Number = {4},
Pages = {25-32},
Month = {WIN},
Abstract = {Throughout the last 65 years of NATO history, the focus of the Alliance
   has evolved and adapted to its geopolitical environment; this is no
   different today. NATO's strength and capabilities are inherently based
   on forming, sustaining, equipping, and training an adaptive force where
   the only constant in the geopolitical terrain is change. The security
   climate in locations such as the Middle East, Afghanistan, Africa, and
   the Balkans continues to shift and evolve around us, demanding our
   constant vigilance. The Alliance was formed because the whole was
   greater than the sum of our parts. The consistent factor is the enhanced
   security each nation receives from being a member of the Alliance.
   Turkey has been a stalwart ally, and there is no question of the
   Alliance's commitment to the security of Turkey.},
