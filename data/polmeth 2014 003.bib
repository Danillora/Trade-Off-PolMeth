@article{ WOS:000436663000003,
Author = {Yu Hong},
Title = {China's ``Ghost Cities{''}},
Journal = {EAST ASIAN POLICY},
Year = {2014},
Volume = {6},
Number = {2},
Pages = {33-43},
Month = {APR-JUN},
Abstract = {Some Chinese cities are now notoriously known as ``ghost cities{''} for
   their empty skyscrapers, luxury apartments and magnificent squares.
   These ``ghost cities{''} are a result of excessive housing and
   infrastructure construction, and speculation on property demand. The
   emergence of ghost cities is a consequence of China's land-centred
   urbanisation. Land-centred urbanisation is rooted in local GDPism, which
   in turn results in aggressive developmentalism and excessive investment
   among China's local governments.},
