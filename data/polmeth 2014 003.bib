@article{ WOS:000421258000005,
Author = {Bleeker, Mo},
Title = {THE CONTRIBUTION OF HISTORICAL COMMISSIONS TO DEMOCRACY AND DIVERSITY},
Journal = {TURKISH POLICY QUARTERLY},
