@article{ WOS:000344658900005,
Author = {Ibenskas, Raimondas},
Title = {Activists or money? Explaining the electoral success and persistence of
   political parties in Lithuania},
Journal = {PARTY POLITICS},
Year = {2014},
Volume = {20},
Number = {6},
Pages = {879-889},
Month = {NOV},
Abstract = {The literature on post-communist party politics frequently suggests that
   membership organizations are not important for political parties in the
   region, as elections can be won with expensive media-based campaigns.
   The article tests this argument using the parliamentary elections in
   Lithuania in the 2000s held under a mixed electoral system. The results
   suggest that both party membership organization and campaign spending
   have roughly equal effects on the electoral persistence of political
   parties. New parties also benefit from both strong membership
   organizations and high campaign spending.},
