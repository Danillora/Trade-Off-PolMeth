@article{ WOS:000340994900015,
Author = {Lefevere, Jonas and Van Aelst, Peter},
Title = {First-order, second-order or third-rate? A comparison of turnout in
   European, local and national elections in the Netherlands},
Journal = {ELECTORAL STUDIES},
Year = {2014},
Volume = {35},
Pages = {159-170},
Month = {SEP},
Abstract = {Second-order elections are characterized by low turnout. According to
   the second-order theory this is because people feel there is less at
   stake. This study tests whether the less at stake argument holds at the
   macro and micro level using panel survey data obtained in three
   different Dutch elections. Furthermore, it examines whether campaigns'
   mobilizing potential differs between first- and second-order elections.
   We find that at the macro level perceived stakes and low turnout go hand
   in hand and differ strongly between national, local and European
   elections. At the micro level the impact of perceived stakes on turnout
   is limited and contingent on the type of election. Also, campaign
   exposure affects turnout, but the effect is substantially larger in
   second-order contests. (C) 2014 Elsevier Ltd. All rights reserved.},
Publisher = {ELSEVIER SCI LTD},
