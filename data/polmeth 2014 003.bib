@article{ WOS:000336341500004,
Author = {Alzate, Elissa B.},
Title = {From Individual to Citizen: Enhancing the Bonds of Citizenship Through
   Religion in Locke's Political Theory},
Journal = {POLITY},
Year = {2014},
Volume = {46},
Number = {2},
Pages = {211-232},
Month = {APR},
Abstract = {This paper examines how Locke employed the notion of religion to answer
   two questions: What binds citizens together into a community? How are
   individuals transformed into citizens? In the Two Treatises, Locke
   creates, through a political contract, a community that proves fragile.
   The bonds necessary bra civil society are unnatural; they go against the
   very self-interest that gave rise to a civil society. According to
   Locke, for a political community to simultaneously appeal to and
   overcome self-interest, the state must provide an institutional place
   for religion. To be sure, Locke defends extensive liberty of conscience
   in A Letter Concerning Toleration. He nonetheless retains a clear role
   for religion within the state and, furthermore, requires that all
   citizens in political society belong to some religious society. Locke's
   carefully designed argument merges community interest with
   self-interest, while being built on individual consent.},
Publisher = {PALGRAVE MACMILLAN LTD},
Address = {BRUNEL RD BLDG, HOUNDMILLS, BASINGSTOKE RG21 6XS, HANTS, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Alzate, EB (Corresponding Author), Washington State Univ, Dept Polit Sci \& Publ Adm, Pullman, WA 99164 USA.
   Washington State Univ, Dept Polit Sci \& Publ Adm, Pullman, WA 99164 USA.},
DOI = {10.1057/pol.2014.2},
ISSN = {0032-3497},
EISSN = {1744-1684},
