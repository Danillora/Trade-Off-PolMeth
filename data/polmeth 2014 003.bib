@article{ WOS:000217332200002,
Author = {Beaudonnet, Laurie and Blais, Andre and Bol, Damien and Foucault,
   Martial},
Title = {The impact of election outcomes on satisfaction with democracy under a
   two-round system},
Journal = {FRENCH POLITICS},
Year = {2014},
Volume = {12},
Number = {1},
Pages = {22-35},
Month = {APR},
Abstract = {Previous research has found a positive relationship between having voted
   for a party that is part of the government and satisfaction with
   democracy. However, no research has examined this relationship in the
   specific case of a two-round system. Relying on original panel data
   survey conducted before and after the 2012 legislative election in
   France, this article addresses the question of how vote choices in the
   first and second rounds affect satisfaction with democracy. We find that
   both rounds have a similar impact and that voters who rallied a winning
   party in the second round are as happy with the democratic process as
   early supporters.},
