@article{ WOS:000330817500021,
Author = {Sjoberg, Fredrik M.},
Title = {Autocratic adaptation: The strategic use of transparency and the
   persistence of election fraud},
Journal = {ELECTORAL STUDIES},
Year = {2014},
Volume = {33},
Pages = {233-245},
Month = {MAR},
Abstract = {Why would an autocrat want, or at least make it appear to want, to
   reduce election fraud? In recent years, non-democratic rulers have
   surprisingly begun to embrace fraud-reducing technologies, like web
   cameras or transparent ballot boxes. The reason for this is found in the
   relative ease by which one type of fraud can be replaced with another.
   With the help of new fraud identification techniques, I argue that the
   installation of web cameras in polling stations changes how fraud is
   conducted. Web cameras do not reduce fraud, but rather make certain
   blatant forms of fraud, like ballot box stuffing, more costly. Autocrats
   then substitute for other types of fraud, such as fabricating the vote
   count out of view of the cameras. (C) 2013 Elsevier Ltd. All rights
   reserved.},
