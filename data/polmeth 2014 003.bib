@article{ WOS:000217031000007,
Author = {Cristobo, Matias},
Title = {?Vladimir Castoriadis vs. Rosa Lefort? On the persistence of a debate
   about the relationship between the party and the class},
Journal = {REFLEXION POLITICA},
Year = {2014},
Volume = {16},
Number = {32},
Pages = {74-84},
Month = {DEC},
Abstract = {After the Second Congress of the Russian Social Democratic Labour Party
   (RSDLP), which took place in mid-1903, a dispute between Vladimir Lenin
   and Rosa Luxemburg on the conformation to be adopted by the Party
   developed. Fifty years later, the above controversy is revived within
   the Socialism or Barbarism group by Cornelius Castoriadis and Claude
   Lefort. Castoriadis, echoing the position of Lenin; Lefort, being more
   close to Luxembourg. We aim to reconstruct both debates with a dual
   purpose. First, approach the first of them to determine their center
   points. Then look at how the proposals of Cornelius Castoriadis and
   Claude Lefort can be recognized in the positions of Vladimir Lenin and
   Rosa Luxemburg. Finally, we tested a short conclusion.},
