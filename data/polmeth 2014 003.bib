@article{ WOS:000332921000004,
Author = {Garcia Sanchez, Miguel},
Title = {Illicit Crops and Institutional Trust in Colombia},
Journal = {POLITICA Y GOBIERNO},
Year = {2014},
Volume = {21},
Number = {1},
Pages = {95-126},
Abstract = {This paper studies, for the Colombian case, the relationship between
   illicit crops, coca crops eradication strategies and citizens'
   perceptions of trust on the local executive and the police. Using survey
   data from the America's Barometer and contextual level data on coca
   crops and eradication strategies it was possible to conclude: a higher
   local incidence of illicit drugs trafficking has a negative relationship
   with citizens' perceptions of trust on the local governments. These
   relationships are not homogeneous as they are conditioned by individual
   level variables such as evaluations of the economy and the provision of
   public goods.},
