@article{ WOS:000346954800004,
Author = {Toro Maureira, Sergio and Jaramillo-Brun, Nathalie},
Title = {Indigenous Voting in Chile: Ideological Preference and Ethnic Support in
   the Mapuche Electorate},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2014},
Volume = {34},
Number = {3},
Pages = {583-604},
Abstract = {This article seeks to explore the political preferences of indigenous
   peoples in Chile. Specifically, it attempts to challenge conventional
   wisdom, which holds that the Mapuche are largely right-wing oriented,
   and that they also bear a low adherence level to their own ethnicity. In
   light of the results of 2950 polls in presidential and parliamentary
   elections in La Araucania we argue that there is no significant
   correlation between Mapuche and right-wing voting. In contrast, there is
   a positive and significant correlation between Indigenous votes and
   Indigenous candidates.},
