@article{ WOS:000337187500006,
Author = {Schermann, Katrin and Ennser-Jedenastik, Laurenz},
Title = {Coalition Policy-Making under Constraints: Examining the Role of
   Preferences and Institutions},
Journal = {WEST EUROPEAN POLITICS},
Year = {2014},
Volume = {37},
Number = {3},
Pages = {564-583},
Month = {MAY},
Abstract = {While much has been written about the formation and termination of
   coalitions, comparatively little attention has been paid to the policy
   output of multiparty governments. The present study attempts to narrow
   this research gap by analysing policy-making in three Austrian coalition
   governments between 1999 and 2008. Drawing on the party mandate
   literature, a manually coded textual analysis of election manifestos is
   conducted that yields a dataset containing over 1,100 pledges. The
   fulfilment of these pledges is taken as the dependent variable in a
   multivariate analysis. The results indicate that institutional
   determinants (adoption in the coalition agreement, ministerial control,
   and policy status quo) significantly influence the chances of pledge
   fulfilment and thus present a powerful predictor of coalition policy
   output. By contrast, factors related to parties' preferences (consensus
   between parties, policy distance, pledge saliency, and majority support
   in parliament) do not have an impact.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
