@article{ WOS:000213011100005,
Author = {Smyth, Regina},
Title = {The Putin Factor: Personalism, Protest, and Regime Stability in Russia},
Journal = {POLITICS \& POLICY},
Year = {2014},
Volume = {42},
Number = {4},
Pages = {567-592},
Month = {AUG},
Abstract = {Vladimir Putin remains central to regime stability in the Russian
   Federation. However, the role that Mr. Putin's personalist appealrooted
   in both charismatic and noncharismatic linkages-plays in maintaining
   regime support is undertheorized. I argue that personalism is a powerful
   political resource in electoral authoritarian regimes because it
   provides a positive logic for skeptical voters to support the leader.
   Personalist linkages obscure the role that electoral bias plays in
   shaping electoral outcomes, diminishing the impulse for mass
   postelection protest. When the effectiveness of personalism declines,
   discontented citizens are more likely to protest biased elections. This
   article shows that Mr. Putin's appeal as a sound steward of the economy
   declined by 2012 but that other sources of issue satisfaction continued
   to shape some respondents' trust in the president.},
