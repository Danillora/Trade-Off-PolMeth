@article{ WOS:000210653800003,
Author = {Roberts, Bruce},
Title = {Productive/Unproductive: Conceptual Topology},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2014},
Volume = {26},
Number = {3},
Pages = {336-359},
Abstract = {Starting from explicit materialist premises, this paper develops a
   general conceptualization of productive (P) and unproductive (U)
   activities. This distinction is implicit in the concept of a social
   surplus derived (often) through exploitation and redistributed
   (typically) through social mechanisms that channel portions of the
   surplus to reproduce U activities. But the P/U distinction for labor is
   inherently multidimensional, so a ``topological{''} partitioning of the
   conceptual space is needed. Relevant ``axes{''} of distinction are (1)
   capitalist versus noncapitalist labors, (2) production versus non
   production labors, and (3) basic versus nonbasic labors. The meaning and
   relevance of each of these axes is explored, referring to the
   literature, and six conceptual viewpoints are distinguished. The
   theoretical and political implications of these different specifications
   are examined, with special focus on the status of the financial (FIRE)
   sector. It is argued that no single specification of the P/U distinction
   is adequate for considering all questions.},
