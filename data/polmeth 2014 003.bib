@article{ WOS:000215175800001,
Author = {Chereji, Christian-Radu},
Title = {IS SEPARATISM A VIABLE SOLUTION FOR THE PRESENT AFRICAN CIVIL WARS?},
Journal = {CONFLICT STUDIES QUARTERLY},
Year = {2014},
Number = {9},
Pages = {3-12},
Month = {OCT},
Abstract = {Separatism as a political movement is as old as humanity. In Africa,
   where state borders are the result of colonial powers' interests and
   strategies, a huge number of separatist movements have engulfed the
   continent in a long series of civil wars. This essay examines whether
   separatism in its most extreme form of secession has led to the
   establishment of more stable, peaceful, and prosperous countries on the
   continent, comparing the emergent new nations' political and economic
   achievements with those of the original countries they left.},
