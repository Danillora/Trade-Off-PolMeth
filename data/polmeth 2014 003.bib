@article{ WOS:000340277000003,
Author = {Krook, Mona Lena},
Title = {Electoral Gender Quotas: A Conceptual Analysis},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2014},
Volume = {47},
Number = {9},
Pages = {1268-1293},
Month = {AUG},
Abstract = {Electoral gender quotas have become the subject of a growing literature
   in comparative politics, with the potential to affect how scholars study
   a wide range of electoral and representative processes. Yet, debates
   have emerged over how to define and categorize these policies, with
   implications for the ability to compare cases and draw broader
   conclusions about their impact in countries around the globe. Reviewing
   these perspectives, this article draws on work on concept formation to
   propose a pragmatic approach, focused on matching typologies to research
   questions. It emphasizes the advantages of this solution for promoting
   cumulative research and concludes with some thoughts for future research
   on gender quotas and comparative politics.},
