@article{ WOS:000214606300001,
Author = {Lowery, David},
Title = {Introduction to the special issue of IG\&A on methodological issues of
   arge-N research on interest representation},
Journal = {INTEREST GROUPS \& ADVOCACY},
Year = {2014},
Volume = {3},
Number = {2, SI},
Pages = {123-125},
Month = {JUN},
Abstract = {The last decade has seen a remarkable transformation in the study of
   European national and EU interest representation toward mid-range
   theories tested with large-N research. One result of these developments
   is renewed anticipation about the emergence of a true comparative theory
   of interest representation, one fully attentive to the importance of
   institutional and issue context. Given this expansion of large-N
