@article{ WOS:000348688100011,
Author = {Klein, Margarete},
Title = {Barely Fit for Combat The Ukrainian Army},
Journal = {OSTEUROPA},
Year = {2014},
Volume = {64},
Number = {9-10},
Pages = {147+},
Month = {SEP-OCT},
Abstract = {Kiev's ``Anti-terrorist Operation{''} against the rebels in the
   country's east is putting the spotlight on the state of the Ukrainian
   security forces. The weaknesses exposed range from obsolete equipment
   and low readiness to inadequate coordination and a lack of democratic
   controls. In order to overcome these shortcomings, structural reforms
   are needed. They would involve budgeting issues (including
   anti-corruption efforts) and technological modernisation as well as
   institutional aspects. Given the extent of the problems and the current
   economic situation, Ukraine will hardly be able to handle these tasks
   without outside help.},
