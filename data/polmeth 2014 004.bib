@article{ WOS:000345834800005,
Author = {Gowder, Paul},
Title = {Market Unfreedom},
Journal = {CRITICAL REVIEW},
Year = {2014},
Volume = {26},
Number = {3-4, SI},
Pages = {306-347},
Month = {OCT 2},
Abstract = {John Tomasi's ``market democrat{''} is right to suppose that Rawlsians
   have erred in omitting economic liberty from their theories of justice.
   A Rawlsian ought to include economic liberty as a basic freedom because
   it facilitates individuals' development and pursuit of their conceptions
   of the good. However, the most plausible version of economic liberty
   will require the state to guarantee, if possible, that no one will be
   driven by economic desperation to engage in immiserating work, which may
   impair rather than facilitate his or her pursuit of the good. There is
   reason to worry that contemporary economies such as the United States
   fall seriously short of that ideal.},
