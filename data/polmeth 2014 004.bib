@article{ WOS:000214359400006,
Author = {Hamid, Abdul},
Title = {A Family Matter: Political Corruption in Banten, Indonesia},
Journal = {ASIAN POLITICS \& POLICY},
Year = {2014},
Volume = {6},
Number = {4, SI},
Pages = {577-593},
Month = {OCT},
Abstract = {This article discusses transformations in local politics in Indonesia
   after the Reformasi 1998, focusing on the situation in Banten province.
   It describes how one family, that of Chasan Sochib, has utilized strong
   kinship networks, exploited state resources, and captured the local
   bureaucracy to strengthen its political machinery and consolidate
   political power in the local arena. It shows the rise of family
   domination in local politics and over economic resources, especially
   when public policy is crafted to reflect the interests of the family
   rather than those of the people. It suggests that despite the rhetoric
   of good governance at the national level, local politics is an important
   locus to analyze the unequal distribution of power and resources in
   Indonesia during the last 14 years of efforts to democratize.},
