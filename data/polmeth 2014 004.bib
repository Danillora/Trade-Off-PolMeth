@article{ WOS:000330862900006,
Author = {Rich, Timothy S.},
Title = {Party Voting Cohesion in Mixed Member Legislative Systems: Evidence from
   Korea and Taiwan},
Journal = {LEGISLATIVE STUDIES QUARTERLY},
Year = {2014},
Volume = {39},
Number = {1},
Pages = {113-135},
Month = {FEB},
Abstract = {How do mixed-member legislative systems influence legislator voting?
   While the literature remains inconclusive, this article suggests party
   influence as an intervening variable. Through an analysis of roll-call
   data from Taiwan and Korea, no deviation is evident between district
   legislators and legislators elected by proportional representation.
   Further disaggregation of what it means to vote against one's party
   again finds little evidence of a tier distinction, while party variables
   remain significant. The findings are suggestive of a contamination
   effect between tiers, consistent with the influence of parties.},
