@article{ WOS:000210652900007,
Author = {Wells, Robert},
Title = {The Multitude: Ambivalence and Antagonism},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2014},
Volume = {26},
Number = {2},
Pages = {239-251},
Abstract = {Ambivalence and antagonism are constitutive of the multitude and its
   relations. Such attributes tend to complicate how the multitude is
   conceived and how it operates; these complications, in turn, often lead
   to inconsistencies and contradictions in terms of the multitude's
   signification and application. This article argues that the various
   complications that arise specifically with respect to ambivalence,
   antagonism, and the multitude-primarily in the works of the contemporary
   postautonomist thinkers Paolo Virno, Antonio Negri, and Michael
   Hardt-need to be reformulated, not removed. It also genealogically
   traces ambivalence and antagonism back to Spinoza's initial conceptions
   of the multitude.},
