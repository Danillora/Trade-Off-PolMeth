@article{ WOS:000214450600003,
Author = {Dagnino, Evelina and Teixeira, Ana Claudia Chaves},
Title = {The Participation of Civil Society in Lula's Government},
Journal = {JOURNAL OF POLITICS IN LATIN AMERICA},
Year = {2014},
Volume = {6},
Number = {3, SI},
Pages = {39-66},
Abstract = {This article discusses the participation of civil society during the
   governments of President Lula, particularly in institutional public
   spaces. The participation of civil society in decision-making processes,
   incorporated in the Brazilian Constitution of 1988, has been a central
   principle in the political project of the Partido dos Trabalhadores (PT)
   since its foundation in 1980. This paper examines the extent to which
   this principle has remained effective and has been actively implemented
   at the federal level since the PT came to power in 2002. It also
   analyzes the concrete results of implementing greater participation and
   the difficulties faced in doing so. In addition, it explores both the
   continuities and new developments that have emerged during the
   government of Lula's successor, Dilma Rousseff.},
Publisher = {GERMAN INST GLOBAL \& AREA STUDIES-GIGA, INST LATIN AMERICAN STUDIES},
