@article{ WOS:000434547800012,
Author = {Weiss, Oliver},
Title = {Economic surplus and capitalist diversity},
Journal = {CAPITAL AND CLASS},
Year = {2014},
Volume = {38},
Number = {1, SI},
Pages = {157-170},
Month = {FEB},
Abstract = {This paper develops a theoretical critique of the varieties of
   capitalism (VoC) approach from the perspective of ideas sourced from
   Marxian political economy. In particular, the concept of economic
   surplus as formulated by Paul Baran is used to question the social
   ontology implicit in VoC, which, it is argued, is severely constrained
   by its imprecise definitions of both capitalism itself and capitalist
   diversity as a specific phenomenon. The result of these two failures is
   that VoC's theoretical apparatus is unable to perceive the true
   significance of capitalist diversity, and is thus likewise incapable of
   telling us anything new about capitalism itself. In this way, key
   aspects of the institutionalist take on capitalist diversity are
   questioned, and an alternative analytical approach based on the labour
   theory of value is put forward.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Weiss, O (Corresponding Author), Univ Sussex, Brighton, E Sussex, England.
   Weiss, Oliver, Univ Sussex, Dept Int Relat, Brighton, E Sussex, England.},
DOI = {10.1177/0309816813514209},
ISSN = {0309-8168},
EISSN = {2041-0980},
Keywords = {Classical political economy; economic surplus; varieties of capitalism},
Research-Areas = {Government \& Law},
