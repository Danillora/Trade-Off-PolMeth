@article{ WOS:000421034000002,
Author = {Alcantara Saez, Manuel},
Title = {Neuropolitics: an approach to micropolitics},
Journal = {REVISTA ESPANOLA DE CIENCIA POLITICA-RECP},
Year = {2014},
Number = {35},
Pages = {31-55},
Month = {JUL},
Abstract = {This paper has a prospective basis on an issue that has been gaining
   importance in the last decade. It deals with the incidence of the
   advancement of neuroscience to understand issues that are unique to
   political science. The study of the relationship between the brain and
   the political thought and brain and political behavior stands as a
   challenge. The brain happens to constitute simultaneously dependent and
   independent variables. It varies depending on different aspect of public
