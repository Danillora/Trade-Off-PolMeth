@article{ WOS:000421251100004,
Author = {Cruz Villalon, Pedro},
Title = {Passive and active legitimacy Constitutional Courts},
Journal = {TEORIA Y REALIDAD CONSTITUCIONAL},
Year = {2014},
Volume = {33},
Pages = {141-150},
Abstract = {Like the ancient god Janus, legitimacy, a classical topic in political
   science, take a doublefaced appearance, when applied to contemporary
   Constitutional Courts. These judicial bodies are supposed to be
   ``legitimate{''} both in a passive and in an active direction, that is,
