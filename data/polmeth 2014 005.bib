@article{ WOS:000410486400009,
Author = {Carvalho, Bruno},
Title = {Nations and Difference: A Discussion about Diversity in Post-Colonialist
   Thought},
Journal = {REVISTA ESTUDOS POLITICOS},
Year = {2014},
Volume = {5},
Number = {2},
Pages = {522-540},
Month = {DEC},
Abstract = {The paper discusses the concepts of nation in post-colonial thinking,
   which emphasizes dimensions of discursive power and cultural difference.
   The aim is to examine, in particular, the consistency of new theoretical
   perceptions on the national question, comparing them to some theories
   that assume a sociocultural homogeneity. The analysis argues that
   postcolonial authors have virtues in observing significant aspects of
   cultural imaginaries and political practices, although their work also
   manifest some inaccuracies, particularly the assumption of a subject
   free of constraints and a dichotomous understanding of the state-society
   relationship.},
