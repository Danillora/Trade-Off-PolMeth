   Kansai Gaidai Univ, Coll Int Language \& Commun, Hirakata, Osaka 5730195, Japan.},
DOI = {10.1177/0951629813493211},
ISSN = {0951-6298},
EISSN = {1460-3667},
Keywords = {Mediation; game theory; international conflict},
Keywords-Plus = {INTERNATIONAL MEDIATION; CONFLICT-RESOLUTION; INFORMATION; PEACE; BIAS},
