@article{ WOS:000340185200008,
Author = {Freeden, Michael},
Title = {Progress and Progressivism: Thoughts on an Elusive Term},
Journal = {POLITICAL STUDIES REVIEW},
Year = {2014},
Volume = {12},
Number = {1},
Pages = {68-74},
Month = {JAN},
Abstract = {Michael Freeden responds to the debate in this issue on progressivism
   and its contested manifestations. He asks, first, is there a cluster of
   policies that collectively merits the designation `progressive'? Second,
   what are the rhetorical usages and power of the term and is it the case
   that those usages override or deflect its substantive content? And
   finally, are the multiple understandings of progressivism equally
   `legitimate' or culturally resonant, and with what effect does the term
   mutate among its various meanings?},
