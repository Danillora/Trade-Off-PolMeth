@article{ WOS:000336474700006,
Author = {Gannon, Philip},
Title = {The bridge that Blair built: David Cameron and the transatlantic
   relationship},
Journal = {BRITISH POLITICS},
Year = {2014},
Volume = {9},
Number = {2},
Pages = {210-229},
Month = {JUN},
Abstract = {This article explores David Cameron's experiences in foreign affairs
   since coming to office by examining his policies through the guise of
   New Labour's use of the Atlantic Bridge. This article will outline the
   history and success of Prime Ministers Blair and Brown in dealing with
   both sides of the Atlantic during their respective governments. From
   here, Cameron's own political position and ideologies will be taken into
   account along with New Labour's legacy before discussing Cameron's
   adventures in foreign policy. In particular, this article will compare
   the intervention in Libya by American and European forces before looking
   at Cameron's European Union (EU) policy and the announcement of a
   referendum on Britain's membership of the EU. The purpose of this
   article is to measure Cameron's success in these initial undertakings in
   transatlantic relations.},
