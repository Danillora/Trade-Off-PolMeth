@article{ WOS:000346954800001,
Author = {Santos, Manoel Leonardo and Perez-Linan, Anibal and Garcia Montero,
   Mercedes},
Title = {Presidential control of the legislative agenda in Latin America},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2014},
Volume = {34},
Number = {3},
Pages = {511-536},
Abstract = {This paper analyzes the conditions under which presidents dominate the
   legislative agenda. To address this issue we analyze the governmental
   period of 30 Latin American Presidents, developing a qualitative
   comparative analysis (QCA). The analysis highlights that the combination
   of a favorable institutional design and a division of Congress in
   different ideological blocs favors the Executive branch. Two other
   conditions, reliance on a parliamentary majority and the absence of
   party fragmentation, are combined with the above elements to ensure the
   predominance of the Executive. The findings of this comparative study
   are confirmed by a supplementary statistical analysis.},
