@article{ WOS:000337970400012,
Author = {Cann, Damon M. and Goelzhauser, Greg and Johnson, Kaylee},
Title = {Analyzing Text Complexity in Political Science Research},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2014},
Volume = {47},
Number = {3},
Pages = {663-666},
Month = {JUL},
Abstract = {This article analyzes the text complexity of political science research.
   Using automated text analysis, we examine the text complexity of a
   sample of articles from three leading generalist journals and four
   leading subfield journals. We also examine changes in text complexity
   across time by analyzing a sample of articles from the discipline's
   flagship journal during a 100-year span. Although it is not surprising
   that a typical political science article is difficult to read, it is
   accessible to intelligent lay readers. We found little difference in
   text complexity across time or subfield.},
