@article{ WOS:000420558800006,
Author = {Raabe, Johannes and Krifft, Roland and Vogel, Joshua and Linhart, Eric},
Title = {Rightful paragon or myth? A comparative analysis of the Personalized
   Proportional Representation system in the German Lander},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2014},
Volume = {8},
Number = {3-4},
Pages = {283-305},
Month = {DEC},
Abstract = {This paper provides a comparative analysis of the Personalized
   Proportional Representation (PPR) electoral system in the German Lander.
   The analysis examines in how far PPR as a mixed electoral system is able
   to simultaneously ensure both the proportional representation of the
   electorate's interests in parliament as well as a high level of party
   system concentration. With regard to these goals, the empirical analysis
   of all state elections after World War II demonstrates that PPR hardly
   differs from a pure PR system, at least in Germany. Furthermore, the
   analysis shows that the party system would rarely be substantially
   affected by overhang seats. Overall, there is little evidence in the
   German Lander for the superiority of PPR in terms of combining the
   ``best of both worlds{''} as a mixed electoral system.},
