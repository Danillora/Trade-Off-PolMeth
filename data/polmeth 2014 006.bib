@article{ WOS:000215064500005,
Author = {Morrison, Scott},
Title = {In Kind or Monetary Restitution? Historic Wrongs and Indigenous Land
   Claims},
Journal = {JOURNAL OF POLITICS AND LAW},
Year = {2014},
Volume = {7},
Number = {2},
Pages = {50-62},
Month = {JUN},
Abstract = {Native peoples in North America, Australia and New Zealand have
   (sometimes successfully) demanded compensation for seizures of their
   ancestral lands. This article sets out the arguments against restitution
   in these and other cases. It then offers up a defense of ``in kind{''}
   restitution, that is, the return of lands or other property
   misappropriated. As a pertinent case study this article narrates the
   story of the Sioux tribes of the mid-western United States. They have
   sought to recover lands in and around the Black Hills of South Dakota.
   Offering a view of the historical and contemporary collective cultural
   and spiritual significance of the Black Hills and the history of
   dispossession therefrom, this article traces the Sioux's individual and
   tribal efforts both through the legal system and the courts and into the
   US federal government where a bill was unsuccessfully put forward in an
   effort to resolve their claims.},
