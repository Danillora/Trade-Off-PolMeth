@article{ WOS:000343698600011,
Author = {Bahcheli, Tozun and Noel, Sid},
Title = {The Quest for a Political Settlement in Cyprus: Is a Dyadic Federation
   Viable?},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2014},
Volume = {44},
Number = {4},
Pages = {659-680},
Month = {FAL},
Abstract = {Greek and Turkish Cypriot leaders have been negotiating to create a
   ``bi-zonal, bi-communal'' federation over a span of thirty-six years,
   but despite support from the United Nations and other intermediaries, no
   agreement has yet been reached. This article examines the reasons why
   the many attempts to negotiate a ``federal solution'' have failed. Our
   argument is that dyadic federations have inherent flaws that make them
   difficult to operate successfully, and even if the two communities could
   somehow be persuaded to form such a union, it would not be viable as a
   system of government.},
