@article{ WOS:000433513200007,
Author = {Diaz Carrasco, Marianela Agar},
Title = {``Women in Polleras{''} and the Proposal for the Decolonization of
   Gender in the Plurinational State of Bolivia},
Journal = {CIENCIA POLITICA},
Year = {2014},
Volume = {9},
Number = {18},
Pages = {133-156},
Month = {JUL-DEC},
Abstract = {This article analyzes, from a decolonial feminist perspective, some
   limits and possibilities identified in the proposal of gender
   decolonization in the Plurinational State of Bolivia from 2006 until
   2014. It also problematizes three aspects that challenge the existing
   continuum of forms of violence, colonialism, discrimination and
   subordination: i) denaturation of inferiority and servility, ii) the
   ``civilizing{''} idea demarcated by western knowledge and iii) the
   permanence of the hegemonic structures of power to appoint and regulate.
   This is done from a feminist qualitative methodology, by conducting
   interviews to key informants, this is, Aymara and Quechua indigenous
   women who held positions of power in the State, and also through
   documentary review of various publications related to the subject. The
   paper questions existing tensions and contradictions between the
   proposal made from the Andean worldview, which shows remaining
   patriarchal practices inside and outside of indigenous social
   organizations and the State.},
