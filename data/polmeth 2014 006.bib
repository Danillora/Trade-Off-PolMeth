@article{ WOS:000354227800005,
Author = {Lazarus, Jeanne},
Title = {Watch out your money and behave. Learning autonomy by the bank},
Journal = {POLITIX},
Year = {2014},
Volume = {27},
Number = {108},
Pages = {75+},
Abstract = {Assuming that regarding young adults ``being autonomous{''} means, in
   most cases, that they be financially independent, this article intends
   to analyse the terms and limitations of this financial autonomy by
   focusing on the relationship between the youth and banks. From an
