@article{ WOS:000336196000010,
Author = {Mahler, Vincent A. and Jesuit, David K. and Paradowski, Piotr R.},
Title = {Electoral Turnout and State Redistribution: A Cross-National Study of
   Fourteen Developed Countries},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2014},
Volume = {67},
Number = {2},
Pages = {361-373},
Month = {JUN},
Abstract = {This study explores the relationship between electoral participation and
   income redistribution by way of social transfers, using data from the
   European Social Survey, the Comparative Study of Electoral Systems, and
   the Luxembourg Income Study. It extends previous research by measuring
   the income skew of turnout rather than using average turnout as a proxy
   for its income bias. We find that, controlling for a number of other
   variables, the income skew of turnout is negatively related to transfer
   redistribution and that electoral participation by those in poverty is
   positively associated with redistribution in their favor.},
