@article{ WOS:000210675000006,
Author = {Black, Ryan and Madonna, Anthony and Owens, Ryan},
Title = {Qualifications or Philosophy? The Use of Blue Slips in a Polarized Era},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2014},
Volume = {44},
Number = {2, SI},
Pages = {290-308},
Month = {JUN},
Abstract = {In recent years, judicial nominations to lower federal courts have been
   blocked privately by negative blue slips returned by home state
   senators. We examine the conditions under which senators return negative
   blue slips and discover two results. First, consistent with existing
   work, ideology plays a strong role in blue slipping. Second, and more
   important, we find that nominee qualifications mitigate ideological
   extremism-but only for district court nominees. That is, in the past,
   presidents could use legal credentials to prop up district court and
   circuit court nominees who shared his views. Today, in a Senate marked
   by high polarization and few moderates, the president cannot.},
