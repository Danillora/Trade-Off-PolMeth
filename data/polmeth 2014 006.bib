@article{ WOS:000342040700001,
Author = {Dubin, Boris},
Title = {Power, Masses, Manipulation Putin's Ratings and Russian Society},
Journal = {OSTEUROPA},
Year = {2014},
Volume = {64},
Number = {7},
Pages = {3+},
Month = {JUL},
Abstract = {If one believes polls, President Vladimir Putin enjoys tremendous
   approval ratings. His high standing, however, says more about the state
   of the political system - which steers public opinion with the help of
   television as well as mobilising, integrative symbols - than it does
   about society's attitudes. After all, society is cut off from the chance
   to influence politics. In a situation of political impoverishment where
   elective possibilities are missing and competition is nonexistent, the
   approval ratings of the Putin regime have a purely ceremonial, imitative
   character.},
