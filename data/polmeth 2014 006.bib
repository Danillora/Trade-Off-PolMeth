@article{ WOS:000211660400008,
Author = {Deacon, Bob},
Title = {Career, charisma, conviction and the challenges facing the Social
   Protection Floor},
Journal = {GLOBAL SOCIAL POLICY},
Year = {2014},
Volume = {14},
Number = {3, SI},
Pages = {406-435},
Month = {DEC},
Abstract = {In this short article, I want to summarize my argument (Deacon, 2013)
   that changes in global social policy at the level of international
   organizations (IOs) can only be completely accounted for if we
   centre-ground the role of individual policy entrepreneurs working within
   those agencies. The Social Protection Floor (SPF) is the case study. One
   of the key drivers of the SPF policy, then working in United Nations
   Children's Fund (UNICEF) has since October 2013 taken over the
   Directorship of the renamed Social Protection Department of the
   International Labour Organization (ILO) responsible for its delivery. In
   the second part of this piece, I examine briefly new initiatives
   instigated by her team and suggest the challenges she faces within and
   outside the ILO if this breakthrough policy is to continue to have
   influence and impact within the United Nation's (UN) system and in
   countries.},
