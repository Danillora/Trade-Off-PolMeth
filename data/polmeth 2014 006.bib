@article{ WOS:000217607800003,
Author = {Rocher, Francois},
Title = {Self-determination and the Use of Referendums: the Case of Quebec},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
Year = {2014},
Volume = {27},
Number = {1},
Pages = {25-45},
Month = {MAR},
Abstract = {On two occasions (1980 and 1995), Quebeckers rejected the Quebec
   government's sovereignty proposal. Many lessons can be drawn from the
   Quebec referendum experience. The purpose of this article is to shed
   light on the origins and motivations of the independence movement. It
   focuses more specifically on the 1980 and 1995 referendums, examining in
   both cases the political context, the judicial-institutional framework
   within which these public consultations were held and the arguments
   raised during the referendum campaigns. Furthermore, it analyses the
   results as well as the political, constitutional and juridical
   consequences of the federalist victories. The article concludes that
   attempts by Quebec sovereignists to question the Canadian political
   system have invariably resulted in a stronger and more consolidated
   central state while significantly weakening the Quebec state's ability
   to determine its own political future.},
