@article{ WOS:000438654000004,
Author = {Korstanje, Maximilian E.},
Title = {Towards an Index of Fear: The Role of Capital in Risk's Construction},
Journal = {INTERNATIONAL JOURNAL OF CYBER WARFARE AND TERRORISM},
Year = {2014},
Volume = {4},
Number = {1},
Pages = {19-26},
Month = {JAN-MAR},
Abstract = {The present essay review propones a new model to understand the nature
   of risk. The authors hereby argue that risks help the society to
   function. Not only the sum-zero risk society is impossible, but also
   would affect seriously the economy of capitalist society. Therefore,
   insurances and risk are inexorably intertwined. The authors have
   explored three valid indicators toward the construction of a
   general-index of risk, which not only can be homologable to other
   nations, but also comparable among cultures. Communities may be studied
   by their way of perceiving risks. Beyond the debate on if terrorism is
   or not a risk, the goals of this review is twofold. Firstly, it is
   important to explore the economic nature of terrorism to discern their
   causes. Secondly, it is necessary to build a valid index that reflects
   how a society feels.},
