@article{ WOS:000218590700009,
Author = {Enos, Ryan D. and Fowler, Anthony},
Title = {Pivotality and Turnout: Evidence from a Field Experiment in the
   Aftermath of a Tied Election},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2014},
Volume = {2},
Number = {2},
Pages = {309-319},
Month = {OCT},
Abstract = {Many citizens abstain from the political process, and the reasons for
   this abstention are of great interest and importance. Most scholars and
   pundits assume that greater electoral competition and the increased
   chance of pivotality will motivate citizens to participate. We test this
   hypothesis through a large-scale field experiment that exploits the rare
   opportunity of a tied election for major political office. Informing
   citizens that an upcoming election will be close has little mobilizing
   effect. Any effect that we do detect is concentrated among a small set
   of frequent voters. The evidence suggests that increased pivotality is
   not a solution to low turnout and the predominant models of turnout
   focusing on pivotality are of little practical use.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {EDINBURGH BLDG, SHAFTESBURY RD, CB2 8RU CAMBRIDGE, ENGLAND},
