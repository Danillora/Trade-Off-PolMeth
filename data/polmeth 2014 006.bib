@article{ WOS:000218761900002,
Author = {Hall, Mark David},
Title = {Madison's Memorial and Remonstrance, Jefferson's Statute for Religious
   Liberty, and the Creation of the First Amendment},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2014},
Volume = {3},
Number = {1},
Pages = {32-63},
Month = {SPR},
Abstract = {Jurists, scholars, and popular writers routinely assert that the men who
   framed and ratified the First Amendment were influenced by James
   Madison's Memorial and Remonstrance ( 1785) and Thomas Jefferson's
   Statute for Religious Liberty (1786). In this essay I demonstrate that
   there is little evidence to support these claims. Because these
   documents represent only one approach to church-state relations in the
   era, jurists and others who believe that the religion clauses should be
   interpreted in light of the founders' views need to look well beyond
   these texts if they want to understand the First Amendment's
   ``generating history.{''}},
Publisher = {UNIV CHICAGO PRESS},
Address = {1427 E 60TH ST, CHICAGO, IL 60637-2954 USA},
Type = {Article},
