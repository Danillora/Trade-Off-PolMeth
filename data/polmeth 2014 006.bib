@article{ WOS:000217608600005,
Author = {Klesse, Christian},
Title = {Poly Economics-Capitalism, Class, and Polyamory},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
Year = {2014},
Volume = {27},
Number = {2, SI},
Pages = {203-220},
Month = {JUN},
Abstract = {Academic research and popular writing on nonmonogamy and polyamory has
   so far paid insufficient attention to class divisions and questions of
   political economy. This is striking since research indicates the
   significance of class and race privilege within many polyamorous
   communities. This structure of privilege is mirrored in the exclusivist
   construction of these communities. The article aims to fill the gap
   created by the silence on class by suggesting a research agenda which is
   attentive to class and socioeconomic inequality. The paper addresses
   relevant research questions in the areas of intimacy and care, household
   formation, and spaces and institutions and advances an intersectional
   perspective which incorporates class as nondispensable core category.
   The author suggests that critical research in the field can stimulate
   critical self-reflexive practice on the level of community relations and
   activism. He further points to the critical relevance of Marxist and
   Postmarxist theories as important resources for the study of polyamory
   and calls for the study of the contradictions within poly culture from a
   materialist point of view.},
Publisher = {SPRINGER},
