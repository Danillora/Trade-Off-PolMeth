@article{ WOS:000433513200011,
Author = {Roth Deubel, Andre-Noel and Bernal Gamboa, Elizabeth},
Title = {Q Methodology: an alternative for participation in the reform of higher
   education policy in Colombia},
Journal = {CIENCIA POLITICA},
