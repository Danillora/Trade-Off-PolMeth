@article{ WOS:000215844000008,
Author = {Cortez Salinas, Josafat and Salazar Rebolledo, Grisel},
Title = {The Party of Democratic Revolution in the Mexican Senate: an analysis of
   success initiatives 2000-2012},
Journal = {REVISTA MEXICANA DE ANALISIS POLITICO Y ADMINISTRACION PUBLICA},
Year = {2014},
Volume = {3},
Number = {2},
Pages = {185-204},
Month = {JUL-DEC},
Abstract = {The paper analyzes the Party of the Democratic Revolution (PRD) in
   Mexico's Senate of the Republic from 2000 to 2012. Under the new
   legislative dynamics in Mexico we attempt to explore the conditions that
   enabled the approval of the bills presented by this party, using a
   rational choice approach. It is argued that a political party is more
   likely to succeed in their bills when it has greater resources within
   the House, in terms of technical negotiation and expertice. We found
   that a positive relationship between the approval rate of PRD's bills
   and a) the number of chairs won by the party; b) the commissions chaired
   by the party, and c) the linkages between the bill's subject and the
   party ideology.},
