@article{ WOS:000343696600001,
Author = {Rush, Michael},
Title = {Engaging with the Enemy: The Parliamentary Participation of Party
   leaders, 1945-2010},
Journal = {PARLIAMENTARY AFFAIRS},
Year = {2014},
Volume = {67},
Number = {4},
Pages = {751-766},
Month = {OCT},
Abstract = {Patrick Dunleavy et al. established some years ago that Prime Ministers
   have engaged less and less with Parliament. This article updates their
   work and extends it to all party leaders in the House of Commons since
   1945. Prime-ministerial participation has continued to decline, has
   become increasingly institutionalised and is matched by the Leader of
   the Opposition and the Leader of the Liberals/Liberal Democrats, whose
   activity appears largely reactive. However, this contrasts with the
   parliamentary activity of the leaders of other parties in the Commons,
   whose activity is invariably greater and more varied.},
