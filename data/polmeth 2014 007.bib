@article{ WOS:000217332200005,
Author = {Francois, Abel and Magni-Berton, Raul},
Title = {The effects of the 2001 French law on multi-holding of electoral
   mandates},
Journal = {FRENCH POLITICS},
Year = {2014},
Volume = {12},
Number = {1},
Pages = {69-76},
Month = {APR},
Abstract = {The article assesses the consequences of the 2001 change in French
   regulation regarding the cumul des mandats, or multi-holding mandates,
   which restricted the opportunity to simultaneously hold several elective
   mandates. Comparing before and after the implementation of the new Law,
   empirical descriptions show that (i) candidates in legislative elections
   adapted to the new rules by reducing the local mandates held; (ii)
   candidates also showed a tendency to change the nature of the mandates
   held. These results can give some insight into the future consequences
   of the law currently being debated.},
