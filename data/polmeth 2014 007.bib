@article{ WOS:000446190700002,
Author = {Rouet, Gilles},
Title = {TURBULENCES, EUROPEAN NEIGHBOURHOOD POLICY AND EUROPEAN IDENTITY},
Journal = {POLITICKE VEDY},
Year = {2014},
Volume = {17},
Number = {2},
Pages = {9-27},
Abstract = {Since 2004, the European Neighbourhood Policy has established
   cooperation instruments similar to those of the pre-accession policy,
   which has contributed to a major misunderstanding, because the countries
   concerned, i.e. new neighbouring partners, were not intended to fully
   integrate with the EU. But now, this policy created in a context of
   relative stability in partner countries must change radically its
   contours. The Arab Springs and the Georgian and Ukrainian revolutions,
   the political developments in Russia, in particular, the overcoming of
   opposition between deepening and enlargement while European citizens
   withdraw into a national level, induce a radical change of the choices
   in matter of EU foreign policy. The construction of EU is in progress
   both inside and outside, because our neighbours are like our mirrors and
   our relationships contribute to the evolution of our European feeling.
   In this context, it is very important to propose selected elements about
   the European neighbourhood. What is new?},
