@article{ WOS:000212137200004,
Author = {Smith, Karen E.},
Title = {The UK and `genocide' in Biafra},
Journal = {JOURNAL OF GENOCIDE RESEARCH},
Year = {2014},
Volume = {16},
Number = {2-3, SI},
Pages = {247-262},
Abstract = {In late August 1968, following a British proposal, Nigeria announced
   that it would allow an international observer team into the country to
   show that it was not pursuing a campaign of genocide in Biafra. This
   article analyses why the United Kingdom pushed for the creation of the
   observer team, and shows how the team's work was incorporated into the
   British government's justifications for its support of the Nigerian
   government. The experience of the observer team illustrates the
   difficulties of providing an `objective' view regarding whether or not
   genocide is taking place.},
