@article{ WOS:000338804400002,
Author = {Krick, Eva},
Title = {Expert advisory committees as participatory instruments? Potentials and
   limitations regarding impact and inclusion},
Journal = {OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT},
Year = {2014},
Volume = {43},
Number = {1},
Pages = {7-22},
Abstract = {Expert advisory committees, like the Hartz-Commission or the Summit on
   Integration in Germany, are often presented as arenas of political
   dialogue and consultation, which enhance the participatory quality of
   political decision-making. Opponents argue that the committees lack
   legitimacy, are often partially composed or used for symbolic purposes
   only. The paper analyses the participatory potential of four expert
   advisory committees set up by the German federal government, by applying
   a two-dimensional standard of political participation, which covers
   inclusion as well as impact of participation. The outcomes of the four
   cases will be typed, discussed and explained with reference to theories
   of negotiation and the science-policy nexus.},
