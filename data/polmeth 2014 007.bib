@article{ WOS:000218542700006,
Author = {Kowalski, Michael},
Title = {Between `sousveillance' and applied ethics: practical approaches to
   oversight},
Journal = {SECURITY AND HUMAN RIGHTS},
Year = {2014},
Volume = {24},
Number = {3-4},
Pages = {280-285},
Abstract = {The issue of the oversight of intelligence and security services is
   playing an increasing role in the debate on global security issues both
   among specialists and the broader public. Beyond theoretical debates on
   intelligence and surveillance ten practical approaches to advance
   oversight are being developed. Core ideas address the implications of
   the political supremacy of oversight, the need for revisiting the focus
   of oversight as well as the possibilities of the proliferation of best
   oversight practices. Furthermore, suggestions are made regarding the
   integration of ethics in security research and the creation of space for
   applied ethics for intelligence practitioners.},
