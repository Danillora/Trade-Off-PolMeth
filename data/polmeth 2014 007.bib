@article{ WOS:000348590400007,
Author = {Fowler, Erika Franklin and Ridout, Travis N.},
Title = {Political Advertising in 2014: The Year of the Outside Group},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
Year = {2014},
Volume = {12},
Number = {4},
Pages = {663-684},
Month = {DEC},
Abstract = {The volume of televised political advertising plateaued in 2014, as did
   levels of negativity. Yet the most interesting story about this
   advertising was the extensive involvement of outside groups, many of
   which did not disclose their donors. In many of the most competitive
   Senate races, groups surpassed the parties - and sometimes even the
   candidates - as the primary sponsors of political advertising. We
   speculate on what these patterns from 2014 are likely to mean for
   advertising in 2016.},
