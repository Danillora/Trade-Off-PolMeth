@article{ WOS:000212002500006,
Author = {Hoenig, Bianca},
Title = {Nature into a socialist landscape? The case of the Polish Tatra
   Mountains after 1945},
Journal = {NATIONAL IDENTITIES},
Year = {2014},
Volume = {16},
Number = {3, SI},
Pages = {253-267},
Abstract = {Scholarly work on socialist landscapes has most often focused on
   industrial areas and planned cities. This study, by contrast, explores
   the impact of socialism on a region that was not obviously suited as a
   symbol of industrial modernity, namely the Polish Tatra Mountains.
   Examining tourist media after 1945, this article analyses written and
   pictorial representations of this natural region that has been so
   central to Polish national imagination. Despite attempts to construct an
   image explicitly of the socialist Tatra Mountains, the overall effect,
   however, was rather ambivalent due to the constraints of the socialist
   narrative vis-a-vis nature.},
