@article{ WOS:000340374600005,
Author = {Kelly, Jamie Terence},
Title = {DEMOCRACY AS THE RULE OF A SMALL MANY},
Journal = {CRITICAL REVIEW},
Year = {2014},
Volume = {26},
Number = {1-2},
Pages = {80-91},
Abstract = {What is the optimal size of a democratic society? While not taking an
   explicit stand on this issue, H l ne Landemore's model of democracy in
   Democratic Reason suggests that democracies ought to be small, certainly
   smaller than many existing states. If, as Landemore argues, we must rely
   on the random selection of representatives, then we should be concerned
   about both the size of the population and the way cognitive diversity is
   distributed within it. Given the realities of party politics and media
   framing, this means that smaller political societies will yield wiser
   decisions than very large ones.},
