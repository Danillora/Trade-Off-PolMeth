@article{ WOS:000210537800003,
Author = {Stavro, Elaine},
Title = {SARS and Alterity: the Toronto-China Binary},
Journal = {NEW POLITICAL SCIENCE},
Year = {2014},
Volume = {36},
Number = {2},
Pages = {172-192},
Abstract = {This article will analyse the mass media's representation of the Chinese
   during the SARS epidemic. Represented as dirty, secretive, the
   embodiment of authoritative and pre-modern practices, the Chinese have
   been constituted as bad global citizens. This sharply contrasts with the
   Torontonians who are portrayed as hygienic, reliable, truthful the
   embodiment of democracy and modern science. Using Edward Said's theory
   of Orientalism as well as Julia Kristeva's theory of abjection, this
   article will not only trace the contours of these binary identities, the
   psychic forces that underpin them, but also speculate upon the hidden
   and not so hidden politics behind their construction.},
