@article{ WOS:000216244700007,
Author = {Siupiur, Elena},
Title = {THE ROLE OF THE EUROPEAN UNIVERSITIES IN SHAPING THE ROMANIAN AND
   SOUTH-EAST EUROPEAN POLITICAL ELITES IN THE NINETEENTH CENTURY},
Journal = {ANALELE UNIVERSITATII BUCURESTI-STIINTE POLITICE},
Year = {2014},
Volume = {16},
Number = {1},
Pages = {113-128},
Abstract = {During the 19th century, the South East European countries experience a
   radical cultural, institutional and political change. Like throughout
   the rest of Europe, this change is intricately related to the
   university, in all its forms and expressions - higher education
   institution, as well as locus of forging, transmitting and reproducing
   ideas, cultures, university elites, and ultimately political elites.
   Within the Romanian space, this impact is particularly noticeable as the
   thousands of young people that studied abroad in European universities
   and returned home became not only its first intellectual and
   professional generations but also its reforming and modernizing elites.},
