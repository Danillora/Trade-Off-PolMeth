@article{ WOS:000447071200009,
Author = {Ozhan, Taha},
Title = {The Longest Year of Turkish Politics: 2014},
Journal = {INSIGHT TURKEY},
Year = {2014},
Volume = {16},
Number = {2},
Pages = {79-98},
Month = {SPR},
Abstract = {Like all long political years, the year 2014 did not begin on January
   1st; rather, 2014 politically began at the end of May with the Taksim
   events. Nevertheless, the year may end on an optimistic note. It could
   be said that, unless the date of the upcoming general elections change,
   the long political year of 2014 will extend to June 2015. Had the
   government been overthrown by the police-judiciary coup in December
   17th., Turkey would have been sentenced to a neo-tutelage regime for
   many years to come. The first phase of the tripartite elections race in
   Turkey ended with Erdogan's victory. The upcoming presidential elections
