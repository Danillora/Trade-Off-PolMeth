@article{ WOS:000341648400004,
Author = {Mintrom, Michael and Salisbury, Chris and Luetjens, Joannah},
Title = {Policy entrepreneurs and promotion of Australian state knowledge
   economies},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2014},
Volume = {49},
Number = {3},
Pages = {423-438},
Month = {SEP},
Abstract = {Policy entrepreneurs seek to shift the status quo in given areas of
   public policy. In doing so, they work closely with others, and their
   activities call for high levels of political skill. This article
   examines the actions of policy entrepreneurs who promoted the
   development of knowledge economies in two Australian states: Queensland
   and Victoria. During the past two decades, national and sub-national
   governments around the world have sought to nurture knowledge economies
   within their borders. Our analysis of knowledge economy advocacy
   improves understanding of how specific individuals - as strategic team
   builders - can promote major policy change. This focus on team work and
   coalition-building as central elements of the process of policy
   entrepreneurship offers a corrective to some earlier studies that
   inappropriately conferred lone hero status to policy entrepreneurs.},
