@article{ WOS:000214698200004,
Author = {Yun, Ding},
Title = {The Doctrine of Contradiction and the Political Philosophy of Mao Zedong},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2014},
Volume = {4},
Number = {3},
Pages = {317-330},
Abstract = {The present research explores the connection between Mao's philosophy
   and his political ideas. Taking as the point of departure the innate
   contradiction implicit in the founding process of the People's Republic
   of China and its state system, I argue that in addition to the unity of
   the discourse on people and on class in Mao's political thinking there
   is also an opposition, and that this opposition can be traced back to
   his philosophy. After reaching a number of conclusions of significance
   for Mao's doctrine of class, conclusions that relate to the very
   approach involved in his ideas of contradiction, we set out to enrich
   and develop these ideas, with the final goal of providing a new
   philosophical fulcrum for explaining and regulating the polity of the
   post-Mao era.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
