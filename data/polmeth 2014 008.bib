@article{ WOS:000212907200002,
Author = {Temelini, Michael},
Title = {Dialogical approaches to struggles over recognition and distribution},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2014},
Volume = {17},
Number = {4},
Pages = {423-447},
Abstract = {This paper contrasts three non-skeptical ways of explaining and
   reconciling political struggles: monologue, instrumental dialogue, and a
   comparative dialogical approach promoted by Charles Taylor and James
   Tully. It surveys the work of Taylor and Tully to show three particular
   family resemblances: their emphasis on practice, irreducible diversity,
   and periodic reconciliation. These resemblances are evident in the way
   they employ dialogical approaches to explain struggles over recognition
   and distribution. They describe these as dialogical actions, and suggest
   that a form of dialogical comparison might reconcile their various
   contested demands.},
