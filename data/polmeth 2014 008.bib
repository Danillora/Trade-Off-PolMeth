@article{ WOS:000330862900002,
Author = {Bonica, Adam},
Title = {The Punctuated Origins of Senate Polarization},
Journal = {LEGISLATIVE STUDIES QUARTERLY},
Year = {2014},
Volume = {39},
Number = {1},
Pages = {5-26},
Month = {FEB},
Abstract = {This article uses a new dynamic ideal-point estimation method that
   incorporates smoothing techniques to construct a more detailed account
   of Senate polarization. The results reveal that the Senate polarized in
   two distinct phases. Member replacement accounts for nearly all of the
   increase from the early 1970s through the mid-1990s after which
   ideological adaptation emerges as the dominant force behind
   polarization. In addition, I find that a few brief periods of
   intensified partisanship account for most of the increase in
   polarization since the mid-1990s, suggesting that these episodes have
   had significant and lasting effects.},
