@article{ WOS:000215514900001,
Author = {Mansilla, H. C. F.},
Title = {The dilemmas of the institutionalist approach on the contemporary
   democracy: a contribution to the history of ideas in the political
   science 1980-2000},
Journal = {REVISTA DE INVESTIGACIONES POLITICAS Y SOCIOLOGICAS},
Year = {2014},
Volume = {13},
Number = {2},
Pages = {9-30},
Abstract = {In Latin America the period 1980-2000 coincided with the predominance of
   public policies, which have been inspired by neoliberalism. Within
   political sciences this period exhibited a strong influence of
   institutional approaches, which had a clear bias towards the
   overestimation of legal-institutional aspects and the undervaluation of
   substantial elements of public life, like popular culture of
   authoritarianism, the strength of the traditional caudillos, the
   consequences of public politics bound to ecological damages and the
   manipulative possibilities offered by mass media. Therefore following
   phenomena, mostly considered as positive, must be put into question:
   modernization and globalization, unrestricted economic growth, the
   mercantile conception of social life, and the expansion of mass media.
   This questioning should be complemented by an approach based of the
   conception of public welfare.},
