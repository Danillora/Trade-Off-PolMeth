@article{ WOS:000338694100006,
Author = {Marx, Axel and Rihoux, Benoit and Ragin, Charles},
Title = {The origins, development, and application of Qualitative Comparative
   Analysis: the first 25 years},
Journal = {EUROPEAN POLITICAL SCIENCE REVIEW},
Year = {2014},
Volume = {6},
Number = {1},
Pages = {115-142},
Month = {FEB},
Abstract = {A quarter century ago, in 1987, Charles C. Ragin published The
   Comparative Method, introducing a new method to the social sciences
   called Qualitative Comparative Analysis (QCA). QCA is a comparative
   case-oriented research approach and collection of techniques based on
   set theory and Boolean algebra, which aims to combine some of the
   strengths of qualitative and quantitative research methods. Since its
   launch in 1987, QCA has been applied extensively in the social sciences.
   This review essay first sketches the origins of the ideas behind QCA.
   Next, the main features of the method, as presented in The Comparative
   Method, are introduced. A third part focuses on the early applications.
   A fourth part presents early criticisms and subsequent innovations. A
   fifth part then focuses on an era of further expansion in political
   science and presents some of the main applications in the discipline. In
   doing so, this paper seeks to provide insights and references into the
   origin and development of QCA, a non-technical introduction to its main
   features, the path travelled so far, and the diversification of
   applications.},
