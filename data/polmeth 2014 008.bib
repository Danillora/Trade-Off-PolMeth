@article{ WOS:000340573800007,
Author = {Rutter, Jill},
Title = {The Quango Conundrum Revisited-Why the Government Still Needs a More
   Coherent Approach to Arm's-Length Bodies},
Journal = {POLITICAL QUARTERLY},
Year = {2014},
Volume = {85},
Number = {2},
Pages = {148-152},
Month = {APR-JUN},
Abstract = {There has been substantial reform of public bodies since the Coalition
   government was formed in 2010. But although numbers have been reduced,
   there has been no attempt to address the confused landscape-indeed,
   departments have interpreted criteria for what should be put at arm's
   length in different ways, introducing further complexity. A clearer link
   between function and form is needed. This would help address some of the
   blurred lines over accountability that we have seen in the recent rows
   over Ofsted and the Environment Agency.},
