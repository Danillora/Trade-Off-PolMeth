@article{ WOS:000340826400009,
Author = {Voessing, Konstantin},
Title = {Predictably exceptional: The failure of social democracy and the
   formation of American labour politics in comparative perspective},
Journal = {PARTY POLITICS},
Year = {2014},
Volume = {20},
Number = {5},
Pages = {767-777},
Month = {SEP},
Abstract = {This article explains the outcome of American labour's formative stage
   in politics between 1860 and 1921 by modelling the decision-making
   process of labour elites under limited and full labour inclusion.
   Several countries featured limited inclusion through a neutral executive
   and democratic institutions, but full inclusion - the incorporation of
   labour into the party system through entrenched partisan elites -
   occurred only in the United States. An analytic narrative illustrates
   the conclusion from my decision analysis that the failure of social
   democracy and the embrace of moderate syndicalism in the United States
   occurred as the rational response of labour elites to this unique
   environment.},
