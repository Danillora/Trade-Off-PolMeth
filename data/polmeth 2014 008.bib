@article{ WOS:000442723700002,
Author = {Sakwa, R.},
Title = {AXIOLOGICAL VS DIALOGICAL POLITICS IN CONTEMPORARY EUROPE},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2014},
Number = {2},
Pages = {8+},
Abstract = {The author analyzes the Russian-European relations through the prism of
   two opposing types of politics. On the one hand, the concept of
   ``axiology{''} is used to denote a form of politics that is categorical,
   monological and ideological. Axiological politics assumes that some
   things have been settled outside of the political process, and thus
   politics becomes a means for the implementation of a priori positions.
   On the other hand, the concept of dialogical politics reflects the
   normative democratic ideal, suggesting that the quotient of axiological
   politics can be tempered by the structured engagement of different
   political subjects in a ``communicative{''} process. Particular
   attention is paid to the conceptual foundations of dialogical politics
   and correlation between the two types of politics in different political
   regimes. The paper also focuses on mimesis, which is at the heart of
   axiological politics. Correspondingly, three types of mimetic politics
   are explored: mimetic violence, adaptive mimesis, and mimetic simulacra.},
