@article{ WOS:000218758600006,
Author = {Taylor, Robert S.},
Title = {Religious Conservatives and Safe Sex: Reconciliation by Nonpublic Reason},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2014},
Volume = {3},
Number = {2},
Pages = {322-340},
Month = {FAL},
Abstract = {Religious conservatives in the United States have frequently opposed
   public health measures designed to combat sexually transmitted diseases
   among minors, such as sex education, condom distribution, and human
   papillomavirus (HPV) vaccination. Using Rawls's method of conjecture, I
   will clear up what I take to be a misunderstanding on the part of
   religious conservatives: even if we grant their premises regarding the
   nature and source of sexual norms, the wide-ranging authority of parents
   to enforce these norms against their minor children, and the potential
   sexual disinhibition effects of the above public health measures, their
   opposition to at least one of these measures, HPV vaccination, cannot be
   justified. In fact, their comprehensive doctrines, when properly
   interpreted, should lead them to back this measure and thereby draw
   closer to a policy consensus with other citizens regarding teenage
   sexual health.},
