@article{ WOS:000421258000011,
Author = {Gamaghelyan, Philip and Rumyansev, Sergey},
Title = {HISTORY EDUCATION IN THE SOUTH CAUCASUS},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2014},
Volume = {13},
Number = {1},
Pages = {121-130},
Month = {SPR},
Abstract = {As the entire South Caucasus has been immersed in ethnically-framed
   conflicts since the late 1980s, an ethos of conflict has been
   constructed in the region. The commonly held views of the
   Armenian-Azerbaijani, Georgian-South Ossetian, Georgian-Abkhaz, but also
   Armenian-Turkish and Georgian-Russian conflicts as ancient, natural,
   intractable, and all-encompassing have been largely shaped by
   professional historians. Considering that the dynamics that sustain
   these ongoing conflicts are a serious impediment to democratization and
   socio-economic progress, we can conclude that the current state of
   history education whose narratives service conflicts is one of these
   impediments. This article's authors therefore argue that comprehensive
   reform of the structure and content of history education is a necessary
   condition for progress and development in the South Caucasus.},
