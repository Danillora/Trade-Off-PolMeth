@article{ WOS:000210089100008,
Author = {Pantojas Garcia, Emilio},
Title = {The two great revolutions in the Caribbean, Haiti and Cuba: examples
   or lessons?},
Journal = {AMERICA LATINA HOY-REVISTA DE CIENCIAS SOCIALES},
Year = {2014},
Volume = {66},
Pages = {169-180},
Month = {APR},
Abstract = {Why did the two great revolutions of the Caribbean, Haiti and Cuba, were
   condemned to ostracism by its contemporaries and are presented as bad
   examples rather than exemplary events? These and other related questions
   are answered examining three arguments: revolution as a critical
   transition event, economic sustainability as a central challenge for
   revolutions, and revolution versus the ontology of the Caribbean as a
   subordinate region.},
Publisher = {EDICIONES UNIV SALAMANCA},
Address = {APARTADO DE CORREOS 325, SALAMANCA, 00000, SPAIN},
Type = {Article},
