@incollection{ WOS:000361130800003,
Author = {Ridge-Newman, Anthony},
Book-Author = {RidgeNewman, A},
Title = {Tory Elites and Centralized Internet Operations},
Booktitle = {CAMERON'S CONSERVATIVES AND THE INTERNET: CHANGE, CULTURE AND CYBER
   TORYISM},
Series = {Palgrave Pivot},
Year = {2014},
Pages = {19-35},
Abstract = {Chapter 2 examines the impact of Web Cameron and two other Tory
   internet-linked technologies, My Conservatives and MERLIN: The chapter
   focuses on the role of Conservative Party elites and CC.11(2 operations
   in the development of these new Tiny technologies. It is argued that the
   innovative WebCarneron platform was a symbolic event that catalysed
   further internet use in the wider party and led the way in terms of
