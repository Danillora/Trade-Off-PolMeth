@article{ WOS:000215986700007,
Author = {Perez Sosa, Felipe Abelardo and Lara Gomez, Graciela and Gomez
   Bobadilla, Alfredo Tercero},
Title = {Corporate social responsibility and financial sustainability in Mexican
   cement industry},
Journal = {CIMEXUS},
Year = {2014},
Volume = {9},
Number = {1},
Pages = {105-125},
Month = {JAN-JUN},
Abstract = {The aim of this work was to determine if the adoption of Corporate
   Social Responsibility (CSR) has any impact on the financial
   sustainability of Mexican cement companies, which was evaluated with the
   Weighted Average Cost of Capital (WACC) and the Earnings Before
   Interests, Taxes, Depreciations and Amortizations (EBITDA). Among the
   analyzed companies, the one with better results in the CSR
   accomplishment was also the company with a stronger financial
   sustainability, that is, that had greater reductions in its financial
   cost and higher improvements in its EBITDA. Therefore, is accepted the
   hypothesis that states that the companies engaged with the CSR keep or
   improve their financial sustainability.},
