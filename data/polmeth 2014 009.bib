@article{ WOS:000339659000004,
Author = {Johal, Sukhdev and Moran, Michael and Williams, Karel},
Title = {Power, Politics and the City of London after the Great Financial Crisis},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2014},
Volume = {49},
Number = {3, SI},
Pages = {400-425},
Month = {JUL},
Abstract = {Four faces of power are summarized, based on the established literature
   in political science and the work of Foucault: they are power over
   decision, the power of agenda control, hegemonic power and capillary
   power. The four faces correspond also to four strategies used by City
   elites in the UK to protect markets from democratic control. Strategies
   have developed out of conjunctural crises. The most recent strategy,
   which involved a form of capillary power, was greatly damaged in the
   financial crisis of 2007-8. Since then the City has been obliged to
   retreat to a reliance on the exercise of power over decision, which
   involves open lobbying.},
