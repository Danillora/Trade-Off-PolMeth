@article{ WOS:000435234100002,
Author = {Fehrs, Matthew},
Title = {Too Many Cooks in the Foreign Policy Kitchen: Confused British Signaling
   and the Falklands War},
Journal = {DEMOCRACY \& SECURITY},
Year = {2014},
Volume = {10},
Number = {3},
Pages = {225-250},
Abstract = {Both Schelling's pioneering work on signaling credibility and Fearon's
   more recent work on this topic posit that democratic states should be
   able to more credibly threaten their opponents than nondemocratic
   states. Recent criticism of the democratic credibility thesis has
   highlighted shortcomings of the theory but has not offered a coherent
   theory explaining why democracies fail to signal clearly. Using the 1982
   Falklands/Malvinas War as a case study, this article introduces a new
   theory on governmental divisions to explain the role that failed British
   signaling played in the conflict. Divisions within Thatcher's foreign
   policy group simultaneously signaled an inability to effectively
   negotiate and an unwillingness to credibly deter the Argentines.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
