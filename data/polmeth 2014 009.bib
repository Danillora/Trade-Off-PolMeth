@article{ WOS:000210300400004,
Author = {Rus Rufino, Salvador},
Title = {Analysis and Political Theory in the Social Reforms of Solon of Athens},
Journal = {FORO INTERNO-ANUARIO DE TEORIA POLITICA},
Year = {2014},
Volume = {14},
Pages = {65-92},
Abstract = {The figure of Solon is crucial in the development of Greek political
   theory. His theoretical contribution arises from his own experience in
   government, his deep knowledge of the Athenian social milieu and his
   role as ruler at a critical stage in the early sixth century B.C.E. The
   purpose of this article is to present some conclusions regarding Solon's
   analysis of his historical and social reality, which led him to develop
   several key ideas that shaped one of the earliest political theories of
   Western thought. Solon's theory served to validate his activity and
   justify his decisions as both a government leader and a public figure.
   All this is reflected in his poems and in certain works of later
   authors.},
