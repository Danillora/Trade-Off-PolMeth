@article{ WOS:000340390800012,
Author = {Reuchamps, Min and Onclin, Francois and Caluwaerts, Didier and
   Baudewyns, Pierre},
Title = {Raising the Threshold, Fighting Fragmentation? Mechanical and
   Psychological Effects of the Legal Electoral Threshold in Belgium},
Journal = {WEST EUROPEAN POLITICS},
Year = {2014},
Volume = {37},
Number = {5},
Pages = {1087-1107},
Month = {SEP},
Abstract = {Many proportional representation systems are characterised by a legal
   electoral threshold. Such a threshold reserves the allocation of seats
   for those parties that reach a minimum share of the votes. In order to
   fight fragmentation, a 5 per cent threshold has been introduced for both
   federal and regional elections in Belgium. This article seeks to explore
   the mechanical and psychological effects of this legal threshold after
   five elections. It is shown that the threshold has had limited
   mechanical and psychological effects on voters but some psychological
   effects on party elites. Moreover, while in the short term the average
   number of lists dropped and several pre-electoral coalitions formed, in
   the longer term the legal threshold has not prevented further
   fragmentation.},
