@article{ WOS:000342563100011,
Author = {Farrell, Henry},
Title = {The Woodgrain of the Chessboard: A Response to Roy Germano},
Journal = {PERSPECTIVES ON POLITICS},
Year = {2014},
Volume = {12},
Number = {3},
Pages = {686-687},
Month = {SEP},
Abstract = {Roy Germano argues for an ``analytic filmmaking,{''} which would use
   film as a means to elucidate social scientific laws and generalizations.
   Yet film, even when it seeks simply to document the truth, is a form of
   narrative. To strip these ambiguities away in the name of a crude
   empiricism would rob these narratives of just that kind of information
   that makes them most valuable-the subtleties and nuances that they can
   capture and that simple transcripts cannot. Better models for
   understanding film can be found either in the direct acknowledgment and
   exploration of these ambiguities, or, alternatively, in the ``analytic
   narratives{''} approach. Unlike Germano's analytic film making, which
   prizes the abstract over the particular, analytic narratives gather
   their energy from the continuing tension between the particular and the
   abstraction.},
