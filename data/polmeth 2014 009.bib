@article{ WOS:000457934300011,
Author = {Rogozin, Dmitry and Galieva, Nadezhda},
Title = {Methodical (in)stability of Mass Survey},
Journal = {POLITEIA-JOURNAL OF POLITICAL THEORY POLITICAL PHILOSOPHY AND SOCIOLOGY
   OF POLITICS},
Year = {2014},
Volume = {74},
Number = {3},
Pages = {169+},
Abstract = {The article presents the results of an ethnographic post-observation of
   conducting standardized personal interviews on route sample. The authors
   reveal an enormous discrepancy between the reports provided by an
   interviewer and reviewed by the leading Russian survey company, and the
   realities of the post-survey situation. Almost none of the respondents
   were identified. Moreover, some apartments and houses were simply
   imputed out of nowhere. Any factual references to route sheets and
   questionnaires completed by an interviewer were absent. On the basis of
   replicating an interviewer's route, D.Rogozin and N.Galieva hypothesize
   about the insolvency of survey technology based on the manipulation of
   the collected data that are not correlated with the realities of the
   conducted survey.},
