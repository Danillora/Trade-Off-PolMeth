@article{ WOS:000343267600017,
Author = {Methmann, Chris and Oels, Angela},
Title = {A new climate for development? The ecological governmentality of
   development politics},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2014},
Number = {48},
Pages = {464+},
Abstract = {In this paper we offer a genealogical analysis of the changing
   problematization of environment and development over the last 30 years.
   From a governmentality perspective we demonstrate that three
   governmental rationalities have sedimented over time, which problematize
   the relationship between environment and development: first,
   ``sustainable development{''} from the Rio Earth Summit in 1992, second
   Kyoto Protocol and Johannesburg Rio+10 Summit as advanced liberal
   government, and third, the latest discussion about dangerous climate
   change as a threat to development, which we call ecopolitics of the
   apocalypse. We claim that the discourse of dangerous climate change is
   re-legitimising advanced liberal government in development policy.
   According to this discourse, the North has to invest in technological
   innovation and green markets, while the fundamental lifestyles and
   economic structures remain unchallenged. The South is called upon to
   become sustainable and resilient.},
