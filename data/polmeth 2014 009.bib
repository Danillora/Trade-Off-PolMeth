@article{ WOS:000219464700007,
Author = {Bee, Cristiano and Pachi, Dimitra},
Title = {Active Citizenship in the UK: Assessing Institutional Political
   Strategies and Mechanisms of Civic Engagement},
Journal = {JOURNAL OF CIVIL SOCIETY},
Year = {2014},
Volume = {10},
Number = {1, SI},
Pages = {100-117},
Abstract = {Shaping active citizenship, motivating civic engagement, and increasing
   political participation of minority groups have become some of the key
   political priorities in the UK since at least the end of the 1980s.
   Academic research shows that this shift goes hand-in-hand with a review
   of the integration policies in the country. The `politics of
   integration' correspond in fact to a policy response to various social
   problems (such as discrimination, racism, intolerance) that emerged in
   various areas, and represent a new political discourse regarding active
   citizenship. This reflects an overall strategy meant to reframe the
   basis for civic and political engagement and participation in Britain.
   Our article is thus meant to highlight the dynamics underlying the
   development of the concept of active citizenship in the UK by looking at
   the factors that intervene in its shaping and enhancement. We identify
   political priorities and key mechanisms of participation that enable
   engagement in the public sphere. This article first considers the
   development of the specific `British discourse' regarding active
   citizenship by taking into consideration the political priorities that
   emerged as part of the New Right discourse in the 1980s and then New
   Labour after 1997. We then refer to a set of data collected during our
   field work conducted in the UK between 2010 and 2011 with civil society
   activists and policy-makers in order to underline the meaning,
   practices, and feasibility of active citizenship.},
