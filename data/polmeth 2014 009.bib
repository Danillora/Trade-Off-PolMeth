@article{ WOS:000420554300002,
Author = {Klinger, Ulrike},
Title = {Informal regulation of media governance in Italy and Mexico: an
   interregional comparison},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2014},
Volume = {8},
Number = {1},
Pages = {35-59},
Month = {SEP},
Abstract = {In modern mass democracies, the principle of freedom of the press limits
   the scope of state intervention in the media sector. Presumably `soft'
   instruments of self-and informal regulation can be potential
   alternatives. But why do informal practices and institutions fail to
   promote media pluralism effectively? Both Italy and Mexico exhibit the
   highest rates of media concentration among OECD member states, both are
   considered defective democracies and both have a long tradition of
   informal regulation. The article concludes that informality does not
   serve as an effective regulatory mechanism of media pluralism. It
   obstructs the creation of transparent rules for new actors in the media
   sector. In addition, a lack of awareness and low professionalization in
   the media sector hamper the promotion of pluralism.},
