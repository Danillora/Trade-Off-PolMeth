@article{ WOS:000437168600005,
Author = {Lawrence, Eric D. and Sides, John},
Title = {The consequences of political innumeracy},
Journal = {RESEARCH \& POLITICS},
Year = {2014},
Volume = {1},
Number = {2},
Month = {JUL-SEP},
Abstract = {The prevalence of political innumeracy - or ignorance of politically
   relevant numbers - is well-documented. However, little is known about
   its consequences. We report on three original survey experiments in
