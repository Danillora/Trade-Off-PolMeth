@article{ WOS:000217020500012,
Author = {Reynisson, Gestur Pall and Kristmundsson, Omar H.},
Title = {Who becomes political advisor to Icelandic ministers?},
Journal = {ICELANDIC REVIEW OF POLITICS \& ADMINISTRATION},
Year = {2014},
Volume = {10},
Number = {2},
Month = {FAL},
Abstract = {The article examines the general background of political advisors to
   Icelandic ministers in the period 1971-2014 and how it has changed.
   Attributes such as age, education, and professional experience are
   analysed for the various governmental terms and by party affiliation.
   The authors propose a model explaining how the advisors' backgrounds
   affect the role they play as political advisors to ministers. The main
   findings are that the general background of political advisors has not
   changed much in the four decades under study. Nevertheless, the evidence
   reveals that it has become increasingly common for advisors to have a
   background of working in the media, while those with expert knowledge in
   the field of the ministry in question remain rare. The position of a
   political advisor seems to be a good launchpad for a career in politics
   later on.},
