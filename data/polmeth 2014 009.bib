@article{ WOS:000210675000002,
Author = {Parry-Giles, Trevor},
Title = {Presidentialism, Political Fiction, and the Complex Presidencies of
   Fox's 24},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2014},
Volume = {44},
Number = {2, SI},
Pages = {204-223},
Month = {JUN},
Abstract = {This article explores how the Fox television network program 24 offers a
   compelling yet oddly ambivalent vision of the U.S. presidency.
   Specifically, I examine 24's articulation of presidentialism in
   depictions of the nation's chief executive and reveal how those
   depictions are actually quite complex and layered. Ultimately, I suggest
   that as 24 continues to circulate as a meaningful popular culture text,
   it may also continue to influence how Americans see the presidency,
   offering its audiences a sense of the presidency that is conflicted and
   complicated, yet strangely reassuring in its vision of presidentialism
   and presidential authority.},
