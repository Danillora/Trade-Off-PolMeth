@article{ WOS:000211589400002,
Author = {Shaw, William},
Title = {Consequentialism, war, and national defense},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
Year = {2014},
Volume = {10},
Number = {1},
Pages = {20-37},
Month = {FEB},
Abstract = {What, if anything, grounds the right of national defense? This essay
   explicates and defends a consequentialist answer to that question. After
   explaining the relevance and importance of this project, I clarify the
   meaning of consequentialism and explain how consequentialists understand
   and justify rights, including the right of national defense.
   International law enshrines that right. After explaining why it is
   correct to do so and why that right should be upheld, I examine some
   issues that this right leaves unsettled and probe its moral limits. I
   then reply to several objections to a consequentialist approach to
   national defense, pursuing in particular the criticism that
   consequentialists cannot justify the Allies' having taken up defensive
   arms against the Axis powers in World War II.},
