@article{ WOS:000218598500003,
Author = {Chaturvedi, Neilan S.},
Title = {The next generation? A reexamination of religious influence on
   Mexican-American attitudes toward same-sex marriage},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2014},
Volume = {2},
Number = {4},
Pages = {589-606},
Abstract = {This article examines Mexican-American political attitudes on same-sex
   marriage (SSM). This article makes a significant contribution to the
   study of Latino politics by examining generational influences and
   reexamining the effects of religious preference on MexicanAmerican
   attitudes on SSM. Using data from the Pew Hispanic Forum in 2006 on
   religion, I find that Mexican-Americans in later generational cohorts
   (i.e., the second generation and beyond) are much more likely to be in
   favor of SSM than earlier cohorts. Likewise, age cohort replacement
   significantly affects SSM in addition to generational differences with
   younger cohorts supporting SSM at a greater rate than older age cohorts,
   though this is limited only to the first and second generations. I also
   find that Catholic opposition to SSM may have been overstated, while
   evangelical opposition to SSM is statistically significant, though
   strongly affected by generational differences. This has fundamental
   implications for the Republican Party, which has attempted to gain
   Latino support by pressing conservative moral positions onto the
   national agenda. Given the results of this study, such a strategy may
   backfire on the Republican Party over the long term.},
