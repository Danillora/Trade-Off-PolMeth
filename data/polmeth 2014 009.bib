@article{ WOS:000210094000005,
Author = {Leitao, Geronimo and Delecave, Jonas},
Title = {Organized occupations of urban land in Rio de Janeiro, in the 1980's
   and 1990's},
Journal = {AMERICA LATINA HOY-REVISTA DE CIENCIAS SOCIALES},
Year = {2014},
Volume = {68},
Pages = {79-93},
Month = {DEC},
Abstract = {This paper addresses the organized occupation of unused urban areas,
   mostly public, in the city of Rio de Janeiro and adjacencies, between
   the early 1980s and mid-1990s. These occupations, of strong political
