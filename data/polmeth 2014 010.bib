@article{ WOS:000219136000019,
Author = {Petrovna, Bakhchivanzhi Valeria},
Title = {Space Identity: The Case of Vyborg},
Journal = {SRAVNITELNAYA POLITIKA-COMPARATIVE POLITICS},
Year = {2014},
Volume = {5},
Number = {4},
Pages = {83-85},
Abstract = {Among the unique meanings and images which form the Vyborg space by the
   citizens one can distinguish at least the images - an European town, a
   multicultural town and a town with the special exclusiveness, moreover
   so, they coexist successfully. In other words, although local identity
   prevails in Vyborg, its space is unique because there are several types
   of identity. Over the recent decades the space has been re-interpreted:
   defense functions of the town (or solely of the fortress town) have
   evolved into connecting, contact, which is demonstrated by the fact that
   Vyborg represents a borderzone between cultures, traditions and values.},
