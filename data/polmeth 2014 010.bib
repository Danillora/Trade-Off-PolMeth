@article{ WOS:000420260000008,
Author = {Salgado, Alfonso},
Title = {Ramona Parra's family in Plaza Bulnes: A gender approach to political
   activism, social protest and state violence in the twentieth century
   Chile},
Journal = {IZQUIERDAS},
Year = {2014},
Volume = {18},
Pages = {128-145},
Month = {APR},
Abstract = {This article reflects on social protest, political activism, and state
   violence in twentieth-century Chile, using the well-known but
   little-studied massacre of Plaza Bulnes as a case study. Drawing on the
   concept of gender, I highlight the masculine ethos of protesting workers
