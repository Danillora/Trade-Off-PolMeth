@article{ WOS:000344308600004,
Author = {Arioli, Daniel H.},
Title = {The First Ruler and the Prophet: On the Identity-in-Difference of
   Politics, Religion, and Philosophy in the Thought of Alfarabi and Ibn
   Sina},
Journal = {POLITY},
Year = {2014},
Volume = {46},
Number = {4},
Pages = {547-561},
Month = {OCT},
Abstract = {This article explores some discrepancies between Alfarabi's and Ibn
   Sina's treatments of the political sphere and of the role of the first
   ruler or prophet in establishing and maintaining the political order.
   While Alfarabi makes much of the religious function of the first ruler
   and calls him by a secular name, Ibn Sina treats the first ruler as
   little more than the guarantor of the trustworthiness of contract law,
   yet consistently refers to him as ``prophet.{''} Alfarabi, in other
   words, is content with secular language, even though his aims are lofty,
   whereas Ibn Sina employs elevated, religious language, even though his
   aims appear more realistic. This article unveils an irony: that
   fundamentally similar understandings of the political situation of man
   and of the philosopher in relation to the city-can account for the
   various differences between these thinkers' emphases and modes of
   presentation.},
