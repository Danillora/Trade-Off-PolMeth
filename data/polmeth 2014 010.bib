@article{ WOS:000215842600008,
Author = {Vazquez Ceballos, Cristo Avimael},
Title = {Governance and Policy Networks: A study of the Network among Government,
   Public-social actors and Private actors at a local tourist town},
Journal = {REVISTA MEXICANA DE ANALISIS POLITICO Y ADMINISTRACION PUBLICA},
Year = {2014},
Volume = {3},
Number = {1},
Pages = {147-178},
Month = {JAN-JUN},
Abstract = {Taking as a starting point the policy network framework, I analyze some
   governance features. I use the social network analyses to systematize a
   policy network at the local government level; the empirical reference is
   a public action in the tourist town of Valle de Bravo. The conclusions
   suggest that it has been an alternative structure to address public
   affairs because there are non-governmental actors with power and control
   taking part in the public action. These actors are more important than
   the government.},
Publisher = {UNIV GUANAJUATO},
Address = {LASCURAIN RETANA 5, CENTRO, GUANAJUATO, 36000, MEXICO},
