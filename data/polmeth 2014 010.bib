@article{ WOS:000344380100002,
Author = {Jenco, Leigh K.},
Title = {Histories of Thought and Comparative Political Theory: The Curious
   Thesis of ``Chinese Origins for Western Knowledge,{''} 1860-1895},
Journal = {POLITICAL THEORY},
Year = {2014},
Volume = {42},
Number = {6},
Pages = {658-681},
Month = {DEC},
Abstract = {How is cultural otherness any different from the historical otherness
   already found in our existing canons of thought? This essay examines an
   influential Chinese conversation that raised a similar question in
   struggling with its own parochialism. Claiming that all ``Western{''}
   knowledge originated in China, these Chinese reformers see the
   differences presented by foreign knowledge as identical to those already
   authorizing innovation within their existing activity of
   knowledge-production. Noting that current academic theory-production
   treats the otherness of past authors in a similar way, I argue that we
   must broach something like a China-origins claim if we are to see
   typically marginalized ({''}non-Western{''}) thought as part of what
   disciplines our thought, rather than serves simply as its target of
   inclusion. Doing so, we blur self/foreign binaries and enable future
   innovation of thought on radically new terms.},
