@article{ WOS:000341648400002,
Author = {Colley, Linda Katurah and Head, Brian},
Title = {Privatisation and new public management: Asset sales and Commissions of
   Audit in Queensland, 1990-2013},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2014},
Volume = {49},
Number = {3},
Pages = {391-407},
Month = {SEP},
Abstract = {Governments since the 1980s have reshaped the public sector by selling
   public businesses and other assets, and by introducing competition in
   the supply of taxpayer-funded services. Explanations for these new
   directions have focused on political ideologies about the proper role of
   government, and economic arguments about business efficiency, while
   noting an international trend towards diffusion of these policy ideas.
   However, the methods for achieving significant policy shifts have often
   been overlooked. This study examines the case of Queensland,
   highlighting a wider range of reform factors, including bureaucratic
   advice and commission of audit recommendations.},
