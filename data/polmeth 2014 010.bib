@article{ WOS:000420762600004,
Author = {Rota, Giuseppina and Pellegrini, Silvia and Pietrini, Pietro},
Title = {The anti-social brain: Novel insights from neuroscience and
   molecular biology},
Journal = {POLITICA \& SOCIETA},
Year = {2014},
Volume = {3},
Number = {2},
Pages = {201-220},
Month = {MAY-AUG},
Abstract = {Human beings are social animals. Life in society requires individuals to
   modulate their behavior accordingly to the norms that rule social
   contexts. Despite being generally sanctioned, violence remains a major
   issue worldwide. Advances in neuroscience and molecular biology are
   shedding new light on the biological mechanisms that underlie poor
   inhibitory control in impulsive aggression and the lack of empathy and
   emotional resonance in predatory violence. The increase in
   neuroscientific knowledge on the biological underpinnings of impulse
   control, moral judgment and social behavior may contribute to a wider
   understanding of the notion of free-will and responsibility. Here, we
   briefly discuss novel findings on the neurobiological correlates of
   aggressive and antisocial behavior and their potential ethical, forensic
   and political implications.},
