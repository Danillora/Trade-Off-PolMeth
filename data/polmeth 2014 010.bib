@article{ WOS:000344690900001,
Author = {Vigour, Cecile},
Title = {Veto Players and Interest Groups in Lawmaking: A Comparative Analysis of
   Judicial Reforms in Italy, Belgium, and France},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2014},
Volume = {47},
Number = {14},
Pages = {1891-1918},
Month = {DEC},
Abstract = {The question of how to explain policy change has led to important
   academic debate over the past two decades. This article challenges
   explanations regarding veto players (VPs), arguing that interest groups
   (IGs) sometimes play a more decisive role than VPs depending on the
   degree of organization and mobilization of such groups. The comparative
   analysis of judicial reforms in Italy, Belgium, and France shows the
   conditions under which IGsunderstood here as legal professionsmatter in
   the lawmaking process. Ceteris paribus, the more cohesive and legitimate
