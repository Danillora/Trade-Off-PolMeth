@article{ WOS:000420558800004,
Author = {Sieberer, Ulrich and Meissner, Peter and Keh, Julia F. and Mueller,
   Wolfgang C.},
Title = {Conceptualizing and measuring change in formal institutions: The case of
   parliamentary rules},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2014},
Volume = {8},
Number = {3-4},
Pages = {237-262},
Month = {DEC},
Abstract = {Describing and explaining institutional change constitutes a major
   challenge for the new institutionalist research program. This article
   discusses key conceptual, methodological, and practical issues in
   analyzing change in formal institutions. Conceptually, it suggests four
   analytical approaches that differ with regard to the trade-off between
   breadth of coverage and analytical depth. Methodologically, we develop
   new software tools that allow a largely automated identification of
   changes in formal rules and that permit a valid, quantitative
   measurement of the amount of changes in a text. We demonstrate the
   application of the four approaches and the new tools using data on
   changes in parliamentary rules in European democracies and discuss
   further fields of application.},
