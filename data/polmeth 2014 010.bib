@article{ WOS:000457934300005,
Author = {Korgunyuk, Yury},
Title = {Regional Map of Electoral Cleavages At 2011 State Duma Elections},
Journal = {POLITEIA-JOURNAL OF POLITICAL THEORY POLITICAL PHILOSOPHY AND SOCIOLOGY
   OF POLITICS},
Year = {2014},
Volume = {74},
Number = {3},
Pages = {75+},
Abstract = {Structure of electoral cleavages is an important indicator of the
   socio-political state of society. However, studying this topic at the
   level of a country at large is fraught with serious mistakes: if
   elections are fair in some places and merely imitative in others, the
   overall picture may be akin to the average temperature in the hospital.
   To solve this problem, Yu.Korgunyuk turns to the analysis of data at the
   level of territorial electoral commissions employing his own
   methodology. His research results in a type of regional map of electoral
   cleavages that allows to understand in which regions of the Russian
   Federation electoral life is still lingering on, and in which its demise
   has become irreversible.},
