@article{ WOS:000214606300006,
Author = {Borang, Frida and Eising, Rainer and Kluver, Heike and Mahoney,
   Christine and Naurin, Daniel and Rasch, Daniel and Rozbicka, Patrycja},
Title = {Identifying frames: A comparison of research methods},
Journal = {INTEREST GROUPS \& ADVOCACY},
Year = {2014},
Volume = {3},
Number = {2, SI},
Pages = {188-201},
Month = {JUN},
Abstract = {Framing plays an important role in public policy. Interest groups
   strategically highlight some aspects of a policy proposal while
   downplaying others in order to steer the policy debate in a favorable
   direction. Despite the importance of framing, we still know relatively
   little about the framing strategies of interest groups due to
   methodological difficulties that have prevented scholars from
   systematically studying interest group framing across a large number of
   interest groups and multiple policy debates. This article therefore
   provides an overview of three novel research methods that allow
   researchers to systematically measure interest group frames. More
   specifically, this article introduces a word-based quantitative text
   analysis technique, a manual, computer-assisted content analysis
   approach and face-to-face interviews designed to systematically identify
   interest group frames. The results generated by all three techniques are
   compared on the basis of a case study of interest group framing in an
   environmental policy debate in the European Union.},
