@article{ WOS:000337573100009,
Author = {Peucker, Mario and Roose, Joshua M. and Akbarzadeh, Shahram},
Title = {Muslim active citizenship in Australia: Socioeconomic challenges and the
   emergence of a Muslim elite},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2014},
Volume = {49},
Number = {2},
Pages = {282-299},
Month = {JUN},
Abstract = {The most recent national Census demonstrated that Australian Muslims
   continue to occupy a socioeconomically disadvantaged position. On key
