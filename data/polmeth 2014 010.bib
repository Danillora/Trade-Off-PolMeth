@article{ WOS:000443310000001,
Author = {Ivanovic, Josip},
Title = {THE IDEA OF JUSTICE WITHIN THE POLITICAL MORALITY OF LIBERAL
   EGALITARIANISM},
Journal = {POLITICKA MISAO-CROATIAN POLITICAL SCIENCE REVIEW},
