@article{ WOS:000214695800001,
Author = {Engdahl, F. William and Zhen, Wang},
Title = {The Rise of China and World Order: An Interview with F. William Engdahl},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2014},
Volume = {4},
Number = {2},
Pages = {131-141},
Abstract = {In order to encircle the German Reich in the period from the 1880s to
   World War I, Britain's strategies included the inciting of war itself to
   defeat the growing might of Germany. It used all financial, military,
   and political means simply because Germany had emerged as an economic
   challenger to British hegemony. Mr Engdahl's understanding of that
   history and how tragic it was for the world prompted him to write Target
   China (Engdahl 2013) as he realized that a very similar strategy has
   been adopted by the West, with Washington today replacing London,
   against the emergence of a sovereign Chinese economic great power. In
   the context of his book, Mr Engdahl shares his views on some significant
   issues of the contemporary world geopolitical situation in this
   interview.},
