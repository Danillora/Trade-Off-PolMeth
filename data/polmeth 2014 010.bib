@article{ WOS:000421326500005,
Author = {Federico Jaef, Jorge},
Title = {Violence as a decisive factor of politics: reflections from Niccolo
   Machiavelli's The Prince},
Journal = {TEMAS Y DEBATES},
Year = {2014},
Number = {27},
Pages = {125-135},
Month = {JAN-JUN},
Abstract = {This communication attempts to analyse the implications of Niccolo
   Machiavelli's thoughts as pictured in The Prince and how these remain
   valid nowadays. The author's starting point is the question about why
   violence is necessary for the survival of a political regime.
