@article{ WOS:000215420000001,
Author = {Gonzalez Calleja, Eduardo},
Title = {Real Madrid, ``team of Spain{''}? Football and identities during the
   francoism},
Journal = {POLITICA Y SOCIEDAD},
Year = {2014},
Volume = {51},
Number = {2},
Pages = {275-296},
Abstract = {In the 50s and 60s of the last century, Real Madrid Club of Football
   became the sportive herald of ``Spanish miracle{''}, generating in Spain
   and abroad a wave of admiration that was promptly capitalized by the
   Franco regime. During the Francoism, the club became one of the most
   effective disseminators of ``banal nationalism{''} that the dictatorship
   would inculcate in the population, especially the emigrants abroad. This
   image of Real Madrid as an internationally successful sport entity
   allows us to reflect on the relationships that are established between
   sport and political power, especially in the elusive theme of national
   identities.},
