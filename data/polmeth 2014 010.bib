@article{ WOS:000354227800008,
Author = {Laplanche-Servigne, Soline},
Title = {When the ``victims of racism{''} mobilize: ethnoracial identifications
   in the antiracist struggle in France and in Germany},
Journal = {POLITIX},
Year = {2014},
Volume = {27},
Number = {108},
Pages = {143+},
Abstract = {Based on a comparative study carried out in France and Germany, this
   article explores the mobilisations of ``victims of racism{''}. It shows
   how in the last two decades the lived experience of racism has became a
   pathway for asserting claims in antiracist struggles. ``Lived
   experience{''} is expressed through ethnoracial identifications, which
   converts racialised bodies into a political ressource. This comparative
   study between France and Germany shines light upon nationally
   contextualised processes which aim to introduce racialised points of
   view in the field of the national antiracist struggle.},
