@article{ WOS:000344371200006,
Author = {Erisen, Cengiz and Villalobos, Jose D.},
Title = {Exploring the invocation of emotion in presidential speeches},
Journal = {CONTEMPORARY POLITICS},
Year = {2014},
Volume = {20},
Number = {4},
Pages = {469-488},
Month = {OCT 2},
Abstract = {Scholars have long explored why presidential rhetoric is important and
   how it matters for public leadership and policy-making. However,
   relatively few works have considered the role that emotion plays in
   leadership communication and no research has conducted a thorough
   examination of the various types of emotions invoked in presidential
   rhetoric, their frequency, or how they have shaped presidential
   discourse over time. In this study, presidential speeches across 13
   administrations (1933-2011) are examined to provide a first assessment
   of the extent to which US presidents have invoked fear, anger, and hope
   across policy domains and key types of speeches.},
