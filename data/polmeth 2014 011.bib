@article{ WOS:000342040600022,
Author = {Meister, Stefan},
Title = {Lessons from the Crisis Ukraine, Russia, and the European Union},
Journal = {OSTEUROPA},
Year = {2014},
Volume = {64},
Number = {5-6},
Pages = {323+},
Month = {MAY-JUN},
Abstract = {Russia's unlawful annexation of the Crimea, the destabilization of
   eastern Ukraine, and the propaganda campaign against both the Maidan
   movement and the `West{''} have caused a deep crisis in international
   relations. The estrangement between Russia and the European Union has
   grown worse. Germany and the EU should use this as an opportunity to
   correct their misperceptions about the character and interests of the
   Putin system, readjust their policy towards Ukraine and their eastern
   neighbors, and define long-term strategic options for developing these
   countries. This would give Germany and the EU a chance to recover their
   credibility and capacity for action.},
