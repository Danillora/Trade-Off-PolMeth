@article{ WOS:000332447900007,
Author = {Kim, Jung Hyoun},
Title = {The Comparative Analysis of Public Support for Developing Regional
   Regime in East Sea Rim (Sea of Japan) Region},
Journal = {JAPANESE JOURNAL OF POLITICAL SCIENCE},
