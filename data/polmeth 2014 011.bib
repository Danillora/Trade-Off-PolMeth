@article{ WOS:000214695800003,
Author = {Brie, Michael},
Title = {Towards a Plural and Polycentric World of Self-Organising Actors Elinor
   Ostrom's Research Programme},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2014},
Volume = {4},
Number = {2},
Pages = {160-177},
Abstract = {The paper analyses the work of Elinor Ostrom along four major lines: the
   new concepts (1) of the pluralism of goods overcoming the dichotomy of
   public vs. private goods, (2) of the plurality of institutions in
   difference to the dichotomy of state vs. markets, (3) of bounded
   rational human agency combining self-related and other-related
   motivations in difference to the assumption of omniscient rational
   egoists, and (4) the vision of polycentric orders. These are
   cornerstones in the development of a different social science combining
   strong theory with broad and in-depth empirical research. Limitations
   within Ostrom's approach are discussed that could stand in the way of
   expanding her approach into an encompassing paradigm for a heterodoxical
   social science that could spur a social and ecological transformation of
   modern societies.},
