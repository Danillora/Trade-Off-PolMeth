@article{ WOS:000341648400005,
Author = {Tate, John William},
Title = {Paul Keating and leadership: Was the `personal' political?},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2014},
Volume = {49},
Number = {3},
Pages = {439-454},
Month = {SEP},
Abstract = {This article proposes three possible reasons why political leaders abide
   by policy decisions in the face of vigorous opposition - because they
   perceive the policy to be `right', because abiding by a decision is
   necessary to appear `strong', or because of personal motives arising
   from the leader themselves. The article applies this framework to Paul
   Keating's apparent refusal, as Treasurer in 1989, to consider mortgage
   relief in the midst of prohibitively high interest rates arising from
   monetary policy at that time. The article concludes that it is the
   `personal' dimension that most explains Keating's decision-making on
   this issue. This is a significant finding given that such personal
   motives are incapable of legitimation in liberal democratic terms, yet
   policy decisions arising from them can have an immense impact.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
