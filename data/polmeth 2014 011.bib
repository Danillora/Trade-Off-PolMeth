@article{ WOS:000343696600006,
Author = {Geys, Benny and Mause, Karsten},
Title = {Are Female Legislators Different? Exploring Sex Differences in German
   MPs' Outside Interests},
Journal = {PARLIAMENTARY AFFAIRS},
Year = {2014},
Volume = {67},
Number = {4},
Pages = {841-865},
Month = {OCT},
Abstract = {This article examines whether sex matters with respect to a type of
   legislator behaviour that has thus far been neglected in the literature
   analysing the distinctive nature of female and male legislators:
   parliamentarians' outside interests. Using data for 614 German Members
   of Parliament (MPs), our analysis confirms that female MPs on average
   hold fewer outside jobs than men-especially in private-sector functions.
   We also find that individual characteristics such as political
   experience, having (young) children and age reflect sources of this
   divergence. These findings and their implications are discussed in the
   light of extensive research on sex and gender effects in other political
   and labour market settings.},
