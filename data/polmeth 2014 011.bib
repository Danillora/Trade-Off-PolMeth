@article{ WOS:000330793200004,
Author = {Celis, Karen and Erzeel, Silvia and Mugge, Liza and Damstra, Alyt},
Title = {Quotas and intersectionality: Ethnicity and gender in candidate
   selection},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2014},
Volume = {35},
Number = {1},
Pages = {41-54},
Month = {JAN},
Abstract = {Gender equality is not fully realised when it is restricted to ethnic
   majority men and women. This article examines how gender quotas as a
   form of equality policy affect ethnic minority groups, in particular,
   the gender balance among ethnic minority candidates for political
   office. Our analysis focuses on the selection of ethnic minority
   candidates in Belgium, where legally binding quotas exist, and in the
   Netherlands, where they do not. Drawing on 23 interviews with central
   actors in four main parties in each country, we find that the process of
   ethnic minority candidate selection is highly gendered: in both
   countries, ethnic minority women are represented in larger numbers than
   ethnic minority men. But gender quotas play a lesser role in this than
   the more general concern for diversity on electoral lists, the
   institutionalisation of gender/ethnicity within political parties and
   the strategic choices of party leaders.},
Publisher = {SAGE PUBLICATIONS LTD},
