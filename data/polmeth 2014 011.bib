@article{ WOS:000218596400005,
Author = {Gutterman, David S. and Murphy, Andrew R.},
Title = {The ``Ground Zero mosque{''}: sacred space and the boundaries of
   American identity},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2014},
Volume = {2},
Number = {3},
Pages = {368-385},
Abstract = {For much of 2010, plans to develop a multipurpose Islamic center near
   the site of the 9/11 attacks in Lower Manhattan occasioned an intense
   nationwide debate. In this study of the ``Ground Zero mosque{''}
   controversy, we focus on questions of identity and the sacred in the
   contemporary USA. We argue that the response to the ``Ground Zero
   mosque{''} illuminates three important phenomena. First, it reveals the
   dynamics of cultural guardianship among those who opposed Park51, an
   effort to ``preserve{''} a national identity tightly linked with
   Christianity (and antithetical to Islam). Second, as illustrated by the
   rights-based response by defenders of the project, we suggest that the
   language of liberalism may be insufficient to address a more expansive
   and complex understanding of the relationship between religion and
   political identity. And third, the controversy highlights the challenges
   that remain in the post-9/11 USA for American Muslims, many of whom
   understood the proposed center as their own, rather conventional,
   attempt to join the American mainstream.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Gutterman, DS (Corresponding Author), Willamette Univ, Dept Polit, 900 State St, Salem, OR 97301 USA.
   Gutterman, David S., Willamette Univ, Dept Polit, 900 State St, Salem, OR 97301 USA.
   Murphy, Andrew R., Rutgers State Univ, Dept Polit Sci, New Brunswick, NJ 08901 USA.},
DOI = {10.1080/21565503.2014.926822},
ISSN = {2156-5503},
EISSN = {2156-5511},
Keywords = {Ground Zero; mosque; identity; religion; liberty; rights; sacred; 9/11},
Keywords-Plus = {RELIGION},
Research-Areas = {Government \& Law},
Web-of-Science-Categories  = {Political Science},
Author-Email = {dgutterm@willamette.edu},
Affiliations = {Willamette University; Rutgers University System; Rutgers University New
   Brunswick},
ORCID-Numbers = {Murphy, Andrew/0000-0002-5569-6098},
Funding-Acknowledgement = {Willamette University Center for Religion, Law Democracy},
Funding-Text = {The authors wish to thank the anonymous reviewers at Politics, Groups,
   and Identities, as well as Claire Hamed, Jan Kubik, Keally McBride,
