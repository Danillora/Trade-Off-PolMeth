@article{ WOS:000340264000002,
Author = {Burns, Natasha and Jindra, Jan},
Title = {Political Spending and Shareholder Wealth: The Effect of the US Supreme
   Court Ruling in Citizens United},
Journal = {AMERICAN POLITICS RESEARCH},
Year = {2014},
Volume = {42},
Number = {4},
Pages = {579-599},
Month = {JUL},
Abstract = {We examine the impact of the Citizens United decision on firm value.
   While the value of U.S. firms do not respond significantly to the
   Citizens United decision on average, we find evidence that firms in
   industries subject to more extensive regulation react significantly and
   positively to the announcement. We also find evidence consistent with
   Justice John Paul Stevens' argument that the Court decision will affect
   state laws. Specifically, our results indicate that firms that are
   headquartered in states with more stringent limits on political spending
   by corporations respond positively to the announcement.},
