@article{ WOS:000421040300003,
Author = {Licea Dorantes, Silvestre},
Title = {Allies or foes? An approach to the relationship between the governor and
   his political party},
Journal = {REVISTA ESPANOLA DE CIENCIA POLITICA-RECP},
Year = {2014},
Number = {36},
Pages = {63-88},
Month = {NOV},
Abstract = {We analyze the conditions under which the governor and his political
   party, both locally and nationally, cooperate or engage in conflict. To
   explain this relationship both rational choice theory in an
   institutional context are utilized, and in order to contrast the
   theoretical development, we have chosen three case studies of Mexican
   governors. The behavior of these actors is in line with the strategies
   in a game, which allows us to understand the actions followed. As a
   result of the above, we can see that there are conditions that
   precipitate confrontation between these actors, and in some cases a
   break in relations. It can be concluded that the levels of reciprocity
   in the exchange of benefits between the governor and his party
   determines the degree of mutual support.},
