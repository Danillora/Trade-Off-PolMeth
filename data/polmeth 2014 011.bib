@article{ WOS:000212003500005,
Author = {Zhemukhov, Sufian},
Title = {Ethnic and state sports in the context of the 2014 Sochi Olympics},
Journal = {NATIONAL IDENTITIES},
Year = {2014},
Volume = {16},
Number = {4},
Pages = {349-364},
Abstract = {Sports have long been an important point of expression in communal
   identity in the North Caucasus and recently came to serve as a forum for
   political dissent. This article examines the emergence and
   transformation of ethnic sport projects in the North Caucasus in
   connection with the 2014 Sochi Olympics. It charts the history and
   progress of two projects known as the Circassiada and the Caucasus Games
   and evaluates the debate of the sport-nation nexus in the North
   Caucasus. While the Circassiada and the Caucasus Games reflect the
   typical elite-driven narratives of border security and terrorism, they
   also highlight discussions over historical and cultural practices of
   nationhood and the boundaries of national identity.},
