@article{ WOS:000212653200002,
Author = {Dickovick, J. Tyler},
Title = {Federalism in Africa: Origins, Operation and (In)Significance},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2014},
Volume = {24},
Number = {5, SI},
Pages = {553-570},
Abstract = {Recent years have seen major advances in the comparative study of
   federalism and a growing literature on decentralization in Africa, but
   there has been surprisingly little systematic comparison of African
   federalism. This article explains several commonalities in the origins
   and operation of Africa's three main federal states: Ethiopia, Nigeria
   and South Africa. Each country used `holding-together' federalism in
   order to accommodate ethnic pluralism. Each country-especially Ethiopia
   and South Africa-also experienced several key centripetal forces:
   dominant governing parties, top-down state administration and high
   degrees of fiscal centralism. Federalism mattered in offering
   accommodative decentralization, but in its operation subnational
   governments have limited autonomy because of these interlocking
   centralizing features. This African variant of federalism can have
   certain salutary features, even as it precludes the possibility of many
   of the theorized advantages of federalism that are predicated on real
   subnational autonomy.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
