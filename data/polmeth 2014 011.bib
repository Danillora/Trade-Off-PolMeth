@article{ WOS:000420761000007,
Author = {Caruso, Loris},
Title = {Neo-Bonapartism and Radical Democracy. Gramsci's Political Science and
   the Crisis of Contemporary Political Systems},
Journal = {POLITICA \& SOCIETA},
