@article{ WOS:000339655300004,
Author = {Feldmann, Magnus},
Title = {Coalitions and Corporatism: The Slovenian Political Economy and the
   Crisis},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2014},
Volume = {49},
Number = {1},
Pages = {70-91},
Month = {JAN},
Abstract = {Slovenia stands out as the only post-communist country to have
   established a corporatist system and centralized wage bargaining at the
   national level in the 1990s. This article analyses the emergence and
   sustainability of Slovenian corporatism as well as the ways in which it
   has shaped policymaking during the economic crisis. Drawing on recent
   advances in institutional analysis, this article develops a coalitional
   argument to account for the emergence of centralized wage bargaining in
   the 1990s and for decentralization in more recent years.},
