@article{ WOS:000327585700004,
Author = {Lefkofridi, Zoe and Wagner, Markus and Willmann, Johanna E.},
Title = {Left-Authoritarians and Policy Representation in Western Europe:
   Electoral Choice across Ideological Dimensions},
Journal = {WEST EUROPEAN POLITICS},
Year = {2014},
Volume = {37},
Number = {1},
Pages = {65-90},
Month = {JAN 2},
Abstract = {Citizens can face a difficult electoral decision when no party even
   broadly represents their views. In Western Europe, this applies to those
   citizens with left-wing preferences on economic issues and
   traditional/authoritarian preferences on socio-cultural issues. There
   are many voters with such left-authoritarian' views, but few parties.
   Hence, the former often have to choose between parties that only match
   their views on one of these two ideological dimensions. This study shows
   that whether these citizens privilege economic or socio-cultural
   congruence in their electoral preferences depends on the issues they are
   concerned about. In general, it is found that left-authoritarians
   privilege economic concerns and therefore prefer parties that are
   left-liberal. These findings have implications for our general
   understanding of electoral choice and of changing patterns of political
   competition in Western Europe.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
