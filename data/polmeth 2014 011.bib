@article{ WOS:000421258000016,
Author = {Celik, Kenan},
Title = {TURKISH-AUSTRALIAN RAPPROCHEMENT IN LIGHT OF THE GALLIPOLI CAMPAIGN},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2014},
Volume = {13},
Number = {1},
Pages = {165-170},
Month = {SPR},
Abstract = {People from Turkey, Australia, New Zealand, and Great Britain come to
   Gallipoli to commemorate their fallen soldiers who were lost nearly one
   hundred years ago, in 1915, during the Great War. This article
   elaborates on the rediscovery of the Gallipoli campaign by Australians,
   New Zealanders, and Turks in the 1980s. The collective remembrance
   enacted by these peoples, divided by nationalities but united by
   history, provides an exemplary precedent of reconciliation that can
   extend to all parts of the world.},
