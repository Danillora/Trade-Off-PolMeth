@article{ WOS:000211589600003,
Author = {Hjorth, Ronnie},
Title = {The poverty of exceptionalism in international theory},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
Year = {2014},
Volume = {10},
Number = {2},
Pages = {169-187},
Month = {JUN},
Abstract = {This article is about the politics of `the exception' and the role of
   `exceptionalism' in contemporary international theory. The concept of
   `the exception' was coined by Carl Schmitt and has in recent years
   become an inspiration for international relations theorists and foreign
   policy analysts, especially when engaging with issues such as great
   power politics, humanitarian intervention and the war against terrorism.
   It is concluded that attempts to apply Schmitt's concept of `the
   exception' seldom are persuasive and sometimes even contradictory to
   Schmitt's theory. When dealt with out of context, `the exception'
