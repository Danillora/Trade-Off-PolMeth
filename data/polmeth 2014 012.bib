@article{ WOS:000216244700006,
Author = {Sigmirean, Cornel},
Title = {ROMANIANS FROM TRANSYLVANIA AND HIGHER EDUCATION IN THE FIRST HALF OF
   NINETEENTH CENTURY},
Journal = {ANALELE UNIVERSITATII BUCURESTI-STIINTE POLITICE},
Year = {2014},
Volume = {16},
Number = {1},
Pages = {99-111},
Abstract = {The Romanians in Transylvania accomplished in the 19th century the
   transition from the old self-sufficient rural society to a society
   dominated by bourgeoisie values. The intellectual elite, trained in the
   educational institutions of the Habsburg Empire, in Vienna, Budapest,
   Oradea, Cluj, Schemmitz, is the expression of the assumption of these
   new values. Overall, more than 2041 Romanians studied in the first half
   of the 19th century in the colleges and universities of the Empire.},
