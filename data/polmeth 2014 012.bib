@article{ WOS:000218232800014,
Author = {Pennell, Catriona},
Title = {A TRULY SHARED COMMEMORATION? BRITAIN, IRELAND AND THE CENTENARY OF THE
   FIRST WORLD WAR},
Journal = {RUSI JOURNAL},
Year = {2014},
Volume = {159},
Number = {4},
Pages = {92-100},
Abstract = {During the First World War, around 35,000 Irishmen - hailing from both
   the north and south of the island - gave their lives fighting for the
   British Empire, of which Ireland was then a part. Many tens of thousands
   more served in Britain's armed forces in theatres around the globe, and
   especially on the Western Front. Yet this contribution has only recently
   been recognised by the general public in Ireland; and is largely
   overlooked by that in Britain. Catriona Pennell explores how tempestuous
   relationships and political division throughout the twentieth century
   have coloured the memory of the war within the British Isles.},
