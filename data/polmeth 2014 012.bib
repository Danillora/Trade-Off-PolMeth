@article{ WOS:000340441700005,
Author = {Hanson, Peter C.},
Title = {Abandoning the Regular Order: Majority Party Influence on Appropriations
   in the US Senate},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2014},
Volume = {67},
Number = {3},
Pages = {519-532},
Month = {SEP},
Abstract = {There is a debate about the extent of the Senate majority party's
   influence and the conditions under which it is used. This article
   demonstrates that the majority party influences legislative outcomes to
   meet electoral goals and protect its reputation by manipulating the
   consideration of the dozen annual appropriations bills. The majority
   party limits amendments and eases passage of the budget when it abandons
   the ``regular order{''} and creates an omnibus appropriations bill. It
   is more likely to abandon the regular order when it is ideologically
   divided, has a small margin of control, or is distant from the minority.},
