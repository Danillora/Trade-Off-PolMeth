@article{ WOS:000210300400007,
Author = {Santander, Pablo and Ravinet, Javier},
Title = {The Case of `The Young Butler'. Pyschoanalytic Reflections regarding the
   Complexity of Evil},
Journal = {FORO INTERNO-ANUARIO DE TEORIA POLITICA},
Year = {2014},
Volume = {14},
Pages = {147-162},
Abstract = {This article establishes an interdisciplinary dialogue between several
   disciplines, including journalism, philosophy and psychoanalysis. The
   case analysis proposes the existence in individuals of a feared
   unconscious phantasy in which, under certain conditions, the individual
   seeks to avoid deep anxieties generated by contact with dead internal
   objects. The authors explore how submission to an authority of
   tyrannical characteristics may become a way of escaping these anxieties.
   Hannah Arendt's concept of the banality of evil is presented and
