@article{ WOS:000214700300009,
Author = {Miao, Zhou},
Title = {The Ideological Functions of Teaching and Research in American
   Universities},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2014},
Volume = {4},
Number = {4},
Pages = {510-518},
Abstract = {American higher education is a highly developed system marked by
   flexibility and comprehensiveness. Although there is a great deal worth
   learning from the American university system, China should also
   recognize clearly the essence of American universities, and the function
   they perform. Only in this way can China chart a correct course for the
   reform of university education. American universities are a key part of
   the national ideological apparatus, allowing important sectors of the
   bourgeoisie to exercise cultural leadership. The governance structure
   and mechanisms of American universities aid it in effectively performing
   a range of hidden functions. The American university is also a vital
   institution permitting financial monopoly capital to implement its
   strategies of cultural imperialism and cultural hegemony.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
