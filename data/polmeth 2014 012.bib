@article{ WOS:000218761900004,
Author = {Najdek, Carl M.},
Title = {``The Earth to be Spann'd, Connected by Net-Work{''}: Walt Whitman's
   Industrial Internationalism},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2014},
Volume = {3},
Number = {1},
Pages = {95-113},
Month = {SPR},
Abstract = {Using his post-Civil War poetry and prose, this essay investigates the
   role of industrialization in Walt Whitman's internationalist thought.
   Reconsidering him as a Hegelian thinker, it explores his
   characterization of progress in the modern world as a product of the
   expansion of industry across the globe. Although Whitman saw this
   process as driven by American industrial progress, his views were not
   conventionally imperialistic. Instead, fusing materialist and idealist
   principles, he sought to transcend the nation in favor of an
   international order of cosmopolitan global communities, as depicted in
   his poetic descriptions of the urban crowd. The essay considers the
   implications of Whitman's internationalism for American political
   theory, particularly in today's rapidly globalizing world.},
