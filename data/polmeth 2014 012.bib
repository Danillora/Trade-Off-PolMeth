@article{ WOS:000217077100009,
Author = {Burfoot, Annette},
Title = {REVISITING MARY O'BRIEN - REPRODUCTIVE CONSCIOUSNESS AND LIQUID
   MATERNITY},
Journal = {SOCIALIST STUDIES},
