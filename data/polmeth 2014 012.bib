@article{ WOS:000337588100006,
Author = {Isaacs, Rico},
Title = {Neopatrimonialism and beyond: reassessing the formal and informal in the
   study of Central Asian politics},
Journal = {CONTEMPORARY POLITICS},
Year = {2014},
Volume = {20},
Number = {2},
Pages = {229-245},
Month = {JUN},
Abstract = {Neopatrimonialism has emerged as the central conceptual label applied by
   scholars to understand the politics of the Central Asian republics. Like
   the use of neopatrimonialism in other regional settings, this article
   argues that the concept has become susceptible to concept misinformation
   and stretching. Adopting a critical perspective, this article highlights
   three significant problems with the application of neopatrimonialism in
   the study of Central Asian politics: its appropriateness and
   operationalisation; the difficulty in ontologically and empirically
   untangling the formal and informal; and an inherent normativty in its
   application. While not advocating an abandonment of the concept, the
   article considers instead how it can be used better in conjunction with
   additional analytical approaches and/or concepts. The article proffers
   that a focus on either formal-institutional structures; discourses of
   power; and the concept of `multiple modernities' would aid comprehension
   of the region and resolve the three issues highlighted in this work.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
