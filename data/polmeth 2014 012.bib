@article{ WOS:000341330100001,
Author = {Hensell, Stephan and Gerdes, Felix},
Title = {From rebels to elites. The rise and fall of armed groups after war},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2014},
Volume = {55},
Number = {2},
Pages = {217-242},
Abstract = {In many post-war contexts, former rebels pursue political careers and
   their advancement is part of the formation of new elites. However, while
   some rise to senior political positions, others have to be content with
   lower level positions or are even completely marginalized. The article
   outlines a theory of the emergence and reproduction of elite power in
   the social space of the post-war society to explain this variance. The
   central proposition is that rebel careers are mainly dependent on power
   resources which have been accumulated after the war and not during the
   conflict. Case studies of Liberia and Kosovo serve as a first
   plausibility probe of our thesis and explain the post-war political
   careers of former rebels in the two cases with the help of the
   theoretical conception.},
Publisher = {SPRINGER},
Address = {233 SPRING ST, NEW YORK, NY 10013 USA},
Type = {Article},
Language = {German},
Affiliation = {Hensell, S (Corresponding Author), Univ Hamburg, Inst Polit Wissensch AKUF, Allende Pl 1, D-20146 Hamburg, Germany.
   Hensell, Stephan; Gerdes, Felix, Univ Hamburg, Inst Polit Wissensch AKUF, D-20146 Hamburg, Germany.},
ISSN = {0032-3470},
EISSN = {1862-2860},
