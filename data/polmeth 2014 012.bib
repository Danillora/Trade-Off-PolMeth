@article{ WOS:000212003500002,
Author = {Smith, Andrew},
Title = {`Je suis socialiste et quinziste': rugby, wine and socialism in
   the Aude since 1976},
Journal = {NATIONAL IDENTITIES},
Year = {2014},
Volume = {16},
Number = {4},
Pages = {291-309},
Abstract = {In the Languedoc, the intersections between rugby, wine and socialism
   give telling insights into the value and significance of regional
   identity. At the end of the 1970s, the Languedoc's wine industry and
   rugby were facing up to the challenges of modernisation; the responses
   which emerged drew upon the principles of regional identity drawn from
   the `banal nationalism' of wine-drinking, left-voting rugby players in
   local administrations and at the head of powerful trade unions. The
   cross-over between board-room and locker-room helped to accent regional
   responses to change and modernization, ensuring that regional heritage
   remained potent and ready to be communicated.},
