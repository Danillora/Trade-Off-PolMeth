@article{ WOS:000344804000016,
Author = {Wilkinson, Betina Cutaia},
Title = {Perceptions of Commonality and Latino-Black, Latino-White Relations in a
   Multiethnic United States},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2014},
Volume = {67},
Number = {4},
Pages = {905-916},
Month = {DEC},
Abstract = {This study examines the extent that Latinos' sense of social, economic,
   and political power shapes their perceptions of commonality with blacks
   and whites. In accordance with my theory, which builds on the racial
   threat and group position theories, I find that Latinos' sense of power
   structures their perceptions of blacks and whites. When native-born
   Latinos feel economically threatened, they are less likely to perceive
   commonality with blacks. When native-born Latinos gain some political
   influence, they identify less with whites. Among Latino immigrants who
   perceive discrimination, residing in a high-threat economic setting is
   negatively related to perceiving commonality with whites.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
