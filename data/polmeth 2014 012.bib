@article{ WOS:000218509500003,
Author = {Stoker, Gerry and Evans, Mark},
Title = {The ``Democracy-Politics Paradox{''} The Dynamics of Political
   Alienation},
Journal = {DEMOCRATIC THEORY-AN INTERDISCIPLINARY JOURNAL},
Year = {2014},
Volume = {1},
Number = {2},
Pages = {26-36},
Month = {DEC},
Abstract = {Contemporary political scientists have observed a democratic paradox
   that has crystallized around the disconnection between how citizens
   imagine their democracy and how politics is practiced. Citizens continue
   to believe in the values of liberal democracy but are increasingly
   disillusioned with how their political systems work and the politics
   that are practiced in the name of democracy. This article revisits the
   root causes of political alienation to better understand this democratic
