@article{ WOS:000342633700001,
Author = {Gingerich, Daniel W.},
Title = {Brokered Politics in Brazil: An Empirical Analysis},
Journal = {QUARTERLY JOURNAL OF POLITICAL SCIENCE},
Year = {2014},
Volume = {9},
Number = {3},
Pages = {269-300},
Abstract = {An emerging literature emphasizes the importance of vote brokers for
   electoral politics in the developing world. Yet in spite of the great
   interest in the topic, attempts to assess the impact of vote brokerage
   on election returns are exceedingly rare. This paper provides the first
   explicit assessment of the electoral returns to politicians from
   purchasing the allegiances of brokers. It does so by drawing upon a
   unique dataset documenting the allocation of off-the-books payments to
   vote brokers during a gubernatorial reelection campaign in the Brazilian
   state of Minas Gerais. The study finds that in the typical municipality
   buying off local brokers was associated with a 4-6 percentage point
   increase in the vote share obtained by the governor. All told, the
   findings suggest that vote brokerage is a resilient form of political
   linkage that can shape vote choice even in contexts of vote secrecy and
   intermediate levels of economic development.},
