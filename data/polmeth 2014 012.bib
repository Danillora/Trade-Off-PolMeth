@article{ WOS:000345460900003,
Author = {Fortin-Rittberger, Jessica and Rittberger, Berthold},
Title = {Do electoral rules matter? Explaining national differences in women's
   representation in the European Parliament},
Journal = {EUROPEAN UNION POLITICS},
Year = {2014},
Volume = {15},
Number = {4},
Pages = {496-520},
Month = {DEC},
Abstract = {The European Parliament (EP) has one of the highest proportions of women
   in its ranks, with over a third in 2009. Although previous research has
   pointed to the use of proportional representation (PR) in European
   elections as friendlier' to women, few have looked at differences in the
   types of PR rules in use in each country. In this article, we argue that
   the conventional wisdom according to which institutional designthe
   choice of electoral rulesshould shape the composition of the EP does not
   hold, and suggest that the well-documented empirical connection between
   electoral rules and descriptive representation might in fact be an
   endogenous rather than a causal relationship.},
