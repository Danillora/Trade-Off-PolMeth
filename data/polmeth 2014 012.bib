@article{ WOS:000210653800009,
Author = {Holman, Christopher},
Title = {Democracy against the State: Marx and the Machiavellian Moment},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2014},
Volume = {26},
Number = {3},
Pages = {434-442},
Abstract = {The year 2011 finally saw the appearance of an English translation of
   Miguel Abensour's groundbreaking interpretation of Marxian political
   theory in Democracy against the State: Marx and the Machiavellian
   Moment. This review essay will provide a brief critical analysis of the
   main elements of Abensour's contribution. Reading Marx in light of a
   radical democratic interpretation of Machiavelli, Abensour locates in
   the former a model of insurgent democratic practice considered in terms
   of the nonsynthetic struggle of the demos against the forces of the
   state, made for the sake of the multiplication of liberty. Despite
   raising certain concerns regarding Abensour's articulation of the
   relation between democracy and the state and democracy and the
   institution, I argue that his work remains not only an innovative
   reinterpretation of Marx's political thought but also an essential
   contribution to contemporary radical democratic literature that seeks to
   affirm the creative, inventive, and nonexhaustible power of the demos.},
