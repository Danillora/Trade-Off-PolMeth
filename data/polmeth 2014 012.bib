@article{ WOS:000343267600012,
Author = {Kunz, Rahel and Schwenken, Helen},
Title = {Migrants as beacons of hope for development? Gendered subjectivities in
   the migration-development nexus},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2014},
Number = {48},
Pages = {323+},
Abstract = {The current phase of the migration-development nexus emphasizes the
   positive potential of migrants. A gender analysis allows for a better
   understanding of this phase. This article focuses on the international
   discourse arena, the Global Forum on Migration and Development (GFMD),
   and on the paradigmatic case of Mexico. Drawing on feminist theory and
   governmentality approaches, it is argued that particular gendered
   subjectivities, based on gender stereotypes, are mobilized, normalizing
   certain forms of behavior and legitimizing political interventions.
   These gendered subjectivities are embedded in the model of the
   heteronormative transnational family, which is expected to guarantee
   stable social relations and the flow of remittances.},
