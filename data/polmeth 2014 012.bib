@incollection{ WOS:000349017000004,
Author = {Rohrer, Judy},
Book-Author = {Rohrer, J},
Title = {Prop 8 and Lesbian and Gay Citizenship},
Booktitle = {QUEERING THE BIOPOLITICS OF CITIZENSHIP IN THE AGE OF OBAMA},
Series = {Palgrave Pivot},
Year = {2014},
Pages = {41-55},
Abstract = {In this chapter, California's Proposition 8 is analyzed using the
   theoretical concepts of homonormativity and homonationalism as tools for
   exposing the workings of biopower and governmentality. I discuss how the
   No on 8 campaign revealed a mainstream, lesbian and gay politics tied to
   a politics of recognition and inclusion through normalcy. I also examine
   how the omnipresent African American civil rights analogy folds
   homonationalism in through the narrative of American exceptionalism, and
   reproduces the representational and material whiteness of the LGBT
   community. All of this builds an analysis of how blood logics and
   heterosexual reproduction construct not just a white citizenry, but a
   straight one as well. This is most evident in the naturalized linkages
   between marriage and procreation.},
