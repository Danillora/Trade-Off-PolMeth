@article{ WOS:000339547800016,
Author = {Swearingen, C. Douglas and Ripberger, Joseph T.},
Title = {Google Insights and U.S. Senate Elections: Does Search Traffic Provide a
   Valid Measure of Public Attention to Political Candidates?},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2014},
Volume = {95},
Number = {3},
Pages = {882-893},
Month = {SEP},
Abstract = {Objective. To propose a new indicator of public attention to electoral
   candidates based on the relative pattern of Internet queries for
   opposing candidates. Methods. To demonstrate the validity of this
   measure, we use ordinary least squares regression and an F-ratio test.
   Results. We find that thismeasure, based onGoogle Insights data, behaves
   in a manner consistent with a valid measure of public attention.
