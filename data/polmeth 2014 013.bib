@article{ WOS:000343869300001,
Author = {Stadelmann, David and Portmann, Marco and Eichenberger, Reiner},
Title = {POLITICIANS AND PREFERENCES OF THE VOTER MAJORITY: DOES GENDER MATTER?},
Journal = {ECONOMICS \& POLITICS},
Year = {2014},
Volume = {26},
Number = {3},
Pages = {355-379},
Month = {NOV},
Abstract = {Does the gender of political representatives affect the extent to which
   they adhere to the voter majority's preferences? By matching individual
   male and female representatives' votes on legislative proposals with
   real referendum outcomes on the same issues, we obtain a direct measure
   of divergence. We find that female and male representatives adhere
   equally close to the majority's preferences if party affiliations are
   taken into account. This suggests that observed gender differences with
   respect to the national majority of voters may be reduced to an
   ideological left-right dimension.},
Publisher = {WILEY PERIODICALS, INC},
Address = {ONE MONTGOMERY ST, SUITE 1200, SAN FRANCISCO, CA 94104 USA},
Type = {Article},
