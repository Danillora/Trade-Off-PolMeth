@article{ WOS:000330979600015,
Author = {Mendez, Fabio},
Title = {Can corruption foster regulatory compliance?},
Journal = {PUBLIC CHOICE},
Year = {2014},
Volume = {158},
Number = {1-2},
Pages = {189-207},
Month = {JAN},
Abstract = {The legal and economic literatures overwhelmingly support the notion
   that regulatory compliance is always less in the presence of corruption.
   This paper departs from those literatures and shows that, whenever
   public officials are paid fixed wages, an increase in corruption may
   actually foster compliance. The conditions that make this possible are
   laid down in a theoretical model. Empirical evidence that corroborates
   the theoretical findings is provided using firm-level data for 26
   transition economies.},
