@article{ WOS:000212650500006,
Author = {Golosov, Grigorii V.},
Title = {The September 2013 Regional Elections in Russia: The Worst of Both
   Worlds},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2014},
Volume = {24},
Number = {2},
Pages = {229-241},
Abstract = {The September 2013 elections in the regions of Russia resulted in
   victories of the major pro-government party, United Russia, in 16
   regional legislative elections, and brought success to incumbent chief
   executives in eight gubernatorial elections. However, the apparent
   recovery of United Russia from the trauma of the 2011 national
   legislative elections stemmed not so much from its increased popularity
   in the electorate, but rather from its ability to engage in manipulative
   vote-splitting strategies against the opposition. The rules of candidate
   nomination in gubernatorial elections were so restrictive that most of
   these elections could not be characterized as truly competitive.},
