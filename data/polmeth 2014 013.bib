@article{ WOS:000342871700003,
Author = {Bejar Algazi, Luisa},
Title = {When the Executive Power is Weak: Who Legislates in Mexico?},
Journal = {POLITICA Y GOBIERNO},
Year = {2014},
Volume = {21},
Number = {2},
Pages = {327-349},
Abstract = {How is legislative process affected when governments have neither
   majority nor institutional devices to confront a plural Congress? To
   answer this question, this study analyzes the level of aggregation in
   bills sent and approved in Mexico's Chamber of Deputies after the
   election of 2000. The research findings show that one of the clues to
   avoid particularistic legislation has been straightening parties'
   leadership through the electoral and legislative institutional design.},
