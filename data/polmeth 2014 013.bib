@article{ WOS:000218591200003,
Author = {Hooker, Juliet},
Title = {Hybrid subjectivities, Latin American mestizaje, and Latino political
   thought on race},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2014},
Volume = {2},
Number = {2},
Pages = {188-201},
Abstract = {This essay interrogates accounts of race in Latino political thought
   (and hence claims about how Latinos should approach race in the USA)
   grounded in a selective borrowing from Latin American philosophical
   sources on mestizaje. It argues that the uncritical reproduction of
   certain conceptions of mestizaje in Latino political thought ignores the
   way in which Latin American ideas about race were developed in direct
   conversation with US empire and US racial politics, is at odds with
   contemporary racial politics in the region, and might serve to obscure
   certain fault lines within Latinidad.},
