   Marcinkowski, Frank, Univ Munster, Commun, Munster, Germany.
   Metag, Julia, Univ Zurich, Zurich, Switzerland.},
DOI = {10.1080/19331681.2014.895690},
ISSN = {1933-1681},
EISSN = {1933-169X},
Keywords = {German politics; online campaigning; state elections; Theory of Planned
   Behavior},
Keywords-Plus = {POLITICAL-PARTIES; INTERNET; ATTITUDES; ELECTION; COMMUNICATION;
