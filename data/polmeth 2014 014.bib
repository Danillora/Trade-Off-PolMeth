@incollection{ WOS:000348362500007,
Author = {Strawser, Bradley Jay},
Book-Author = {Strawser, BJ},
Title = {Sovereignty Issues and Precedent Setting Problems},
Booktitle = {KILLING BIN LADEN: A MORAL ANALYSIS},
Year = {2014},
Pages = {56-67},
Abstract = {This chapter continues to deal with objections to the claim that the
   killing of Osama bin Laden was justifiable. Here I address the related
   worries that the raid itself violated Pakistan's sovereignty and that
   the killing creates a bad precedent making the killing wrong; all things
   considered. In response, I concede much of the objection regarding the
   ways in which the United States engaged their ally on the raid, but
   discuss the countervailing reasons the United States had to worry about
   the risk of the mission being compromised were they to partner with
   Pakistan on the raid. I also discuss the ``all things considered{''}
   objection and argue that, indeed, the raid was likely more than merely a
   pro tanto good, given the epistemic constraints in play.},
