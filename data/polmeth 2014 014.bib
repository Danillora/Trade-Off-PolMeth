@article{ WOS:000338998000002,
Author = {Carkoglu, Ali and Baruh, Lemi and Yildirim, Kerem},
Title = {Press-Party Parallelism and Polarization of News Media during an
   Election Campaign: The Case of the 2011 Turkish Elections},
Journal = {INTERNATIONAL JOURNAL OF PRESS-POLITICS},
