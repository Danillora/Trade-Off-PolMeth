@article{ WOS:000345549200012,
Author = {Boutin, Delphine},
Title = {Climate vulnerability, communities' resilience and child labour},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2014},
Volume = {124},
Number = {4},
Pages = {625-638},
Month = {JUL-AUG},
Abstract = {This article clarifies and quantifies the causal impact of climate
   change vulnerability on child labour incidence and intensity. For this
   purpose, we create an index of vulnerability to climate change, composed
   of biophytical vulnerability and communities' resilience. Both,
   participation to economic activities and to household chores have been
   taken into account. We find that climate vulnerability negatively
   affects child labour incidence and intensity, while has no significant
   impact on household chores. We conclude that child labour is an
   adjustment variable to local labour market conditions, not correlated
   with communities' resilience.},
