@article{ WOS:000340218700007,
Author = {Kunze, Frederik and Kramer, Jens and Rudschuck, Norman},
Title = {Interest rate forecasts in times of financial crisis: What might be
   interesting to know?},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2014},
Volume = {34},
Number = {S, SI},
Pages = {S45-S52},
Month = {JUN},
Abstract = {As a basic requirement to be a feasible predictor for the shape of the
   yield curve survey forecasts should be cointegrated with the realized
   path of interest rates. The short end of the curve is determined to a
   large extend by the monetary policy of central banks. Especially in
   times of financial crisis uncertainty about the coming policies rises
   and leads to higher dispersion within survey forecasts as well as
   structural breaks within the long-run relationship. Using a simple
   empirical model it can be stated that emergence of uncertainty may be
   explained by worsening economic sentiment or liquidity constraints in
   the money market. (C) 2013 Elsevier B.V. All rights reserved.},
