@article{ WOS:000341582300006,
Author = {Chowdhury, Arnab Roy},
Title = {`Repertoires of Contention' in Movements Against Hydropower Projects in
   India},
Journal = {SOCIAL MOVEMENT STUDIES},
Year = {2014},
Volume = {13},
Number = {3},
Pages = {399-405},
Abstract = {Applying Charles Tilly's notion of `repertoires of contention', this
   paper discusses strategies and modes of protest carried out by the
   movements against hydropower projects in India. It compares the Narmada
   Bachao Andolan (NBA; Save Narmada Movement) in Madhya Pradesh and the
   Shramik Mukthi Dal (SMD; Labour Liberation Party) led mobilisations
   against hydropower projects in Maharashtra. It explains the more
   dramatic repertoires deployed by NBA as opposed to moderate and
   pragmatic movement repertoires of SMD. The paper concludes that the
   differences in ideology, organisation and cultural-historical legacy of
   the two organisations, as well as their different modes of interaction
   with the state, are the causes behind different contentious repertoires
   deployed by them, which resulted in different trajectories and outcomes.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
