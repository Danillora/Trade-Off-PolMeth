Book-Author = {Lutz, B
   DuToit, P},
Title = {The Development of Methods for Measuring Expressions on Democracy},
Booktitle = {DEFINING DEMOCRACY IN A DIGITAL AGE: POLITICAL SUPPORT ON SOCIAL MEDIA},
Series = {Palgrave Pivot},
Year = {2014},
Pages = {37-62},
Abstract = {Chapter 3 is divided into four broad sections. Firstly, the value of
   measuring expressions on democracy is briefly examined. Secondly, an
   examination of traditional methods of measuring expressions on democracy
   can be found. Thirdly, Twitter is examined as a new form of Habermasian
   public sphere. Finally, a section on novel emerging methods of measuring
   public expressions from online sources is found. This section specically
   looks at a number of recent studies which have utilised Twitter in order
   to gauge public expressions. The broad aim of this chapter is to
   illustrate novel methods - such as the method developed in this book -
   that might lead to an alternative method of measurement. We do not claim
   that this method will replace traditional methods of measuring
   expressions on democracy; but we do believe that it could supplement and
   enhance the data gathered using traditional methods.},
