@article{ WOS:000342913900002,
Author = {Bleaney, Michael and Halland, Havard},
Title = {NATURAL RESOURCE EXPORTS, FISCAL POLICY VOLATILITY AND GROWTH},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2014},
Volume = {61},
Number = {5},
Pages = {502-522},
Month = {NOV},
Abstract = {The combination of poor institutions and erratic macroeconomic policy,
   as measured by the volatility of fiscal policy, is associated with
   slower growth. We show that macroeconomic policy is more erratic in
   countries that are rich in natural resources, especially minerals and
   fuels, and in those that receive large aid inflows. Poor institutions
   also play a role. Although Africa is a major receiver of aid and
   exporter of natural resources, this is not purely an African phenomenon.
   Output volatility is not associated with slower growth after controlling
   for institutions and the volatility of fiscal policy.},
