@article{ WOS:000336075400003,
Author = {Jakubikova, Emilia and Tkacova, Andrea and Banociova, Anna},
Title = {COMPOSITE LEADING INDICATORS OF ECONOMIC CYCLES OF V4 COUNTRIES AND
   THEIR COMPARISON TO THE CLI OF THE EUROSTAT AND THE OECD},
Journal = {POLITICKA EKONOMIE},
Year = {2014},
Volume = {62},
Number = {2},
Pages = {194-215},
Abstract = {This article's objective is to suggest and create the composite leading
   indicators and their importance for monitoring in a short term
   prediction of economic cycles in the V4. We describe in detail the
   methodologies of OECD and Eurostat, which deal with the prediction of
   the economic cycles in the V4 countries through CLI. The relation of 475
   selected economic indicators and reference series, which represent the
   economic cycle of the V4 countries, is analyzed in the empirical part of
   this article. Based on the performed analysis, we define groups of
   leading, parallel and lagged indicators. We choose those leading
