@article{ WOS:000338010900003,
Author = {Seaward, Louise},
Title = {The Societe Typographique de Neuchatel (STN) and the Politics of the
   Book Trade in Late Eighteenth-Century Europe, 1769-1789},
Journal = {EUROPEAN HISTORY QUARTERLY},
Year = {2014},
Volume = {44},
Number = {3},
Pages = {439-457},
Month = {JUL},
Abstract = {Disseminating literature in eighteenth-century Europe was an immense and
   intricate undertaking. This article concentrates on a somewhat neglected
   facet of the print trade during this age; namely the role and
   significance of political support and protection. Making use of the
   extant records of an eighteenth-century publishing house, this article
   highlights how government agents collaborated and cooperated with book
   traders. The Societe typographique de Neuchatel was a Swiss publishing
   business which harnessed the power of government agents from across the
   continent by enlisting their assistance as commercial intermediaries.
   Whilst the capabilities, willingness and opportunities available to
   these individuals limited the scope of their actions, they were
   nevertheless participants in a network of patronage which helped to
   sustain the international print trade of eighteenth-century Europe.},
