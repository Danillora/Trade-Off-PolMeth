@article{ WOS:000341902800009,
Author = {Raleigh, Clionadh},
Title = {Political hierarchies and landscapes of conflict across Africa},
Journal = {POLITICAL GEOGRAPHY},
Year = {2014},
Volume = {42},
Pages = {92-103},
Month = {SEP},
Abstract = {Almost all African states experience substantial and widespread
   political insecurity in a variety of forms. This analysis explains how
   relationships between groups and governments create incentives and
   disincentives for distinct forms of political violence to emerge. It
   argues that ethno-regional communities across Africa are situated within
   a power hierarchy that determines their relative importance to, and
   inclusion in, regimes. A dynamic power landscape emerges from relative
   group positions. Various positions within a hierarchy are associated
   with particular dominant forms of organized political violence as groups
   challenge political elites, but are bounded by their goals and
   characteristics. A failure to consider the political hierarchies and
   landscapes operating within African states has led to an under
   specification of the causal mechanisms driving different forms of
   violence, and an overstatement of benefits from declining civil war
   rates and inclusive governing coalitions. (C) 2014 Elsevier Ltd. All
   rights reserved.},
