@article{ WOS:000333156600005,
Author = {Dahm, Matthias and Dur, Robert and Glazer, Amihai},
Title = {How a firm can induce legislators to adopt a bad policy},
Journal = {PUBLIC CHOICE},
Year = {2014},
Volume = {159},
Number = {1-2},
Pages = {63-82},
Month = {APR},
Abstract = {This paper shows why a majority of legislators may vote for a policy
   that benefits a firm but harms all legislators. The firm may induce
   legislators to support the policy by suggesting that it is more likely
   to invest in a district where voters or their representative support the
   policy. In equilibrium, no one vote may be decisive, so each legislator
   who seeks the firm's investment votes for the policy, though all
   legislators would be better off if they all voted against the policy.
   And when votes reveal information about the district, the firm's
   implicit promise or threat can be credible. Unlike influence mechanisms
   based on contributions or bribes, the behavior considered is time
   consistent and in line with the low campaign contributions by special
   interests.},
