@article{ WOS:000336018100004,
Author = {Menkyna, Fusako Tsuchimoto},
Title = {A theory of ethnic diversity and income distribution: A legislative
   bargaining approach},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2014},
Volume = {34},
Pages = {52-67},
Month = {JUN},
Abstract = {In this paper, I examine how the two dimensions of income inequality and
   ethnic diversity affect the formation of political coalitions and the
   reallocation of income. I construct a legislative bargaining model to
   analyze when coalitions form along the income dimension and when it
   forms along the ethnic dimension. The results of the model suggest that
   the agenda setter selects a partner with a lower aggregate income: a
   smaller population size and a lower income level. Further, I extend the
   model from a one-round to a two-round game. The extended model shows
   that forming an oversized coalition is a possible equilibrium outcome
   and is consistent with the empirical findings. (C) 2013 Elsevier B.V.
   All rights reserved.},
