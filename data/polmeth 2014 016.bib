@article{ WOS:000336018100022,
Author = {Redoano, Michela},
Title = {Tax competition among European countries. Does the EU matter?},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2014},
Volume = {34},
Pages = {353-371},
Month = {JUN},
Abstract = {This paper provides a simple theoretical model of capital tax
   competition between countries that differ in spatial location, and where
   cross-border investment costs are proportional to distance (a gravity
   model). We model EU membership as a reduction in `distance' between
   countries. Precise predictions about reaction functions' intercepts and
   slopes are derived. In particular we find that joining the Union lowers
   the intercept and that all countries react more to member countries than
   they do to non-members. These predictions are largely confirmed using a
   panel data set of statutory corporate tax rates on Western European
   countries. (C) 2014 Elsevier B.V. All rights reserved.},
