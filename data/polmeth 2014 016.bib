@incollection{ WOS:000342053600028,
Author = {Arendt, Florian and Matthes, Joerg},
Editor = {Reinemann, C},
Title = {Cognitive effects of political mass media},
Booktitle = {POLITICAL COMMUNICATION},
Series = {Handbooks of Communication Science},
Year = {2014},
Volume = {18},
Pages = {547-568},
Abstract = {This chapter reviews the major lines of research on five prominent
   concepts of political communication research linked to the cognitive
   effects of political mass media: Knowledge gains and gaps, cultivation,
   agenda setting, priming, and framing. Basic ideas, typical
   methodologies, key findings and the cognitive processes behind the
   concepts are discussed in detail, common conceptual roots are
   identified, and key methodological challenges are highlighted. Finally,
   some of the overlaps and differences between the approaches are
   discussed in order to take a step toward a more integrative and coherent
   view of cognitive media effects in political communication. The chapter
   argues that both empirical and theoretical advances are needed to get to
   a better and less fragmented understanding of cognitive media effects in
   political communication.},
