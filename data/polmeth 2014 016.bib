@article{ WOS:000343184600003,
Author = {Verkuyten, Maykel and Maliepaard, Mieke and Martinovic, Borja and
   Khoudja, Yassine},
Title = {Political Tolerance among Muslim Minorities in Western Europe: The Role
   of Denomination and Religious and Host National Identification},
Journal = {POLITICS AND RELIGION},
Year = {2014},
Volume = {7},
Number = {2},
Pages = {265-286},
Month = {JUN},
Abstract = {Islam has emerged as the focus of immigration and diversity debates in
   Europe, especially in relation to the incorporation of Islam within
   political democracy. Using the least-liked group approach, the present
   study investigates political tolerance among Sunni and Alevi Muslims of
   Turkish origin living in Germany and the Netherlands. A relatively low
   level of political tolerance was found with higher intolerance of Alevis
   compared to Sunnis which was due to Alevis' strong rejection of
   religious fundamentalists. For both Muslim subgroups and in both
   countries, stronger religious group identification was associated with
   higher tolerance. Political tolerance was also found to be lower in
   Germany than in the Netherlands and in the latter country tolerance was
   positively associated with host national identification. The findings
   show that Islamic belief, Muslim group identification and the host
   national context are important for political tolerance.},
