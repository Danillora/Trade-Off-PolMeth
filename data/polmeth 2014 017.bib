@article{ WOS:000341636200004,
Author = {Krupnikov, Yanna},
Title = {How Negativity Can Increase and Decrease Voter Turnout: The Effect of
   Timing},
Journal = {POLITICAL COMMUNICATION},
Year = {2014},
Volume = {31},
Number = {3},
Pages = {446-466},
Abstract = {Negative ads dominate campaign communication, but scholars continue to
   disagree over the effects of negativity on voter turnout. While some
   studies show that negativity leads to a lower likelihood of turnout,
   others find precisely the opposite. In this article, I leverage the role
   of timing to unify findings that were heretofore perceived as largely
   conflicting. I use the same data to show that at a certain time exposure
   to negativity can be mobilizing, but at other points in time exposure
   can be demobilizing. A combination of observational data and
   experimental results highlight these crucial conditions.},
