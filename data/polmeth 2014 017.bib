@article{ WOS:000337599000004,
Author = {Dixon, Chris},
Title = {Redeeming the Warrior: Myth-making and Australia's Vietnam Veterans},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2014},
Volume = {60},
Number = {2},
Pages = {214-228},
Month = {JUN},
Abstract = {In late 1960s a powerful myth developed in the United States that
   Vietnam veterans were spat on when they returned home. A parallel myth
   survives in Australia with widespread claims that paint or even blood
   was routinely thrown at returning soldiers. In a 1966 incident, red
   paint was thrown on Lieutenant Colonel Alex V. Preece as he led the
   First Battalion through Sydney. The Australian myth remains central to
   perceptions of Australian Vietnam veterans as despised outsiders and
   feeds into contemporary demands that Australians support their soldiers
   and the wars in which they are involved. This paper explores connections
   between cultural politics in the Unites States and Australia,
   particularly as they pertain to the contentious legacies of the 1960s.},
