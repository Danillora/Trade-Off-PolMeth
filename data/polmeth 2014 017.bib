@article{ WOS:000327633100004,
Author = {Caporale, Guglielmo Maria and Ciferri, Davide and Girardi, Alessandro},
Title = {TIME-VARYING SPOT AND FUTURES OIL PRICE DYNAMICS},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2014},
Volume = {61},
Number = {1},
Pages = {78-97},
Month = {FEB},
Abstract = {In this paper, we investigate the role of crude oil spot and futures
   prices in the process of price discovery by using daily data over the
   period from January 1992 to September 2012. We provide evidence that
   futures markets play a more important role than spot markets, but their
   relative contributions turn out to be highly unstable, especially for
   the most deferred contracts. Furthermore, considering the time-varying
   dynamics provides evidence of a smaller role for futures markets and a
   greater role for fundamental factors in driving oil prices during the
   global financial turmoil of 2007-2008. The implications of the main
   results for hedging and forecasting crude oil spot prices are also
   discussed.},
