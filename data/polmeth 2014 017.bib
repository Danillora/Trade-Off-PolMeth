@article{ WOS:000345911900011,
Author = {Riley, Dylan},
Title = {The Third Reich as Rogue Regime Adam Tooze's Wages of Destruction},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2014},
Volume = {22},
Number = {3-4},
Pages = {330-350},
Abstract = {What was the connection between the structure of the German economy in
   the 1930s and German aggression in World War II? Adam Tooze's Wages of
   Destruction forcefully poses this issue, but fails to adequately resolve
   it. Instead, on this decisive question, his analysis oscillates uneasily
   between two equally unconvincing models: rational-choice theory and
   cultural determinism. This surprising explanatory failure derives from
   an inadequate theorisation of German imperialism as the expression of
   the combined and uneven development of the German economy and society in
   the late-nineteenth and early-twentieth centuries.},
