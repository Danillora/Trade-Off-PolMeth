@article{ WOS:000331785300001,
Author = {Halas, Matus},
Title = {Searching for the Perfect Footnote: Friedrich Ratzel and the Others at
   the Roots of Lebensraum},
Journal = {GEOPOLITICS},
Year = {2014},
