@article{ WOS:000335204600002,
Author = {Cozic, Mikael},
Title = {Economic Methodology, from millian deductivism to neo-positivism},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2014},
Volume = {124},
Number = {1},
Pages = {23-73},
Month = {JAN-FEB},
Abstract = {The aim of this paper is to provide a survey of the main issues and of
   the most important trends in economic methodology. It is organized
   around the generalized Mill's problem: does economics obey the
   methodological standards of empirical science? The study is divided in
   two parts. The first one expounds and discusses the views of J-S. Mill
   and of his contemporary heirs (Hausman, Cartwright). The second part
   tackles views that can be associated to neo-positivism or logical
   empiricism. We conclude by identifying convergence points, gaps and
   trends of actual research.},
Publisher = {EDITIONS DALLOZ},
Address = {31-35 RUE FROIDEVAUX, PARIS CEDEX 14, 75685, FRANCE},
Type = {Article},
Language = {French},
Affiliation = {Cozic, M (Corresponding Author), Univ Paris Est Creteil, Dept Philosophie, 29 Rue Ulm, F-75005 Paris, France.
   Univ Paris Est Creteil, Dept Philosophie, F-75005 Paris, France.},
DOI = {10.3917/redp.241.0023},
