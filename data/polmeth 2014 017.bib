@article{ WOS:000346768800004,
Author = {Brostrom, Jannika},
Title = {Re-evaluating Australia's ``Cooperative Security{''} in Cambodia},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2014},
Volume = {60},
Number = {4},
Pages = {549-563},
Month = {DEC},
Abstract = {Australia's role in resolving the conflict in Cambodia has been
   described as a triumph of cooperative security that achieved a balance
   between principles and pragmatism. The pursuit of cooperative security
   is a familiar theme in discussions of Australian diplomacy during the
   1990s, yet there has been little scholarly consideration of whether this
   accurately captures the nature of Australian foreign policy at the time.
   This article explains Australia's conflict resolution role in Cambodia
   using an alternative, neoclassical realist framework. Specifically I
   demonstrate that expectations of reciprocity meant that Australia, when
   negotiating for peace in Cambodia, preferred bilateral over multilateral
   diplomacy. Secondly, Australia actively sought to lead the Cambodian
   peace-keeping operation to enhance its regional security credentials.
   Finally, building closer ties with Vietnam was an important, often
   overlooked policy outcome.},
