@article{ WOS:000343185300004,
Author = {Jelen, Ted G.},
Title = {The Subjective Bases of Abortion Attitudes: A Cross National Comparison
   of Religious Traditions},
Journal = {POLITICS AND RELIGION},
Year = {2014},
Volume = {7},
Number = {3},
Pages = {550-567},
Month = {SEP},
Abstract = {The subjective correlates of abortion attitudes for six different
   religious traditions (Roman Catholicism, Protestantism, Eastern
   Orthodoxy, Buddhism, Hinduism, and Islam). For all six groups, attitudes
   toward sexual morality exhibit the strongest relationship with abortion
   attitudes, followed by the effects of attitudes toward human life.
   Gender role attitudes are much less powerful predictors of abortion
   attitudes. Further, the multivariate models which explain abortion
   attitudes are remarkably similar across religious traditions, with
   inter-religious differences largely being attributable to differences in
   the marginal distributions of the independent variables.},
