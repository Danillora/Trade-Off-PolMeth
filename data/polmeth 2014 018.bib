@article{ WOS:000338034700002,
Author = {Di Gioacchino, Debora and Profeta, Paola},
Title = {Lobbying for Education in a Two-Sector Model},
Journal = {ECONOMICS \& POLITICS},
Year = {2014},
Volume = {26},
Number = {2},
Pages = {212-236},
Month = {JUL},
Abstract = {Firms specialized in two different sectors lobby to induce the
   government to subsidize the type of education complementary to their
   production. Lobbying is endogenous. We show that, if lobbying is not
   costly, both sectors will lobby in equilibrium and the education policy
   will induce the same skill composition that would be chosen by the
   social planner. However, if lobbying is costly and there is sufficient
   asymmetry between the sectors, only one sector will exert pressure on
   the policy-maker in the attempt to direct resources toward the type of
   education required by its production. Which sector will engage in
   lobbying depends on relative size, productivity, and price. We also
   provide some preliminary evidence that lobbying activity by firms may
   influence the production of skills needed by those firms.},
