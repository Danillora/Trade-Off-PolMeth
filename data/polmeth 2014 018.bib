@article{ WOS:000338128000002,
Author = {Shehata, Adam},
Title = {Game Frames, Issue Frames, and Mobilization: Disentangling the Effects
   of Frame Exposure and Motivated News Attention on Political Cynicism and
   Engagement},
Journal = {INTERNATIONAL JOURNAL OF PUBLIC OPINION RESEARCH},
Year = {2014},
Volume = {26},
Number = {2},
Pages = {157-177},
Month = {SUM},
Abstract = {This study combines a media content analysis (N = 1158) and panel survey
   data (N = 1612) conducted during the Swedish 2010 national election
   campaign, to analyze the effects of both game-framed and issue-framed
   news on political cynicism, institutional trust, and political interest.
   The findings show that news framing matters. Whereas game-framed news
   increases cynicism and depresses interest, issue-framed news has
   mobilizing effects. Furthermore, by conceptually and empirically
   distinguishing frame exposure from motivated news attention as two
   different modes of news media use, the results show that the effects of
   exposure to game-framed and issue-framed news are distinct from
   motivated news attention. These findings suggest two different
   mechanisms behind media effects and shine new light on the spiral of
   cynicism-virtuous circle controversy.},
