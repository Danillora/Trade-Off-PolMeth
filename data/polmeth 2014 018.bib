@article{ WOS:000353001700005,
Author = {Vuillemey, Guillaume},
Title = {Sovereign Default and Bank CDS Payments in Europe},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2014},
Volume = {124},
Number = {6},
Pages = {927-952},
Month = {NOV-DEC},
Abstract = {This paper investigates the redistributive effects of CDS payments in
   case of a sovereign credit event in Europe. A large number of default
   scenarios are simulated using bank-level data on European banks' bond
   and CDS portfolios and the theoretical framework put forth by Vuillemey
   and Peltonen {[}2013]. I estimate a probit model to document the
   determinants of the redistributivity of bank-to-bank CDS payments,
   according to four criteria. There is no evidence of massive
   redistributive effects. However, a set of bank-level and country-level
   characteristics drive the observed payment patterns to a significant
   extent. Additional results concern the relative magnitude of bank losses
   on their bond and CDS portfolios.},
