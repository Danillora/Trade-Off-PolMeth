@article{ WOS:000336018100027,
Author = {Nolivos, Roberto Delhy and Vuletin, Guillermo},
Title = {The role of central bank independence on optimal taxation and
   seigniorage},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2014},
Volume = {34},
Pages = {440-458},
Month = {JUN},
Abstract = {Should inflation be thought of as ``just another tax?{''} The
   theoretical basis for doing so dates back to Phelps (1973) and has been
   greatly refined ever since. Since optimal taxation minimizes the
   deadweight loss by equalizing the marginal distortions of all available
   taxes, including the inflation tax, a key distinctive theoretical
   implication obtained by these models is that inflation and tax rates
   have a positive relationship. While theoretically appealing, empirical
   studies find virtually no support for this key implication.
   We show that, considering the role of central bank independence (CBI),
   it is possible to reconcile the main theoretical implications of models
   of optimal taxation and seigniorage with the empirical evidence.
   Different degrees of CBI capture the extent to which monetary policy is
   effectively controlled by the fiscal authority. Our model generates
   three testable implications: i) if CBI is low, the optimal relationship
   between inflation and tax rates is positive, ii) such relationship is a
   decreasing function of the degree of CBI, and iii) the relationship is
   negative for high levels of CBI. We show that these hypotheses hold for
   alternative measures of tax policy, seigniorage, and CBI as well as
   after controlling for several macroeconomic performance, ideology,
   political instability, governance, and economic structural/development
   factors. (C) 2013 Elsevier B.V. All rights reserved.},
Publisher = {ELSEVIER SCIENCE INC},
Address = {STE 800, 230 PARK AVE, NEW YORK, NY 10169 USA},
Type = {Article},
Language = {English},
Affiliation = {Vuletin, G (Corresponding Author), Brookings Inst, 1775 Massachusetts Ave NW, Washington, DC 20036 USA.
   Nolivos, Roberto Delhy, Colby Coll, Waterville, ME 04901 USA.
   Vuletin, Guillermo, Brookings Inst, Washington, DC 20036 USA.},
DOI = {10.1016/j.ejpoleco.2013.09.010},
ISSN = {0176-2680},
EISSN = {1873-5703},
