@article{ WOS:000213011700005,
Author = {Michael, Gabriel J.},
Title = {Politics and Rulemaking at the Copyright Office},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2014},
Volume = {11},
Number = {1},
Pages = {64-81},
Abstract = {Studies of rulemaking often conclude that members of the public
   participate infrequently, while businesses participate actively and are
   more likely to influence final rules. I analyze a series of five
   Copyright Office rulemakings spanning the years 2000-2012. In contrast
   to the existing literature, I find high levels of public participation
   and a lack of influence by businesses. Instead, legal representation is
   the key predictor of influence over final rules. I also argue that the
   existing literature has wrongly theorized about how to measure public
   participation in rulemaking, thereby mischaracterizing the rulemaking
   process as undemocratic.},
