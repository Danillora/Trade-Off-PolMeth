@article{ WOS:000346768800002,
Author = {Kobayashi, Ai},
Title = {Australia and Japan's Admission into the Colombo Plan},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2014},
Volume = {60},
Number = {4},
Pages = {518-533},
Month = {DEC},
Abstract = {In October 1954 Japan was admitted to the Colombo Plan as a donor
   country with Australia's sponsorship. The dramatic shift in Australia's
   position on Japan's involvement in the Colombo Plan, from strong
   opposition to sponsorship, was recognised by the Japanese government as
   the first Australian initiative to improve the bilateral relationship
   since the resumption of diplomatic relations in April 1952. This article
   examines Japan's attempts to participate in the Colombo Plan and
   considers how Australia's actions and reactions determined the course of
   events. The episode provides an insight into how Australian diplomacy
   was conducted towards Japan and the Japanese view of its relations with
   Australia at the initial stage of the bilateral relationship.},
