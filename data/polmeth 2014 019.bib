@article{ WOS:000342724800008,
Author = {Thomas, Martin},
Title = {Gramsci without the Prince},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2014},
Volume = {22},
Number = {2},
Pages = {158-173},
Abstract = {Peter Thomas, in The Granzscian Moment, explains well how Gramsci strove
   to re-educate the communist movement in an expansive spirit, around the
   united front. He makes clear that the united-front approach advocated by
   Gramsci, based on working-class mobilisation and accompanied by clear
   communist criticism, was distinct from the policy of bourgeois alliances
   to be advocated by the Stalinist parties after 1935 under the name
   `popular front'. He demystifies the concept in Gramsci of working-class
   `hegemony', from which so many speculations are spun, showing that it
   meant nothing other than working-class political leadership, achieved
   through sound use of united-front tactics. Yet Thomas makes the formula
   of `united front' do too much, or bundles into it more than it can
   rationally contain. Meanwhile, the question of the revolutionary
   working-class political party is almost entirely absent in Thomas's
   discussion.},
