@article{ WOS:000409503300004,
Author = {Dove, John A.},
Title = {Antitrust enforcement by state attorneys general: institutional, legal
   and political considerations},
Journal = {BUSINESS AND POLITICS},
Year = {2014},
Volume = {16},
Number = {2},
Pages = {291-312},
Month = {AUG},
Abstract = {Since deregulation in 1980, there has been a major shift in antitrust
   enforcement to the states, specifically to state attorneys general
   (AGs). For advocates of deregulation, this move has had important
   unintended consequences. Specifically, it has allowed antitrust
   litigation to become hitched to the political incentives of AGs.
   Analysis of a panel dataset of all state antitrust actions between 1990
   and 2008 suggests that antitrust enforcement by state attorneys general
   appears to follow electoral cycles, which is especially pronounced
   depending on specific institutional constraints that exist between the
   states.},
