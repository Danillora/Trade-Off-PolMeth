@article{ WOS:000342802300005,
Author = {Sheng, Li},
Title = {Income Inequality, Financial Systems, and Global Imbalances: A
   Theoretical Consideration},
Journal = {GLOBAL POLICY},
Year = {2014},
Volume = {5},
Number = {3},
Pages = {311-320},
Month = {SEP},
Abstract = {This paper illustrates the effects of two major macroeconomic factors on
   global imbalances with regard to China as a growing giant in the global
   economy. The country has observed a continuous drop in the labor income
   share of GDP and a considerable rise in income inequality, giving rise
   to a savings glut and high investment rates. China's inflation, housing
   bubble, and capital losses in foreign markets are attributable to its
   rushed transition to market-based financing and its inherent
   vulnerability to international capital flows. The US is also
   experiencing a rise in inequality, although this rise is associated with
   a decline in savings as a fundamental cause of its current account
   deficit, whereas its exorbitant privilege from the dollar's reserve
   currency status is accompanied by increasing difficulties posed by the
   Triffin dilemma. It is shown that global imbalances may be less related
   to misaligned exchange rates than to distorted wage differences.},
