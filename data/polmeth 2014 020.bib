@incollection{ WOS:000364351300007,
Author = {Maria Escobar, Anna},
Editor = {Zajicova, L and Zamec, R},
Title = {Making the Invisible, Visible: Language Contact and Instruments of
   Linguistic Vitality},
Booktitle = {LENGUA Y POLITICA EN AMERICA LATINA: PERSPECTIVAS ACTUALES},
Year = {2014},
Pages = {149-175},
Abstract = {Starting from the premise that the linguistic vitality of Andean
   languages in the Peruvian context is intimately related to the evolution
   of Andean Spanish and to its diffusion within Peru, the present paper
   analyzes the apparent incongruence between the reported vitality of
   Quechua as it is presented in the UNESCO Atlas of Endangered Languages,
   and what is reported from research on Spanish in contact with Quechua
   and Peruvian Andean Spanish. Through an analysis of the criteria
   presented by UNESCO as relevant to determining the vitality of a
   language, it is proposed that these criteria only include institutional
   criteria that give a top-down perspective of the vitality of a language.
   A set of additional criteria is proposed, using the Peruvian case as an
   example, as relevant to a missing bottom-up perspective; which would
   make visible, what the present instruments leave as invisible.},
