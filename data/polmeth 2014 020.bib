@article{ WOS:000218636900004,
Author = {Sandstrom, Karl},
Title = {Remoteness and `demonitored space' in Afghanistan},
Journal = {PEACEBUILDING},
Year = {2014},
Volume = {2},
Number = {3},
Pages = {286-302},
Abstract = {A physical and psychological separation between intent and outcome of
   aid programmes in Afghanistan makes aid and development increasingly at
   risk of capture by alternative interests and less tied to actual
   outcomes. Physical risk mitigation has led to a partial insulation from
   a surrounding perceived threat but consequently also from recipient
   communities. In many cases, it has become doing things `to' and `at',
   rather than `with' and `for' - in other words output over outcome.
   Building on extensive fieldwork in Afghanistan, this article seeks to
   explain how remoteness develops in the Afghan aid context and highlight
   some of its implications. The primary problem is the creation of
   `demonitored space' - a section of the operational environment that
   contains the illusion of monitoring and control but that in reality has
   been surrendered to alternative interests. This space requires closing
   with context-specific solutions and increased contact with the point of
   implementation - not increased isolation and remoteness.},
