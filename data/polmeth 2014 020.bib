@article{ WOS:000347604900008,
Author = {Tsygankov, Andrei P.},
Title = {The frustrating partnership: Honor, status, and emotions in Russia's
   discourses of the West},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
