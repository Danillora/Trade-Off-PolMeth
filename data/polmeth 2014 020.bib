@article{ WOS:000333033800017,
Author = {Siverson, Randolph M. and Johnson, Richard A. I.},
Title = {Politics and Parasites: The Contribution of Corruption to Human Misery},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
Year = {2014},
Volume = {58},
Number = {1},
Pages = {199-206},
Month = {MAR},
Abstract = {We address the consequences of corruption within a state on the extent
   to which populations have shortened life expectancy due to political
   corruption. Using three variables to estimate corruption, the results
   support the expectation that corruption increases average disability
   shortened life years (DALY). The results persist when estimating the
   model including measures of deaths from civil war and the number of
   bordering states experiencing civil war, both previously shown to have
   an effect on DALY, although these two variables are not statistically
   significant in our models. While the estimated effects of the corruption
   variables continue to capture much of the variation in DALY for a global
   group of nations, they completely fail to explain any variation in
   Sub-Saharan African states, probably because of the devastating effects
   of AIDS and malaria in that region. Finally, we discuss why this failure
   occurs and discuss the implications of our results.},
