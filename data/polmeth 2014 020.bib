@article{ WOS:000342806900004,
Author = {Alvarez-Rosete, Arturo and Mays, Nicholas},
Title = {Understanding NHS Policy Making in England: The Formulation of the NHS
   Plan, 2000},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
Year = {2014},
Volume = {16},
Number = {4},
Pages = {624-644},
Month = {NOV},
Abstract = {Through a case study of the formulation of the English NHS Plan 2000,
   this article investigates which of two competing models of the British
   policy process-the Differentiated Polity and Asymmetric Power
   Models-better describes the reality of major NHS reform policy making
   under New Labour. The process of developing the Plan showed signs of a
