@article{ WOS:000342398900005,
Author = {Seabra, Pedro},
Title = {A harder edge: reframing Brazil's power relation with Africa},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2014},
Volume = {57},
Number = {1},
Pages = {77-97},
Abstract = {Brazil's rapprochement with Africa during the last decade has been
   mostly explained as an attempt to improve political dialogue, raise
   economic interactions, and provide technical cooperation assistance.
   This article, however, argues that such framework does not sufficiently
   account for the use of Brazilian material resources in order to attain
   harder strategic goals towards the continent. By focusing on defense
   cooperation ties, it highlights an important niche in South-South
   relations that has also grown exponentially, with important consequences
   on how Brazil exerts power across the Atlantic.},
