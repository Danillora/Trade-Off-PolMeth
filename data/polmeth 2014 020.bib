@article{ WOS:000342981600008,
Author = {Wood, Matthew},
Title = {Bridging the Relevance Gap in Political Science},
Journal = {POLITICS},
Year = {2014},
Volume = {34},
Number = {3},
Pages = {275-286},
Month = {OCT},
Abstract = {This article argues that scholars overestimate the ability of a
   methodologically pluralistic' political science to gain impact in
   policymaking, which in Britain is increasingly positivist', privileging
   quantitative evidence. This relevance gap' between pluralistic political
   science and positivistic policymaking means that political scientists
   are disadvantaged in achieving impact' compared to disciplines like
   behavioural economics. The article proposes two solutions to bridge this
   gap': improving the accessibility' of research and stimulating
   interaction' between researchers and policymakers through methodological
   workshops.},
