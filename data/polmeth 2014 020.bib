@article{ WOS:000330825600003,
Author = {McGowan, Francis},
Title = {Regulating innovation: European responses to shale gas development},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2014},
Volume = {23},
Number = {1},
Pages = {41-58},
Month = {JAN 2},
Abstract = {The interplay between regulation and innovation is examined through the
   case of the development of shale gas, by comparing the regulatory
   context that facilitated the diffusion of shale gas techniques (in the
   United States) with the responses to its potential development in
   different European settings (the national cases of Britain, France,
   Germany, the Netherlands and Poland and the EU institutions). The US
   regime has favoured its development, transforming the energy economy but
   raising concerns about environmental impacts. In Europe, different
   economic and political contexts have resulted in divergent responses
   amongst the countries examined, making an effective EU response
   difficult to achieve.},
