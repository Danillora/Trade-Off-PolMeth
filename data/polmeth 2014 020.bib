@article{ WOS:000214274200007,
Author = {Kutting, Gabriela},
Title = {Rethinking global environmental governance: coordinating ecological
   policy},
Journal = {CRITICAL POLICY STUDIES},
Year = {2014},
Volume = {8},
Number = {2},
Pages = {227-234},
Abstract = {This forum piece seeks evaluate the milestones in global environmental
   politics and policy research in the past 20 years in order to identify
   the challenges for this policy area. Many achievements have been made
   but it is obvious that the challenges faced in terms of global
   environmental degradation have not become any less. While an impressive
   amount of knowledge has been gathered and the field of global
   environmental politics is now firmly established as one of the main
   subfields of political science and/or international relations, it cannot
   really be said that a concomitant amount of progress has been made in
   defining effective pathways for designing international networks
