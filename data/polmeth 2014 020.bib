@article{ WOS:000343821500004,
Author = {Corbett, Jack},
Title = {But why do we need politicians? A critical review},
Journal = {POLICY STUDIES},
Year = {2014},
Volume = {35},
Number = {5},
Pages = {498-512},
Month = {SEP 3},
Abstract = {In response to growing popular dissatisfaction with politics and
   politicians there has been a marked increase in academic work about
   anti-politics and depoliticization with numerous scholars seeking to
   defend politics by restating why it matters. However, these efforts have
   largely glossed over the related question of why politicians also
   matter. To fill this gap I propose a typology that captures how the
   different intellectual perspectives in this debate see the role of
   politicians - identifying six in particular: procedural, legitimacy,
   values, authority, persuasion and dissimulation. In doing so I review
   each contribution and highlight synergies and disagreements between them
   that in-turn reveal important insights and new lines of inquiry.},
