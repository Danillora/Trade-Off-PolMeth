@article{ WOS:000339014200010,
Author = {Vlcek, William},
Title = {From Road Town to Shanghai: Situating the Caribbean in Global Capital
   Flows to China},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
