@article{ WOS:000342886500006,
Author = {Towns, Ann E.},
Title = {Manning the State: European Masculinities and the New Norway},
Journal = {INTERNASJONAL POLITIKK},
Year = {2014},
Volume = {72},
Number = {3},
Pages = {388-404},
Abstract = {Gender historians have identified the period around 1800 as a crucial
   time for the transforming of elite masculinities in Europe. This article
   contributes to scholarship on the intersection of masculinities and
   states by centring on the formation of a new, partially independent,
   Norway around 184, when the Norwegian constitution was crafted. I use
   the analysis of Norway as an entry to exploring broader European trends
   at that time. As this article shows, Norwegian ideals of masculinity
   suitable for state office are a good illustration of more general
   interpretations of elite manhood in northern Europe. Norwegian
   constructions did offer a twist, however, as they idealized the
   Norwegian elite male as rough, unsophisticated and natural. In contrast
   to other forms of elite state-bearing European masculinity, which
   emphasized advancement and cultivation, Norwegian masculinity was
   celebrated for its lack of refinement.},
