@article{ WOS:000344308000009,
Author = {Zwart, Tom},
Title = {Safeguarding the Universal Acceptance of Human Rights Through the
   Receptor Approach},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2014},
Volume = {36},
Number = {4},
Pages = {898-904},
Month = {NOV},
Abstract = {The receptor approach relies on ethnographic research to identify social
   institutions and cultural values that match international human rights
   obligations. Where these institutions and values fall short, home-grown
   remedies are used to amplify them. The receptor approach provides a
   practical tool to activists and states. In addition, it welcomes culture
   as a potential source of human rights rather than dismissing it as an
   impediment to their protection. Yvonne Donders and Vincent Vleugel's
   position that it is ``old wine in new bags{''} is therefore unfounded.
   The same is true for their argument that it pits ``the West against the
   rest.{''} Research shows that regional values are still notably
   different. States are entitled to take these cultural differences into
   account when implementing their human rights obligations. Denying them
   their right to do so will force Southern states to disengage.},
