@article{ WOS:000421307600007,
Author = {Lafontaine, Fannie},
Title = {Universal jurisdiction and Africa: interference or complementarity?},
Journal = {ETUDES INTERNATIONALES},
Year = {2014},
Volume = {45},
Number = {1},
Pages = {129-151},
Month = {MAR},
Abstract = {The exercise of universal jurisdiction highlights a potential
   contradiction between fundamental principles of international law, i.e.,
   on the one side, State sovereignty and non-interference in States'
   internal affairs and, on the other, the fight against impunity for
   international crimes. Whereas the African Union has frequently affirmed
   its support for a robust system to fight impunity, it has opposed what
   it perceives as an abusive exercise of universal jurisdiction by certain
   States, mostly European. This article addresses, in light of the African
   Union's main legal positions, a few potential limits that allow for a
   sensible notion of universal jurisdiction.},
