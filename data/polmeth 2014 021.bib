@article{ WOS:000344417700003,
Author = {Carnes, Matthew E. and Mares, Isabela},
Title = {Coalitional realignment and the adoption of non-contributory social
   insurance programmes in Latin America},
Journal = {SOCIO-ECONOMIC REVIEW},
Year = {2014},
Volume = {12},
Number = {4},
Pages = {695-722},
Month = {OCT},
Abstract = {What explains the recent rise in non-contributory social insurance
   programmes in Latin America? Since the 1990s, Latin American countries
   have enacted significant social policy reforms that have supplemented
   contributory insurance policies with new or expanded non-contributory
   programmes, financed by general tax revenues. We present a political
   explanation for this phenomenon, arguing that the process of
   deindustrialization, and especially the increased labour insecurity that
   it entails, has changed the mix of social insurance policies favoured by
   different individuals in the income and employment distributions. A
   long-term increase in economic insecurity has increased the relative
   demand for tax-financed, rather than contribution-financed, social
   insurance. Elected officials, especially those in the recent `rise of
   the Left' in Latin America, have proved eager to meet these demands. We
   find strong evidence for this argument using both cross-national data on
   non-contributory pension policy adoption and individual-level data on
   policy preferences.},
