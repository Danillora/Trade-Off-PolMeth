@article{ WOS:000360410000007,
Author = {Ozohu-Suleiman, Yakubu and Ishak, Sidin Ahmad},
Title = {Local Media in Global Conflict: Southeast Asian Newspapers and the
   Politics of Peace in Israel/Palestine},
Journal = {INTERNATIONAL JOURNAL OF CONFLICT AND VIOLENCE},
Year = {2014},
Volume = {8},
Number = {2},
Pages = {284-295},
Abstract = {It is often assumed that local media are a potential deescalating tool
   in global conflict. This study examines how four leading newspapers in
   Southeast Asia (Star of Malaysia, Philstar of the Philippines, Jakarta
   Post of Indonesia, and The Nation of Thailand) reported the
   Israeli-Palestinian conflict during the year after the 2009 Gaza War. A
   census of 536 reports was coded for tones (to detect alignment), frames
   (to detect characterization of the conflict), and sources (to examine
   correlation with coverage tones). The results show fragmented alignment
   of the newspapers with Palestine and Israel. Conflict frames on
   offensives, fighting, threats, military strategies, demonization, death,
   and destruction were most prevalent. Coverage tones were significantly
   correlated with sources, suggesting that the potential of local media to
   serve as deescalating tools in global conflicts is subject to the
   varying political contexts in which they operate in relation to specific
   conflicts.},
