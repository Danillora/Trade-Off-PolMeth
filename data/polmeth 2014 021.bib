@article{ WOS:000333033800006,
Author = {Poloni-Staudinger, Lori and Ortbals, Candice},
Title = {The Domestic Determinants of Transnational Activity: An Examination of
   Women's Groups in the United Kingdom, France, and Germany},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
Year = {2014},
Volume = {58},
Number = {1},
Pages = {68-78},
Month = {MAR},
Abstract = {We examine the degree to which national political setting, namely
   domestic political opportunity structures, influences the transnational
   activities of women's groups in the United Kingdom, France, and Germany.
   The literature suggests that social groups are more likely to choose
   international activity when national institutions provide fewer
   opportunities for domestic activity (Keck and Sikkink 1998; della Porta
   and Tarrow 2005). Using data about women's groups' activity from a
   content analysis of news wires from 1980 to 2008, we conclude that
   women's groups act in the domestic sphere significantly more than they
   act in the international arena-even when acting on transnational
   issues-and that groups choose international action when domestic
   opportunities are less hospitable to group action. Thus, we argue that
   the domestic sphere continues to be a major influence on social movement
   activity even as globalization and transnationalism increase.},
