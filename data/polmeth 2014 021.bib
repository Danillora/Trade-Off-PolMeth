@article{ WOS:000344594800004,
Author = {Pautz, Hartwig},
Title = {British Think-Tanks and Their Collaborative and Communicative Networks},
Journal = {POLITICS},
Year = {2014},
Volume = {34},
Number = {4},
Pages = {345-361},
Month = {DEC},
Abstract = {This article looks at UK-based think-tanks and their communicative and
   cooperative networks with other actors in Britain and beyond. Which
   institutions and people do British think-tanks talk to most to exert
