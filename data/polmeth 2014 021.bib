@article{ WOS:000331661200005,
Author = {Kruck, Andreas},
Title = {Theorising the use of private military and security companies: a
   synthetic perspective},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
