@article{ WOS:000328607600006,
Author = {Cerullo, Margaret and Valino, Raziel},
Title = {The Lives of Lupe/Lupin A Queer Mexican Story in the Time of
   Neoliberalism},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2014},
Volume = {41},
Number = {1},
Pages = {89-106},
Month = {JAN},
Abstract = {The life history of a Mexican hombre/mujer (man/woman) who was brutally
   murdered in her home village in 2007 can best be understood by queering
   the framework of violence against women or feminicide and emphasizing
   the impact of neoliberalism and transnational migration on the gender
   order. Attention to the ways in which queerness has entered the
   vernacular discourses of transnational migration reveals little-noticed
   dimensions of the epochal transformations through which we are
   all-differentially-living.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
