@article{ WOS:000330190000006,
Author = {Van Damme, Julie and Ansoms, An and Baret, Philippe V.},
Title = {Agricultural innovation from above and from below: Confrontation and
   integration on Rwanda's Hills},
Journal = {AFRICAN AFFAIRS},
Year = {2014},
Volume = {113},
Number = {450},
Pages = {108-127},
Month = {JAN},
Abstract = {In its 2008 World Development Report, the World Bank pleaded for a
   `Green Revolution' for sub-Saharan Africa, pointing particularly to the
   importance of including smallholder farmers. This article focuses on the
   banana cropping system in Rwanda, and on the agricultural innovations
   introduced within this system. We first consider macro-level innovations
   that are designed to promote a modernized agricultural sector and that
   correspond to the rationale of the Green Revolution. We analyse how such
   `top-down' innovations are received on the ground and show how
   smallholders seek to evade new government policies when they fail to
   reflect local economic and social realities. This demonstrates how some
   rural Rwandans are challenging the authority of the government in
   disguised ways in order to protect their local livelihoods. The Rwandan
   experience should inspire continent-wide Green Revolution policies to
   take account of the risk-coping rationale of small-scale farmers and
   their capacity to innovate `from below'.},
Publisher = {OXFORD UNIV PRESS},
Address = {GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Van Damme, J (Corresponding Author), Catholic Univ Louvain, Earth \& Life Inst, Louvain, Belgium.
   Van Damme, Julie, Catholic Univ Louvain, Earth \& Life Inst, Louvain, Belgium.
   Ansoms, An; Baret, Philippe V., Catholic Univ Louvain, Louvain, Belgium.},
DOI = {10.1093/afraf/adt067},
ISSN = {0001-9909},
