@article{ WOS:000341407300005,
Author = {Aran, Amnon and Ginat, Rami},
Title = {Revisiting Egyptian Foreign Policy towards Israel under Mubarak: From
   Cold Peace to Strategic Peace},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2014},
Volume = {37},
Number = {4},
Pages = {556-583},
Abstract = {This article is the first academic study of Egyptian foreign policy
   towards Israel under Hosni Mubarak (1981-2011). It challenges a deeply
   entrenched conventional wisdom that Egypt pursued a cold-peace foreign
   policy towards Israel throughout this period. We demonstrate that
   Egyptian foreign policy towards Israel was dynamic - comprising cold
   peace (1981-91), a hybrid foreign policy of cold peace and strategic
