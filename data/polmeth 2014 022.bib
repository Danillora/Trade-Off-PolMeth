@article{ WOS:000435289100005,
Author = {Naughton, Tim},
Title = {When permissiveness constrains: money, regulation and the development of
   party politics in the Czech Republic (1989-2012)},
Journal = {EAST EUROPEAN POLITICS},
Year = {2014},
Volume = {30},
Number = {3},
Pages = {372-388},
Abstract = {Party laws and regulation in the Czech Republic are largely permissive
   and have served the interests of the established parties, helping to
   underpin the largely oligopolistic nature of Czech party politics.
   Whereas non-financial regulation remained virtually unchanged in the
   first two post-communist decades, party financing laws have been
   amended, often in response to scandal. Although the existing system of
   regulation has helped entrench the position of the main political
   parties, the system is not impervious to change. Indeed, a ruling of the
   Constitutional Court combined with the anti-corruption appeal of new
   parties helped weaken the position of the largest parties in the 2010
   elections. Subsequent scandals have ensured the continuing fragile
   stability of Czech party politics.},
