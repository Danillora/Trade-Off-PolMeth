@article{ WOS:000344140500005,
Author = {Triandafyllidou, Anna},
Title = {Greek Migration Policy in the 2010s: Europeanization Tensions at a Time
   of Crisis},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2014},
Volume = {36},
Number = {4},
Pages = {409-425},
Abstract = {During the past two years Greek migration policy has seen important
   developments concerning the legislative framework for irregular
   migration/asylum management and migrant integration. Given that several
   among these developments are related to the transposition of related EU
   directives, one obvious answer might be that of Europeanization: these
   developments had less to do with the Greek government's plans about
   migration, rather they were the direct impact of Europeanization; Greece
   simply transposed relevant EU directives. I am arguing here for a more
   careful reading of the Europeanization effect which not only
   distinguishes the differential impact of Europeanization on policies and
   discourses, but also actually shows how Europeanization tendencies at
   different level can contrast one another. The findings of this paper
   contribute to a better understanding of Europeanization processes. They
   highlight that Europeanization involves also resistance to Europe
   especially at times of crisis.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
