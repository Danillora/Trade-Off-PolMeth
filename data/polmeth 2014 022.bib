@article{ WOS:000334290500003,
Author = {Bleek, Philipp C. and Lorber, Eric B.},
Title = {Security Guarantees and Allied Nuclear Proliferation},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2014},
Volume = {58},
Number = {3, SI},
Pages = {429-454},
Month = {APR},
Abstract = {As Iran continues its apparent pursuit of a nuclear weapons breakout
   capability and North Korea resists efforts to roll back its
   proliferation, policy makers in Washington eager to prevent further
   proliferation in both regions regard security guarantees to allies as
   crucial tools. But recent scholarship calls into question whether
   security guarantees ameliorate proliferation risks. Relying on a
   combination of large-N quantitative analysis and a case study of South
   Korea from the late 1960s to the mid-1980s, this article argues that,
   consistent with policy makers' conventional wisdom, security guarantees
   significantly reduce proliferation proclivity among their recipients.},
