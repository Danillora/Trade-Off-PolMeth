@article{ WOS:000214274200006,
Author = {Braun, Kathrin and Herrmann, Svea L. and Brekke, Ole A.},
Title = {Sterilization policies, moral rehabilitation and the politics of amends},
Journal = {CRITICAL POLICY STUDIES},
Year = {2014},
Volume = {8},
Number = {2},
Pages = {203-226},
Abstract = {The article studies policy debates about coming to terms with historic
   sterilization policies in Germany, Norway, and the Czech Republic.
   Starting from the assumption that involuntary sterilization forms a
   double injury that both destroys a person's reproductive faculty and
   attaches a stigma of inferior worth to the person, the article examines
   what - if anything - was framed as wrong about involuntary sterilization
   and whether the aspect of stigmatization and differential worth was
   addressed in these debates. It argues that a politics of amends can, in
   principle, revoke the stigma and provide moral rehabilitation but that
   governments were unwilling or reluctant to do so, particularly
   concerning stigmatization along notions of normality and fitness.
   Problematizations were largely confined to an unlawfulness frame and a
   group discrimination frame. Within the former, the logic of differential
   worth could not be addressed, within the latter only insofar as it
   disproportionately affected ethnoracially constructed groups but not
   individuals marked as ill, disabled or unfit.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Braun, K (Corresponding Author), Leibniz Univ Hannover, Dept Polit Sci, Hannover, Germany.
   Braun, Kathrin; Herrmann, Svea L., Leibniz Univ Hannover, Dept Polit Sci, Hannover, Germany.
   Brekke, Ole A., Stein Rokkan Ctr, Uni Res, Bergen, Norway.},
