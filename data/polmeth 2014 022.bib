@article{ WOS:000335835600004,
Author = {Chatterje-Doody, P. N.},
Title = {Harnessing History: Narratives, Identity and Perceptions of Russia's
   Post-Soviet role},
Journal = {POLITICS},
Year = {2014},
Volume = {34},
Number = {2},
Pages = {126-137},
Month = {JUN},
Abstract = {Russian political elites have long been aware of the power of myths to
   forge national unity. However, the past six or seven years have seen
   core myths increasingly situated within a highly selective narrative of
   Russian history. This narrative is accepted as contextual information
   for policy discussion, and so sets cognitive parameters for evaluations
   of Russia's history, identity and role. This standard narrative of
   Russian history prioritises the state, supports gradualism and
   continuity, and dramatically reduces the potential for
   re-conceptualising Russia's role in contemporary international
   relations.},
