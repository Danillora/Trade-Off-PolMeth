@incollection{ WOS:000364351300008,
Author = {von Gleich, Utta},
Editor = {Zajicova, L and Zamec, R},
Title = {Bolivia: Intercultural Multilingualism in a Plurinational State},
Booktitle = {LENGUA Y POLITICA EN AMERICA LATINA: PERSPECTIVAS ACTUALES},
Year = {2014},
Pages = {177-196},
Abstract = {The Bolivian answers to the challenges of national multilingualism
   within a pluricultural environment have been: first, a new Constitution
   in 2009, setting the fundamentals of the Plurinational State of Bolivia
   and second, a flexible and coherent language policy marking the
   guidelines for the management of the local and foreign languages in
   Bolivia and supporting the process of multilingual interculturalization.
   The objectives of this new language policy are: the democratic
   intercultural cohabitation among all Bolivians, the maintenance and
   revitalization of the indigenous languages within a framework of a
   multilingual and multicultural society. Moreover, the present
   contribution explains the strategies and advances of the actual
   implementation of the respective legislation.},
