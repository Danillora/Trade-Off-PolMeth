@article{ WOS:000409503300005,
Author = {Hwang, Wonjae and Down, Ian},
Title = {Trade, societal interests, and political parties},
Journal = {BUSINESS AND POLITICS},
Year = {2014},
Volume = {16},
Number = {2},
Pages = {313-338},
Month = {AUG},
Abstract = {We argue that international trade affects party systems but that this
   impact is conditioned on the types of societal interests trade brings
   about. When factor mobility is high, trade promotes class-based
   grievances that are unlikely to affect the structure of the party
   system. However, when factor mobility is low, trade will increase the
   diversity of group interests and policy preferences, thereby pressuring
   structural change in the party system. A consequence is an increase in
   the effective number of political parties. The empirical analysis
   supports these expectations. This paper contributes to our understanding
   of the political impact of economic liberalization on representative
   democracy.},
