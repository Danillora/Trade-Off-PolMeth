@incollection{ WOS:000396054400006,
Book-Author = {Razeq, ZM},
Title = {The United Nations Development Programme and the Private Sector for
   Development},
Booktitle = {UNDP'S ENGAGEMENT WITH THE PRIVATE SECTOR, 1994-2011},
Series = {Palgrave Pivot},
Year = {2014},
Pages = {40-80},
Abstract = {In the past two decades, the UNDP went through a significant change in
   its strategies towards the role and involvement of the private sector in
   development. Short-term and contractual relations of the 1990s and
   earlier gave way to more systematic and productive partnerships based on
   common interests and values for development. The purpose of this chapter
   is to reconstruct, based on primary sources and official documents, the
   analytical narrative of policy transformation at the UNDP between 1994
   and 2011. It divides chronologically the process of change into shorter
   periods and focuses specifically on agenda setting, policy formulation,
   and launch of UNDP-private sector initiatives in order to highlight the
   dynamics of the involvement of the Programme in CSR-related activities
   and interaction with the private sector.},
