@article{ WOS:000345379900003,
Author = {Ramas Canete, Reuben and Santamaria, Matthew C. M. and San Valentin,
   Marc},
Title = {Sulu sojourns: Photo-ethnography and political discourses on four
   ethno-linguistic groups in the Sulu and Tawi-Tawi Archipelagoes},
Journal = {PHILIPPINE POLITICAL SCIENCE JOURNAL},
Year = {2014},
Volume = {35},
Number = {2},
Pages = {136-157},
Month = {JUL 3},
Abstract = {This paper examines how photography is used to project particular images
   of the nation as well as to facilitate imagining the nation.
   Specifically, it looks at the images of peoples, artifacts and places
   taken in the Sulu Archipelago from 1974 to 1980 by researchers of the
   Philippine Center for Advanced Studies (PCAS), the acknowledged
   think-tank of the Marcos regime within the academe. Analyzed together
   with texts coming from publications which carry them, these photographs
   reveal multiple if not contending discourses of the nation that richly
   characterize the fabric of identity politics, developmental hierarchy
   and modernity in the ``problematic{''} locale that is Sulu. Part 1
   interrogates photography and its relationship to discourses of power and
   nation. It also gives a brief background of the PCAS and its immediate
   context of the authoritarian rule of President Ferdinand E. Marcos. Part
   2 presents an analysis of a selection of photographs from the Sulu
   Archipelago, its captions and its other allied texts. Part 3, by way of
   conclusion, presents a model of discursive projection and reception of
   photographs and texts.},
