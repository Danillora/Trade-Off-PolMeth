@article{ WOS:000340716900004,
Author = {Frangi, Lorenzo and Memoli, Vincenzo},
Title = {Confidence in Brazilian Unions A Longitudinal Analysis},
Journal = {LATIN AMERICAN PERSPECTIVES},
