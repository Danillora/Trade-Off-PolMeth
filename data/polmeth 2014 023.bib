@incollection{ WOS:000342927800007,
Author = {Paul, Erik},
Book-Author = {Paul, E},
Title = {Post-Democracy},
Booktitle = {AUSTRALIA AS US CLIENT STATE: THE GEOPOLITICS OF DE-DEMOCRATIZATION AND
   INSECURITY},
Year = {2014},
Pages = {75-86},
Abstract = {Australian neoliberalism and neoconservatism undermines democratic
   institutions and depoliticizes citizens. Structural violence is built in
   Australia's role in the American empire project. Climate change is
   likely to become a serious challenge to the existing culture of
   contentment. The slowing of global economic growth and the
   internalization of social and environmental costs will lower living
   standards for most Australians as the country prepares for more military
   interventions and wars. Pathways to peace require the conversion of
   Australia's war economy, which is unlikely in view of the outcome of the
   2013 Federal election.},
