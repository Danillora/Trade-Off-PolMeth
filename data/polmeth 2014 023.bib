@article{ WOS:000334661300003,
Author = {Herve, Adrienne},
Title = {Roles of brokerage networks in transnational advocacy networks},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2014},
Volume = {23},
Number = {3},
Pages = {395-416},
Month = {MAY 4},
Abstract = {Transnational advocacy networks (TANs) with similar goals are linked to
   one another through brokerage networks. Analysis of a network of 10 TANs
   devoted to environmental issues and economic reform, and consisting of
   436 advocacy organisations in 75 countries, revealed the existence of
   two subnetworks whose members served in several capacities as brokers:
   12 advocacy organisations, which belonged to four or more TANs; eight
