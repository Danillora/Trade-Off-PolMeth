@article{ WOS:000331259400007,
Author = {Hearn, Eddie},
Title = {Harm, fairness and trade policy preferences: An experimental examination
   of sincere fair-trade preferences},
Journal = {INTERNATIONAL POLITICS},
Year = {2014},
Volume = {51},
Number = {1},
Pages = {124-135},
Month = {JAN},
Abstract = {Although fair trade has traditionally been considered protectionism in
   disguise, recent literature has found that some fair-trade preferences
   are in fact sincere. This article provides a psychological theory of
   sincere fair-trade preferences. This theory is grounded in a current
   cognitive-social model of human cognition and the do-no-harm
   decision-making heuristic. Through a laboratory experiment, an implicit
   measure is used to estimate the extent to which respondents consider
   fairness when forming trade attitudes. It is found that, when exposed to
   information about low labor standards abroad, thinking about fairness
   decreases support for free trade. The results show that fairness plays a
   meaningful role in trade attitudes formation when trade is framed as
   violating accepted social norms.},
