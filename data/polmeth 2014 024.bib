@article{ WOS:000337785500008,
Author = {Zweig, David and Yang, Feng},
Title = {Overseas Students, Returnees, and the Diffusion of International Norms
   into Post-Mao China},
Journal = {INTERNATIONAL STUDIES REVIEW},
Year = {2014},
Volume = {16},
Number = {2},
Pages = {252-263},
Month = {JUN},
Abstract = {This paper applies the model of diffusion outlined by Solingen
   (International Studies Quarterly, 56, 2012, 631) to the case of Chinese
   who studied abroad after 1978. It assesses the ability of those who have
   not returned to pressure the state to introduce Western academic,
   scientific, and business norms. It looks at the role of the returnees
   and national leaders in introducing these norms, particularly as a means
   to create world-class universities, scientific research centers, and
   modern private firms. It demonstrates the power of firewalls (including
   institutional leaders, the Chinese marketplace, and administrators who
   lose under reform) to block the diffusionary process.},
