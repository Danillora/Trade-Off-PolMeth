@article{ WOS:000340098900002,
Author = {Ndjio, Basile},
Title = {``MAGIC BODY{''} AND ``CURSED SEX{''}: CHINESE SEX WORKERS AS
   ``BITCH-WITCHES{''} IN CAMEROON},
Journal = {AFRICAN AFFAIRS},
