@article{ WOS:000339858900002,
Author = {Badulescu, Alina and Badulescu, Daniel and Borma, Afrodita},
Title = {Enhancing Cross-Border Cooperation through Local Actors' Involvement.
   The Case of Tourism Cooperation in Bihor (Romania)-Hajdu-Bihar (Hungary)
   Euroregion},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2014},
Volume = {12},
Number = {3},
Pages = {349-371},
Month = {JUL},
Abstract = {For centuries, borders have been considered as firm lines legitimating
   and defending the essence of the nation-state. However, present times
   report a redefinition of the role and importance of the border and of
   the border regions. At European level, the emergence and development of
   a common European policy have fostered cross-border cooperation (CBC)
   and self-governance through an increased involvement of local
   authorities and communities. The paper investigates, both theoretically
   and based on an empirical quantitative and qualitative research, the
   role and effectiveness of CBC cooperation in tourism sector in, Bihor -
   Hajdu-Bihar Euroregion.},
