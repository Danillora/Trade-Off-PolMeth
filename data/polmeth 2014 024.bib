@article{ WOS:000333945300004,
Author = {Hawkins, Ben and Holden, Chris},
Title = {`Water dripping on stone'? Industry lobbying and UK alcohol policy},
Journal = {POLICY AND POLITICS},
Year = {2014},
Volume = {42},
Number = {1},
Pages = {55-70},
Month = {JAN},
Abstract = {Despite a growing literature on corporations as political actors,
   relatively little is known about how alcohol industry actors attempt to
   influence public policy. In the context of contemporary debates about
   the minimum pricing of alcoholic beverages, and drawing on
   semi-structured interviews with a range of key informants, this article
   investigates the means by which alcohol industry actors gain access to
   policy makers and the strategies used to influence policy. It finds that
   the strategies of alcohol industry actors are focused on long-term
   relationship building with policy makers, involving the provision and
   interpretation of information and the promotion of various forms of
   self-regulation.},
