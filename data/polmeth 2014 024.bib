@article{ WOS:000344246800003,
Author = {Youde, Jeremy},
Title = {Shame, ontological insecurity and intercountry adoption},
Journal = {CAMBRIDGE REVIEW OF INTERNATIONAL AFFAIRS},
Year = {2014},
Volume = {27},
Number = {3},
Pages = {424-441},
Month = {JUL 3},
Abstract = {Romania and South Korea were once among the top sending countries for
   adoptive American families. In recent years, though, both have adopted
   significant restrictions on intercountry adoption. What leads countries
   to introduce such severe restrictions? This article argues that shame
   plays a significant, yet underappreciated, motivating factor for leading
   governments to change their laws on intercountry adoption. Political
   leaders seize on nationalist rhetoric to argue that intercountry
   adoption is shameful. The article explores the interaction between
   intercountry adoption and shame through brief case studies on Romania
   and South Korea. Finally, it examines the role of the Hague Convention
   on Protection of Children and Cooperation in Respect of Intercountry
   Adoption (Hague Adoption Convention) in ameliorating these feelings.},
