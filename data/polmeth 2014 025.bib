@article{ WOS:000340163900008,
Author = {Tatham, Michael and Bauer, Michael W.},
Title = {Competence ring-fencing from below? The drivers of regional demands for
   control over upwards dispersion},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2014},
Volume = {21},
Number = {9, SI},
Pages = {1367-1385},
Abstract = {Powers have been transferred and delegated to varying degrees of depth
   and scope at both the sub- and supranational levels. The resulting
   competence overlap is sometimes substantial. This has driven a number of
   regional authorities to demand a greater say in future power dispersion
   movements to the supranational level, so as to pre-empt their
   proportional disempowerment. We investigate what drives these demands
   using interview data collected from over 300 senior regional officials
   in 60 regions and five countries. Controlling for economic and
   demographic characteristics, we find that the status quo institutional
   arrangement in place for each region, both supranationally (the
   Committee of the Regions) and domestically (shared rule and self-rule)
   significantly affects such demands. These findings have implications for
