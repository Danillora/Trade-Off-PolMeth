@article{ WOS:000343621100006,
Author = {Foster, Liam},
Title = {Women's pensions in the European Union and the current economic crisis},
Journal = {POLICY AND POLITICS},
Year = {2014},
Volume = {42},
Number = {4},
Pages = {565-580},
Month = {OCT},
Abstract = {The current financial and economic crisis has serious implications for
   the underlying ageing challenge. It has demonstrated weaknesses in the
   design and long-term sustainability of numerous pension schemes. While
   many EU countries have undertaken measures to adapt to these challenges
   it is important to ensure that the effects of the crisis and austerity
   plans do not adversely affect women. Initially this article outlines
   women's employment and pension situation before considering the
   implications of the crisis on their pension prospects. Finally, it
   highlights the need for in-depth discussions about pension systems in
   the EU which place women at the centre.},
