@article{ WOS:000340186100004,
Author = {Vergani, Matteo},
Title = {Neo-Jihadist Prosumers and Al Qaeda Single Narrative: The Case
   Study of Giuliano Delnevo},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2014},
Volume = {37},
Number = {7},
Pages = {604-617},
Abstract = {Scholars in the field of terrorism and violent extremism often refer to
   the so-called Al Qaeda single narrative. This article suggests that the
   Internet challenges the existence of a ``single narrative,{''} by
   arguing that neo-jihadist prosumers may reinterpret Al Qaeda's narrative
   and create hybrid symbols and identities. The article discusses the case
   study of an Italian neo-jihadist allegedly killed in Syria, Giuliano
   Delnevo, presenting research on his YouTube and Facebook production.
   Delnevo's narrative, which emerges from the diverse messages circulating
   on the Internet, recasts the Al Qaeda narrative by hybridizing it with
   other cultural backgrounds and political symbols.},
