   Hellwig, Timothy, Indiana Univ, Bloomington, IN 47405 USA.
   Hellwig, Timothy, Indiana Univ, Inst European Studies, Bloomington, IN 47405 USA.},
DOI = {10.1111/isqu.12129},
ISSN = {0020-8833},
EISSN = {1468-2478},
