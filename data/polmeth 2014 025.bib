@article{ WOS:000435289100001,
Author = {Bertoa, Fernando Casal and van Biezen, Ingrid},
Title = {Party regulation and party politics in post-communist Europe},
Journal = {EAST EUROPEAN POLITICS},
Year = {2014},
Volume = {30},
Number = {3},
Pages = {295-314},
Abstract = {Recent years have seen a considerable increase in the scope and
   magnitude of party regulations. Despite the increased propensity of the
   modem democratic states to intervene in the activities, organisation and
   behaviour of parties, the phenomenon of party regulation has received
   relatively little systematic and comparative scholarly attention. In
   order to fill part of the existing gap in the literature, we concentrate
   on the regulation of political parties in the post-communist democracies
   of East-Central and Southeast Europe. Our focus is on the formal rules
   as stipulated in Party Laws, finance laws and national constitutions,
   while we explore their impact on the party organisations and party
   systems.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
