@article{ WOS:000346586300006,
Author = {Meyer, Renate E. and Egger-Peitler, Isabell and Hoellerer, Markus A. and
   Hammerschmid, Gerhard},
Title = {OF BUREAUCRATS AND PASSIONATE PUBLIC MANAGERS: INSTITUTIONAL LOGICS,
   EXECUTIVE IDENTITIES, AND PUBLIC SERVICE MOTIVATION},
Journal = {PUBLIC ADMINISTRATION},
Year = {2014},
Volume = {92},
Number = {4, SI},
Pages = {861-885},
Month = {DEC},
Abstract = {Over recent decades, a number of managerial reform initiatives in
   continental Europe have aimed at moving away from the traditional
   Weberian model of public administration. Such shifting bases of
   legitimacy are brought about by changes in the institutional logics in
   place, which not only provide frames of reference but also social
   identities and vocabularies of motive for the actors in the field. In
   this article, we approach the expanding research on public service
   motivation (PSM) by employing an institutional prism. Based on an
   executive survey in a continental European context, we examine the
   assumption that high PSM is associated with the traditional ethos and
   social identity in the public sector. What we find is that a Weberian
   legalistic-bureaucratic logic supports neither a high attraction to
   policy-making nor a high level of compassion. A managerial orientation,
