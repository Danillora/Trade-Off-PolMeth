@article{ WOS:000435286500008,
Author = {Albert, Craig Douglas},
Title = {The ethno-violence nexus: measuring ethnic group identity in Chechnya},
Journal = {EAST EUROPEAN POLITICS},
Year = {2014},
Volume = {30},
Number = {1},
Pages = {123-146},
Abstract = {If scholars and policy-makers are to understand contemporary ethnic
   conflict, we must first understand ethnic group identity. I make the
   case that ethnic group identity has substantial effects on collective
   action, particularly violent conflict, and a mechanism must exist to
   predict behaviour to properly measure ethnic group identity. This study
   asks, then: what exactly is ethnic group identity and can it be
   accurately measured? I address this by developing the Ethnic Group
   Identity Index (EGII), which seeks to measure the strength of ethnic
   group identity. I then use the EGII to measure strength of ethnic group
   identity within Chechnya. The implications abound for policy-makers in
   matters of conflict management strategies for the Russo-Chechen
   conflict, and more generally, for all ethnic politics.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
