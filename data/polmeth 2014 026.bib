@article{ WOS:000347681500005,
Author = {Teixeira, Conceicao Pequito and Tsatsanis, Emmanouil and Belchior, Ana
   Maria},
Title = {Support for Democracy in Times of Crisis: Diffuse and Specific Regime
   Support in Portugal and Greece},
Journal = {SOUTH EUROPEAN SOCIETY AND POLITICS},
Year = {2014},
Volume = {19},
Number = {4},
Pages = {501-518},
Month = {OCT 2},
Abstract = {In the context of a cross-national economic crisis, questions about
   democratic legitimacy are again topical. We explore the question of
   democratic legitimacy in Portugal and Greece by employing the conceptual
   differentiation between diffuse and specific regime support. Our
   findings indicate that while specific support has fallen in both
