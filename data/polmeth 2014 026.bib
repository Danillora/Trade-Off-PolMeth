@article{ WOS:000328670300014,
Author = {Cohen, Katie and Johansson, Fredrik and Kaati, Lisa and Mork, Jonas
   Clausen},
Title = {Detecting Linguistic Markers for Radical Violence in Social Media},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2014},
