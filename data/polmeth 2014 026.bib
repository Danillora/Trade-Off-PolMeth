
@article{ WOS:000212587300004,
Author = {Hayward, Margaret},
Title = {The influence of party leaders on women's representation in parliament,
   1935-1975: the case of New Zealand},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2014},
Volume = {52},
Number = {2},
Pages = {254-270},
Abstract = {New Zealand women gained the vote in 1893, the first women in the world
   to do so. But they could not stand as candidates for Parliament. Early
   twentieth-century party leaders were reluctant to allow women full
   suffrage. This article examines the attitudes and influence of New
   Zealand party leaders from 1935 to 1975 in helping or hindering women
   gain representation in Parliament. It concludes that during those 40
   years the most influential gatekeepers were the party leaders. By 1972,
   however, the Second Wave of Feminism and the changing attitudes of some
   party leaders resulted in women gaining more opportunities to become MPs
   and by 2001 an unprecedented number of women held leadership positions
   in the New Zealand Parliament.},
