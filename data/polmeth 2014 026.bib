@article{ WOS:000333033800008,
Author = {Hwang, Wonjae and Lee, Hoon},
Title = {Globalization, Factor Mobility, Partisanship, and Compensation Policies},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
