@article{ WOS:000330825600011,
Author = {MacNeil, Robert},
Title = {Canadian environmental policy under Conservative majority rule},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2014},
Volume = {23},
Number = {1},
Pages = {174-178},
Month = {JAN 2},
Abstract = {Prior to the May 2011 federal election, Canada had never really
   experienced an intensely right-wing majority government. Given the basic
   nature of electoral coalition building (which had traditionally required
   that any majority government win large swaths of left-leaning Quebec, as
   well as left-leaning urban centres like Toronto, Vancouver, and
   Montreal), Canadian parliaments had habitually maintained a more-or-less
   moderate and centrist orientation. Since the early 1970s, these
   parliaments had overseen the creation of a relatively far-reaching
   federal environmental regulatory apparatus, which had remained in place
   (and continued to expand in some instances) even as the Canadian state
   took on an increasingly neoliberal complexion. In the wake of the
   Conservative Party's majority victory, however, many feared the birth of
   a new era of anti-environmentalism in Ottawa - one that could
   potentially decimate this legacy of ecological protection. Three years
   into the Conservative government's majority mandate, I consider whether
   or not such fears were well founded.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
