@article{ WOS:000218523100008,
Author = {Tol, Gonul and Akbaba, Yasemin},
Title = {Minorities on `Civilizational' Fault Lines: An Assessment of Religious
   Discrimination},
Journal = {POLITICS RELIGION \& IDEOLOGY},
Year = {2014},
Volume = {15},
Number = {1},
Pages = {161-177},
Abstract = {Previous studies examined various aspects of Huntington's controversial
   `clash of civilizations' thesis. This study aims to quantitatively
   explore the theory to understand whether civilizational ethnoreligious
   minorities, i.e. ethnoreligious minorities that belong to a different
   civilization than the majority, face religious discrimination more than
   non-civilizational ones, especially in Western and Islamic
   civilizations. Analysis shows that non-civilizational minorities are
   discriminated against more than civilizational minorities. Findings also
   suggest that while civilizational minorities are discriminated against
   less compared to non-civilizational ones in Islamic civilizations,
   Western civilizations discriminate against civilizational minorities
   more compared to non-civilizational minorities.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
