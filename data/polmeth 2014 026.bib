@incollection{ WOS:000334191900006,
Author = {Bechtol, Jr., Bruce E.},
Book-Author = {Bechtol, BE},
Title = {The ROK Military: Ominous Future Scenarios Make Readiness and
   Capabilities Vital Policy Priorities},
Booktitle = {NORTH KOREA AND REGIONAL SECURITY IN THE KIM JONG-UN ERA: A NEW
   INTERNATIONAL SECURITY DILEMMA},
Series = {Palgrave Pivot},
Year = {2014},
Pages = {78-105},
Abstract = {The ROK military faces important and compelling national defense issues
   because North Korea - one of the most unpredictable and threatening
   nation-states on earth - sits on its border. In coming years, South
   Korea must address important issues such as ballistic missile defense
   (BMD), maintaining and upgrading key systems in its conventional forces,
   reaching a burden-sharing agreement with its key ally (the United
   States) when it comes to the cost of American troops being stationed on
   the Korean Peninsula, and perhaps most importantly, the projected
   disestablishment of Combined Forces Command and the command and control
   relationship that will exist with the United States when that occurs.
   These issues are fiscally expensive and often controversial politically.
   Thus, South Korea's military and the challenges that it must face will
   be very important for security in the region in coming years.},
