@article{ WOS:000340244700012,
Author = {Mann, Joseph},
Title = {Saudi-Palestinian Relations During the Run-Up to and the Aftermath of
   Black September},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2014},
Volume = {26},
Number = {4},
Pages = {713-724},
Abstract = {The Black September events in Jordan in 1970 are an example of the
   conflict the Palestinian issue presented for monarchic regimes. On the
   one hand, wealthy regimes such as Saudi Arabia and Kuwait wanted to
   assist the Palestinians in their struggle against Israel, but on the
   other hand, the moment they understood that siding with the Palestinians
   could weaken their regimes, they renounced their support. This article,
   therefore, emphasizes the importance the monarchic regimes in the
   Persian Gulf attributed to their own stability, and the influence that
   issue had on their policies within the Arab world.},
