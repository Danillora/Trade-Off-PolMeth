@article{ WOS:000336094100001,
Author = {Steinberg, Jonny},
Title = {Policing, state power, and the transition from apartheid to democracy: A
   new perspective},
Journal = {AFRICAN AFFAIRS},
Year = {2014},
Volume = {113},
Number = {451},
Pages = {173-191},
Month = {APR},
Abstract = {With some exceptions, scholarship on post-apartheid policing has been
   too preoccupied by continuities with the apartheid era. While this is
   understandable, it has blinded scholars to profound changes. I argue
   that what has changed most since the end of apartheid is the
   relationship between policing and political order. During the late
   apartheid era, the structure and ethos of the South African Police was
   animated by the task of containing an insurgency. In the democratic era,
   policing is increasingly animated by the task of managing conflict in
   the ruling party. The difference is profound and the implications ripple
   right to the edges of the police organization, fashioning the manner in
   which street life is policed and impinging on the functioning and the
   durability of the detective service. The article concludes by arguing
   that instruments used in the past survive only when agents in the
   present find them useful, and that accounts of continuity need to train
   their analytical attention on the politics of the here and now.},
