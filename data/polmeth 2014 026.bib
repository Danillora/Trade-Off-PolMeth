@article{ WOS:000339053900004,
Author = {Patalano, Alessio},
Title = {Japan as a Seapower: Strategy, Doctrine, and Capabilities under Three
   Defence Reviews, 1995-2010},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2014},
Volume = {37},
Number = {3},
Pages = {403-441},
Abstract = {This article draws upon previously unavailable document materials to
   question views pointing to a degree of stagnation in Japanese maritime
   thinking. It similarly reviews claims about trends to compensate the
   decline of national military power with the build-up of projection
   capabilities. The article's main argument is that Japanese seapower is
   not declining. The Japanese Navy is evolving to combine enhanced
   capabilities to retain sea control in the Sea of Japan and the East
   China Sea with extended operational reach and flexibility, including an
   expeditionary component to meet alliance and diplomatic commitments in
   East Asia and beyond its confines.},
