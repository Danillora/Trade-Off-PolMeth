@article{ WOS:000334825600003,
Author = {Williams, Gavin},
Title = {Ruth First: the analysis and practice of politics in South Africa},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2014},
Volume = {41},
Number = {139, SI},
Pages = {18-37},
Month = {JAN 2},
Abstract = {This is the text prepared for a lecture given to the Walter Rodney
   Memorial Series, African Studies Centre, Boston University, 8 November
   1982, in tribute to Ruth First. It discusses her writing on South Africa
   and Namibia in the context of her political commitments and ultimately
   her assassination. It considers the forms and themes of her writing; her
   role as communist and journalist; the Congress Alliance; nationalism,
   socialism and the Freedom Charter; capital and labour; the treason trial
   to the sabotage campaign; Namibia; peasants and politics; detention
   without trial; exile and solidarity; historical interpretation and
   revolutionary strategy.},
