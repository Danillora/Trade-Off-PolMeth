@article{ WOS:000345304800020,
Author = {Kulakevich, Tatsiana},
Title = {Twenty Years in the Making: Understanding the Difficulty for Change in
   Belarus},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
Year = {2014},
Volume = {28},
Number = {4},
Pages = {887-901},
Month = {NOV},
Abstract = {Even after twenty years of Lukashenka's rule, Belarusians still have to
   struggle for democracy. However, there has been a modest growth in the
   number of protesters during presidential elections and in the pro-EU
   mood of the Belarusian population. This article analyzes the dynamic of
   this growth through the prism of social movements literature and such
   concepts as framing, political opportunity, and mobilizing structures.
   The argument is that the weakness of the mobilizing structures and
   framing processes at times when political opportunities presented
   themselves in Belarus resulted in an absence of large-scale protests and
   a failure to sustain the development of social movements in the country.
   At the same time, Belarus cannot be considered as being in a static or
   retrogressive state since transnational flows characteristic of a
   globalizing world have exposed people to wider flows of information,
   providing them with counterframes and resulting in a modest growth in
   the numbers of protesters and a change in the preferences of the
   Belarusian population.},
