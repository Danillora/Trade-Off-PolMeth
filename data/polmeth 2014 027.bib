@article{ WOS:000335883200003,
Author = {Kibris, Arzu},
Title = {The Polarization Trap},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2014},
Volume = {37},
Number = {6},
Pages = {492-522},
Month = {JUN 3},
Abstract = {This article studies the association between ethnic conflicts and
   political attitudes. It employs a new data set on the casualties of the
   ethnic conflict in Turkey to identify the effects of the Kurdish
   insurgency on the electoral behavior of the Turkish voters. The results
   indicate that the conflict leads to the political polarization of the
   society along ethnic nationalist lines. Further investigation of the
   data also provides some empirical support for the constructivist
   argument that the salience of ethnic identities is subject to change in
   response to external stimuli, of which ethnic conflict constitutes a
   drastic example.},
