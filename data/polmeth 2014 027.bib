@article{ WOS:000421315100002,
Author = {Dieng, Moda},
Title = {Maintain the peace. Inputs and limits of South African diplomatic action},
Journal = {ETUDES INTERNATIONALES},
Year = {2014},
Volume = {45},
Number = {2},
Pages = {207-227},
Month = {JUN},
Abstract = {This document looks at the contributions and limitations of South Africa
   for peacekeeping. As the top economic and military power in Africa, and
   an emerging country, South Africa plays a major diplomatic role in
   Africa, the continent with the most peacekeeping operations. Its
   abilities and leadership enable it to lead mediation processes, bring
   together external stakeholders, be on the front lines of certain
   peacekeeping operations, enhancing its credibility in managing
   conflicts. However, for some time, South Africa's commitment, which was
   already exhibiting certain weaknesses both in terms of abilities and
   leadership with regard to its reputation and status, seems to be losing
   momentum. At the same time, we have seen a rise in Realpolitik power,
   which has drawn considerable criticism.},
