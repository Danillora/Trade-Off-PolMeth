@article{ WOS:000347681500006,
Author = {Tsatsanis, Emmanouil and Freire, Andre and Tsirbas, Yannis},
Title = {The Impact of the Economic Crisis on the Ideological Space in Portugal
   and Greece: A Comparison of Elites and Voters},
Journal = {SOUTH EUROPEAN SOCIETY AND POLITICS},
Year = {2014},
Volume = {19},
Number = {4},
Pages = {519-540},
Month = {OCT 2},
Abstract = {This work addresses the dimensions and content of ideological space in
   Portugal and Greece after the onset of the sovereign debt crisis and the
   imposition of austerity policies in the two countries. By examining both
   elite interviews and public opinion data, the article principally
   attempts to determine whether the economic crisis has created a new
   division in ideological space by cross-cutting or completely replacing
   older ones. In addition, we attempt to gauge the position of
   crisis-related issues relative to issue divides on European integration
   and globalisation which, up to now, have been dormant in Southern
   European countries.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXFORDSHIRE, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Tsatsanis, E (Corresponding Author), CIES IUL Inst Univ Lisboa, Lisbon, Portugal.
   Tsatsanis, Emmanouil, CIES IUL Inst Univ Lisboa, Lisbon, Portugal.
   Freire, Andre, Inst Univ Lisboa ISCTE IUL, Lisbon, Portugal.
