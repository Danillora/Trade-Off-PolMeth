@article{ WOS:000339413400005,
Author = {Rotulo, Daniel and Baudean, Marcos and Abrucio, Fernando and Cohn,
   Natalia},
Title = {Governance and Cross-border Cooperation Policies: the Case of
   Brazil-Uruguay},
