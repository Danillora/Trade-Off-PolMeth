@article{ WOS:000337977000003,
Author = {Kronenberg, Clive W.},
Title = {Equality, Solidarity, and the Human Condition Categories of Humanism in
   Jose Marti's Anticolonial Critique},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2014},
Volume = {41},
Number = {4},
Pages = {28-47},
Month = {JUL},
Abstract = {An examination of the humanist thought embedded in Jose Marti's
   anticolonial critique reveals his deep devotion to human solidarity and
   human equality, with education, in particular, seen as ``an act of
   love.{''} Underpinning this commitment is Marti's considerate awareness
   of the ``human condition{''} of the oppressed and colonized. Whereas
   humanism as theorized and practiced in Western liberal circles has
   neglected to challenge the current order of oppression, misery, and
   suffering, Marti's humanist critique is situated squarely on the side of
   victims, waving the banner of human dignity and equality as well as of
   solidarity with the disfavored.},
Publisher = {SAGE PUBLICATIONS INC},
