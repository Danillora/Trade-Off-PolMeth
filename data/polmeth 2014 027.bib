@article{ WOS:000338596300006,
Author = {Kim, Yeon Joo},
Title = {Economic sanctions and the rhetorical responses of totalitarian regimes:
   Examining North Korean rhetorical strategies, 1949-2010},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2014},
Volume = {47},
Number = {2, SI},
Pages = {159-169},
Month = {JUN},
Abstract = {This study investigates the impact of economic statecraft on the North
   Korean Government. As a totalitarian regime, which is characterized by a
   controlled mass media, the North Korean Government tries to contain
   potential problems caused by sanctions by using three types of political
   rhetoric: appeasement, backlash, and surveillance. Using time-series
   data from 1949 to 2010 derived from a content analysis of the New Year's
   Day addresses by Kim Il Sung, Kim Jong Il, and Kim Jong Un, the
   empirical results suggests that the North Korean Government does alter
   its rhetorical strategies in response to external economic sanctions.
   Negative sanctions cause the regime to use appeasement strategies (or
   calls for reforms and internal changes). It tends to use backlash
   rhetoric (or blaming the sanctioning powers) in response to,
   interestingly, positive sanctions. Surveillance rhetoric, or the call
   for internal vigilance against enemies, on the other hand, does not have
   any statistical connection with sanctions, rather driven by other
   factors, such as the Korean War, external instability, and so on. (C)
   2014 The Regents of the University of California. Published by Elsevier
   Ltd. All rights reserved.},
