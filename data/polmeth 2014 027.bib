@article{ WOS:000338003500001,
Author = {Evans, Bryan and Vesely, Arnost},
Title = {Contemporary policy work in subnational governments and NGOs: Comparing
   evidence from Australia, Canada and the Czech Republic},
Journal = {POLICY AND SOCIETY},
