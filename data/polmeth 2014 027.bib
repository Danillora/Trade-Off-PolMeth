@article{ WOS:000421315100004,
Author = {Paquin, Stephane},
Title = {Quebec's International Policy towards the United States: A Periodization
   Essay (Research Note)},
Journal = {ETUDES INTERNATIONALES},
Year = {2014},
Volume = {45},
Number = {2},
Pages = {261-284},
Month = {JUN},
Abstract = {The objective of this paper is to propose a periodization of the
   international policy between Quebec and the United States from a
   framework of analysis developed by Peter Hall. This framework covers two
   levels of analysis : the first level is interested in public policy
   instruments, while the second focuses on the paradigm of public action.
   From these two main criteria, we propose a periodization into five
   periods. The first period (1867-1960) is characterized by the absence of
   structured paradigm and the virtual absence of instrument with the
   exception of a commercial and tourist office in New York. The second
   period (1960-1976), reflects a desire to develop Quebec's largest
   presence in the United States with the creation of many instruments as
   the paradigm of public policy is gradually being built. The third period
   (1976-1980) is characterized by an awareness of the Quebec sovereignists
   of the importance of the United States. This period is noted for a
   significant paradigm shift. The fourth period (1980-2001) is marked by
   the free trade turn. The last period (2000 to present) is characterized
   by the importance of the new challenges that extend the paradigm of
   public policy such as security issues after 9/11, but also the energy
   and environmental issues.},
