@article{ WOS:000343990300008,
Author = {Liu, Hongsong},
Title = {China's proposing behavior in Global Governance: the cases of the WTO
   Doha Round negotiation and G-20 process},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2014},
Volume = {57},
Number = {SI},
Pages = {121-137},
Abstract = {This article examines China's proposals on the reform of global
   governance, and discusses the main features of China's proposing
   behavior in the cases of the WTO Doha Round negotiation and G-20
   Process. The main findings are: (1) in the critical junctures of global
   governance reform, China engaged the reform of the global governance
   institutions proactively, and put forward a series of reform proposals;
   (2) in proposing behavior, China argued the global governance
   institutions should be properly adjusted without intention to change the
   basic principles, refrained from playing a leadership role while
   proposing jointly with other countries, and upheld the principled idea
   of pro-development.},
