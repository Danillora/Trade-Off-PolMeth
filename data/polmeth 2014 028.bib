@article{ WOS:000214232200008,
Author = {Shawki, Noha},
Title = {New Rights Advocacy and the Human Rights of Peasants: La Via Campesina
   and the Evolution of New Human Rights Norms},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
