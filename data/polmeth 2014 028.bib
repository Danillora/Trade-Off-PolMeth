@article{ WOS:000334090700009,
Author = {Grdesic, Marko},
Title = {Television and protest in East Germany's revolution, 1989-1990: A
   mixed-methods analysis},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2014},
Volume = {47},
Number = {1},
Pages = {93-103},
Month = {MAR},
Abstract = {This article uses a mixed-methods approach to analyze the relationship
   between television and protest during East Germany's revolution. The
   content of television newscasts, both West German and East German, is
   analyzed together with protest event data. There are two key findings.
   First, West German coverage of protests is associated with an increase
   in protest in the first phase of the revolution. This finding emerges
   from time series analysis. Second, West German and East German
   television coverage were interacting, with the latter reacting to the
   former. This finding emerges from both quantitative and qualitative
   analysis. (C) 2014 The Regents of the University of California.
   Published by Elsevier Ltd. All rights reserved.},
