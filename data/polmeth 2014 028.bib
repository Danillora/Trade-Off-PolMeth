@article{ WOS:000329722100004,
Author = {Kozlowska, Magdalena},
Title = {``In Sunshine and Joy{''}? The Story of Medem Sanatorium in Miedzeszyn},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
