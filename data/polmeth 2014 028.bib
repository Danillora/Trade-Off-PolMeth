@article{ WOS:000344308000003,
Author = {Kennedy, Cara L.},
Title = {Toward Effective Intervention for Haiti's Former Child Slaves},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2014},
Volume = {36},
Number = {4},
Pages = {756-778},
Month = {NOV},
Abstract = {Restavek is a form of child domestic slavery in Haiti that affects an
   estimated 300,000 children. This article describes the results of two
   linked qualitative studies conducted with former restavek children and
   their caregivers. Results indicate a range of problems and mental health
   symptoms that children experience upon leaving restavek, and include
   detailed information on four of the most prevalent and severe problems:
   internalizing and externalizing mental health symptoms, experiences of
   violence, experiences of being unwelcome upon return home, and the
   mental health impact of children's economic realities upon returning
   home. Recommendations for intervention design and services are provided.},
