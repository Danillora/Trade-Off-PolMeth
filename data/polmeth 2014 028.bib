@article{ WOS:000337585100002,
Author = {Rangnekar, Dwijen},
Title = {Geneva Rhetoric, National Reality: The Political Economy of Introducing
   Plant Breeders' Rights in Kenya},
Journal = {NEW POLITICAL ECONOMY},
