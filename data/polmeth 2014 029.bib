@article{ WOS:000344554500002,
Author = {Golosov, Grigorii V.},
Title = {Authoritarian Electoral Engineering and its Limits: A Curious Case of
   the Imperiali Highest Averages Method in Russia},
Journal = {EUROPE-ASIA STUDIES},
Year = {2014},
Volume = {66},
Number = {10},
Pages = {1611-1628},
Month = {NOV 26},
Abstract = {This study examines the limits of electoral engineering in a
   consolidating authoritarian regime by focusing on the case of the
   Imperiali highest averages method of proportional seat allocation in
   Russia's regional legislative elections. The Imperiali method strongly
   disadvantages the opposition. But, in the absence of political
   constraint or trends towards liberalisation, most of the regional
   decision makers still chose a more permissive formula. The trade-off
   among the incentives to solidify the power monopoly, to maintain the
   democratic facade of the regime and to co-opt the opposition was
   achieved by rejecting the least permissive electoral formula and
   choosing a middle-of-the-road solution instead.},
