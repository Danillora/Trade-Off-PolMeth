@article{ WOS:000342288400006,
Author = {Hlousek, Vit and Kopecek, Lubomir},
Title = {Caretaker Governments in Czech Politics: What to Do about a Government
   Crisis},
Journal = {EUROPE-ASIA STUDIES},
Year = {2014},
Volume = {66},
Number = {8},
Pages = {1323-1349},
Abstract = {Czech politics suffers from a low durability of most of its governments,
   and frequent government crises. One of the products of this situation
   has been the phenomenon of caretaker governments. This article analyses
   why political elites have resorted to this solution, and discusses how
   this has reflected an older Czech tradition. Two cases of such
   governments are analysed in detail. The Tosovsky government was
   characterised by the ability of the Czech president to advance his
   agenda through this government at a time when the party elites were
   divided. The Fischer government was characterised by the considerably
   higher role of parties that shaped and limited the agenda of the
   cabinet, and the president played a more static role.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
