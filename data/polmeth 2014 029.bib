@article{ WOS:000210813200002,
Author = {Marchio, Jim},
Title = {Analytic Tradecraft and the Intelligence Community: Enduring Value,
   Intermittent Emphasis},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2014},
Volume = {29},
Number = {2},
Pages = {159-183},
Abstract = {The emphasis and visibility afforded analytic tradecraft in the
   Intelligence Community's analytic production has fluctuated throughout
   its existence. The explanation for this intermittent emphasis on
   tradecraft may lie in changes in consumer preferences and collection
   means, the role played by individual tradecraft advocates, and the lack
   of an intelligence failure matching the severity of 9/11 and the absence
   of weapons of mass destruction in Iraq. Both failures served as a
   forceful reminder that while strong analytic tradecraft does not
   guarantee `getting a judgment right', it increases the likelihood that
   the assessments produced are transparent, relevant, and rigorous.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
