@article{ WOS:000337189500003,
Author = {Paneyakh, Ella},
Title = {Faking performance together: systems of performance evaluation in
   Russian enforcement agencies and production of bias and privilege},
Journal = {POST-SOVIET AFFAIRS},
Year = {2014},
Volume = {30},
Number = {2-3},
Pages = {115-136},
Month = {MAR-MAY},
Abstract = {A specialist on Russian law enforcement examines a critical source of
   prosecution and conviction bias in that country - the system by which
   prosecutors, police, judges, and other legal professionals are
   evaluated. More specifically, she demonstrates how that system
   (exclusive of any inherent corruption or bias) institutionalizes
   incentives for the prosecution of large numbers of defendants in routine
   cases for the purpose of meeting informal quotas. Officials from a
   variety of law enforcement agencies, seeking to ``hit their numbers,{''}
   develop techniques of selecting the ``right{''} cases (and avoiding
   ``wrong{''} ones), manipulating charges depending on the victim's and
   defendant's statuses.},
