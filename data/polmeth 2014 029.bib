@article{ WOS:000346589500002,
Author = {Rittberger, Berthold},
Title = {Integration without Representation? The European Parliament and the
   Reform of Economic Governance in the EU},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2014},
Volume = {52},
Number = {6},
Pages = {1174-1183},
Month = {NOV},
Abstract = {While the Lisbon Treaty has been heralded as victory for the European
   Parliament, the crisis-related reforms of the European Union's economic
   governance regime are commonly being considered to have empowered
   governments and supranational institutions - such as the Commission and
   ECB - at the expense of the EP and national parliaments. This article
   argues that the picture is more nuanced than suggested by the
   conventional wisdom: legitimacy-seeking and interinstitutional
   bargaining arguments, which have been applied effectively to account for
   the expansion of the EP's power in the past, highlight the conditions
   under which the EP's struggle for more institutional power is either met
   with success (as in the case of the single supervisory mechanism) or
   faces virtually insurmountable obstacles (as in the case of the Troika
   and the European Stability Mechanism).},
