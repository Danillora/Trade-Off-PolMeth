@article{ WOS:000337514500011,
Author = {Faust, Joerg and Garcia, Maria Melody},
Title = {With or Without Force? European Public Opinion on Democracy Promotion},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
