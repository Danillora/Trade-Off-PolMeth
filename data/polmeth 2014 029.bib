@article{ WOS:000214232200002,
Author = {Darling, Jonathan},
Title = {Emotions, Encounters and Expectations: The Uncertain Ethics of `The
   Field'},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
