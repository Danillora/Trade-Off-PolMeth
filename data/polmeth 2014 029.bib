@article{ WOS:000342288400005,
Author = {Stanley, Ben},
Title = {The Dynamics of Party-System Supply and Demand in Poland, 1997-2007.
   Cleavage Change or Shapeless Shifting?},
Journal = {EUROPE-ASIA STUDIES},
Year = {2014},
Volume = {66},
Number = {8},
Pages = {1295-1322},
Abstract = {The 2007 Polish general election confirmed a significant shift in
   patterns of elite political competition, as the inherited `regime
   divide' was superseded by a `transition divide' defined by the
   `liberal-orthodox' model of post-communist reform. However, the existing
   literature on Polish voting behaviour lacks diachronic analyses of the
   changing relationships between structures, attitudes, party preferences
   and voting behaviour. This article seeks to fill that gap. It finds that
   there was no clear evidence of cleavage change over the period: there
   was more consistency than flux in preferences, and limited shifts in the
   influence of particular variables on voting behaviour.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
