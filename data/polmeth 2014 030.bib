@article{ WOS:000341150400004,
Author = {Engdahl, Oskar},
Title = {Ensuring regulatory compliance in banking and finance through effective
   controls: The principle of duality in the segregation of duties},
Journal = {REGULATION \& GOVERNANCE},
Year = {2014},
Volume = {8},
Number = {3},
Pages = {332-349},
Month = {SEP},
Abstract = {Today the segregation of duties is commonly used to ensure regulatory
   compliance in various industries. This article considers the
   organizational requirements for the effective implementation of this
   principle, through an examination of a duality-based
   segregation-of-duties type control system and its fundamental
   characteristics. Cases from the Swedish banking and finance sector are
   discussed to show how breakdowns in duality-based systems have
   compromised compliance and even encouraged crime, and how crimes could
   be carried out in practice. Particular attention is paid to the critical
   role that gullibility, loyalty, and dependency relations among employees
   played in these cases, in leading control persons to neglect their
   responsibility to review and approve their colleagues' work, while
   bringing no consequences for their ability to carry on performing their
   work tasks. The argument is made that an effective duality-based
   segregation-of-duties type control system presupposes social relations
   characterized by relative autonomy and third-party dependence, along
   with work task interdependence.},
