@article{ WOS:000342288400003,
Author = {Crotty, Jo and Hall, Sarah Marie and Ljubownikow, Sergej},
Title = {Post-Soviet Civil Society Development in the Russian Federation: The
   Impact of the NGO Law},
Journal = {EUROPE-ASIA STUDIES},
Year = {2014},
Volume = {66},
Number = {8},
Pages = {1253-1269},
Abstract = {The passing of the Russian NGO Law in mid-2006 set clear parameters for
   Russian NGO activity and civil society development. In this paper we
   assess the impact of the NGO Law on both NGOs and Russian civil society.
   Our findings illustrate that the NGO Law has led to a reduction in NGO
   activity and curtailment of civil society development. We conclude that
   Russian civil society appears to be dominated by groups funded and thus
   controlled by the state. This has implications for Russia's on-going
   democratic development.},
