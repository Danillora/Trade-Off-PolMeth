@article{ WOS:000344358800003,
Author = {Garrick, Jessica},
Title = {Repurposing American Labor Law: Immigrant Workers, Worker Centers, and
   the National Labor Relations Act},
Journal = {POLITICS \& SOCIETY},
Year = {2014},
Volume = {42},
Number = {4},
Pages = {489-512},
Month = {DEC},
Abstract = {The National Labor Relations Act (NLRA) of 1935 has been widely
   portrayed as an anachronistic piece of legislation that needs to be
   reformed or abandoned. In the absence of reform, many US labor unions
   try to avoid the NLRA process altogether by organizing workers outside
   the confines of the law. But Somos un Pueblo Unido, or Somos, a worker
   center in New Mexico, has been using a novel interpretation of the NLRA
   less to boost union density than to develop an alternative to contract
   unionism. By helping nonunionized workers use Section 7 of the NLRA to
   act concertedly in their own defense, I argue, Somos is combating
   employer abuse, in the short run, and demonstrating that worker centers
   and their memberships may be transforming the US labor movement, in the
   long run. Their experiences illustrate the ability of organizations to
   redeploy existing institutional resources with potentially
   transformative results.},
