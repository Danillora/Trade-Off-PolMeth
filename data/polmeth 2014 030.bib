@article{ WOS:000340191400001,
Author = {Deckman, Melissa},
Title = {A Gender Gap among Evangelicals? An Examination of Vote Choice by Gender
   and Religion in the 2008 Presidential Elections},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2014},
Volume = {35},
Number = {3},
Pages = {199-221},
Abstract = {Barack Obama appeared poised to capture more votes among Evangelical
   women than Evangelical men given the prominence that economic and
   wartime issues played in the 2008 election. However, I find that no
   gender gap existed among Evangelicals in 2008; instead, religious
   tradition trumps gender as a predictor of presidential vote choice.
   While Obama fared similarly to John Kerry, Evangelical women were
   significantly more likely to vote for Al Gore than Evangelical men,
   demonstrating that there may be circumstances in which Democratic
   presidential candidates can mitigate some of their voting losses to
   Evangelical women.},
