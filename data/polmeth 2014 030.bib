@article{ WOS:000343858700007,
Author = {Howard, Lise Morje},
Title = {Kosovo and Timor-Leste: Neotrusteeship, Neighbors, and the United
   Nations},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2014},
Volume = {656},
Number = {1},
Pages = {116-135},
Month = {NOV},
Abstract = {Why do some states transition, with foreign assistance, from fragile to
   robust? Scholars in state-building have argued that neotrusteeship is an
   effective strategy by which external organizations might build
   postconflict states. This article tests this hypothesis, and two related
   propositions, in a paired comparison of Kosovo and Timor-Leste. The two
   states are similar in many respects and both experienced regional peace
   enforcement operations to end violent conflict, followed by massive
   neotrusteeship operations. They have had divergent results, however, in
   postconflict state-building: while the state and economy are gradually
   becoming stronger in Timor-Leste, the same cannot be said of Kosovo,
   which continues to be plagued by high unemployment, low growth,
   corruption, and organized crime. I argue that many of Kosovo's problems
   can be traced back to the strategy of dividing international
   responsibility for neotrusteeship operations.},
