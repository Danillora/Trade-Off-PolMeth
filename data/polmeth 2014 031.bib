@article{ WOS:000338037600008,
Author = {Dunn, Kevin C.},
Title = {Pussy Rioting THE NINE LIVES OF THE RIOT GRRRL REVOLUTION},
Journal = {INTERNATIONAL FEMINIST JOURNAL OF POLITICS},
