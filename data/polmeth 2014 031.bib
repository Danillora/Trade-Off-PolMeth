@article{ WOS:000331062800019,
Author = {Tosun, Jale},
Title = {Absorption of Regional Funds: A Comparative Analysis},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2014},
Volume = {52},
Number = {2},
Pages = {371-387},
Month = {MAR},
Abstract = {This article evaluates the determinants of the absorption performance of
   European Union Member States with regard to the European Regional
   Development Fund's (ERDF) 2000-06 programming period. Theoretically
   grounded in Europeanization research and studies of regional governance,
   the empirical analysis concentrates on the ERDF absorption performance
   of 25 Member States at the end of the programming period in 2008. The
   results of Tobit models show a positive relationship between ERDF
   absorption and government capacity. The estimation findings also
   demonstrate that new Member States generally had higher absorption rates
   than old Member States. Furthermore, Member States with high income
   levels were less likely to maximize the absorption of their ERDF
   allocations. Finally, there is some support for the hypothesis that
   fiscal decentralization has a negative impact on absorption performance.},
