@article{ WOS:000217496300011,
Author = {Lamoreaux, Jeremy W.},
Title = {Acting small in a large state's world: Russia and the Baltic states},
Journal = {EUROPEAN SECURITY},
Year = {2014},
Volume = {23},
Number = {4},
Pages = {565-582},
Abstract = {The interest in small states ebbs and flows as important international
   affairs include small states. Russia's actions and policies vis-a-vis
   Ukraine, and the resultant intensified apprehension among Russia's
   smaller neighbours, aim the proverbial microscope at the size and power
   discrepancies between states. Russia, by most metrics, is a large state
   and the Baltic states, by those same metrics, are small states.
   Small-state scholars expect large and small states to act differently.
   However, the case of Russia and the Baltic states indicates that large
   and small states do not, in fact, act all that different. This being the
   case, this article calls into question many of the assumptions made by
   small-state scholars about the difference between large-and small-state
   action and argues for changes within small-state studies as a
   subdiscipline of the larger international relations discipline.},
