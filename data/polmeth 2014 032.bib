@article{ WOS:000340875700005,
Author = {Fuentes, Alberto},
Title = {A Vocation for Industrial Transformation: Ideology, Organizational
   Isomorphism, and Upgrading in the Guatemalan Sugar Industry},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
Year = {2014},
Volume = {49},
Number = {3},
Pages = {370-401},
Month = {SEP},
Abstract = {Between the late 1970s and the 2000s, the Guatemalan sugar industry
   transitioned from a production model with deplorable labor conditions
   and low productivity to a highly efficient model with improved
   conditions. This paper traces the origin and diffusion of this upgraded
   model to a small team of managers motivated by Elite Solidarism, an
   interpretation of the Vatican II Catholic Social Doctrine. It suggests
