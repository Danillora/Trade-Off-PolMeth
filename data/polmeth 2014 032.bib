@article{ WOS:000340495500004,
Author = {Nielsen, Tobias Dan},
Title = {The role of discourses in governing forests to combat climate change},
Journal = {INTERNATIONAL ENVIRONMENTAL AGREEMENTS-POLITICS LAW AND ECONOMICS},
