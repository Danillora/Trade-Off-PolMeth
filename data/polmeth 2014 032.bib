@article{ WOS:000331604200007,
Author = {Falleti, Tulia G.},
Title = {The Organization of the Executive Branch in Latin America: What We Know
   and What We Need to Know},
Journal = {LATIN AMERICAN POLITICS AND SOCIETY},
Year = {2014},
Volume = {56},
Number = {1},
Pages = {23-26},
Month = {SPR},
Abstract = {This article proposes a research agenda for the organization of the
   executive branch in Latin America by reviewing the literature on the
   U.S. and Latin American presidencies and outlining the research gap
   between them. The study finds that while strong, regionwide patterns
   have been established about cabinets in Latin America, research is
   lagging behind on the presidential center, presidential advisory
   networks, and their effects in policymaking. The article sets forth a
