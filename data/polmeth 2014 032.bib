@article{ WOS:000333521600005,
Author = {Hiskey, Jonathan and Daniel Montalvo, Jorge and Orces, Diana},
Title = {Democracy, Governance, and Emigration Intentions in Latin America and
   the Caribbean},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
Year = {2014},
Volume = {49},
Number = {1},
Pages = {89-111},
Month = {MAR},
Abstract = {It is now clear that the global shift toward democracy in recent decades
   has resulted in a highly uneven democratic landscape in which the
   quality and performance of democracies around the world vary greatly. In
   an era characterized by increasingly open borders to goods, services,
   information, and, at times, labor, we argue that poorly performing,
   uneven democracies have become an important, yet underexplored,
   component in one's emigration calculus. We test this argument through
   analysis of survey data across 22 Latin American countries and find
   strong and consistent evidence that both the quality of a democratic
   system and its ability to fulfill basic governance responsibilities
   influence the degree to which an individual considers emigration as a
   viable life strategy. These findings in turn have implications for the
   subsequent impact emigration may have on the democratic development of
   high migration communities.},
