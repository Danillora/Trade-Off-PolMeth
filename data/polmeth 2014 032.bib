@article{ WOS:000330968400002,
Author = {Kim, Rakhyun E. and Mackey, Brendan},
Title = {International environmental law as a complex adaptive system},
Journal = {INTERNATIONAL ENVIRONMENTAL AGREEMENTS-POLITICS LAW AND ECONOMICS},
