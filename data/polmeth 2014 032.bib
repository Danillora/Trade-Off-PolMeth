@article{ WOS:000330512100004,
Author = {Turner, James Morton},
Title = {Counting Carbon: The Politics of Carbon Footprints and Climate
   Governance from the Individual to the Global},
Journal = {GLOBAL ENVIRONMENTAL POLITICS},
Year = {2014},
Volume = {14},
Number = {1},
Pages = {59-78},
Month = {FEB},
Abstract = {This article considers carbon footprints as a form of climate
   governance. Drawing on science studies to consider the contingent nature
   of calculative devices and governmentality studies to examine the
   intrinsic relationship between how problems are framed and remedied,
   this article advances two arguments. First, it argues that efforts to
   define and deploy carbon footprints contributed to a conceptual shift in
   emissions accounting, from a narrower metric focused on emissions from
   fossil fuel and electricity useCarbon Footprint 1.0to a more expansive
   metric that includes emissions embodied in consumption and tradeCarbon
   Footprint 2.0. Second, this article argues that these approaches to
   carbon footprints at the individual level have intersected with broader
   discussions about allocating emissions responsibilities and examining
   mitigation strategies at the national and international levels, offering
   alternative grounds for assigning responsibility for climate-change
   mitigation and expanding the range of policy options available for
   addressing emissions.},
