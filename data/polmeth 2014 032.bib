@article{ WOS:000331692800013,
Author = {Stein, Ewan},
Title = {Studying Islamism after the Arab Spring},
Journal = {MEDITERRANEAN POLITICS},
Year = {2014},
Volume = {19},
Number = {1},
Pages = {149-152},
Month = {JAN 2},
Abstract = {This intervention argues that the events associated with the `Arab
   Spring', particularly in Egypt, raise important questions for the study
   of political Islam as a discrete phenomenon or uniquely resonant set of
   ideas in Muslim societies. It stresses the need for a better
   understanding of how specific groups utilize Islamist ideas in reshaping
   the collective imagination over time, and how these processes in turn
   affect the popularity, strategies and political behaviour of state and
   non-state actors.},
