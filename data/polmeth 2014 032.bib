@article{ WOS:000330450300004,
Author = {Esarey, Justin and DeMeritt, Jacqueline H. R.},
Title = {Defining and Modeling State-Dependent Dynamic Systems},
Journal = {POLITICAL ANALYSIS},
Year = {2014},
Volume = {22},
Number = {1},
Pages = {61-85},
Month = {WIN},
Abstract = {A state-dependent dynamic system is one in which (1) the marginal effect
   of x on y at time (partial derivative y(t)/partial derivative X-t) and
   (2) the persistence of the dependent variable depends on partial
   derivative y(t)/partial derivative(t-1) depends on x(t). We present a
   methodological strategy for dealing with state-dependent dynamic systems
   and demonstrate the consequences of ignoring state-dependence. As an
   applied example, we find evidence of state-dependence in the
   relationship between presidential approval and economic performance:
   high unemployment rates are most damaging to presidential approval among
   presidents with the highest initial approval ratings.},
Publisher = {OXFORD UNIV PRESS},
Address = {GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND},
