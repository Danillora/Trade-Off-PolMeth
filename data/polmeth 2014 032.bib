@article{ WOS:000344736800005,
Author = {Martinez Franzoni, Juliana and Sanchez-Ancochea, Diego},
Title = {Filling In the Missing Link Between Universalism and Democracy: The Case
   of Costa Rica},
Journal = {LATIN AMERICAN POLITICS AND SOCIETY},
Year = {2014},
Volume = {56},
Number = {4},
Pages = {98-118},
Month = {WIN},
Abstract = {This article explores a missing link in the recent literature on the
   formation of social policies: that between democracy and universalism,
   one desirable yet elusive feature of these policies. We base our
   argument on a case study of Costa Rica, the most successful case of
   universalism in Latin America. We proceed by first depicting Costa
   Rica's peculiar policy architecture, based on the incremental expansion
   of benefits funded on payroll taxes. Then we reconstruct the policy
   process to stress the key role played by technopoliticians in a
   democratic context. Backed by political leadership and equipped with
   international ideas, technopoliticians drove social policy design from
   agenda setting to adoption and implementation. Third, we argue that key
   aspects of the policy architecture established in the early 1940s were
   fundamental building blocks for a distinctive and seldom explored road
   to universalism. We conclude considering contemporary implications.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {32 AVENUE OF THE AMERICAS, NEW YORK, NY 10013-2473 USA},
