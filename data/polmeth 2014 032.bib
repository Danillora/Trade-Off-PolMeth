@article{ WOS:000340021100006,
Author = {Suiseeya, Kimberly R. Marion},
Title = {Negotiating the Nagoya Protocol: Indigenous Demands for Justice},
Journal = {GLOBAL ENVIRONMENTAL POLITICS},
