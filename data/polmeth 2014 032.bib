@article{ WOS:000217495800008,
Author = {Christou, George},
Title = {The European Union's human security discourse: where are we now?},
Journal = {EUROPEAN SECURITY},
Year = {2014},
Volume = {23},
Number = {3},
Pages = {364-381},
Abstract = {The language of human security has been prominent in the European
   Union's (EU) official discourse for a number of years. However, whilst
   it has been promoted as a new approach for the EU in the development of
   its security and defence policy, the aim of this article is to assess
   the extent to which it actually features in the EU's contemporary
   strategic discourse and practice. It seeks to uncover where and how the
   concept is spoken within the EU's institutional milieu, how it is
   understood by the relevant policy-makers in the EU and the implication
   of this across key areas of human security practice. It is argued in the
   article that human security has not been embedded as the driving
   strategic concept for Common Security and Defence Policy (CSDP) in an
   era of crisis and change in Europe and beyond and that the prospects for
   this materialising in the near future are rather thin.},
