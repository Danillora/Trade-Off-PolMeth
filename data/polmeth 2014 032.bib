@article{ WOS:000343202300008,
Author = {Tourangeau, Roger and Presser, Stanley and Sun, Hanyu},
Title = {THE IMPACT OF PARTISAN SPONSORSHIP ON POLITICAL SURVEYS},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2014},
Volume = {78},
Number = {2},
Pages = {510-522},
Month = {SUM},
Abstract = {Although it is widely believed that revealing the topic or sponsor of a
   survey to potential respondents can produce large non-response biases,
   measurement errors, or both, recent research has shown that the effects
   of the framing of the survey request are often quite modest. However,
   research has not experimented with partisan or candidate sponsors, which
   are most likely to produce large effects. We carried out three
   experiments with political surveys that varied whether a partisan
   sponsor was identified (and in one of the experiments also varied the
   identity of that sponsor), expecting that this would have dramatic
   effects. Instead, all three studies found only minor effects. Thus, even
   in the context of partisan election surveys, sponsorship may not be the
   powerful cue it is often thought to be.},
