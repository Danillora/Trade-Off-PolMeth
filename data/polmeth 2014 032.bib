@article{ WOS:000339952000004,
Author = {Barber, Michael J. and Mann, Christopher B. and Monson, J. Quin and
   Patterson, Kelly D.},
Title = {Online Polls and Registration-Based Sampling: A New Method for
   Pre-Election Polling},
Journal = {POLITICAL ANALYSIS},
