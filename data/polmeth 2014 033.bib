@article{ WOS:000217889900003,
Author = {Varga, Balint},
Title = {Multilingualism in urban Hungary, 1880-1910},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2014},
Volume = {42},
Number = {6},
Pages = {965-980},
Abstract = {Based on statistical sources, this paper examines to what extent
   citizens of the most advanced provincial cities in fin-de-siecle Hungary
   were multilingual. I argue that multilingualism was a far less present
   phenomenon than scholarship suggests. The exact features of
   multilingualism were closely connected to the local social relations
   (religion, gender, etc.) and market of languages. The growing
   nationalism promoted by the Hungarian government and the expansion of
   Magyar public space had an ambiguous impact on multilingualism,
   depending on the local society.},
