@article{ WOS:000217822400001,
Author = {Nikitina, Yulia},
Title = {Russia and the Baltic states: problematizing the Soviet legacy discourse},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2014},
Volume = {42},
Number = {1},
Pages = {1-7},
Abstract = {Most studies of the post-Soviet space often explicitly or implicitly
   analyze Russia not as a new independent state but as the political
   successor of the USSR, thereby almost automatically leading to
   conclusions about Russian neo-imperialism. This paper explains how
   distorted discourses on the Soviet legacy originated and how they
   obstruct equal relations between Russia and other former Soviet
   republics using the example of the Baltic states.},
