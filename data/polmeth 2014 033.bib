@article{ WOS:000217888300003,
Author = {Brykczynski, Paul},
Title = {Reconsidering ``Pilsudskiite nationalism{''}},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2014},
Volume = {42},
Number = {5},
Pages = {771-790},
Abstract = {This paper examines the intellectual underpinnings of the nationalism
   articulated by the followers of Marshal Jozef Pilsudski (Pilsudskiites),
   who ruled Poland between 1926 and 1939. Scholarly consensus holds that
   modern Polish nationalism was solely the domain of the National
   Democratic movement. Conversely, the Pilsudskiites' conception of the
   nation is generally seen as anachronistic, poorly articulated,
   selfcontradictory, and lacking a deeper intellectual foundation.
   Focusing on the formative years of the Second Polish Republic
   (1918-1922), this paper draws a link between Pilsudskiite political
   thought and the philosophy of the heterodox Marxist theorist Stanislaw
   Brzozowski. Re- examining the early writings of Pilsudski's followers in
   light of Brzozowski's philosophy, the paper presents the argument that
   ``Pilsudskiite nationalism{''} was in fact deeply constructivist,
   surprisingly sophisticated, and no less ``modern{''} than the
   nationalist discourse articulated by the National Democrats. In the
   process, the article interrogates and problematizes the classic
   ``ethnic{''} vs. ``civic{''} typology of nationalist movements.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
