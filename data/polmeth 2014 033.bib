@article{ WOS:000217888300002,
Author = {Liebich, Andre and Myshlovska, Oksana},
Title = {Bandera: memorialization and commemoration},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2014},
Volume = {42},
Number = {5},
Pages = {750-770},
Abstract = {This article examines the current heroization of Ukrainian nationalist
   leader, Stepan Bandera, as manifested in monuments and commemorative
   practices. It offers a topographic survey that reveals the extent and
   variety of modes of Bandera heroization. It examines the esthetic and
   historical controversies that surround Bandera memorialization. It
   enquires into the personal motivations and political strategies that
   underlie the effort to project the chosen image of Bandera upon the
   public space in highly visible terms. It suggests that the campaign in
   favor of memorializing Bandera can best be understood in performative
   terms. It is in depicting Bandera as a hero of Ukraine that Bandera
   becomes a hero of Ukraine.},
