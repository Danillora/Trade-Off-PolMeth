@article{ WOS:000340844800001,
Author = {Copus, Colin and Wingfield, Melvin},
Title = {Are Independent Councillors Really Conservatives in Disguise?},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2014},
Volume = {40},
Number = {5},
Pages = {647-669},
Month = {OCT},
Abstract = {A feature of English local political folklore is the belief among
   national political parties, that Independent councillors are
   Conservatives in disguise. Yet, little evidence is given to support this
   idea because little is known about these councillors' beliefs and
   assumptions or how they distinguish themselves from the party politics
   they eschew. In this article we address the question: Are all
   Independent councillors really Conservatives? The article answers the
   question by reporting the findings of research conducted among
   Independent and Conservative councillors in England to explore the
   complex, multidimensional and fluid world that is Independent politics.},
