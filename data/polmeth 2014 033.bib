@article{ WOS:000217822800003,
Author = {Kind-Kovacs, Friederike},
Title = {Memories of ethnic cleansing and the local Iron Curtain in the
   Czech-German borderlands},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2014},
Volume = {42},
Number = {2},
Pages = {199-222},
Abstract = {The Czech-German borderlands are an archetypal European border region.
   They evoke not only Cold War histories, but also shelter layers of
   European memories of the ethnic reshaping of early post-war Europe. By
   means of life story interviews with German speakers of the border
   region, this article analyzes the symbolic meaning of and the individual
   dealing with the local Iron Curtain. It will shed light on the
   biographical and narrative interconnectedness of experiences of ethnic
   cleansing in the early postwar period and retrospective perceptions of
   the Iron Curtain in these borderlands. In particular, it inquires
   whether and to what extent the local Iron Curtain intensified fractures
   caused by the region's post-and pre-war attempts to halt the multiethnic
   composition of the border communities. The article suggests that the
   local Czech-German Iron Curtain would have never endured as strongly if
   the border communities' common identity had not already been severely
   damaged in the course of the region's traumatic history and forced
   population transfers.},
