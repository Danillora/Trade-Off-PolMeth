@incollection{ WOS:000450757300010,
Author = {Klotz, Robert},
Book-Author = {Solo, AMG},
Title = {Sources and Formats of Campaign Information on YouTube},
Booktitle = {POLITICAL CAMPAIGNING IN THE INFORMATION AGE},
Series = {Advances in Human and Social Aspects of Technology},
Year = {2014},
Pages = {165-178},
Abstract = {This chapter empirically, longitudinally, and systematically examines
   U.S. Senate campaign information on YouTube over three election cycles.
   The Internet broadcast yourself world of YouTube offers some sharp
   contrasts to the television broadcast world. Unlike on television,
   candidates and the traditional news media are being challenged by
   citizens in shaping their video presence on YouTube. Interest groups
   that leverage financial resources to force viewers to watch broadcast
   advertisements are marginalized on YouTube where accidental exposure is
   limited. While the broadcast world converges on a few video formats,
   YouTube campaign videos exhibit substantial diversity of format.},
