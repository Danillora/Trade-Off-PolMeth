@article{ WOS:000217822400008,
Author = {Goikoetxea, Jule},
Title = {Nation and democracy building in contemporary Europe: the
   reproduction of the Basque demos},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2014},
Volume = {42},
Number = {1},
Pages = {145-164},
Abstract = {The article analyzes the material or objectified reproduction of the
   Basque demos since democracy was established in Spain in 1980. Spain
   holds within its territory diverse regions and political communities and
   the Basque case is a highly illustrative example of how the development
   of regional state institutions is fundamental for the reproduction of
   distinct democratic demoi not merely in their political but also
   socio-economic dimension. This paper argues that, in our current
   European context, political distinctions cannot become effectively
   objectified and instituted power structures without state institutions
   being able to uphold a differentiated system of stratification.},
