@incollection{ WOS:000416733300010,
Author = {Guzzo, Tiziana and Grifoni, Patrizia and Ferri, Fernando},
Book-Author = {Mahmood, Z},
Title = {Risk Communication Methods and Participatory Approaches},
Booktitle = {IT IN THE PUBLIC SPHERE: APPLICATIONS IN ADMINISTRATION, GOVERNMENT,
   POLITICS, AND PLANNING},
Series = {Advances in Electronic Government Digital Divide and Regional
   Development},
Year = {2014},
Pages = {184-198},
Abstract = {The chapter provides an overview of the main methods, tools, and actions
   of risk communication used in the different phases of risk governance,
   analyzing their advantages and disadvantages. A practical case study
   carried out in Italy shows an application of some of these methods,
   comparing data before and after communication actions. Finally, a
