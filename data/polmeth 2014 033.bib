@article{ WOS:000217886700006,
Author = {Sharafutdinova, Gulnaz},
Title = {The Pussy Riot affair and Putin's demarche from sovereign democracy to
   sovereign morality},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2014},
Volume = {42},
Number = {4},
Pages = {615-621},
Abstract = {This essay brings attention to the recent discursive turn in Russian
   politics that is reflected in the Kremlin's turn to issues of
   traditional values and morality. Expressed in Russia's domestic and
   foreign policies, this new ``morality politics{''} is dated by the Pussy
   Riot trial in 2012 that the Kremlin used to advance its new discursive
   frame in the public sphere. Although not entirely new in its
   orientation, this new stage of ``morality politics{''} differs from the
   earlier policy initiatives in its intensity, scope and political
   significance for the regime. The moralizing stance taken by the regime
   is accompanied by a divide and rule political tactic, whereby the
   establishment has tried to marginalize the protesters from the rest of
   the Russian public that the regime is attempting to reconsolidate based
   on traditional, conservative values. The essay interprets this recent
   morality turn as a strategy selected by the Kremlin to restore the
   regime's legitimacy that has been shaken by the protests of 2011-2012
   and looks at the social and political consequences of the selected
   strategy.},
