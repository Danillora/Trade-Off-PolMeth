@incollection{ WOS:000343976400007,
Author = {Clark, Terry D. and Mordeson, John N. and Wierman, Mark J.},
Book-Author = {Casey, PC
   Gibilisco, MB
   Goodman, CA
   Pook, KN
   Mordeson, JN
   Wierman, MJ
   Clark, TD},
Title = {Predicting the Outcome of the Government Formation Process: A Fuzzy
   Two-Dimensional Public Choice Model},
Booktitle = {FUZZY SOCIAL CHOICE MODELS: EXPLAINING THE GOVERNMENT FORMATION PROCESS},
Series = {Studies in Fuzziness and Soft Computing},
Year = {2014},
Volume = {318},
Pages = {129-147},
Abstract = {Under the most basic of assumptions of Euclidean preferences, majority
   rule erupts into cycling in two or more dimensional space, and no
   alternative remains undefeated. The resulting McKelvey's Chaos Theorem
   (McKelvey 1976) forces scholars to reconsider basic assumptions about
   the rational behavior of political actors and their attempts to form
   coalitions. The government formation literature remains divided on how
   to best solve the problem. More recently, proposed models either assume
   cabinet ministers are virtual dictators over their policy jurisdiction
