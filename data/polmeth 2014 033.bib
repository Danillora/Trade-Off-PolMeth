@article{ WOS:000331519200006,
Author = {Im, Tobin and Lee, Hyunkuk and Cho, Wonhyuk and Campbell, Jesse W.},
Title = {Citizen Preference and Resource Allocation: The Case for Participatory
   Budgeting in Seoul},
