@article{ WOS:000368311700006,
Author = {Traunmueller, Richard},
Title = {Preaching to the Converted? Theory Problems in Political Science
   Research on Religion and Politics},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2015},
Volume = {56},
Number = {4},
Pages = {672-681},
Abstract = {Religion and politics is the turf of normative theorists and
   historically or culturally orientated scholars who have little interest
   in generalizable and testable propositions about political reality. This
   contribution argues for an analytical political science approach that is
   based on clearly stated assumptions from which testable propositions can
   be logically deduced. To illustrate the benefits of this approach, a
   recent study on the policy positions of the German Evangelical Church
   (EKD) (Thieme and Liedhegener 2015) is theoretically re-formulated. The
   EKD is viewed as a strategic collective actor interested in
   institutional survival. Its behavior is explained by referring to
   conditions defined by members' preferences as well as levels of
   religious diversity and political competition.},
