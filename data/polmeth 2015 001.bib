@article{ WOS:000369875500002,
Author = {Ehs, Tamara},
Title = {The Austrian Constitutional Court as a political player Implications of
   a change in jurisdiction},
Journal = {AUSTRIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2015},
Volume = {44},
Number = {2},
Pages = {15-27},
Abstract = {As constitutional courts in principle, also the Austrian Constitutional
   Court (VfGH) fulfills a political function. Therefore, the analysis of
   its adjudications may not be left to jurisprudence only but requires a
   stronger contribution from political science. Common statements by
   journalists or politicians about the power of the judges{''} or the
   silent rulers{''} must be confronted by scientific analysis. Since the
   1980s, the Austrian Constitutional Court pursues a new line in its
   adjudications concerning fundamental rights. This change contributed to
   its political stance within the institutional structure and caused some
   controversy among the political parties, because the court rulings go
   beyond the individual decision, and therefore they are most influential
   with regard to the res publica.},
