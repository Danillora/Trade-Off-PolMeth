@article{ WOS:000370951500004,
Author = {Dardot, Pierre and Laval, Christian},
Title = {Europe out of Democracy},
Journal = {POLITICA \& SOCIETA},
Year = {2015},
Volume = {4},
Number = {1},
Pages = {43-59},
Month = {JAN-APR},
Abstract = {Neoliberalism is a global lever meant to undermine liberal democracy, as
   Wendy Brown has demonstrated in her analysis. The European Union
   participates in this general process of de-democratization. The specific
   nature of the European exit of democracy lies in the production of norms
   and the imposition of ``structural reforms{''} decided at an European
   level and presented as indisputable by citizens. The current system of
   rules, which is perpetuating and aggravating today's economic crisis, is
   now menacing the so-called ``European social model{''}. It is not the
   result of an ``economic mistake{''} as neokeynesians think. It is the
   very consequence of the ordoliberal foundations of the European Union.
   The historical task of the left is the radical refoundation of Europe.},
