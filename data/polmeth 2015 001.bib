@article{ WOS:000374871500008,
Author = {Roy, Jason and Perrella, Andrea M. L. and Borden, Joshua},
Title = {Rural, Suburban and Urban Voters: Dissecting Residence Based Voter
   Cleavages in Provincial Elections},
Journal = {CANADIAN POLITICAL SCIENCE REVIEW},
Year = {2015},
Volume = {9},
Number = {1},
Pages = {112-127},
Abstract = {We explore provincial-level cleavages by drawing on surveys administered
   during eight elections. More specifically, we examine
   rural-suburban-urban divisions in regards to party support within
   Canadian provinces. Our results show a clear division according to place
   of residence, even after controlling for a host of individual-level
   characteristics. We argue that recognition of this rural-suburban-urban
   division is an important and often overlooked aspect of understanding
   Canadian political preferences and political behaviour more generally.},
