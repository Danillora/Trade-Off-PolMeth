@article{ WOS:000357090600002,
Author = {Hug, Simon and Wegmann, Simone and Wueest, Reto},
Title = {Parliamentary Voting Procedures in Comparison},
Journal = {WEST EUROPEAN POLITICS},
Year = {2015},
Volume = {38},
Number = {5, SI},
Pages = {940-968},
Month = {SEP 3},
Abstract = {Increasingly, scholars of legislative politics propose comparative
   analyses of parliamentary voting behaviour across different countries
   and parliaments. Yet parliamentary voting procedures differ dramatically
   across parliamentary chambers and ignoring these differences may, in the
   extreme, lead to meaningless comparisons. This paper presents a first
   glimpse at a comprehensive data collection effort covering more than 250
   parliamentary chambers in 176 countries. Focusing on European
   legislatures it assesses what explains the differences in the rules
   among chambers. It is found that incentives linked to MPs' visibility
   contribute to explain the transparency of the adopted voting procedures.},
