@article{ WOS:000435234900001,
Author = {Fowler, Michael W.},
Title = {Brief Survey of Democracy Promotion in US Foreign Policy},
Journal = {DEMOCRACY \& SECURITY},
Year = {2015},
Volume = {11},
Number = {3},
Pages = {227-247},
Abstract = {Democracy promotion has been a centerpiece of US foreign policy for over
   half a century. This article explores the evolution of democracy
   promotion in US foreign relations from 1821 to 2014. While the quantity
   and quality of US democracy promotion policy evolved in a nonlinear
   fashion, US presidents varied the ways and means of democracy promotion
   as a way to achieve national security objectives. There is signification
   variation among US presidents on the specific linkages between democracy
   and security, resulting in divergent policy applications and technical
   approaches.},
