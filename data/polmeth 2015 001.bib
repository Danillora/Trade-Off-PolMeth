@article{ WOS:000370930000001,
Author = {Prezelj, Iztok and Kopac, Erik and Ziberna, Ales and Kolak, Anja and
   Grizold, Anton},
Title = {Evolutionary Reality of the Revolution in Military Affairs: Results of a
   Comparative Study},
Journal = {ROMANIAN JOURNAL OF POLITICAL SCIENCE},
