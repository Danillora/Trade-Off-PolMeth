@article{ WOS:000357090600006,
Author = {Curini, Luigi and Zucchini, Francesco},
Title = {Legislative Committees as Uncertainty Reduction Devices in Multiparty
   Parliamentary Democracies},
Journal = {WEST EUROPEAN POLITICS},
Year = {2015},
Volume = {38},
Number = {5, SI},
Pages = {1042-1061},
Month = {SEP 3},
Abstract = {Most research on committees in multiparty legislatures in parliamentary
   democracies focuses on their role in solving intra-cabinet delegation
   problems. Using a straightforward spatial model, this article discusses
   how committees can also solve uncertainty problems that arise in
   settings characterised by unstable coalitions, weak governmental agenda
   control and a lack of government change. In order to explore empirically
   how committees solve these problems, the article focuses on the success
   (and later decline) over the last 30years of the sede legislativa, a
   law-making procedure that formalises universalism' in Italian
   legislative committees. The statistical results largely confirm the
   theoretical expectations.},
