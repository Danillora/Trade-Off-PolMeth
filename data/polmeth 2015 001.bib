@article{ WOS:000367012100002,
Author = {Dunford, Robin Frederick and Madhok, Sumi},
Title = {Vernacular rights cultures and the `Right to Have Rights'},
Journal = {CITIZENSHIP STUDIES},
Year = {2015},
Volume = {19},
Number = {6-7},
Pages = {605-619},
Month = {AUG 18},
Abstract = {We use a case study of the Landless Workers Movement (MST) in Brazil and
   the Via Campesina network of which they are part to develop the concept
   of vernacular rights cultures'. Vernacular rights cultures calls
   attention to the way in which demands for the right to have rights call
   on particular cultures, histories and political contexts in a manner
   that can transform the rights inscribed in constitutions and political
   imaginaries. What Ranciere (1999) and Balibar (2002) call the
   democratisation of democracy, we therefore argue, does not just involve
   a logic of equality and inclusion through which dispossessed groups
   demand already existing rights. Rather, it also occurs as mobilisations
   alter the means through which rights are delivered and transform the
   content and meaning of the rights demanded.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
