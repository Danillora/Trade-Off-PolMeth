@article{ WOS:000421679100002,
Author = {Aloisio, Carlos},
Title = {Transitional Justice in Uruguay: ?a negative case?},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
Year = {2015},
Number = {16},
Pages = {10-22},
Month = {JUL},
Abstract = {This article aims primarily to classify the Uruguayan experience of post
   - dictatorship within the theoretical framework of transitional justice.
   Specifically, it seeks to establish if it can be said that transitional
   justice mechanisms were implemented or not in Uruguay. After pointing
   out a serious epistemological problem associated with the emergence of
   the concept of transitional justice, it briefly reviews the extent of
   that problem once this approach began to be implemented in Latin
   America. Then, some dimensions of analysis that establish the conditions
   of possibility of transitional justice in general are exposed. After
   that, the Uruguayan case is studied, showing that it cannot be
   understood as a positive case of transitional justice. Finally, the
   article ends with a section of concluding remarks.},
