@article{ WOS:000362338700008,
Author = {Ennser-Jedenastik, Laurenz and Mueller, Wolfgang C.},
Title = {Intra-party democracy, political performance and the survival of party
   leaders: Austria, 1945-2011},
Journal = {PARTY POLITICS},
Year = {2015},
Volume = {21},
