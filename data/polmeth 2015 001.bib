@article{ WOS:000355594500002,
Author = {Margulies, William},
Title = {Liberal party performance and polarisation},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2015},
Volume = {50},
Number = {2},
Pages = {241-257},
Month = {APR 3},
Abstract = {Nagel and Wlezien's `vacant centre' theory suggests that parties close
   to the centre tend to do better when a political system is highly
   polarised, creating an empty space in the political market.
   Cross-nationally, this does not seem to be true when overall system
   polarisation is used as the independent variable: indeed, there seems to
   be a slight negative relationship. The farther apart conservative and
   social democratic parties are, however, the better that liberal parties
   tend to do. This parallels Nagel and Wlezien's findings for Great
   Britain specifically.},
