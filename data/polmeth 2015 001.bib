@article{ WOS:000210543500004,
Author = {Angelo, Nathan},
Title = {What Happened to Educational Equality? Tracing the Demise of
   Presidential Rhetoric on Racial Inequality in Higher Education},
Journal = {NEW POLITICAL SCIENCE},
Year = {2015},
Volume = {37},
Number = {2},
Pages = {224-240},
Abstract = {This article asks why discussion of racial inequality in higher
   education is absent from presidential speeches, which helps us
   understand why it is absent from the public agenda. I find a decrease in
   presidential speech about race and education after the 1980 and 1984
   elections. By analyzing Jimmy Carter and Ronald Reagan's rhetoric during
   those elections, we can see that Carter struggled to promote affirmative
   action and, instead, supported historically black colleges and
   universities (HBCUs). Reagan adopted Carter's arguments about HBCUs and
   used them to support his strategy to appeal to whites' and white
   ethnics' racial resentments. This dynamic led to a political stalemate:
   Democrats could not address educational inequality, distance themselves
