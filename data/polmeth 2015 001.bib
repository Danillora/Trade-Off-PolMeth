@article{ WOS:000356048200005,
Author = {Murer, Jeffrey Stevenson},
Title = {The Emergence of a Lumpen-Consumerate: the Aesthetics of Consumption and
   Violence in the English Riots of 2011},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
