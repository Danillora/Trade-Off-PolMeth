@article{ WOS:000433515000008,
Author = {Tellez, Andrea Marcela Barrera},
Title = {The Humanization as a Movement between the Alterity and the Otherness in
   Paulo Freire's Political Thinking},
Journal = {CIENCIA POLITICA},
Year = {2015},
Volume = {10},
Number = {20},
Pages = {177-200},
Month = {JUL-DEC},
Abstract = {In this article about Paulo Freire's political thinking, built from the
   review of some of the most representative texts of his work, I will
   assert that for this Brazilian pedagogue, humans beings are, above all,
   humanity being made at each instant. In this regard, I will continuously
   refer to what I have defined as the humanization movement in Freire's
   thinking. This movement is essentially the continuous process
   (individual and collective) that does the transition between the old and
   new societies. In this work, I will show that this transition requires
   the elimination of oppression relationships, as well as the transition
   from a necrophiliac to a biophiliac praxis, the one of liberty. In
   addition, I will affirm that in this transition from the oppression and
   the humanization -that is always an unfinished movement-, there is a
