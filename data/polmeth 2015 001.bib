@incollection{ WOS:000377044800008,
Author = {Gardiner, Michael},
Book-Author = {Gardiner, M},
Title = {Scotland, Queued},
Booktitle = {TIME AND ACTION IN THE SCOTTISH INDEPENDENCE REFERENDUM},
Year = {2015},
Pages = {72-80},
Abstract = {The demand for self-determination is not seen by the state as something
   to be `answered' by a refusal, which would confirm it as present and
   historical, but as something to be subjected to that British form of
   inclusion that is the queue. We see this in the last defence of
   September 2014 that is `the vow', but more widely can trace the role of
   `the economy' as the production of queues The queue is both a principle
   of inclusion and a suspension from a prior and universal time, something
   quite familiar from the extraordinary penetration of surveillance in the
   UK, a Lockean extension of all personal relationships in universal terms
   of exchange. In this sense self-determination can be understood as a
   decay of the queue as a principle offranchise - after which, as we see
   after 2014, popular sovereignty becomes much more difficult to hold off.},
Publisher = {PALGRAVE},
