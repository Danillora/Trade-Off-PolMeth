@article{ WOS:000370508700008,
Author = {Johnson, Heather L.},
Title = {These fine lines: locating noncitizenship in political protest in Europe},
Journal = {CITIZENSHIP STUDIES},
Year = {2015},
Volume = {19},
Number = {8, SI},
Pages = {951-965},
Month = {NOV 17},
Abstract = {Since 2012, refugee protest camps and occupations have been established
   throughout Europe that contest the exclusion of refugees and asylum
   seekers, but that also make concrete demands for better living
   conditions and basic rights. It is a movement that is led by migrants as
   noncitizens, and so reveals new ways of thinking of the political agency
   and status of noncitizenship not as simply reactive to an absence of
   citizenship, but as a powerful and transgressive subjectivity in its own
   right. This paper argues that we should resist collapsing analysis back
   into the frameworks of citizenship, and instead be attentive to the
   politics of presence and solidarity manifest in these protest camps as a
   way of understanding, and engaging, noncitizen activism.},
