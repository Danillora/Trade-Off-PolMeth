@article{ WOS:000359703800003,
Author = {Kirkland, Paul E.},
Title = {Sorrow Songs and Self-Knowledge: The Politics of Recognition and Tragedy
   in W. E. B. Du Bois's Souls of Black Folk},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2015},
Volume = {4},
Number = {3},
Pages = {412-437},
Month = {SUM},
Abstract = {This article takes up the philosophical influences on Du Bois and argues
   that these help us to see his distinct contribution to political
   thought. It examines the way in which his treatment of cultural
   alternatives offers an alternative to the dominant strand of American
   commercial life and reveals limits to politics. The considerations of
   politics and culture found in The Souls of Black Folk exhibit the way in
   which spiritual strivings, directed toward political fulfillment,
   ultimately transcend the politics of recognition. In his treatment of
   education, infinite longing, and the sorrow songs, he presents a tragic
   vision as a source for an aesthetics of profound cheer and the path to
   self-knowledge.},
Publisher = {UNIV CHICAGO PRESS},
Address = {1427 E 60TH ST, CHICAGO, IL 60637-2954 USA},
