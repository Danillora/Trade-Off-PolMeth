@incollection{ WOS:000401637200005,
Author = {Oh, Jennifer S.},
Editor = {Howe, B},
Title = {Non-regular Workers in Japan},
Booktitle = {DEMOCRATIC GOVERNANCE IN NORTHEAST ASIA: A HUMAN-CENTERED APPROACH TO
   EVALUATING DEMOCRACY},
Series = {Palgrave Pivot},
Year = {2015},
Pages = {87-104},
Abstract = {This chapter examines the deficiencies of Japanese democratic governance
   through the perspective of non-regular workers. In Japan, organized
   labor unions, the labor policymaking system, and electoral competition
   serve to secure the rights of regular workers at the expense of
   non-regular workers. As a result, non-regular workers, who constitute
   nearly 40 percent of the total employed labor force, are excluded from
   the very institutions that should represent and empower them. The case
   of Japanese non-regular workers demonstrates that procedurally stable
   and strong democracies can systematically disempower and underrepresent
   a large section of the population.},
