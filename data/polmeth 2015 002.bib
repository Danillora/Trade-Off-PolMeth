@article{ WOS:000217973400003,
Author = {Monge, Julia},
Title = {The Uneasiness in the City: Politics of Truth and Tragedy in Foucault's
   Reading of Oedipus Rex},
Journal = {ANACRONISMO E IRRUPCION},
