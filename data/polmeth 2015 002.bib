@article{ WOS:000370951700005,
Author = {Torpey, John},
Title = {The ``Axial Age{''} vs. Weber's Comparative Sociology of the World
   Religions},
Journal = {POLITICA \& SOCIETA},
Year = {2015},
Volume = {4},
Number = {2},
Pages = {231-248},
Month = {MAY-AUG},
Abstract = {This paper explores the notion of an Axial Age, much-discussed of late,
   and compares it with Weber's comparative sociology of the world
   religions. In contrast to the coherence and similarity of approach
   implied in the Axial Age thesis, the article argues that Weber was more
   inclined to emphasize the differences between the various world
   religions and their divergent consequences for the worldviews of those
   socialized into them.},
