@article{ WOS:000364706500001,
Author = {Martens, Linsay and McNutt, Kathleen and Rayner, Jeremy},
Title = {Power to the People? The Impacts and Outcomes of Energy Consultations in
   Saskatchewan and Nova Scotia},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2015},
Volume = {48},
Number = {1},
Pages = {1-27},
Month = {MAR},
Abstract = {Like other policy subsectors, power generation has been affected by the
   governance changes of the last two decades, including a shift to more
   collaborative state-society relations. Collaborative governance implies
   new kinds of public engagement designed to provide both input
   legitimacy, through the involvement of a broader range of actors in
   policy design, and output legitimacy, through enhanced feedback and
   policy learning. This paper compares the impact of ``governance-driven
   engagement{''} in the power generation subsectors in Nova Scotia and
   Saskatchewan, arguing that engagement increased the complexity of the
   policy mix in the sub-sector without succeeding in providing better
   feedback and learning. The paper notes a recent trend towards more
   expert-driven and less collaborative processes, as both provinces
   struggled to rationalize and simplify power generation policy goals and
   instruments.},
