@article{ WOS:000354240300001,
Author = {Smith, Sarah},
Title = {`When ``gender{''} started': the United Nations in post-occupation
   Timor-Leste},
Journal = {GLOBAL CHANGE PEACE \& SECURITY},
Year = {2015},
Volume = {27},
Number = {1, SI},
Pages = {55-67},
Month = {JAN 2},
Abstract = {This article examines gender mainstreaming processes in successive UN
   peacebuilding missions in Timor-Leste, with a focus on the relationship
   between these missions and the national women's organizations who were
   vehicles for implementation. Apparent frictions occur in this process
   and the article suggests that the gender rhetoric and practice
   incorporated into UN peacebuilding since 2000 can have potentially
   destabilizing effects for women's activism in post-conflict settings.
   Women's organizations socialize and negotiate around gender norms in
   order to mitigate this potential and aim to identify the synergies
   between women's activism before peacebuilding, and gender mainstreaming
   policies and practice post-conflict. This article provides insight into
   how national women's organizations socialize gender norms, as well as
   how women's post-conflict activism can be shaped by the presence of UN
   peacebuilding.},
