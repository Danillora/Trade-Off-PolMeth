Affiliations = {Rutgers University System; Rutgers University Camden; Rutgers University
   New Brunswick},
Funding-Acknowledgement = {CAPS at Harvard University; IQSS at Harvard University; Ash at Harvard
   University; Taubman at Harvard University},
Funding-Text = {This work was supported by several research centers at Harvard
   University, including CAPS, IQSS, Ash, and Taubman. Thanks also to
   Harvard Law School, HKS, and Suffolk Law School, for permission to
