@article{ WOS:000346181700005,
Author = {Akirav, Osnat},
Title = {Re-election: Different Skills for Different Roles},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2015},
Volume = {50},
Number = {1},
Pages = {90-118},
Month = {JAN},
Abstract = {The current research examines the ability of legislators to be
   re-elected through fulfilling their three roles: legislation, oversight
   and representation. This study also re-examines Mayhew's claim (1974)
   that most legislators are motivated by a desire to be re-elected, and
   that this desire drives their legislative activity through its utility
   in advertising, credit claiming and position taking. We argue that the
   skills required for selection and re-election are different from those
   required for enacting legislation. Thus, we distinguish between two
   types of legislators - those who are electable and those who are
   successful in the legislature.},
