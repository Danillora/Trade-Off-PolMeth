   Shames, Shauna L., Rutgers State Univ, Dept Polit Sci, Camden, NJ 08102 USA.},
DOI = {10.1080/13621025.2015.1074348},
ISSN = {1362-1025},
EISSN = {1469-3593},
Keywords = {political ambition; silent citizenship; women of color; black women;
   Hispanic women; Latinas; running for office; candidates; gender},
