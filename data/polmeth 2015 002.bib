@article{ WOS:000354189700005,
Author = {Finch, Ray},
Title = {The Kremlin's Economic Checkmate Maneuver},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2015},
Volume = {62},
Number = {3},
Pages = {187-191},
Month = {MAY-JUN},
Abstract = {Much has been written over the past year describing how Russian security
   forces were able to seize the Crimean peninsula, which ultimately led to
   annexation in March 2014. The West responded to this aggression with
   both political and economic measures. Russia's membership in the G8 was
   annulled and a series of economic sanctions have been applied to force
   Russia to renounce its claims in Crimea and discontinue its support for
   separatists in southeastern Ukraine. The Russian leadership has
   undertaken its own retaliatory measures. Besides enacting its own
   sanctions, the Kremlin has redoubled its efforts to attack what it
   perceives as the Achilles heel of American leadership: U.S. financial
   indebtedness. This brief essay examines the Kremlin's strategy to weaken
   faith in the U.S. economy and to unseat the dollar as the global reserve
   currency.},
