@article{ WOS:000420461400003,
Author = {Mella Polanco, Marcelo},
Title = {Marxism-Leninism, iconoclastic thinking and socialist new common sense
   in Chile during the 1980s},
Journal = {IZQUIERDAS},
Year = {2015},
Volume = {24},
Pages = {57-81},
Month = {JUL},
Abstract = {This article analyzes the process of reconstruction of the ideological
   and strategic repertoires in the Chilean socialism after the fall of
   President Allende and his coalition of parties, the Unidad Popular.
   Particularly, will be examined the ideological displacements of the left
   in the context of the process of ``renovacion socialista{''}. At the
   same time, will investigate the linkages between ``iconoclastic{''}
   theories as ``analytical marxism{''} and reconstruction of socialism
   during the 1980s, in the context of the consolidation of the moderate
   opposition to the authoritarian regime of Pinochet. It is argued in this
   paper that the critique of marxism-leninism developed by the
   ``renovacion socialista{''} constituted a form of internalization
   compared to the experience of the defeat of the Unidad Popular project
   and the subsequent political annihilation. In this course, the
   appropriation of different expressions of ``iconoclastic{''} thought
   allows to reconstruct a new common sense for socialism with adaptive
   high propensity regarding preexisting historical conditions.},
Publisher = {UNIV SANTIAGO CHILE, INST ESTUDIOS AVANZADOS},
Address = {ROMAN DIAZ 89, PROVIDENCIA, SANTIAGO, 00000, CHILE},
Type = {Article},
Language = {Spanish},
Affiliation = {Polanco, MM (Corresponding Author), Univ Santiago Chile, IDEA, Santiago, Chile.
