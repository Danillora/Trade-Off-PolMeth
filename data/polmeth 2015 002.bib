@article{ WOS:000373999300007,
Author = {Palacios Cruz, Victor H.},
Title = {The Desire for Knowledge as an Incentive for Life in Community,
   following Michel de Montaigne},
Journal = {FORO INTERNO-ANUARIO DE TEORIA POLITICA},
Year = {2015},
Volume = {15},
Pages = {159-179},
Abstract = {A tendency towards truth indicates an inclination towards being in tune
   with the world and inserting one's own life within it. It is a process
   that requires the concurrence of other viewpoints, fragmentary and
   diverse, the incorporation of which ensures the objectivity of knowledge
