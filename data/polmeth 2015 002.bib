@article{ WOS:000359986400007,
Author = {Dohotariu, Anca},
Title = {FAMILY POLICIES, GENDERED NORMS, AND CULTURAL MEANINGS IN POST-COMMUNIST
   ROMANIA},
Journal = {ANALELE UNIVERSITATII BUCURESTI-STIINTE POLITICE},
Year = {2015},
Volume = {17},
Number = {1},
Pages = {119-137},
Abstract = {Based on a qualitative sociological research carried out on unmarried
   parents cohabiting in Bucharest, this article examines the way in which
   young middle-class people living in the capital city of Romania embody
   and contribute to the dynamics of the cultural understandings of
   post-communist family policy. First, the analysis of the main
   theoretical axes and concepts relevant for family policies in Central
   and Eastern Europe countries is useful in order to gain greater
   understanding of the domestic familialism that promotes essentialist
   gender roles and conservative family relations at the macro level in
   Romania. Furthermore, qualitative research focused on the cultural
   understandings of family policies at the micro level shows that family
   provisions (parental leave and daycare services) are not perceived as a
   part of the current social rights. This qualitative field research shows
   that, due to the inherited domestic familialism and state paternalist
   practices, family policies in post-communist Romania are not shaped by
   proactive social reforms.},
