@article{ WOS:000420561300001,
Author = {Holtkamp, Lars K and Bathge, Thomas and Friedhoff, Caroline},
Title = {Local parties and voters' associations in Eastern and Western Germany},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2015},
Volume = {9},
Number = {1-2},
Pages = {1-18},
Month = {JUN},
Abstract = {Based on a survey of city councilors, we seek to study the extent of
   party politicization of local democracy in two western and two eastern
   German states. These cases paint a nuanced picture of concordant and
   competitive democratic orientations. Apparently, the orientations of
   local voters' associations are far more consociational than the
   orientations of political parties. Due to the fact that voters'
   associations' share of the votes is increasing we assume that the extent
   of party politicization is slightly decreasing. We primarily attribute
   the varying degree of party politicization of local democracy between
   the German states, which is considerable in part, to the impact of the
   different municipal constitutions and less so to the varying political
   cultures.},
Publisher = {SPRINGER VIEWEG-SPRINGER FACHMEDIEN WIESBADEN GMBH},
