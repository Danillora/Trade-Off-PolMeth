@article{ WOS:000420459300009,
Author = {Gaido, Daniel},
Title = {The origins of the Transitional Program in the Comintern},
Journal = {IZQUIERDAS},
Year = {2015},
Volume = {23},
Pages = {191-214},
Month = {APR},
Abstract = {The roots of the Transitional Program in earlier writings of Trotsky
   have been traced in the secondary literature. Has received much less
   attention to the origins of the Transitional Program in discussions of
   the Comintern between the Third and Fourth Congress, and in particular
   the contribution of its largest outside Russia national section, the
   Communist Party of Germany, which was the source of the united front
   tactic in 1921. In this article the roots Transition Program are
   discussed in the debates of the Communist International. This is
   important because it shows that the slogans Transition Program are not
   sectarian dogmas, but the result of collective revolutionary experience
   of the working class during the period considered, since the Bolshevik
   Revolution to the founding conference of the Fourth International (1917
   - 1938).},
