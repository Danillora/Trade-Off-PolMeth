@article{ WOS:000214719000001,
Author = {Hanzel, Igor},
Title = {Marx's Methods of Theory Construction: Categories, Magnitudes, and
   Variations of Sizes of Magnitudes under Certain Idealizations},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2015},
Volume = {5},
Number = {4},
Pages = {413-438},
Abstract = {The paper reconstructs the conceptual methods used in the construction
   of volume 1 of Capital (Marx 1976a) as well as in the manuscripts of its
   books 2 and 3, published in section 2 of the MEGA (Marx/Engels
   Gesamtausgabe 2). Drawing on them it identifies the key components of
   the conceptual methods Marx employed in Capital. It starts with a
   delineation of the methodological implications of Hegel's (2010) Science
   of Logic for Marx. Next it reconstructs the principal methods employed
   by Marx in volume 1 of Capital as well as in the manuscripts he intended
   for its book 2 and book 3. Finally, it presents a general
   characterization of these methods.},
