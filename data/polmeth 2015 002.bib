@article{ WOS:000383906900005,
Author = {Lightfoot, Simon},
Title = {THE EU AND THE 2015 YEAR OF DEVELOPMENT},
Journal = {ANALELE UNIVERSITATII BUCURESTI-STIINTE POLITICE},
Year = {2015},
Volume = {17},
Number = {2, SI},
Pages = {71-80},
Abstract = {2015 has been an ``unprecedented year for international development{''}
   and as such it makes it timely to offer an assessment of the actions of
   the EU as a global development actor. This article examines the actions
   of the EU in the financing for development summit in Addis Ababa in
   July, the UN summit on ``sustainable development{''} in New York in
   September and it will then look forward to the United Nations Climate
   Change Conference in Paris in December. The article first reviews the EU
   as a development actor before moving on to examine its positions and
   role of the FFD and SDG talks. It will then look forward to the Paris
   talks on climate change by identifying issues from the Copenhagen talks.
   As noted in the literature the EU has ambitions to play a leadership
   role in a variety of global fora and this article will analyse the
   actions of the EU to see if this leadership ambition was present in
   2015.},
