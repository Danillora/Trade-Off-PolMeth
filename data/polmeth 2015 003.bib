@article{ WOS:000352852900003,
Author = {Chatagnier, John Tyson},
Title = {Conflict bargaining as a signal to third parties},
Journal = {JOURNAL OF THEORETICAL POLITICS},
Year = {2015},
Volume = {27},
Number = {2},
Pages = {237-268},
Month = {APR},
Abstract = {Within the international system, states frequently fight even when
   opponents have little or nothing to offer them. Yet, international
   relations scholars envision conflict as a means for states to acquire
   some amount of a desired good, and view bargaining through this lens.
   This paper presents a model in which war and conflict bargaining can
   serve as signals to potentially hostile third parties. The analysis
   indicates that states sometimes have incentives to bargain harder than
   they would otherwise, in order to conceal information from future
   enemies. This can lead to war, even when a peaceful settlement should be
   possible.},
