@article{ WOS:000371114800006,
Author = {Nawara, Steven P.},
Title = {Who Is Responsible, the Incumbent or the Former President? Motivated
   Reasoning in Responsibility Attributions},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2015},
Volume = {45},
Number = {1},
Pages = {110-131},
Month = {MAR},
Abstract = {This article establishes that transitions of power from one presidential
   administration to the next can act as a source of uncertainty regarding
   whom citizens view as responsible for national conditions. I argue that
   citizens confront this ambiguity in a partisan manner when making
   responsibility attributions of credit and blame. Using the economy and
   the Iraq War as examples, ordinary partisans frequently ascribe
   responsibility using motivated reasoning by crediting the president of
   their own political party for perceived successes and blaming the
   president of the opposite party for perceived failures.},
Publisher = {WILEY},
