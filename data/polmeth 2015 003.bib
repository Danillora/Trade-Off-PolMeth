@article{ WOS:000356047900006,
Author = {Khosrokhavar, Farhad},
Title = {The Constrained Role of the Muslim Chaplain in French Prisons},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
Year = {2015},
Volume = {28},
Number = {1},
Pages = {67-82},
Month = {MAR},
Abstract = {The article explores the situation of Muslims in French prisons. It
   shows the lines of fracture between the Muslim chaplains on one hand,
   the authorities, and some Muslim groups on the other. Formalized Muslim
   chaplaincy in French prisons has limited effectiveness in keeping in
   check radicalization in prison, due to structural obstacles and due to
   the emergence of new groups of Muslim prisoners, the Salafists, who
   develop a version of Islam that most of the time is out of tune with
   that of the imams in prison.},
