@article{ WOS:000348851200006,
Author = {Clemens, Austin C. and Crespin, Michael H. and Finocchiaro, Charles J.},
Title = {The Political Geography of Distributive Politics},
Journal = {LEGISLATIVE STUDIES QUARTERLY},
Year = {2015},
Volume = {40},
Number = {1},
Pages = {111-136},
Month = {FEB},
Abstract = {Distributive politics plays an integral role in theories of legislative
   politics because it represents a fundamental aspect of legislators'
   electoral connection to constituents. We argue that because distributive
   politics is at its core a constituency-centered process, it is essential
   to take geography into account in both substantive and statistical
   terms. Our analysis, which employs Geographic Weighted Regression,
   reveals substantial spatial heterogeneity in traditional models of
   pork-barreling. The results challenge the implicit assumption that one
   size fits all when modeling the behavior of members of Congress in the
   realm of distributive politics.},
