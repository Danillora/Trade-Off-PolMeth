Author = {Di Virgilio, Aldo and Giannetti, Daniela and Pedrazzani, Andrea and
   Pinto, Luca},
Title = {Party Competition in the 2013 Italian Elections: Evidence from an Expert
   Survey},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2015},
Volume = {50},
Number = {1},
Pages = {65-89},
Month = {JAN},
Abstract = {In this article, we examine the structure of party competition in Italy
   in the February 2013 elections. We rely on the spatial approach to party
   competition to analyse the most salient dimensions of the policy space
   in the Italian context. Our analysis is based on quantitative estimates
   from expert survey data. These data highlight the salience of the
   socioeconomic policy dimension and capture the change in the importance
   of the EU dimension. Finally, this study provides an analysis of
   potential coalition governments in the aftermath of the 2013 general
   election that is grounded on the spatial approach to coalition
   formation.},
