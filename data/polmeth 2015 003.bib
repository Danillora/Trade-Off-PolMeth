@article{ WOS:000433515000012,
Author = {Cunial, Santiago},
Title = {Framing Controversial Issues: the Use of Drugs as a Policy Problem in
   Argentina},
Journal = {CIENCIA POLITICA},
Year = {2015},
Volume = {10},
Number = {20},
Pages = {267-294},
Month = {JUL-DEC},
Abstract = {This article aims to answer the following question: how do policymakers
   frame the issue of illegal drugs use in Argentina? The main objective is
   to describe and compare the ways in which policymakers think the problem
   of illegal drug use in Argentina. The synchronic, descriptive and
   exploratory study is based on the analysis of the regulatory body of
   national public policies in force to May 2014 in Argentina and verbal
   sources of policymakers involved in the issue. The article describes
   four ways in which the problem of drug use is framed: as a security
   issue, as public health issue, as a development issue, and finally as an
   issue that concerns the autonomy of individuals. The article concludes
   promoting the search for comprehensive policies that stop associating
   drug users with the figure of criminals and sick persons.},
