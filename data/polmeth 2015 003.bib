@incollection{ WOS:000377044800005,
Author = {Gardiner, Michael},
Book-Author = {Gardiner, M},
Title = {The Spirit of `57},
Booktitle = {TIME AND ACTION IN THE SCOTTISH INDEPENDENCE REFERENDUM},
Year = {2015},
Pages = {40-47},
Abstract = {This chapter suggests a historical reorientation away from those
   greatest victories of consensus, which are the greatest victories of
   empty time, to the less visible victories of the signs of
   self-determination that interrupt consensus. In particular it notes how
   even within the highest moment of consensus there was a constitutional
   questioning of financial sovereignty that was closely followed by a
   cultural questioning, and indeed by the possibility of culture as a
   shared determination of the present. Moreover the 20105 pressure for
   sell-determination as action can in large part be understood through
   this counter-consensual moment, as can my definition of action itself.},
