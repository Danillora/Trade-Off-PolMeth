@article{ WOS:000364706500005,
Author = {Vucetic, Srdjan and Tago, Atsushi},
Title = {Why Buy American? The International Politics of Fighter Jet Transfers},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2015},
Volume = {48},
Number = {1},
Pages = {101-124},
Month = {MAR},
Abstract = {When it comes to buying military aircraft, what leads states to prefer
   one supplier over the other? This paper explores this question from the
   perspective of international relations theory. First we use social
   network analysis to map out fighter jet transfers during and after the
   Cold War and examine the extent to which historical structures of
   international hierarchy shape contemporary supplier-receiver
   relationships. Next, we use a basic probit model to analyse the origins
   of fighter jets in the world's air forces today to evaluate the effect
   of interstate orders of super-ordination and sub-ordination on sourcing
   patterns. All things being equal, the more a state is embedded in US
   security and economic hierarchy, the more it is likely to buy
   American-made fighter jets.},
