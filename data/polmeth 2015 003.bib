@article{ WOS:000420462400005,
Author = {Retamal, Pablo Fuentes},
Title = {In search of traces, evidence and signs: a meticulous look at three
   anarchist characters from Manuel Rojas' narrative tetralogy},
Journal = {IZQUIERDAS},
Year = {2015},
Volume = {25},
Pages = {120-142},
Month = {OCT},
Abstract = {This article examines three characters from Manuel Rojas' narrative
   tetralogy for the purpose of revealing some people's identities whose
   inspiration was useful for the narrator to shape the fiction in novels.
   Since Rojas' diegesis is placed temporarily in de first decades of the
   twentieth century during the validity of Jaramillo's Law (legal
   regulation which empowers authority to exile every activist who may have
   ideas that are contrary to the state security) the narrator accepts some
   protection and camouflages the writer Gonzalez Vera, the poet Gomez
   Rojas, and the doctor Juan Gandulfo between the pages of the story. This
   narrative procedure keeps these anarchists' ideas and projects safe,
   considering that, making their names and activities evident would
   represent a problem for the state security apparatus.},
