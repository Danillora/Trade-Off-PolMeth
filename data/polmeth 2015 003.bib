@article{ WOS:000354146200001,
Author = {Battaglino, Jorge},
Title = {Politicians and the Military in the Governments of the New Left in South
   America},
Journal = {POLITICA Y GOBIERNO},
Year = {2015},
Volume = {22},
Number = {1},
Pages = {3-43},
Abstract = {The goal of this article is to build an explanatory typology of the
   political-military relations in the governments of the new left in South
   America. In particular the main modes of interaction are discussed as
   well as the factors that explain their variations in the cases of
   Argentina, Bolivia, Brazil, Chile, Ecuador and Venezuela. Models are
   analyzed in the light of the impact of: a) the political relevance that
   defense and armed forces have acquired to such governments, and b) the
   political power of the military at the beginning of such
   administrations. The main argument is that the growing political
   relevance of defense and armed forces has generated a space of
   convergence of interests between civilian and the military that has
   favored both the stability of the democratic regime and the corporate
   interests of the armed forces.},
