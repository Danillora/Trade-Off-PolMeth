@article{ WOS:000211453900005,
Author = {Kinclova, Lenka},
Title = {Legitimacy of the ``Humanitarian Military Intervention{''}: An Empirical
   Assessment},
Journal = {PEACE ECONOMICS PEACE SCIENCE AND PUBLIC POLICY},
Year = {2015},
Volume = {21},
Number = {1},
Pages = {111-152},
Month = {JAN},
Abstract = {This article represents an innovative perspective on the controversial
   issue of legitimacy of the ``humanitarian military intervention{''}
   (HMI), by situating the normative debate into a quantitative
   perspective. It evaluates legitimacy of HMI by assessing a degree of
   ``humanitarianism{''} behind the ``motives and means{''} and the
   ``outcomes{''} of military interventions from the post-WWII era. The
   ``motives and means{''} of the interveners are evaluated based on the
   theoretical framework of the Just War Theory (JWT), more particularly
   based on the quantified ``jus ad bellum{''} JWT criteria; and they are
   compared with the existence of ``humanitarian outcomes{''} of these
   interventions in the light of consequentialist ethics. An overall goal
   is to generate a comprehensive picture of the dynamics and relationships
   within the HMI concept based on the empirical evidence.},
Publisher = {WALTER DE GRUYTER GMBH},
Address = {GENTHINER STRASSE 13, D-10785 BERLIN, GERMANY},
Type = {Article},
Language = {English},
Affiliation = {Kinclová, L (Corresponding Author), Univ New York Prague, Inst Econ Relat, Londynska 41, CR-12000 Prague, Czech Republic.
   Kinclova, Lenka, Univ New York Prague, Inst Econ Relat, Londynska 41, CR-12000 Prague, Czech Republic.
   Kinclova, Lenka, Maastricht Univ, Maastricht Grad Sch Governance, NL-6211 TC Maastricht, Netherlands.},
DOI = {10.1515/peps-2014-0006},
ISSN = {1079-2457},
