@article{ WOS:000446487800003,
Author = {Romm, Celia and Raisinghani, Mahesh and Rippa, Pierluigi},
Title = {The Politics of e-Learning: A Play in Four Acts},
Journal = {INTERNATIONAL JOURNAL OF E-POLITICS},
Year = {2015},
Volume = {6},
Number = {2},
Pages = {30-42},
Month = {APR-JUN},
Abstract = {The goal of this research is to study the political strategies utilized
   in the context of e-Learning. The paper is based on the e-Learning
   Political Strategies (ELPoS) model. The model is based on two
   dimensions: (1) the direction of the political strategy (upward or
   downward), and (2) the scope of the political strategy (individual or
   group based). The model assumes that the interaction between these
   dimensions defines four different types of e-Learning political
   strategies, which, in turn, lead to different outcomes. The model is
   discussed in the context of the literature on e-Learning and is
   accompanied with a case study that is divided into four parts
   ({''}acts{''}). Each of the four acts provides an example of each of the
   four strategies in the model. The discussion and conclusions section
   integrates the findings from the case study, outlines the rules that
   govern the utilization of political behavior in the context of
   e-Learning, and lists the practical conclusions that can be drawn from a
   better understating of the politics of e-Learning.},
