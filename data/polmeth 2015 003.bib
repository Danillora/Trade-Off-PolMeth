@incollection{ WOS:000386041700006,
Author = {Larimer, Christopher W.},
Book-Author = {Larimer, CW},
Title = {Iowa Voters on Iowa Governors},
Booktitle = {GUBERNATORIAL STABILITY IN IOWA: A STRANGLEHOLD ON POWER},
Series = {Palgrave Pivot},
Year = {2015},
Pages = {106-131},
Abstract = {Chapter 4 presents findings from an original statewide survey sent to
   1,500 randomly selected Iowa voters in December of 2013. Respondents
   view Governor Ray most favorably, followed by Governors Vilsack and
   Branstad, with Governor Culver significantly lower. On both ``management
   powers{''} (related to managing the affairs of the state) and
   ``connecting powers{''} (related to work effort and ability to
   communicate), Culver is viewed less favorably by out- and in-party
   respondents. In other words, partisan loyalty alone does not explain the
   differences observed among survey respondents in terms of their views of
   recent Iowa governors. This chapter provides further evidence as to why
   models of gubernatorial popularity need to be expanded to include more
   personal measures of a governor's ability to relate to voters.},
