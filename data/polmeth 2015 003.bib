@article{ WOS:000364387700001,
Author = {Faber, Michael J.},
Title = {The Federal Union Paradigm of 1788: Three Anti-Federalists Who Changed
   Their Minds},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2015},
Volume = {4},
Number = {4},
Pages = {527-556},
Month = {FAL},
Abstract = {The Constitution would not have been ratified without the support of
   many who harbored doubts about the document. Samuel Adams, the logical
   Anti-Federalist leader in Massachusetts, helped broker a compromise that
   set the stage for ratification in other states. Edmund Randolph, who
   declined to sign the Constitution in the Philadelphia Convention, became
   one of its chief spokesmen in the ratifying convention in Richmond.
   Melancton Smith, one of New York's most prominent opponents to the
   Constitution, voted to ratify the document he attacked earlier in the
   debates. All three ultimately supported the Constitution because,
   although they supported substantial state sovereignty, they believed
   that such sovereignty would not be meaningful if it threatened the union
   of the states. These three shared belief in what I have called the
   Federal Union Paradigm, the idea that union was essential to maintain
   state sovereignty and that a substantial degree of state sovereignty was
   equally essential to an effective federal union.},
