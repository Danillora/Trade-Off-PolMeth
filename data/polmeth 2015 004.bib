@article{ WOS:000216889100001,
Author = {Jiemian, Yang},
Title = {China's ``New Diplomacy{''} under the Xi Jinping Administration},
Journal = {CHINA QUARTERLY OF INTERNATIONAL STRATEGIC STUDIES},
Year = {2015},
Volume = {1},
Number = {1},
Pages = {1-17},
Month = {APR},
Abstract = {Since the establishment of the Xi Jinping administration, China's
   diplomacy has undergone significant changes while adopting many legacies
   of the previous times. It has increasingly manifested the diplomatic
   thinking of a great rising nation, strategic designs which keep pace
   with the times, and strong measures to tackle new challenges as well as
   to promote favorable changes for China's continuing development. With a
   review of the historical background and current situations faced by
   China, this paper closely examines the innovations and improvement of
   China's diplomatic thinking, the characteristics of China's current
   diplomatic strategy, and the new features of China's diplomatic
   practice. It is concluded that, despite the large advance in China's
   diplomatic thinking, strategies and practice, they need to be further
   theorized, institutionalized and operationalized, in order to fulfill
   the varying tasks under the rapidly changing domestic and international
   environments.},
