@article{ WOS:000211456100010,
Author = {Haar, Roberta and Krebs, Lutz F.},
Title = {Choosing to Intervene: US Domestic Politics and Moral Imperatives},
Journal = {PEACE ECONOMICS PEACE SCIENCE AND PUBLIC POLICY},
Year = {2015},
Volume = {21},
Number = {4},
Pages = {497-505},
Month = {DEC},
Abstract = {The end of the Cold War meant fewer constraints on humanitarian
   intervention, and the third pillar of the nascent R2P norm suggests at
   least a moral imperative to intervene when another country's population
   is threatened. Yet US leaders continue to shy away from protecting
   innocents outside of the United States from harm - despite the fact that
   presidential candidates often campaign on restoring America's moral lead
   in the world and, in particular, on US responsibilities to avert mass
   atrocities. This paper investigates the extent to which US military
   intervention abroad is driven by domestic considerations. Using logistic
   regression analysis, we aim to explain decisions by Presidents Bush Sr.,
