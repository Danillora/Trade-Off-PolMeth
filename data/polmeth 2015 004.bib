@article{ WOS:000214871900004,
Author = {Stinebrickner, Bruce},
Title = {Robert A. Dahl and the essentials of Modern Political Analysis:
   politics, influence, power, and polyarchy},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2015},
Volume = {8},
Number = {2, SI},
Pages = {189-207},
Abstract = {In Modern Political Analysis (MPA), Robert A. Dahl presents what he saw
   as the essentials of politics and political science. Spanning four
   decades of Dahl's scholarly career, the six editions of MPA address (i)
   the nature of politics; (ii) `influence', the constituent element of
   politics and MPA's term for what political scientists often call
   `power'; and (iii) similarities and differences among political systems.
   Seven `forms of influence' - power, coercion, force, persuasion,
   manipulation, inducement, and authority - are distinguished and
   analyzed. In exploring similarities and differences among the world's
   political systems, MPA presents an overview of Dahl's insights on
   democracy and polyarchy. The six MPA editions also provide an
   opportunity to observe how Dahl's thinking about the essentials of his
   discipline evolved over forty years.},
