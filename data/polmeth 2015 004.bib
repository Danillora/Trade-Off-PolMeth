@article{ WOS:000365605100010,
Author = {Mollett, Sharlene},
Title = {``Displaced futures{''}: indigeneity, land struggle, and mothering in
   Honduras},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2015},
Volume = {3},
Number = {4},
Pages = {678-683},
Month = {OCT 2},
Abstract = {This paper draws from the insights of feminist political ecology and
   ethnographic fieldwork in the coastal Miskito communities of the
   Honduran Rio Platano Biosphere Reserve. In this paper, I highlight how
   Miskito matriarchal land organization is challenged by both the state
   and Miskito men. Through a reflection of multi-scaled narratives, I
   argue that Miskito women work to counter entrenched patriarchal
   assumptions and racialized state development practices that deny women's
   labor in agriculture and undermine their means to mother.},
