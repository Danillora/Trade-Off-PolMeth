@article{ WOS:000213909500009,
Author = {Nevalsky, Eric Chien},
Title = {Developing terrorism coverage: variances in news framing of the January
   2015 attacks in Paris and Borno},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2015},
Volume = {8},
Number = {3},
Pages = {466-477},
Abstract = {Written news coverage of an event influences public perception and
   understanding of that event. Through agenda setting and news framing,
   journalists control the importance and substance of readers' beliefs
   about the event. While existing research has been conducted on the
   relationship between media coverage and the geographic location of the
   country an event took place in, there is limited understanding of this
   relationship in terms of terrorist events. Utilising an agenda-setting
   theory and news framing theory lens to compare news coverage of the
   January 2015 terrorist attacks in Paris, France, and Borno, Nigeria
   revealed significant variances in the overall coverage, headline style
   and discourse usage based on the event. In particular, the American news
   coverage positively framed France through detailed, sympathetic coverage
   and negatively framed Nigeria by overgeneralising and placing blame.
   Determining the origin and impacts of these variances is integral to
   forming a more comprehensive understanding of international terrorism
   and the most effective ways to combat it.},
