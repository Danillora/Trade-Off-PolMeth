@article{ WOS:000219470200006,
Author = {Aberg, Martin and Hogman, Ann-Kristin},
Title = {Histories Meet Histories: A Pilot Study of Migration and Civil Society
   in Swedish Medium-Sized Cities, Small Towns, and Villages},
Journal = {JOURNAL OF CIVIL SOCIETY},
Year = {2015},
Volume = {11},
Number = {2},
Pages = {187-203},
Abstract = {This article presents the results of a pilot study of social networking
   among immigrants in different types of local contexts. Although
   systematic research and conclusive evidence is lacking, previous studies
   suggest that civil society is differently organized in urban
   environments than it is in rural environments. If accurate, this
   suggestion implies that variation in local context is a decisive factor
   in the patterning of social networking among immigrants and,
   consequently, a decisive factor in the degree of inclusion of immigrants
   into civil society. In our research material we observe (1) that among
   first-generation immigrants a differentiation of social networking along
   the rural-urban continuum does exist and (2) that the historical
   backgrounds of both the receiving community and the recent
   immigrants-including the immigrants' motives for immigrating-are
   important to the success of networking activities. These findings show
   the need for further research into the co-variation of social
   networking, local context, and civil society structures.},
