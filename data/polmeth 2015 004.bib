@article{ WOS:000360327200004,
Author = {Thieme, Daniel and Liedhegener, Antonius},
Title = {``Far Left{''}, Political Centre or Something Completely Different? The
   Positioning of the Evangelical Church in Germany (EKD) within the
   Spectrum of Party Competition in Postsecular Society},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2015},
Volume = {56},
Number = {2},
Pages = {240-277},
Abstract = {Jurgen Habermas and Jose Casanova introduced the idea of a desired
   political role of religious actors in postsecular societies. Therefore
   political science has to readdress the political role of churches. This
   article is the first to analyze the text body of all politically
   relevant ``Denkschriften{''} (memorandums) published by the Evangelical
   Church in Germany (EKD) from 1990 to 2010 by using in depth content
   analysis. Based on the categories, data and analytical techniques of the
   Comparative Manifesto Project all memorandums were electronically
   processed in order to analyze the position of the EKD in the
   left-right-continuum of German domestic politics. Across all political
   fields our analysis indicates that the EKD is not to be found ``far
   left{''}, as various critics presume, but that it is in clear proximity
   to SPD and Greens in the left-right-continuum of German politics.},
