@article{ WOS:000210658000002,
Author = {Fromm, Georg H.},
Title = {Empiricism, Science, and Philosophy in The German Ideology},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2015},
Volume = {27},
Number = {1},
Pages = {9-32},
Abstract = {Ever since the full text of The German Ideology appeared in 1932 it has
   been subject to persistent misreading due to the fact that the
   idiosyncratic nature of this work-as merely a rough, unedited, and
   unfinished draft as well as a fiercely polemical text-has been
   neglected. As a result, the majority of scholars have tended to claim
   that Marx and Engels in this work propose an empiricist or positivistic
   conception of human history and society. A critical reading that is
   sensitive to the peculiar nature of this text reveals that these common
   interpretations are wholly ungrounded and that, on the contrary, the
   authors develop their polemic against the Young Hegelians from an
   outlook that is deeply indebted to the Hegelian philosophical legacy
   (even though paradoxically the latter espouses an absolute idealistic
   viewpoint).},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
