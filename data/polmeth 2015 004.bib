@article{ WOS:000379415400002,
Author = {Bertranou, Julian},
Title = {Effective Stateness. Reflections on the State building on Guillermo
   O'Donnell's work},
Journal = {TEMAS Y DEBATES},
Year = {2015},
Number = {29},
Pages = {33-51},
Month = {JAN-JUN},
Abstract = {Guillermo O'Donnell's work paid extensive attention to the issue of
   State's effectiveness mid power. For O'Donnell, the State and its
   different expressions are key elements of contemporary democracies. For
   the Argentine political scientist, the State should not be studied
   observing only bureaucracies and its instniments, but contemplating
   principally the effectiveness of the established legal order that it
   seeks to guarantee. With the help of O'Donnell's work this paper
   overviews several issues around State effectiveness: its infrastructural
   power; the inevitable, egalitarian and intra-subjective character of
   public order; and the links between effectiveness and capacity.},
Publisher = {UNIV NAC ROSARIO, FAC CIENCIAS POLITICAS \& RELACIONES INT},
