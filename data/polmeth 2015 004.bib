@article{ WOS:000219257500002,
Author = {Maggini, Nicola and Emanuele, Vincenzo},
Title = {Contextual effects on individual voting behaviour: the impact of party
   system nationalization in Europe},
Journal = {ITALIAN POLITICAL SCIENCE REVIEW-RIVISTA ITALIANA DI SCIENZA POLITICA},
Year = {2015},
Volume = {45},
Number = {2},
Pages = {105-130},
Month = {JUL},
Abstract = {This article has the purpose to assess if and how party system
   nationalization affects individual voting behaviour. Previous studies on
   party system nationalization have focused on systemic processes,
   exclusively dealing with aggregate data. The authors address this topic
   from a new empirical perspective, arguing that party system
   nationalization could act as a context dimension interacting with the
   vote choice function. How does this specific context dimension moderate
   the explanatory power of individual-level characteristics? On which
   determinants of vote choice does party system nationalization have a
   greater impact? To answer these questions, the authors focus on 23
   European countries through the use of the 2009 European Election Study.
   The empirical analysis shows that in nationalized contexts the impact of
   the left-right dimension on party support is higher than in
   territorialized contexts, whereas that of class as well as of
   culture-related variables is lower. The authors also discuss the
   implications of these findings.},
