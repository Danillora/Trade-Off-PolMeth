@article{ WOS:000219137100004,
Author = {Korostikov, Mikhail Yurievich},
Title = {Power and Property in Chinas},
Journal = {SRAVNITELNAYA POLITIKA-COMPARATIVE POLITICS},
Year = {2015},
Volume = {6},
Number = {2},
Pages = {50-65},
Abstract = {Despite the fact that China has historically symbolized an
   ``Oriental{''} pattern of power-property relations, with everything in
   the country belonging to a sovereign ruler, this has been changing over
   the last 35 years. Steadily and voluntarily the state is ceding control
   of property to different social groups. This process entails several
   problems: legal vacuum engenders rent-oriented behavior of bureaucracy,
   reforms often turn out to be half-way and cannot solve a problem.
   Nevertheless, the process seems to be irreversible and likely to foster
   political and social changes in China in the near future.},
