@article{ WOS:000365395700001,
Author = {Frye, Timothy},
Title = {What Do Voters in Ukraine Want? A Survey Experiment on Candidate
   Ethnicity, Language, and Policy Orientation},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2015},
Volume = {62},
Number = {5},
Pages = {247-257},
Abstract = {Language, ethnicity, and policy orientation toward Europe are key
   cleavages in Ukrainian society, but there is much debate about their
   relative importance. Based on a survey experiment of 1000 residents of
   Ukraine that manipulated three features of a fictional candidate running
   for parliament, I find that a candidate's ethnicity and language had
   little impact on reported vote choice, whereas economic policy
   orientation toward Europe was strongly associated with vote preference.
   Despite the intense politicization of both ethnicity and language and
   the violence in eastern Ukraine vote choice has not been reduced to an
   ethnic or linguistic census.},
