@article{ WOS:000357764600002,
Author = {Emmenegger, Patrick},
Title = {Maximizing Institutional Control Union Power and Dismissal Protection in
   Western Europe in the First Half of the Twentieth Century},
Journal = {COMPARATIVE POLITICS},
Year = {2015},
Volume = {47},
Number = {4},
Pages = {399+},
Month = {JUL},
Abstract = {During the first half of the 20th century, some of the strongest union
   movements failed to provide much protection against dismissal. This
   contrasts with countries with comparatively weak union movements, where
   workers benefitted from far-reaching statutory protection. This
   counterintuitive outcome can be explained by the unions' interest in
   maximizing institutional control by regulating dismissal protection in
   collective agreements. Yet such preemptive regulation was only possible
   under unique circumstances: it required a strong trade union Movement
   that could conclude collective agreements before the advent of
   employment contracts being regulated by statutory labor law. The
   regulation of dismissal protection by means of collective agreements had
   unintended consequences. As regulation progressed faster in statutory
   labor law, countries with weaker union movements soon obtained higher
   levels of dismissal protection.},
Publisher = {SHERIDAN PRESS},
