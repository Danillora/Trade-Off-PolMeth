   Berry, Justin A., Kalamazoo Coll, Dept Polit Sci, Kalamazoo, MI 49007 USA.
   Junn, Jane, Univ So Calif, Dept Polit Sci, Los Angeles, CA USA.},
DOI = {10.1080/13621025.2015.1074342},
ISSN = {1362-1025},
EISSN = {1469-3593},
