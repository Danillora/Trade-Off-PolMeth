@article{ WOS:000395622100015,
Author = {Liechtenstein, Stephanie},
Title = {The 22nd osce Ministerial Council Meeting in Belgrade An Anniversary
   Without Celebration},
Journal = {SECURITY AND HUMAN RIGHTS},
Year = {2015},
Volume = {26},
Number = {2-4},
Pages = {337-349},
Abstract = {This article analyses the outcome of the 22nd osce Ministerial Council
   (mc) meeting, held in Belgrade on 3 and 4 December 2015, the year that
   the osce celebrated the 40th anniversary of the Helsinki Final Act, its
   founding document. The article argues that the mc meeting was
   characterized by entrenched positions and that it illustrated the
   distrust and deep divides among the 57 osce participating States. The
   article explains that the negotiation process was overshadowed by the
   ongoing Ukraine crisis and by a number of bilateral conflicts between
   states. The author specifies some of the bilateral conflicts and shows
   how they took direct influence on the negotiation process and how they
   led to the fall of important draft documents. As a result, the Belgrade
   mc adopted only 5 declarations, among them on combating violent
   extremism and radicalization and on combating illicit drug trafficking.},
