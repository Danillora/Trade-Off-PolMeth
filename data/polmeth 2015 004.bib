@article{ WOS:000347500900001,
Author = {Balazs, Zoltan},
Title = {Artificial eternity: The problem of political succession in Pedro
   Calderon della Barca's Life Is a Dream and Heinrich von Kleist's
   The Prince of Homburg},
Journal = {CONTEMPORARY POLITICAL THEORY},
Year = {2015},
Volume = {14},
Number = {1},
Pages = {2-22},
Month = {FEB},
Abstract = {This article examines the problem of succession in Hobbesian terms.
   Hobbes tried to solve it by means of principles of `artificial eternity'
   that fitted his political science. But, evidently, political decisions
   cannot be explained only by principles. Securing artificial eternity is
   a goal that has ever been in the forefront of political thinking and
   practice, often better captured by literary works than by scientific
   treatises. Accordingly, the present approach makes use of two such
   works, Calderon's Life Is a Dream and Kleist's The Prince of Homburg. It
   is argued that both plays have valid and compelling solutions to the
   problem of succession, and that these solutions are surprisingly
   similar: political succession is a matter of a successful merging of
   identities while preserving the personal nature of political authority.},
