@incollection{ WOS:000386041700004,
Author = {Larimer, Christopher W.},
Book-Author = {Larimer, CW},
Title = {Gubernatorial Popularity and Power in Iowa Since 1969},
Booktitle = {GUBERNATORIAL STABILITY IN IOWA: A STRANGLEHOLD ON POWER},
Series = {Palgrave Pivot},
Year = {2015},
Pages = {40-75},
Abstract = {Drawing on over 200 measures of job approval of Iowa governors from 1969
   to 2014 from the JAR database and the Des Moines Register Iowa Poll,
   this chapter tests whether the popularity of Iowa governors is explained
   by the same set of political and economic factors as shown in previous
   research. While the results tend to fit with such research, when
   estimating popularity for each governor independently, the effect of
   economic factors is inconsistent across models. The chapter concludes by
   asking: If economic factors are not consistently predictive of electoral
   success of recent Iowa governors, what is missing from the model? More
   importantly, why were Iowa voters less apt to punish Governors Branstad
   and Vilsack for poor economic conditions, but seemingly more likely to
   do so with Governor Culver?},
