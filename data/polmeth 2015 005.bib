Type = {Article},
Language = {English},
Affiliation = {Zanotti, L (Corresponding Author), Virginia Tech, Dept Polit Sci, 509 Major Williams Hall 0130, Blacksburg, VA 24060 USA.
   Zanotti, Laura, Virginia Tech, Polit Sci, Blacksburg, VA USA.},
DOI = {10.1177/1755088214555044},
ISSN = {1755-0882},
EISSN = {1755-1722},
Keywords = {Epistemology; ethics; ontology; practical knowledge; universalism},
Research-Areas = {Government \& Law},
