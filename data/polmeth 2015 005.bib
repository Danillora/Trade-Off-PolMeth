@article{ WOS:000362230300007,
Author = {Vilalta, Aura Esther},
Title = {The training of experts in mediation and quality standards Peculiarities
   impressed by the electronic environment},
Journal = {IDP-INTERNET LAW AND POLITICS},
Year = {2015},
Number = {20},
Month = {JUN},
Abstract = {The training of experts in mediation is a key issue to secure excellence
   in a professional field that implies the first access to justice in
   today's societies. The European Union has opted to leave Member States
   to determine the training requirements and, in this context, Spain has
   recently legislated to provide mediators with a legal framework that is
   uniform and enables all mediators to practice in civil, commercial and
   bankruptcy issues, throughout the national territory regardless of the
   existing regional legislation at the place of practice. In any case some
   key issues for the training and expertise of these experts may be
   identified today in an increasingly complex and diverse society that
   requires to become familiar with a number of different economic and
   socio-cultural contexts, to be aware of the legal framework in which the
   conflict may be settled, and to feel comfortable into the electronic
   environment.},
