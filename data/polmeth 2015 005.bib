@article{ WOS:000371119400003,
Author = {Kriner, Douglas L. and Reeves, Andrew},
Title = {Presidential Particularism in Disaster Declarations and Military Base
   Closures},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2015},
Volume = {45},
Number = {4},
Pages = {679-702},
Month = {DEC},
Abstract = {In The Particularistic President we challenge the dominant paradigm
   asserting presidential universalism. Instead, we argue that presidents
   possess strong electoral and partisan incentives to pursue policies that
   prioritize the needs of some Americans over those of others. In this
   article, we review our larger argument and expand upon our analyses of
   two policy areas where presidents have unilateral authority over
   distributive outcomes: natural disaster declarations and military base
   closures. In the former, we find strong evidence that President Barack
   Obama favored both swing and core states in the 2012 election year. In
   the latter, we find evidence of both electoral and partisan
   particularism.},
