@article{ WOS:000360455300007,
Author = {Fazal, Tanisha M. and Greene, Brooke C.},
Title = {A Particular Difference: European Identity and Civilian Targeting},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2015},
Volume = {45},
Number = {4},
Pages = {829-851},
Month = {OCT},
Abstract = {Recent scholarship has found identity variables to be insignificant
   predictors of civilian targeting in war. Drawing on the European origins
   of the law of war, this article argues that previous scholarship has
   neglected the one specification of identity' that is most theoretically
   justified for understanding civilian targeting: whether a European state
   is fighting a non-European state. This article replicates and extends
   three recent statistical analyses - Downes; Valentino, Huth and Croco;
   and Morrow - of civilian targeting by including a variable capturing
   whether a European state fought a non-European state. The study finds
   that civilian targeting, and non compliance with the law of war more
   generally, is significantly more likely in European v. non-European
   dyads than in other types of dyads.},
