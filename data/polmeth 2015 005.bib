@article{ WOS:000379976700008,
Author = {Sheridan, Paul and Shankardass, Ketan},
Title = {The 2012 Cuts to Refugee Health Coverage in Canada: The Anatomy of a
   Social Policy Failure},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2015},
Volume = {48},
Number = {4},
Pages = {905-931},
Month = {DEC},
Abstract = {In 2012, Canada's federal government announced cuts to refugee health
   coverage. Evidence suggesting that the cuts represent a social policy
   failure has since been accumulating, including the 2014 Federal Court
   ruling ordering their reversal. This explanatory case study uncovers the
   problem definition process that led policy development by applying
   coding methods to governmental publications, transcripts of
