@article{ WOS:000388602400004,
Author = {Schoettmer, Patrick},
Title = {Mobilization and the Masjid: Muslim political engagement in post-9/11
   America},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2015},
Volume = {3},
Number = {2},
Pages = {255-273},
Abstract = {As the number of non-Christian religious adherents increases in the USA,
   we have an opportunity to examine the limits of the linkages between
   religion, race/ethnicity, and civic engagement. American Muslims found
   themselves a central focus of social and governmental attention
   following the attacks of September 11, 2001. Furthermore, Muslims have a
   tradition of non-participation in the politics of non-Islamic states and
   contain within the religious tradition tremendous ethnic diversity.
   These factors have the potential to work against the generally robust
   linkages between religious and political participation. Utilizing a pair
   of surveys conducted by Zogby International in 2001 and 2004, this
   article presents evidence that Muslims were motivated to greater
   political participation as they began to perceive a greater threat from
   both the federal government and their local environments across ethnic
   lines. However, this article finds that private religious devotionalism
   is playing a negative role in Muslim political mobilization, which runs
   contrary to what is generally found in studies of Americans from the
   Judeo-Christian traditions.},
