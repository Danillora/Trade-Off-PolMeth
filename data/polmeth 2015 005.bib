@article{ WOS:000409057800009,
Author = {Tarditi, Valeria},
Title = {TIMES OF CRISIS: SUCCESS OR DECLINE OF THE LEFT?},
Journal = {PARTECIPAZIONE E CONFLITTO},
Year = {2015},
Volume = {8},
Number = {1, SI},
Pages = {215-240},
Abstract = {The crisis that especially since the 2008 has shown the deep weakness of
   the capitalistic economic model induces to reflect on the role of the
   radical left parties and on their capacity to propose alternative
   policies, interpreting the citizens' demands. Starting from these
