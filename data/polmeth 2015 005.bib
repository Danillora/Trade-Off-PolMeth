@article{ WOS:000219470200002,
Author = {Mulinari, Diana},
Title = {Human Rights in Argentina: Between Family Memories and Political
   Identities},
Journal = {JOURNAL OF CIVIL SOCIETY},
Year = {2015},
Volume = {11},
Number = {2},
Pages = {123-136},
Abstract = {Scholarship has identified the centrality of civil society organizations
   in the cultural/political debate over what should be remembered and why.
   The article analyses the narratives of 25 human rights activists from
   Argentina organized in response to the 1976-1983 military regime.
   Inspired by gender and postcolonial scholarship, the article explores
   how notions of family are named and dilemmas regarding who and what to
   be remembered are understood in the struggle for `Memory, Truth, and
   Justice'.},
