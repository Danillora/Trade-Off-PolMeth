@article{ WOS:000372045800011,
Author = {Opitz, Petra},
Title = {Need for Renewal The Energy Industry in Armenia, Azerbaijan, and Georgia},
Journal = {OSTEUROPA},
Year = {2015},
Volume = {65},
Number = {7-10},
Pages = {191+},
Abstract = {Armenia, Azerbaijan, and Georgia are expecting a growing demand for
   energy in the next few years. Azerbaijan can take care of itself given
   its own oil and gas reserves. Armenia is dependent on fuel imports from
   Russia and is planning to expand nuclear power. Georgia's energy mix
   varies seasonally due to its large share of hydroelectric power. In
   exchange for the transit of gas and oil, Georgia is remunerated with
   natural gas which it then uses for the production of inexpensive
   electricity. With the exception of hydropower, alternative sources of
   energy play little role in these three countries. There is hardly any
   investment in improving energy efficiency.},
