@article{ WOS:000364755000004,
Author = {Austin, Ian Patrick},
Title = {Becoming a Wealth Management Centre and International Relations
   Implications: The Singapore Policy Approach and Global and Regional
   Responses},
Journal = {JAPANESE JOURNAL OF POLITICAL SCIENCE},
