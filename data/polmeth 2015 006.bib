@incollection{ WOS:000387939500002,
Author = {Gritter, Matthew},
Book-Author = {Gritter, M},
Title = {The Personal Responsibility and Work Opportunity Reconciliation Act of
   1996 and the Saving of Food Stamps 1995-2000},
Booktitle = {POLICY AND POLITICS OF FOOD STAMPS AND SNAP},
Series = {Palgrave Pivot},
Year = {2015},
Pages = {23-36},
Abstract = {During 1995 and 1996, the Food Stamp Program seemed likely to be
   reformed and reduced in a way similar to the conversion of welfare to a
   block grant program. However, it received only a few modest reforms and
   became a program with an increasingly positive image of assisting the
   working poor. This change was facilitated by three factors: calls by
   some lawmakers to retain the program as a safety net to prevent
   starvation and respond to downturns and disasters; the place of Food
   Stamps in the Farm Bill; and the way the program was re ramed as
   providing incentives to the working poor rather than keeping an
   undeserving population under the dependency of the government.},
