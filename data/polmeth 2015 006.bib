@incollection{ WOS:000354352000004,
Author = {Cox, Gary W.},
Editor = {Levi, M and Rosenblum, N},
Title = {Electoral Rules, Mobilization, and Turnout},
Booktitle = {ANNUAL REVIEW OF POLITICAL SCIENCE, VOL 18},
Series = {Annual Review of Political Science},
Year = {2015},
Volume = {18},
Pages = {49-68},
Abstract = {This essay reviews models of strategic mobilization and turnout,
   focusing on two important questions about the effects of electoral
   rules. First, how does the disproportionality of the electoral system
   affect the variance and mean of mobilization and turnout? This question
   has been investigated at least since Gosnell (1930). In addition to
   reviewing the literature, I argue that extant models should pay more
   explicit attention to secondary mobilization (conducted by interest
   groups, activists, and ordinary voters). Second, how do electoral rules
   regulating the electoral calendar and vote fusion affect mobilizational
   spillovers and, hence, incentives to build mobilizational alliances?
   This question has attracted less attention from modelers but is well
   represented in the empirical literature.},
