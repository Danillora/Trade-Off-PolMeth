@article{ WOS:000442737500010,
Author = {Diesen, G.},
Title = {COERCING ``EUROPEAN INTEGRATION{''}? ASSESSING THE OFFENSIVE POSTURE OF
   THE EU CSDP},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2015},
Number = {6},
Pages = {81-102},
Abstract = {While much academic attention has been devoted to the conceptual
   comparison between peacekeeping and imperialism, the CSDP is rarely
   assessed through this prism. The EU's ``Europeanisation of conflict
   resolution{''} suggests that the political settlement in European
   conflicts is linked to EU integration. This is envisioned as a
   positive-sum tool to encourage compromise between the conflicting sides
   and to provide another tier of governance to mitigate tensions. However,
   the Europeanisation of conflict resolution also denotes that EU's
   conditionality for integration becomes criteria for conflict resolution.
   Thus, Brussels becomes an impediment to conflict resolution by adding
   criteria and rejecting compromise if it conflicts with the EU's
   objectives. In the EU's eastern neighbourhood, the interests of Brussels
   become even more pertinent since the exclusive conception of
   ``Europe{''} results in ``European integration{''} becoming a zero-sum
   geopolitical project versus Russia.},
Publisher = {NONCOMMERCIAL PARTNERSHIP EDITORIAL BOARD POLIS-POLITICAL STUDIES},
