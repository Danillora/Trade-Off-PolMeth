@article{ WOS:000367538000009,
Author = {del Rio Almagro, Alfonso and Cordero Rodriguez, Oihana},
Title = {Art, bodies and public toilets Artistic strategies that question
   architectural devices of sex-gender segregation},
Journal = {POLITICA Y SOCIEDAD},
Year = {2015},
Volume = {52},
Number = {2},
Pages = {465-486},
Abstract = {The HUM-425 Research Group develops a research line which main aim is to
   analyze the artistic strategies that are used to analyze and modify the
   spatial discourse and the architectural devices, inasmuch as they become
   control technologies that aim to regulate our lives according to the
   established rules. Through this text we present the results of a
   research which is based on artistic proposals that question since the
   1990s the discourses related to the public toilets as technological
   prostheses that support the sex-gender segregation by means of concealed
   representation and behavior strategies, which provoke the exclusion of
   non standardised identities. The analysed artistic proposals provide new
   approaches to spatial discourse that can be enriched with the analysis
   of other architectural devices and might be transfered to other fields
   of knowledge.},
