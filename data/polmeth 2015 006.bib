@article{ WOS:000366204100007,
Author = {Doyle, II, Thomas E.},
Title = {When liberal peoples turn into outlaw states: John Rawls' Law of
   Peoples and liberal nuclearism},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
