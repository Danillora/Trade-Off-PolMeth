DOI = {10.17976/jpps/2015.06.10},
ISSN = {1026-9487},
EISSN = {1684-0070},
Keywords = {EU; CSDP; UN; Russia; Bosnia; Moldova; Georgia},
Keywords-Plus = {UNION},
Research-Areas = {Government \& Law},
