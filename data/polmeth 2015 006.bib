@article{ WOS:000359501200004,
Author = {Wigura, Karolina},
Title = {The Burnt-out Generation The 2015 Polish Presidential Election},
Journal = {OSTEUROPA},
Year = {2015},
Volume = {65},
Number = {4},
Pages = {47+},
Month = {APR},
Abstract = {The Polish presidential elections in May 2015 resulted in a victory for
   the previously unknown Andrzej Duda from the conservative party PiS. The
   surprising defeat of incumbent Bronislaw Komorowski revealed that the
   ``Round Table{''} generation is discredited. Poland may be considered a
   politically stable and economically prosperous model of success, but
   within society disappointment and anger have accumulated. This is the
   reason for the low electoral turnout and the surprising success of
   anti-system candidate Pawel Kukiz in the first round.},
