@article{ WOS:000383906900009,
Author = {Nastav, Eva and Bucar, Maja},
Title = {THE SECURITY-DEVELOPMENT NEXUS: SLOVENIA'S ENGAGEMENT IN AFGHANISTAN},
Journal = {ANALELE UNIVERSITATII BUCURESTI-STIINTE POLITICE},
Year = {2015},
Volume = {17},
Number = {2, SI},
Pages = {129-148},
Abstract = {The paper addresses the development-security nexus through an analysis
   of the different interactions of the Slovenian government, which shaped
   its policy towards Afghanistan through parallel activities in
   peacekeeping and development cooperation. With its limited experiences
   in the field of military assistance / peacekeeping, as well as in
   development cooperation, Slovenia followed other NATO countries and
   engaged in a peacekeeping mission in Afghanistan in 2004. Few years
   later, a stronger Slovenian involvement in development cooperation in
   this fragile state evolved, following again the policies of other more
   experienced countries, this time EU member states. The government
   engaged NGOs to lead the development cooperation efforts. However, the
   evidence from the field shows no cooperation between Slovenian
   peacekeeping teams and the NGOs working in the country. The paper
   explores the reasons for such decisions and the consequences it brings,
   assessing the appropriateness of the development-security nexus.},
