@article{ WOS:000362601200005,
Author = {Coleman, Eric A. and Mwangi, Esther},
Title = {Conflict, Cooperation, and Institutional Change on the Commons},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
Year = {2015},
Volume = {59},
Number = {4},
Pages = {855-865},
Month = {OCT},
Abstract = {Rational choice scholarship posits two principal theories to explain why
   political institutions emerge and change: cooperation theory and
   conflict theory. We evaluate cooperation theory and conflict theory to
   explain the emergence of common property institutions among a group of
   Maasai pastoralists in southwestern Kenya. Our empirical results show
   that the change to common property is best explained by conflict theory.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
