@article{ WOS:000374872100006,
Author = {Belanger, Eric and Pedersen, Eva Falk},
Title = {The 2014 Provincial Election in Quebec},
Journal = {CANADIAN POLITICAL SCIENCE REVIEW},
Year = {2015},
Volume = {9},
Number = {2},
Pages = {112-120},
Abstract = {After having been in power for just a little over a year and a half, the
   Parti Quebecois government led by Pauline Marois decided to call an
   early election in the hope of getting the majority of seats that had
   evaded them the last time around. That bet was lost as the 2014 election
   campaign resulted in the worst showing for the PQ since 1973. The
   interelection period was dominated by the debate over the PQ's proposed
   Charter of Quebec Values. The Charter also influenced the debate during
   the campaign, as did the issue of Quebec's sovereignty due in part to
   the arrival of Pierre Karl Peladeau as a star candidate for the PQ. The
   Liberal Party of Quebec is now back in government with a majority of
   seats and under the leadership of Philippe Couillard.},
