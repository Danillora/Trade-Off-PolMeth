@article{ WOS:000362230300008,
Author = {Rodean, Neliana},
Title = {Protection of Individual Rights in the time of Internet - a Delicate
   Balance into Centralized Judicial Review Systems},
Journal = {IDP-INTERNET LAW AND POLITICS},
Year = {2015},
Number = {20},
Month = {JUN},
Abstract = {In the framework of a centralized judicial review system, the Italian
   Constitutional Court and the French Constitutional Council fulfill a
   crucial role and embody veritable milestones in the protection of
   fundamental rights and the implementation of the rule of law. This much
   is uncontested and, in the light of the evolution of constitutional
   adjudication in both countries, is not contestable.
   Such a premise could easily suggest that cyber law has become a crucial
   field for Italian and French constitutional case law, simultaneously
   with the growth of the Internet's influence over almost every area of
   law. Both the Italian Constitutional Court and the French Constitutional
   Council ( the latter at least since 2010) are endowed with competences
   that allow them - at least in theory - to perform an efficient
   protection of individual rights relevant to the Internet framework.
   Despite the few judgments concerning the Internet, this paper aim to
   analyze constitutional case law in order to confirm this hypothesis or
   to refute it.},
