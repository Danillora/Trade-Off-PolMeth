@article{ WOS:000371114800002,
Author = {Greene, Benjamin P.},
Title = {``Captive of a Scientific-Technological Elite{''}: Eisenhower and the
   Nuclear Test Ban},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2015},
Volume = {45},
Number = {1},
Pages = {29-45},
Month = {MAR},
Abstract = {This analysis of Dwight D. Eisenhower's pursuit of a nuclear test- ban
   agreement assesses the role of science advice in his decision making. It
   argues that a ``scientific-technological elite{''} within the
   administration monopolized the technical conclusions in policy
   questions, inhibiting the president's inclination to pursue an accord
   with the Soviet Union for the first half of his presidency. This episode
   has important implications for understanding Eisenhower's record on
   nuclear arms control and assessments of his presidential leadership.},
