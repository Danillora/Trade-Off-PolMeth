@article{ WOS:000212913500008,
Author = {Breen, Keith},
Title = {Freedom, republicanism, and workplace democracy},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2015},
Volume = {18},
Number = {4},
Pages = {470-485},
Abstract = {This article explores the republican case for worker voice in economic
   enterprises based on the ideal of freedom as non-domination and assesses
   its merits relative to two influential arguments for workplace
   democratization grounded on freedom understood as autonomy and
   self-determination. Two claims are advanced. The first is that the
   republican case for worker voice avoids difficulties associated with
   these two arguments. The second, however, is that the ideal of
   non-domination is insufficient and that an adequate understanding and
   defense of workplace democracy will also have to make significant
   reference to freedom understood as autonomy.},
