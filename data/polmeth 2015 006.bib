@article{ WOS:000211456100003,
Author = {Ahrens, Achim},
Title = {Civil Conflicts, Economic Shocks and Night-time Lights},
Journal = {PEACE ECONOMICS PEACE SCIENCE AND PUBLIC POLICY},
