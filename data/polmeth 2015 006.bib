@article{ WOS:000433515000003,
Author = {Rodriguez Sanchez, Nathaly},
Title = {``Mas poeta que monja, mas monja que mujer{''}. More Poet than Woman:
   the Critique of Patriarchal Culture in Maria Luisa Bemberg's Feminist
   Filmography},
Journal = {CIENCIA POLITICA},
Year = {2015},
Volume = {10},
Number = {20},
Pages = {49-73},
Month = {JUL-DEC},
Abstract = {This piece presents an introduction to the feminist outlook contained in
   the work of Argentine film director Maria Luisa Bemberg. To this end, it
   explores the context in which second wave feminism arrived in Argentina,
   and Bemberg's close association with it. The text focuses on the
   director's interest in fleshing out the making of female otherness in
   Latin America - in conditions of subordination, weakness and dependency.
   Bemberg's criticism is delivered along a call for equality of all things
   male and female, and an aim to activate a process of self-awareness in
   women, so they would recognize the role and station that had been
   assigned to them, and which they reproduced.},
