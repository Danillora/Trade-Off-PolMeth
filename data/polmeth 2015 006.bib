@article{ WOS:000370951500003,
Author = {Brunkhorst, Hauke},
Title = {The Crisis of the Legislative Power in Europe},
Journal = {POLITICA \& SOCIETA},
Year = {2015},
Volume = {4},
Number = {1},
Pages = {27-42},
Month = {JAN-APR},
Abstract = {This article illustrates how the Kantian constitutional mindset has been
   removed from the European political scenario by virtue of an alliance
   between the legal technique and the interests of powerful players. It
   explains how ordoliberalism paved the way for neoliberalism. At the same
   time, it analyses the strenuous persistence of the Kantian mindset and
   how it is being translated into a public legislation without public
   life. The article goes on to say that a viable way out of this
   predicament is the renewal of transnational democratic class struggle
   that has so far been hampered by three decades of neoliberal
   transformations.},
