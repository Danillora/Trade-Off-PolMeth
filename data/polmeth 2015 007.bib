@article{ WOS:000366611600002,
Author = {Delcour, Laure},
Title = {Between the Eastern Partnership and Eurasian Integration: Explaining
   Post-Soviet Countries' Engagement in (Competing) Region-Building
   Projects},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2015},
Volume = {62},
Number = {6, SI},
Pages = {316-327},
Month = {NOV-DEC},
Abstract = {This article enriches the understanding of region-building processes in
   the post-Soviet space by explaining why countries engage into regional
   projects. Based upon the analysis of the drivers behind Armenia's and
   Moldova's commitment to Eurasian integration and association with the
   European Union, respectively, it develops a more nuanced approach to
   region-building. The article points to the domestic factors behind
   engagement in regional projects and shows that the range of variables
   explaining countries' choices is actually much broader (and their
   interaction more complex) than suggested by existing theories.},
