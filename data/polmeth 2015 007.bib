@article{ WOS:000372045800031,
Author = {Thun-Hohenstein, Franziska},
Title = {Russia's Georgian Myth The Sovietization of the Romantic Utopia},
Journal = {OSTEUROPA},
Year = {2015},
Volume = {65},
Number = {7-10},
Pages = {549+},
Abstract = {In 19th-century Russian literature, Georgia was romanticised. Poets and
   writers of the Soviet period did not break with this tradition. This is
   seen in the works of Sergei Tretiakov, Konstantin Paustovskii, or Andrei
   Bitov. In poetics and imagery, they drew on the romantic Georgian myth.
   Despite the ideological transformations, their texts fed off the
   traditional arsenal of metaphors, themes, and symbols, which is why
   Russian imperial gestures and interpretations of Georgia remained
   recognisable. On the linguistic level, basic elements of Soviet ideology
   often collide with traditional romantic imagery and allow continuities
   or breaks to emerge.},
