@article{ WOS:000368311700004,
Author = {Heinz, Dominic},
Title = {Joint-Decision-Making in School Policy: Coordination in Motion},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2015},
Volume = {56},
Number = {4},
Pages = {626-647},
Abstract = {For a long time, ``ideological battles{''} and the joint decision trap
   characterized the coordination of school policy. Nevertheless
   international school benchmarks managed to become established as a new
   mode of coordination in school policy. However, rather than the
   `PISA-shock' of 2001, it was the Konstanz Decision of 1997 by the KMK
   that set the precondition for this change. Only after the loosened
   coupling of the arenas of party competition and joint-decision-making
   could the coordination between Bund and Lander as well as among the
   Lander themselves operate in a different context. Under the condition of
   loosely coupled arenas, well-known instruments of the concept of joint
   decision-making motivated the Konstanz Decision of the KMK.},
