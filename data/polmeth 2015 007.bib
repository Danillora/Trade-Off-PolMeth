@article{ WOS:000212758100006,
Author = {Busby, Robert and Cronshaw, Sue},
Title = {Political Branding: The Tea Party and Its Use of Participation Branding},
Journal = {JOURNAL OF POLITICAL MARKETING},
Year = {2015},
Volume = {14},
Number = {1-2, SI},
Pages = {96-110},
Abstract = {The emergence of the Tea Party movement in 2009 witnessed the surfacing
   of a populist, anti-Obama libertarian mobilization within the United
   States. The Tea Party, a movement that brought together a number of
   disparate groups-some new, some established-utilized participation
   branding where the consumer attributed the movement its own identity and
   brand. Its consumer-facing approach, lack of one single leader, and lack
   of a detailed party platform, in combination with its impact on the 2010
   election races in America, earmarks it as a contemporary and
   unconventional brand phenomenon worthy of investigation.},
