@article{ WOS:000351718800001,
Author = {Burbank, Jane},
Title = {Eurasian Sovereignty The Case of Kazan},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2015},
Volume = {62},
Number = {1},
Pages = {1-25},
Abstract = {From 1917 through the present, sovereignty has been repeatedly recovered
   and reconfigured in Kazan and its hinterlands as the area was
   transferred from one complex polity to another. It is the frequent
   renegotiation of authority over multiple and redefinable units of
   political and economic control, rather than stability of institutions,
   that keeps the political class engaged in the reproduction of both the
   state and the Eurasian sovereignty regime.},
