@article{ WOS:000364706500009,
Author = {Kukucha, Christopher J.},
Title = {Internal Trade Agreements in Canada: Progress, Complexity and Challenges},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2015},
Volume = {48},
Number = {1},
Pages = {195-218},
Month = {MAR},
Abstract = {Despite criticism of ongoing protectionism, this study argues that
   Canada's internal trade regime has evolved significantly since the
   implementation of the original Agreement on Internal Trade (AIT). This
   includes thirteen formal amendments to the AIT and numerous regional
   agreements in Western, Central, and Atlantic Canada. Borrowing from the
   liberal International Political Economy (IPE) literature it argues that
   existing regional agreements complement and offer a potential framework
   for broader internal trade reforms at the national level. Of these the
   New West Partnership Trade Agreement (NWPTA) offers a promising model
   for Canada's internal market, with revised rules and norms related to
   negative lists, labour mobility, procurement, and investment and dispute
   settlement. Ongoing efforts to remove barriers will also be linked to a
   strengthened Internal Trade Secretariat, confidence-building measures,
   increased transparency and further reform of technical language.},
