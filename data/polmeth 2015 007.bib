@article{ WOS:000449429200001,
Author = {Bou-Habib, Paul},
Title = {Locke's Tracts and the anarchy of the religious conscience},
Journal = {EUROPEAN JOURNAL OF POLITICAL THEORY},
Year = {2015},
Volume = {14},
Number = {1},
Pages = {3-18},
Month = {JAN},
Abstract = {This article reconstructs the main arguments in John Locke's first
   political writings, the highly rhetorical, and often obscure, Two Tracts
   on Government (1660-1662). The Tracts support the government's right to
   impose religious ceremonies on its people, an astonishing fact given
   Locke's famous defense of toleration in his later works. The
   reconstruction of the Tracts developed here allows us to see that rather
   than a pessimistic view of the prospects for peace under religious
   diversity, what mainly animates the young Locke is a desire to defend
   the rule of law against an anarchical conception of religious freedom.
   The article also argues that the evolution of Locke's thinking on
   religious freedom was in large part governed by Locke's attempt to
   interpret religious freedom in a way that avoids its having anarchical
   implications.},
