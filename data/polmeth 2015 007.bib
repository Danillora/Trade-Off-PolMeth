@incollection{ WOS:000377044800002,
Author = {Gardiner, Michael},
Book-Author = {Gardiner, M},
Title = {Empty Time},
Booktitle = {TIME AND ACTION IN THE SCOTTISH INDEPENDENCE REFERENDUM},
Year = {2015},
Pages = {8-15},
Abstract = {The British constitution's refusal to be limited by any specific
   determinable history must itself be seen historically: rising during the
   English Enlightenment, British franchise is described naturally and
   permanently in financial terms, specifically as incorporation into
   stable exchange - rather than as having any basis in its people. In a
   way that becomes more familiar with neoliberalism, British citizenship
   is based only in adjustment to the permanent good of `the economy'. We
   know that the British constitution must avoid any historical
   codification to be absolutely and automatically inclusive, and that
   since it has to maintain these terms of exchange, it must become ever
   more personal' and ever better at describing itself as neutral and
   natural - which means, beyond determinable time. This chapter suggests
   that self'-determination is simply the end of this empty financial time,
   and that the signs are readable outside Scotland as well as within.},
