DOI = {10.1017/S0007123413000227},
ISSN = {0007-1234},
EISSN = {1469-2112},
Keywords-Plus = {WAR CHESTS; CURVILINEAR DISPARITY; POLITICAL-PARTIES; ELECTORAL SYSTEMS;
