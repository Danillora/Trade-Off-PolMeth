@article{ WOS:000363872800003,
Author = {Bader, Julia},
Title = {Propping up dictators? Economic cooperation from China and its impact on
   authoritarian persistence in party and non-party regimes},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
Year = {2015},
Volume = {54},
Number = {4},
Pages = {655-672},
Month = {NOV},
Abstract = {This article investigates how China's economic cooperation affects
   authoritarian persistence elsewhere. For the period 1998-2008, the
   article assesses quantitatively whether the effects of economic
   cooperation from China vary, conditioned by the regime type of the
   recipient. The analysis finds that China's economic cooperation is
   associated with regime durability in party-based regimes. In non-party
   regimes, in contrast, it is associated with regime collapse.},
