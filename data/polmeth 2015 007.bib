@article{ WOS:000357805800003,
Author = {Keaney, Michael},
Title = {Globalisation, Hegemony and Perspective},
Journal = {POLITICAL STUDIES REVIEW},
Year = {2015},
Volume = {13},
Number = {3},
Pages = {339-350},
Month = {AUG},
Abstract = {Globalisation has been treated as a homogenising, even Americanising,
   process. Its complex nature means that its effects can be observed at
   various levels of analysis: economic, political, social, cultural and
   legal. The books under review here tackle different aspects of
   globalisation, with the overarching theme of US hegemony and how that
   has been employed in the reconfiguration of the global order. Two titles
   deal specifically with British decline, albeit without a perspective
   informed by political economy, the absence of which is argued to weaken
   their respective analyses. The analytical purchase provided by Marxist
   political economy is also highlighted.},
