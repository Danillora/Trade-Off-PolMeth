@article{ WOS:000346237200004,
Author = {Potter, Joshua D. and Tavits, Margit},
Title = {The Impact of Campaign Finance Laws on Party Competition},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2015},
Volume = {45},
Number = {1},
Pages = {73-95},
Month = {JAN},
Abstract = {The comparative literature on party systems has convincingly
   demonstrated that electoral rules, social cleavages and their
   interaction can explain much of the cross-national variation in the size
   of party systems. This literature, however, has thus far ignored
   campaign finance laws. This article argues that various campaign finance
   laws exert more or less restrictive pressures on party competition. It
   develops a new theoretical concept, fund parity. The study demonstrates
   the positive relationship between fund parity and party system size and
   employs additional tests to supplement the regression analysis in order
   to account for potential endogeneity issues. The findings underscore an
   intuitive - but heretofore untested - relationship: increasing parity
   makes party competition more permissive and increases the size of the
   party system.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {32 AVENUE OF THE AMERICAS, NEW YORK, NY 10013-2473 USA},
