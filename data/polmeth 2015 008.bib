@article{ WOS:000354743300008,
Author = {Rummery, Kirstein and McAngus, Craig},
Title = {The Future of Social Policy in Scotland: Will Further Devolved Powers
   Lead to Better Social Policies for Disabled People?},
Journal = {POLITICAL QUARTERLY},
Year = {2015},
Volume = {86},
Number = {2},
Pages = {234-239},
Month = {APR-JUN},
Abstract = {Scotland has laid claim to being different' from the rest of the UK with
   regards to disability policy. This article examines the evidence for
   that with regard to long-term and social care, and discusses the
   possibilities opened up by the devolution of disability benefits. It
