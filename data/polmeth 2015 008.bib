@article{ WOS:000367256200009,
Author = {Richert, Joern},
Title = {COAL'S NOT COOL: ENERGY \& TURKEY'S REPUTATION},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2015},
Volume = {14},
Number = {2},
Pages = {87-96},
Month = {SUM},
Abstract = {At the 2015 global climate summit in Paris this December, the world's
   leaders will not only decide on international climate policy. Fighting
   climate change necessitates a de-carbonization of energy systems.
   Leaders will thus also decide about the world's energy future. In this
   article, the author argues that in the medium and long run, the
   increasing socioeconomic impacts of climate change will intensify the
   pressure for Turkey to adopt a serious climate and sustainable energy
   policy. More importantly, basing economic growth on unsustainable energy
   choices would be a large burden for Turkey's international reputation as
   well as its ambition to become one of the world's 10 largest economies.},
