@article{ WOS:000361605300011,
Author = {Schiera, Giuliana Nobili},
Title = {About the Recent Translation of a Work of Otto Brunner},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
Year = {2015},
Volume = {27},
Number = {52},
Pages = {221-237},
Abstract = {Taking the cue from the translation of Land und Herrschaft by Otto
   Brunner, the essay explores how his work challenges the historian as
   well as the translator. Using ideologized concepts as Staat, Land,
   Herrschaft, Friede, Recht, Brunner gives to these expressions a specific
   value from the temporal point of view. After the transformation of the
   social and political concepts between the XVIII and XIX centuries, which
   made many concepts so temporally determined that they are of little use,
   Brunner redefines them radically and makes intelligible a language which
   is otherwise obscure.},
