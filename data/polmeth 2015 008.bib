@article{ WOS:000373411600010,
Author = {Rennert, Anne},
Title = {``Becoming a Bear{''} in Berlin Rodchenko's Photographs to Mayakovsky's
   Pro eto},
Journal = {OSTEUROPA},
Year = {2015},
Volume = {65},
Number = {11-12},
Pages = {125+},
Month = {NOV-DEC},
Abstract = {Aleksandr Rodchenko's photomontages to Vladimir Mayakovsky's love poem
   Pro eto in 1923 gave expression to the close cultural connection between
   Moscow and Berlin. The avant-garde artist was inspired by George Grosz's
   watercolour collages and used excerpts from illustrated magazines in
   Berlin, as well as a photographic image that anticipates his later
   bird's eye view. Places in Berlin, such as the villa home of silent film
   diva Henny Porten and the Zoological Garden, became the venue of
   Mayakovsky's Russian tale in verse. But Prussian militarism is also
   present among the illustrations.},
