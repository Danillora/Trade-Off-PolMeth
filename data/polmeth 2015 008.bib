@article{ WOS:000373411600008,
Author = {Engerer, Hella},
Title = {Unimportant for the Economy as a Whole East Central Europe and the EU
   Sanctions against Russia},
Journal = {OSTEUROPA},
Year = {2015},
Volume = {65},
Number = {11-12},
Pages = {103+},
Month = {NOV-DEC},
Abstract = {In 2014, the European Union imposed economic sanctions against Russia in
   the defence, energy, and finance sectors. Moscow responded with a ban on
   imports for selected agricultural and food products. Critics of Western
   sanctions spoke of the massive losses that the economies of the EU
   countries have to would suffer. Trade with Russia has in fact decreased
   in recent years. But this has to do with the general weakness of the
   Russian economy and started before 2014. Even the economies of Central
   and Southeastern Europe, which are comparatively more intertwined with
   Russia, have recorded no serious slump since mid-2014.},
