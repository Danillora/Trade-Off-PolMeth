@article{ WOS:000219468500005,
Author = {HalimatusA'Diyah, Iim},
Title = {Zakat and Social Protection: The Relationship Between Socio-religious
   CSOs and the Government in Indonesia},
Journal = {JOURNAL OF CIVIL SOCIETY},
Year = {2015},
Volume = {11},
Number = {1},
Pages = {79-99},
Abstract = {This article analyses the relationship between zakat institutions, as
   socio-religious civil society organizations, and government in dealing
   with poverty in Indonesia. The fact that both share similar concerns
   about poverty and have comparable programmes to address it underlines
   the need for coordination. Departing from the idea that coordination
   prevents duplication and increases effectiveness, this study sets out to
   assess the level of coordination between zakat institutions and
   government and the factors determining that level of coordination. The
   study finds that, due to bureaucracy and lack of internal coordination,
   the existing level of coordination can best be described as low, and
   that this level of coordination has led to suboptimal outcomes. The
   article further analyses the relationship between zakat agencies and
   government and finds it to be guided by low linkages and rules set
   unilaterally by the government. The article ends by providing several
   recommendations for both types of actors to improve their relationship
   and mutual coordination.},
