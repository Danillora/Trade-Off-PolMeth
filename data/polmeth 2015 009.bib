@article{ WOS:000382489400004,
Author = {Speckman, McGlory},
Title = {Student leadership and advocacy for social cohesion: A South African
   perspective},
Journal = {AFRICAN JOURNAL ON CONFLICT RESOLUTION},
Year = {2015},
Volume = {15},
Number = {3, SI},
Pages = {61-84},
Abstract = {This article utilises the insights of sociology and social psychology in
