@article{ WOS:000354661900006,
Author = {Katz, Mark N.},
Title = {Aggression and Response Russia, Ukraine, and the West},
Journal = {OSTEUROPA},
Year = {2015},
Volume = {65},
Number = {1-2},
Pages = {57+},
Month = {JAN-FEB},
Abstract = {Even after the ceasefire negotiated in Minsk, Russia's goals in Ukraine
   remain unclear. Is it just about the territory occupied so far,
   Ukrainian membership in the European Union and NATO, or a comprehensive
   confrontation with the West? Since the West has decided not to help
   Ukraine militarily, Russia holds the better cards in the short term if
   its goals are limited. However, Moscow cannot win a prolonged conflict
   with the West. In addition to economic vulnerability, the global
   political changes that the Kremlin has set in motion are also
   undermining Russia's position. Nevertheless, the United States and the
   EU member states must quickly agree on a common policy of containment in
   order to prevent further territorial expansion into Ukraine.},
