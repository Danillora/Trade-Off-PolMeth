@article{ WOS:000347779100004,
Author = {Teles, Filipe},
Title = {The Distinctiveness of Democratic Political Leadership},
Journal = {POLITICAL STUDIES REVIEW},
Year = {2015},
Volume = {13},
Number = {1},
Pages = {22-36},
Month = {FEB},
Abstract = {One recognises that political leadership still makes a difference.
   However, scholarship seems incapable of offering the proper tools to
   provide further and relevant understanding on the theme, particularly
   when we consider liberal democracy's current trends, namely its
   egalitarian ethos and the personalisation of politics. One of its key
   features, even in strongly controlled democracies, with tight systems of
   checks and balances, is the ascendancy of assertive leaders, to whom
   voters look when deciding on which party to vote for. Therefore, a
   dispassionate assessment of political leadership is needed in order to
   adjust our analysis: from the qualities of particular individuals, their
   intentions, perspectives and preferences, to the way leaders perform
   their role. This article intends to identify what is the specific nature
   and distinctiveness of contemporary political leadership, arguing that
   the nature of the task sets new problems in the political science
   literature and asks for new approaches.},
