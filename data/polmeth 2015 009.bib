@article{ WOS:000363868100006,
Author = {Mishima, Ko},
Title = {Unattainable Mission? The Democratic Party of Japan's Unsuccessful
   Policymaking System Reform},
Journal = {ASIAN POLITICS \& POLICY},
Year = {2015},
Volume = {7},
Number = {3},
Pages = {433-454},
Month = {JUL},
Abstract = {In December 2012, the Democratic Party of Japan (DPJ) fell from power
   after ruling the country for only three years. An important cause of the
   DPJ government's downfall concerned its failure in policymaking system
   reform. When it came to power in September 2009, the DPJ vowed to
   dissolve the policymaking system created under the lengthy dominance of
   the Liberal Democratic Party. It pledged to practice ``Cabinet-led
   policymaking'' by strengthening the Cabinet's leadership fundamentally.
   It also promised to curtail bureaucratic power. But in reality, the
   DPJ's policymaking system reform turned out to be an utter failure. This
   article points out that there are four major reasons for the DPJ
   government's failure in policymaking system reform: (i) lack of internal
   unity, (ii) ineffective top leadership, (iii) institutional depth of
   bureaucratic power, and (iv) the twisted Diet.},
