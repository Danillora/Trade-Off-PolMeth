@article{ WOS:000218125100004,
Author = {Furuta, Takuya},
Title = {From Kent to Japan: the reception history of Robert Filmer as a straw
   man},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2015},
Volume = {20},
Number = {3},
Pages = {284-303},
Abstract = {In his famous A History of Western Philosophy (1945), Bertrand Russell
   suggested that Japan before the Second World War was a true Filmerian
   state. This diagnosis had a real impact on Japanese scholars, including
   Maruyama Masao, who identified Filmerism with pre-war Japan and in turn
   accepted the Lockean social view as a theoretical guideline of post-war
   Japan. The aim of this article is to show the ideological role of
   `Filmerism' in three Japanese `Filmerian controversies.' Before the
   Second World War, both `Filmerians' and anti-Filmerians upheld neither
   Filmernor Locke. What both parties upheld was the idea of Kokutai or
   `Japaneseness,' and they used the label of `Filmerism' to dismiss
   particular political theories. This pre-war relationship amongst
   `Filmerism,' `Lockeanism' and `Japaneseness,' however, underwent a rapid
   transformation after the War. Unlike the pre- war theorists, the
   post-war theorists came to see `Japaneseness' as `Filmerism,' which
   should be denied by Lockeanism. By inquiring into the trajectory of
   Filmer in Japan, we can shed light upon the significant role of a
   cross-cultural straw man in the comparative mode of political theory as
   well as an intellectual and ideological dimension of the pre- and
   post-war Japanese political situations.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
