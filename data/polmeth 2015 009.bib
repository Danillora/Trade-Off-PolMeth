@incollection{ WOS:000354352000012,
Author = {Carmines, Edward G. and D'Amico, Nicholas J.},
Editor = {Levi, M and Rosenblum, N},
Title = {The New Look in Political Ideology Research},
Booktitle = {ANNUAL REVIEW OF POLITICAL SCIENCE, VOL 18},
Series = {Annual Review of Political Science},
Year = {2015},
Volume = {18},
Pages = {205-216},
Abstract = {Over 50 years of research into American political ideology have left
   scholars with a contested paradigm. One side argues that the mass public
   is distinctly nonideological. The other side argues that ideological
   thinking is not beyond the public. The way forward for research in
   political ideology does not lie in rehashing this debate but in
   advancing two new areas of work. The first considers the role that
   values and principles play in determining the political and ideological
   thinking of individuals. The second questions the current conception and
   measurement standards of political ideology. This research argues that
   ideology among the American mass public is formed by positions along two
   related but separate dimensions. In this article, we summarize the major
   arguments of and criticisms of current ideology research. Then we
   discuss recent research on principles and values and the measurement of
   ideology.},
