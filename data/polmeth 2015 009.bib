@incollection{ WOS:000377044800007,
Author = {Gardiner, Michael},
Book-Author = {Gardiner, M},
Title = {The Nuclear Eternal},
Booktitle = {TIME AND ACTION IN THE SCOTTISH INDEPENDENCE REFERENDUM},
Year = {2015},
Pages = {61-71},
Abstract = {This chapter argues that in the total, or historically unlimited, state,
   total weapons have a particular time-fixing function. Where violence is
   always eternal, invisible, and beyond experience, cultures of
   self-determination will in contrast aim to make this total violence
   perceptible and experiential - pointing to a Gothic conflict over the
   empty time of nuclear terror. This question of `total violence' or its
   interruption then becomes central to self-determination, and can be
   understood as a contest over time-binding. Its importance is seen in the
   way, in the wake of an 85\% turnout, nuclear weapons were confirmed as a
   key issue, perhaps the key issue, in relating to Westminster authority.},
