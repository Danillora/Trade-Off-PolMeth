   Qi, Huaigao; Shen, Dingli, Fudan Univ, Inst Int Studies, Shanghai 200433, Peoples R China.},
DOI = {10.1080/13621025.2015.1006580},
ISSN = {1362-1025},
EISSN = {1469-3593},
Keywords = {Chinese citizen; world citizen; citizen awareness; Chinese universities},
Research-Areas = {Government \& Law},
Web-of-Science-Categories  = {Political Science},
