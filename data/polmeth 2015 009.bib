@article{ WOS:000436694700005,
Author = {Shan Wei},
Title = {Who Are More Nationalistic on the Chinese Internet?},
Journal = {EAST ASIAN POLICY},
Year = {2015},
Volume = {7},
Number = {4},
Pages = {39-51},
Month = {OCT-DEC},
Abstract = {A web-based public opinion survey shows that Chinese nationalism is
   associated with conservative ideologies in politics, economy and
   culture. A nationalist in China generally supports an authoritarian
   government, respects national unity and security, endorses state
   influences in socioeconomic issues and values traditional cultures.
   People with higher income are generally less nationalistic. Education is
   negatively related to nationalism. The oldest and youngest cohorts are
   more nationalistic than those in their middle age.},
