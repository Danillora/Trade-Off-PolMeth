@article{ WOS:000358434000006,
Author = {Jansiz, Ahmad and Hojaste, Mohammad Reza},
Title = {Conflicts in the Caucasus Region and Its Effects on Regional Security
   Approach},
Journal = {JOURNAL OF POLITICS AND LAW},
