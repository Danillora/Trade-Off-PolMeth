@article{ WOS:000367276800012,
Author = {Kiss, Aron},
Title = {Identifying strategic voting in two-round elections},
Journal = {ELECTORAL STUDIES},
Year = {2015},
Volume = {40},
Pages = {127-135},
Month = {DEC},
Abstract = {This paper proposes a new method to analyze strategic voting in `wasted
   vote' situations. Two-round elections, in which the second round
   features three candidates, provide an ideal testing ground since the
   first round offers precise baseline measurement of the support for the
   third-place candidate. Estimations are conducted on 403 single-member
   district races from six general elections in Hungary. It is estimated
   that if the first-round vote share difference between the two leading
   candidates becomes smaller by one percentage point, the number of votes
   the third-place candidate receives in the second round diminishes by 1.4
   percent. The effect is estimated to be strongest in 2010, while no
   strategic voting is found in the first democratic election in 1990. (C)
   2015 Elsevier Ltd. All rights reserved.},
