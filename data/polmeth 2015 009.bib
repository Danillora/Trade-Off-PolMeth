@article{ WOS:000446215100001,
Author = {Horemuz, Martin},
Title = {ECONOMIC INTEGRATION OF THE POST-SOVIET SPACE AND SOFT POWER OF RUSSIAN
   FEDERATION},
Journal = {POLITICKE VEDY},
Year = {2015},
Volume = {18},
Number = {2},
Pages = {8-24},
Abstract = {Under the influence of globalisation, current tendencies and trends in
   the area of international relations more intensively head toward the
   integration of relevant areas and sectors of activities within nation
   states. This process, taking place in local, regional and as well as in
   the international (global) dimension has resulted in significant
   political, social, economic as well as cultural transformations in the
   structures of nation states. A robust and constantly increasing pace
   along with the changing dynamics of the processes in economic and social
   fields redraw the political map of the world, the image of which
   reflects a more complicated system of factors of uncertainty and
   unpredictability since the collapse of the bipolar system of
   international relations arrangements. In the above context, the aim of
   the paper is the description of the potential for economic integration
   of the post-Soviet space in the relation to the soft power of Russia.
   For the above mentioned goal the author mainly identifies factors
   limiting or supporting the economic integration process of post-Soviet
   space.},
