@article{ WOS:000210660800011,
Author = {Mckenna, Tony},
Title = {The Suicide Forest: A Marxist Analysis of the High Suicide Rate in Japan},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2015},
Volume = {27},
Number = {2},
Pages = {293-302},
Abstract = {Suicide in Japan is a significant issue. The samurai warrior who commits
   hari-kari (hara-kiri) and the kamikaze pilot have long been icons of
   self-immolation. But even today the suicide rate in Japan is roughly 60
   percent above the global average, and sociologists have long puzzled
   over the enigma. What about Japanese society has made it so prone to
   this phenomenon? Many have referenced forms of Japanese religious and
   cultural beliefs, but this article seeks to register a more profound
   explanation through an account of the trajectory and peculiarities of
   Japanese historical development. The author uses Trotsky's theory of
   uneven and combined development to better elucidate this trajectory
   before combining this analysis with some of the concepts deployed by the
   classical sociologist Emile Durkheim in his seminal work on the subject.},
