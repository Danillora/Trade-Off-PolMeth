@article{ WOS:000372045800030,
Author = {Andronikashvili, Zaal},
Title = {Brothers in Faith or Evil Empire Perceptions of Russia in Georgian
   Literature},
Journal = {OSTEUROPA},
Year = {2015},
Volume = {65},
Number = {7-10},
Pages = {531+},
Abstract = {The conflict-ridden relationship between Georgia and Russia has been
   emotionally charged for centuries. This was manifested not only in
   Russian, but also in Georgian literature. The loss of Georgian
   independence starting in 1801 and the conquest by Bolshevik troops in
   1921 are major turning points. The spectrum of Georgian images of Russia
   range from Christian protector to colonial oppressor. Within this
   spectrum resonates the promise of individual liberty, but also hatred
   for the great collective prison. Georgia's ever evolving perception of
   itself is also always reflected in the literary depictions of its large
   neighbour.},
