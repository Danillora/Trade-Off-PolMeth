@article{ WOS:000352727300003,
Author = {Howard, Jeffrey W.},
Title = {The Instability of Democratic Contractarianism},
Journal = {POLITICAL STUDIES REVIEW},
Year = {2015},
Volume = {13},
Number = {2},
Pages = {184-195},
Month = {MAY},
Abstract = {Democratic contractarianism aspires to unite two previously unconnected
   strands in political philosophy: a democratic commitment to a social
   world in which citizens wield equal power; and a contractarian
   commitment to a strictly prudential justification of political morality.
   It is argued in this article that the commitment to democratic equality
   is theoretically unmotivated and risks limiting the applicability of the
   view to societies in which equal bargaining power already obtains. It is
   also argued that the view cannot account for why a political order
   governed by democratic contractarianism will be stable over time.
   Ultimately, it is suggested that these problems are traceable to a
   fundamental incompatibility between the two dimensions of democratic
   contractarianism. The view can be democratic, and it can be
   contractarian, but it cannot be both.},
Publisher = {WILEY-BLACKWELL},
