@article{ WOS:000212007400006,
Author = {Bryant, Christopher},
Title = {National art and Britain made real: the London 2012 Olympics opening
   ceremony},
Journal = {NATIONAL IDENTITIES},
Year = {2015},
Volume = {17},
Number = {3},
Pages = {333-346},
Abstract = {Danny Boyle's opening ceremony for the London 2012 Olympics presented an
   extraordinary spectacle to the watching world. This article is about
   what it said to people in Britain, how it said it and why. By comparing
   (1) Boyle's version of a new Jerusalem with William Blake's `Jerusalem',
   and (2) the content of the ceremony with the realisation of Britain in
   works of art around Blake's time as analysed in Anthony D. Smith's The
   Nation Made Real, it is possible to see how Boyle reworks old themes and
   where he breaks new ground. There follow discussions of the ceremony's
   reception according to polling evidence, its broad-left political
   message, and the singular character of an event that sparked something
   akin to a moment of `collective effervescence'.},
