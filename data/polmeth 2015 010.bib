@article{ WOS:000383165000010,
Author = {Bellido Caceres, Juan Manuel},
Title = {Popular power: Self-management, self-reliance and autonomy},
Journal = {PENSAMIENTO AL MARGEN},
Year = {2015},
Number = {3},
Abstract = {In the movements that make up the different actions of popular power,
   reflected in most of the occasions, protests against abuses by
   governments and anti-social laws that affect society, cutting rights and
   demanding increasingly the acceptance of precarious jobs, tax hikes,
