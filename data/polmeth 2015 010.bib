@article{ WOS:000422104000003,
Author = {Burgos, Alonso},
Title = {THE IMPLEMENTATION OF PRIOR CONSULTATION IN ECUADOR IN THE FRAMEWORK OF
   XI RONDA PETROLERA},
Journal = {REVISTA ANDINA DE ESTUDIOS POLITICOS},
Year = {2015},
Volume = {5},
Number = {1},
Pages = {47-61},
Abstract = {This article analyzes the prior consultation implementation in Pastaza
   province, Ecuador, one of the places where XI Ronda Petrolera is being
   developed. This territory is inhabited by kichwa, andoas, shuar, achuar,
   zaparas and waorani nationalities, which have different responses to oil
   exploitation, to prior consultation and to offers from Ecuadorian
   government. The political behavior from the actors and its positions for
   and against the prior consultation process can be explained, in many
   cases, through their interests, which are placed above their traditions
   and institutions.},
