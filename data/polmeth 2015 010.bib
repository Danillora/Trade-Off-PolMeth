@article{ WOS:000371114800009,
Author = {Murray, Shoon Kathleen},
Title = {The Contemporary Presidency Stretching the 2001 AUMF: A History
   of Two Presidencies},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2015},
Volume = {45},
Number = {1},
Pages = {175-198},
Month = {MAR},
Abstract = {In a hasty response to the 9/11 attacks, Congress authorized the
   ``war{''} against al-Qaeda in a statute known as the 2001 Authorization
   for the Use of Military Force (AUMF). It was meant to be a temporary
   grant of powers to allow the president to fight the perpetrators of the
   attack. This article analyzes how two very different presidents have
   used the 2001 AUMF to justify controversial policies in ways unforeseen
   by its congressional makers, resulting in an accruement of power to the
   executive branch that could become long term, if not permanent.},
