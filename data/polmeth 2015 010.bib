@article{ WOS:000371485500002,
Author = {Castillo, Rodrigo Morales and Maldonado, Gerardo and Schiavon, Jorge A.},
Title = {To Know or Not To Know? Realist and Liberal Theories on Foreign Affairs
   and Public Opinion in Latin America},
Journal = {LATIN AMERICAN POLICY},
Year = {2015},
Volume = {6},
Number = {1},
Pages = {2-18},
Month = {JUN},
Abstract = {We contrast the central theoretical claims of the realist and liberal
   traditions of international relations concerning the importance of
   public opinion in foreign affairs, providing sufficient empirical
   evidence to uphold the liberal view and refute the realist hypothesis.
   Using data collected over the last decade by The Americas and The World
   Project in Brazil, Colombia, Ecuador, Mexico, and Peru, we show that,
   even if Latin Americans have important limitations in terms of knowledge
   of international affairs, a strong system of political beliefs endows
   them with the conceptual tools to face the complexity of global issues,
   permitting them to hold coherent and consistent preferences on
   international issues.},
