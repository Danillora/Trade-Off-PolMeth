@article{ WOS:000213907100004,
Author = {Boukalas, Christos},
Title = {Class war-on-terror: counterterrorism, accumulation, crisis},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2015},
Volume = {8},
Number = {1, SI},
Pages = {55-71},
Abstract = {This article discusses US counterterrorism from a class perspective. It
   sees counterterrorism as a state policy with differential effects on
   different social classes. In doing so, the article starts to address a
   lacuna in critical studies of counterterrorism, which tend to be rather
   structural and formal, thus ignoring the pertinence of counterterrorism
   to the field of social dynamics. To partly rectify this blind spot by
   addressing some class implications of counterterrorism, the article
   examines the effects of counterterrorism policy on capital accumulation
   and its social conditions. It notes that counterterrorism has different
   implications along class-lines: for dominant capital, it signifies
   appropriation of public money and direct participation in political
   decisions; for everyone else, it means material dispossession and
   political exclusion. Given that counterterrorism was developed between
   two crises of neoliberalism, the article distinguishes between economic
   crises, which tend to benefit capitalism, and political crises, which
   can be destructive, and suggests that counterterrorism is partly a
   restructuring of the neoliberal state so that it can manage recurring
   economic crises, while preventing their evolution into political ones.},
Publisher = {TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OR14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Boukalas, C (Corresponding Author), Cardiff Univ, Cardiff Univ Law Sch, Cardiff, S Glam, Wales.
   Boukalas, Christos, Cardiff Univ, Cardiff Univ Law Sch, Cardiff, S Glam, Wales.},
DOI = {10.1080/17539153.2015.1005932},
ISSN = {1753-9153},
EISSN = {1753-9161},
Keywords = {homeland security economy; counterterrorism; social dynamics; capital
   accumulation; accumulation-as-crisis; political crisis; economic crisis;
   capitalist state; neoliberalism; universal adversary; political
   exclusion},
Research-Areas = {Government \& Law},
Web-of-Science-Categories  = {Political Science},
Author-Email = {BoukalasC@cardiff.ac.uk},
Affiliations = {Cardiff University},
ORCID-Numbers = {boukalas, christos/0000-0001-9329-4906},
Funding-Acknowledgement = {Journal of Law and Society; ESRC},
Funding-Text = {His current research project on the Preemptive Turn in UK Criminal Law
