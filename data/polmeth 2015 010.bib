@article{ WOS:000371486300004,
Author = {Chavez-Segura, Alejandro},
Title = {Can Truth Reconcile a Nation? Truth and Reconciliation Commissions in
   Argentina and Chile: Lessons for Mexico},
Journal = {LATIN AMERICAN POLICY},
Year = {2015},
Volume = {6},
Number = {2},
Pages = {226-239},
Month = {DEC},
Abstract = {The emergence of a moral power of the state-legitimacy through
   accountability and prosecution of perpetrators of human rights
   abuses-provides a new dimension of power in Latin American countries.
   Following a normative international approach, this article presents the
   relevance of truth commissions for the consolidation of a decisive
   democratic turn in Latin American countries, highlighting the relevance
   of having a trustworthy morality of the state through just policies
   toward its citizens. Truth commissions are important tools to restore
   citizens' trust in their authorities and set the basis for
   reconciliation. By engaging in active restorative and transitional
   justice, states can be reconciled, and the conditions for an effective
   peacebuilding within borders can be established as pillars of an
   improved, stronger democratic state, as the cases of truth commissions
   in Argentina and Chile represent. By examining the outcomes of these
   commissions, I argue that a truth commission in Mexico could promote a
   much-needed reconciliation.},
