@incollection{ WOS:000377044800004,
Author = {Gardiner, Michael},
Book-Author = {Gardiner, M},
Title = {The Golden Country},
Booktitle = {TIME AND ACTION IN THE SCOTTISH INDEPENDENCE REFERENDUM},
Year = {2015},
Pages = {25-39},
Abstract = {One of the toughest tasks for self-determination campaigns has been
   overcoming the realist characterisation of empty time as something that
   holds us together, is to be desired, is the `people's'. In the UK this
   has been understood as consensus, a mass promise which permanently
   delays action, and which makes the defence against popular sovereignty
   `communal' and moral. However strongly and dynamically it promotes
   financial sovereignty, a `Golden' image of welfare consensus still has
   an extraordinary hold over the British-left media, as we see in the
   run-up to September 2014. In the terms of state-creation, the creation
   of citizens through access to the universalist time of credit is
   franchise. The imagescape of the Golden Country is what
   self-determination is trying to unlearn.},
Publisher = {PALGRAVE},
Address = {HOUNDMILLS, BASINGSTOKE RG21 6XS, ENGLAND},
