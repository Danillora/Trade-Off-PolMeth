@article{ WOS:000378742600010,
Author = {Munoz Sanchez, Olmer Alveiro and Vieco Maya, Luis Eduardo},
Title = {The American foreign policy for Latin America in the period 2001-2014},
Journal = {ANALECTA POLITICA},
Year = {2015},
Volume = {5},
Number = {8},
Pages = {199-217},
Month = {JAN-JUN},
Abstract = {The paper analyzes the behavior of the United States, particularly its
   status as an international power in charge of maintaining order and
   international security, during the transition between the governments of
   George Bush and Barack Obama. The political tradition states that the
   country should maintain a strong position in order to safeguard
   equilibrium in the international system. However, two confronting
   theories emerge when it comes to resolving a dilemma of international
   character. On one hand, Mearsheimer, offensive realism theorist,
   proposes a context in which states are always in competition due to the
   existence of an unbalanced international system; the liberal trend
   leaded by Ikemberry. On the other hand, notes that the liberal
   international order, proposed by the United States, should not be
   defined as a scheme of radical competition but quite the opposite,
   namely, a space for international cooperation aiming to reduce
   international tension. Is in this latter frame that the United States
   could set a course of action regarding Latin America.},
