@incollection{ WOS:000354352000008,
Author = {Schultz, Kenneth A.},
Editor = {Levi, M and Rosenblum, N},
Title = {Borders, Conflict, and Trade},
Booktitle = {ANNUAL REVIEW OF POLITICAL SCIENCE, VOL 18},
Series = {Annual Review of Political Science},
Year = {2015},
Volume = {18},
Pages = {125-145},
Abstract = {Research on the relationship between territorial disputes, militarized
   conflict, and economic integration occurs at the intersection of two
   large research programs in international relations: one linking
   territorial disputes to violence, and another exploring the effects of
   conflict on trade and vice versa. Although we know that territorial
   disputes fuel conflict and that conflict dampens trade, we know less
   about whether the prospects of economic gains contribute to the
   settlement of disputes and subsequent compliance with those settlements.
   I argue that research in this area could profitably adopt an emerging
   view of borders as institutions that not only distribute territory but
   also allow cooperation and the production of joint gains. This review
   identifies gaps in the existing literature on the resolution of
   territorial disputes and helps to reframe a persistent methodological
   challenge in this area: missing and noisy data on trade flows.},
