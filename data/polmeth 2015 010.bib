@article{ WOS:000366611600005,
Author = {Paenke, Julian},
Title = {The Fallout of the EU's Normative Imperialism in the Eastern
   Neighborhood},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2015},
Volume = {62},
Number = {6, SI},
Pages = {350-363},
Month = {NOV-DEC},
Abstract = {This article argues that the European Neighborhood Policy is
   characterized by normative imperialism. The EU's institutional set-up
   implies a specific logic of external behavior which differs
   significantly from conventional state foreign policy, as empires need to
   provide their large polity' with a sense of community. The argument
   offers an explanation to the question why the EU needs to adopt a
   normative agenda in external relations. The article continues to briefly
   assess three EU neighborhoods. In Eastern Europe, the EU's approach had
   to fail, being based on deficient assumptions about political realities
   in the EaP states and underestimating the antagonistic relations with
   Russia.},
