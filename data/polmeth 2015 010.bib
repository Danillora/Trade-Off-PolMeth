@article{ WOS:000219470200004,
Author = {Rathzel, Nora and Uzzell, David and Lundstrom, Ragnar and Leandro,
   Beatriz},
Title = {The Space of Civil Society and the Practices of Resistance and
   Subordination},
Journal = {JOURNAL OF CIVIL SOCIETY},
Year = {2015},
Volume = {11},
Number = {2},
Pages = {154-169},
Abstract = {We argue that the majority of civil society conceptualizations employ a
   narrow concept of the state and a narrow concept of civil society. The
   life history of a Brazilian woman demonstrates that as individuals
   travel through state institutions and civil society organizations
   (CSOs), they carry conflicting worldviews with them which bear on the
   practices of CSOs. With Gramsci we recognize civil society as a space
   where movements and the state struggle for hegemony; beyond him we
   conceptualize CSOs as contradictory, being simultaneously of and against
   the state, while the state is simultaneously outside and within them.},
