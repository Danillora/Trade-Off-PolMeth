@article{ WOS:000365554000005,
Author = {Grande, Edgar and Kriesi, Hanspeter},
Title = {The Euro Crisis: A Quantum Leap in the Politicization of the European
   Integration Process?},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2015},
Volume = {56},
Number = {3},
Pages = {479-505},
Abstract = {This article examines whether the crisis caused a quantum leap in the
   scope and intensity of politicization compared with previous integration
   debates. We compare the public debate on the Eurozone crisis with
   previous debates on European integration in six West European countries
   (Austria, Britain, France, Germany, Sweden, and Switzerland) spanning
   the past forty years. The article concludes that the Eurozone crisis was
   politicized in the countries under scrutiny, but that it did not cause a
   quantum leap in the level and scope of political conflict. Rather, it
   reinforced the dominance of executive elites in public debate.},
