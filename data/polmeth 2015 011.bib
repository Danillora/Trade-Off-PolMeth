@article{ WOS:000215988600003,
Author = {Ayvar Campos, Francisco Javier and Ochoa Lupian, Luz Elena},
Title = {Migration and its influence on the development of the municipality of
   Paracuaro, Michoacan},
Journal = {CIMEXUS},
Year = {2015},
Volume = {10},
Number = {2},
Pages = {35-48},
Month = {JUL-DEC},
Abstract = {The problems of poverty, marginalization, illiteracy, inequality in
   access to health care and unemployment have led to the municipality
   Paracuaro, Michoacan to be classified as high migratory. This migration
   has resulted in a gradual increase in remittances; in 2010 11.3\% of
   households in the municipality received such transfers. The aim of this
   investigation is to determine the impact of migration and remittances on
   the development of Paracuaro, Michoacan. To achieve this objective was
   designed and implemented a survey in the municipality. The analysis
   results showed that remittances significantly impacted on health,
   housing and income of the family, allowing 89.3\% of the households
   interviewed live with incomes above the welfare line.},
