@article{ WOS:000352817500017,
Author = {Gay, Oonagh and Schleiter, Petra and Belu, Valerie},
Title = {The Coalition and the Decline of Majoritarianism in the UK},
Journal = {POLITICAL QUARTERLY},
Year = {2015},
Volume = {86},
Number = {1},
Pages = {118-124},
Month = {JAN-MAR},
Abstract = {The United Kingdom has traditionally featured many aspects of the
   majoritarian model of democracy: its first-past-the-post electoral
   system tends towards producing single-party majorities, while its
   legislative decision rules concentrate policy-making power in the hands
   of the resulting single-party governments. However, in an unprecedented
   break with the UK's postwar conventions, the Conservatives and Liberal
   Democrats formed a coalition following the general election of 2010. In
   this article, we examine some of the Coalition's impacts on governing
   and constitutional conventions, placing them in a comparative European
   context. We conclude that the Coalition reflects a shift towards the
   less majoritarian forms of politics prevalent in continental Europe, and
   that some of these changes are likely to persist even after the end of
   the current government.},
