@article{ WOS:000370952000006,
Author = {Renault, Emmanuel},
Title = {The Phases of the French Reception of Critical Theory},
Journal = {POLITICA \& SOCIETA},
Year = {2015},
Volume = {4},
Number = {3},
Pages = {405-419},
Month = {SEP-DEC},
Abstract = {After having explained why the reception of the critical theory was so
   late in France, this article distinguishes three stages of the
   reception. In the first one (1974-1986), centered on Adorno and
   Horkheimer, the discussion is organized by the possibility of a critical
   Marxism. In the second stage (1986-1997), the main question at issue is
   that of the discontinuity of Habermas' theory with the very idea of a
   critical theory of society. The third stage is still going on (1998-).
   Mainly inspired by Honneth, it gave rise to various research programs
   (about the history of critical theory, about recognition, about
   alienation and social experience).},
