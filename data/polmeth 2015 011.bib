@article{ WOS:000365605100001,
Author = {Conroy, Meredith and Oliver, Sarah and Breckenridge-Jackson, Ian and
   Heldman, Caroline},
Title = {From Ferraro to Palin: sexism in coverage of vice presidential
   candidates in old and new media},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2015},
Volume = {3},
Number = {4},
Pages = {573-591},
Month = {OCT 2},
Abstract = {An analysis of news coverage of vice presidential candidates finds the
   previously observed media biases for women who run for other political
   positions to be present for women vice presidential nominees, and
   especially in the arena of new media, where editorial filters are mostly
   absent. Using content analysis of major print news and online blogs, we
   find sex inequalities in coverage tone, type, and hard sexism (overtly
   gendered insults). Coverage of female vice presidential candidates is
   more negative, more focused on her appearance and familial role, and
   more sexist, than coverage of male vice presidential candidates.
   Furthermore, we find that negative tone and hard sexism are more
   pronounced in the online blogosphere. The implication for women,
   especially for those with presidential ambitions, is that known media
   hostility may be a deterrent, and further stimulates the chronic
   underrepresentation of women in our governing institutions.},
