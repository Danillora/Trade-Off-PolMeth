@article{ WOS:000378600100004,
Author = {Ramirez, Helen and Coulter, Natalie},
Title = {Locked In: Feminist Perspectives on Surviving on Academic Piecework},
Journal = {NEW PROPOSALS-JOURNAL OF MARXISM AND INTERDISCIPLINARY INQUIRY},
Year = {2015},
Volume = {7},
Number = {2},
Pages = {28-36},
Month = {MAR},
Abstract = {While increasing media attention is given to examining the status of
   contract faculty on university campuses there is little note made of the
   pervasiveness of women in these positions. This paper, by drawing on
   Marxist and feminist theory ties the gender precarity faced by academic
   contract female workers to the historical practices of industries to use
   female labour to reduce labour costs. The textile piece worker system of
   the 19th century has found a 21st century form represented in the
   unlikely position of the female academic contract worker. The argument
   builds on the autoethnographic narratives of two contract women to
   demonstrate how the university administration's ``economic pressure{''}
   justification is an economic myth to occlude the exploitation of female
   workers.},
