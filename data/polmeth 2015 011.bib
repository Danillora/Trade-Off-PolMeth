@article{ WOS:000359501200003,
Author = {Libman, Alexander},
Title = {A Fragmented Science The Field of Economics in Russia and Its Political
   Influence},
Journal = {OSTEUROPA},
Year = {2015},
Volume = {65},
Number = {4},
Pages = {29+},
Month = {APR},
Abstract = {The field of economics in Russia is deeply divided. Only a small share
   of academics takes part in international economic debates. Others work
   with internationally established methods and approaches, but limit
   themselves to discussions in Russia. Several national schools oppose the
   current understanding of economics. The great mass of lecturers at
   economic faculties outside Moscow and St. Petersburg has at best an
