@article{ WOS:000217339300004,
Author = {Lux, Julia},
Title = {France in limbo: On the struggles over accumulation strategies in models
   of capitalism - The case of the pacte de responsabilite},
Journal = {FRENCH POLITICS},
Year = {2015},
Volume = {13},
Number = {1},
Pages = {84-102},
Month = {APR},
Abstract = {In January 2014, France's socialist president announced a major turn in
   his crisis exit strategy, the pacte de responsabilite, providing up to
   {[}SIC ]35 billion in tax reductions to companies. By analysing the
   discourses of different actors of the left bloc (trade unions and
   parties), I seek to show how France is currently struggling with the
   accumulation strategy at the base of the French model of capitalism.
   While some actors of the left bloc pursue the continuation or
   strengthening of an accumulation strategy led by internal demand, others
   are accepting of or pursuing the export orientation favoured by
   supranational actors like the EU. Meanwhile, it remains to be seen
   whether the path of capital-friendly austerity that bolsters export
   orientation will be a successful strategy for Hollande and for France's
   political economy.},
