@article{ WOS:000356958300005,
Author = {Gwiazda, Anna},
Title = {Women's representation and gender quotas: the case of the Polish
   parliament},
Journal = {DEMOCRATIZATION},
Year = {2015},
Volume = {22},
Number = {4},
Pages = {679-697},
Month = {JUN 7},
Abstract = {Representation is inherent to democracy and truly representative
   institutions are vital for a good quality democracy. However, the
   argument that parliaments are not sufficiently representative because of
   female under-representation is widespread. A number of countries around
   the world have introduced gender quotas in order to enhance the
   descriptive representation of women. This article analyses women's
   representation and the adoption of gender quotas in Poland. After
   several unsuccessful attempts, the law was finally approved in 2011.
   Veto players analysis is used to explain this policy change.},
