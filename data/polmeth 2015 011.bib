@article{ WOS:000371114800007,
Author = {Cain, Sean A.},
Title = {Polls and Elections Leviathan's Reach? The Impact of Political
   Consultants on the Outcomes of the 2012 Republican Presidential
   Primaries and Caucuses},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2015},
Volume = {45},
Number = {1},
Pages = {132-156},
Month = {MAR},
Abstract = {Recent scholarship into American political parties argues that the power
   to nominate presidential candidates has not entirely devolved from the
   party to voters but retains party influence via a network of allied
   actors including political consultants. Once party elites coalesce
   around a preferred candidate, primary voters tend to comply with their
   will. However, scholarship has not yet shown whether consultants help
   produce this effect. Consultants, even those with close contractual ties
   to the party organization, do not behave as this view expects, and they
   worked for several GOP candidates in 2012, enhancing intraparty
   competition rather than rallying around the front-runner.},
