@article{ WOS:000362052700009,
Author = {Hertzberg, Benjamin R.},
Title = {Daniel, Esther, and the Minority Politics of the Hebrew Bible},
Journal = {POLITY},
Year = {2015},
Volume = {47},
Number = {3},
Pages = {397-416},
Month = {JUL},
Abstract = {Hebraic political theory is in the midst of a renaissance. Contemporary
   scholarship, however; overlooks prominent discussions in Biblical texts
   about the ways in which the Jews, as an exiled minority, ought to
   interact with majority political power. Through an analysis of the books
   of Daniel and Esther; this article argues that the Hebrew Bible advances
   different positions on this question. Daniel enjoins a politics of
   conviction focused on the preservation of the Jews as adherents of God's
   law, while Esther recommends a savvy politics of responsibility that
   will preserve the Jews as a people. I explore the implications of my
   reading of Daniel and Esther for prominent contemporary engagements with
   the Hebraic political tradition and discuss the debate's implications
   for the Hebraic renaissance.},
