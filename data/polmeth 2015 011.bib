@article{ WOS:000357024500002,
Author = {Monogan, III, James E.},
Title = {The 2011 Debt-Ceiling Controversy and the 2012 US House Elections},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2015},
Volume = {48},
Number = {3},
Pages = {420-424},
Month = {JUL},
Abstract = {This article considers how a key legislative vote-that is, the August
   2011 vote to raise the federal debt ceiling-influenced the 2012
   elections for the US House of Representatives. Two outcomes are
   analyzed: (1) the incumbents' ability to retain their seats through the
   2012 general election, and (2) their share of the two-party vote for
   members who faced a general-election competitor. In developing this
   study, the research design was registered and released publicly before
   the votes were counted in 2012. Therefore, this article also illustrates
   how study preregistration can work in practice for political science.
