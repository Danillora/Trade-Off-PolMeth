@incollection{ WOS:000354352000005,
Author = {Horowitz, Michael C.},
Editor = {Levi, M and Rosenblum, N},
Title = {The Rise and Spread of Suicide Bombing},
Booktitle = {ANNUAL REVIEW OF POLITICAL SCIENCE, VOL 18},
Series = {Annual Review of Political Science},
Year = {2015},
Volume = {18},
Pages = {69-84},
Abstract = {This article reviews the existing literature, mostly from political
   science, on suicide bombing. A prominent weapon in the toolkit of
   violent nonstate actors for a generation, suicide bombing generates a
   significantly larger number of casualties per attack than other uses of
   force by terrorist groups, insurgents, and others. Scholars tend to
   agree that no single reason leads individuals to become suicide bombers.
   Moreover, groups use suicide bombing for strategic reasons-although
   whether suicide bombing campaigns make groups more likely to achieve
   their goals is unclear. Scholars continue to disagree about what drives
   groups to adopt or eschew suicide bombing, including the role of
   religion. Although groups that are more religious are significantly more
   likely to adopt suicide bombing, the underlying cause and its
   relationship to particular religious ideologies (such as Salafi Jihadi
   movements) versus the structure of religious groups remain matters of
   contention.},
