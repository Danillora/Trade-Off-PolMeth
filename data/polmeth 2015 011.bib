@article{ WOS:000355959700002,
Author = {Templer, Rachel},
Title = {From Democracy to Empire: Transgression and Substitution in Thucydides'
   Periclean Narrative},
Journal = {POLITY},
Year = {2015},
Volume = {47},
Number = {2},
Pages = {147-174},
Month = {APR},
Abstract = {This article investigates the imperial resonances of a transgressive, or
   revolutionary, conception of democracy. In the narrative that Thucydides
   constructs in his History, Pericles appropriates for an imperial project
   a revolutionary democratic self-understanding from the available
   resources of Athenian civic ideology. His appropriation privileges the
   beginnings of Athens' democratic regime over its ongoing practice.
   Pericles, thereby, obscures both the monologic character of his rule and
   the deep diversity within the democratic institutions of Athens. The
   study closes with an examination of the erotic images of citizenship
   that Pericles employs in his various speeches in the History and with a
   discussion, as a counterpoint, of the practices of lot and rotation that
   he ignores.},
