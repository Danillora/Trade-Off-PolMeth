@article{ WOS:000354661900008,
Author = {Jampol'skij, Michail},
Title = {The Winner, Alas A Brief Psychological Profile of Russia},
Journal = {OSTEUROPA},
Year = {2015},
Volume = {65},
Number = {1-2},
Pages = {73+},
Month = {JAN-FEB},
Abstract = {Russia is suffering from post-imperial trauma. Large parts of society
   see themselves and the state as victims of hostile intrigues. They are
   trying to counteract this sense of national humiliation by means of
   aggressive, heroic self-assertion. The annexation of the Crimea has
