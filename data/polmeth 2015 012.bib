@article{ WOS:000371111500005,
Author = {McKenzie, Mark Jonathan and Rugeley, Cynthia R. and Unger, Michael A.},
Title = {Testing Voter Responses to New Style Judicial Campaign Appeals: What
   Works-Policy Promotion or Experience?},
Journal = {POLITICS \& POLICY},
Year = {2015},
Volume = {43},
Number = {4},
Pages = {562-585},
Month = {AUG},
Abstract = {Historically, judicial campaigns in America centered on candidate
   appeals to experience and expertise in the law. In more recent years,
   state supreme court campaigns have become more issue-based, representing
   a ``new style{''} campaign as termed by public law scholars. This study
   examines how voters respond to judicial candidates policy-based appeals
   versus traditional appeals to experience. Using an experiment embedded
   in the 2010 Cooperative Congressional Elections Study survey, we examine
   how these different messages affect the vote choice of individuals
   toward judicial candidates. Our results show that judicial voters do not
   react negatively to policy promises per se, as some public law scholars
   have feared. Rather, these voters respond favorably to specific issues
   when a judicial candidate promotes a position consistent with the
   voter's opinions. Interestingly, however, we find that some candidates
   benefit more from ``New Style{''} campaign messages than others.},
