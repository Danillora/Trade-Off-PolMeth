@article{ WOS:000367256200008,
Author = {Habip, D. Hakan},
Title = {A HOLISTIC MODEL TO EMPOWER HUMANITY},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2015},
Volume = {14},
Number = {2},
Pages = {77-85},
Month = {SUM},
Abstract = {Most politicians polarize society through the direction, the values, and
   the words they use; in other words, they do not unite. Instead, they
   play up ethnic, religious, and nationalist sentiments, as well as other
   concepts that can be easily inflamed, in order to ensure specific voting
   outcomes. This process summarizes a violent vicious cycle that has been
   in use for centuries. In this article, the author lays out a vision for
   a holistic model that combines a direction, a set of values, and a
   language that can unite everyone. According to the author, experiences
   built around this model can empower humanity, which will in turn root
   out systems that breed prejudices and begin the process of healing.},
