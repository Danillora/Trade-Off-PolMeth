@article{ WOS:000421679700003,
Author = {Vasquez Mejias, Ainhoa Montserrat},
Title = {Femicide in Chile, more than a classification problem},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
Year = {2015},
Number = {17},
Pages = {36-47},
Month = {DEC},
Abstract = {In 2014, 67 women were murdered in Chile due to gender reasons; however,
   only 40 were reported as femicide under current legislation that
   punishes only those murders of women by their partners or former
   partners, excluding other types of gender violence, resulting in death.
   This omission adds the fact that this type of crime has been recorded as
   femicidio in detriment of feminicidio, a name that is used in other
   parts of the world. This, because chilean state denies its share of
   responsibility in these crimes. Questioning the definition and rethink
   the laws designed to safeguard the lives of women should be the first
   step for a particular change in legislation, as well as in the
   consciousness of all the Chilean people.},
