@article{ WOS:000356394700006,
Author = {Parsons, Bryan M.},
Title = {The Social Identity Politics of Peer Networks},
Journal = {AMERICAN POLITICS RESEARCH},
Year = {2015},
Volume = {43},
Number = {4, SI},
Pages = {680-707},
Month = {JUL},
Abstract = {Building on recent studies of social pressure and group conformity in
   peer groups, I apply a social identity framework from social psychology
   to the study of political networks. Using the American National Election
   Studies (ANES) 2008-2009 Panel Study, I test several hypotheses
   regarding the relationship between peer networks, the salience of
   partisan identity, and the consequences for intergroup attitudinal and
   affective polarization. The findings suggest that peer network
   homogeneity produces the strongest association with more salient
   partisan identities across several network characteristics, including
