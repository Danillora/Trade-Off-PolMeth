@article{ WOS:000354908700006,
Author = {Lynch, Philip},
Title = {Conservative modernisation and European integration: From silence to
   salience and schism},
Journal = {BRITISH POLITICS},
Year = {2015},
Volume = {10},
Number = {2, SI},
Pages = {185-203},
Month = {JUN},
Abstract = {The re-emergence of European integration as a difficult issue for the
   Conservative Party exposed the limits of Cameron's modernisation
   project. In opposition, Cameron had defused the EU issue by lowering its
   salience but this suppressed rather than effectively addressed the
   issue, allowing Eurosceptics who favour withdrawal or fundamental
   renegotiation to shape the agenda. In office, new and familiar
   challenges emerged. The Eurozone sovereign debt crisis changed the
   dynamics of the UK's relationship with the EU. Domestically, coalition
   with the Liberal Democrats and dissent from Eurosceptic Conservative MPs
   restricted Cameron's room for manoeuvre, while the rise of the UK
   Independence Party (UKIP) added a new dimension to the problems the EU
   issue poses for the Conservatives. Cameron responded by promising that
   if the Conservatives win the 2015 general election, he will negotiate a
   `new settlement' in the EU and hold an `in-out' referendum. This article
   assesses whether this position amounts to a belated modernisation of
   Conservative policy that might both resolve inta-party divisions and
   settle the question of the UK's place in the EU.},
