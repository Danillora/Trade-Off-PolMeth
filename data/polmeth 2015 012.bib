@article{ WOS:000352336600003,
Author = {Linhart, Eric and Tepe, Markus},
Title = {Rational Voting in Multi-Party Systems with Coalition Governments. A
   Laboratory Experimental Study},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2015},
Volume = {56},
Number = {1},
Pages = {44-76},
Abstract = {In this contribution, we address the question if and under which
   circumstances voters are able to identify the utility maximizing choice
   in multi-party systems with coalition governments. With help of a
   laboratory experiment we test the influence of specific variables on the
   likelihood of rational voting decisions. In accordance with our
   theoretical expectations, we find that the subjects are more likely to
   identify and vote for the rational choice, if i) the rational choice
   corresponds to the closest party, ii) the distance between the rational
   choice and the voter's ideal point is smaller, iii) simple heuristics
   are available to identify the rational choice, and iv) the scenarios do
   not include characteristics making them more complex (like, e. g.,
   signals for unconnected coalitions).},
