@article{ WOS:000371228700003,
Author = {Panagopoulos, Costas and Endres, Kyle},
Title = {The Enduring Relevance of National Presidential Nominating Conventions},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
