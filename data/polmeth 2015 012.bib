@article{ WOS:000365827500007,
Author = {Cairney, Paul and Jordan, Grant},
Title = {Theories of the policy process: What is British and what is universal? A
   polite reply to Marsh and McCaffrie},
Journal = {BRITISH POLITICS},
Year = {2015},
Volume = {10},
Number = {4},
Pages = {486-492},
Month = {DEC},
Abstract = {The `policy communities' literature has stood the test of time because
   it describes a `universal' logic for groups and governments to interact.
   To deal with complex government, policymakers divide their
   responsibilities into manageable units, and delegate responsibility for
   policymaking to bureaucrats, who form relationships with the groups from
   which they seek information and advice. Governments come and go, and
   these relationships vary from issue to issue, but the logic of
   consultation does not change. In this article, we describe this process,
   situate it within a broader discussion of policy theory, and use these
   insights to respond to the points made by Marsh and Mc Caffrie.},
Publisher = {PALGRAVE MACMILLAN LTD},
Address = {BRUNEL RD BLDG, HOUNDMILLS, BASINGSTOKE RG21 6XS, HANTS, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Cairney, P (Corresponding Author), Univ Stirling, Div Hist \& Polit, Stirling FK9 4LA, Scotland.
