@article{ WOS:000358598100002,
Author = {Ormston, Rachel and Curtice, John and Hinchliffe, Stephen and
   Marcinkiewicz, Anna},
Title = {A SUBTLE BUT INTRACTABLE PROBLEM? PUBLIC ATTITUDES TO SECTARIANISM IN
   2014},
