@article{ WOS:000364706500008,
Author = {Lyons, Joseph},
Title = {Local Government Structure and the Co-ordination of Economic Development
   Policy},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2015},
Volume = {48},
Number = {1},
Pages = {173-193},
Month = {MAR},
Abstract = {This paper explores how local government structure affects policy
   co-ordination. It compares the delivery of economic development services
   in two Ontario cities. In London, a specialized agency is the lead, but
   lacks full functional control, while in Hamilton policy control is
   consolidated in a single municipal department. The findings support the
   hypothesis of those who argue in favour of general-purpose governments,
   as the municipal hierarchy in Hamilton was instrumental in achieving
   policy co-ordination. Proponents of specialization argue that it limits
   the need for coordination between governments but, as the London case
   demonstrates, this logic breaks down when there are too many players
   with too few responsibilities.},
Publisher = {CAMBRIDGE UNIV PRESS},
