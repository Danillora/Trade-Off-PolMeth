@article{ WOS:000446215100008,
Author = {Walczak, Jaroslaw},
Title = {LEGAL STATUS OF NATIONAL MINORITIES IN THE REPUBLIC OF POLAND AGAINST
   THE BACKDROP OF CULTURAL SECURITY},
Journal = {POLITICKE VEDY},
Year = {2015},
Volume = {18},
Number = {2},
Pages = {178-199},
Abstract = {Multicultural societies have been formed since the dawn of history, and
   the approach of a democratic state and society to the treatment and
   understanding of minorities reflects the changes occurring in the social
   and political awareness of the citizens. For some, ethnicity is just a
   relic of history, and a source of conflicts. For others, it is a
   much-valued pluralism, a touchstone of social justice, contributing to
   the exchange of cultural property and general development. The
