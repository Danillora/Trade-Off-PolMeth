@article{ WOS:000448120300002,
Author = {Kukovic, Simona},
Title = {NEW FORMS OF PARTICIPATORY DEMOCRACY AT LOCAL LEVEL: eCITIZENS?},
Journal = {JOURNAL OF COMPARATIVE POLITICS},
