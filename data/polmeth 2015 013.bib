@article{ WOS:000218195200009,
Author = {Contessi, Nicola},
Title = {TRADITIONAL SECURITY IN EURASIA THE CASPIAN CAUGHT BETWEEN
   MILITARISATION AND DIPLOMACY},
Journal = {RUSI JOURNAL},
Year = {2015},
Volume = {160},
Number = {2},
Pages = {50-57},
Abstract = {The brewing conflict over the division of resources located in the
   Caspian Sea has long been cited as a driver of the growth in naval
   capabilities of the five littoral states. However, to focus on this
   aspect would be to neglect the political and diplomatic advances of
   recent years, as well as other local and global geopolitical factors
   that have contributed to increased tension. Nicola Contessi explores the
   intricate dynamics at play in the region and the ongoing efforts to
   unpick them, arguing that while the 2014 Astrakhan Summit did not herald
   the breakthrough hailed by the five participants, it did mark a step
   towards resolution.},
