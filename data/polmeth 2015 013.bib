@article{ WOS:000360327200007,
Author = {Daase, Christopher and Deitelhoff, Nicole},
Title = {Beyond Anarchy: Resistance and Rule in the International System},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2015},
Volume = {56},
Number = {2},
Pages = {299-318},
Abstract = {The ``Anarchy Problematique{''} in International Relations has given way
   to what we call the ``Problematique of Rule{''}. Increasingly, normative
   and empirical questions arise on who can and who may determine how
   states have to behave in international politics. Structures of rule in
   the international system are less direct and observable than in national
   systems, however. Therefore, they are best identified through analysing
   the resistance they provoke. Resistance in turn manifests itself either
   as ``opposition{''} within the framework of established institutions or
   as ``dissidence{''} through unconventional means. Focussing on rule and
   resistance allows analysing institutionalized structures of super- and
   sub-ordination and the attendant conflicts of legitimacy.},
