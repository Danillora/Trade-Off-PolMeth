@article{ WOS:000458020400003,
Author = {Fishman, Leonid and Davydov, Dmitry},
Title = {From Capitalism to Rent Society?},
Journal = {POLITEIA-JOURNAL OF POLITICAL THEORY POLITICAL PHILOSOPHY AND SOCIOLOGY
   OF POLITICS},
Year = {2015},
Volume = {76},
Number = {1},
Pages = {39+},
Abstract = {This article analyzes the term ``rent{''} as a metaphor and a possible
   conceptual framework for studying the modern Western society. Contesting
   the validity of the argument that the capitalist system is based on
   overcoming rent-seeking behavior, L.Fishman and D.Davydov demonstrate
   that rent relations permeate modern capitalism to a greater extent than
   the preceding societal formations. According to their conclusion,
   capitalism as a society of material welfare maximizers through work and
   investment represents a fragile shell that hides old rental principles,
   and this shell can break at any moment exposing an unprecedented in
   history rent society that is developing inside this shell.},
