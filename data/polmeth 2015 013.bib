@article{ WOS:000379416000002,
Author = {Annunziata, Rocio},
Title = {Diminished citizenship: the idea of ``construction of citizenship{''} in
   contemporary participatory mechanisms},
Journal = {TEMAS Y DEBATES},
Year = {2015},
Number = {30},
Pages = {39-57},
Month = {JUL-DEC},
Abstract = {This paper aims to discuss the idea of ``construction of citizenship{''}
   that appears in contemporary participative experiences encouraged by
   local governments. The practices that are actually developed in the
   so-called ``participatory mechanisms{''} suppose an enlargement but at
   the same time a reduction of the notion of citizenship. On the one hand,
   citizenship is conceived as ``under construction{''}: it does not depend
   on passive rights but on the involvement of citizens. On the other hand,
   it remains associated with practices of management of the immediate
   environment, where the citizen becomes a neighbor and his/her activity
   is identified with a complaint.},
