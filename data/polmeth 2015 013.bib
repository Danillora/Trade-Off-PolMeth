@article{ WOS:000354908700008,
Author = {Smith, Martin and Jones, Rhonda},
Title = {From big society to small state: Conservatism and the privatisation of
   government},
Journal = {BRITISH POLITICS},
Year = {2015},
Volume = {10},
Number = {2, SI},
Pages = {226-248},
Month = {JUN},
Abstract = {The Conservative's modernisation strategy contained a renewed commitment
   to public services and a continuing role for the state in the delivery
   of collective goods. Promises about health and education spending were
   central to the detoxification strategy of David Cameron. However, the
   article argues that faced with the combination of austerity economics
   and public support for health and education spending, the Coalition has
   pushed towards a radical shrinking of the state, but one that is
   focussed on particular policy areas such as welfare. At the same time,
   we have also seen a growing integration of the public and private in the
   supply and delivery of public policy. Hence, the article demonstrates
   that although the Conservatives' trajectory initially followed closely
   from New Labour's, they have since made significant changes to the role
   of the state.},
