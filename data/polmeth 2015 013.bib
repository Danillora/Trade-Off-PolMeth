@article{ WOS:000436666000010,
Author = {Lam, Peng Er},
Title = {Japan at a Crossroads: Abe Shinzo's Politics and Abenomics},
Journal = {EAST ASIAN POLICY},
Year = {2015},
Volume = {7},
Number = {1},
Pages = {121-129},
Month = {JAN-MAR},
Abstract = {In 2014, Prime Minister Abe Shinzo dominated Japanese politics. He
   successfully hiked the consumption tax in April and decisively won the
   Lower House Election in November. In an ideological tilt to the right,
   Abe also shifted Japan away from its post-war pacifism by making a
   cabinet decision to accept collective security. By paving the way for
   Japan to exercise military force through the assistance of third parties
   beyond its US ally, Abe will fundamentally change the role and identity
   of Japan in the post-Cold War era.},
Publisher = {WORLD SCIENTIFIC PUBL CO PTE LTD},
