@article{ WOS:000360693400003,
Author = {Overy, Richard},
Title = {BOMBED INTO DEFEAT? AIR POWER AND THE END OF THE SECOND WORLD WAR},
Journal = {RUSI JOURNAL},
Year = {2015},
Volume = {160},
Number = {4},
Pages = {10-13},
Month = {JUL 4},
Abstract = {While the Second World War is by far the most studied global conflict,
   prolific scholarship on its contours and causes has yet to yield
   consensus. The effectiveness of different military strategies, most
   notably air bombing, is still disputed. Richard Overy examines the claim
   that strategic bombing was a major factor in bringing both the European
   and Pacific wars to an end in 1945. Rejecting the argument that bombing
   led to surrender in either Germany or Japan, he highlights the
   significance of outright military defeat and political calculation.},
