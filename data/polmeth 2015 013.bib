@article{ WOS:000383165000002,
Author = {Solano Baez, Ma del Carmen},
Title = {Disguise of tourism development: privatization, dispossession and
   resistance in the rural media of Mexico},
Journal = {PENSAMIENTO AL MARGEN},
Year = {2015},
Number = {3},
Abstract = {This article is a critical approximation to the tourist development
   model predominant in the rural means in Mexico. It does an approximation
   from two perspectives, one the institutions have managed since it and
   other one how the rural communities of Mexico have assumed since it. It
   shows the insertion of the tourism in the most vulnerable zones, with
   privileged natural and cultural resources. It expresses an approach, of
   the value of the landscape, of the land, of the culture and of the
   social access to the pleasure, as well as of the secondary ways of
   understanding the territory and the tourism. It leaves of side the
   innocent theory of the tourism to denounce the voracity of an activity
   that from his beginnings has proposed the challenge of eradicating the
   poverty, diversify the economic activities and to achieve a social
   development, ending in privatizations of lands. Show to the protagonists
   of the spoliation, communities organized in defense of his territory.},
