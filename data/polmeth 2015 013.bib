@article{ WOS:000216889700005,
Author = {Wei, Li and Han, Su},
Title = {Road to a Great Monetary Power China's Changing Role in the
   International Monetary System},
Journal = {CHINA QUARTERLY OF INTERNATIONAL STRATEGIC STUDIES},
Year = {2015},
Volume = {1},
