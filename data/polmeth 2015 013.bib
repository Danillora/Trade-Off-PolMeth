@article{ WOS:000368311700007,
Author = {Liedhegener, Antonius and Thieme, Daniel},
Title = {Interpretation and Explanation in Empirical Political Theory about
   Religion. Plea for a Well-Founded Pluralism in Methodology},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2015},
Volume = {56},
Number = {4},
Pages = {682-693},
Abstract = {The current research field of politics and religion is a cross-sectional
   task in political science. It requires hermeneutical and explanatory
   approaches. Our response to Richard Traunmuller underlines the shared
   interest in empirical research on religions and religious collective
   actors. However, we reject the methodological monism of Traunmuller's
   approach and, drawing on the debate on the status of churches in the
   intermediary sphere and the functional-structural model of political
   influence of religious actors, we demonstrate that mere theoretical
   axioms and simple causal models alone will not suffice to substantially
   improve the discipline's knowledge within this field of research. A
   pluralism of methodologies is needed.},
