@article{ WOS:000213907100005,
Author = {Furtado, Henrique Tavares},
Title = {Against state terror: lessons on memory, counterterrorism and resistance
   from the Global South},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2015},
Volume = {8},
Number = {1, SI},
Pages = {72-89},
Abstract = {Critical theory avows that ``where there is power there is always
   resistance{''}. However, the practical implications and consequences of
   particular modes of resistance remain, within World Politics,
   under-theorised. In critical terrorism studies (CTS), this critical
   imperative to resist has recently emerged in the proposal to remember
   state terrorism. With this move, CTS aims to disturb the legitimacy of
   forms of violence/terror that emerge from the state. In this article, I
   argue that such an agenda of ``resistance through memory{''} has already
   been put forth in the Global South (specifically, Latin America).
   Drawing on this historical experience, I elucidate some problems with
   the critical imperative to resist. More specifically, I show how in
   Brazil the Global South countermemorial narratives of state terror share
   a common ground with the Global North counterterrorism discourses. I do
   so by analysing three underlying tropes of Brazilian remembrance that
   replicate Global North representations of terrorists: bestialism,
   pathology and dehumanisation.},
