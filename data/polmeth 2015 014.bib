@article{ WOS:000351957000004,
Author = {Husson, Michel},
Title = {Capital in the Twenty-First Century by Thomas Piketty
   `Wealth of Data, Poverty of Theory'},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2015},
Volume = {23},
Number = {1},
Pages = {70-85},
Abstract = {Thomas Piketty's book, Capital in the Twenty-First Century, provides a
   monumental database for the history of capitalism. But the author's
   interpretation of these data is based on an inconsistent theoretical
   framework that constantly oscillates between two definitions of capital:
   either capital as accumulated drawing rights on the value created; or
   capital as a factor of production in the neoclassical tradition. Capital
   as a social relation is forgotten and the history of capitalism appears
   as an accounting mechanism.},
