@article{ WOS:000376180000003,
Author = {Solis Delgadillo, Juan Mario and Torre Delgadillo, Vicente},
Title = {Democrats but violent: a preliminary approach to the structural
   variables to understand the insecurity in Latin America},
Journal = {REVISTA MEXICANA DE ANALISIS POLITICO Y ADMINISTRACION PUBLICA},
Year = {2015},
Volume = {4},
Number = {1},
Pages = {29-61},
Month = {JAN-JUN},
Abstract = {Over the last years Latin America has experienced as never before the
   expansion of violence. According to many studies, this phenomenon is
   closely linked to inequality and manifests itself in many different
   ways. This means that violence in Latin America does not have a unique
   pattern of behavior. The goal of the present investigation is to seek a
   descriptive approach to the problem and to explore what might be the
   causes of insecurity in the region based on institutional and
   socio-economic variables and indicators. Accordingly, it is considered
   that factors such as institutional weakness, corruption, inequality and
   social capital influence in one way or another, or at least in different
   degrees, when explaining why the Latin American sub-continent is such a
   violent region while being a territory in peace.},
