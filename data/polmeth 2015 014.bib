@article{ WOS:000364545500011,
Author = {Klinke, Ian and Perombelon, Brice},
Title = {Notes on the Desecuritisation of the Rhineland Frontier},
Journal = {GEOPOLITICS},
Year = {2015},
Volume = {20},
Number = {4},
Pages = {836-852},
