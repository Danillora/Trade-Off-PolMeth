@article{ WOS:000210663700010,
Author = {Buck, Pem Davidson},
Title = {Whiteness, Communism, and Possibility},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2015},
Volume = {27},
Number = {3},
Pages = {378-380},
Abstract = {This commentary on the exchange between Jodi Dean and Stephen Healy at
   the 2013 Rethinking Marxism International Conference focuses on three
   interrelated questions concerning the handling of race and class. The
   first concerns the race of participants who took part in the question
   and answer session that followed the authors' presentations: why were
   nearly all white, and how does this affect theory and analysis? Second,
   why is surprisingly little said directly about social class, either of
   the class of the participants or of class's impact on strategy and
   people's options? And third, what strategies are proposed specifically
   to dismantle white privilege, given that challenging capitalism cannot
   succeed while white privilege holds sway? This commentary concludes
   that, despite these issues, the idea of community economies provides a
   sense of hope.},
