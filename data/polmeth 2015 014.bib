@article{ WOS:000214064200002,
Author = {Hemmingsen, Ann-Sophie},
Title = {Viewing jihadism as a counterculture: potential and limitations},
Journal = {BEHAVIORAL SCIENCES OF TERRORISM AND POLITICAL AGGRESSION},
Year = {2015},
Volume = {7},
Number = {1},
Pages = {3-17},
Abstract = {Studies of jihadism mostly focus on the violent and religio-political
   aspects of the phenomenon. By adding to these a countercultural
   perspective, we may become able to better grasp and make sense of
   jihadism, not least because we can begin to see its violent and
   political aspects in their immediate social context. The present paper
   is based on lengthy fieldwork in Denmark and presents a first attempt at
   applying a combination of theories on cultic milieus and countercultures
   to the gallery of characters in and around three Danish terrorism cases.},
