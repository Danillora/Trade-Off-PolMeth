@incollection{ WOS:000377044800006,
Author = {Gardiner, Michael},
Book-Author = {Gardiner, M},
Title = {Permanent Labour},
Booktitle = {TIME AND ACTION IN THE SCOTTISH INDEPENDENCE REFERENDUM},
Year = {2015},
Pages = {48-60},
Abstract = {This chapter describes self-determination as an interruption of the
   streaming of personal time into labour time (and debt time) that stands
   as a natural and permanent demand of British franchise, Recently there
   has been much scepticism over unlimited or `24/7' work, but we should
   also see that `flexible labour' is precisely what has always been
   guaranteed by the flexible constitution' (the unwritten or eternalist
   constitution). And similarly, the questioning of franchise through
   labour time also means a questioning of its most progressive
   parliamentary representatives, which is what we see in the Labour Party
   meltdown that follows the referendum, and which seems to cause a crisis
   for British parliamentary conceptions of the progressive in general.},
