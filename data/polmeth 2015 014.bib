@article{ WOS:000356236400001,
Author = {Matthes, Joerg},
Title = {Observing the ``Spiral{''} in the Spiral of Silence},
Journal = {INTERNATIONAL JOURNAL OF PUBLIC OPINION RESEARCH},
Year = {2015},
Volume = {27},
Number = {2},
Pages = {155-176},
Month = {SUM},
Abstract = {Time is of the utmost importance when designing studies to test the
   spiral of silence theory. The theory posits that individuals who feel
   they are in the majority become more dominant and louder over time,
   while the minority camp becomes increasingly silent. However, few
   studies have tested the dynamic nature of the theory. Therefore, the
   aims of this article are to revisit the role of time in spiral of
   silence research and to demonstrate how dynamic processes can be modeled
   with three-wave panel data. Using survey data on the topic of
   unemployment, the relationship between change in the opinion climate and
   change in opinion expression is estimated with a latent growth model.
   Findings confirm the dynamic processes predicted by the theory.},
Publisher = {OXFORD UNIV PRESS},
Address = {GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND},
Type = {Article},
