@article{ WOS:000421262100017,
Author = {Spoormans, Huub and Broekhuijse, Irene},
Title = {The Regulation of Political Parties in the Netherlands},
Journal = {TEORIA Y REALIDAD CONSTITUCIONAL},
Year = {2015},
Volume = {35},
Pages = {577-603},
Abstract = {This article focuses on the relation between political parties and the
   state in the Netherlands; a polity where the main actors in the
   political scene are not even mentioned in the Constitution, and where
   there is not a specific party act. The authors describe the origins and
   development of political parties in the Netherlands, and the question of
   its regulation, together with a general and comparative glimpse to other
   European democracies. The thesis is that legal regulation took a
   different route via international law - the ECHR and the CEDAW -,
   applied by the national courts.},
