@article{ WOS:000367006900009,
Author = {Orr, Graeme},
Title = {Does an Unqualified but Losing Candidacy Upset an Election?},
Journal = {ELECTION LAW JOURNAL},
Year = {2015},
Volume = {14},
Number = {4},
Pages = {424-429},
Month = {DEC},
Abstract = {All electoral systems erect qualifications and disqualifications for
   would-be candidates. An unqualified candidate cannot be duly elected.
   But what isor should bethe effect on an elective race of an unqualified
   losing candidate appearing on the ballot? This commentary examines the
   law and policy on point, to conclude that only in limited circumstances
   could an election be argued to be void because of a losing candidate
   being found to be unqualified. Those circumstances would be where
   first-past-the-post voting is used and the loser's vote tally well
   exceeded the winning margin. In any event, it is a point that
   legislators everywhere would do well to clarify.},
