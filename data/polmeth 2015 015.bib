@article{ WOS:000358746200004,
Author = {Hausken, Kjell and Banuri, Sheheryar and Gupta, Dipak K. and Abbink,
   Klaus},
Title = {Al Qaeda at the bar: coordinating ideologues and mercenaries in
   terrorist organizations},
Journal = {PUBLIC CHOICE},
