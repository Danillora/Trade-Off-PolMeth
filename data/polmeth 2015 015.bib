@article{ WOS:000366959300004,
Author = {Harootunian, Harry},
Title = {Piercing the Present with the Past Reflections on Massimiliano
   Tomba's Marx's Temporalities},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2015},
Volume = {23},
Number = {4},
Pages = {60-74},
Abstract = {This response to Tomba's Marx's Temporalities homes in on its critical
   interrogation of linear conceptions of development which have distorted
   Marxism's capacity to seize the present. The article foregrounds the
   resources on which Tomba draws, from Walter Benjamin's theses on history
   to Marx's account of the struggles over the working day, and enlists
   them in an encounter with the questions of unevenness, archaism and
   pre-capitalism in Postcolonial theory, as well as in the attempts to
   `overcome modernity' that marked the thought and practice of interwar
   fascism in Europe and Japan.},
Publisher = {BRILL ACADEMIC PUBLISHERS},
