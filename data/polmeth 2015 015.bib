@article{ WOS:000368900000004,
Author = {Fernandez de Miguel, Daniel},
Title = {THE MISTRUST OF THE AMERICAN MODEL OF MODERNIZATION IN SPAIN},
Journal = {HISTORIA Y POLITICA},
Year = {2015},
Number = {34},
Pages = {81-111},
Month = {JUL-DEC},
Abstract = {Throughout the first third of the twentieth century, the ongoing debates
   on modernization in Spain took Europe as their main reference point, but
   there were also partisans and detractors of American-style
   modernization, which clearly was in the ascendant. Following the
   Francoist victory in the Civil War, which coincided with the United
   States' rise to the condition of world superpower, American-style
   modernization further troubled important sectors of Spanish society, at
   times with the clear connivance of Francoist officials. The gradual
   improvement of bilateral relations, confirmed by the signing of the
   Treaty of Madrid of 1953, lead new reformist elites in the second half
   of the Fifties to look toward the other side of the Atlantic in search
   of economic and ideological references to modernization, thus breaking
   with the anti-American tradition that up to that time was dominant in
   Spanish conservatism.},
