@incollection{ WOS:000370688700004,
Author = {Jarman, Holly},
Book-Author = {Jarman, H},
Title = {Investors versus States},
Booktitle = {POLITICS OF TRADE AND TOBACCO CONTROL},
Series = {Palgrave Pivot},
Year = {2015},
Pages = {71-90},
Abstract = {Investor-state dispute mechanisms are built into many different trade
   agreements and form the core of the Bilateral Investment Treaties (BITs)
   that have proliferated among states in the past few decades. They are
   unique in trade policy, because they allow foreign investors to initiate
   investment disputes against governments that they believe have violated
   their rights. Drawing from a set of core trade and tobacco control
   disputes involving investor-state protections, Jarman argues that
   extensive forum and rule choice for firms and a lack of transparency
   narrow the scope of conflict in these cases, preventing adequate public
   scrutiny.},
