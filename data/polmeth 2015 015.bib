@article{ WOS:000352498000003,
Author = {Williams, Callum},
Title = {FAMINE: ADAM SMITH AND FOUCAULDIAN POLITICAL ECONOMY},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2015},
Volume = {62},
Number = {2},
Pages = {171-190},
Month = {MAY},
Abstract = {It is often assumed that the attitudes of the early political
   economists, such as Smith, to issues of scarcity and famine were
   dogmatically laissez-faire. This view has been given fresh impetus by
   Michel Foucault's recently published lectures on the history of
   political economy. The article challenges this view. By examining
   Smith's texts and analyzing the way that Smith was received by critics
   in the century following the publication of the Wealth of Nations, the
   article argues that contemporary interpretations of Smith's views on
   scarcity and famine must be nuanced.},
