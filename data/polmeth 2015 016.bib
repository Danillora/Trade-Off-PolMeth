@article{ WOS:000358323800008,
Author = {Montgomery, Kathleen A. and Winter, Ryan},
Title = {Explaining the Religion Gap in Support for Radical Right Parties in
   Europe},
Journal = {POLITICS AND RELIGION},
Year = {2015},
Volume = {8},
Number = {2},
Pages = {379-403},
Month = {JUN},
Abstract = {Across Europe populist radical right (PRR) parties advertise themselves
   as defenders of Christian identity and values, but they do not seem to
   strongly attract religious Christian voters. This article tests a
   general framework for understanding this religion gap in 13 countries.
   Findings extend earlier research on religiosity and radical right
   attitudes, provide insight on East-West differences in the PRR
   phenomenon, and lend conditional support to the notion of a vaccine
   effect suggested by prior research.},
