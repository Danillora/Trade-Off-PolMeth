@article{ WOS:000353478400006,
Author = {Klofstad, Casey A.},
Title = {Exposure to Political Discussion in College is Associated With Higher
   Rates of Political Participation Over Time},
Journal = {POLITICAL COMMUNICATION},
Year = {2015},
Volume = {32},
Number = {2},
Pages = {292-309},
Month = {APR 3},
Abstract = {While individuals who are exposed to political discussion are more
   politically active, analytical biases make it difficult to show evidence
   of causation. It is also uncertain how long the relationship between
   discussion and participation lasts. Here both questions are addressed
   with panel data collected from individuals who were randomly assigned to
   their college dormitories. The data show that exposure to political
   discussion in college leads to higher levels of participation,
   immediately while still in college and years into the future after
   graduation. As political behavior is habitual, the initial increase in
   participation after being exposed to political discussion is a mechanism
   underlying the long-run relationship between discussion and
   participation.},
