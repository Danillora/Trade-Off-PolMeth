@article{ WOS:000214138300001,
Author = {Kingsley, Sara Constance and Gray, Mary L. and Suri, Siddharth},
Title = {Accounting for Market Frictions and Power Asymmetries in Online Labor
   Markets},
Journal = {POLICY AND INTERNET},
Year = {2015},
