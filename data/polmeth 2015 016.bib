@article{ WOS:000359891300001,
Author = {Daniele, Gianmarco and Geys, Benny},
Title = {Interpersonal trust and welfare state support},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2015},
Volume = {39},
Pages = {1-12},
Month = {SEP},
Abstract = {The economic importance of the welfare state has increased strongly over
   time, which has generated a vast academic literature studying the
   determinants of (preferences towards) redistribution. This article
   argues that citizens' trust in their fellow citizens can play a central
   role for welfare state support, because it buttresses the belief that
   others will not use the welfare system inappropriately. Using the fourth
   wave of the European Social Survey, we confirm a strong positive
   association between interpersonal trust and welfare state support
   (controlling for institutional trust). we also show that: i) this link
   is driven at least in part by the mechanism discussed above; ii)
   causality runs from interpersonal trust to welfare state support (using
   a sub-sample of second generation immigrants); and iii) the effect of
   interpersonal trust appears conditional on the perceived quality of a
   country's institutions. (C) 2015 Elsevier BM. All rights reserved.},
