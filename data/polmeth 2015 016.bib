@article{ WOS:000355742900004,
Author = {Sylwester, Kevin},
Title = {Does Democracy Increase Growth More in New Countries?},
Journal = {ECONOMICS \& POLITICS},
Year = {2015},
Volume = {27},
Number = {2},
Pages = {266-289},
Month = {JUL},
Abstract = {Countries face governing challenges at their inception, albeit not of
   the same degree or type. Challenges such as creating governing
   structures and forming one nation from disparate groups can create
   uncertainty and so lower economic growth. Does democracy exacerbate or
   lessen such problems? This paper considers an empirical specification
   where the effect of democracy upon economic growth is allowed to vary
   over time. I find that democracy is more greatly associated with
   economic growth in newer countries. This suggests that democracy helps
   to mitigate governing challenges that can lower economic growth.},
