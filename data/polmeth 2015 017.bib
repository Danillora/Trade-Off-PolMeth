@article{ WOS:000351552200001,
Author = {Moore, Clive},
Title = {The Pacific Islanders' Fund and the Misappropriation of the Wages of
   Deceased Pacific Islanders by the Queensland Government},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2015},
Volume = {61},
Number = {1},
Pages = {1-18},
Month = {MAR},
Abstract = {The Queensland Pacific Islanders' Fund operated between 1885 and the
   1900s but is largely unknown today. It was established in the Treasury
   to facilitate the operation of the Pacific Island Labourers Act 1880
   Amendment Act 1885 to safeguard return fares and to ensure that the
   money due to deceased Islanders was returned to their families. However,
   over time, because of the high death rates, the wages of deceased
   Islanders became so substantial that they were able to be used to
   supplement the administration of the whole labour trade. Money was drawn
   off to subsidize Islander hospitals and Christian missions, to an extent
   that in the final 1900s deportation years there was no longer enough
   money available to pay full fares. The Queensland government seldom
   returned the full wages to the families of the deceased Islanders and
   profited largely from their deaths. In today's money, millions of
   dollars were misappropriated, in similar fashion to wages
   misappropriated by the same government from Aborigines.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Moore, C (Corresponding Author), Univ Queensland, Hist \& Philosoph Inquiry, Brisbane, Qld 4072, Australia.
   Univ Queensland, Hist \& Philosoph Inquiry, Brisbane, Qld 4072, Australia.},
DOI = {10.1111/ajph.12083},
ISSN = {0004-9522},
EISSN = {1467-8497},
Keywords-Plus = {LABORERS},
