@article{ WOS:000368947200002,
Author = {Couttenier, Mathieu and Soubeyran, Raphael},
Title = {A Survey of the Causes of Civil Conflicts: Natural Factors and Economic
   Conditions},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2015},
Volume = {125},
Number = {6},
Pages = {787-810},
Month = {NOV-DEC},
Abstract = {We provide an overview of the roots of civil conflict and distinguish
   between economic conditions and natural factors. We discuss the very
   recent (quasi-experimental) evidence on the effect of economic wealth,
   commodity prices and climate on the likelihood of civil conflict. As a
   preamble, we present an overview of the theoretical literature on the
   roots of conflict and distinguish between ``capacity-related{''} and
   ``opportunity-related{''} causes of conflict. We also provide policy
   implications regarding the prevention of civil conflicts.},
Publisher = {EDITIONS DALLOZ},
Address = {31-35 RUE FROIDEVAUX, PARIS CEDEX 14, 75685, FRANCE},
