@article{ WOS:000350457700015,
Author = {Lusardi, Annamaria and Schneider, Daniel and Tufano, Peter},
Title = {The Economic Crisis and Medical Care Use: Comparative Evidence from Five
   High-Income Countries},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2015},
Volume = {96},
Number = {1},
Pages = {202-213},
Month = {MAR},
Abstract = {ObjectiveWe examine how the economic crisis has affected individuals'
   use of routine medical care and assess the extent to which the impact
   varies depending on national context.
   MethodsData from a new cross-national survey fielded in the United
   States, Great Britain, Canada, France, and Germany are used to estimate
   the effects of employment and wealth shocks and financial fragility on
   the use of routine care.
   ResultsWe document reductions in individuals' use of routine
   nonemergency medical care in the midst of the economic crisis. Americans
   reduced care more than individuals in Great Britain, Canada, France, and
   Germany. At the national level, reductions in care are related to the
   degree to which individuals must pay for it, and within countries,
   reductions are linked to shocks to wealth and employment and to
   financial fragility.
   ConclusionsThe economic crisis has led to reductions in the use of
   routine medical care, and systems of national insurance provide some
   protection against these effects.},
