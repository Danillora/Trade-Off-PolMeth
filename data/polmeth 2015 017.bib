@article{ WOS:000357841300005,
Author = {Machek, Ondrej and Hnilica, Jiri},
Title = {EVALUATING THE IMPACT OF FAMILY PRESENCE IN OWNERSHIP AND MANAGEMENT ON
   FINANCIAL PERFORMANCE OF FIRMS USING MATCHED-PAIR INVESTIGATION},
Journal = {POLITICKA EKONOMIE},
Year = {2015},
Volume = {63},
Number = {3},
Pages = {347-362},
Abstract = {The role of family businesses in management theory has been getting a
   topic of interest of academics and practitioners. This paper deals with
   the analysis of performance and financial structure gaps between Czech
   family and nonfamily firms. Using surname matching definition, we
   created a sample of 890 Czech large and medium-sized family businesses.
   Then we used the matched-pair investigation to determine the major gaps
   in profitability, labor productivity, liquidity and leverage between
   family and nonfamily firms. The results suggest that Czech family
   businesses are more profitable in terms of return on assets, profit
   margin and absolute values of earnings. Czech family businesses also
   have higher labor productivity and capital intensity, however, the asset
   turnover appears to be lower. Another important finding is that the
   overall level of debt is lower in the case of family companies which is
   accompanied by a higher level of liquidity and working capital.},
