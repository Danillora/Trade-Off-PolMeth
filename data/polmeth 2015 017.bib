@incollection{ WOS:000401435700009,
Author = {D'Aoust, Anne-Marie},
Editor = {Montegary, L and White, MA},
Title = {Moving Stories: Love at the Border},
Booktitle = {MOBILE DESIRES: THE POLITICS AND EROTICS OF MOBILITY JUSTICE},
Series = {Palgrave Pivot},
Year = {2015},
Pages = {94-107},
Abstract = {When it comes to marriage migration and family reunification more
   generally, love becomes transport, a way of traveling. Love moves people
   across borders, as attachment to a citizen or a permanent resident often
   becomes the very ground for securing legal entry into another territory.
   This intervention examines the relationship between love, nationhood,
   and mobility through a mobilization of the concept of `technologies of
   love.' Focusing on the case of Friendship Park/El Parque de la Amistad
   located along the San Diego/Tijuana border, D'Aoust gestures toward the
   movements brought about by love, along with their diversions and
   resistance, while also taking into account how they are interwoven in
   other flows and assemblages (those of law and sovereignty, especially),
   whose material and spatial features should be addressed.},
