@article{ WOS:000358323800007,
Author = {Mckay, Niall},
Title = {Apartheid Resistance and Biblical Interpretation: From Christian
   Confession to Materialist Analysis},
Journal = {POLITICS AND RELIGION},
Year = {2015},
Volume = {8},
Number = {2},
Pages = {358-378},
Month = {JUN},
Abstract = {The rhetorical resistance to Apartheid in South Africa appealed to many
   sources of authority. In Christian communities, the New Testament was
   brought into creative conversation with traditional confessional texts,
   modern scholarship, and Marxist class analysis in order to develop
   contextually-located theologies and practices of resistance. This
   article highlights the important insights of Allan Boesak, Albert Nolan,
   and Itumeleng Mosala, which served the Apartheid struggle. After noting
   key differences in the methods and conclusions of these scholars, I
   suggest that another look at their insights may well be critical in
   developing Christian responses to the deep structural injustices of the
   post-Apartheid period.},
