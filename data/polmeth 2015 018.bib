@incollection{ WOS:000436873600002,
Author = {Squire, Vicki},
Book-Author = {Squire, V},
Title = {The Sonoran Borderzone},
Booktitle = {POST/HUMANITARIAN BORDER POLITICS BETWEEN MEXICO AND THE US: PEOPLE,
   PLACES, THINGS},
Series = {Mobility \& Politics},
Year = {2015},
Pages = {1-25},
Abstract = {This chapter examines how relations of privilege and violence are
   integral to the formation and maintenance of the US-Mexico border, and
   considers the significance of humanitarian activism across the Sonoran
   borderzone. There is a longstanding relationship of asymmetry between
   Mexico and the US, which is evident in contemporary struggles over
   migration across the Sonoran desert. Such struggles can have lethal
   consequences where those travelling without authorisation are funnelled'
   through the desert as a remote and dangerous terrain. Squire reflects
   upon the potentials and limitations of humanitarian activism in this
   context, considering how humanitarianism emerges both as a mechanism of
   the power and as an ambiguous form of activism that mediates between
   contending forces of migration and of control.},
