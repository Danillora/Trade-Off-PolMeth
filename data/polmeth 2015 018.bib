@article{ WOS:000361221500005,
Author = {Tsuchiya, Takahiro and Synodinos, Nicolaos E.},
Title = {Searching for Alternatives: Comparisons Between Two Sample Selection
   Methods in Japan},
Journal = {INTERNATIONAL JOURNAL OF PUBLIC OPINION RESEARCH},
Year = {2015},
Volume = {27},
Number = {3},
Pages = {383-405},
Month = {FAL},
Abstract = {This study compared answers of Japanese respondents to two
   self-administered conditions. For the mail condition, individuals (aged
   20-79 years) were randomly selected from government registers, and
   questionnaires were mailed to them. For the drop condition,
   questionnaires were inserted in the mailboxes of households selected via
   area sampling and could be completed by any member 20-79 years old. Drop
   and mail respondents had similar demographic characteristics (e.g.,
   gender, age, occupation, education), and the two samples exhibited
   similar biases in representing the population. Respondents were mostly
   similar in expressed opinions, nonsubstantive responses, and other
   responses. However, some differences were found, and the response rate
   of the drop (18.0\%) was substantially lower than for mail (28.7\%).
   Reasons for the observed differences are discussed.},
