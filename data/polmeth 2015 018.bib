@article{ WOS:000366959300002,
Author = {Osborne, Peter},
Title = {Out of Sync: Tomba's Marx and the Problem of a Multi-layered Temporal
   Dialectic},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2015},
Volume = {23},
Number = {4},
Pages = {39-48},
Abstract = {This piece reconstructs and reflects upon the terms of the theoretical
   projection underlying Max Tomba's book, Marx's Temporalities, with
   particular reference to his use of the concepts of multiple
   temporalities (Ernst Bloch) and temporal layers (Bloch and Reinhart
   Koselleck). Tomba's use of these concepts, it is argued, productively
   relocates Marx's writings within the framework of the twentieth-century
   philosophy of time. However, Tomba's dependence upon received versions
   of these concepts, untransformed, reproduces theoretical problems
   implicit within them, which have been intensified by recent developments
   within global capital. The application of these concepts to an
   understanding of the historical present, understood as a situation of
   globally disjunctive contemporaneity, is seen to be, in part, vitiated
   by their embeddedness within an increasingly exhausted past.},
