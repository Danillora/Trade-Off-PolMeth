@incollection{ WOS:000363928600005,
Author = {Bedi, Sonu},
Editor = {Brooks, T},
Title = {Anti-Perfectionism and the Right to Privacy: Case Studies in American
   and Canadian Constitutional Law},
Booktitle = {CURRENT CONTROVERSIES IN POLITICAL PHILOSOPHY},
Series = {Current Controversies in Philosophy},
Year = {2015},
Pages = {63-82},
Abstract = {Is a right to privacy necessary to strike down laws and policies that
   regulate consensual sexual activity? Given the prominence of rights in
   liberal democratic thought and practice, the answer seems to be
   ``yes.{''} After all, it is hard to imagine a liberal democracy without
   such a right. The right to privacy carves out an intimate area that is
   off-limits to state regulation. I argue that such a right may not be
   necessary to strike down democratically enacted legislation that
   regulates consensual sexual activity. This chapter argues that if we
   adopt a version of anti-perfectionism to constrain democratic
   decision-making, we no longer need a right to privacy. I suggest that
   the more we emphasize this kind of justificatory approach, the less
   relevant is a rights-based one. If the state may not base legislation on
   religious or moral conceptions of the good life, the right to privacy
   becomes obsolete in striking down laws that prohibit consensual sexual
   activity. And this argument is not simply normative and only of abstract
   interest; it informs two cases in American and Canadian constitutional
   law.},
