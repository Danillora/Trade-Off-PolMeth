DOI = {10.17356/ieejsp.v1i1.28},
EISSN = {2416-089X},
Keywords = {post-communist capitalism; managed and illiberal democracy; transition
   from communism to capitalism; neo-patrimonial and neo-prebendal forms of
   ownership; traditionalism/neo-conservatism},
