@incollection{ WOS:000371158600004,
Author = {Flanagan, Richard M.},
Book-Author = {Flanagan, RM},
Title = {The Break: The Fight for Charter Reform and the 1961 Mayoral Election},
Booktitle = {ROBERT WAGNER AND THE RISE OF NEW YORK CITY'S PLEBISCITARY MAYORALTY:
   THE TAMER OF THE TAMMANY TIGER},
Series = {Palgrave Pivot},
Year = {2015},
Pages = {47-75},
Abstract = {The tension between party leader Carmine De Sapio and Mayor Robert
   Wagner came to a head when the mayor called for De Sapio's resignation
   as party chief of Manhattan and head of Tammany Hall. De Sapio organized
   a campaign to depose the mayor by denying him support of Tammany and
   many of the other regular political clubs and county organizations in
   the 1961 Democratic primary for mayor. In a bitterly contested election
   fight, Wagner beat the candidate backed by the party bosses, state
   comptroller Arthur Levitt. Simultaneously, Mayor Wagner engineered
   changes to the city charter that increased the power of the mayoralty
   relative to other politicians in New York.},
