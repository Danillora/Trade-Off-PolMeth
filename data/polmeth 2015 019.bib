@article{ WOS:000367863800011,
Author = {Swee, Eik Leong},
Title = {On war intensity and schooling attainment: The case of Bosnia and
   Herzegovina},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2015},
Volume = {40},
Number = {A},
Pages = {158-172},
Month = {DEC},
Abstract = {This paper identifies war intensity effects of the 1992-1995 Bosnian War
   on schooling attainment, and explores possible channels of influence.
   Empirical identification relies on spatial variation in war intensity
   and variation in birth cohorts. I find that cohorts that endured greater
   war intensity are less likely to complete secondary schooling but not
   primary schooling. These effects are much stronger for males than for
   females, and draftee male cohorts experience deterioration in their
   physical and mental health relative to female and non-draftee cohorts,
   suggesting that the military draft may play an important role. Other
   possible mechanisms do not seem to matter. (C) 2015 Elsevier B.V. All
   rights reserved.},
Publisher = {ELSEVIER SCIENCE INC},
