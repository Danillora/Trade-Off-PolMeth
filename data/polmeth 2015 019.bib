@incollection{ WOS:000363928600007,
Author = {Anderson, Scott A.},
Editor = {Brooks, T},
Title = {Objectification: A 21st-Century Reassessment},
Booktitle = {CURRENT CONTROVERSIES IN POLITICAL PHILOSOPHY},
Series = {Current Controversies in Philosophy},
Year = {2015},
Pages = {100-116},
Abstract = {Feminism has made objectification a key explanatory concept in its
   analysis of the perpetuation of male gender domination, with the
   objectification of women's bodies offered as a crucial means by which
   domination is practiced and reproduced. However, not all bodily
   objectification appears to have this character or to be morally
   problematic. Some objectification appears to be voluntary and
   consensual, and to provide a source of shared pleasure. This essay
   distinguishes two aspects of objectification-immediate and symbolic-and
   argues that even if some people objectify themselves voluntarily and
   consensually for their own pleasure and that of others, the symbolism of
   such objectification can present particular difficulties for the
   promotion of gender equality, and can affect women who are not
   consenting parties to such objectification.},
