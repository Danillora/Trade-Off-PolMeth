@article{ WOS:000356393400004,
Author = {Teachman, Jay and Anderson, Carter and Tedrow, Lucky M.},
Title = {Military Service and Alcohol Use in the United States},
Journal = {ARMED FORCES \& SOCIETY},
Year = {2015},
Volume = {41},
Number = {3},
Pages = {460-476},
Month = {JUL},
Abstract = {It is well known that enlistees and veterans in the United States are
   more likely to use alcohol than civilians. However, most of this
   research is potentially biased in that it often does not employ control
   variables (other than age) and is based on cross-sectional data. Much of
   this research also fails to consider the relationship between military
   service and alcohol use among women. Using longitudinal data taken from
   the 1997 National Longitudinal Study of Youth, we investigate the
   relationship between military service and alcohol consumption employing
   a fixed-effects approach. We find that military service appears to
   encourage young men to consume alcohol. It is also the case that the
   effect of military service is not limited to the time that men spend in
   the military given that male veterans are also more likely to consume
   alcohol than are comparable nonveterans. We find, however, that women
   who serve, both enlistees and veterans, are less likely to drink than
   their civilian counterparts.},
