@article{ WOS:000365426500001,
Author = {Potthoff, Richard F. and Munger, Michael C.},
Title = {Condorcet polling can yield serendipitous clues about voter views},
Journal = {PUBLIC CHOICE},
Year = {2015},
Volume = {165},
Number = {1-2},
Pages = {1-12},
Month = {OCT},
Abstract = {Condorcet polling provides additional information about pairwise
   rankings often obscured in standard polls when there are three or more
   candidates. This paper analyzes an original dataset collected from Duke
   University students in North Carolina concerning the 2014 Senate race,
   wherein a Democrat, a Republican, and a Libertarian contested the
   election. The results illustrate that Condorcet polling is feasible in
   such a context, and that the information provided changes the strategic
   calculus of voters in ways that may have a positive impact on the way
   votes are cast and choices considered.},
