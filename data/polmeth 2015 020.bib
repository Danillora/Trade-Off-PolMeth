@article{ WOS:000352698400005,
Author = {Boettke, Peter J. and Marciano, Alain},
Title = {The past, present and future of Virginia Political Economy},
Journal = {PUBLIC CHOICE},
Year = {2015},
Volume = {163},
Number = {1-2, SI},
Pages = {53-65},
Month = {APR},
Abstract = {We present a short history of the Virginia School of Political Economy
   in its institutional settings of University of Virginia (UVA), Virginia
   Polytechnic Institute and State University, or Virginia Tech (VPI), and
   George Mason University (GMU). We discuss the original research and
