@article{ WOS:000356432800005,
Author = {Ceron, Andrea and Memoli, Vincenzo},
Title = {Trust in Government and Media Slant: A Cross-Sectional Analysis of Media
   Effects in Twenty-Seven European Countries},
Journal = {INTERNATIONAL JOURNAL OF PRESS-POLITICS},
