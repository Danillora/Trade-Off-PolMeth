@article{ WOS:000355373700012,
Author = {Chemin, Matthieu and Mbiekop, Flaubert},
Title = {Addressing child sex tourism: The Indian case},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2015},
Volume = {38},
Pages = {169-180},
Month = {JUN},
Abstract = {The paper investigates policy options for a successful fight against
   child sex tourism in a context of increased human mobility inherent to
   the global economy era. Using state and district-level data on
   child-related crimes in India we find that international policy
   coordination is necessary. Coordination involves a combination of
   policies aimed at providing a safer environment for children by removing
   them from the streets (supply-side policies) with those intended to deal
   with child-related offences (demand-side policies). One-standard
   deviation increase in coordination efforts reduces the illegal
   importation of minor girls by 124 standard deviation. (C) 2015 Elsevier
   B.V. All rights reserved.},
