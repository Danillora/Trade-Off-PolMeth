@article{ WOS:000368900000007,
Author = {Millan, Jesus and Cruz Romeo, Ma},
Title = {THE CATHOLIC NATION UNDER LIBERALISM. PERSPECTIVES ON RELIGIOUS UNITY IN
   LIBERAL SPAIN, 1808-1868},
