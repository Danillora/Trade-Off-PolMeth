@article{ WOS:000360759900006,
Author = {Zelko, Frank},
Title = {The Umweltmulti Arrives: Greenpeace and Grass Roots
   Environmentalism in West Germany},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2015},
Volume = {61},
Number = {3, SI},
Pages = {397-413},
Month = {SEP},
Abstract = {From the mid-1970s, Greenpeace began to expand its influence beyond its
   original base in North America. As it did so, it encountered an array of
   environmentalist and social movement cultures in the various countries
   in which it sought to set up branches. This article focusses on the
   establishment of the West German branch in the early 1980s. It analyses
   the friction Greenpeace by then an increasingly professional
   organisation with a hierarchical structure caused when it entered a West
   German environmentalist culture that was committed to grass roots
   democracy and consensus politics.},
