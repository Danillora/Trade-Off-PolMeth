@article{ WOS:000354617200005,
Author = {Kollars, Nina A.},
Title = {War's Horizon: Soldier-Led Adaptation in Iraq and Vietnam},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2015},
Volume = {38},
Number = {4},
Pages = {529-553},
Month = {JUN 7},
Abstract = {Wartime adaptation is a process of adjustment from the war you planned
   for to the one you have. This process of adjustment is done, in part, by
   the practitioners of war in the theater of conflict-soldier-led
   adaptation. Drawing upon two case studies of gun truck development in
   Iraq and Vietnam I argue that soldiers created networks in order to
   adapt to battlefield challenges and that the pattern of those networks
   carries implications for the likelihood of formal adoption by the
   organization. Simply put, the pattern of the flow of ideas, resources,
   and skills across the battlefield may affect the likelihood of bottom-up
   adaptation.},
