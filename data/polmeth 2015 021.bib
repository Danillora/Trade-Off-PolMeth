@article{ WOS:000348900700003,
Author = {Lovett, John and Bevan, Shaun and Baumgartner, Frank R.},
Title = {Popular Presidents Can Affect Congressional Attention, for a Little
   While},
Journal = {POLICY STUDIES JOURNAL},
Year = {2015},
Volume = {43},
Number = {1},
Pages = {22-43},
Month = {FEB},
Abstract = {Does the president have the ability to set the congressional agenda?
   Agenda setting is a prerequisite for influence, so this is an important
   element in understanding presidential-legislative relations. We focus on
   the State of the Union address and show that popular presidents can,
   indeed, cause Congress to shift attention to those topics most
   emphasized. The impact is tempered by divided government and time,
   however. No matter the state of divided government, however, popular
   presidents can direct congressional attention, at least for a little
   while. Unpopular presidents, by contrast, are irrelevant.},
