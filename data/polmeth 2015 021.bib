@article{ WOS:000362905700004,
Author = {Hartig, Falk},
Title = {Communicating China to the World: Confucius Institutes and China's
   Strategic Narratives},
Journal = {POLITICS},
Year = {2015},
Volume = {35},
Number = {3-4, SI},
Pages = {245-258},
Month = {NOV},
Note = {Conference on the Soft Power on Hard States, Newcastle Univ, Newcastle
   upon Tyne, ENGLAND, MAY, 2014},
Abstract = {This article analyses the strategies, goals and impact of the Confucius
   Institutes (CIs). It examines CIs as an important tool in China's public
   diplomacy, employed by its government to communicate specific strategic
   narratives about China to foreign publics, and, in so doing, increase
   China's soft power. Based on empirical data from CIs in different parts
   of the world, the article demonstrates that the impact of CIs is
   hampered both by political/ideological concerns and by a number of
   practical issues. It is argued that CIs do not present the real' China
   to the world, but rather a correct version' of it, which in turn limits
   their ability to project China's strategic narratives effectively and
   increase its soft power.},
