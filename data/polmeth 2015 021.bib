@article{ WOS:000435298000001,
Author = {Bandelj, Nina and Finley, Katelyn and Radu, Bogdan},
Title = {Democracy in Central and Eastern Europe: test of early impact},
Journal = {EAST EUROPEAN POLITICS},
Year = {2015},
Volume = {31},
Number = {2},
Pages = {129-148},
Abstract = {Ten years after European Union (EU) enlargement, democracy in the East
   European new member states is still a topic of discussion. This paper
   focuses on the initial years of democratisation in these countries, and
   asks what economic, political, and social processes explain its
   variation. Cross-sectional time series analysis (1990-2002) shows the
   impact of political orientation of ruling parties to reform, electoral
   institutions, size of ethnic minorities, and the extent of EU
   integration, but not economic development. Further, we disaggregate the
   effect of EU integration and find that early Europe Agreements signing
   has a more significant effect than the pre-accession membership
   negotiation stage.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
