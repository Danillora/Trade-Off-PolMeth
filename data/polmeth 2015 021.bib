@incollection{ WOS:000363928600006,
Author = {Papadaki, Lina},
Editor = {Brooks, T},
Title = {What Is Wrong About Objectification?},
Booktitle = {CURRENT CONTROVERSIES IN POLITICAL PHILOSOPHY},
Series = {Current Controversies in Philosophy},
Year = {2015},
Pages = {87-99},
Abstract = {Objectification is a concept that is hard to define. Until today, there
   has not been a complete and coherent definition of this concept. People
   often use the term `objectification' quite carelessly to refer to a
   variety of cases, which involve seeing and/or treating a person as a
   thing. But what does it mean to see and/or treat a person as a thing?
   This essay will attempt to answer this question, through focusing on
   some prominent thinkers' views on objectification. My aim is, first of
   all, to familiarize the reader with the most influential accounts of
   objectification. Through evaluating the strengths and weaknesses of
   these accounts, I will propose an improved definition of
   objectification. My hope is that this discussion will help the reader
   make up her own mind about what, if anything, is wrong with
   objectification.},
