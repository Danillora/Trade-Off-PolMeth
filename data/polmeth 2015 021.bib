@article{ WOS:000354103600008,
Author = {Binderkrantz, Anne Skorkjaer and Christiansen, Peter Munk},
Title = {From classic to modern corporatism. Interest group representation in
   Danish public committees in 1975 and 2010},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2015},
Volume = {22},
Number = {7},
Pages = {1022-1039},
Month = {AUG 9},
Abstract = {Involving interest groups in public committees is a crucial feature of
   policy-making in many democratic systems. This article investigates how
   the composition of committees in Denmark has developed from 1975 to
   2010, with specific focus on interest group seats. We argue that the
   committee system has been adapted in response to societal changes. We
   expect relatively better representation of citizen groups over time and
   a decline in the level of concentration in access. These expectations
   are tested in an analysis of the composition of all public committees in
   the two years. We find remarkable stability in the 35-year period, but
   also an adaptation of the committee system largely in the expected
   direction. This traditional corporatist institution appears less
   weakened than often assumed.},
