@article{ WOS:000368900000008,
Author = {Shubert, Adrian},
Title = {BEING - AND STAYING - FAMOUS IN 19TH-CENTURY SPAIN: BALDOMERO ESPARTERO
   AND THE BIRTH OF POLITICAL CELEBRITY},
Journal = {HISTORIA Y POLITICA},
Year = {2015},
Number = {34},
Pages = {211-237},
Month = {JUL-DEC},
Abstract = {This article argues that Baldomero Espartero remained widely famous and
   popular for decades after the end of his active political career and his
   political failures. While the initial Espartero cult was created from
   above following his heroic victory at Lu-chana and consolidated after
   the Embrace of Vergara, the maintenance of the cult through the 1860s
   was largely driven from below. The article uses a range of sources: oral
   and family traditions, cheap books and images and, above all letters
   written to Espartero, to explore what was the first case of modern
   political celebrity in Spain.},
Publisher = {CENTRO ESTUDIOS POLITICOS CONSTITUCIONALES},
Address = {PLAZA DE LA MARINA ESPANOLA 9, 28071 MADRID, SPAIN},
Type = {Article},
