@article{ WOS:000350753300001,
Author = {Dommett, Katharine and Flinders, Matthew},
Title = {THE CENTRE STRIKES BACK: META-GOVERNANCE, DELEGATION, AND THE CORE
   EXECUTIVE IN THE UNITED KINGDOM, 2010-14},
Journal = {PUBLIC ADMINISTRATION},
Year = {2015},
Volume = {93},
Number = {1},
Pages = {1-16},
Month = {MAR},
Abstract = {A growing literature on `agencification', `quangocratization', and the
   `autonomization' of the state has highlighted a coordination dilemma in
   contemporary public governance whereby governments rely on delegated
   governance but are frustrated by the lack of control that arises from
   such structures. In the run-up to the 2010 General Election in the
   United Kingdom this coordination dilemma was prominent as the capacity
   of the Cabinet Office to exert control over arm's-length bodies, either
   directly or indirectly, received intense criticism. This article
   presents the findings of the first detailed research project to examine
   the subsequent Coalition Government's approach to this dilemma. It
   argues that in relation to the governance of public bodies, the role and
   capacity of the Cabinet Office has been transformed. In mapping this
   development the article explores the implications of the centre striking
   back in the context of `post-New Public Management' reforms.},
Publisher = {WILEY},
