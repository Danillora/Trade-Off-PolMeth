@article{ WOS:000371468800002,
Author = {Finlay, Andrew},
Title = {Liberal intervention, anthropology and the ethnicity machine},
Journal = {PEACEBUILDING},
Year = {2015},
Volume = {3},
Number = {3},
Pages = {224-237},
Abstract = {This article explores the turn towards anthropology and sociology among
   `critical interventionists', i. e. those who are not opposed to liberal
   intervention as such, but merely worry that actually existing
   intervention tends to fail because it is top-heavy with `Western' ideas
   of `good governance' and lacks an `anthropological sensitivity' to the
   communal nature of the local, indigenous identities and practices
   implicated in conflict. The article problematises the critical
   interventionists' conception of indigenous practice (culture) and
   communal conflict and questions the role assigned by them to the
   anthropologist/sociologist, i. e. as expert in the `communal' and
   translator of indigenous culture to the international purveyors of `good
   governance'. What this division of labour rules out is an
   anthropology/sociology of governance. Using as an illustrative example
   the phenomena of `telling' - a supposedly irenic form of group
   designation indigenous to Northern Ireland that has become foundational
   to the peace process - this article argues that anthropology can help us
   understand communalism not as the antithesis of liberal `good
   governance' but as one of its foremost techniques.},
