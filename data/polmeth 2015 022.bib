@article{ WOS:000353630500005,
Author = {Sutton, Barbara},
Title = {Collective Memory and the Language of Human Rights Attitudes toward
   Torture in Contemporary Argentina},
Journal = {LATIN AMERICAN PERSPECTIVES},
