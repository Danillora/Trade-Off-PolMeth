@article{ WOS:000354664400003,
Author = {Walling, Carrie Booth},
Title = {Human Rights Norms, State Sovereignty, and Humanitarian Intervention},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2015},
Volume = {37},
Number = {2},
Pages = {383-413},
Month = {MAY},
Abstract = {Though increasingly legitimate, humanitarian intervention by the United
   Nations Security Council is selective and rare. This article illustrates
   how the increasing legitimacy of human rights norms is changing the
   meaning of state sovereignty and the purpose of military force at the
   United Nations. By examining Security Council discourse during debates
   about Iraq, Somalia, Rwanda, Bosnia, Kosovo, Sierra Leone, Darfur, and
