@article{ WOS:000368855700001,
Author = {Stepanova, Elena},
Title = {`The Spiritual and Moral Foundation of Civilization in Every Nation for
   Thousands of Years': The Traditional Values Discourse in Russia},
Journal = {POLITICS RELIGION \& IDEOLOGY},
Year = {2015},
Volume = {16},
Number = {2-3},
Pages = {119-136},
Month = {APR 3},
Abstract = {The concept of traditional values' occupies the place of honor in public
   discourse in Russia today being one of the components of the new
   ideological narrative. The article is focused on the strategy of the
   promotion of traditional values by its secular and religious
   protagonists. It includes three thematic sections: (1) the analysis of
   the interpretation of the historical past and its place in the concept
   of traditional values; (2) the investigation of the role of the West' as
   enemy' in constructing national identity and defending traditional
   values; and (3) the examination of the opposition of universality and
   particularism with regard to traditional values. The author concludes
   that the concept of traditional values as a response to social and
   political challenges is promoted by secular powers and religious
   authorities for the ideological consolidation of Russian society, as
   well as for the recruiting potential allies on the international arena.
   At the same time, the noticeable presence of traditional values in
   public discourse indicates the thirst for reliable meanings in the
   process of the search for national identity.},
