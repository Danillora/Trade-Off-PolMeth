@article{ WOS:000212595700005,
Author = {Gomez, Eduardo J.},
Title = {Understanding the BRIC response to AIDS: political institutions, civil
   society, and historical policy backlash in comparative perspective},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2015},
Volume = {53},
Number = {3},
Pages = {315-340},
Abstract = {The BRIC nations (Brazil, Russia, India, and China) have differed in
   their government response to health epidemics. It is argued that Brazil
   eventually outpaced her emerging counterparts in response to AIDS
   (Acquired Immune Deficiency Syndrome) due to the presence of political
   institutional, civil societal, and foreign policy strategies that both
   sustained and encouraged the introduction of innovative policies. The
   concept of historical policy backlash is introduced in order to explain
   how the BRIC' differences in their historic roles as foreign aid donors
   in health shaped their incentive to either focus on domestic AIDS policy
