@article{ WOS:000371468800004,
Author = {Dauphinee, Elizabeth},
Title = {Narrative voice and the limits of peacebuilding: rethinking the politics
   of partiality},
Journal = {PEACEBUILDING},
Year = {2015},
Volume = {3},
Number = {3},
Pages = {261-278},
Abstract = {This article proceeds from the claim that memory and emotion are
   indivisible from practice, experience and history. It argues that an
   enhanced understanding of the impact of war and the subsequent logics of
   peacebuilding require narrative techniques and strategies that re-focus
   our attention on lived experiences. These experiences, however, should
   not be understood as a supplement to academic forms of knowledge, but
   must instead register as legitimate knowledge in their own right. This
   does not mean that they enjoy any fuller veracity than academic
   accounts. Instead, it shows how access to a plurality of knowledge
   provides valuable insights into the limitations of scholarship on
   conflict and its aftermath.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
