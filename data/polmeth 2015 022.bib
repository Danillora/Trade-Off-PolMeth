@article{ WOS:000358414000001,
Author = {Felli, Romain},
Title = {Environment, not planning: the neoliberal depoliticisation of
   environmental policy by means of emissions trading},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2015},
Volume = {24},
Number = {5},
Pages = {641-660},
Month = {SEP 3},
Abstract = {The turn to market-based instruments, such as emissions trading, in
   environmental policy has received considerable attention. Contributing
   to a critical assessment of these instruments by investigating the
   political theory of emissions trading, one of their central mechanisms,
   namely their depoliticising effect, is highlighted by discussing the
   early contributions of neoliberal thinkers and proponents of
   market-based instruments (Hayek, Coase, Dales) in environmental
   governance. These thinkers responded to the growing politicisation of
   environmental limits to economic growth by devising a mechanism by which
   the implementation of these limits could be depoliticised. This ensured
   that the fundamental questions of `what is produced, by whom, and for
   whom' (Hayek) are not raised politically. Emissions-trading mechanisms
   are neoliberal, not in the sense that they commodify or privatise
   nature, but because they entrench the power of investors.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Felli, R (Corresponding Author), Univ Geneva, Dept Polit Sci \& Int Relat, CH-1211 Geneva 4, Switzerland.
   Univ Geneva, Dept Polit Sci \& Int Relat, CH-1211 Geneva 4, Switzerland.},
DOI = {10.1080/09644016.2015.1051323},
ISSN = {0964-4016},
EISSN = {1743-8934},
