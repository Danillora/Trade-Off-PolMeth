@article{ WOS:000356199400005,
Author = {Restrepo Echavarria, Ricardo},
Title = {The Emergence of the Constitutional Democracy of Rights and Justice in
   Ecuador},
Journal = {REVISTA DEL CLAD REFORMA Y DEMOCRACIA},
Year = {2015},
Number = {61},
Pages = {133+},
Month = {FEB},
Abstract = {This article intends to develop the content of the theory of a rights
   and justice-centered constitutional democracy, as well as show its
   utility, by using it to explain the new stability of the Ecuadorian
   State, after its period of neoliberal minimalist democracy, born with
   the death of Jaime Roldos in 1981.
   The document shows the correlation between the installation of a State
   characterized by neoliberal minimalist democracy, highly damaging to
   rights and justice, and State instability. With the emergence of the new
   constitutional framework of 2008, propelled by the government of the
   Citizens' Revolution, as an alternative for constructing a democratic
   State centered on rights and justice, the Ecuadorian State has
   experienced a remarkable new stability. The new State stability is
   explained by the emergence of a clear alternative elected and with high
   informed approval by the people. This alternative is identified by the
   major public policies employed, especially the economic and social ones,
   and their effects.},
