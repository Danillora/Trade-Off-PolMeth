@article{ WOS:000358163200002,
Author = {Dwyer, Maggie},
Title = {PEACEKEEPING ABROAD, TROUBLE MAKING AT HOME: MUTINIES IN WEST AFRICA},
Journal = {AFRICAN AFFAIRS},
Year = {2015},
Volume = {114},
Number = {455},
Pages = {206-225},
Month = {APR},
Abstract = {This article draws attention to a trend in which military deployments as
   part of peacekeeping missions have triggered army mutinies in some West
   African countries. It explains how participation in peacekeeping
   missions created new material grievances and a sense of injustice
   amongst the peacekeepers, which under certain conditions sparked
   domestic mutinies. These uprisings in West Africa follow a history of
   military disobedience in the region, and the article places them in the
   context of long-standing tensions within military organizations.
   Mutinies often symbolize and intensify divisions within armed forces,
   which can lead to further instability even after the mutiny is resolved.
   Therefore, it is important for those interested in building and
   maintaining effective militaries to understand the ways in which
   deployments and peacekeeping participation can contribute to unrest
   within the armed forces. The article draws on interviews with former
   mutineers, including peacekeepers, and others military personnel in West
   Africa, as well as media reporting, including public statements made by
   mutineers, academic writing, and archival research.},
