@article{ WOS:000364955100004,
Author = {Cooper, Ian},
Title = {A yellow card for the striker: national parliaments and the defeat of EU
   legislation on the right to strike},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
