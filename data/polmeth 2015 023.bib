   Ammons, David N.; Roenigk, Dale J., Univ N Carolina, Chapel Hill, NC 27515 USA.},
DOI = {10.1093/jopart/muu014},
ISSN = {1053-1858},
EISSN = {1477-9803},
Keywords-Plus = {ORGANIZATIONAL ROUTINES; KNOWLEDGE; MANAGEMENT; CAPACITY; SERVICES;
