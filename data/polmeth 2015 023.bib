@article{ WOS:000363052300005,
Author = {Blunt, Gwilym David},
Title = {Is global poverty a crime against humanity?},
Journal = {INTERNATIONAL THEORY},
Year = {2015},
Volume = {7},
Number = {3},
Pages = {539-571},
Month = {NOV},
Abstract = {Pogge has repeatedly compared the causes of global poverty with
   historical crimes against humanity. This claim, however, has been
   treated as mere rhetoric. This article argues that there are good
   reasons to take it seriously. It does this by comparing Pogge's thesis
   on the causes of global poverty with the baseline definition of crimes
   against humanity found in international law, especially the Rome Statute
   of the International Criminal Court. It argues that the causes of global
   poverty are comparable with the crimes of slavery and apartheid. This
   has important consequences for cosmopolitan thought, as it makes the
   need for practical solutions to global poverty more urgent and raises
   questions about the global poor's right to resist the international
   system by violent means.},
