@article{ WOS:000349993100005,
Author = {Haidar, Julieta},
Title = {Interpreting Argentine Business Unionism},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2015},
Volume = {42},
Number = {2, 2},
Pages = {60-73},
Month = {MAR},
Abstract = {Institutionalist and corporatist interpretations of the business
   unionism strategy adopted by some unions in Argentina when faced with
   the 1990s market reforms are inadequate in that they consider unions as
   interest groups, play down the context of capitalist restructuring, and
   fail to account for the novelty of the strategy. These limitations can
   be overcome by reviving some ideas from Marxist analysis, such as the
   classist nature of unions and their participation in the historical
