@article{ WOS:000356199400001,
Author = {Ramio, Carles},
Title = {Reactionary Tensors to Discourage and Progressive Tensors to Encourage
   the Public Institutionalization and Innovation in Latin America},
Journal = {REVISTA DEL CLAD REFORMA Y DEMOCRACIA},
Year = {2015},
Number = {61},
Pages = {5+},
Month = {FEB},
Abstract = {The objective of this paper is to provide a set of theoretical and
   practical reflections to analyze the current situation and to point out
   future proposals about the institutionalization of public
   administrations and the public sector innovation in Latin America. More
   precisely, we can distinct two different objectives. Firstly, by way of
   diagnosis, expose the reactionary tensors that, in recent years, have
   been hindered and even aborted the institutionalization of public
   administrations and the public sector innovation in Latin America.
   Secondly, by way of future proposals, present a set of progressive
   tensors which are necessary to stimulate and achieve public
   administrations that combine greater institutional strength with higher
   capacity for innovation in the definition and management of their
   policies.
   The challenge is to move towards public administrations with higher
   institutional capacities to provide more institutional and legal
   security to Latin American societies and with superior innovation
   capabilities to meet the demands and requirements of modernity.},
