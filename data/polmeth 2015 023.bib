@article{ WOS:000352951600009,
Author = {Gstohl, Sieglinde},
Title = {Models of external differentiation in the EU's neighbourhood: an
   expanding economic community?},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2015},
Volume = {22},
Number = {6, SI},
Pages = {854-870},
Month = {JUL 3},
Abstract = {The neighbouring countries of the European Union (EU) have gradually
   developed `a stake' in its internal market. This contribution asks to
   what extent and how the EU is expanding its economic community based on
   an analysis of the different neighbourhood models of deep economic
   integration, including current discussions about their future
   development. It shows that the EU's neighbourhood relations range from
   narrow, bilateral, static models to broad, multilateral, dynamic models
   and that a shift in this direction can be observed over time. The EU
   increasingly attempts to ensure market homogeneity beyond its borders by
   concluding agreements which include a dynamic adaptation to the evolving
   relevant acquis, its uniform interpretation as well as an independent
   surveillance and judicial enforcement.},
