@article{ WOS:000367345400002,
Author = {Curini, Luigi},
Title = {An Italian Leitmotiv? Corruption and Competence in the Debates of the
   Italian Chamber of Deputies (1946-2014)},
Journal = {SOUTH EUROPEAN SOCIETY AND POLITICS},
Year = {2015},
Volume = {20},
Number = {4},
Pages = {509-531},
Month = {OCT 2},
Abstract = {By making use of an original data-set built based on a codification of
   all investiture debates of the Italian governments from 1946 to 2014,
   the paper investigates the main factors that explain the choice of a
   party to devote its attention to the valence issues of corruption and
   competence in its legislative speeches. Two classes of hypotheses are
   tested; the first concentrates on spatial reasons, and the second
   concentrates on contextual factors. Both sets of factors appear to play
   a significant role, although no clear temporal trend emerges in party
   attention over almost seventyyears of Italian parliamentary debates.},
