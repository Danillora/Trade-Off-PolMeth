@incollection{ WOS:000361324000003,
Author = {Hill, Rickey},
Editor = {Cunnigen, D and Bruce, M},
Title = {THE RACE PROBLEMATIC IN THE AGE OF BARACK OBAMA},
Booktitle = {RACE IN THE AGE OF OBAMA, PT 2},
