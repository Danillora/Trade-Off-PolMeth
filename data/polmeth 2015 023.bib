@article{ WOS:000348858200001,
Author = {Dommett, Katharine and Flinders, Matthew},
Title = {The politics of quangocide},
Journal = {POLICY AND POLITICS},
Year = {2015},
Volume = {43},
Number = {1},
Pages = {3-25},
Month = {JAN},
Abstract = {This article returns to Christopher Hood's influential work, The
   politics of quangocide, to examine the United Kingdom's coalition
   government's approach to public bodies reform since May 2010. It
   combines theoretical-innovation and fresh empirical research to argue
   that the coalition has not simply engaged in quangocide, but has adopted
   a dual-track strategy based upon `bureau-shuffling' and a focus on
   strengthening internal control rather than outright abolition. This has
   significant international and comparative relevance due to the manner in
   which these findings resonate with broader `post New Public Management'
   narratives of state restructuring.},
Publisher = {POLICY PRESS},
