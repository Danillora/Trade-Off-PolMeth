@article{ WOS:000353385300008,
Author = {de Francisco, Jean Carlo Rodriguez and Boelens, Rutgerd},
Title = {Payment for Environmental Services: mobilising an epistemic community to
   construct dominant policy},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2015},
Volume = {24},
Number = {3},
Pages = {481-500},
Month = {MAY 4},
Abstract = {The alleged capacity of Payment for Environmental Services (PES) to
   reach conservation policy goals, while reducing poverty in a
   cost-effective manner, makes it an extremely attractive development
   instrument for policymakers and international funding agencies. This
   article reconstructs the process of envisioning and building the
   National PES Strategy in Colombia. It reveals how this conservation
   policy has resulted from the mobilisation of the transnational/national
   PES epistemic community and its globally expanding discourse. The
   influential PES network generates internally defined standards of
   success that proceed without reference to empirical evidence as to the
   impacts of the implemented policies. PES adoption is influenced by
   regulatory instruments' unsatisfactory outcomes, the ways in which
   market-environmentalist models induce profound indifference towards
   on-the-ground policy impacts, the discursive power and alignment
   properties of the PES policy epistemic community, and financial and
   political pressures by international banks and environmental NGOs.},
