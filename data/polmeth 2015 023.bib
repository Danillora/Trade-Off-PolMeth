@article{ WOS:000361560400004,
Author = {Ozdemir, Yonca},
Title = {Political conditions for successful inflation stabilization: comparing
   Brazil and Argentina},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2015},
Volume = {58},
Number = {1},
Pages = {63-83},
Abstract = {This study tries to understand how Brazil and Argentina, two countries
   with chronically high inflation, achieved permanent stabilization by
   looking at political factors necessary for stabilization. It suggests
   that, although state autonomy or political unity may contribute to
   successful price stabilization, reconciling the interests of different
   stakeholders and thereby building a broad political support base is more
   essential for stabilization to succeed. Political skills of the leaders
   and compensatory policies may help raise such support and they are more
   crucial where other political conditions are less favorable. Also, rapid
   improvement in the economy is important for sustained political support,
   and thus, for successful stabilization.},
