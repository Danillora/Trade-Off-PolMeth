@article{ WOS:000212739100011,
Author = {Peleg, Ilan},
Title = {Self-determination and Majority-Minority Relations in Deeply Divided
   Societies: Towards a Comparative Analytical Framework},
Journal = {ETHNOPOLITICS},
Year = {2015},
Volume = {14},
Number = {5, SI},
Pages = {523-530},
Abstract = {The focus of this article is on the role played by self-determination as
   a legitimizing principle in the contemporary international system. More
   specifically, the interest of this article is in examining the concept
   of self-determination as potentially regulating-positively or
   negatively-the complex relations between majorities and minorities in
   deeply divided societies and especially political systems that are
   dominated by the identity politics of several opposing groups. There is
   a need to, first, clarify the basic concepts of this article and the way
   in which they are used. Of particular interest are the notions of
   self-determination, majority-minority relations, and deeply divided
   societies. Once this is done, the article will introduce an analytical
   framework, a way of systematically thinking about self-determination in
   the context of conflict within deeply divided societies, with the hope
   that such an analytical framework might effectively facilitate the study
   of the complex interrelationships between groups in conflictual
   societies.},
