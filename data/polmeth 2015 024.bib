@article{ WOS:000369919200006,
Author = {Brooks, John},
Title = {Preparing for Armageddon: Gunnery Practices and Exercises in the Grand
   Fleet Prior to Jutland},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2015},
Volume = {38},
Number = {7, SI},
Pages = {1006-1023},
Abstract = {The article examines for the pre-First World War period: the
   developments in gunnery techniques; the gunnery practices and especially
   the firing ranges; and the tactical games and exercises conducted in the
   Grand Fleet, with their emphasis on the perceived threats from mines
   dropped from destroyers and from torpedoes from submarines. It
   concludes, contrary to Professor Sumida's currently accepted
   interpretation, that at no time was `the development and maintenance of
   the capability to outfight the German battle fleet at medium range. the
   primary objective of British tactical preparation'.},
