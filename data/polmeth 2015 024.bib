@article{ WOS:000367352400001,
Author = {Stone, Diane and Ladi, Stella},
Title = {GLOBAL PUBLIC POLICY AND TRANSNATIONAL ADMINISTRATION},
Journal = {PUBLIC ADMINISTRATION},
