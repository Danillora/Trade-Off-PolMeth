@article{ WOS:000361485800002,
Author = {Harkness, Kristen A. and Hunzeker, Michael},
Title = {Military Maladaptation: Counterinsurgency and the Politics of Failure},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2015},
Volume = {38},
Number = {6},
Pages = {777-800},
Month = {SEP 19},
Abstract = {Tactical learning is critical to battlefield success, especially in a
   counterinsurgency. This article tests the existing model of military
   adaption against a most-likely' case: the British Army's
   counterinsurgency in the Southern Cameroons (1960-61). Despite meeting
   all preconditions thought to enable adaptation - decentralization,
   leadership turnover, supportive leadership, poor organizational memory,
   feedback loops, and a clear threat - the British still failed to adapt.
   Archival evidence suggests politicians subverted bottom-up adaptation,
   because winning came at too high a price in terms of Britain's broader
   strategic imperatives. Our finding identifies an important gap in the
   extant adaptation literature: it ignores politics.},
