@incollection{ WOS:000348592600003,
Author = {Furlong, Paul},
Editor = {BalossiRestelli, LM and Whitman, RG and Edwards, G},
Title = {Processes of Italian foreign policy since 1992},
Booktitle = {ITALY'S FOREIGN POLICY IN THE TWENTY-FIRST CENTURY: A CONTESTED NATURE?},
Series = {Routledge Advances in European Politics},
Year = {2015},
Volume = {110},
Pages = {23-40},
Abstract = {This chapter considers foreign policy in Italy as a function of the
   domestic policy process as well as of the external and internal systemic
   conditioning with which it is usually associated. The argument locates
   foreign policy prior to 1992 within a framework characterised by a clear
   dichotomy between substantive and partisan policy, in which foreign
   policy occupied a unique position because of the importance of the
   vincolo esterno and the paucity of partisan political resources
   available to it. The chapter analyses the changes to the role of foreign
   policy within the broader policy framework since 1992, arguing that the
   greater emphasis on substantive policy throughout has assimilated
   foreign policy closer to the norm, but that the tendency of the
   Berlusconi governments in particular to more privatised forms of
   policy-making in this area have mitigated the effect.},
