@article{ WOS:000352824100005,
Author = {Umoh, Ubong Essien},
Title = {Cameroon, Nigeria and the Bakassi conflict: building blocks for a
   Non-Democratic Peace Theory},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2015},
Volume = {18},
Number = {2},
Pages = {227-247},
Month = {APR},
Abstract = {The Democratic Peace Theory, which argues in part that democracies
   hardly go to war against each other, has made a lasting impression in
   the field of war and peace studies. While the theory has been mostly
   applied to the `peace zones' of the Global North (made up of stable
   democracies and where war is `unthinkable'), the `conflict zones' of the
   Global South (a significant part of which Africa is made of) has been
   left in relative theoretical neglect, as it relates to peaceful
   interstate relations. The Nigeria Cameroon crisis over the oil-rich
   Balcassi Peninsula provides a sensitive case study in the Global South
   for laying a foundation for a Non-Democratic Peace Theory (NDPT). Why
   did the Nigeria Cameroon conflict over the strategic oil-rich Bakassi
