@article{ WOS:000363871700004,
Author = {Douglas, James W. and Raudla, Ringa and Hartley, Roger E.},
Title = {Shifting Constellations of Actors and Their Influence on Policy
   Diffusion: A Study of the Diffusion of Drug Courts},
Journal = {POLICY STUDIES JOURNAL},
Year = {2015},
Volume = {43},
Number = {4},
Pages = {484-511},
Month = {NOV},
Abstract = {The existing studies of policy diffusion have paid insufficient
   attention to how policy actors in the macroenvironment of local
   jurisdictions influence the process of policy diffusion, what kind of
   mechanisms drive the interdependent spread of a policy, and how the
   diffusion process changes over time. We explore, both theoretically and
   empirically, the role different go-betweens can play in policy
   diffusion, what kind of interactions take place between the micro level
   and macro level, and how shifts in the constellations of actors in the
   macroenvironment of a local jurisdiction can influence the mechanisms
   and rate of policy diffusion. Our empirical study of the diffusion of
   drug courts in four U.S. states demonstrates that there can indeed be
   significant interactions between the micro level and the macro level and
   that these interactions are likely to shape the diffusion process.},
