@article{ WOS:000359838100004,
Author = {Karlborg, Lisa},
Title = {The Ambiguous Host-Citizen Contract: An Evolving Notion of Duty in the
   U.S. Military Quest for Local Legitimacy},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2015},
Volume = {38},
Number = {10},
Pages = {864-884},
Month = {OCT 3},
Abstract = {The article explores how the recent quest for local legitimacy in Iraq
   and Afghanistan has shaped the U.S. military notion of duty toward host
   citizens. It argues that military duty is conceptualized as a
   ``host-citizen contract.{''} Based on a qualitative comparison of the
   2006 and 2014 versions of FM3-24, the U.S. counterinsurgency field
   manual, it finds that U.S. forces are obligated to suppress insurgents,
   build host-nation agency, and protect the host population in exchange
   for legitimacy. The article's main finding is that the notion of
   legitimacy has changed in ways that fundamentally limit the scope of
   duty and justify a breach of contract should the host nation fail to
   comply.},
