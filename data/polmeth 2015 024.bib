@incollection{ WOS:000348592600008,
Author = {della Rocca, Roberto Morozzo},
Editor = {BalossiRestelli, LM and Whitman, RG and Edwards, G},
Title = {Sant'Egidio's diplomacy in crisis areas},
Booktitle = {ITALY'S FOREIGN POLICY IN THE TWENTY-FIRST CENTURY: A CONTESTED NATURE?},
Series = {Routledge Advances in European Politics},
Year = {2015},
Volume = {110},
Pages = {130-147},
Abstract = {This chapter explores the distinctive peace-mediation features
   characterising the Comunita di Sant'Egidio, whose headquarters are in
   Rome. This community (not NGO) consists of a worldwide network of
   Christian groups, located in 70 different countries. Its approach to
   conflicts and crisis areas consists primarily of seeking to understand
   the divergences directly on the ground, the claims of the `war-makers',
   and the problems which led to the violence. Its approach portrays the
   community's typical choice when dealing with crises without following
   rigidly pre-established and fixed action-plans. This process entails a
   much wanted interpretation, both political and psychological, of the
   challenges to be faced locally. It implies reconciliation, and its
   handling requires time, dialogue, building trust in the mediators and
   shaping a common language among the conflicting parties. The pursuit of
   uniting the opposing sides is more relevant to Sant'Egidio than
   criminalising one of the parties. Criminalising would prioritise the use
   of arms over political negotiation. This chapter offers a concise
   outline of the main cases where a Sant'Egidio peace-facilitator has
   impacted on the ground. Guatemala, Algeria, Kosovo and Burundi, among
   others, are included in the peacemaking framework that will be
   highlighted.},
