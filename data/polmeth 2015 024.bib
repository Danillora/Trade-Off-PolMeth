@incollection{ WOS:000386203900004,
Author = {Megas, Achilleas},
Book-Author = {Megas, A},
Title = {Standing Policy Towards the GDR},
Booktitle = {SOVIET FOREIGN POLICY TOWARDS EAST GERMANY},
Series = {Contributions to Political Science},
Year = {2015},
Pages = {35-58},
Abstract = {The Soviet political elite perceived the German question to be an
   immutable issue embedded in the threat constituted by German militarism,
   which forced them in the early years of the Cold War to implement
   significant measures against Western imperialism. The Soviet leadership
   showed an unpredictably flexible policy towards German unification,
   since they believed that the consolidation of the socialist GDR might
   not be favouring Soviet interests. Rolf Steininger and Rund Van Dijk
   have stressed that the period of interregnum was the first and the last
   chance for German unification before Gorbachev's advent. Then, in June
   1953 the East German Politburo's announcement of the `New Course'
   explicitly opposed the early attempt to construct a socialist GDR as
   quickly as possible. It is important to ask how contested Soviet policy
   towards the German question was reflected in the lack of a strategic
   concept in the Soviet military, and why the interests of the state were
   hard to separate from the perception of the political elite interests
   themselves.},
