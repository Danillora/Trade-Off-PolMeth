@article{ WOS:000435299200003,
Author = {Park, Ausra},
Title = {A society of departure in post-communist Europe: socio-political reasons
   behind mass Lithuanian emigration},
Journal = {EAST EUROPEAN POLITICS},
Year = {2015},
Volume = {31},
Number = {4},
Pages = {407-428},
Abstract = {Migration occurs among all states, even under ``normal{''} political
   conditions. When traumatic changes unfold, migration usually increases
   dramatically. Although all post-communist states experienced significant
   migration outflows since the 1990s, Lithuania stands as an anomaly among
   EU member states, exhibiting the net outmigration rates of -25.2\% in
   2010 and - 12.6\% in 2011, which is significantly higher than other
   post-communist emigrant-states. Situating this article within migration
   literature debates, and by testing the explanatory powers of migration
   theories on the selected case, understudied political and social causes
   that also impact Lithuania's abnormally high emigration rates are
   closely examined.},
