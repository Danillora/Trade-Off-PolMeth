@article{ WOS:000409510300002,
Author = {Ozcan, Gul Berna and Gunduz, Umut},
Title = {Political connectedness and business performance: evidence from Turkish
   industry rankings},
Journal = {BUSINESS AND POLITICS},
Year = {2015},
Volume = {17},
Number = {1},
Pages = {41-73},
Month = {APR},
Abstract = {This paper examines the degree to which political connections affect
   business rankings through a statistical analysis of Turkey's industry
   rankings between 2003 and 2011. The analysis demonstrates that business
   performance is associated with connectedness through industry and firm
   level data. We show that political connectedness varies according to the
   firm's channel of access to obtain favouritism either through direct
   personal ties or institutional networks. Ideological motivations emerge
   to be significant in mobilizing, shaping and tying firm behaviour to
   broader political agendas. In the conclusion we discuss the impact of
   deepening connectedness on long-term business fortunes and political
   institutions.},
