@article{ WOS:000358068300001,
Author = {Jentzsch, Corinna and Kalyvas, Stathis N. and Schubiger, Livia Isabella},
Title = {Militias in Civil Wars},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
