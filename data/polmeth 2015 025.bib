@article{ WOS:000435297500006,
Author = {Jarabinsky, Ivan},
Title = {Appearances are deceptive: credibility of the Russian Election
   Commissions},
Journal = {EAST EUROPEAN POLITICS},
Year = {2015},
Volume = {31},
Number = {1},
Pages = {88-103},
Abstract = {This text focuses on the Russian Electoral Commission (EC) and its
   performance during the Russian parliamentary elections in 2011. The
   three-step analysis consists of (1) the EC's role and organisational
   structure; (2) a description of the complaints over the EC's performance
   and (3) a detailed focus on the EC's decisions on these complaints'
   relevance. Quantitative content analysis is used to analyse the original
   data and a new forensic method is developed to look for potential biases
   in the EC's decisions on raised complaints. Evidence indicate loss of
   professionalism, dependence, and biased behaviour. Therefore the EC
   lacks its credibility.},
