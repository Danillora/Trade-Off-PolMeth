@article{ WOS:000363250600004,
Author = {Mehiriz, Kaddour},
Title = {The Influence of Redistributive Politics on the Decision Making of
   Quasi-autonomous Organizations. The Case of
   Infrastructures-Transport (Quebec-Canada)},
Journal = {JOURNAL OF PUBLIC ADMINISTRATION RESEARCH AND THEORY},
Year = {2015},
Volume = {25},
Number = {4},
Pages = {1081-1098},
Month = {AUG},
Abstract = {This article presents the findings of a study on the influence of
   redistributive politics on grant distribution of a municipal
   infrastructure funding program managed by a quasi-autonomous
   organization. The study indicates that grant distribution among the
   municipalities is not exempt from redistributive politics.
   Municipalities located in swing districts and those represented by
   members from the party in power or by members having more seniority have
   a greater propensity to receive grants than other municipalities. The
   study thus casts doubt on one of the main claims of the proponents of
   quasi-autonomous organizations according to which the decision making of
   these bodies is exempt from partisan politics.},
