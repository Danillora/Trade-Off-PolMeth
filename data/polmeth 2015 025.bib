@article{ WOS:000363871700003,
Author = {Zhu, Ling and Xu, Ping},
Title = {The Politics of Welfare Exclusion: Immigration and Disparity in Medicaid
   Coverage},
Journal = {POLICY STUDIES JOURNAL},
Year = {2015},
Volume = {43},
Number = {4},
Pages = {456-483},
Month = {NOV},
Abstract = {The rapid growth of the immigrant population in the United States, along
   with changes in the demographics and the political landscape, has often
   raised questions for understanding trends of inequality. Important
   issues that have received little scholarly attention thus far are
   excluding immigrants' social rights through decisive policy choices and
   the distributive consequences of such exclusive policies. In this
   article, we examine how immigration and state policies on immigrants'
   access to safety net programs together influence social inequality in
   the context of health care. We analyze the combined effect of
   immigration population density and state immigrant Medicaid eligibility
   rules on the gap of Medicaid coverage rates between native- and
   foreign-born populations. When tracking inequality in Medicaid coverage
