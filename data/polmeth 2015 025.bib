@article{ WOS:000348858200008,
Author = {Durose, Catherine and Justice, Jonathan and Skelcher, Chris},
Title = {Governing at arm's length: eroding or enhancing democracy?},
Journal = {POLICY AND POLITICS},
Year = {2015},
Volume = {43},
Number = {1},
Pages = {137-153},
Month = {JAN},
Abstract = {This review article debates the democratic consequences of arm's length
   governance challenging the prevailing view that its use is necessarily
   counter-democratic and a poor substitute for direct control by elected
   politicians. The article explores the roots of the dominant `democratic
   deficit' perspective on arm's length bodies in the agency problems
   generated by political delegation, but also addresses the potential for
   democratic enhancement posed by the different theoretical lens of
   polycentrism. The article considers the conditions that are necessary to
   activate citizens to engage with arm's length governance. Our conclusion
   develops the normative implications for the design of public governance.},
