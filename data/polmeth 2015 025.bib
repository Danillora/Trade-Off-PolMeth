@incollection{ WOS:000387938900003,
Author = {Onderco, Michal},
Book-Author = {Onderco, M},
Title = {India},
Booktitle = {IRAN'S NUCLEAR PROGRAM AND THE GLOBAL SOUTH: THE FOREIGN POLICY OF
   INDIA, BRAZIL, AND SOUTH AFRICA},
Series = {Palgrave Pivot},
Year = {2015},
Pages = {25-42},
Abstract = {Faced with Iran's nuclear program in its strategic backyard, India's
   response has been cautious and marked by attempts to appease both the
   United States and Iran. As such, it was received with dismay in both the
   United States as well as in Iran.
   Indian officials have been vocal on their opposition towards any new
   nuclear powers in their region. On the other hand, India has tried to
   maintain a working business relation with Iran and was not eager to
   implement any sanctions above the bare minimum.
   India's position towards Iran has been driven by the same forces as have
   been driving it since its reorientation in 1990s. In the light of the
   uncertain results of the security rebalancing in the region with the
   view towards advancing the goals of domestic economic development, India
   tried to advance multiple strategies.},
