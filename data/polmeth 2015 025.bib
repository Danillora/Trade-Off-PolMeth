@article{ WOS:000348537600004,
Author = {Ayson, Robert and Leah, Christine M.},
Title = {Missile Strategy in a Post-Nuclear Age},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2015},
Volume = {38},
Number = {1-2},
Pages = {74-97},
Month = {JAN 2},
Abstract = {While the arrival of nuclear weapons coincided roughly with the
   development of short, medium, intermediate, and eventually
   intercontinental missiles, the contribution of missile technology to the
   deterrence equation is often lost. If nuclear weapons were eliminated,
   even new generation missiles with conventional payloads could struggle
   to render effective deterrence. But some of the physical and
   psychological effects commonly ascribed to nuclear weapons could still
   be in play. And in a world without nuclear weapons, thinking about the
   use and control of force from the nuclear age would also deserve renewed
   attention.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
