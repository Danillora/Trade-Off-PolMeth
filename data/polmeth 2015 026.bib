@article{ WOS:000363909500007,
Author = {Poniatowicz, Marzanna and Wyszkowska, Dorota},
Title = {Dysfunction of Fiscal Equalization Mechanisms in the System of Local
   Government Finances in Poland},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2015},
Volume = {13},
Number = {4},
Pages = {1019-1045},
Month = {OCT},
Abstract = {The main objective of this paper is to determine the substance of
   equalization systems and the causes and effects of their use in local
   finances, as well as the analysis of the differences of existing
   equalization mechanisms in selected countries. However, the specific
   objective is the identification of the key drawbacks and dysfunctions of
   the equalization system in Poland and to propose specific
   recommendations in the context of introducing necessary reforms. The
   authors of the paper present the view that firstly, actions should be
   undertaken to strengthen the financial autonomy of Polish local
   government entities, and only later, as a result, build a new
   equalization system adapted to the level of revenue and expenditure
   independence and different expenditure needs of local government in
   Poland.},
