@article{ WOS:000364333600001,
Author = {Hellmuth, Dorle},
Title = {Countering Jihadi Terrorists and Radicals the French Way},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2015},
Volume = {38},
Number = {12},
Pages = {979-997},
Month = {DEC 2},
Abstract = {The 2015 Charlie Hebdo attacks warrant a detailed profile of the French
   approach to countering Jihadi terrorism. Much has been written about the
   tough French counterterrorism regime, which originated in 1986 and
   remains unique among Western democracies. There has been less analysis
   of France's lengthy list of post-9/11 reforms, and even less discussion
   of the French approach to counterradicalization. In fact, France was
   among only few European countries that did not engage in any soft
   counterradicalization programs after the 2004 Madrid and the 2005 London
   bombings. The mass exodus of foreign fighters to Syria led to a first
   national counterradicalization plan in 2014. In response to the Paris
   attacks, much in line with its security-oriented methods and outlook,
   the French government increased counterterrorism spending and
   surveillance powers. Various other measures are noteworthy, however, as
   they focus on prison radicalization and represent an effort to
   strengthen the counterradicalization campaign.},
