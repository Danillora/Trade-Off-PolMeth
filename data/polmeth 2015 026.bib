@article{ WOS:000348306700005,
Author = {Lopreite, Debora},
Title = {Gender Policies in Argentina after Neoliberalism Opportunities and
   Obstacles for Women's Rights},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2015},
Volume = {42},
Number = {1, 1},
Pages = {64-73},
Month = {JAN},
Abstract = {After a decade of neoliberalism under the social-conservative
   administration of Carlos Menem (1989-1999), with its negative effects on
   women's rights, the 2001-2002 economic crises created new opportunities
   for women. The Menem administration initiated a gender-equality agenda
   with its quota for women candidates for the Congress and its adoption of
   the UN Convention on the Elimination of All Forms of Discrimination
   against Women, but it clashed with women's groups on reproductive
   rights. While the two Kirchner administrations (2003-present)
   established programs for sexual health and responsible procreation, they
   reverted to maternalism in their programs for social assistance for poor
   mothers and family support. The Consejo Nacional de la Mujer (National
   Women's Council), created during the Menem administration, took on more
   responsibility for delivering social assistance but abandoned its
   original gender-equality mandate.},
