@article{ WOS:000360850300005,
Author = {Normann, Hans-Theo and Rau, Holger A.},
Title = {Simultaneous and Sequential Contributions to Step-level Public Goods:
   One versus Two Provision Levels},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2015},
Volume = {59},
Number = {7},
Pages = {1273-1300},
Month = {OCT},
Abstract = {In a step-level public-good experiment, we investigate how the order of
   moves (simultaneous vs. sequential) and the number of step levels (one
   vs. two) affects public-good provision in a two-player game. We find
   that the sequential order of moves significantly improves public-good
   provision and payoffs, even though second movers often punish first
   movers who give less than half of the threshold contribution. The
   additional second step levelwhich is not feasible in standard Nash
   equilibriumleads to higher contributions but does not improve
   public-good provision and lowers payoffs. We calibrate the parameters of
   Fehr and Schmidt's model of inequality aversion to make quantitative
   predictions. We find that actual behavior fits remarkably well with
   several predictions in a quantitative sense.},
