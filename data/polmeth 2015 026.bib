@incollection{ WOS:000348592600010,
Author = {Chelotti, Nicola and Johansson-Nogues, Elisabeth},
Editor = {BalossiRestelli, LM and Whitman, RG and Edwards, G},
Title = {Stable unpredictability? An assessment of Italian-Libyan relations},
Booktitle = {ITALY'S FOREIGN POLICY IN THE TWENTY-FIRST CENTURY: A CONTESTED NATURE?},
Series = {Routledge Advances in European Politics},
Year = {2015},
Volume = {110},
Pages = {178-196},
Abstract = {Lacking a European multilateral framework, Italy has constantly sought
   to build stable relations with Libya through bilateral agreements and
   contacts. For security and migration issues, economic and energetic
   interests, Libya is a key strategic partner for Italian policy-makers.
   However, in spite of Italian diplomatic efforts and the different
   upgraded bilateral accords since the early 1990s, a number of incidents
   testify to that Rome's much vaunted stability in its Libya relations
   continues to be quite elusive. The chapter employs a neoliberal
   institutional perspective and concepts such as free-riding, asymmetry
   and game of chicken to analyse the Italian-Libyan relations and to show
   how incentives to reassess, defer and delay agreements, and engage in
   verbal stand-offs exist for Libyan authorities. As a result, there is an
   underlying pattern of unpredictability about relations between the two
   countries which has characterised the Qaddafi rule and to some extent
   appears to rear its head even in the Arab Spring and post-Qaddafi
   scenario.},
