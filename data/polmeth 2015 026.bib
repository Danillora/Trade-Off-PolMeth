@article{ WOS:000359506700008,
Author = {Kirschbaum, Charles},
Title = {Shifts in control disciplines and rescaling as a response to network
   governance failure: the BCJ case, Brazil},
Journal = {POLICY AND POLITICS},
Year = {2015},
Volume = {43},
Number = {3, SI},
Pages = {443-458},
Month = {JUL},
Abstract = {I suggest that rescaling might be a response to network failure. In
   order to describe a network's modes of governance, I borrow the idea of
   `disciplines of control'. This framework is applied to the Brazil Grows
   Together (BCJ) case, a policy aimed at pregnant women and early
   childhood care in a state in Brazil. I show that constituency pressures
   at state level led to rescaling, and higher selectiveness. Subsequently,
   network managers steered vertical communication, and promoted broader
   inclusion of stakeholders. I conclude the article by suggesting general
   mechanisms that govern rescaling within the network governance
   framework.},
