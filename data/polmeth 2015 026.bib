@article{ WOS:000212733300002,
Author = {Kipgen, Nehginpao},
Title = {Ethnicity in Myanmar and its Importance to the Success of Democracy},
Journal = {ETHNOPOLITICS},
Year = {2015},
Volume = {14},
Number = {1},
Pages = {19-31},
Abstract = {The article examines the historically rooted conflicts between the
   Myanmar government and ethnic minorities with reference to the case of
   Kachin and Karen ethnic groups. Since the country's independence in
   1948, the central government in Rangoon (now Yangon) has attempted
   unsuccessfully to suppress the minorities' political movement. The Karen
   began armed movement against the central government as early as 1949,
   while the Kachin took up arms in 1961. The armed groups initially
   demanded independence from the Union of Burma but later opted for
   autonomy. The 17-year-old ceasefire between the Kachin armed group and
   the Myanmar military ended in June 2011, while the Karen armed group
   signed a ceasefire agreement with the government in January 2012. The
   article argues that accommodating the interests of minorities by the
   majority group and cooperation among the different ethnic groups are
   essential for national reconciliation and for the success of democracy
   in the Union of Myanmar.},
