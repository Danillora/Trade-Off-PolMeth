@incollection{ WOS:000348592600011,
Author = {Tassinari, Fabrizio and Holm, Ulla},
Editor = {BalossiRestelli, LM and Whitman, RG and Edwards, G},
Title = {Values promotion and security management in Euro-Mediterranean relations
   The case of Italy},
Booktitle = {ITALY'S FOREIGN POLICY IN THE TWENTY-FIRST CENTURY: A CONTESTED NATURE?},
Series = {Routledge Advances in European Politics},
Year = {2015},
Volume = {110},
Pages = {197-217},
Abstract = {Even before the Arab uprisings of 2011, European policy towards the
   southern Mediterranean was painfully fragmented. Underpinning these
   tensions is an ongoing `securitisation' of the Mediterranean debate
   which centres on threats emanating from the South, including Islamic
   fundamentalism, terrorism and immigration - or on challenges such as
   energy. On the other hand, the stated European goal in the region has
   long sought the advancement of EU norms and values - to be attained
   primarily through governance reforms aimed at improving the rule of law.
   This article will exemplify these discourses by focusing on the case of
   Italy's Mediterranean policy. The contradictions contained in the
   competing European and Italian discourses came to a head during the Arab
   uprisings of 2011. The paper concludes by laying out two possible
   scenarios for the future development of Euro-Mediterranean discourse:
   one based in the normative logic termed `making democracy work'; the
   other rooted in security logic and termed `good enough governance'.},
