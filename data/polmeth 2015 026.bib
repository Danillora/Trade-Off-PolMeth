@article{ WOS:000347808800006,
Author = {Hirt, Nicole},
Title = {THE ERITREAN DIASPORA AND ITS IMPACT ON REGIME STABILITY: RESPONSES TO
   UN SANCTIONS},
Journal = {AFRICAN AFFAIRS},
