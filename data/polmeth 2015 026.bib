@article{ WOS:000352019900010,
Author = {Font, Joan and Wojcieszak, Magdalena and Navarro, Clemente J.},
Title = {Participation, Representation and Expertise: Citizen Preferences for
   Political Decision-Making Processes},
Journal = {POLITICAL STUDIES},
Year = {2015},
Volume = {63},
Number = {1, SI},
Pages = {153-172},
Month = {APR},
Abstract = {In this article, it is shown that citizen process preferences are
   complex and include several dimensions. The argument relies on data from
   a representative sample of Spanish citizens (N=2,450) to assess these
   dimensions. Using confirmatory factor analysis as well as Mokken
   analysis, it is shown that citizen process preferences capture support
   for three different models: participatory, representative and
   expert-based. The relationships between these dimensions (where the
   opposition between representation and participation stands as the
   clearest result) and the substantive and methodological implications of
   these findings are discussed.},
