@article{ WOS:000369919200004,
Author = {Seligmann, Matthew S.},
Title = {Naval History by Conspiracy Theory: The British Admiralty before the
   First World War and the Methodology of Revisionism},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2015},
Volume = {38},
Number = {7, SI},
Pages = {966-984},
Abstract = {Revisionist interpretations of British naval policy in the Fisher era
   claim that an elaborate smokescreen was created to hide the Royal Navy's
