@article{ WOS:000435297500005,
Author = {Laverty, Nicklaus},
Title = {The ``party of power{''} as a type},
Journal = {EAST EUROPEAN POLITICS},
Year = {2015},
Volume = {31},
Number = {1},
Pages = {71-87},
Abstract = {The post-Soviet ``party of power{''} and the United Russia (UR) party
   have received increased attention in recent years, but it is not clear
   where they fit in comparison with other authoritarian parties. This
   article argues that by examining the relationship between the state and
   the party, and the degree of manipulation used to secure the party's
   dominance, we can move towards placing the party of power in a
   typological framework. Compared to other authoritarian parties, the
   party of power relies almost exclusively on the state for its political
   position, and is vulnerable to shifts in the state's priorities.},
