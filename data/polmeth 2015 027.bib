@incollection{ WOS:000348592600007,
Author = {Catalano, Claudio},
Editor = {BalossiRestelli, LM and Whitman, RG and Edwards, G},
Title = {The untold story The Italian role in the development of a European
   defence},
Booktitle = {ITALY'S FOREIGN POLICY IN THE TWENTY-FIRST CENTURY: A CONTESTED NATURE?},
Series = {Routledge Advances in European Politics},
Year = {2015},
Volume = {110},
Pages = {110-129},
Abstract = {This chapter defines the contribution of security and defence policy to
   European integration. It highlights how the military pursued its
   administrative interests as a consequence of the lack of interest of
   government in military matters and its unconditional support to
   multilateralism and European construction. It analyses the inputs the
   military has offered to European defence. They conceived new concepts
   such as the Helsinki Headline Goal. In absence or undiscerning consent
   of political decision-making, the military decided on their own
   organisational reform, and on equipment, as in the case of the A400M.
   The chapter argues that they added value to European defence much more
   than expected. It discusses this aspect contending that the lack of
   direction in defence policy was the responsible factor which led to the
   administrative interests of domestic bureaucratic elites playing a
   greater role as supporters of Italy in European defence integration.},
