@article{ WOS:000351219300006,
Author = {Gallagher, Kevin P.},
Title = {Contesting the Governance of Capital Flows at the IMF},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2015},
Volume = {28},
Number = {2},
Pages = {185-198},
Month = {APR},
Abstract = {Financial crises can trigger different actors to reassess their ideas,
   interests, and policies, and sometimes change them. The Global Financial
   Crisis triggered a reassessment at the International Monetary Fund (IMF)
   regarding the utility of capital account liberalization and the
   management of capital flows. In 2010, the IMF embarked upon an official
   reassessment of these issues and in 2012 published an official
   institutional view on capital account liberalization and managing
   capital flows that gives more caution toward capital account
   liberalization and endorses the use of capital controls in certain
   circumstances. This article traces the political process that yielded
   the IMF's new view and draws out lessons for thinking about policy
   change in global economic governance institutions.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Gallagher, KP (Corresponding Author), Boston Univ, Boston, MA 02215 USA.
   Boston Univ, Boston, MA 02215 USA.},
DOI = {10.1111/gove.12100},
ISSN = {0952-1895},
