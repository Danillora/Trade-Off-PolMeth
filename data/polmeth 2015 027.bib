@article{ WOS:000363061300009,
Author = {Rothstein, Bo},
Title = {The Chinese Paradox of High Growth and Low Quality of Government: The
   Cadre Organization Meets Max Weber},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2015},
Volume = {28},
Number = {4},
Pages = {533-548},
Month = {OCT},
Abstract = {Much research has argued for the importance of state's administrative
   capacity for development. Disregard for the rule of law and failure to
   get corruption under control are seen as detrimental to economic and
   social development. The China paradox refers to the fact that in all
   commonly used measures of levels of corruption and the quality of
   government, China is a country that scores quite low. China also lacks
   the Weberian model of bureaucracy that is seen as central for
   development. It is argued that this paradox is the result of
   disregarding the existence of a different public administration model in
   China-the cadre organization. Instead of rule following, this
   organization is marked by high commitment to a specific policy doctrine.
   The argument is that while very different from Weberian bureaucracy,
   this organization is well suited for effectively implementing policies
   for economic and social development.},
