@article{ WOS:000349783100007,
Author = {Hakelberg, Lukas},
Title = {The power politics of international tax co-operation: Luxembourg,
   Austria and the automatic exchange of information},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2015},
Volume = {22},
Number = {3},
Pages = {409-428},
Month = {MAR 16},
Abstract = {Luxembourg and Austria resisted exchanging bank data on non-resident
   interest income with European Union (EU) partners for over a decade. In
   March 2014 they eventually gave in. Theories of tax competition analysts
   usually apply to intra-EU bargaining over taxation cannot explain their
   change of tack. Instead, I argue that American imposition of bilateral
   exchange of information on the two countries unlocked negotiations at EU
   level. Concessions made to the United States (US) by Luxembourg and
   Austria activated a most-favoured nation clause contained in an EU
   directive. Moreover, the US also forced third countries to exchange bank
   data, thus reducing the risk of capital flight from Luxembourg and
   Austria.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
