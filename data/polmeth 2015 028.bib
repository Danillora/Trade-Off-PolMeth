@article{ WOS:000361223400011,
Author = {Lundin, Martin and Oberg, Perola and Josefsson, Cecilia},
Title = {LEARNING FROM SUCCESS: ARE SUCCESSFUL GOVERNMENTS ROLE MODELS?},
