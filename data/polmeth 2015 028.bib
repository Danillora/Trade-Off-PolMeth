@article{ WOS:000364250400013,
Author = {Bustikova, Lenka},
Title = {Voting, identity and security threats in Ukraine: who supports the
   Radical ``Freedom{''} Party?},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2015},
Volume = {48},
Number = {2-3},
Pages = {239-256},
Month = {JUN-SEP},
Abstract = {This article investigates voters and sympathizers of Ukraine's radical
   right party, Svoboda. Using an original survey conducted in 2010, it
   shows that support for Svoboda was rooted less in extreme levels of
   xenophobia vis-a-vis Russians, and more in concerns about the support
   that the Russian minority receives from the state, fear of losing
   Ukrainian sovereignty, and economic anxiety. In contrast to the
   conventional view, the analysis suggests that support for Svoboda was
   not a function of inter-group ethnic hostilities; instead, it originated
   in perceived threats and anxieties about the character of the Ukrainian
   state. (c) 2015 The Regents of the University of California. Published
   by Elsevier Ltd. All rights reserved.},
