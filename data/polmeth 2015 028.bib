@article{ WOS:000354169500003,
Author = {Gruenewald, Jeff and Allison-Gruenewald, Kayla and Klein, Brent R.},
Title = {Assessing the Attractiveness and Vulnerability of Eco-Terrorism Targets:
   A Situational Crime Prevention Approach},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
