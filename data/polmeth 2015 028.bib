@incollection{ WOS:000348592600004,
Author = {Croci, Osvaldo},
Editor = {BalossiRestelli, LM and Whitman, RG and Edwards, G},
Title = {All quiet on the Western front Italy and transatlantic relations},
Booktitle = {ITALY'S FOREIGN POLICY IN THE TWENTY-FIRST CENTURY: A CONTESTED NATURE?},
Series = {Routledge Advances in European Politics},
Year = {2015},
Volume = {110},
Pages = {43-64},
Abstract = {Atlanticism and Europeanism have represented the two major pillars of
   Italian foreign policy throughout the cold war period. This chapter
   challenges a view that has come to enjoy widespread acceptance since the
   mid-1990s, namely that centre-right government privilege Atlanticism
   over Europeanism while centre-left ones favour Europeanism over
   Atlanticism. This chapter argues instead that neither the place of
   Atlanticism in Italian foreign policy nor its relationship with
   Europeanism have changed since the two policies were first developed in
   the late 1940s. The chapter begins by identifying the variables
   affecting continuity and change in foreign policy. It then sketches the
   main characteristics of the policy of Atlanticism, its relation to
   Europeanism, and its subsequent developments. Next, the chapter examines
   the more proactive foreign policy Italy developed after the end of the
   cold war and argues that this new activism has not altered the basic
   characteristics of Atlanticism and its relationship with Europeanism.
   The chapter ends with some predictions about the future of Atlanticism
   in Italian foreign policy.},
