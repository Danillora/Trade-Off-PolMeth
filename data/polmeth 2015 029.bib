@article{ WOS:000445193800007,
Author = {Duarte, Paulo},
Title = {Regionalism in Central Asia: A Stock-taking},
Journal = {FORO INTERNACIONAL},
Year = {2015},
Volume = {55},
Number = {4},
Pages = {1165+},
Month = {OCT-DEC},
Abstract = {This article seeks to illustrate the main characteristics of regionalism
   in Central Asia. This regionalism, as conceived by the Central Asian
   republics, has been rather timid, has had little effect, and contributed
