@article{ WOS:000356492500002,
Author = {Kaarbo, Juliet},
Title = {A Foreign Policy Analysis Perspective on the Domestic Politics Turn in
   IR Theory},
Journal = {INTERNATIONAL STUDIES REVIEW},
