@article{ WOS:000354454300004,
Author = {Fabbrini, Sergio},
Title = {The European Union and the Puzzle of Parliamentary Government},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2015},
Volume = {37},
Number = {5},
Pages = {571-586},
Month = {JUL},
Abstract = {The parliamentary election of Jean-Claude Juncker as the Commission
   president re-launched the debate on the parliamentary transformation of
   the EU. The article critically discusses the argument that EU can or
   should become a parliamentary federation. Based on the analytical
   distinction between federation by aggregation and by disaggregation, and
   assuming the EU as a species of the former type, the article shows the
   difficulty of adopting a parliamentary government at the horizontal
   level because of the demographic asymmetry and cultural differentiation
   between the EU Member States. The constraints on adopting a
   parliamentary government are reflected in the Lisbon Treaty. But, also
   with a change of the Treaty, parliamentary government in the EU would be
   obstructed by inter-institutional and structural dilemmas.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
