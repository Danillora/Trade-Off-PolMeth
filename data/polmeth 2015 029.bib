@article{ WOS:000368922300007,
Author = {Begg, Iain and Bongardt, Annette and Nicolaidis, Kalypso and Torres,
   Francisco},
Title = {EMU and Sustainable Integration},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2015},
Volume = {37},
Number = {7, SI},
Pages = {803-816},
Abstract = {This paper considers what will be required to make Economic and Monetary
   Union (EMU) sustainable following the successive crises of recent years.
   It starts by laying out the policy benchmark, namely the successive
   `President Reports' produced by EU institutions. It then suggests three
   dimensions of sustainable integration relevant to EMU, namely the
   pursuit of sustainable growth, the need to take into account what we
   call `varieties of modernisation' and the `ownership' of democratically
   sustainable reforms. It then evaluates the recasting of EMU governance
   against the benchmark of sustainable integration.},
