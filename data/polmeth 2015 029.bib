@article{ WOS:000348507300007,
Author = {Henriksen, Lasse Folke},
Title = {The global network of biofuel sustainability standards-setters},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2015},
Volume = {24},
Number = {1},
Pages = {115-137},
Month = {JAN 2},
Abstract = {The role of network structure in shaping the regulatory scope and
   content of sustainability standards for biofuels is examined. A critical
   review of the literature on hybrid governance networks suggests the need
   to bring in network theory. Through a specific network analysis of the
   standards-setters, it is shown that not only does the institutional
   hybridity of the standards boards influence the regulatory scope of the
   standards, but the network centrality and specific topology in which
   standards-setters are embedded are equally important structural features
   of hybrid governance. These findings foreground the relevance of
   incorporating in the current attributional conception of hybridity a
   network element, taking seriously the role of network structure in
   shaping regulatory fields. Social Network Analysis as an analytical tool
   holds great potential for further research into the structural features
   of hybrid governance.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
