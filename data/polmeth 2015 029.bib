@article{ WOS:000368450100005,
Author = {Moodie, T. Dunbar},
Title = {`Igneous' means fire from below: the tumultuous history of the National
   Union of Mineworkers on the South African platinum mines},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2015},
Volume = {42},
Number = {146, SI},
Pages = {561-576},
Month = {OCT 2},
Abstract = {From the time Impala dismissed its entire workforce in 1986 up to and
   well beyond the Marikana massacre, the National Union of Mineworkers has
   struggled to organise the platinum mines of the Bushveld Igneous
   Complex. This article focuses on two case studies that highlight the
   fundamental importance of informal networks for organising mine workers.
   While the union now seems seriously at risk, it has never had an easy
   time in Rustenburg. Worker committees are not a new phenomenon there.
   Nor is insurgency. Mineworkers in South Africa, like mineworkers
   worldwide, have never been passive recipients of direction from above.},
