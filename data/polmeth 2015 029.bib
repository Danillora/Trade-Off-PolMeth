@article{ WOS:000368450100003,
Author = {Bezuidenhout, Andries and Buhlungu, Sakhela},
Title = {Enclave Rustenburg: platinum mining and the post-apartheid social order},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2015},
Volume = {42},
Number = {146, SI},
Pages = {526-544},
Month = {OCT 2},
Abstract = {In the absence of a levelling out of income and resources, as well as
   arbitrary violence in everyday life, the post-apartheid social order is
   characterised by the formation of various enclaves. In the platinum
   mining town of Rustenburg, these enclaves are constructed on the
   foundations of the apartheid categories suburb', compound', township'
   and homeland'. Such enclaves include security villages, converted
   compounds with access control, and informal settlements with distinctive
   gender, linguistic and class formations. The article draws on David
   Harvey's formulation of absolute, relative and relational space and the
   case of Rustenburg to elaborate the concept of enclave further.},
