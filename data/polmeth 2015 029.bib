@article{ WOS:000364746500006,
Author = {Cercel, Cristian},
Title = {Philo-Germanism without Germans in Romania after 1989},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
Year = {2015},
Volume = {29},
Number = {4},
Pages = {811-830},
Month = {NOV},
Abstract = {This article argues that positive representations of the German minority
   in post-1989 Romania, discernible in specific memory and identity
   discourses, are linked to an internalized self-orientalizing view of
   Romanianness and to a symbolic wish to belong to Europe, present in
   Romanian society and displayed on the Romanian political scene. In other
   words, it maintains that a phenomenon describable as philo-Germanism
   without Germans in contemporary Romania is tightly connected with the
   production and reproduction of symbolic geographies whose aim is to
   insert Romania into the civilized Western/European world.},
