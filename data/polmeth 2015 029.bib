@article{ WOS:000362422700005,
Author = {Hager, Sandy Brian},
Title = {Corporate ownership of the public debt: mapping the new aristocracy of
   finance},
Journal = {SOCIO-ECONOMIC REVIEW},
Year = {2015},
Volume = {13},
Number = {3, SI},
Pages = {505-523},
Month = {JUL},
Abstract = {In various writings Karl Marx made references to an `aristocracy of
   finance' in Western Europe and the USA that dominated ownership of the
   public debt. Drawing on original research, this article offers the first
   comprehensive analysis of public debt ownership within the US corporate
   sector. The research shows that over the past three decades, and
   especially in the context of the current crisis, a new aristocracy of
   finance has emerged, as holdings of the public debt have become rapidly
   concentrated in favour of large corporations classified within Finance,
   Insurance and Real Estate. Operationalizing Wolfgang Streeck's concept
   of the `debt state', the article goes on to demonstrate how
   concentration in ownership of the public debt reinforces patterns of
   social inequality and proceeds in tandem with a shift in government
   policy, one that prioritizes the interests of government bondholders
   over the general citizenry.},
