@article{ WOS:000352609900001,
Author = {Hecker, Tobias and Haer, Roos},
Title = {Drugs Boosting Conflict? A Micro-Level Test of the Linkage Between
   Substance Use and Violence},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2015},
Volume = {27},
Number = {2},
Pages = {205-224},
Abstract = {Many studies have emphasized the role of natural resources in the onset
   and duration of armed conflict. Due to its characteristics, narcotics
   are considered to be one of the most influential resources. However, the
   dynamics of how this particular commodity is linked to conflict is still
   not well understood. Most scholars have focused on the revenue aspects
   of narcotics and only a few have mentioned the micro-level aspect, i.e.,
   the effect of drug intake and alcohol consumption on combatants'
   behavior during conflict. With the help of a dataset based on 224
   interviews held with former combatants in the Democratic Republic of the
   Congo, we examined this latter dynamic. Our analyses show, after
   controlling for armed group-level and individual-level variables, that
   drug intake and alcohol consumption boost the number of violent actions
   perpetrated by combatants.},
