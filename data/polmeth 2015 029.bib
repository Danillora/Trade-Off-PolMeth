@article{ WOS:000360216200005,
Author = {Smid, Tomas and Mares, Miroslav},
Title = {`Kadyrovtsy': Russia's Counterinsurgency Strategy and the Wars of
   Paramilitary Clans},
Journal = {JOURNAL OF STRATEGIC STUDIES},
