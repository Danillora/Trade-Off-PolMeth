@incollection{ WOS:000361320800005,
Author = {Lewis, Alexandra},
Book-Author = {Lewis, A},
Title = {Transnational Security: Piracy, Terrorism and the Fragility Contagion},
Booktitle = {SECURITY, CLANS AND TRIBES: UNSTABLE GOVERNANCE IN SOMALILAND, YEMEN AND
   THE GULF OF ADEN},
Series = {Palgrave Pivot},
Year = {2015},
Pages = {92-108},
Abstract = {Neither Yemen nor Somalia has the internal stability or the state
   capacity necessary to effectively contain local and national security
   threats within their borders, or to keep external threats out from their
   territories. This not only has led to a spill-over of their insurgencies
   into neighbouring countries, but has also led to the entrenchment of
   transnational criminal and terrorist organisations within their borders.
   Violent movements (such as Al Shabaab and the Houthis) operate in the
   same contexts as local branches of global terrorist groups (such as Al
   Qaeda), piracy networks and trafficking organisations. These various
   threats affect the entire Gulf of Aden region and are both thematically
   independent of one another and inherently interconnected. This chapter
   analyses each in turn to provide the reader with a better understanding
   of regional insecurity in this part of the world.},
