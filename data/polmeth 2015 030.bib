@article{ WOS:000347781100008,
Author = {Gilmore, Jonathan},
Title = {Still a `Force for Good'? Good International Citizenship in British
   Foreign and Security Policy},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
