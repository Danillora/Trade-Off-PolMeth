@article{ WOS:000353997200001,
Author = {Hatch, Megan E. and Rigby, Elizabeth},
Title = {Laboratories of (In)equality? Redistributive Policy and Income
   Inequality in the American States},
Journal = {POLICY STUDIES JOURNAL},
Year = {2015},
Volume = {43},
Number = {2},
Pages = {163-187},
Month = {MAY},
Abstract = {Prior literature has emphasized demographic, economic, and political
   explanations for increasing income inequality in the United States, with
   little attention paid to the role of state-level policy. This is despite
   great variation across states in both the level of inequality and the
   rate at which it is rising. This paper asks whether differences in state
   policy choices can help explain this variation; specifically, we
   examined a range of state redistributive policies enacted between 1980
   and 2005 and identified four common approaches likely to impact
   inequality: taxes on the wealthy, taxes on the poor, spending on the
   poor, and labor market policies. We used pooled cross-sectional
   time-series data and a fixed-effects model to assess the relationship
   between states' use of each policy approach and two measures of market
   income inequality: the Gini coefficient and the income share of the top
   1 percent. We find policies played a significant role in shaping income
   inequality in the states. For three of these four policy approaches, we
   found less inequality following expansions of state redistributive
   policy. Yet, for another, we identified the opposite pattern. These
   findings highlight the importance of state policy choices in shaping
   market inequality, and have implications for designing state policies to
   reduce income inequality since the success of these efforts depends on
   the policy approach used to redistribute income and wealth.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Hatch, ME (Corresponding Author), Cleveland State Univ, Urban Policy \& City Management, Cleveland, OH 44115 USA.
