@article{ WOS:000214280800005,
Author = {Barbehoen, Marlon and Haus, Michael},
Title = {Middle class and welfare state - discursive relations},
Journal = {CRITICAL POLICY STUDIES},
Year = {2015},
Volume = {9},
Number = {4},
Pages = {473-484},
Abstract = {Within current public, political and academic debates, `the middle
   class' functions as a major point of reference for assessing societal
   developments, welfare arrangements and even the society as a whole.
   However, by identifying the middle class with quantitative indicators
   and methods stemming from social structural analysis, it is largely
   neglected that class structures are social artifacts. By contrast, in
   this forum contribution, we want to provide reasons for investigating
   the nexus between middle class and welfare state as a discursive
   relation. We argue in favor of and provide first insights from a
   perspective that sheds light on how the middle class and its
   relationship with the welfare state are discursively constructed and how
   these practices shape the basic rationales of social policy design. We
   want to sketch how an interpretive perspective may contribute to the
   long-standing question of how the middle class is entangled with welfare
   policies.},
