@article{ WOS:000350820700008,
Author = {Paret, Marcel},
Title = {Violence and democracy in South Africa's community protests},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2015},
Volume = {42},
Number = {143},
Pages = {107-123},
Month = {JAN 2},
Abstract = {Community protests in South Africa are often described as violent.
   Drawing from newspaper articles, interviews with protesters and
   statements by public officials, this paper unpacks the meaning of
   `violent protest'. It shows that violence is both ambiguous and deeply
   entangled with democracy. On the one hand, violent practices may become
   a tool of liberation, promoting democracy by empowering marginalised
   groups. On the other hand, democracy may become a tool of domination,
   undermining dissent by constituting as violent those persons and actions
   that deviate from formal institutional channels. The analysis urges
   scholars to adopt a critical and nuanced view of violence.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
