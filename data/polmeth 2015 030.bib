@article{ WOS:000358300900004,
Author = {Jensen, Richard Bach},
Title = {Anarchist Terrorism and Global Diasporas, 1878-1914},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2015},
Volume = {27},
Number = {3},
Pages = {441-453},
Abstract = {During the quarter century before the First World War, anarchist
   terrorism was often blamed on the impact of anarchist agitators on naive
   immigrants. This article seeks to investigate the truth of this claim,
   focusing particularly on Italian emigrants, but also looking at some
   examples of Spanish, French, and Russian emigrants. My conclusion is
   that, with a few exceptions, radicals emigrated, but emigration did not
   create radical terrorists. A particularly good example of this can be
   found by examining the large Italian emigration to Argentina. At most,
   the emigrant experience may have heightened a pre-existing radicalism or
   given more precise configuration to its violent expression.},
