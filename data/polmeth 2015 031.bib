@article{ WOS:000356785400005,
Author = {Hodson, Dermot},
Title = {The IMF as a de facto institution of the EU: A multiple supervisor
   approach},
Journal = {REVIEW OF INTERNATIONAL POLITICAL ECONOMY},
Year = {2015},
Volume = {22},
Number = {3, SI},
Pages = {570-598},
Month = {MAY 4},
Abstract = {This paper seeks to understand and explain the International Monetary
   Fund's (IMF) evolving relationship with the European Union (EU) before
   and after the global financial crisis of 2007-2008. Prior to this
   crisis, the two sides operated on parallel tracks with little scope for
   mutual adjustment even during the economic turmoil of the 1970s. After
   the global financial crisis, the IMF emerged as a de facto institution
   of the EU thanks to European leaders' delegation of supervisory powers
   to both the Fund and the European Commission. The reasons for, and
   consequences of, this dual delegation are explored here by means of
   amultiple supervisor variation on the classic principal-agent-supervisor
   approach.},
