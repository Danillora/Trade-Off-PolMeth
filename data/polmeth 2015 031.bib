@incollection{ WOS:000378903400009,
Author = {Longo, Michael and Murray, Philomena},
Book-Author = {Longo, M
   Murray, P},
Title = {Problems in Search of a Solution, Solutions in Search of Legitimacy},
Booktitle = {EUROPE'S LEGITIMACY CRISIS: FROM CAUSES TO SOLUTIONS},
Series = {Palgrave Pivot},
Year = {2015},
Pages = {101-130},
Abstract = {This final chapter examines several options for dealing with the current
   crisis, preferring to evaluate key choices and opportunities rather than
   focusing on a preferred path. To date, responses have focused
   inordinately on austerity and fiscal compacts without proper regard for
   the economic, social and political consequences of this policy choice.
   National and European Union (EU) decision-makers have made choices that
   have penalised citizens in some member states. These citizens have borne
   considerable pain to safeguard an imperfect economic and monetary system
   to the detriment of social justice and legitimacy in the EU. We conclude
   that to secure its legitimacy, the EU must work towards buttressing both
   its output and input legitimacy. This may involve a return to
   foundational values as the basis of EU policies to deliver effective
   outcomes that achieve prosperity and security for all. It may also
   involve the implementation of a plan to promote citizen participation in
   EU debates and decision-making.},
