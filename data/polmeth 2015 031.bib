@article{ WOS:000214237300003,
Author = {Sorell, Tom},
Title = {Human Rights and Hacktivism: The Cases of Wikileaks and Anonymous},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2015},
Volume = {7},
