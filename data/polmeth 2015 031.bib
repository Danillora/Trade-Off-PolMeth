@article{ WOS:000363982400007,
Author = {Storrie, Bridget},
Title = {`The Mighty Life-Creating and Transforming Power' of Carnival: Why the
   Canadian Truth and Reconciliation Commission Does Not Seem to Have It,
   but Indigenous Resurgence Does},
Journal = {INTERNATIONAL JOURNAL OF TRANSITIONAL JUSTICE},
Year = {2015},
Volume = {9},
Number = {3},
Pages = {469-485},
Month = {NOV},
Abstract = {In an article in this Journal in 2012, Matt James describes the
   victim-centred approach of the Truth and Reconciliation Commission (TRC)
   of Canada as `carnivalesque,' following Soviet-era literary theorist and
   philosopher Mikhail Bakhtin(2) and legal scholar Teresa Godwin
   Phelps.(3) He argues that in the Canadian context this approach
   `unfortunately fosters the absence of a more detailed and
   accountability-promoting examination' of the agents responsible for the
   residential school system.(4) I suggest that this is not because the
   Canadian TRC has been a carnival. For Bakhtin, carnival possesses a
   `mighty life-creating and transforming power' that comes from a
   `carnivalistic leaven' largely absent in the Canadian TRC.(5) Despite
   this, the Commission has sparked new conversations and activities around
   reconciliation and indigenous resurgence that seem more promising. Now
   that the formal work of the Canadian TRC is over, can the `carnival of
   truth' begin?(6)},
