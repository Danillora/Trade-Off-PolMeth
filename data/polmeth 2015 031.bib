@article{ WOS:000354044100004,
Author = {Risley, Amy},
Title = {``America Will Not Tolerate Slave Traders{''}: Counter-Trafficking
   Policies and US Power},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2015},
Volume = {36},
Number = {2},
Pages = {213-238},
Month = {APR 3},
Abstract = {The US government has implemented an ambitious set of policies designed
   to combat human trafficking and sex trafficking in women and girls in
   particular. This article argues that anti-trafficking discourse and
   policy can be understood as a project to sustain and strengthen US
   power. This power has been wielded through the use of foreign aid, which
   influences the actions of both state and non-state actors overseas.
   Existing policies reinforce unilateralism and executive-branch
   dominance. Policymakers have also used gender strategically to moralize
   their actions and assert global leadership on this issue. Gender is thus
   deployed to serve US interests.},
