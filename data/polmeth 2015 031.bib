EISSN = {1531-3298},
Keywords-Plus = {FOREIGN-POLICY; UNITED-STATES; PEACE; WAR},
Research-Areas = {History; International Relations; Government \& Law},
Web-of-Science-Categories  = {History; International Relations; Political Science},
Cited-References = {Andrew A.J., 1993, The Rise Fall of a Middle Power.
