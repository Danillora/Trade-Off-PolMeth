@article{ WOS:000367004900008,
Author = {Low, Choo Chin},
Title = {Same System, Different Outcomes: Comparing the Implementation of Dual
   Nationality Treaties in East Germany and China},
Journal = {EUROPE-ASIA STUDIES},
Year = {2015},
Volume = {67},
Number = {10, SI},
Pages = {1656-1684},
Month = {NOV 26},
Abstract = {Since 1955, one important trend regarding dual nationality was
   observable in East Germany and the People's Republic of China (PRC): the
   official rejection of dual nationality itself. While East Germany's
   effort in preventing dual nationality was motivated by the socialist
   nationality principles of its Eastern European allies, the PRC's effort
   was aimed at ending its diplomatic isolation in the Southeast Asian
   region. In both states, bilateral treaties were preferred over national
   legislation in overcoming dual nationality. This article compares the
   implementation of a single nationality principle through bilateral
   treaties and suggests that the PRC's effort was unsuccessful because it
   was confronted with unfavourable international circumstances compared to
   East Germany.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
