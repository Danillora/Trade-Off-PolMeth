@article{ WOS:000349035100002,
Author = {Lizotte, Mary-Kate},
Title = {The Abortion Attitudes Paradox: Model Specification and Gender
   Differences},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2015},
Volume = {36},
Number = {1},
Pages = {22-42},
Month = {JAN 2},
Abstract = {Rather unexpectedly, prior work has failed to find consistent gender
   differences in public support for legal abortion. Given that gender
   differences in public opinion emerge for a wide range of other issue
   areas, it seems paradoxical that there is no consistent gender
   difference on the issue of abortion. I propose that this failure to find
   a consistent gender difference is due to how abortion attitudes are
   modeled. Controlling for religiosity, which research has shown women to
   score higher on, results in a small and consistent gender gap in support
   for legal abortion with women more likely than men to support.},
