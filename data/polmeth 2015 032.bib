@article{ WOS:000371286800002,
Author = {Stjepanovic, Dejan},
Title = {Territoriality and Citizenship: Membership and Sub-State Polities in
   Post-Yugoslav Space},
Journal = {EUROPE-ASIA STUDIES},
Year = {2015},
Volume = {67},
Number = {7},
Pages = {1030-1055},
Abstract = {This article deals with the issue of sub-state citizenship in the
   post-Yugoslav countries and focuses on the emergence and definitions of
   membership. The article analyses conceptions of nationhood and
   territorial compositions of the states, proceeds with the analysis of
   sub-state entities' governance arrangements and compares conceptions of
   nationhood and membership in state-wide and sub-state polities. The
   article identifies four broad categories of sub-state polities and two
   diverging tendencies in the definition of membership. At one end of the
   spectrum, there are cases in which membership in regional polities is
   based on territorial, multi-ethnic and civic principles. At the other
   end, membership is defined in ethno-national terms.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
