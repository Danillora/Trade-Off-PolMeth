@article{ WOS:000356235200001,
Author = {Hierman, Brent},
Title = {Central Asian Ethnicity Compared: Evaluating the Contemporary Social
   Salience of Uzbek Identity in Kyrgyzstan and Tajikistan},
Journal = {EUROPE-ASIA STUDIES},
Year = {2015},
Volume = {67},
Number = {4},
Pages = {519-539},
Month = {APR 21},
Abstract = {In this article, I utilise a contextual understanding of ethnicity and
   unique data to demonstrate that the ethnic Uzbek identity category is
   both widely available and frequently a useful means of making sense of
   the world in both Kyrgyzstan and Tajikistan. While Uzbek ethnicity is
   generally salient in both states, the context in which it becomes so
   varies across space. In particular, there are significant urban-rural
   distinctions that affect when Uzbek ethnicity is utilised to interpret
   the world. In addition, compared to others, rural Tajikistani Uzbeks
   perceive that the boundary between Uzbeks and the titular groups is
   particularly permeable.},
