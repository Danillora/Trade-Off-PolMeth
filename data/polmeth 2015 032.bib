@article{ WOS:000352789600020,
Author = {Cappella, Joseph N. and Yang, Sijia and Lee, Sungkyoung},
Title = {Constructing Recommendation Systems for Effective Health Messages Using
   Content, Collaborative, and Hybrid Algorithms},
