@article{ WOS:000349032900003,
Author = {Stattman, Sarah L. and Gupta, Aarti},
Title = {Negotiating Authority in Global Biofuel Governance: Brazil and the EU in
   the WTO},
Journal = {GLOBAL ENVIRONMENTAL POLITICS},
Year = {2015},
Volume = {15},
Number = {1},
Pages = {41+},
Month = {FEB},
Abstract = {The global demand for biofuels (liquid or gas fuels deriving from
   biomass) has grown dramatically in recent years. European Union policies
   that promote biofuels as more sustainable sources of transport fuel are
   partly driving this development. In this article, we analyze how Brazil,
   as a key producer of biofuels, navigates an emerging global governance
   context for sustainable biofuels. We do so by examining how Brazil
   responds to EU biofuel sustainability imperatives, including by evoking
   World Trade Organization disciplines in questioning their transnational
   validity and reach. While domestically Brazil emphasizes the social and
   developmental objectives of its biofuel policies, globally it frames
   itself as a leading producer of sustainable biofuels. In so doing, it
   navigates intersecting spheres of authority in a manner that promotes
   its own biofuel policy agenda, partly by seeking to reframe
   ``sustainability{''} debates internationally to reflect its
   developmental agenda.},
