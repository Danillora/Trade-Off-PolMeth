@article{ WOS:000357528400004,
Author = {Igreja, Victor},
Title = {Amnesty Law, Political Struggles for Legitimacy and Violence in
   Mozambique},
Journal = {INTERNATIONAL JOURNAL OF TRANSITIONAL JUSTICE},
