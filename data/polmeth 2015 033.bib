@article{ WOS:000359388400004,
Author = {Denk, Thomas and Christensen, Henrik Serup and Bergh, Daniel},
Title = {The Composition of Political Culture-A Study of 25 European Democracies},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
