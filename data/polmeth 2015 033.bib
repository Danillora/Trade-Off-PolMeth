@article{ WOS:000359724300012,
Author = {Hughey, Matthew W. and Byrd, W. Carson},
Title = {Beautiful Melodies Telling Me Terrible Things: The Future of Race and
   Genetics for Scholars and Policy-Makers},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
