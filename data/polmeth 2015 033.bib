@incollection{ WOS:000378903400008,
Author = {Longo, Michael and Murray, Philomena},
Book-Author = {Longo, M
   Murray, P},
Title = {Contesting Europe's Past and Imagining Europe's Future: Do Identity and
   Participation Matter?},
Booktitle = {EUROPE'S LEGITIMACY CRISIS: FROM CAUSES TO SOLUTIONS},
Series = {Palgrave Pivot},
Year = {2015},
Pages = {80-100},
Abstract = {This is chapter examines the disjuncture between the EU and its
   citizens. It assesses the lack of EU solidarity among member states. If
   solidarity provides a basis for burden sharing, its absence during
   crisis provides a foundation for division. is chapter examines the
   longstanding aspiration of European elites to give a public dimension to
   the EU; to enhance its relevance and to invoke ties of identity and
   belonging. Citizens are acutely aware of the EU's failures in monetary
   union. Populists depict Europe as a failed experiment that must be
   reversed. is message resonates strongly, with deep citizen disaffection
   towards the EU and the hardening of cleavages across Europe. The EU
   lacks the means to generate a Europe-wide positive case for unity and
   social solidarity.
   As Lokkegaard (2014, p.115) said, `We have Europe but we do not have any
   Europeans'.},
