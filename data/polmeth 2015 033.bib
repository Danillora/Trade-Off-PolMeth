@article{ WOS:000356235200002,
Author = {Cekota, Jaromir and Trentini, Claudia},
Title = {The Educational Achievement and Employment of Young Roma in Bulgaria,
   Hungary and Romania: Increasing Gaps and Policy Challenges},
Journal = {EUROPE-ASIA STUDIES},
Year = {2015},
Volume = {67},
Number = {4},
Pages = {540-578},
Month = {APR 21},
Abstract = {This article investigates the educational attainment, employment and
   living conditions of young Roma adults in Bulgaria, Hungary and Romania
   with the aid of national generations and gender surveys and other
   sources of information. It shows that in spite of a small improvement in
   the educational attainment of young Roma in comparison to the generation
   of their parents, the educational achievement and employment gaps have
   increased considerably during the post-communist period. The article
   also compares living conditions of the Roma with other population
   groups. It concludes with a discussion of policy challenges.},
