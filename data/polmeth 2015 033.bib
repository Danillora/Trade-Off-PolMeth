@article{ WOS:000357528400006,
Author = {Lixinski, Lucas},
Title = {Cultural Heritage Law and Transitional Justice: Lessons from South
   Africa},
Journal = {INTERNATIONAL JOURNAL OF TRANSITIONAL JUSTICE},
Year = {2015},
Volume = {9},
Number = {2},
Pages = {278-296},
Month = {JUL},
Abstract = {This article explores the relationships between cultural heritage law
   and transitional justice using South Africa as a case study. It argues
   that heritage law has so far been neglected in the transitional justice
   literature. The effect of this neglect has been to ignore a set of
   domestic and international institutions that can play an important role
   in how countries in transition tell their stories. Rather than as a
   means to retell a story, heritage law has been exploited as a means to
   promote a clean break with the past, disenfranchising communities it is
   supposed to serve. Heritage law authorizes versions of what heritage is
   and what it is used for to the benefit of goals other than transition
   (often economic development). This can create resentment among the
   population, who may feel alienated from their heritage and thereby
   unable to come to terms with that part of their past.},
