@article{ WOS:000360561000005,
Author = {Mason, Corinne L.},
Title = {``Cripping{''} the World Bank DISABILITY, EMPOWERMENT AND
   THE COST OF VIOLENCE AGAINST WOMEN},
Journal = {INTERNATIONAL FEMINIST JOURNAL OF POLITICS},
Year = {2015},
Volume = {17},
Number = {3},
Pages = {435-453},
Month = {JUL 3},
Abstract = {Using a mathematical measurement called disability-adjusted life years
   (DALYs), the World Bank claims that violence against women is costly. In
   order to wrestle with representative grammars associated with this
   technocratic measurement, this article uses crip theory to investigate
   how the World Bank conceptualizes violence against women as requiring
   individual adjustments, resiliency and flexibility rather than
   structural and systemic change. It suggests the DALY measurement fits
   squarely within World Bank neoliberal economic schemas, especially the
   promotion of women's labor engagements as empowerment.},
