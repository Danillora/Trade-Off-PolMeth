@article{ WOS:000352789600016,
Author = {Shah, Dhavan V. and Hanna, Alex and Bucy, Erik P. and Wells, Chris and
   Quevedo, Vidal},
Title = {The Power of Television Images in a Social Media Age: Linking
   Biobehavioral and Computational Approaches via the Second Screen},
