@article{ WOS:000354035800003,
Author = {Touquet, Heleen},
Title = {Non-ethnic Mobilisation in Deeply Divided Societies, the Case of the
   Sarajevo Protests},
Journal = {EUROPE-ASIA STUDIES},
Year = {2015},
Volume = {67},
Number = {3},
Pages = {388-408},
Month = {MAR 16},
Abstract = {How do groups mobilise around non-ethnic issues in deeply divided
   societies and cities? This article seeks to answer this question by
   offering a close reading of one particular exemplary episode in the
   recent history of post-conflict Bosnia \& Herzegovina: the 2008 Sarajevo
   protests. Sarajevans took to the streets to demand more security in the
   wake of the murder of a young boy on a tram. The article analyses the
   framings used by the mobilising social movements in depth, exploring in
   detail the connections with the local cultural environment, as well as
   the demobilising authorities' counter-frames.},
