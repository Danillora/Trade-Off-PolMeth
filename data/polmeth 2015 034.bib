@article{ WOS:000217497300001,
Author = {Schilde, Kaija E.},
Title = {Cosmic top secret Europe? The legacy of North Atlantic Treaty
   Organization and cold war US policy on European Union information policy},
Journal = {EUROPEAN SECURITY},
Year = {2015},
Volume = {24},
Number = {2},
Pages = {167-182},
Abstract = {As the EU has expanded its authority into areas of high politics such as
   monetary, defense, and foreign policy, it has simultaneously developed
   procedures for handling more sensitive and classified information. These
   critical policy domains require standards regulating secure information
   and personnel, but the concept of official secrets is in tension with
