@article{ WOS:000364321600001,
Author = {Ceccorulli, Michela and Coticchia, Fabrizio},
Title = {Multidimensional Threats and Military Engagement: The Case of the
   Italian Intervention in Libya},
Journal = {MEDITERRANEAN POLITICS},
Year = {2015},
Volume = {20},
Number = {3},
Pages = {303-321},
Month = {SEP 2},
Abstract = {The military operation in Libya (2011) is a paradigmatic case regarding
   the growing interaction of new security challenges: regional
   instability, transnational organized crime and illegal immigration. The
   main aim of the paper is to answer the question: Why has Italy employed
   a specifically military instrument to tackle transnational and
   non-military threats? Through process tracing the research looks at the
   political debate over the decision-making process in the case of the
   Italian military engagement in Libya, emphasizing the role played by the
   strategic culture, international norms and domestic dynamics.},
