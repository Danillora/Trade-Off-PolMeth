@article{ WOS:000354730900008,
Author = {Lucas, Christopher and Nielsen, Richard A. and Roberts, Margaret E. and
   Stewart, Brandon M. and Storer, Alex and Tingley, Dustin},
Title = {Computer-Assisted Text Analysis for Comparative Politics},
Journal = {POLITICAL ANALYSIS},
