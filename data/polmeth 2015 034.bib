@article{ WOS:000354730900010,
Author = {Gill, Michael and Spirling, Arthur},
Title = {Estimating the Severity of the WikiLeaks US Diplomatic Cables Disclosure},
Journal = {POLITICAL ANALYSIS},
Year = {2015},
