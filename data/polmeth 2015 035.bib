@article{ WOS:000357015700006,
Author = {Suarez Collado, Angela},
Title = {Territorial Stress in Morocco: From Democratic to Autonomist Demands in
   Popular Protests in the Rif},
Journal = {MEDITERRANEAN POLITICS},
Year = {2015},
Volume = {20},
Number = {2, SI},
Pages = {217-234},
Month = {MAY 4},
Abstract = {This article analyses the evolution of popular protest in the Rif within
   the Moroccan context of contention. It considers the specificity of the
   demands expressed and the strategies for mobilization adopted as a
   result of a long-term process of regional activism. The article finds
   that protesters in the Rif have had agency to conduct their own
   strategies, using the opportunity structure opened at state level to
   advance their own agenda. The pre-existing mobilizing structures and the
   reproduction of patterns of centre-periphery tension in the course of
   the contention have fostered a progressive localization of protest in
   the region, which has strengthened regional identity and regionalist
   activism in the Rif.},
