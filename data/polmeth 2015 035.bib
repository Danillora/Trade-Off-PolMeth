@article{ WOS:000349151400003,
Author = {Cole, Michael and McAllister, Laura},
Title = {Evaluating and Theorising Committee Scrutiny: A UK Comparative
   Perspective},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2015},
Volume = {41},
Number = {2},
Pages = {220-239},
Month = {MAR 4},
Abstract = {This article addresses committee scrutiny undertaken through the three
   main UK devolved institutions (the Scottish Parliament, the National
   Assembly for Wales and the Northern Ireland Assembly). This is
   undertaken using a framework derived from scholarship about House of
   Commons select committees and a substantive comparative literature.
   Devolved committee scrutiny is thus compared and contrasted in relation
   to a three-phase analysis in terms of selection, evaluative and output
   elements.
   The findings are also discussed in relation to core elements of
   institutional theory - institutional path dependency, ideational path
   dependency, the logic of appropriateness and the logic of consequence.
   This connection is used to develop several testable theoretical
   propositions concerning committee scrutiny. Findings about UK devolved
   committee scrutiny (and also scrutiny undertaken through House of
   Commons select committees) are also related to scholarship about local
   government scrutiny in Britain and contrasts and similarities specified.
   Furthermore, some testable theoretical propositions are applied to
   British local scrutiny.},
