@article{ WOS:000355586500002,
Author = {Fan, Yongmao},
Title = {The Centre Decides and the Local Pays: Mandates and Politics in Local
   Government Financial Management in China},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2015},
Volume = {41},
Number = {4},
Pages = {516-533},
Month = {JUL 4},
Abstract = {In China, `the centre decides and the local pays' means the central
   government decides on policies but requires its local subordinates to
   provide the financial resources. The politics of this practice implies
   that local government has to take different strategies to cope with the
   unfunded mandates with various consequences. As an empirical study
   framed by Niskanen's rational choice theory and Dunleavy's
   `bureau-shaping' model, this paper examines how the unfunded mandates
   impact local government behaviour. Its main focus is the question of how
   the local officials respond, the extent to which they comply or resist
   and the techniques they use to adapt to these mandates. This paper finds
   when deciding how to pay the bill for the centre, local officials have
   to take a number of principles into consideration. They need to stick to
   the people-orientated principle and to finance money for salaries and
   operation to the extent that they can; they also have to see if the
   mandates are strictly implemented or popular among local people.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
