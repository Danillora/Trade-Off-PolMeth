@article{ WOS:000407681600004,
Author = {Nahuelpan Moreno, Hector},
Title = {Mapuche micropolitics against dispossession in neoliberal Chile. The
   dispute for the lafkenmapu (coastal territory) in Mehuin},
Journal = {IZQUIERDAS},
Year = {2016},
Volume = {30},
Pages = {89-123},
Month = {OCT},
Abstract = {This article analyzes the history of a Chilean coastal community's
   organizing to prevent the construction of a sewage pipe to dump
   industrial cellulous waste into the sea. This sewage pipe would be
   constructed by one of the largest cellulose producers in the world, and
   one of the most powerful economic groups in Chile (ARAUCO-Angelini). I
   analyze the strategies of resistance developed by the Mapuche-Lafkenche
   in opposition to the aggression of extractive capitalism, which in the
   current process of neoliberal globalization, revives historic patterns
   of racial dispossession against indigenous peoples. I argue that one of
   the main effects of the dispute has been the resurgence of new Mapuche
   political identities, which develop concepts of autonomy as daily
   construction, confronting the new forms of regulation of indigenous
   populations deployed by the state and capital.},
