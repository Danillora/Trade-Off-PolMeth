@article{ WOS:000395217200017,
Author = {Pickel, Susanne},
Title = {Concepts and Understandings of Democracy in Eastern and Western Europe},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {318+},
Abstract = {Democracy is a much-examined concept that combines several supposedly
   well-known characteiistics of a political system. Macro-concepts largely
   serve to identify the quality of democracy. Micro-concepts were
   previously known from research on political culture. The European Social
   Survey 2012 module `Europeans' understandings and evaluations of
   democracy' derives various items from these macro concepts and surveys
   citizens from 27 European countries. The frequency distributions of
   central concept variables suggest that understandings of democracy vary
   in respect to liberal and social-liberal characteristics of political
   systems. The divide between different understandings runs along
   differences in the economic situations and in the degree and extent of
   previous experience with democratic procedures.},
