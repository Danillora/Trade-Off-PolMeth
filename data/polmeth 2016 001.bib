@article{ WOS:000387973500003,
Author = {Marchildon, Allison},
Title = {Corporate responsibility or corporate power? CSR and the shaping of the
   definitions and solutions to our public problems},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2016},
Volume = {9},
Number = {1},
Pages = {45-64},
Abstract = {In this article, I will examine the notion of Corporate Social
   Responsibility (CSR), which is increasingly presented as the socially
   acceptable way to deal with ethical, social and environmental issues
   stemming from economic activities. Using a public problems framework of
   analysis, I will build upon empirical work in the field of
   bio-engineering in order to argue that, on the contrary, CSR is a
   contentious notion. I will demonstrate that this is because CSR
   increases corporations' involvement in the shaping of the definitions
   and solutions to our public problems and is thus based on a distribution
   of power and responsibility which is seen as potentially securing the
   power of corporations rather than offsetting it. In addition, this
   distribution does not correspond to most social actors' representation
   of how power and responsibility should be shared in our society.},
