@article{ WOS:000410450200005,
Author = {Aslan, Mustafa Cagatay},
Title = {THE RESTRICTIVE IMPACT OF PARTY IDEOLOGY ON PARTY STRATEGY: TURKEY'S
   RADICAL RIGHT THE NATIONALIST MOVEMENT PARTY AFTER JUNE 2015 ELECTIONS},
Journal = {ISTANBUL UNIVERSITY JOURNAL OF FACULTY OF POLITICAL SCIENCES-SIYASAL
   BILGILER FAKULTESI DERGISI},
Year = {2016},
Number = {55},
Pages = {67-86},
Month = {OCT},
Abstract = {Nationalist Action Party (MHP) embraces nativist ideology; thereby,
   representing radical-right politics in Turkey. The party's attitudes
   during the negotiations after the June 2015 parliamentary elections come
   within the purview of this article. The article reached to the
   conclusion that the party's ideology acted as a restrictive factor
   hampering political efforts to form a coalition government. Contrary to
   the argument that MHP has always been a political party defending
   established system, this article suggests that the MHP turned into an
   anti-political establishment party after June 2015 parliamentary
   elections due to the party's radical right ideology based on a strong
   belief in nativism.},
