@article{ WOS:000383832000004,
Author = {Raimondi, Sara},
Title = {From Schmitt to Foucault Inquiring the Relationship between Exception
   and Democracy},
Journal = {DEMOCRATIC THEORY-AN INTERDISCIPLINARY JOURNAL},
