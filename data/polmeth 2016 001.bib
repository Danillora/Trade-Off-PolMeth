Language = {English},
Affiliation = {Baumann, M (Corresponding Author), Univ Mannheim, Mannheim Ctr European Social Res MZES, Mannheim, Germany.
   Baumann, M (Corresponding Author), Univ Mannheim, Sch Social Sci, Mannheim, Germany.
   Baumann, Markus, Univ Mannheim, Mannheim Ctr European Social Res MZES, Mannheim, Germany.
   Baumann, Markus, Univ Mannheim, Sch Social Sci, Mannheim, Germany.},
DOI = {10.1111/1467-9477.12070},
