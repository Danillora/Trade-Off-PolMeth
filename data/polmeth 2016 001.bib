@article{ WOS:000410447600005,
Author = {Cristante, Nevio},
Title = {``HOW ABOUT RECONSTRUCTING ORIENTALISM?{''}: A SUMMARY AND OVERVIEW OF
   EDWARD SAID'S SATURATED CRITICISM},
Journal = {ISTANBUL UNIVERSITY JOURNAL OF FACULTY OF POLITICAL SCIENCES-SIYASAL
   BILGILER FAKULTESI DERGISI},
Year = {2016},
Number = {54},
Pages = {103-127},
Month = {MAR},
Abstract = {Edward Said is viewed as the creator of the academic development of
   post-colonialism, largely with his transmission of the concept
   ``Orientalism.{''} Orientalism is considered his magnum opus. He is a
   forerunner in the deconstruction of the so-called `Western' forms of
