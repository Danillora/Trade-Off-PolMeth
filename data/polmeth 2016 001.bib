@article{ WOS:000385628500003,
Author = {Benwell, Matthew C. and Pinkerton, Alasdair},
Title = {Brexit and the British Overseas Territories: Changing Perspectives on
   Security},
Journal = {RUSI JOURNAL},
Year = {2016},
Volume = {161},
Number = {4},
Pages = {8-14},
Abstract = {On 23 June 2016 the United Kingdom voted in a referendum to leave the
   European Union. As well as citizens of the UK, residents of the UK
   Overseas Territory of Gibraltar were also allowed to vote, with 96 per
   cent voting Remain. While the exact modes and timings of the UK's exit
   from the EU remain unclear, the campaign was characterised by
   increasingly heated debate and sharply contrasting visions for Britain
   and its relationship with the wider world in the twenty-first century.
   Matthew C Benwell and Alasdair Pinkerton argue that the UK's 2016 EU
   referendum campaign and the political and economic evaluations that it
   has invited have exposed a shifting relationship between the UK and its
   Overseas Territories (OTs) and demonstrate the role played by the EU in
   fostering the OTs' political, economic and regional security - a
   perspective often ignored by the OTs' so-called friends' and supporters.},
