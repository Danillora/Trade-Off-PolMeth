@article{ WOS:000450372600011,
Author = {Yorulmaz, Murat},
Title = {The Relation between Identity and Security: A Comparative Study on
   Kosovo and Macedonia},
Journal = {INSIGHT TURKEY},
Year = {2016},
Volume = {18},
Number = {1},
Month = {WIN},
Abstract = {As a concept, identity and security are deeply intertwined on many
   different levels. The relationship between identity and security evokes
   structural correlation and identity necessitates security. In this
   context, both the security of identity and the identity of the security
   are explored. The constructivist approach, which attaches significance
   to ``social construction{''} and ``interaction{''} in International
   Politics, claims that the meanings that are given by the traditional
   theories to the concepts of security and foreign policy must be
   reconsidered. This paper is an attempt to interpret the effect of
   identity perceptions on security as a means to avert conflicts and
   security threats. The aim is to provide an identity based explanation to
   security problems. In the study, Macedonia and Kosovo are chosen as case
   studies. The findings propose improved solutions to security problems
   and contribute knowledge applicable to other similar security threats.},
