@article{ WOS:000389246100004,
Author = {Baumann, Markus},
Title = {Constituency Demands and Limited Supplies: Comparing Personal Issue
   Emphases in Co-sponsorship of Bills and Legislative Speech},
Journal = {SCANDINAVIAN POLITICAL STUDIES},
Year = {2016},
Volume = {39},
Number = {4},
Pages = {366-387},
Month = {DEC},
Abstract = {Parliamentary party groups typically comprise members of parliament
   (MPs) with diverse preferences and different personal issue emphases. At
   the same time, speaking in plenary debates is a scarce resource
   controlled and allocated by parliamentary party group leaders. This has
   led recent research to investigate how speakers for plenary debates are
   selected. This contribution connects with this literature by asking
   whether MPs' personal issue emphases deviate from their parliamentary
   party groups' issue emphases. In order to answer this question, the
   issue emphases which individual MPs devote to a set of issues in an open
   access parliamentary instrument is measured and compared to the emphases
   MPs devote to these issues in speeches. The results for the 2005-9
   legislative period of the Norwegian Storting indicate that MPs differ in
   how closely aligned their issue emphases are in these two instruments
   and that these differences vary in a way consistent with theories on
   candidate selection and individualized MP behaviour.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
