@article{ WOS:000395217200010,
Author = {Hausteiner, Eva Marlene},
Title = {Democracy As A Provocation: Russian concepts of democracy between theory
   and propaganda},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {167+},
Abstract = {The article examines the boundaries of democratic ideas by examining two
   competing claims on the concept of democracy from post-Soviet Russia on
   the one hand Kremlin-based ideas of a ``sovereign democracy{''}, on the
   other hand organicist notions of democracy in current Neo-Eurasianism.
   These two alternative conceptions of democracy formulated as explicit
   attacks onto Western universalism are outlined in their discursive
   manifestations and contrasted with theciretical concepts and political
   programs from the ``West{''}. On the basis of these examples, the
   article discusses the fraught relationship between theory and propaganda
   in political thought on democracy as well as the discursive
   interdepencies between ``Western{''} and ``non-Western{''} (or even
   ``anti-Western{''}) claims on democracy.},
