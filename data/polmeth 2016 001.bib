@article{ WOS:000395253200004,
Author = {Tcach, Ivan},
Title = {Political Identities and Party Institutionalization. An Exercise of
   Comparative Analysis: Argentina (2003-2007) and Uruguay (2005-2010)},
Journal = {TEMAS Y DEBATES},
Year = {2016},
Number = {31},
Pages = {81-97},
Month = {JAN-JUN},
Abstract = {Is it possible to articulate notions from the field of political
   philosophy such as ``agonistic pluralism{''} and ``radical democracy{''}
