Author-Email = {mbaumann@uni-mannheim.de},
Affiliations = {University of Mannheim; University of Mannheim},
Funding-Acknowledgement = {German Research Foundation {[}DE1667/2-1]},
Funding-Text = {Support from the German Research Foundation (DE1667/2-1) is gratefully
