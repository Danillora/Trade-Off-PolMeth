@article{ WOS:000374488900005,
Author = {Wenzelburger, Georg and Staff, Helge},
Title = {German Exceptionalism? An Empirical Analysis of 20 Years of Law and
   Order Legislation},
Journal = {POLITICS \& POLICY},
Year = {2016},
Volume = {44},
Number = {2},
Pages = {319-350},
Month = {APR},
Abstract = {German law and order policies have been described as exceptional
   compared to other Western industrialized countries in that they seem to
   have been immune to a general trend toward increased toughness. However,
   recent studies have cast some doubt on this finding. The present article
   addresses this divergence and contributes to the literature in two
   respects. First, drawing on a new dataset on German federal legislation
   on law and order (1994-2013), we show that a slight trend toward tougher
   legislation is visible although talking about a punitive turn would be
   largely exaggerated. Second, we find that the explanation of this
   pattern is to be found in the peculiarities of the German political
   system which seems to have constrained any attempts to implement tougher
   policies: strong institutional veto points have hindered far-reaching
   proposals from being adopted and have pushed German law and order
   policies toward a middle way.},
Publisher = {WILEY},
