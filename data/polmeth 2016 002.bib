@article{ WOS:000386779200007,
Author = {Platte, Hendrik and Leuffen, Dirk},
Title = {German Arms Exports: Between Normative Aspirations and Political Reality},
Journal = {GERMAN POLITICS},
Year = {2016},
Volume = {25},
Number = {4},
Pages = {561-580},
Abstract = {Deciding on arms exports is a delicate matter in western democracies.
   Potential economic and security gains have to be weighed against
   normative and security concerns. This article explores how this tension
   is solved in Germany, a country that holds strong moral aspirations but
   at the same time lists among the top arms exporting nations worldwide.
   Using a newly compiled dataset, we quantitatively analyse German exports
   of major conventional weapons (MCWs) from 1992 to 2013. Our statistical
   findings do not support the claim that the human rights situation in
   recipient countries plays an important role for German arms export
   decisions. Our two-stage model therefore supports a trading state rather
   than civilian power reading of Germany, at least when it comes to arms
   export practices.},
