@article{ WOS:000379741600008,
Author = {Caglar, Ayse},
Title = {Displacement of European citizen Roma in Berlin: acts of citizenship and
   sites of contentious politics},
Journal = {CITIZENSHIP STUDIES},
Year = {2016},
Volume = {20},
Number = {5, SI},
Pages = {647-663},
Month = {AUG},
Abstract = {This article reflects on the tensions and anomalies of European
   citizenship from the perspective of European Union (EU)-citizen Roma,
   people who have an ambiguous positioning within the EU space because
   they are both European citizens, but also located as in but not of the
   EU space.' On the basis of events, discourses and negotiations
   surrounding the displacement of EU-citizen Roma in Berlin in 2009, this
   article explores how the borders of European citizenship are policed.
   The paper examines how the flattening of the inner EU political space is
   achieved through a set of semantic tactics, as well as extra-legal
   means, and how the desire of EU-citizen Roma to become asylum seekers
   becomes an act of citizenship constituting them as claimants of rights
   beyond their legal status and thus a threat to the social and political
   space of the EU. The article draws attention to the mutual constitution
