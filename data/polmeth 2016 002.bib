@article{ WOS:000399120200006,
Author = {Cameron, Michael P. and Barrett, Patrick and Stewardson, Bob},
Title = {Can Social Media Predict Election Results? Evidence From New Zealand},
Journal = {JOURNAL OF POLITICAL MARKETING},
Year = {2016},
Volume = {15},
Number = {4},
Pages = {416-432},
Abstract = {The importance of social media for election campaigning has received a
   lot of attention recently. Using data from the 2011 New Zealand general
   election and the size of candidates' social media networks on Facebook
   and Twitter, we investigate whether social media is associated with
   election votes and probability of election success. Overall, our results
   suggest that there is a statistically significant relationship between
   the size of online social networks and election voting and election
   results. However, the size of the effect is small and it appears that
   social media presence is therefore only predictive in closely contested
   elections.},
