@article{ WOS:000449594600011,
Author = {Folloni, Andre Parmo and Pitasi, Andrea},
Title = {Systemic epistemology in pierre delattre and construction of a complex
   legal science},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2016},
Volume = {112},
Pages = {339-373},
Month = {JAN-JUN},
Abstract = {The article sets out the main epistemological issues raised by the
   systems theory according to Pierre Delattre. The goal is to understand,
   from the thought of this author, implications for the development of a
   legal knowledge that does not disregard the social and economic problems
   in its theoretical formulations. Therefore, from the review of the
   author's main work and other writings, it presents the reflections of
   Delattre on the epistemological problems related to interdisciplinary
   scientific language, reductionism and holism as methods, relations
   between the parts and the whole, boundaries between system and
   environment, causal and purposive explanations, order out of chaos, and
   the formulation of scientific hypotheses for complex systems. It raises
   some of the questions that these problems pose to legal science and
   concludes by demonstrating the need for production of a complex legal
   knowledge and some of the problems that this construction must face.},
Publisher = {REVISTA BRASILEIRA ESTUDOS POLITICOS},
