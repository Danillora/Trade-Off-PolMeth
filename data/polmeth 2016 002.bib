@article{ WOS:000375947700007,
Author = {Piscopo, Jennifer M.},
Title = {When Informality Advantages Women: Quota Networks, Electoral Rules and
   Candidate Selection in Mexico},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2016},
Volume = {51},
Number = {3, SI},
Pages = {487-512},
Month = {JUL},
Abstract = {As gender quotas change the formal rules governing candidate selection,
   party leaders use informal practices in order to preserve the choicest
   candidacies for men. This article uses a critical case to highlight how
   the opposite also occurs. In Mexico, female elites built informal,
   cross-partisan networks that, in collaboration with state regulators,
   successfully eliminated political parties' practices of allocating women
   the least-viable candidacies. Traditional party elites rely on informal
   tactics to secure the status quo, but female party members devise their
   own strategies to force changes to candidate selection, signalling that
   informality cannot be theorized as wholly negative for women.},
