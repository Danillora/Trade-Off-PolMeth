@article{ WOS:000377265700011,
Author = {Curtice, John},
Title = {A Question of Culture or Economics? Public Attitudes to the European
   Union in Britain},
Journal = {POLITICAL QUARTERLY},
Year = {2016},
Volume = {87},
Number = {2},
Pages = {209-218},
Month = {APR-JUN},
Abstract = {Previous research has suggested that attitudes towards the European
   Union (EU) are shaped by two sets of considerations-economic and
   cultural. Using data from the 2015 British Social Attitudes survey, this
   article assesses which matters more in shaping attitudes in Britain
   towards the EU as the country prepares to vote in a referendum on
   whether it should remain in or leave the European Union. It shows that
   while concern about the cultural consequences of EU membership is
   widespread, voters are inclined to think that membership is economically
   beneficial. This cultural concern underpins a widespread scepticism
   about Europe, but voters are only likely to want to leave the EU if they
   are also convinced of the economic case for doing so.},
