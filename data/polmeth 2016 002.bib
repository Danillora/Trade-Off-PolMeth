@article{ WOS:000373376900008,
Author = {Boehmke, Frederick J. and Chyzh, Olga and Thies, Cameron G.},
Title = {Addressing Endogeneity in Actor-Specific Network Measures},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2016},
Volume = {4},
