@article{ WOS:000395217200028,
Author = {Weber, Ralph and Froehlich, Thomas},
Title = {Are There Elements of Deliberative Democracy in China? Comments on the
   discussion of the Zeguo experiment},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {550+},
Abstract = {This article discusses the international debate that evolved around a
   political experiment conducted for the first time in 2005 in the Chinese
   city of Zeguo. The central question we pose is whether the experiment
   can be understood as a successful theoretical and practical approach of
   deliberative democracy. We present recent criticism of James Fishkin's
   theoretical work on deliberative polling and analyse He Baogang's
   attempts at establishing an alternative model to deliberative democracy
   on the basis of deliberation in China in general and the
   Zeguo-experiment in particular by invoking Chinese traditions. The
   chosen example of the debate about the experiment in Zeguo shows that
   research on democracy in contexts beyond the West requires a challenging
   mediation of insights from democratic theory and politico-historical as
   well as empirical findings.},
