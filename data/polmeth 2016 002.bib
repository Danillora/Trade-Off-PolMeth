@article{ WOS:000374640600005,
Author = {Christensen, Skye and Bardall, Gabrielle},
Title = {Gender quotas in single-member district electoral systems},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2016},
Volume = {4},
Number = {2},
Pages = {246-267},
Month = {JUN},
Abstract = {Conventional knowledge on the effectiveness of gender quotas for
   enhancing women's political participation has, to date, been unanimous
   on the superiority of quotas in proportional representation (PR)
   systems. Yet this view overlooks the many possible alternatives to
   implementing gender quotas in single-member district (SMD) systems. This
   paper studies gender quotas (or temporary special measures) in SMD
   electoral systems. Drawing on case examples from Uganda, France, India
   and elsewhere, we refute the myth of the incompatibility of quotas in
   SMDs. Our research investigates and presents multiple ways in which
   quotas can be successfully implemented in SMDs.},
