@article{ WOS:000373044100002,
Author = {Musil, Pelin Ayan and Bilgin, Hasret Dikici},
Title = {Types of outcomes in factional rivalries: Lessons from non-democratic
   parties in Turkey},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2016},
Volume = {37},
Number = {2},
Pages = {166-183},
Month = {MAR},
Abstract = {In non-democratic parties, oppositional factions have difficulty making
   inroads to the top executive party organs. There are two consequences
   for these groups: party split or leadership removal. In the former case,
   the oppositional faction exits and establishes its own party. In the
   latter, the opposition succeeds in altering the balance of power by
   removing the leader and the party goes through change. This article
   suggests that the level of power concentration within the dominant
   faction matters for the type of outcome in factional rivalries. If the
   power is concentrated in the hands of a small elite, the divisions
   within the elite can help the oppositional faction remove the party
   leader. If the power resides only with a single leader, the oppositional
   faction is likely to lose the struggle against the dominant faction and
   decide to exit. This study explores the causal mechanisms involved by
   comparing six non-democratic parties from Turkey.},
Publisher = {SAGE PUBLICATIONS LTD},
