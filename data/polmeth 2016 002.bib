@article{ WOS:000391140100002,
Author = {Lario Bastida, Manuel},
Title = {Zapatismo in Mexican literature: the case of Juan Villoro},
Journal = {PENSAMIENTO AL MARGEN},
Year = {2016},
Number = {5},
Pages = {1-29},
Abstract = {The zapatismo chiapaneco is a sociopolitical movemen that has used
   widely and successfully speech as a combat weapon, in an attempt to
   construct a statement that was favorable to gain the social support of
   the Mexican people. This speech has been, in addition, eminently
   literary, whit the statements and novels that there his spokesperson,
   the subcommander Marcos has signed. This way, we see how the impact of
   the EZLN has been notable in the Mexican literature and in many other
   countries. In this article we propose to investigate the impact that
   Juan Villoro has had in the work of one of the most famous and respected
   Mexican writers, as well as to reveal the evolution of his political
   position before this movement in the last twenty years.},
