@article{ WOS:000391140100019,
Author = {Khan, Salah J.},
Title = {The poet facing the political: Rimbaud and the Paris Commune},
Journal = {PENSAMIENTO AL MARGEN},
Year = {2016},
Number = {5},
Pages = {307-323},
Abstract = {Rimbaud's literary production is not a self-contained body of work that
   can be isolated from the revolutionary world in which it is written. Far
   from an example of l'art pour l'art, his oeuvre, both practical and
   theoretical, demonstrates the poet's consistent support for radical
   historical movements, notably the Paris Commune, which he followed
   closely. In particular, he sustained a strong interest in a far-reaching
   critique of work. The figure of ``l `eclair{''} is one of the loci where
   the link between his poetics and his politics is most visible.},
