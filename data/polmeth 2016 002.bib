@article{ WOS:000393300500005,
Author = {Knaus, Gerald},
Title = {KEEPING THE AEGEAN AGREEMENT AFLOAT},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2016},
Volume = {15},
Number = {3},
Pages = {43-50},
Month = {FAL},
Abstract = {The EU-Turkey agreement on refugees that was reached on 18 March 2016
   has led to the relative slowing of refugee and migrant flows into
   Europe, with crossings across the Aegean from Turkey dropping
   significantly since it went into effect. However, due to several
   unresolved aspects of the agreement as well as the overall lack of
   effective implementation, it faces pressure from both sides and could
   risk collapse. In this article, the author argues that the EU needs to
   take several key steps: resettle refugees from Turkey directly, provide
   the financial assistance it promised to the refugees in Turkey, and
   commit to helping Greece with its overburdened asylum and relocation
   services. In turn, the Turkish side needs to send a clear signal to the
   EU that it is a safe third country, and ensure that there is a
   transparent procedure for what happens to every person returned from
   Greece. The EU needs to recognize this as the key condition for visa
   liberalization.},
