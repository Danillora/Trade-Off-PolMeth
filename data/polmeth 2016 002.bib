@article{ WOS:000382727700008,
Author = {Callari, Antonio},
Title = {Marxism from the Outside: A Take on World of the Third and Global
   Capitalism},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2016},
Volume = {28},
Number = {2},
Pages = {263-270},
Abstract = {This essay situates the book World of the Third and Global Capitalism on
   the landscape of a theoretical journey of Marxist theory over the last
   half century. It paints the beginning of the journey as marked by the
   unraveling of a certain form of the Marxist subject of history, the
   classical working class; it characterizes the journey as an exploration
   into and a reconstruction of the domains of economy and of subjectivity;
   it posits World of the Third at the end of the journey, as a text that,
   by virtue of a Lacanian psychoanalytic construction of agency,
   reauthorizes a Marxist subject. The essay argues that World of the Third
   comes to reauthorize a Marxist subject because of its reflection on and
   transformation of the category of the third world, but also that the
   reauthorization it produces extends beyond ``third world/world of the
   third{''} and captures the nature of the Marxist subject in the center
   as well.},
