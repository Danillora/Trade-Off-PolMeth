@article{ WOS:000408328100006,
Author = {Pan, Zhongqi},
Title = {Guanxi, Weiqi and Chinese Strategic Thinking},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Year = {2016},
Volume = {1},
Number = {2},
Pages = {303-321},
Month = {JUN},
Abstract = {In their perceptions and preferences of Guanxi, the Chinese follow a
   relational way of thinking, which is in stark contrast to the American
   generic way of thinking. This difference lies in how Chinese and
   Americans behave in playing board games and engaging with others in real
   life. The game of Weiqi signifies the Chinese way of gaming, whereas the
   game of Chess denotes the Western way of gaming. Chinese strategic
   thinking is relational and best exemplified in the way that the Chinese
   perceive and deal with various Guanxi in the game of Weiqi. The Chinese
   prefer to define their strategic goals in relative terms, employ a
   combination of various means to achieve good and stable Guanxi with
   other countries without defining any of them, or being defined by any of
   them, as an enemy. The Chinese also tend to favor a flexible foreign
   strategy with contingent adaptation as its principle, a favorable
   balance of shi as its linchpin, and such shortcomings as
   nearsightedness, greediness and impatience as its taboos.},
