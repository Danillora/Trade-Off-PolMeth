@article{ WOS:000379604200009,
Author = {Christie, Edward Hunter},
Title = {The Design and Impact of Western Economic Sanctions against Russia},
Journal = {RUSI JOURNAL},
Year = {2016},
Volume = {161},
Number = {3},
Pages = {52-64},
Month = {JUN-JUL},
Abstract = {The economic sanctions imposed by the West against Russia in 2014,
   following the latter's aggression in Ukraine, were deliberately limited
   but nevertheless significant, their impact distinguishable from that of
   the fall in oil prices that occurred in late 2014. Edward Hunter
   Christie argues that these sanctions, in combination with credible
   threats of further sanctions, appear to have had an effect in limiting
   Russian aggression in Ukraine, even though they have not led to a
   reversal of facts on the ground. This article also explores the
   possibility that, in the absence of other coercive components to
   underpin diplomatic efforts, the earlier application of more robust
   economic sanctions might have had stronger effects on Russia's
   behaviour.},
