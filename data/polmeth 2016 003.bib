@article{ WOS:000415329900008,
Author = {Qian Jiwei},
Title = {Rural Poverty Alleviation in China: Recent Reforms and Challenges},
Journal = {EAST ASIAN POLICY},
Year = {2016},
Volume = {8},
Number = {4},
Pages = {82-90},
Month = {OCT-DEC},
Abstract = {The ``13th Five-Year Outline Plan{''} (2016-2020) has poverty
   alleviation as one of the major tasks for the Chinese government. Two
   sets of policies have been underlined: poverty alleviation (jinzhun
   fupin) plan to improve the targeting accuracy of these programmes and
   policies to focus on adjacent regions with disadvantaged natural and
