@article{ WOS:000371658500003,
Author = {Green, Donald P. and Schwam-Baird, Michael},
Title = {Mobilization, participation, and American democracy: A retrospective and
   postscript},
Journal = {PARTY POLITICS},
Year = {2016},
Volume = {22},
Number = {2, SI},
Pages = {158-164},
Month = {MAR},
Abstract = {Rosenstone and Hansen's (1993) treatise on the mobilizing effects of
   campaign activity and social interactions marks a turning point in
   scholarship on political participation. This essay looks back at this
   pivotal work and the experimental research agenda that emerged from it.
   Decades of subsequent research have reinforced and extended the book's
   key claims about the role of mobilization in promoting voter turnout.
