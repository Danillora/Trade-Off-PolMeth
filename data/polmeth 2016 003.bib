@article{ WOS:000388731900001,
Author = {Greig, Alastair},
Title = {SENTIMENT ANALYSIS OF THE BBC'S DIGITAL CONTENT DURING THE 2014 SCOTTISH
   INDEPENDENCE REFERENDUM CAMPAIGN},
Journal = {SCOTTISH AFFAIRS},
Year = {2016},
Volume = {25},
Number = {4},
Pages = {419-435},
Month = {NOV},
Abstract = {In Scotland, the UK's media's coverage of the Scottish independence
   Referendum 2014 remains controversial. This was probably most true of
   the coverage from the UK's public service broadcaster, the BBC. In fact,
   perceived bias in favour of the No campaign by the BBC resulted in
   several demonstrations against the organisation outside its Scottish
   headquarters. This paper presents empirical results of a word frequency
   analysis of BBC's reporting of the referendum campaign between June 2014
   until the day of the referendum on the 18th September. It finds that
   while the BBC's digital content appears less subjective than other major
   news outlets, there is nevertheless evidence that suggests that the
   BBC's coverage, on the whole, was more favourable to the No campaign and
   offers an empirical explanation of why that might be the case.},
Publisher = {EDINBURGH UNIV PRESS},
