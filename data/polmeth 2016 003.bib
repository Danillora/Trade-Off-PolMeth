@article{ WOS:000395217200029,
Author = {Mosler, Hannes B.},
Title = {Democratic Discourse in South Korea: The tension of two meanings of
   liberal-democratic basic structure},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {567+},
Abstract = {This chapter examines based on representative controversies during the
   recent ten years the most recent manifestations of the discourse
   competition over determining the conceptualization of democracy in South
   Korea the state identity controversy (2004-5), the textbook controversy
   (2008-9), the curriculum controversy, and the party ban controversy
   (2013-4). The basic currents of the present democracy discourse are
   presented along the competing narratives of ``liberal democratic basic
   order{''} (right-conservatives) and ``free democratic basic order{''}
   (liberal-progressives). The analysis shows a weakly developed discursive
   disposition represented in exclusively opposing interpretations of the
   fdBO leading to an idiosyncratic concept of democracy.},
