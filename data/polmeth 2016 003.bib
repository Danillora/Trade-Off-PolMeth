@article{ WOS:000407490200006,
Author = {Zingher, Joshua},
Title = {The relationship between bias and swing ratio in the Electoral College
   and the outcome of presidential elections},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2016},
Volume = {26},
Number = {2},
Pages = {232-252},
Abstract = {In this paper, I assess how the outcomes of presidential elections are
   affected by the presence (or lack) of partisan bias in the Electoral
   College. There have been three instances (1876, 1888 and 2000) since the
   end of the Civil War where the party that lost the popular vote won the
   Electoral College. These instances raise the question of whether
   partisan bias consistently influences presidential election outcomes? I
   answer this question by first measuring partisan bias and then using
   these estimates to assess how partisan bias affects a party's odds of
   winning a presidential election. I find that the presence of partisan
   bias provides a sizable, but not insurmountable, obstacle for the
   disadvantaged party.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
