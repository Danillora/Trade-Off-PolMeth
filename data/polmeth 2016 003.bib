@article{ WOS:000382726000003,
Author = {Diskin, Jonathan},
Title = {Ethics and Epistemology in The Economist's
   Oath},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2016},
Volume = {28},
Number = {1},
Pages = {16-24},
Abstract = {This essay examines the epistemological implications of DeMartino's
   ethical demand of the economics profession. In particular, I explore why
   some on the left are ill at ease with an ethical challenge that is, in
   some respects, neutral about the method of producing economic knowledge,
   a challenge that allows that ``one can be an ethical neoliberal just as
   one can be an ethical Marxist.{''} I argue that DeMartino's focus on the
   ``epistemic insufficiency{''} of economists' truth claims opens up a
   broader epistemological challenge that remains implicit in his book and
   that dissolves a kind of rationalist scientism as an epistemological
   practice.},
