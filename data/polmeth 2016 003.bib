@article{ WOS:000365515200004,
Author = {James, Toby S.},
Title = {Neo-Statecraft Theory, Historical Institutionalism and Institutional
   Change},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2016},
Volume = {51},
Number = {1},
Pages = {84-110},
Month = {JAN},
Abstract = {This article provides a critical examination of the contribution that
   statecraft theory, which has been subject to recent revision and
   development, makes to the literature on institutional change. It
   articulates an emergent neo-statecraft approach that offers an agent-led
   form of historical institutionalism. This overcomes the common criticism
   that historical institutionalists underplay the creative role of actors.
   The article also argues that the approach brings back into focus the
   imperatives of electoral politics as a source of institutional change
   and provides a macro theory of change which is also commonly missing
   from historical institutionalist work. It can therefore identify
   previously unnoticed sources of stability and change, especially in
   states with strong executives and top-down political cultures.},
