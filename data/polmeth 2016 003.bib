@article{ WOS:000371546300008,
Author = {Bogdanor, Vernon},
Title = {The British General Election of 2015 and the Rise of the Meritocracy},
Journal = {POLITICAL QUARTERLY},
Year = {2016},
Volume = {87},
Number = {1},
Pages = {39-45},
Month = {JAN-MAR},
Abstract = {Many on the left hoped that the 2015 general election in Britain would
   prove a social democratic moment. Instead, it proved a nationalist
   moment, since the only parties radically to increase their vote were
   UKIP and the SNP. This mirrored trends on the Continent, where
   nationalist parties on the right and the left have been the
   beneficiaries of the financial collapse of 2008. These parties exploit a
   new social cleavage between those who benefit from globalisation and
   those left behind. The new parties exploit issues of identity rather
   than economics, and these issues-whether Britain remains in the European
   Union, whether mass immigration continues and whether Scotland remains
   in the United Kingdom-are likely to dominate the 2015 parliament.},
