@article{ WOS:000380717000003,
Author = {Anderson, Sarah E. and Butler, Daniel M. and Harbridge, Laurel},
Title = {Legislative Institutions as a Source of Party Leaders' Influence},
Journal = {LEGISLATIVE STUDIES QUARTERLY},
Year = {2016},
Volume = {41},
Number = {3},
Pages = {605-631},
Month = {AUG},
Abstract = {Legislators' actions are influenced by party, constituency, and their
   own views, each weighted differently. Our survey of state legislators
   finds that legislator's own views are the strongest influence. We also
   find that institutions are an important source of party leaders'
   influence. Legislators in states where members rely more on party
   leadersstates without term limits, with less professional legislatures,
   and where the majority party controls the agendaput more weight on
   leaders' preferences. Beyond direct party influence, the views of party
   leaders are preemptively incorporated into legislators' preferences when
   the rules of the legislature make party leaders more powerful.},
