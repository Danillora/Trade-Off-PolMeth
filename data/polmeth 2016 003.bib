@article{ WOS:000378586400006,
Author = {Ewert, Stefan and Hein, Michael},
Title = {How the Types of Procedure Affect the Degree of Politicization of
   European Constitutional Courts. A Comparative Study of Germany,
   Bulgaria, and Portugal},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Volume = {57},
Number = {1},
Pages = {55-80},
Abstract = {This article examines the influence of the types of procedure on the
   degree of politicization of European constitutional courts. Based on an
   analysis of the German, Bulgarian and Portuguese cases, we show that the
   lower the legal requirements on the part of the applicants and the
   broader their opportunities to weaken political opponents, the higher is
   the degree of politicization of the court decisions. This moderating
   effect appears equally for all groups of applicants.},
