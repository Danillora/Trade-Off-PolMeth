@article{ WOS:000391223000011,
Author = {Garcia Ruiz, Pablo and Buil, Pilar and Sole Moratilla, Maria Jose},
Title = {Risk Consumption: Children and Online Gaming. The Problem of
   ``Responsible Gambling{''}},
Journal = {POLITICA Y SOCIEDAD},
Year = {2016},
Volume = {53},
Number = {2},
Pages = {551-575},
Abstract = {Online gambling is a popular activity among adolescents. However, there
   has been a notable increase in the number of young people who suffer or
   are on the verge of pathological gambling. We review the impact of
   online gambling on young people and discuss the desiderability of the
   concept of ``gambling responsibly{''} in order to alert of their risks
   and effectively prevent access to minors. The main factors associated
   with pathological gambling are the age of start, the family environment,
   the influence of advertising, the consumption of stimulants, and the
   attitudes of the peer group. Both the government and the gaming industry
   itself should consider these factors and develop comprehensive plans
   that ensure a safe and controlled model game. In this context,
   advertising must take into account criteria of consumer protection
   knowing that even if they are not allowed, children can easily have
   access to online gambling. All agents involved, including public and
   social agents, must provide mechanisms for prevention and awareness of a
   problem that arouses little social consciousness and excessive
   carelessness.},
