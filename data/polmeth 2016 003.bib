@article{ WOS:000396405900001,
Author = {Mousazadeh, Reza and Basiri, Abbas and Babaee, Mojtaba and Tabasi, Ali
   Tavakoli},
Title = {Analyzing the Legal Dimensions of Unmanned Combat Aerial Vehicle in the
   International Law},
Journal = {JOURNAL OF POLITICS AND LAW},
