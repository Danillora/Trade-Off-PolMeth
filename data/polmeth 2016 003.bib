@article{ WOS:000395217200002,
Author = {Schubert, Sophia and Weiss, Alexander},
Title = {Conceptions of Democracy beyond the West: A theoretical framework for a
   new research field},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {6+},
Abstract = {In this introductory chapter we argue for the consideration of
   non-Western conceptions of democracy in theoretical and empirical
   democracy research and we develop a theoretical framework for the
   investigation of non-Western conceptions of democracy in an
   interdisciplinary perspective. First, we distinguish three forms in
   which conceptions of democracy appear: as theories, discourses, and
   attitudes and we show that these have mainly been analysed within the
   context of three different but compatible research strands: comparative
   political theory, area studies and comparative democracy research. In
   order to integrate these three perspectives, we then suggest two common
   premises a context premise and a travelling premise, two types of
   possible research questions and three methodological characteristics. In
   a final step, we suggest a common basic working definition of both
   `democracy' and the `West' and, as an alternative to both conceptual
   universalism and relativism, we present a double taxonomy of democracy
   which 1) proposes deduction from a heuristic and `minimal' concept of
   democracy in order to identify concrete non-Western conceptions of
   democracy which are to be investigated within contextual analyses and
   which 2) proposes cross-culturally informed inductive derivations
   towards a global and transcultural concept of democracy.},
