@article{ WOS:000443095200001,
Author = {Kuosmanen, Jaakko},
Title = {Worlds Apart: Bridging the Gap between Human Rights and Fiscal
   Transparency},
Journal = {NORDIC JOURNAL OF HUMAN RIGHTS},
Year = {2016},
Volume = {34},
Number = {1},
Pages = {1-20},
Abstract = {This article examines the existing gap between human rights and fiscal
   transparency discourses, and the reasons for bridging this gap. Fiscal
   transparency work is very much insulated from human rights work,
   regardless of the fact that there are close links between the two
   concepts. Human rights constitute a justificatory foundation for fiscal
   transparency measures, and fiscal transparency can be instrumentally
   helpful for the realisation of human rights. This article suggests that
   closer dialogues between the two discourses can be helpful for human
   rights work. Fiscal transparency measures can, depending on how they are
   designed, be utilised in human rights work to hold states accountable
   for non-compliance with obligations arising from human rights. Also,
   fiscal transparency measures can be helpful for deliberative efforts to
   establish what human rights actually demand from fiscal policies.},
