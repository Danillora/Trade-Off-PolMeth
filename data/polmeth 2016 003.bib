@article{ WOS:000395217200022,
Author = {Schmidt, Rainer},
Title = {Challenging Liberal Democracy: Constitutional narratives of validity in
   reform projects from the political left in Latin America},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {437+},
Abstract = {Latin America moved to the left in the 2000s. During this time a new
   constitutionalism looked for answers to the problems of lack of
   participation, integration and legitimation of traditional
   constitutional models of mostly Western origin. How can democratic
   governance be stabilized and institutionalized in contexts in which
   those narratives that guarantee stability and validity, like the
   separation of law and politics or the independence of the constitutional
   court, are heavily disputed. Examples from different constitutional
   cultures like Bolivia and Brazil can present different strategies to
   deal with personalized models of domination in Latin American populism.
   These strategies can be of interest for the European answers to growing
   populist movements.},
Publisher = {SPRINGER},
Address = {ONE NEW YORK PLAZA, SUITE 4600, NEW YORK, NY, UNITED STATES},
Type = {Article},
Language = {German},
