@article{ WOS:000397459100004,
Author = {Loisel, Sebastien},
Title = {The ``Peace Facility for Africa{''}: sectoral interactions in the
   definition of a European crisis management instrument},
Journal = {POLITIQUE EUROPEENNE},
Year = {2016},
Volume = {51},
Number = {1},
Pages = {86-116},
Abstract = {The interactions at play in the definition of an EU comprehensive
   approach to crisis management are often analysed in the literature
   through intergovernmental or interinstitutional lenses. The study of the
   creation of the Peace Facility for Africa in 2003-2004 shows on the
   contrary struggles that cut across Member States and European
   institutions and follow sectoral and cross-sectoral logics. These
   struggles reflect not only divergent bureaucratic interests but also
   different social representations pertaining to the security and
   development sectors.},
