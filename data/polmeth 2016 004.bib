@article{ WOS:000374765100002,
Author = {`t Hart, Paul and Schelfhout, David},
Title = {Assessing prime-ministerial performance in a multiparty democracy: The
   Dutch case},
Journal = {ACTA POLITICA},
Year = {2016},
Volume = {51},
Number = {2},
Pages = {153-172},
Month = {APR},
Abstract = {This article discusses the use of expert ranking methodology for
   assessing the performance and `place in history' of heads of government,
   in particular prime ministers (PMs). It reports an expert ranking study
   of PMs in the Netherlands. Open/spontaneous as well as criteria-led,
   more detailed modes of performance assessment are compared. Moreover,
   the study's findings, pertaining as they do to a PM hemmed in by the
   need to manage tenuous coalitions in a multi-party consensual democracy,
   are compared against those of similar exercises conducted for PMs in
   majoritarian, Westminster style democracies, suggesting that
   prime-ministerial reputations in multiparty democracies are made on the
   strength of their longevity in office, their coalition management skills
   and the policy legacy of their governments.},
