@article{ WOS:000386916000010,
Author = {Fernandez, Luciana Moretti},
Title = {Brazil: communicational and political dimensions in the world of crime},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2016},
Volume = {9},
Number = {2},
Pages = {356-379},
Abstract = {This article discusses the political positioning of the crime group
   Primeiro Comando da Capital (PCC) during the attacks on Sao Paulo in
   2006 and on the continuous recruitment of new generations in crime. A
   situational map of programmatic violence in Brazil was built from 30
   apology for crime videos, submitted to Situational Analysis. The attacks
   stand in the map as a phenomenon of political communication of the world
   of crime in the public sphere, showing continuities between the 2006
   terrorist format and a situation suggestive of the radicalisation of new
   generations based on hate, adherence to the political/armed wing of the
   commands, and cognitive-affective shielding of young crime soldiers.},
