@article{ WOS:000382883200008,
Author = {Inoguchi, Takashi and Lien Thi Quynh Le},
Title = {Toward Modelling a Global Social Contract: Jean-Jacques Rousseau and
   John Locke},
Journal = {JAPANESE JOURNAL OF POLITICAL SCIENCE},
