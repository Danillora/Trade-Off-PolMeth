@article{ WOS:000372555900001,
Author = {Brooke, Henry St. George and Gans-Morse, Jordan},
Title = {Putin's Crackdown on Mortality: Rethinking Legal Nihilism and State
   Capacity in Light of Russia's Surprising Public Health Campaigns},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2016},
Volume = {63},
Number = {1},
Pages = {1-15},
Month = {JAN-FEB},
Abstract = {Facing a demographic crisis, the Russian government recently introduced
   public health measures to reduce alcohol consumption, traffic
   fatalities, and tobacco use. The relative success of these measures
   challenges common assumptions about Russia's culture of legal nihilism
   and lack of state capacity. Drawing on two original surveys of smokers
   at Russian universities, we provide evidence that low legal compliance
   results from expectations about low enforcement, not from a unique legal
   culture. To account for unexpectedly stringent enforcement of public
   health laws, we offer a theory of selective state capacity. In
   contemporary Russia, the top leadership's personal endorsements of
   policy initiatives make clear to lower-level officials which rules must
   be enforced.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
