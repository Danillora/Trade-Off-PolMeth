@article{ WOS:000411079300012,
Author = {Rrustemi, Arlinda},
Title = {Some Challenges of the State- and Peace-Building Governance Mechanisms
   The Case of Kosovo},
Journal = {SECURITY AND HUMAN RIGHTS},
Year = {2016},
Volume = {27},
Number = {1-2, SI},
Pages = {187-199},
Abstract = {It is imperative to understand global developments in statebuilding in
   order to more efficiently influence peace and conflict processes. This
   article discusses the evolution of global governance mechanisms on
   state- and peace-building that can assist researchers and practitioners
   in gaining an in depth understanding of different case studies. It
   uncovers some limitations and benefits of the global governance
   mechanisms assisting fragile zones. It concludes that institutions
   attempt to increase performance, as seen with the regular establishment
   of new deals and the deployment of new actors on the ground, however
   challenges remain, as illustrated in the case of Kosovo, in relation to
   lack of legitimacy, mismatch of national identity needs, economic
   inequality gap and fragile security and inter-ethnic relations. Instead,
   it argues to use the resources on the ground rather than devise new
   missions to deal with peacebuilding challenges.},
