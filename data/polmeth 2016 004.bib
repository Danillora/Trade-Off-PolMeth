@article{ WOS:000393492100002,
Author = {Castilla, Victoria and Fernanda Miguel, Maria},
Title = {Care, inequity and market. Concepts and experiences of maternity in
   women-mothers migrants' domestic workers and middle and high class
   employer's women in Buenos Aires},
Journal = {CIUDAD PAZ-ANDO},
Year = {2016},
Volume = {9},
Number = {1},
Pages = {11-21},
Month = {JAN-JUN},
Abstract = {Based on an ethnographic study this paper analyzes and describes the
   concepts arid experiences of maternity in both women-mothers migrants
   who are domestic workers in the Metropolitan Buenos Aires area arid the
   professionals working women who employ them. We analyze the
   characteristics, tensions, conflicts or correspondences that arise from
   the perspectives of the mothers between the normative models of
   motherhood, affection and experience of care, attention arid
   containment; considering the differential logic that social and economic
   conditions printed on them. We argue the existence of a stratified
   reproduction in which class, gender and nationality appear as
   distinguishing features of the various maternity experiences, and
   however, they share a common resolution of private care activities. For
   some it is the commodinc.ation and familiarization, for the others the
   transnational networks of care.},
