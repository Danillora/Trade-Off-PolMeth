@article{ WOS:000372679200006,
Author = {Lena Krook, Mona and Restrepo Sanin, Juliana},
Title = {Gender and Political Violence in Latin America: Concepts, Debates and
   Solutions},
Journal = {POLITICA Y GOBIERNO},
Year = {2016},
Volume = {23},
Number = {1},
Pages = {127-162},
Abstract = {Violence against women in politics is increasingly recognized around the
   world but especially in Latin America as an emerging tactic to deter
   women's political participation. We survey how this concept has been
   defined by academics and practitioners across the region largely in
   terms of physical and psychological violence and draw on global data and
   research in various disciplines to propose expanding this concept to
   include two further forms of violence: economic and symbolic. We provide
   examples of all four types of violence in Latin American countries and
   then consider a range of solutions that might be pursued in light of
   this broader definition. We emphasize that a comprehensive approach
   provides the best means for tackling violence in all its forms.},
Publisher = {CENTRO DE INVESTIGACION Y DOCENCIA ECONOMICAS},
