@article{ WOS:000363665900002,
Author = {Grande, Edgar and Hutter, Swen},
Title = {Beyond authority transfer: explaining the politicisation of Europe},
Journal = {WEST EUROPEAN POLITICS},
Year = {2016},
Volume = {39},
Number = {1, SI},
Pages = {23-43},
Month = {JAN 2},
Abstract = {While there is increasing evidence that European integration has been
   politicised, knowledge of the driving forces of this process is still
   limited. This article contributes to the research by examining the
   importance of authority transfers to the EU as drivers of
   politicisation. It innovates in two ways. First, it extends the
   authority transfer argument by highlighting the mobilising power of
   membership conflicts; second, it analyses the relevance of national
   opportunity structures, referenda in particular, and mobilising
   strategies for politicisation. Empirically, it traces politicisation in
   public debates on every integration step (treaty reforms and
   enlargement) from the 1970s to the late 2000s in six West European
   countries (Austria, Britain, France, Germany, Sweden, and Switzerland),
   based on a quantitative content analysis of newspaper coverage.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
