@article{ WOS:000390388400006,
Author = {Greer, Christina M.},
Title = {African-American candidates for the presidency and the foundation of
   black politics in the twenty-first century},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2016},
Volume = {4},
Number = {4},
Pages = {638-651},
Month = {DEC},
Abstract = {Numerous scholars have attempted to better evaluate the behavior of
   blacks in the electorate. This paper places black electoral behavior on
   the national level in historical context. This paper explores the
   history of African-Americans running for executive office, how the
   50-plus failed campaigns+ and the significance of alternative party and
   symbolic candidacies directly influenced the successful presidential
   campaign of Barack Obama in 2008. I argue that symbolic and alternative
   party candidates have contributed to the increase in participation and
   African-American incorporation into the two-party system and have
   contributed to the election of the first African-American president
   after roughly 150 years of black involvement in executive politics.},
