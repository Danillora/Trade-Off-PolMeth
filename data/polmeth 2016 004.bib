@article{ WOS:000382490100002,
Author = {Ogutcu-Fu, Sema Hande},
Title = {Outside the Battlefield: In-Group Political Dynamics of Civil Conflict
   Negotiations and Settlements},
Journal = {POLITICAL RESEARCH QUARTERLY},
