@article{ WOS:000382726000005,
Author = {Kayatekin, Serap A.},
Title = {The Antimodernist Challenge in George F. DeMartino's The
   Economist's Oath: A Call to Humility},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2016},
Volume = {28},
Number = {1},
Pages = {38-46},
Abstract = {This commentary focuses on two dimensions of DeMartino's The Economist's
   Oath. The first is the antimodernist critique the author raises, of
   economics as a profession and economists as its practitioners, on the
   grounds of epistemic insufficiency: the impossibility of predicting the
   impact of economic policies. As economists cannot predict the outcome of
   their policies and the harm they can cause, DeMartino issues a call to
   humility as the central element of a professional economic ethics. The
   second point concerns DeMartino's thesis about the political agnosticism
   of professional ethics in economics. It ends by extending DeMartino's
   argument with a preliminary conceptualization of harm.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
