@article{ WOS:000399173600001,
Author = {Balmori de la Miyar, Jose Roberto},
Title = {The Economic Consequences of the Mexican Drug War},
Journal = {PEACE ECONOMICS PEACE SCIENCE AND PUBLIC POLICY},
Year = {2016},
Volume = {22},
Number = {3},
Pages = {213-246},
Month = {AUG},
Abstract = {Military crackdowns often disrupt economic development by exacerbating
   violence. This paper examines the case of the Mexican Drug War,
   employing synthetic control methods. To prove causality, I use variation
   on statewide military operations, as well as the rollout of the war.
   Findings indicate a decrease in GDP per capita equal to 0.5\%, in states
   with military operations. Determinants by which the Mexican Drug War
   hampered economic development include a proportional reduction in
   consumption per capita, and a decline in productive investment of at
   least 0.3\%, driven by a drop of 3.2\% in commercial credit granted to
   businesses.},
