@article{ WOS:000390671200001,
Author = {Di Gregorio, Michael and Merolli, Jessica L.},
Title = {Introduction: affective citizenship and the politics of identity,
   control, resistance},
Journal = {CITIZENSHIP STUDIES},
Year = {2016},
Volume = {20},
Number = {8, SI},
Pages = {933-942},
Month = {DEC},
Abstract = {In this introduction, we situate the growing and diverse literature on
   affect and citizenship as only beginning to capture the world around us,
   where tensions and hope run high. We present a brief overview of the
   literature on affective citizenship, with particular attention to how
   affect is implicated in the politics of identity, resistance, and
   control. We argue that this issue builds on nascent discussions already
   underway in journals like Citizenship Studies, and that the pieces
   included here extend our understanding of both the emancipatory and
   exclusionary potential of affective citizenship.},
