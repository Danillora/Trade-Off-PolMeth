@article{ WOS:000382868600006,
Author = {Pino Uribe, Juan Federico and Cardenas Diaz, Javier Alonso},
Title = {Patronage: the incessant game of politicians and customers in the
   construction and reproduction of subnational democracy},
Journal = {REFLEXION POLITICA},
Year = {2016},
Volume = {18},
Number = {35},
Pages = {58-70},
Month = {JUN},
Abstract = {Patronage has been regarded as one of the main ways in which a political
   system, mostly in developing and rural societies, is legitimized and
   reproduced to establish interactive relations between politicians and
   citizens in a framework of exchange for political support. This
   relationship has been mostly considered depraved for democracy, while
   other research has pointed out the role that patronage has played in
   articulating the policy of marginal sectors and the provision of
   services and goods in contexts where institutions are precarious. From
   this tension, this article resumes some of the major works in this topic
   to analyze different theoretical, conceptual and methodological
   perspectives of patronage.},
