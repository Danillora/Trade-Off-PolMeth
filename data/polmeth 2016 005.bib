@article{ WOS:000397462900005,
Author = {Bastos, Francisco Roa},
Title = {``European Academy{''} or ``europaische Akademie{''} ? The German
   Rooting of European Studies, and its conditions of possibility},
Journal = {POLITIQUE EUROPEENNE},
Year = {2016},
Volume = {52},
Number = {2},
Pages = {114-144},
Abstract = {This article studies the development of ``European Studies{''} in
   Germany since 1945. The quantitative importance of German scholar
   publications about Europe, leads to the analysis of the factors that
   favoured an early institutionalization of ``European Studies{''} in
   Germany. Among them, the existence of significant public support for a
   ``political education{''} (politische Bildung) on national and European
   Affairs, but also the means available for the structuring of an active
   European federalist movement around Europa Union Deutschland and its
   satellite organizations, are essential for understanding the German
   rooting of the ``European Academy{''} and ``European studies{''} in
   general.},
