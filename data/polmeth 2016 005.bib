@article{ WOS:000386924400010,
Author = {Madonna, Anthony J. and Monogan, III, James E. and Vining, Jr., Richard
   L.},
Title = {Confirmation Wars, Legislative Time, and Collateral Damage: The Impact
   of Supreme Court Nominations on Presidential Success in the US Senate},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2016},
Volume = {69},
Number = {4},
Pages = {746-759},
Month = {DEC},
Abstract = {Presidents often see a Supreme Court nomination as an opportunity to
   leave a lasting mark on policy. Recent studies speculate that focusing
   on Supreme Court nominees affects presidential success beyond the
   confirmation process, but this has not been established systematically.
   We develop and test a hypothesis stating that presidents who get into a
   battle to promote a controversial Supreme Court nominee will see delays
   and failures in their efforts to promote their legislative agenda in the
   Senate and fill lower level judicial vacancies. We test our theory using
   data on presidential policy agenda items from 1967 to 2010 and lower
   level judicial nominations from 1977 to 2010. We find that increased
   efforts in promoting confirmation reduce the likelihood of timely Senate
   approval of important policy proposals and nominees to federal district
   courts.},
