@article{ WOS:000395217200014,
Author = {Hoss, Anne},
Title = {Therisanyo: Imagination and praxis of deliberatice democracy in Botswana},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {245+},
Abstract = {Political thought in Botswana is importantly influenced by the
   traditional principle of Therisanyo ({''}consultation and
   consensus{''}). The article presents the core ideas and the present-day
   practice of the Botswanian model of consultation and introduces them to
   the theory of deliberative democracy. It is argued that the latter and
   Therisanyo offer both similar and diverging answers to the common
   objective of discursive will and opinion formation. Thus, they can
   inspire each other in many ways: on the one hand, considerations from
   ``the West{''} draw the attention to important democratic qualities of
   discursive practices. On the other hand, the Botswanian model of
   consultation offers empirical findings and exhibits alternative emphases
   which can broaden the ``Western{''} perspective of deliberative
