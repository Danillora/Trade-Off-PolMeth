@article{ WOS:000406913800008,
Author = {Gremigni, Elena},
Title = {THE ``MISERE DE L'EDUCATION{''} IN THE AGE OF CRISIS},
Journal = {PARTECIPAZIONE E CONFLITTO},
Year = {2016},
Volume = {9},
Number = {2, SI},
Pages = {441-465},
Abstract = {Over twenty years after the publication of La misere du monde (Bourdieu
   1993), Pierre Bourdieu's work is still highly relevant. The social order
   that has led to a proliferation of the ``small miseries{''} of the world
   seems to have prevailed: Pierre Bourdieu's micro-sociological essay
   highlighted the symptoms of a disease that has gotten worse in the past
   few years, and prophetically predicted many of its current consequences.
   This article aims at analysing the consequences on the educational field
   where practices of ``service-sector Taylorism{''} are emerging alongside
   the usual mechanisms of social reproduction. These practices are
   allegedly inspired by meritocracy, but actually aimed at the utilitarian
   exploitation of human resources. The students' potential is thus
   trivialised, dumbed down to a few measurable variables on which teaching
   and selecting are based, according to a view that treats humans as
   merely means to an end. Thus, by concealing the social fabric in which
   individuals are rooted, any failure can be surreptitiously blamed on
   individual responsibilities. Sociological research has the crucial task
   of revealing the peculiar logic of this kind of mechanisms, more and
   more common in several fields, and their ideological implications,
   typical of neoliberalism: because, as Bourdieu says, ``what the social
   world has done, it can, armed with this knowledge, undo{''} (Bourdieu
   1993d, 629)},
Publisher = {COORDINAMENTO SIBA},
Address = {VIA VALESIO, LECCE LE, 73100, ITALY},
Type = {Article},
Language = {English},
Affiliation = {Gremigni, E (Corresponding Author), Univ Pisa, Sociol Cultural \& Communicat Proc, Pisa, Italy.
   Gremigni, Elena, Univ Pisa, Sociol Cultural \& Communicat Proc, Pisa, Italy.},
DOI = {10.1285/i20356609v9i2p441},
ISSN = {1972-7623},
EISSN = {2035-6609},
Keywords = {Neoliberalism; Educational field; Social reproduction; ``Service-sector
   Taylorism{''}; Symbolic violence},
Research-Areas = {Government \& Law},
Web-of-Science-Categories  = {Political Science},
