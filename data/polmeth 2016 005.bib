@article{ WOS:000377030500002,
Author = {Zakaras, Alex},
Title = {Democracy, children, and the environment: a case for commons trusts},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2016},
Volume = {19},
Number = {2},
Pages = {141-162},
Abstract = {This essay argues that our unsustainable degradation of the natural
   environment violates fundamental, democratic obligations to children. I
   argue, first, that obligations to children are clearer and more
   determinate than obligations to future generations. I then argue that
   our democratic obligations to children alone, which can be derived from
   a theoretically modest, procedural conception of democracy, demand
   substantial institutional reforms. Finally, I propose that commons
   trusts - which have received very little attention in the theoretical
   literature - are the best way of meeting these obligations.},
