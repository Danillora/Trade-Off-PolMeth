@article{ WOS:000397459100003,
Author = {Egger, Clara},
Title = {A European comprehensive approach. The impact of intersectoral rivalries
   on European crisis management in Somalia},
Journal = {POLITIQUE EUROPEENNE},
Year = {2016},
Volume = {51},
Number = {1},
Pages = {60-85},
Abstract = {This article seeks to assess how the rivalries between European players
   within the external policy field have shaped a European model of crisis
   management in Somalia. Drawing on data collected in the field through
   interviews, this article advocates for taking into account the
   conflictual strategies of European actors in European foreign policy
   analysis. In this context, two results are elaborated to explain some of
   the specificities of the European approach, respectively on the autonomy
   of the humanitarian policy and on the strategic objectives of the EU's
   action in Somalia.},
