@article{ WOS:000384087500004,
Author = {Vercellotti, Timothy and Matto, Elizabeth C.},
Title = {The Role of Media Use in the Classroom and at Home in Improving News
   Consumption and Political Knowledge},
Journal = {JOURNAL OF POLITICAL SCIENCE EDUCATION},
Year = {2016},
Volume = {12},
Number = {2},
Pages = {151-168},
Abstract = {Concerns regarding political knowledge and participation among young
   people continue to spur research into effective civic instruction.
   Through a quasi-experiment involving 361 students in four high schools
   as well as the parents of 152 of these students, we measured the effects
   of incorporating news magazines and discussion into social studies
   curricula on students' levels of news consumption and political
   knowledge. We found that reading and discussing news at home and in
   school positively affected news consumption and political knowledge for
   students who were not in honors or advanced placement courses. This was
   also true for political knowledge when taking into account parents'
   knowledge of politics. Students outside of honors and advanced classes,
   and whose parents had low levels of knowledge, were among those most
   likely to benefit from the intervention.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
