@article{ WOS:000402635700001,
Author = {Spori, Tobias and Jaitner, Felix},
Title = {The Recent Protest Movements in Eastern Europe as an Indicator for
   Multiple Crises?},
Journal = {AUSTRIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2016},
Volume = {45},
Number = {4},
Pages = {1-5},
Abstract = {Since the outbreak of the financial and economic crisis in 2008, Eastern
   European countries have been affected severely by the crisis through
   intensified austerity measures, higher unemployment rates, and
   increasing dissatisfaction with democracy. Against this backdrop, the
   region has experienced several large-scale protest movements in recent
   years. The paper critically assesses the state of the art on political
   participation in Eastern Europe. Special attention is paid to the still
   under-played effects of the global economic crisis. Subsequently, the
   question is raised whether the increased number of protests indicates a
   deeper societal crisis. Thereby, the authors conclude, Eastern Europe
   serves to a certain extend as a trendsetter for similar developments in
   other parts of Europe.},
