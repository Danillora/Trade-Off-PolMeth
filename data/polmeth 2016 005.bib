@article{ WOS:000373392700002,
Author = {Fiva, Jon H. and Folke, Olle},
Title = {Mechanical and Psychological Effects of Electoral Reform},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2016},
Volume = {46},
Number = {2},
Pages = {265-279},
Month = {APR},
Abstract = {To understand how electoral reform affects political outcomes, one needs
   to assess its total effect, incorporating how the reform affects the
   outcomes given the political status quo (the mechanical effects) and the
   additional reactions of political agents (the psychological effects).
   This article proposes a framework to ascertain the relative magnitude of
   mechanical and various psychological effects. The empirical approach is
   based on pairwise comparisons of actual and counterfactual seat
   allocation outcomes. It uses the design to analyze a nationwide
   municipal electoral reform in Norway, which changed the seat allocation
   method from D'Hondt to Modified Sainte-Lague. The study documents clear
   psychological effects.},
