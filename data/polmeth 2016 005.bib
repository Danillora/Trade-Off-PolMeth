@article{ WOS:000393864600001,
Author = {Kneuer, Marianne and Salzborn, Samuel},
Title = {Digital Media and its Effect on Democratic Processes},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2016},
Volume = {10},
Number = {2},
Pages = {1-14},
Month = {JUL},
Abstract = {For some, the increasing presence and usage of digital media aroused
   utopian expectations concerning its democratic potential. On the other
   hand, there also exist sceptical voices positing a worsening of
   democratic quality instead. The authors argue for a more realistic
   approach in order to capture the ambivalence of the impact of digital
   media on democratic processes. It is on this basis that one can better
   evaluate the effect of social media in terms of improving or damaging
   the quality of central democratic principles like representation,
   participation, responsiveness and the public sphere.},
