@article{ WOS:000388731900003,
Author = {McKendrick, John H. and Asenova, Darinka and MacRae, Claire and
   Reynolds, Rebecca and Egan, James and Hastings, Annette and Mooney,
   Gerry and Sinclair, Stephen},
Title = {CONCEPTUALISING AUSTERITY IN SCOTLAND AS A RISK SHIFT: IDEAS AND
   IMPLICATIONS},
Journal = {SCOTTISH AFFAIRS},
