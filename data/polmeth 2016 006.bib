@article{ WOS:000397349600007,
Author = {Lizcova, Zuzana},
Title = {Under Pressure On the Situation of the Czech Press},
Journal = {OSTEUROPA},
Year = {2016},
Volume = {66},
Number = {11-12},
Pages = {103+},
Abstract = {In the Czech Republic, the situation of quality journalism is in a bad
   way. Newspapers and news magazines often cannot do justice to their
   claims of serious reporting. Responsibility for the tabloidisation was
   for a long time attributed to the foreign, mainly German media houses in
   the Czech Republic in the 1990s and dominated the press market. Since
   the global economic crisis in 2008, these investors have withdrawn. The
