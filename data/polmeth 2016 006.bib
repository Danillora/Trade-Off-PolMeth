@article{ WOS:000395217200021,
Author = {Mohamad-Klotzbach, Christoph and Schlenkrich, Oliver},
Title = {Determinants of Input- and Output-oriented Conceptions of Democracy in
   the Arab World: A multi-level analysis},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {404+},
Abstract = {Since the 1990's, more and more studies have been analyzing conceptions
   of democracy. This study focuses on determinants of conceptions of
   democracy in the Arab world. Therefore, we distinguish between input-
   and output-oriented conceptions of democracy. Whereas the former ones
   are highlighting procedural elements of democracy, the latter ones focus
   on materialistic aspects of democracy. We test different theories
   (modernization theory, social capital theory, political culture theory,
   diffusion theory, culture theory and the impact of the Arab Spring) on
   the basis of the third wave of the Arabbarometer (2012-2014) using a
   logistic multilevel model allowing us to include both individual and
   context effects. Our results show that the likelihood of having
   internalized an input-oriented conception of democracy rises with higher
   formal education, income, bridging social capital, political interest or
   the regular attending of the Friday prayers. Additionally, we can show
   that especially in Egypt and Tunisia democracy is understood in the
   sense of an output-conception.},
