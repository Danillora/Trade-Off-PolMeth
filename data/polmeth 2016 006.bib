@article{ WOS:000371991100005,
Author = {Blair, Stephanie and Brown, Maureen},
Title = {BEYOND MUDDLING THROUGH TOWARDS A BLUEPRINT FOR UK INTERNATIONAL
   POLICING AND LAW ENFORCEMENT},
Journal = {RUSI JOURNAL},
Year = {2016},
Volume = {161},
Number = {1},
Pages = {62-69},
Month = {JAN 2},
Abstract = {The UK appears to be making a cautious return to international policing
   after a decade of following an approach best described as muddling
   through'. Recent reforms and new co-ordinating structures seek to
   improve coherence and promote the British model of policing abroad.
   These are timely initiatives as international policing is also under
   review at the UN. Yet what is missing - argue Stephanie Blair and
   Maureen Brown - is a clear, pragmatic, realistic and holistic UK
   government strategy for international policing and law enforcement that
   would provide a sense of purpose, ambition and direction, and deliver
   coherence across UK domestic and international security objectives.},
