@article{ WOS:000373700900001,
Author = {Sides, John},
Title = {Stories or Science? Facts, Frames, and Policy Attitudes},
Journal = {AMERICAN POLITICS RESEARCH},
Year = {2016},
Volume = {44},
Number = {3},
Pages = {387-414},
Month = {MAY},
Abstract = {How much do factual information and other kinds of frames affect policy
   attitudes? And whose attitudes are most malleable? I address these
   questions via two original survey experiments about the estate tax. I
   demonstrate that correct information about who is potentially subject to
   the estate tax increased support for the estate tax. Furthermore, this
   information did not appear any less persuasive than pro- or antitax
   arguments that emphasized values such as equality or fairness. Finally,
   the effects of this information were concentrated among lower income
   conservatives and Republicans. These findings contrast with previous
   research on estate tax attitudes and have broader implications for the
   study of political information, framing, and policy attitudes.},
