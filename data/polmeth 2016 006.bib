@article{ WOS:000379182300006,
Author = {Gherghina, Sergiu},
Title = {Rewarding the ``traitors'? Legislative defection and re-election in
   Romania},
Journal = {PARTY POLITICS},
Year = {2016},
Volume = {22},
Number = {4},
Pages = {490-500},
Month = {JUL},
Abstract = {Legislative defection in political systems characterized by party
   dominance and cartelization is usually seen as damaging the members of
   parliament's (MPs') careers. Switching legislators does not portray an
   image of confidence and reliability and the MPs' re-election is
   unlikely. Drawing on a dataset that includes all defector MPs in
   post-communist Romania until 2008, this article indicates that one in
   five defectors gets re-elected. The quest for explanations focuses on
   individual and party features. Results indicate that re-election of
   defectors is a function of the strategic behavior (the choice of the
   destination party) and the legislative experience of the defector. All
   these items count against party and legislative activity.},
