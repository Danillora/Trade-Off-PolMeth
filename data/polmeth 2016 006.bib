@article{ WOS:000394814700006,
Author = {Prosser, Brenton and Denniss, Richard},
Title = {Minority Government: Non-ministerial Members Speak about Governing and
   Democracy},
Journal = {AUSTRALASIAN PARLIAMENTARY REVIEW},
