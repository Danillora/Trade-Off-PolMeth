@article{ WOS:000375948200004,
Author = {Lambkin, Brian},
Title = {Academic Antagonism and the ``Resetting' of the Northern Ireland
   ``Problem', 1969-1970: Owen Dudley Edwards vs. Hugh Trevor-Roper},
