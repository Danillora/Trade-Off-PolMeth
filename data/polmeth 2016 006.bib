@article{ WOS:000385367700008,
Author = {Hirsch-Hoefler, Sivan and Canetti, Daphna and Rapaport, Carmit and
   Hobfoll, Stevan E.},
Title = {Conflict will Harden your Heart: Exposure to Violence, Psychological
   Distress, and Peace Barriers in Israel and Palestine},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2016},
Volume = {46},
Number = {4},
Pages = {845-859},
Month = {OCT},
Abstract = {Does exposure to political violence prompt civilians to support peace?
   We investigate the determinants of civilian attitudes toward peace
   during ongoing conflict using two original panel datasets representing
   Israelis (n=996) and Palestinians in East Jerusalem, the West Bank and
   Gaza (n=631) (149 communities in total). A multi-group estimation
   analysis shows that individual-level exposure to terrorism and political
   violence makes the subject populations less likely to support peace
   efforts. The findings also confirm psychological distress and threat
   perceptions as the mechanism that bridges exposure to violence and
   greater militancy over time. The study breaks ground in showing that
   individual-level exposure - necessarily accompanied by psychological
   distress and threat perceptions - is key to understanding civilians'
   refusal to compromise in prolonged conflict.},
