@article{ WOS:000371156300008,
Author = {Moufawad-Paul, Joshua},
Title = {QUARTERMASTERS OF STADIUMS AND CEMETERIES: NORMATIVE INSURRECTIONISM AND
   THE UNDER-THEORIZATION OF REVOLUTIONARY STRATEGY},
Journal = {SOCIALIST STUDIES},
Year = {2016},
Volume = {11},
Number = {1},
Pages = {127-144},
Month = {WIN},
Abstract = {In this article, I examine the problematic of revolutionary strategy and
   how it is under-theorized at the centres of global capitalism, often
   confused with the theory of organization. Arguing that the theory of
   insurrection is uncritically accepted as normative, I discuss the
   necessity of returning to a critical engagement with the theory of
   strategy in the context of a modern capitalist military. By examining
   Karl Liebknecht's discussion of militarism, the a priori acceptance of
   the theory of insurrection by contemporary theorists in both the
   communist and anarchist traditions (i.e. Jodi Dean and the Invisible
   Committee), and the counter-tradition of protracted people's war, I
   demonstrate that the theory of insurrection is philosophically
   deficient, unable to account for the problems produced by capitalist
   militarism and pacification.},
