@article{ WOS:000377532800006,
Author = {Chari, Anita},
Title = {The Political Potential of Mindful Embodiment},
Journal = {NEW POLITICAL SCIENCE},
Year = {2016},
Volume = {38},
Number = {2},
Pages = {226-240},
Abstract = {This article argues that the emerging literature on mindfulness tends to
   de-emphasize embodiment, both in how it theoretically defines
   mindfulness, as well as in terms of the practices it tends to study.
   Alternatively, the article argues that a focus on embodiment is key to
   articulating the political potential of mindfulness. An emphasis on
   mindful embodiment, both in theory and in practice, can issue a
   challenge to the latent mind-body dualism of much of the theoretical
   discourse on mindfulness and highlights the capacities of relationality,
   empathy, and attunement, capacities which are the foundation of the
   political significance of mindfulness. Practices of mindful embodiment
   provide a counterpoint to the focus upon contemplation in the
   mindfulness literature and help to refute the concern that mindfulness
   practice is solipsistic and apolitical. The article concludes by
   exploring the Occupy Wall Street movement as illustrative of the
   significance of mindful embodiment to contemporary social movements.},
