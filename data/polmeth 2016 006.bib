@article{ WOS:000393521600002,
Author = {Colazingari, Silvia and Palermo, Vicente},
Title = {Classical and Plebeian Peronism. The Populism of Globalization},
Journal = {TEMAS Y DEBATES},
Year = {2016},
Number = {32},
Pages = {27-49},
Month = {JUL-DEC},
Abstract = {A comparison of the two variants of populism that we propose shows that
   only classical Peronism developed upon a process of constitution of
   social actors, particularly the working class and the trade unions.
   While classical Peronism was politically inclusive, and accompanied that
   inclusion with a subjective experience of social ascent, the plebeian
   populism of the Kirchners governments reproduces the ways of life of the
   communities that constitute its social and electoral base. The
   discursive and identity dimensions of this latter variant of populism
   are notably weak, while those connected to the patrimonial exercise of
   power, the predatory administration of government, and the manipulation
   of fiscal resources are remarkably strong. Plebeian populism is
   disconnected from the labor market in its relation to the working and
   lower classes. As a result, this variety of populism is centered on the
   management of the poor, and consists in a pragmatic and successful
   adaptation to the new social conditions presented by structural poverty.},
Publisher = {UNIV NAC ROSARIO, FAC CIENCIAS POLITICAS \& RELACIONES INT},
