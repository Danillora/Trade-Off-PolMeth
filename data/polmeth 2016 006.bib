@article{ WOS:000419292900005,
Author = {Staikova, Evelina},
Title = {EUROPEAN FORMS OF A NEW CITIZENSHIP: THE ORIGIN OF GLOBAL CITIZENSHIP},
Journal = {ANALELE UNIVERSITATII BUCURESTI-STIINTE POLITICE},
Year = {2016},
Volume = {18},
Number = {2},
Pages = {61-72},
Abstract = {Processes of globalization and urbanization, international migration and
   free movement of people put the theory of citizenship in an entirely new
   context. This new context changed the content of citizenship; provoked
   new thoughts of the body of concept; opened new analytical terrain. Many
   analysts develop theories of new forms of citizenship. This text
   presents such conceptualization - that of a glocal citizenship. The
   analysis goes beyond formally specified limits, rights and obligations
   of citizenship. Its representatives are mobile people, and key features
   are the civil activism and self-reflection.},
