@article{ WOS:000408328700001,
Author = {Schmitter, Philippe C.},
Title = {The Design of Social and Political Research},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Year = {2016},
Volume = {1},
Number = {4},
Pages = {577-609},
Month = {DEC},
Abstract = {Good social and political research requires an explicit design. In this
   article, this is illustrated by a cycle of choices that begin with the
   transformation of an idea into a topic and end with the drawing of
   inferences from the patterns of association one finds. In between lie
   critical decisions about concepts, hypotheses, indicators, case
   selection, measurement and tests for association.},
