@article{ WOS:000395217200020,
Author = {Zapf, Holger},
Title = {`Democracy' within the Arab-Islamic Discourse - or: Why and how to
   contextualize non-Western normative theories?},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {378+},
Abstract = {Past research has demonstrated the vast spectrum of political ideas that
   have been held by Muslim-Arab intellectuals, especially with regard to
   the concept of democracy. Their approaches range from showing the
   compatibility of Islamic sources and democracy to claiming their
   irreconcilability. However, these approaches have hardly been attached
   systematically to their socio-political circumstances. As a consequence,
   the sincerity of the intellectuals as well as the representativeness of
   their thought has remained highly contested. Therefore, it remains
   unclear how this spectrum of approaches has to be interpreted. The
   article highlights specific problems of `Western' research with regard
   to `non-Western' political thought in general, depicts major findings
   regarding Islamic-Arab normative theories of democracy, and discusses
   approaches that help to contextualize theories systematically. These
   approaches will also help to better assess the sincerity of
   intellectuals and the representativeness of their ideas.},
Publisher = {SPRINGER},
Address = {233 SPRING ST, NEW YORK, NY 10013 USA},
