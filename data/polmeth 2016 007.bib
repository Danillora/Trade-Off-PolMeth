@article{ WOS:000382883200003,
Author = {Niakooee, Seyed Amir},
Title = {Exploring the Crisis of the Reform Movement in Iran (1997-2005)},
Journal = {JAPANESE JOURNAL OF POLITICAL SCIENCE},
