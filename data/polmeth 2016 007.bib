Language = {English},
Affiliation = {Conroy-Krutz, J (Corresponding Author), Michigan State Univ, Dept Polit Sci, E Lansing, MI 48824 USA.
   Conroy-Krutz, Jeffrey, Michigan State Univ, Dept Polit Sci, E Lansing, MI 48824 USA.
   Moehler, Devra, Univ Penn, Annenberg Sch Commun, Philadelphia, PA 19104 USA.},
DOI = {10.1016/j.electstud.2016.01.010},
