@article{ WOS:000393284400003,
Author = {Dargent, Eduardo and Urteaga, Madai},
Title = {State Response by External Pressures: The Determinants of the State's
   strengthening to face the Illegal Gold Mining Boom in Peru (2004-2015)},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2016},
Volume = {36},
Number = {3},
Pages = {655-677},
Abstract = {In the context of the current commodity boom, informal and illegal
   mining have become acute challenges to the Peruvian state. This article
   documents and explains the state's response to these challenges. After a
   stage of inaction (2004-2008), the Peruvian state initially reacted with
   a disarticulated response (2008-2011) before mounting a more coherent
   and organized response (2011-2015). Although the state's response to the
   challenge has been limited, it is has nevertheless resulted in an
   observable increase in state capacity. We propose that this phenomenon
   is better explained by factors external to the state (international
   pressures and domestic demands) rather than by processes triggered from
   within.},
Publisher = {PONTIFICIA UNIV CATOLICA CHILE, INST CIENCIA POLITICA},
