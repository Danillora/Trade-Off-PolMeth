@article{ WOS:000372245200002,
Author = {Atkison, Larissa M.},
Title = {Antigone's Remainders: Choral Ruminations and Political Judgment},
Journal = {POLITICAL THEORY},
Year = {2016},
Volume = {44},
Number = {2},
Pages = {219-239},
Month = {APR},
Abstract = {This paper reads Antigone from the perspective of the Chorus. Whereas
   most interpreters read Antigone from the perspective of Creon and
   Antigone's respective laws, I maintain that the protagonists represent
   laws that are distinctly apolitical. Alternatively, I argue that the
   Chorus make the polispast, present, and futurethe center of their
   thought and action and are therefore uniquely political. Through close
   attention to the Chorus's composition as a body that is both one and
   many at the same time, and by tracing their evolving position throughout
   the action of the drama and in their choral odes, I argue that they
   model a reflective and political orientation to judgment rooted in
   plurality, tradition, and innovation.},
