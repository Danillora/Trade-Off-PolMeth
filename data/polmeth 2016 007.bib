@article{ WOS:000391223000010,
Author = {De Miguel Calvo, Estibaliz},
Title = {Women, Drug use and Imprisonment. Intersectional Approach},
Journal = {POLITICA Y SOCIEDAD},
Year = {2016},
Volume = {53},
Number = {2},
Pages = {529-549},
Abstract = {Research on women prisoners and drug use is scarce in our context and
   needs theoretical tools to understand their life paths. In this article,
   I introduce an intersectional perspective on the experiences of women in
   prison, with particular focus on drug use. To illustrate this, I draw on
   the life story of one of the women interviewed in prison, in order to
   explore the axes of inequality in the lives of women in prison. These
   are usually presented as accumulated and articulated in complex and
   diverse ways. The theoretical tool of intersectionality allows us to
   gain an understanding of the phenomenon of women prisoners who have used
   drugs. This includes both the structural constraints in which they were
   embedded and the decisions they made, considering the circumstances of
