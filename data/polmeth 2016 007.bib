@article{ WOS:000387810400016,
Author = {Cox, Ed},
Title = {Devolution in England Is the Genie out of the Lamp?},
Journal = {POLITICAL QUARTERLY},
Year = {2016},
Volume = {87},
Number = {4},
Pages = {565-571},
Month = {OCT-DEC},
Abstract = {This article explores the extent to which devolution within England can
   address the general malaise that is often described as the English
   Question'. It considers how far the devolution genie might be considered
   out of the lamp. It takes as its basis three broad arguments for
   devolutionan economic case, a case concerning public service reform and
   a democratic argument for the decentralisation of powerand for each of
   these three it explores the theoretical underpinnings for the argument,
   how things are currently playing out in the current political context
   and what we might expect to see in the future. The article concludes
   with some suggestions as to how debates about the English Question might
   play out in the years ahead.},
