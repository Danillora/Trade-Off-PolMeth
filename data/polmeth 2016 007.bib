@article{ WOS:000374507300006,
Author = {Bakker, Bert N. and Rooduijn, Matthijs and Schumacher, Gijs},
Title = {The psychological roots of populist voting: Evidence from the United
   States, the Netherlands and Germany},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
Year = {2016},
Volume = {55},
Number = {2},
Pages = {302-320},
Month = {MAY},
Abstract = {What are the psychological roots of support for populist parties or
   outfits such as the Tea Party, the Dutch Party for Freedom or Germany's
   Left Party? Populist parties have as a common denominator that they
   employ an anti-establishment message, which they combine with some host'
   ideology. Building on the congruency model of political preference, it
   is to be expected that a voter's personality should match with the
   message and position of his or her party. This article theorises that a
   low score on the personality trait Agreeableness matches the
   anti-establishment message and should predict voting for populist
   parties. Evidence is found for this hypothesis in the United States, the
   Netherlands and Germany. The relationship between low Agreeableness and
   voting for populist parties is robust, controlling for other personality
   traits, authoritarianism, sociodemographic characteristics and ideology.
   Thus, explanations of the success of populism should take personality
   traits into account.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
