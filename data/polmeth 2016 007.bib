@article{ WOS:000391222800005,
Author = {Anderson, Paul},
Title = {The 2016 Scottish Parliament election: a nationalist minority, a
   Conservative comeback and a Labour collapse},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2016},
Volume = {26},
Number = {4},
Pages = {555-568},
Abstract = {This article examines the political context, campaign, election results
   and outcomes of the 2016 Scottish Parliament election. The Scottish
   National Party (SNP) secured its third electoral victory, yet failed to
   achieve a widely predicted majority. With just two MSPs short of a
   majority, the SNP has ruled out any formal coalition with the opposition
   and will instead govern as a minority administration. The composition of
   the parliament's opposition also changed significantly. The Scottish
   Conservatives increased their share of the constituency and regional
   votes, and became, for the first time, the largest opposition party in
   the chamber. Scottish Labour suffered a severe electoral drubbing,
   losing 13 of its seats. The election was also important for the Scottish
   Liberal Democrats and Scottish Green Party. The latter increased its
   vote share and number of seats, leapfrogging the Lib Dems to become the
   fourth largest party in the chamber.},
