@article{ WOS:000385366700006,
Author = {Lo, James and Proksch, Sven-Oliver and Slapin, Jonathan B.},
Title = {Ideological Clarity in Multiparty Competition: A New Measure and Test
   Using Election Manifestos},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2016},
Volume = {46},
Number = {3},
Pages = {591-610},
Month = {JUL},
Abstract = {Parties in advanced democracies take ideological positions as part of
   electoral competition, but some parties communicate their position more
   clearly than others. Existing research on democratic party competition
   has paid much attention to assessing partisan position taking in
   electoral manifestos, but it has largely overlooked how manifestos
   reflect the clarity of these positions. This article presents a scaling
   procedure that better reflects the data-generating process of party
   manifestos. This new estimator allows us to recover not only positional
   estimates, but also estimates for the ideological clarity or ambiguity
   of parties. The study validates its results using Monte Carlo tests, a
   manifesto-drafting simulation and a human coding exercise. Finally, the
   article applies the estimator to party manifestos in four multiparty
   democracies and demonstrates that ambiguity can enhance the appeal of
   parties with platforms that become more moderate, and lessen the appeal
   of parties with platforms that become more extreme.},
