@article{ WOS:000400012600006,
Author = {Cassani, Andrea},
Title = {Was it worth the trouble? Limited competition and citizen well-being
   under authoritarian rule},
Journal = {ITALIAN POLITICAL SCIENCE REVIEW-RIVISTA ITALIANA DI SCIENZA POLITICA},
Year = {2016},
Volume = {46},
Number = {1},
Pages = {93-113},
Month = {MAR},
Abstract = {The last part of 20th century saw the collapse of a dramatic number of
   dictatorships. Rather than democracy, several of these transitions
   brought regimes where limited political competition coexists with
   persistently authoritarian practices. The diffusion of this form of
   authoritarianism in the developing world raises several questions about
   its broader consequences. Most importantly, does political change short
   of democratization matter for ordinary citizens? Recent research
   demonstrates that nominally democratic institutions, even in the absence
   of people empowerment, can result in better living conditions. The paper
   adds to this debate by formulating and testing new hypotheses. I compare
   electoral authoritarianism with democracy and full dictatorship,
   including specific subtypes of the latter, and focus on both policy
   outputs and outcomes.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {32 AVENUE OF THE AMERICAS, NEW YORK, NY 10013-2473 USA},
Type = {Article},
Language = {English},
Affiliation = {Cassani, A (Corresponding Author), Univ Milan, Dept Social \& Polit Sci, Milan, Italy.
   Cassani, Andrea, Univ Milan, Dept Social \& Polit Sci, Milan, Italy.},
DOI = {10.1017/ipo.2015.27},
ISSN = {0048-8402},
EISSN = {2057-4908},
Keywords = {electoral authoritarianism; democratization; development; quantitative
   analysis},
Keywords-Plus = {COMPARATIVE POLITICAL-ECONOMY; DEMOCRACY; DICTATORSHIP; INSTITUTIONS;
   HYBRID; POLICY; LEGISLATURES; SURVIVAL; HEALTH},
