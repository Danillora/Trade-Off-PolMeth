@article{ WOS:000405581800005,
Author = {Romero Wimer, Fernando Gabriel and Fernandez Hellmund, Paula Daniela},
Title = {ARGENTINE-CHINESE RELATIONS: HISTORY, PRESENT AND PROSPECTS},
Journal = {REVISTA ANDINA DE ESTUDIOS POLITICOS},
Year = {2016},
Volume = {6},
Number = {2},
Pages = {60-91},
Abstract = {This article aims to analyze the background and the evolution of the
   relations between Argentina and the People's Republic of China,
   connecting this approach to the political, institutional an economic
   scenario that has promoted Chinese investments in the South American
   country and the development of businesses that has linked capital from
   both nations. Firstly, this paper presents some theoretical
   considerations on international relations and global capitalist
   dynamics. Secondly, it describes the evolution of the Chinese economy
   since the reforms initiated in 1978. Thirdly, it examines the coinciding
   interests of part of the Argentine business sector and the Chinese
   capital, considering how they are expressed in the political and
   economic field. Finally, it analyzes possible and alternative scenarios
   for the Argentine-Chinese relations.},
