@article{ WOS:000372005000009,
Author = {Beetschen, Marion and Rebmann, Frederic},
Title = {The Switzerland neocorporatism declining? The extra-parliamentary
   committees in a changing environment (1957-2010)},
Journal = {SWISS POLITICAL SCIENCE REVIEW},
Year = {2016},
Volume = {22},
Number = {1},
Pages = {123-144},
Month = {MAR},
Abstract = {Resume L'analyse diachronique des commissions extra-parlementaires (CEP)
   suisses montre un fort declin de leur composition neocorporatiste dans
   la seconde moitie du 20ieme siecle. Lieux de rencontre et de negociation
   pour les principaux acteurs des champs economique, scientifique,
   politique et administratif, les CEP sont traditionnellement considerees
   comme une importante instance de representation des groupes d'interet en
   Suisse. Notre contribution interroge la validite de cette position a
   l'aune des reformes du systeme commissionnaliste des annees 1970-1980 et
   de la remise en cause du neocorporatisme des les annees 1990. Bases sur
   l'analyse de l'affiliation des membres de CEP sur quatre dates (1957,
   1980, 2000 et 2010), nos resultats indiquent que les CEP perdent leur
   fonction de concertation et d'intermediation des differents acteurs
   impliques dans le processus decisionnel, au travers notamment d'une
   baisse de la representation des interets economiques organises et d'une
   augmentation de la representation des acteurs scientifiques ainsi que
   d'une diminution de la multi-expertise.},
