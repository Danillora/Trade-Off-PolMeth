@article{ WOS:000372692300029,
Author = {Stegmaier, Mary and Williams, Laron K.},
Title = {Forecasting the 2015 British election through party popularity functions},
Journal = {ELECTORAL STUDIES},
Year = {2016},
Volume = {41},
Pages = {260-263},
Month = {MAR},
Abstract = {To forecast the May 7, 2015 British General Election, we develop party
   popularity models based on Continuous Monitoring Survey (CMS) data from
   April 2004 to February 2015. Our models predict party vote shares three
   months prior to the election, using previous support levels, national
   economic evaluations, macro-partisanship and political measures. Our
   Seemingly Unrelated Regression (SUR) methodology allows us to predict
   support for the Tories, Labour, Liberal Democrats and ``other{''}
   parties, separately, yet simultaneously, by constraining total support
   for all parties to 100\%. Our model, estimated with data from February
   2015, predicts that Labour will win the highest vote share in Great
   Britain, but that no party will win a majority of seats in parliament.
