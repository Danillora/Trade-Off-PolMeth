@article{ WOS:000393284400007,
Author = {Soto Zazueta, Irvin Mikhail},
Title = {The Effect of Political Competition on the Provision of Local Public
   Goods in Mexico},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2016},
Volume = {36},
Number = {3},
Pages = {749-772},
Abstract = {Political competition may have virtuous effects similar to the effect
   that economic competition has in markets. Using cross-sectional data on
   local elections in Mexico, we evaluate the effect of political
   competition on the provision of local public goods, controlling for
   socio-economic and demographic factors. Evidence suggests that political
   competition is positively related to the coverage of municipal public
   services, having a greater effect in those areas with the highest
   poverty and income inequality conditions.},
