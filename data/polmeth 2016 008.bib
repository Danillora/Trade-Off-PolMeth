@article{ WOS:000386916000001,
Author = {Cherney, Adrian and Murphy, Kristina},
Title = {What does it mean to be a moderate Muslim in the war on terror? Muslim
   interpretations and reactions},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2016},
Volume = {9},
Number = {2},
Pages = {159-181},
Abstract = {The rhetorical use of labels in the war on terror has become an
   important tactic post 9/11. One such example is the deployment of the
   categories of ``moderate{''} and ``extremist{''} within
   counter-terrorism discourse, with Muslims distinguished as either friend
   or foe based on this dichotomy. The moderate Muslim label is a
   relational term, only making sense when it is contrasted with what is
   seen as non-moderate (i.e., extremism). Such binary constructs carry a
   range of implicit assumptions about what is regarded as an acceptable
   form of Islam and the risks posed by the Islamic religion and Muslim
   communities. In this article, we explore the implications of this
   labelling for Muslim communities. In particular, we explore the
   interpretations Muslims themselves accord to the dichotomy of moderate
   and extremist and consider whether the use of such binary terms is at
   all helpful as a way of rallying Muslims to the cause of tackling
   terrorism and radicalisation. We draw on focus group data collected from
   Muslims living in Australia to inform our analysis.},
