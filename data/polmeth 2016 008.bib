@article{ WOS:000374640600008,
Author = {Tripp, Aili Mari},
Title = {Comparative perspectives on concepts of gender, ethnicity, and race},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2016},
Volume = {4},
Number = {2},
Pages = {307-324},
Month = {JUN},
Abstract = {This essay examines, with broad strokes, the ways in which different
   historical experiences have given rise to varied understandings of
   equality based on gender, ethnicity, and race, contrasting concepts that
   have emerged in Liberia and other African contexts with US approaches.
   The essay evaluates the strengths and weaknesses of the different
   conceptualizations for mobilization around women's rights, political
   power, and peace and shows that approaches that work in some contexts
   may fail in others because of the varied societal and historical
   orientations. More specifically, the essay argues that, in general,
   women's movements in societies that have emphasized equality over
   difference in gender relations have also tended to emphasize diversity
   and the importance of identity when it comes to racial and ethnic
   difference as well as other forms of difference. Conversely, women's
   movements in societies that have adopted a difference frame for
   understanding gender - understood here in the narrow sense of gendered
   binary thinking - have been more likely to focus on commonalities
   between various identities, particularly similarities between women of
   different ethnicities. This is because those orientations which
   emphasize the ways in which women ought to be equal to men (equality
   feminism) are more likely to focus on individual and group differences
   between women, whereas those approaches which focus on the similarities
   among women as a group as distinct from men as a group (difference
   feminism), are more likely to downplay differences among women.},
