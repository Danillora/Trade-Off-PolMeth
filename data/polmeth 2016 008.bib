@article{ WOS:000390180200002,
Author = {Park, Jeeyoung and Norpoth, Helmut},
Title = {Policy popularity: The Arizona immigration law},
Journal = {ELECTORAL STUDIES},
Year = {2016},
Volume = {44},
Pages = {15-25},
Month = {DEC},
Abstract = {As a maker of policy, a president or a governor invites public approval
   or disapproval for policy decisions. Public reaction is likely to occur
   for issues of great salience and clear candidate positions. We focus on
   immigration policy. Illegal immigration has become a hot issue in recent
   years, especially in Arizona. The state's governor took a clear stance
   in 2010 by signing a law that gives police sweeping powers to deal with
   illegal immigration (Arizona SB 1070). Using an aggregate time-series
   model, we find that this action affected gubernatorial approval ratings.
   Indeed the gain in approval proved enduring enough to turn a losing race
   for re-election into a victory for Governor Brewer. Using
   individual-level survey data, we find that presidential approval also
   was affected by reactions to the Arizona Law among residents of the
   state. When elected officials take clear stances on a salient issue -
