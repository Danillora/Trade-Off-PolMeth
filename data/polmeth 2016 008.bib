@article{ WOS:000395217200005,
Author = {Dallmayr, Fred R.},
Title = {Rule of, by, and for the People: An apophatic democracy?},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {71+},
Abstract = {The essay takes its departure from President Lincoln's famous definition
   of democracy as ``rule by, of, and for the people{''}. Although the
   definition enjoys the status of a widely accepted mantra, Dallmayr calls
   into question the prevalent conceptions of ``people{''} and ``rule{''}.
   In the dominant procedural or ``minimalist{''} conception, the
   ``people{''} are defined as selfish utility-maximizers, that is,
   individuals who seek to maximize benefits in exchange for minimal
   investments; ``rule{''} is simply government by the most successful
   self-seekers. The chapter also discusses the related or alternative
   conceptions of agonistic and ``deliberative{''} democracy. By contrast
   to the homo economicus extolled by liberal minimalism, agonistics
   privileges homo politicus (human beings as power seekers), while the
   deliberative model stresses rational argumentation (animal rationale).
   By introducing the crucial criteria of ``relationality{''} and
   ``potentiality{''}, the chapter lifts up the aspirational or
   ``promissory{''} quality of democracy paying especial attention to
   Derrida's notion of ``democracy to come{''} and to the open-ended,
   unfinished character of ``people{''} and humanity. Seen from this angle,
   democracy can also be called aporetic or ``apophatic{''}.},
