@article{ WOS:000388737700008,
Author = {Jackson, Camille},
Title = {Legislation as an Indicator of Free Press in Russia: Patterns of Change
   from Yeltsin to Putin},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2016},
Volume = {63},
Number = {5-6, SI},
Pages = {354-366},
Month = {SEP-DEC},
Abstract = {This study measures the restrictiveness of media legislation in Russia
   from 1991 through 2015, identifying patterns of change in the
   government's attitude toward its responsibility over the press. Using a
   unique coding system against 67 separate pieces of legislation, the
   study finds that media legislation during the Yeltsin period was
   sporadically restrictive, while legislation under Putin has become more
   consistently controlled, particularly since 2012. The restrictiveness of
   media legislation under Medvedev closely mirrors Putin's record,
   although laws protecting children from information deemed dangerous and
   higher criminal penalties for violence against journalists may have
   emerged from initiatives unique to Medvedev. These findings allow for
   tentative conclusions about the future of press freedom in Russia.},
