@article{ WOS:000395217200016,
Author = {Fuchs, Dieter and Roller, Edeltraud},
Title = {Citizens' Conceptions of Democracy and Orientations towards Democratic
   Political Community: A comparison of Western, East-Asian and Arab
   countries},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {296+},
Abstract = {According to Huntington (1996) the political culture of the East-Asian
   and Arab countries is not compatible with democracy. Starting from this
   assumption the analysis differentiates between four conceptions of
   democracy (electoral, liberal, paternalistic, religious) and three
   orientations towards democratic political community (equality,
   tolerance, trust in unknown others). Three questions are analysed:
   First, which conception of democracy is preferred by the citizens of
   these three groups of countries. Second, to what extent the citizens of
   these three civilizations show democratic orientations towards the
   political community. Third, whether the East-Asian and Arab countries
   significantly differ from the Western countries. The empirical results
   (based on the World Values Surveys data from 2010-14) demonstrate that
   the citizens of East-Asian and Arab countries prefer other conceptions
   of democracy than the liberal one. Additionally, their democratic
   orientations towards the political community are clearly less pronounced
   than in Western countries.},
