@article{ WOS:000409205300008,
Author = {Sochorova, Mahuliena},
Title = {INFRASTRUCTURE AND ELECTORAL BEHAVIOUR AS DIFFERENTIAL CRITERIONS OF
   MUNICIPALITY SIZE (CASE STUDY NITRA REGION)},
