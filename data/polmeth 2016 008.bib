@article{ WOS:000392732100012,
Author = {Layman, Daniel},
Title = {ROBUST DELIBERATIVE DEMOCRACY},
Journal = {CRITICAL REVIEW},
Year = {2016},
Volume = {28},
Number = {3-4},
Pages = {494-516},
Abstract = {Deliberative democracy aspires to secure political liberty by making
   citizens the authors of their laws. But how can it do this in the face
   of deep disagreement, not to mention imperfect knowledge and limited
   altruism? Deliberative democracy can secure political liberty by
   affording each citizen an equal position as a co-author of public laws
   and norms. Moreover, fundamental deliberative democracy-in which
   institutional design is ultimately accountable to public deliberation
   but not necessarily subject to its direct control-does not strain
   knowledge or altruism. Thus, there is a place for deliberative democracy
   in a robust political economy.},
