@article{ WOS:000449013100003,
Author = {Mateus Moreno, Laura},
Title = {Agroecology as a Political Option for Peace in Colombia},
Journal = {CIENCIA POLITICA},
Year = {2016},
Volume = {11},
Number = {21},
Pages = {57-91},
Month = {JAN-JUN},
Abstract = {This article contributes elements to view agroecology as a political
   option for peace building in Colombia within the framework of the land
   and rural development agreements made in La Habana, Cuba. The proposals
   resulting from using academic and political analysis documents and from
