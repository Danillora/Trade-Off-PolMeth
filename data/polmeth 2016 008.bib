@article{ WOS:000399983200001,
Author = {Chen Dongxiao and Feng Shuai},
Title = {The Russia-India-China Trio in the Changing International System},
Journal = {CHINA QUARTERLY OF INTERNATIONAL STRATEGIC STUDIES},
Year = {2016},
Volume = {2},
Number = {4},
Pages = {431-447},
Month = {WIN},
Abstract = {Shifting power balances on the Eurasian continent present both
   challenges and opportunities for further trilateral cooperation among
   Russia, India, and China (RIC). The call for a new Asia-Pacific security
   architecture underscores the strategic importance of a closer trilateral
   relationship. From a macro perspective, the current international system
