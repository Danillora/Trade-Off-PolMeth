@article{ WOS:000389589100010,
Author = {Knutsen, Carl Henrik and Moller, Jorgen and Skaaning, Svend-Erik},
Title = {Going historical: Measuring democraticness before the age of mass
   democracy},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2016},
Volume = {37},
Number = {5},
Pages = {679-689},
Month = {NOV},
Abstract = {Most studies of democratic developments are limited to the period after
   World War II. However, political regimes varied according to different
   aspects of democracy long before the establishment of modern liberal
   mass democracies. We come down strongly in favor of collecting
   disaggregate and fine-grained historical data on democratic features.
   Based on a distinction between competition, participation, and
   constraints on the executive, we discuss previous attempts at historical
   measurement and address the specific challenges that pertain to scoring
   political regimes in, first, the long 19th century and, second, medieval
   and early modern Europe.},
