@article{ WOS:000384779900004,
Author = {Diaz, Nicolas},
Title = {An Unconcluded Journey. Divisions within Renovacion Nacional During the
   Administration of Sebastian Pinera},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2016},
Volume = {36},
Number = {2},
Pages = {481-502},
Abstract = {This paper investigates the internal divisions of Renovacion Nacional
   during the Sebastian Pinera administration through a series of
   interviews to party members, congressman and party leaders. Even though
   the press and the literature tends to explain differences within RN due
   to the existence of a liberal pole and a conservative pole, current
   fractures follow a more complex logic. During the last decade party
   conflicts have been restructured around the party leadership and its
   democratic appeal (verticalists vs. horizontalists), and around the
   confrontation between the ruling board (mesa directiva) and the
   government (larrainistas vs. pineristas). This new cleavages indicate an
   internal reconfiguration and repositioning of party leaders.},
