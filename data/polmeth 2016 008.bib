@article{ WOS:000387973700007,
Author = {Zuba, Krzysztof},
Title = {Power holders: one versus many. Leadership and elite theories},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2016},
Volume = {9},
Number = {2},
Pages = {269-287},
Abstract = {`Leadership' and `elites' are two crucial concepts that refer to
   individuals who hold power within a community. The aim of the article
   was to demonstrate to what extent the two concepts and theories are
   linked with each other in the literature. A quantitative and qualitative
   content analysis of the encyclopedic and dictionary entries demonstrates
   a divergence of approaches: on the one hand, a fundamental separation
   between the two theories, while on the other hand, a tendency to
   identify the one with the other. This shows the need for much more
   dialog between the frameworks of these two closely related phenomena.},
