   Ajei, Martin Odei, Univ Ghana, POB LG 211, Legon, Ghana.},
DOI = {10.1177/1474885116666451},
ISSN = {1474-8851},
EISSN = {1741-2730},
Keywords = {Consensual democracy; majoritarian democracy; democracy; consensus;
   deliberation; representation; liberalism; liberal democracy},
Research-Areas = {Government \& Law},
