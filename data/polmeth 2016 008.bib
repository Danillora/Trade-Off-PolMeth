@article{ WOS:000443331600004,
Author = {Veselinovic, Velimir},
Title = {FRANJO TUDMAN AND CROATIAN PARTIES OF RIGHT},
Journal = {POLITICKA MISAO-CROATIAN POLITICAL SCIENCE REVIEW},
Year = {2016},
Volume = {53},
Number = {1},
Pages = {71-102},
Month = {JUL},
Abstract = {Based on the archive materials, press analysis and relevant literature
   review, the author reconstructs the relationship between Franjo Tudman
   and far right political parties in Croatia. The main argument is that
   the relationship between Tudman and HSP (leading far right party) had
   two phases of development. Throughout the first phase (1990-1993) there
   was radical critique directed towards Tudman and his government. Tudman
   on the other hand never approved of the fact that HSP was using Ustase
   symbols and myths to gain popularity. The beginning of the second phase
   was marked by the change in HSP leadership. Throughout the second phase
   HSP was less critical about Tudman and the state leadership. Tudman on
   the other hand tolerated HSP and its political activity.},
