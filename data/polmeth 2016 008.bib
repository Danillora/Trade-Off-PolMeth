@article{ WOS:000390938500002,
Author = {Walters, William and Luethi, Barbara},
Title = {The Politics of Cramped Space: Dilemmas of Action, Containment and
   Mobility},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
Year = {2016},
Volume = {29},
Number = {4},
Pages = {359-366},
Month = {DEC},
Abstract = {This article introduces a special issue oriented to the theme of cramped
   space. We look to cramped space to advance critical understanding of the
   relationship between space, mobility and politics. Cramped space
   registers degrees of deprivation, constriction, and obstruction, but
   always and simultaneously a concern for the ways in which such limits
   operate to stimulate and incite movements of becoming and remaking. With
   this point in mind we argue that the idea of cramped space promises to
   foster a fruitful dialogue between two lines of research that have
   hitherto unfolded separately. The first of these is the recent shift
   within globalization theories and mobility studies away from the initial
   overly-optimistic focus on unbounded flows, and towards questions of
   friction, containment, blockage, mediation and infrastructure. The
   second is Deleuze and Guattari's argument for minor politics (from where
   the idea of cramped space originates). Addressing an age when talk of
   fences and walls saturates public rhetoric, the essays collected here
   think through cramped space to imagine new vistas of the politics of
   mobility.},
