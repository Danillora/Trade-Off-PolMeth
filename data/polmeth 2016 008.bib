@article{ WOS:000384462900007,
Author = {Vrana, Heather A.},
Title = {`Our ongoing fight for justice': the pasts and futures of
   genocidio and justicia in Guatemala},
Journal = {JOURNAL OF GENOCIDE RESEARCH},
Year = {2016},
Volume = {18},
Number = {2-3, SI},
Pages = {245-263},
Abstract = {After the abrogation of the guilty verdict against Efrain Rios Montt,
   young people from the group Sons and Daughters of the Disappeared
   (H.I.J.O.S.) issued the following statement: `More than a failure, this
   can breathe life into our ongoing fight for justice'. While this
   affirmation seems at first eccentric, or incidental, this article
   demonstrates how H.I.J.O.S.'s claim situates the Rios Montt verdict
   within a longer history of justice and genocide in Guatemala. First, I
   trace the history of the meaning of justice and genocide among urban
   ladino university students from the 1940s to the 1990s. Next, I discuss
   youth politics and culture in the postwar period and locate H.I.J.O.S.
   within this context. Finally, I demonstrate how H.I.J.O.S.'s ongoing
   fight compels us to rethink the meanings of justice. Against apparently
   objective sums of the dead and disappeared that might be calculated and
   have their debts settled, a call emerges for `justice-ongoing' after the
   annulment of the Rios Montt verdict, one that insists upon
   incalculability and the imperative to remember and to remain provoked.
   For H.I.J.O.S.'s justice-ongoing, the past is not merely passed.},
