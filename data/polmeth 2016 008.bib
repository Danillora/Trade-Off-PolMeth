@article{ WOS:000395217200024,
Author = {De La Rosa, Sybille},
Title = {Politics of Liberation: Enrique Dussel's radical theory of democracy},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {475+},
Abstract = {This paper attempts to show that Enrique Dussel's radical democratic
   theory has a stronger focus on a claim for a transformation of the
   democratic idea and democratic institutions for the benefit of those
   excluded than the democratic theory of Ernesto Laclau and Chantal
   Mouffe. Furthermore, I show that Dussel not just calls for
   counter-hegemonic thinking; he is practicing it, by entangling European,
   indigenous and Latin American ideas and practices, creating a new
   democratic imaginary.},
