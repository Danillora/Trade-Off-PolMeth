@article{ WOS:000374005500014,
Author = {Cowen, Nick},
Title = {Millian Liberalism and Extreme Pornography},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
Year = {2016},
Volume = {60},
Number = {2},
Pages = {509-520},
Month = {MAR},
Abstract = {How sexuality should be regulated in a liberal political community is an
   important, controversial theoretical and empirical questionas shown by
   the recent criminalization of possession of some adult pornography in
   the United Kingdom. Supporters of criminalization argue that Mill, often
   considered a staunch opponent of censorship, would support prohibition
   due to his feminist commitments. I argue that this account
   underestimates the strengths of the Millian account of private conduct
   and free expression, and the consistency of Millian anticensorship with
   feminist values. A Millian contextual defense of liberty, however,
   suggests several other policy approaches to addressing the harms of
   pornography.},
