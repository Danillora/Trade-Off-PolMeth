@article{ WOS:000396405900008,
Author = {Emamgholizadeh, Mehdi and Javanmard, Habib Allah},
Title = {The Effect of Learning Standards and Market-Orientation in Competitive
   Strategy and Organizational Survival of the Islamic Republic of Iran
   Broadcasting (IRIB)},
