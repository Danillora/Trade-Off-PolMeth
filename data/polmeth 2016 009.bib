@article{ WOS:000393103700004,
Author = {Skogstad, Grace and White, Linda A.},
Title = {SIMEON'S ``STUDYING PUBLIC POLICY{''} 40 YEARS ON - A SYMPOSIUM
   Revisiting Richard Simeon's ``Studying Public Policy{''}},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2016},
Volume = {49},
Number = {4},
Pages = {665-679},
Month = {DEC},
Abstract = {The articles in this symposium reflect on Richard Simeon's article,
   ``Studying Public Policy,{''} published forty years ago in this journal.
   In this introduction, we review these articles' contribution to three
   themes in `` Studying Public Policy{''}: first, the goal of the study of
   public policy should be policy analysis and explanation, not policy
   prescription; second, the analysis of public policy outcomes requires a
   holistic and contextually situated analytical framework; and third,
   building theory requires methods of comparative analysis, not single
   case studies. We also propose items for a future policy studies agenda.},
