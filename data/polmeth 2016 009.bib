@article{ WOS:000375704100005,
Author = {Agirdag, Orhan and Phalet, Karen and Van Houtte, Mieke},
Title = {European identity as a unifying category: National vs. European
   identification among native and immigrant pupils},
Journal = {EUROPEAN UNION POLITICS},
Year = {2016},
Volume = {17},
Number = {2},
Pages = {285-302},
Month = {JUN},
Abstract = {This article investigates whether European identity is a feasible and
   functional alternative to national identity. We examine the extent,
   determinants and consequences of national and European identification
   among (immigrant) Turkish and native Belgian pupils, with data gathered
   from 1629 pupils across 68 Belgian schools. The results show that
   immigrant Turkish pupils identify more strongly with Europe than with
   Belgium. The groups are closer to each other with respect to their
   European identification, while the latter is not in conflict with
   national identification. Moreover, European identity is less ethnically
   and more civically defined than national identity. Importantly, European
   identification was moderately related to academic achievement, though it
   is hard to make a causal claim.},
