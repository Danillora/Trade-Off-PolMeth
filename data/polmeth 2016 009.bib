@article{ WOS:000384445800021,
Author = {Milani, Ali Reza and Gharibi, Zahra},
Title = {Criminal Responsibility of Offenders with Personality Disorders with an
   Emphasis on Crime Psychological Factors},
Journal = {JOURNAL OF POLITICS AND LAW},
