@article{ WOS:000370308500006,
Author = {Turner, Brandon},
Title = {Privates Vices, Public Benefits: Mandragola in Machiavelli's
   Political Theory},
Journal = {POLITY},
Year = {2016},
Volume = {48},
Number = {1},
Pages = {109-132},
Month = {JAN},
Abstract = {Scholars often interpret Machiavelli's Mandragola as a contest of
   would-be princes displaying their virtu. I offer an alternative
   interpretation of the play in which the characters, each vicious and
   self-interested, constitute an ``economy of vice.{''} This
   interpretation highlights Machiavelli's appreciation for the public (and
   political) import of ordinary vice and thereby provides when considered
   in light of medieval influences on Machiavelli's understanding of
   constitutions new purchase on Machiavelli's political theory. In
   contrast to the image of Machiavelli as a profoundly political thinker,
   my argument underscores Machiavelli's role as a precursor to
   comparatively anti-political modes of thought: in particular, commercial
   liberalism.},
