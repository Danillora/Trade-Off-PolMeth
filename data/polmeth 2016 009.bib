@article{ WOS:000388474100005,
Author = {Koch, Michael T.},
Title = {Some Agents are Freer than Others: Variation in the Reelection
   Incentive, Agency Loss, and the Timing of Democratic Interstate Conflict},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2016},
Volume = {46},
Number = {4},
Pages = {828-848},
Month = {DEC},
Abstract = {I ask whether variations in electoral systems affect the conflict
   behavior of democratic leaders, specifically the timing of that
   conflict. Building from theories of political accountability and agency
   loss, I argue that the degree of agency loss coupled with variations in
   the principals that hold leaders accountable affects the timing of
   conflict in democratic systems. The results confirm the expectation that
   leaders in candidate-centered systems are more likely to gamble for
   resurrection as the next election approaches than are party-centered
   executives given the differences in the ability of their principals to
   select, sanction, and monitor them.},
Publisher = {WILEY},
