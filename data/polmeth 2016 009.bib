@article{ WOS:000381841000004,
Author = {Beste, Simon},
Title = {Studying dynamics of content and form in discourses: Evidence from an
   exploratory study of US legislative discourse on American-Iraqi
   relations 1995-2012},
Journal = {ACTA POLITICA},
Year = {2016},
Volume = {51},
Number = {3},
Pages = {346-371},
Month = {JUL},
Abstract = {The linguistic turn in policy analysis has immensely spurred interest in
   investigating political discourses. This article seeks to compensate for
   this shortcoming. The theoretical approach taken merges two basic
   dimensions of deliberation form and content of speeches and looks at
   Congressional debates over the course of nearly 20 years from 1995 to
   2012 surrounding the American Iraqi conflict. The findings suggest that
   there is a great potential for combining deliberative and discourse
   analytical elements into one comprehensive research design. The effects
   between content and form are largely driven by exogenous factors,
   particularly the temporal context, while institutional differences
   between the House and the Senate are rather secondary.},
