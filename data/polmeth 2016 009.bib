@article{ WOS:000383814300007,
Author = {Petrenko, Olena},
Title = {The Organization of Ukrainian Nationalists and Women},
Journal = {OSTEUROPA},
Year = {2016},
Volume = {66},
Number = {4},
Pages = {83+},
Abstract = {Women also participated in the violent struggle of the Organization of
   Ukrainian Nationalists (OUN) against the Polish state. But this does not
   mean that the Ukrainian nationalists represented another role model or
   even a new gender order. The opposite was the case. Their conservative
   notions of the role of women led to conflict between the ideologues of
   integral Ukrainian nationalism and the Union of Ukrainian Women, which
   was devoted to strengthening and advancing the role of Ukrainian women
   in society and political life. But because a shortage of young followers
   existed, the OUN had no qualms about allowing women to participate in
