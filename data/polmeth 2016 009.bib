@article{ WOS:000391132000003,
Author = {Trautsch, Jasper M.},
Title = {The origins and nature of American nationalism},
Journal = {NATIONAL IDENTITIES},
Year = {2016},
Volume = {18},
Number = {3},
Pages = {289-312},
Abstract = {This article critically analyzes the origins and nature of American
   nationalism. The first part examines the historiographical debate on the
   question in what period the formation of an American national identity
   occurred, i.e. before or after the American Revolution. The second part
   is concerned with the nature of American nationalism, casting doubt on
   the claim that American nationalism is exceptional, i.e. inherently
   different and morally superior to other nationalisms. It refutes the
   exceptionalist claim by applying recent findings in European nationalism
   research to the American case and by reinterpreting American nationalism
   not as an introspective phenomenon but as a demarcation process.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
