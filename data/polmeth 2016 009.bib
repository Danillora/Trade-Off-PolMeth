@article{ WOS:000386639000041,
Author = {Bennion, Elizabeth A. and Nickerson, David W.},
Title = {I Will Register and Vote, If You Teach Me How: A Field Experiment
   Testing Voter Registration in College Classrooms},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2016},
Volume = {49},
Number = {4},
Pages = {867-871},
Month = {OCT},
Abstract = {College students are young, have little or no history of voting, and are
   residentially mobile, which makes them a population in great need of
   registering to vote. Universities have a civic, pedagogical, and legal
   obligation to register their students to vote. In 2006, we conducted a
   controlled experiment across 16 college campuses to test the efficacy of
   classroom presentations to increase voter registration. The 25,256
   students across more than 1,026 classrooms were randomly assigned to one
   of three conditions: (1) a control group receiving no presentation; (2)
   a presentation by a professor; and (3) a presentation by a student
   volunteer. Verifying registration and voter turnout from a national
   voter database, we found that both types of presentations increased
   overall registration by 6 percentage points and turnout rates by
   approximately 2.6 percentage points. These results demonstrated that
   universities can take simple steps to engage their students in politics.},
