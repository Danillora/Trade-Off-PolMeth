@article{ WOS:000400013200007,
Author = {Caiani, Manuela and Graziano, Paolo R.},
Title = {Varieties of populism: insights from the Italian case},
Journal = {ITALIAN POLITICAL SCIENCE REVIEW-RIVISTA ITALIANA DI SCIENZA POLITICA},
Year = {2016},
Volume = {46},
Number = {2},
Pages = {243-267},
Month = {JUL},
Abstract = {Academic attention to populism has sharply increased in recent years.
   Yet, a commonly accepted definition is still lacking, with scholars
   disagreeing on categorization, labels, and boundaries between its
   different manifestations. In this article, through an analysis of
   Euromanifestos and party statutes, we address the interactions between
   Italian political parties and populism by adopting the various
   definitions available in populism theory, that is, populism as (i)
   ideology, (ii) rhetoric, (iii) communication style, and (iv)
   organization. Our aim is to identify all major attributes linked to
   populism and empirically investigate them with lower level indicators,
   measuring and comparing them across Italian parties and over time. The
   empirical analysis, conducted through a formalized content analysis of
   the European electoral programmes and party statutes, considers populism
   not as a discrete concept but rather as a continuous one and therefore
   enables us to undercover the variety of populisms within the Italian
   parties. The main finding of the article is that over the last decades
   all Italian parties have shown some signs of populism and that the
   intensification of populist features is strongly linked to the
   exarcebation of the financial and economic crisis, and its governance.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {32 AVENUE OF THE AMERICAS, NEW YORK, NY 10013-2473 USA},
Type = {Article},
Language = {English},
Affiliation = {Graziano, PR (Corresponding Author), Univ Padua, Dept Polit Sci Law \& Int Studies, Padua, Italy.
   Caiani, Manuela, Scuola Normale Super, Inst Humanities \& Social Sci, Florence, Italy.
   Graziano, Paolo R., Univ Padua, Dept Polit Sci Law \& Int Studies, Padua, Italy.},
DOI = {10.1017/ipo.2016.6},
ISSN = {0048-8402},
EISSN = {2057-4908},
Keywords = {populism; European Union; political parties; Italy},
