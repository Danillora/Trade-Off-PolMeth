@article{ WOS:000383284900002,
Author = {Dutra Fonseca, Pedro Cezar and Arend, Marcelo},
Title = {Neoschumpeterian contributions to the debate on Brazilian economic
   growth patterns in the XXI century},
Journal = {AMERICA LATINA HOY-REVISTA DE CIENCIAS SOCIALES},
Year = {2016},
Volume = {72},
Pages = {19-39},
Month = {APR},
Abstract = {Currently in Brazil there is an intense academic debate on the most
   appropriate follow growth pattern: wage-led or export-led. The first is
   defended by continuing economic growth, income distribution, while the
   second seeks to minimize imbalances in the balance of payments. We try
   to show that both growth patterns have limitations in ensuring sustained
   economic growth with income distribution for neglecting structural
   aspects of the Brazilian economy, as its productive structure and its
   international trade pattern.},
