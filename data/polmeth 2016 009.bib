@article{ WOS:000382757000008,
Author = {Bordat-Chauvin, Elodie},
Title = {Cultural Policies and Change: Mexico and Argentina after the Neoliberal
   Turn (1983-2012)},
Journal = {LATIN AMERICAN POLICY},
Year = {2016},
Volume = {7},
Number = {1},
Pages = {147-162},
Month = {JUN},
Abstract = {This article examines the changes that neoliberalism brought to the
   Mexican and Argentine cultural policies. Did the neoliberal turn of the
   1980s and the deepening of the reforms in the 1990s have the same
   effects in both countries? The findings of this article show that
   neoliberalism had a greater effect on cultural policy in Mexico because,
   since the Miguel de La Madrid administration in 1982, all governments
   have deepened neoliberal policies. On the other hand, in Argentina,
   where cultural policy is less institutionalized and more dependent on
   politics, neoliberalism had a major influence in the 1990s but less so
   since 2003 and Nestor Kirchner's election.},
