@article{ WOS:000384462900010,
Author = {Drouin, Marc},
Title = {`The realities of power': David Stoll and the story of the 1982
   Guatemalan genocide},
Journal = {JOURNAL OF GENOCIDE RESEARCH},
Year = {2016},
Volume = {18},
Number = {2-3, SI},
Pages = {305-322},
Abstract = {In a short opinion piece published in late 2013, anthropologist David
   Stoll claimed that genocide did not occur in Guatemala under the
   military dictatorship of Jose Efrain Rios Montt (1982-83), that the
   charges against the former general and his subsequent conviction were
   unsubstantiated, and that human rights conditions for the country's
   Indigenous peoples, including the Ixil population of northern Quiche
   department, actually improved under his government. By looking at the
   definition of protected groups under the United Nations Genocide
   Convention, and such basic notions as perpetrator motives and intent in
   international humanitarian law, this article will address Stoll's latest
   contribution to a `counter-narrative by Guatemalans who perceive that
   their side of the story {[}was] left out' of the 2013 genocide trial.},
