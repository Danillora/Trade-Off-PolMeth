@article{ WOS:000378274700008,
Author = {Hertel-Fernandez, Alexander},
Title = {How Employers Recruit Their Workers into Politics-And Why Political
   Scientists Should Care},
Journal = {PERSPECTIVES ON POLITICS},
Year = {2016},
Volume = {14},
Number = {2},
Pages = {410-421},
Month = {JUN},
Abstract = {In the wake of the Citizens United Supreme Court decision, many American
   private-sector employers now have the legal right to recruit their
   workers into politics and to fire or discipline employees who refuse to
   participate. How many firms and workers are engaged in this kind of
   political recruitment and why? And how have the opportunities for the
   political recruitment of workers by their employers changed over time?
   Drawing on national surveys of top corporate managers and workers, as
   well as a review of the legal literature, I provide initial answers to
   these questions and illustrate the implications of employer political
   recruitment for a range of substantive and normative issues in American
   politics. My findings invite further research and discussion about this
   feature of the American workplace and its effects on politics and
   policy.},
