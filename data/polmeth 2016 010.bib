   Swift, Clint S., Univ Missouri, 909 Univ Ave, Columbia, MO 65211 USA.
   VanderMolen, Kathryn A., Univ Missouri, Kinder Inst Constitut Democracy, Columbia, MO 65211 USA.},
DOI = {10.1177/1532440015608760},
ISSN = {1532-4400},
EISSN = {1946-1607},
Keywords = {legislative behavior; legislative politics; legislator preferences; term
