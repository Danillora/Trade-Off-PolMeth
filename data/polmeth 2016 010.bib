@article{ WOS:000385021200004,
Author = {Schultz, David},
Title = {(Un) Conventional Wisdom and Presidential Politics: The Myth of
   Convention Locations and Favorite-Son Vice Presidents},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
