@article{ WOS:000385988700007,
Author = {Kluge, Janis},
Title = {Limited Access Order State Weakness as Investment Risk in Russia},
Journal = {OSTEUROPA},
Year = {2016},
Volume = {66},
Number = {5},
Pages = {111+},
Abstract = {The state is considered a key political risk for foreign investors in
   Russia. But companies can be forced from the market by competitors using
   unfair methods. This is seen in the case of Swedish mobile operator
   Tele2. The reason for this double risk is a social order in which an
   elite coalition has secured economic privileges while competitors are
   granted only limited access. Property rights are no more protected from
   political intervention than the political world is from corruption by
   powerful economic interests. Russia does not need ``more{''} state or
   ``less{''} state, but a state based on the rule of law.},
