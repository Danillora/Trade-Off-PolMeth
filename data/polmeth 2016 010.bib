@article{ WOS:000393863300002,
Author = {Stoiber, Michael and Toeller, Annette Elisabeth},
Title = {Studying the Causes of Privatization of Hospital Order Treatment in
   Germany. A Qualitative Comparative Analysis of the German Lander},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2016},
Volume = {10},
Number = {1},
Pages = {9-36},
Month = {JUL},
Abstract = {The privatization of prisons is unheard of in Germany. A closely related
   yet more sensitive public responsibility - hospital order treatment
   (HOT) of criminals with diminished capacity or criminally incapable
   offenders in forensic hospitals was privatized in 5 out of 16 Lander
   during the last 15 years. In this article, we apply mvQCA in order to
   identify the causes of this privatization. MvQCA allows us to identify
   conjunctions of causes. Unlike suggested by the literature, neither
   problem pressure in hospital order treatment itself, nor partisan
   politics had an effect. Instead, we identified the institutional setting
   (the Land is the operator of hospital order treatment) as a necessary
   condition of the privatization of HOT. Furthermore, HOT was largely
   privatized in those Lander where the budget consolidation policy was
   above average.},
Publisher = {SPRINGER VIEWEG-SPRINGER FACHMEDIEN WIESBADEN GMBH},
