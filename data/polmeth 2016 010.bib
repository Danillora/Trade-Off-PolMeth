@article{ WOS:000374971200004,
Author = {Kogan, Vladimir},
Title = {When Voters Pull the Trigger: Can Direct Democracy Restrain Legislative
   Excesses?},
Journal = {LEGISLATIVE STUDIES QUARTERLY},
Year = {2016},
Volume = {41},
Number = {2},
Pages = {297-325},
Month = {MAY},
Abstract = {Direct democracy is sometimes described as a ``gun behind the door,{''}
   but how do legislators react when voters pull the trigger? Leveraging
   the high-profile referendum defeat of a controversial law passed by the
   Ohio legislature, I examine how legislators respond to voter
   disaffection. Using interest groups to ``bridge{''} votes before and
   after the election, I show that the measure's defeat induced moderation
   on the part of the Republican legislative majority, while leaving the
   behavior of opposition Democrats largely unchanged. The results suggest
   that direct democracy has the potential to restrain legislative excesses
   and alleviate polarization in state legislatures.},
