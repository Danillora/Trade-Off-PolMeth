@article{ WOS:000395217200003,
Author = {Hidalgo, Oliver},
Title = {The Idea of `Non-Western-Democracy': A genealogical reconstruction of
   the conceptional scope of the antinomic concept of democracy},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {35+},
Abstract = {This article demonstrates that the semantic contradiction between
   Western and Non Western democracies is based upon particular tensions
   and antinomies which constitute the conceptual history of democracy and
   can therefore be reconstructed genealogically. In search of crucial
   characteristics the traditional Western idea of liberal democracy and
   Non-Western innovations have in common, this contribution argues against
   the assumption that there is a definite transcultural interpretation or
   also a minimal consensus concerning the `core idea' of democracy.
   Instead, it focusses on the overlapping elements the global discourse
   about democracy shows at its boundaries. Accordingly, democratic
   antinomies offer a conceptual framework that illustrates both the common
   grounds and differences between the established ideas of democracy in
   Europe, America, Africa or Asia as well as the legitimacy and
   authenticity Western and Non-Western approaches may each claim.},
Publisher = {SPRINGER},
