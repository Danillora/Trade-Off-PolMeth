@article{ WOS:000390180200019,
Author = {Fukumoto, Kentaro and Horiuchi, Yusaku},
Title = {Identifying the effect of mobilization on voter turnout through a
   natural experiment},
Journal = {ELECTORAL STUDIES},
Year = {2016},
Volume = {44},
Pages = {192-202},
Month = {DEC},
Abstract = {Although numerous get-out-the-vote field experiments have identified the
   effects of particular mobilization tactics (e.g., canvassing, phone
   calls, direct mails) on voter turnout, we do not yet have a full
   understanding of the causal effect of overall mobilization. We study
   this by leveraging a natural experiment in Japan, in which the timing of
   a municipal election is as-if randomly assigned. The results show that
   almost concurrently held municipal elections boost these municipalities'
   voter turnout in prefectural elections by one to two percentage points.
   We argue that some unique settings in Japan allow us not only to
   mitigate omitted variable bias but also to attribute the estimated
   effect only to mobilization, rather than the effects of cost sharing and
   psychological stimulus. (C) 2016 Elsevier Ltd. All rights reserved.},
Publisher = {ELSEVIER SCI LTD},
