@article{ WOS:000391292700003,
Author = {Munnich, David},
Title = {Rousseau and the voluntary servitude?},
Journal = {STORIA DEL PENSIERO POLITICO},
Year = {2016},
Volume = {5},
Number = {2},
Pages = {203-220},
Month = {MAY-AUG},
Abstract = {Among the variety of readers of Etienne de La Poetic through history,
   many were in Geneva, where the text was published very early on. The
   great author from Geneva, lean JacquesRousseau, may have known this text
   since his early years. The article analyzes the influence that La Boetie
   may have had on the first text of Rousseau, Le Dfccours sir les Sciences
   el les Arts, drawing on previous analyses by Miguel Abeusour. This
   reading of Rousseau with La Boetie in mind also builds on the famous
   article Leo Strauss dedicated to the first Dzscours of Rousseau, but
   reaches `fundamentally opposite conclusions. This article, closer to the
   understanding that Ernst Cassirer had of Rousseau, attempts to explore
   this potential relationship between Rousseau and La Boetie and to
   understand where this can lead us in our view of a different Rousseau.},
