@article{ WOS:000382574600004,
Author = {Hagemann, Sara and Franchino, Fabio},
Title = {Transparency vs efficiency? A study of negotiations in the Council of
   the European Union},
Journal = {EUROPEAN UNION POLITICS},
Year = {2016},
Volume = {17},
Number = {3},
Pages = {408-428},
Month = {SEP},
Abstract = {Recent studies suggest there is a direct trade-off between transparency
   and efficiency in legislative politics. We challenge this conclusion and
   present a bargaining model where one particular kind of transparency -
   the publication of legislative records - works to overcome problems of
   incomplete information. We also present empirical findings from
   legislative activities in the Council of the European Union from 1999 to
   2014 and from 23 interviews with senior officials in Brussels. Our
   results show that increased transparency, in the form of publication of
   legislative records, does not lead to gridlock or prolonged
   negotiations. On the contrary, recordings of governments' positions help
   facilitate decision-making as it increases credibility of policy
   positions. This, in turn, lowers risk of negotiation failure and screens
   out marginal amendments.},
