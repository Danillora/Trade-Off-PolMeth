@article{ WOS:000395571600002,
Author = {Sour Vargas, Laura},
Title = {A Review of the Mexican ``Flypaper Effect{''} (1990-2012)},
Journal = {REVISTA MEXICANA DE ANALISIS POLITICO Y ADMINISTRACION PUBLICA},
Year = {2016},
Volume = {5},
Number = {2},
Pages = {9-28},
Month = {JUL-DEC},
Abstract = {This paper contributes to the lack of analysis on the flypaper effect
   for the Mexican case, analyzing the larger number of municipalities
   (2,441) over 20 years (1990-2012). The presence of the flypaper effect
   is confirmed, but unlike Ibarra and Varella (2008); Cardenas and Sharma
   (2011) and Sour (2013), this paper finds that the increase in
   unconditional transfers proportionally increases local public spending.
   These results verify the prediction of Hines and Thaler (1995): money
   sticks where it hits. Also, the presence of the asymmetrical effect is
   confirmed. These results are incorporated into the discussion on fiscal
   federalism reform in Mexico.},
