@article{ WOS:000437187800005,
Author = {Derpanopoulos, George and Frantz, Erica and Geddes, Barbara and Wright,
   Joseph},
Title = {Are coups good for democracy?},
Journal = {RESEARCH \& POLITICS},
Year = {2016},
Volume = {3},
Number = {1},
Month = {JAN-MAR},
Abstract = {A number of recent studies argue that coups can help usher in democracy.
   We examine this relationship empirically by looking at the political
   regimes that follow coups in autocracies, as well as the level of
   repression against citizens. We find that, though democracies are
   occasionally established in the wake of coups, more often new
   authoritarian regimes emerge, along with higher levels of
   state-sanctioned violence.},
