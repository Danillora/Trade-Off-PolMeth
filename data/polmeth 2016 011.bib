@article{ WOS:000374656800004,
Author = {Siroky, David S. and Mahmudlu, Ceyhun},
Title = {E Pluribus Unum? Ethnicity, Islam, and the Construction of Identity in
   Azerbaijan},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2016},
Volume = {63},
Number = {2},
Pages = {94-107},
Month = {MAR-APR},
Abstract = {In multinational states, developing a strong sense of civic nationalism
   among minorities is critical to creating social order. Countries that
   fail to cultivate civic nationalism among minorities can face persistent
   problems of separatism on the periphery. This article investigates the
   role of three factors that may explain the variable loyalty of
   individuals from minority groups to the state-inter-ethnic networks,
   in-group solidarity and religiosity. Drawing on an original survey of
   Lezghin and Talysh minorities in Azerbaijan, the analysis suggests that
   individuals with a stronger Islamic identity and more interethnic
   networks were more likely to identify with state-promoted civic
   nationalism, due to their ability to cut across ethnic identities.
   Against our expectations, group solidarity did not influence an
