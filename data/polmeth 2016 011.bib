@article{ WOS:000395217200008,
Author = {Birsl, Ursula and Salzborn, Samuel},
Title = {Unwritten Rules of `Democracy': The interrelatedness of `unwritten
   constitutions' underlying political governance in and beyond the West},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {127+},
Abstract = {It is generally accepted among democracy and autocracy researchers that
   when analyzing how a political society is actually governed, it is often
   less useful to look at constitutional norms than to look at the
   constitutional reality. But how can we investigate this constitutional
   reality in comparative research without immediately imposing a
   categorical differentiation between Western and non-Western or between
   democracies and autocracies? Instead, we should try to consider their
   commonalities and interrelationships, and their historical development.
   This article looks at how formal and informal institutions, along with
   unwritten rules of political governance (and of legitimation), can
   condense into an ``unwritten constitution{''} or quasi-constitution. It
   examines historical interdependencies within and beyond the West and its
   colonial and postcolonial relationships, by drawing upon histories
   reaching back through the Middle Ages to antiquity. The goal is to
   uncover the interrelatedness underlying the development of political
   governance, an interrelatedness in which Europe, or more precisely
   Western Europe, is not treated as the normative and empirical benchmark.},
Publisher = {NOMOS},
