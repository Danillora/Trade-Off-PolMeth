@article{ WOS:000385366700007,
Author = {Greene, Zachary and Haber, Matthias},
Title = {Leadership Competition and Disagreement at Party National Congresses},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2016},
Volume = {46},
Number = {3},
Pages = {611-632},
Month = {JUL},
Abstract = {Theories often explain intraparty competition based on electoral
   conditions and intraparty rules. This article further opens this black
   box by considering intraparty statements of preferences. In particular,
   it predicts that intraparty preference heterogeneity increases after
   electoral losses, but that candidates deviating from the party's median
   receive fewer intraparty votes. Party members grant candidates greater
   leeway to accommodate competing policy demands when in government. The
   study tests the hypotheses using a new database of party congress
   speeches from Germany and France, and uses automated text classification
   to estimate speakers' relative preferences. The results demonstrate that
   speeches at party meetings provide valuable insights into actors'
   preferences and intraparty politics. The article finds evidence of a
   complex relationship between the governing context, the economy and
   intraparty disagreement.},
