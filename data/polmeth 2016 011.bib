@article{ WOS:000378823100001,
Author = {Ludwig, Gundula},
Title = {The ``liberal dispositive of separation{''} as a state-founding
   construct. A queer-feminist hegemony-theoretical perspective on the
   public-private-divide},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Volume = {57},
Number = {2},
Pages = {193-216},
Abstract = {During the last decades the dichotomy of a masculine public sphere and a
   feminine private sphere as well as the strict heteronormative character
   of the public have blurred. However, these changes have rarely been
   theorized in feminist political theory even though the public/private
   separation has long been one of the core topics in feminist political
   theory. The paper aims at re-activating these debates by bringing to
   light the current meaning of gender and heteronormativity for
   constituting the public and the private. From a state-theoretical
   perspective the paper focuses on two aspects citizenship and the
   construction of a national imaginary to reveal the continuities that lie
   beyond the transformations.},
