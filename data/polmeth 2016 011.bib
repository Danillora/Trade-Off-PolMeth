@article{ WOS:000385369400007,
Author = {Bensel, Richard},
Title = {Lost in Translation: An Epistemological Exploration of the Relation
   between Historical Analysis and the NOMINATE Algorithm},
Journal = {STUDIES IN AMERICAN POLITICAL DEVELOPMENT},
Year = {2016},
Volume = {30},
Number = {2},
Pages = {185-201},
Month = {OCT},
Abstract = {The NOMINATE algorithm has become the most important analytical tool
   used in the study of the United States Congress. As such, congressional
   scholars have developed a great many social conventions, practices, and
   assumptions that enable interpretation of the statistical artifacts the
   algorithm produces. However, as many of these scholars recognize,
   serious problems emerge whenever we try to translate these statistical
   artifacts into language and thus attempt to assign them meaning in
   historical analysis. These problems are irresolvable because they reside
   in the very construction of the algorithm itself.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {32 AVENUE OF THE AMERICAS, NEW YORK, NY 10013-2473 USA},
Type = {Article},
