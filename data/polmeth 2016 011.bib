@article{ WOS:000385743000003,
Author = {Stockemer, Daniel},
Title = {Is the Turnout Function in Democracies and Nondemocracies Alike or
   Different?},
Journal = {POLITICS \& POLICY},
Year = {2016},
Volume = {44},
Number = {5},
Pages = {889-915},
Month = {OCT},
Abstract = {Are the turnout functions in democracies and nondemocracies different
   beasts that cannot be compared or are there more similarities than
   differences in the constituents of electoral turnout in the two
   universes of cases? Interested in this question, I compare electoral
   turnout in democracies and nondemocracies. Based on a large-scale
   dataset, which includes data for over 540 elections over an 18-year
   period (1994-2012), I find distinct patterns in the predictors of
   turnout across the two regime types. One the one hand, my results
   indicate that the influence of institutions such as compulsory voting or
   electoral rules is alike in democracies and nondemocracies. However, the
   same cannot be said for contextual factors such as corruption or
   electoral closeness, whose influence fundamentally differs in the two
   settings.},
