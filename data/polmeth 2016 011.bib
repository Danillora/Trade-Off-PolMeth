@article{ WOS:000390863300011,
Author = {Ferras, Graciela and Aguirre, German},
Title = {Ricardo Rojas and the Fate of Proscription: Liberalism and Democracy in
   the 1930s.},
Journal = {ANACRONISMO E IRRUPCION},
