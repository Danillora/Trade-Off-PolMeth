@article{ WOS:000378853600022,
Author = {Ghafoori, Akbar and Kafrudi, Esmail Ahmadi},
Title = {Political Development and the Influential and Distributive Crises},
Journal = {JOURNAL OF POLITICS AND LAW},
Year = {2016},
Volume = {9},
