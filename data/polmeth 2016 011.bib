@article{ WOS:000377532800003,
Author = {Malleson, Tom},
Title = {Offending the One Percent: Seven Arguments Against Distributive Desert},
Journal = {NEW POLITICAL SCIENCE},
Year = {2016},
Volume = {38},
Number = {2},
Pages = {178-200},
Abstract = {The belief that people morally deserve the income they acquire in the
   market is both powerful and deep-rooted. Nevertheless, most political
   philosophers are skeptical of the idea that market income is morally
   deserved. There is thus a large and uncomfortable chasm between the
   philosophical mainstream and the actual public. The purpose of this
   article was to inject new intellectual effort in closing this gap. The
   goal is the ambitious one of a comprehensive demolition of the notion of
   distributive desert. To this end, I put forward seven critical
   arguments. Four of them are common in the literature. Since these have
   been adequately discussed elsewhere, I mention them here briefly and
   only for completeness. The core of the article focuses on three original
   arguments. Overall, my aim is to show that, taking these arguments
   together, the case against distributive desert is conclusive.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
