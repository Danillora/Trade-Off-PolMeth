@article{ WOS:000377109600007,
Author = {Vainikka, Joni Tuomas},
Title = {Finland's Parliamentary Election 2015: Certainty of the Result with
   Uncertainty of the Outcome},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2016},
Volume = {26},
Number = {2},
Pages = {269-285},
Abstract = {The winner of the Finnish parliamentary election 2015 was certain long
   before the elections. The Centre Party of Finland would win, but for the
   composition of the government and future societal changes, it was more
   interesting who would be the runners-up. With eight parliamentary
   parties and with eight different political agendas, the Finnish
   political system forms a hard to predict political landscape. Key issues
   for the Centre Party, rebounding from the catastrophic elections of
   2011, were to find a coalition with trust between its members and to
   generate trust in getting things done. One evident societal outcome of
   the election is the deceleration of centralization policies furthered by
   the previous government. The disagreement over regional politics and
   devolution and related cleavages within the country brought the
   coalition government between the Centre Party, the National Coalition
   Party and the Finns Party to the verge of falling after only six months.},
