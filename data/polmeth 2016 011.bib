@article{ WOS:000366281900007,
Author = {Claassen, Christopher},
Title = {Group Entitlement, Anger and Participation in Intergroup Violence},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2016},
Volume = {46},
Number = {1},
Pages = {127-148},
Month = {JAN},
Abstract = {There is little research on the thousands of individuals who take part
   in intergroup violence. This article proposes that their participation
   is motivated by the emotion of intergroup anger, which, in turn, is
   triggered by a comparison between the intergroup distribution of
   resources and the distribution that is believed to be desirable. Thus,
   when another group is perceived to violate group entitlements - by
   taking jobs thought to belong to the ingroup, for example - anger is
   experienced and individuals become more willing to take part in violence
   against the outgroup. Support for this theory is found in a new survey
   dataset, collected in a slum in South Africa where anti-immigrant
   violence occurred in 2008.},
