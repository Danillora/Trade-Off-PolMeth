@article{ WOS:000375733100020,
Author = {Urban, Thomas},
Title = {Change of Perspective The Edition Deutsch-Polnische
   Erinnerungsorte},
Journal = {OSTEUROPA},
Year = {2016},
Volume = {66},
Number = {1-2},
Pages = {237+},
Abstract = {Germany and Poland are closely bound by their common history. The
   violent culmination of this was the German war of aggression from 1939
   to 1945. To this day, mutual perceptions are marked by mistrust and
   prejudice and provoke a deep sense of guilt on one side and tremendous
   resentment on the other. The nine-volume edition Deutsch-Polnische
   Erinnerungsorte is dedicated to reciprocal views of key historical
   events, fills in gaps of knowledge in national histories, and explores
   on the basis of the respective cultures of remembrance differences of
   identity and commonalities. This new approach in historiography also
   contributes to understanding in the present-day evaluation of sensitive
   issues such as flight and expulsion.},
