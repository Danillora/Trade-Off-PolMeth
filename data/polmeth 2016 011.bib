@article{ WOS:000395217200026,
Author = {Gsaenger, Matthias},
Title = {``We are not going just to copy{''}: The Indian discourse on democracy
   from the 1920s until the Constituent Assembly},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {511+},
Abstract = {The topic of the present paper is the question to what degree new
   elements of democratic order have been developed in the Indian discourse
   of the 1920s till the 1940s, elements that go beyond the liberal
   standard model of democracy. It is assumed here that the main
   innovations lie in the special group rights debated in this discourse.
   The standard liberal model only knows individuals as bearers of rights.
   In the text, contributions of Gandhi, Nehru and Ambedkar to the Indian
   discourse will be analyzed. Which challenges to the Indian Society did
   they perceive, and how did they intend to react to them by using the
   means of democracy? How should democracy be shaped with respect to the
   Indian culture and the connected societal constellations? The competing
   conceptions the three authors are standing for, made compromises in the
   Constituent Assembly concerning the design of the corresponding rights
   both necessary and possible. Thus, the Indian Democracy looks like a
   ``Development Democracy{''}, but also successfully negates the
   implications of that model, namely that the Indian democracy should not
   function at all.},
