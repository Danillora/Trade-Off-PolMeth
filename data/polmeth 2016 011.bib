@article{ WOS:000386156000005,
Author = {Banducci, Susan and Elder, Laurel and Greene, Steven and Stevens, Daniel},
Title = {Parenthood and the polarisation of political attitudes in Europe},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
Year = {2016},
Volume = {55},
Number = {4},
Pages = {745-766},
Month = {NOV},
Abstract = {Becoming a parent can affect the lives of men and women by introducing
   salient new social roles and identities, altered social networks and
   tighter constraints on financial resources and time. Even though modern
   family life has evolved in many important respects, parenthood continues
   to shape the lives of men and women in very different ways. Given that
   parenthood can change the lives of men and women in profoundly different
   ways, it seems that it would bring about changes in the way women and
   men think about politics and policy issues. Using data from the Wave 4
   of the European Social Survey, this article investigates how parenthood,
