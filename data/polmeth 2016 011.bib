@article{ WOS:000389852800005,
Author = {Esquivel Alonso, Yessica},
Title = {Dreams of Freedom and Censorship. Freedom of the Press in 1810},
Journal = {REVISTA DE ESTUDIOS POLITICOS},
Year = {2016},
Number = {174},
Pages = {143-172},
Month = {OCT-DEC},
Abstract = {This article focuses on the study of freedom of the press. The main
   objective is to discover if freedom of the press is the same as free
   speech in the constitutional model of 1812. We analyse the Daily Session
   of the Parliament of Cadiz and some newspapers of the time, focusing on
   the specific period from 1810 to 1812. The legal background of freedom
