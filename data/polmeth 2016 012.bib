@article{ WOS:000404759800001,
Author = {Metelits, Claire},
Title = {Challenging U.S. Security Assessments of Africa},
Journal = {AFRICAN SECURITY},
Year = {2016},
Volume = {9},
Number = {2},
Pages = {89-109},
Abstract = {For decades, Africa lay at the periphery of US security policy.
   Attention to the continent intensified after the 9/11 terrorist attacks
   amid claims that East Africa and the Sahel had become hubs of terrorist
   activity. Despite this shift, US analysts and policymakers continue to
   frame security threats in Africa using a traditional, Cold
   War-influenced paradigm. Commonly used indicators are based on
   mainstream security studies, which provides a narrow view of security.
   Using Critical Security Studies, this article provides a broader and
   more inclusive assessment of security threats in Africa.},
