@article{ WOS:000374862100004,
Author = {Johnson, Joel A.},
Title = {Mosses, Manses, and Mores: Hawthorne on the Construction and
   Preservation of Political Institutions},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2016},
Volume = {5},
Number = {2},
Pages = {277-302},
Month = {SPR},
Abstract = {Long considered a literary master, Nathaniel Hawthorne was also a shrewd
   political thinker, often embedding his insights in seemingly
   nonpolitical works. In The Old Manse, Hawthorne uses an imaginative tour
   of his Concord residence to prompt reflection on the core nature and
   functions of well-built human institutions. After considering the value
   of narrative fiction in gaining a deeper understanding of political
   institutions, I draw out the implications of Hawthorne's essay for
   institution building. Hawthorne's observations are well worth
   considering amid the political turbulence of recent years, for they
   serve as a guide for constructing institutions that can reconcile the
   claims of security and liberty, while also building multiple layers of
   community.},
