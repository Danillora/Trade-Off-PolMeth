   Mecham, Quinn, Brigham Young Univ, Polit Sci, Provo, UT 84602 USA.
   Mecham, Quinn, Brigham Young Univ, Middle East Studies, Provo, UT 84602 USA.},
DOI = {10.1111/lsq.12144},
