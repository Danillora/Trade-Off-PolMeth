@article{ WOS:000388578400003,
Author = {Geys, Benny and Mause, Karsten},
Title = {The Limits of Electoral Control: Evidence from Last-Term Politicians},
Journal = {LEGISLATIVE STUDIES QUARTERLY},
Year = {2016},
Volume = {41},
Number = {4},
Pages = {873-898},
Month = {NOV},
Abstract = {In modern democracies, politicians' accountability is often linked to
   the disciplining mechanism of electoral control. For politicians in
   their final term, this mechanism is impaired. Using a novel data set
   covering 910 members of the UK House of Commons active within the period
   1997-2010, we investigate how reduced electoral control affects
   last-term MPs' trade-off between work effort inside parliament, leisure,
   and outside interests. Our main contributions lie in providing the first
   explicit consideration of (1) MPs' final-term intra-/extraparliamentary
   work balance and (2) MPs' reasons for leaving parliament (i.e.,
   retirement, career change, electoral defeat). These extensions provide
   important fresh insights concerning the boundaries of elections'
   disciplining power.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Geys, B (Corresponding Author), Norwegian Business Sch Oslo, Oslo, Norway.
   Geys, B (Corresponding Author), Vrije Univ Brussel, Brussels, Belgium.
   Geys, Benny, Norwegian Business Sch Oslo, Oslo, Norway.
   Geys, Benny, Vrije Univ Brussel, Brussels, Belgium.
   Mause, Karsten, Univ Munster, Dept Polit Sci, Polit Econ, Munster, Germany.},
DOI = {10.1111/lsq.12136},
ISSN = {0362-9805},
