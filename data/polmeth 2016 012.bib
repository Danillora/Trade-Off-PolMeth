@article{ WOS:000374862100005,
Author = {Winkelman, Joel},
Title = {John Dewey's Theory of Vocation},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2016},
Volume = {5},
Number = {2},
Pages = {303-325},
Month = {SPR},
Abstract = {This article explores the concept of vocation in the political theory of
   John Dewey. Primarily relying on his political thought from the years
   prior to World War I, it analyzes in particular his early books and
   periodical essays, along with his lectures to teachers' organizations,
   talks before groups of political activists, and seminars at the
   University of Chicago from this time. Using these early and formative
   sources, it argues that Dewey's theory of democracy can rightly be
   understood as being centered on vocation's role in society. In making
   this case, the article focuses on this concept, examining its relation
   to work, its role in democratic education, and its meaning for the
   professions, including experts in business and government. The essay
   concludes by discussing how Dewey's understanding of vocation can help
   us reconsider the continuing political challenges and possibilities
   posed by the contemporary American experience of work.},
