@article{ WOS:000391132000002,
Author = {Moustafa, Salih},
Title = {Kirkuk conflict in its ethnic/political media: the Turkmen newspaper
   Alqal'a as a model},
Journal = {NATIONAL IDENTITIES},
Year = {2016},
Volume = {18},
Number = {3},
Pages = {265-287},
Abstract = {This article explores the Kirkuk conflict through the Turkmens'
   newspaper Alqal'a. Using agenda-setting and framing/priming for
   theoretical background and qualitative content analysis as a method, two
   macro-themes, related to the conflict, are found to be fundamental
   therein. These two main themes are: (1) exclusion and (2)
   self-confirmation. The two macro-themes overarch several micro-or
   sub-themes and are closely related to each other. They are set to inform
   the readers about what to think and how to think about the conflict.
   While `exclusion' constitutes the cause, `self-confirmation' is the
   latter's effect.},
