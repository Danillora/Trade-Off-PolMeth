@article{ WOS:000395217200006,
Author = {Keane, John},
Title = {The New Despoties: Imaginaries of the end of democracy},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {86+},
Abstract = {This article recommends the term `despoties' for those Eurasian regimes
   combining democratic rhetoric with undemocratic rule. Despotism and
   totalitarian dictatorship differ in the fact the first one does
   guarantee high output without mobilizing the people. Other phenomena,
   however, are more classic, e.g., systems of clientelism and patronage
   wrongly considered as only pre-modern features by theories of
   modernization. The differentiation with democracies is more difficult,
   since despotism is a ``polysemic rule of a new kind{''}. Within
   despoties undemocratic features are combined with democratic ones (like
   formal execution of elections) and highly efficient governance. It is
   argued that a new type of regime is emerging beyond known notions, such
   as `defect' `delegative', or `illiberal' democracy.},
