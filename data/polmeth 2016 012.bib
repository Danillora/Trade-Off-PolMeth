@article{ WOS:000381630400005,
Author = {Strezhneva, Marina V.},
Title = {THE ROLE OF THE EUROPEAN UNION IN FOOTBALL GOVERNANCE},
Journal = {SRAVNITELNAYA POLITIKA-COMPARATIVE POLITICS},
