@article{ WOS:000375733100009,
Author = {Wigura, Karolina},
Title = {Demonology Poland's Prospect of Four Lost Years},
Journal = {OSTEUROPA},
Year = {2016},
Volume = {66},
Number = {1-2},
Pages = {109+},
Abstract = {In Poland, two irreconcilable camps stand opposite one another: the
   ``right-wing conservatives{''} and ``liberal Poland{''}. In the
   self-perception of the party Law and Justice (PiS), this stand-off is a
   Manichaean struggle between good and evil for the ``recovery{''} of a
   system that is seriously ill. The demonization of Jarostaw Kaczynski and
   his party on the part of the liberal camp follows the same Manichean
   logic. As a result of this confrontation, urgently needed reforms, such
   as those in healthcare, are at risk of being dragged out. And the way
   PiS has acted since taking over power calls to mind Edmund Burke's
   insight: change based on indignation can lead to disaster.},
