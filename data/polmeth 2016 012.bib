@article{ WOS:000383814300003,
Author = {von Weikersthal, Felicitas Fischer},
Title = {Social-revolutionary in Form, National in Content},
Journal = {OSTEUROPA},
Year = {2016},
Volume = {66},
Number = {4},
Pages = {19+},
Abstract = {During the Revolution of 1905/1907, a terrorist ``epidemic{''} broke out
   in the Russian Empire. Russian Poland was one of the main centres. A
   disproportionate number of attacks and assassinations took place in
   Warsaw, Lodz, and other cities within the once autonomous kingdom. This
   massive wave of terrorist violence did not have socioeconomic causes,
   but rather fed on resurgent Polish nationalism. That the imperial centre
   perceived the Polish periphery as essentially unreliable and,
   accordingly, pursued a repressive policy, contributed to the escalation.
   Broad swathes of the Polish population perceived themselves as
   humiliated.},
