@article{ WOS:000409203000006,
Author = {Dubsky, Zbynek and Srsen, Radim and Slajs, Jan},
Title = {EUROPEAN UNION AS A SPECIFIC ACTOR OF NEW PARADIGM FOR CONFLICT
   PREVENTION},
Journal = {POLITICKE VEDY},
Year = {2016},
