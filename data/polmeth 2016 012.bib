@article{ WOS:000382727700004,
Author = {el-Ojeili, Chamsy and Taylor, Dylan},
Title = {Across and Beyond the Far Left: The Case of Gilles Dauve},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2016},
Volume = {28},
Number = {2},
Pages = {187-203},
Abstract = {This essay explores the work of French ultraleftist Gilles Dauve.
   Situating his contribution against a discussion of left communism as a
   unified intellectual-political current, it identifies and discusses
   three crucial moments in Dauve's work. A first moment, 1969-1979, sees
   Dauve attempting to critically draw together council communist,
   Bordigist, and situationist contentions into a unified and unique
   communist perspective. During a second moment, coincident with the
   crisis of Marxism, Dauve continued to solidify this position, in
   particular criticizing the confluence of liberal-democratic thought with
   antifascism. In a third moment, 1999-present, Dauve has engaged in
   important rethinking and clarification, further underscoring communism
   as communization. The essay's conclusion underscores the importance of
   Dauv's singular intellectual journey in terms of its novel synthetic
   quality, its resonance with contemporary discussions of the appearance
   of a new global Left, and its important contribution to the
   communization current.},
