@article{ WOS:000395217200027,
Author = {Noesselt, Nele},
Title = {Chinese Discourses on Democracy: Governance imaginations in the national
   and international context},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {535+},
Abstract = {People's democracy, socialist democracy, people's democratic
   dictatorship the PRC's official political discourses operate with
   various special conceptions of ``democracy{''}. However, it would be
   more than misleading to evaluate and judge these debates according to
   the standard definition found in pluralist, liberal democracies. The
   article examines the state philosophy underlying the democracy concept
   used by the fifth generation of Chinese leaders and tries to identify
   divergences/convergences between the fifth and the fourth generation's
   ideas about democracy. The strengthening of deliberative democracy
   accompanied by a recentralization of political power under Xi Jinping
   allows insights into the current reconfiguration of China's governance
   model.},
