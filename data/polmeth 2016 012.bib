@article{ WOS:000395869100004,
Author = {Moreno Segura, Laura Natalia},
Title = {UNA CIUDAD INCONEXA. EL DESASTRE EN PUERTO PRINCIPE},
Journal = {AMERICA LATINA HOY-REVISTA DE CIENCIAS SOCIALES},
Year = {2016},
Volume = {74},
Pages = {55-79},
Month = {DEC},
Abstract = {Hundreds of help programs have been deployed in Port-au-Prince, after
   the earthquake of 2010, with the intention of re-establishing the order
   and the life lost under the rubbles. Near six years after the tragedy
