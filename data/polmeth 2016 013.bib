@article{ WOS:000394815000012,
Author = {Orr, Graeme and Cassar, Samara},
Title = {When Referendums Go Wrong - Queensland's 2016 Fixed Four-Year Term
   Proposal},
Journal = {AUSTRALASIAN PARLIAMENTARY REVIEW},
Year = {2016},
Volume = {31},
Number = {2},
Pages = {161-175},
Month = {SPR-SUM},
Abstract = {Constitutional referendums are an opportunity to showcase direct
   democracy and popular sovereignty over the shape of the institutions
   that govern representative democracy. But to be deliberatively
   meaningful, referendums on such legal questions require lengthy voter
   education, and fair structuring of questions. In 2016, Queensland voted
   narrowly to join the other Australian States and Territories in having
   fixed, four-year terms. This article explains the context behind the
   referendum, and analyses the partisan and regional drivers of its
   outcome. It also critiques the process, which bundled two distinct
   questions, in a hurried `campaign' that offered minimal voter education.
   In the result, the major parties won the longer terms they had long
   craved. However Queensland is left with less frequent elections, and a
   powerful executive with neither an upper house nor proportional
   representation to check it.},
Publisher = {AUSTRALASIAN STUDY PARLIAMENT GROUP},
