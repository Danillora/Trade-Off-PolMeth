@article{ WOS:000395217200015,
Author = {Pickel, Gert},
Title = {Worldwide Democratization as Path-Dependent Consequence of
   Modernization? In defense of theories of modernization applied in a
   differentiated way},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {268+},
Abstract = {In the last decades modernization theory, once dominant in the mid-20th
   century, has faced strong criticism, if not outright rejection.
   Especially postcolonial studies criticized its ethnocentrism and the
   ignorance of indigenous possibilities of development. This critique was
   also applied to western-dominated democracy research, even if it was not
   rejected as fully as modernization theory. Keeping this in mind, it is
   surprising that differences in the degree of modernization seem to have
   significant power in the empirical explanation of democratization.
   Results show a strong effect of modernization on political culture. One
   reason is the example set by Western countries, another reason are other
   aspects of modernization. Thus, worldwide cross-national analyses reveal
   high support for basic democratic principles nearly everywhere. Even if
   the understanding of democracy can differ between cultures and regions,
   it does not support the thesis of the complete irrelevance of a
   democratic theory relying on modernization theory; not even if conflicts
   between political cultures and identity groups have diversified the
   paths of political-cultural developments.},
