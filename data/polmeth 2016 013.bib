@article{ WOS:000374507300007,
Author = {Soderlund, Peter},
Title = {Candidate-centred electoral systems and change in incumbent vote share:
   A cross-national and longitudinal analysis},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
