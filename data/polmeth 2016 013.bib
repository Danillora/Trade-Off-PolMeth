@article{ WOS:000384884200026,
Author = {Maher, Amanda},
Title = {What Skinner Misses about Machiavelli's Freedom: Inequality, Corruption,
   and the Institutional Origins of Civic Virtue},
Journal = {JOURNAL OF POLITICS},
Year = {2016},
Volume = {78},
Number = {4},
Pages = {1003-1015},
Month = {OCT},
Abstract = {Does law habituate republican citizens toward virtue? This article
   addresses the ambiguous relationship between law and civic virtue in
   Quentin Skinner's interpretation of Machiavelli's liberty. I trace
   Skinner's inability to reconcile the role of law with a politics of
   virtue to his humanist interpretation of Machiavelli's theory of
   corruption as a condition caused by mankind's innate selfishness. I
   argue instead that Machiavelli uses the concept of corruption to
   identify the deleterious effects of wealth inequalityand the culture of
   dependence generated by iton the political identity and behavior of
   ordinary citizens. I argue that Skinner's failure to appreciate the
   socioeconomic origins of corruption for Machiavelli prevents him from
   understanding the significance of a constitutional order that nurtures a
   class-based form of social discord for freedom.},
