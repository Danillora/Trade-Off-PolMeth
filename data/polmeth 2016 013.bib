@article{ WOS:000385527900008,
Author = {Moss, Jeremy},
Title = {Mining, morality and the obligations of fossil fuel exporting countries},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2016},
Volume = {51},
Number = {3},
Pages = {496-511},
Month = {SEP},
Abstract = {In this article I argue that countries exporting fossil fuels, such as
   Australia, have an obligation to bear some of the costs of the harms
   caused by the use of those fuels. I argue that there is an analogy
   between other harmful exports - medical waste, tobacco, unsafe jobs,
   uranium - and fossil fuels. If this is the case, then current methods
   for allocating emissions and responsibilities for their harms are
   inadequate and more complex than they appear. I consider several
   counter-arguments to this claim, such as that it does not recognise the
   benefits of coal and that exporters are not really responsible. Finally,
   I consider some of the consequences of this argument and claim that
   Australia and other fossil fuel exporters ought to have a higher carbon
   budget' if this argument is true and that exporters ought to bear a
   higher share of the costs of climate harms.},
