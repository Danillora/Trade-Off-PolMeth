@article{ WOS:000392701400007,
Author = {Soderstrom, Johanna},
Title = {The resilient, the remobilized and the removed: party mobilization among
   former M19 combatants},
Journal = {CIVIL WARS},
Year = {2016},
Volume = {18},
Number = {2},
Pages = {214-233},
Abstract = {Armed groups often transform into political parties, which involves a
   profound transformation of the organizational culture. How these parties
   condition the continued political mobilization of their members is
   unclear. Using life history interviews with former combatants of the
   armed group M19 in Colombia, this article demonstrates what aspects of
   the party mobilize and stymie their political mobilization. Through
   exploring three typical political life paths - the Resilient, the
   Remobilized and the Removed - this article demonstrates the long-term
   challenges of post-war politics, the role of the party, as well as the
   personal journey from (war and) peace to democracy.},
