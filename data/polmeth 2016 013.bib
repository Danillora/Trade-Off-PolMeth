@article{ WOS:000395217200030,
Author = {Schubert, Sophia and Weiss, Alexander},
Title = {For a Global-Transcultural Approach to Democracy Research},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {589+},
Abstract = {In this concluding chapter we take up main and transversal findings and
   research experiences from the contributions to this book in order to,
   first, fill our double taxonomy of democracy with non-Western
   conceptions of democracy and, second, further develop our new approach
   of,global-transcultural democracy research' (GTDR). To this end, we now
   differentiate between four basic research designs within
   global-transcultural democracy research: 1) the deduction from the
   heuristic and abstract `minimal' concept of democracy towards concrete
   conceptions of democracy, 2) the contextualistic analysis of these
   concrete conceptions, 3) the induction towards a more abstract
   global-transcultural concept of democracy, and 4) the abduction by
   which, in a reflexive loop, the original `minimal' concept of democracy
   is continuously being questioned by the experiences with the concrete
   conceptions of democracy. Finally, we discuss the implications and
   consequences of our new approach of global-transcultural democracy
   research for democratic theory and empirical democracy research.},
Publisher = {SPRINGER},
Address = {233 SPRING ST, NEW YORK, NY 10013 USA},
Type = {Article},
Language = {German},
