@article{ WOS:000386925200004,
Author = {Guinaudeau, Isabelle and Palau, Anna M.},
Title = {A matter of conflict: How events and parties shape the news coverage of
   EU affairs},
Journal = {EUROPEAN UNION POLITICS},
Year = {2016},
Volume = {17},
Number = {4},
Pages = {593-615},
Month = {DEC},
Abstract = {This article argues that external factors of EU coverage in the media
   need to be reassessed against domestic factors, in particular how
   parties modulate media attention to EU affairs. We explain which parties
   may set the EU on the media agenda, and how parties interact with events
   depending on the level of conflict over EU issues. Drawing on the first
   long-term analysis of partisan agenda-setting of EU affairs in the media
