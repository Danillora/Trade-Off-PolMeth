@article{ WOS:000443341000001,
Author = {Fouskas, Vassilis K. and Roy-Mukherjee, Shampa},
Title = {Austerity and Growth in Europe: Germany's Impossible Mission},
Journal = {POLITICKA MISAO-CROATIAN POLITICAL SCIENCE REVIEW},
Year = {2016},
Volume = {53},
Number = {4},
Pages = {7-29},
Month = {MAR},
Abstract = {This article provides a critique of neo-liberal, supply-side austerity
   policies as they unfold asymmetrically in the EU/Euro-zone and beyond.
   The main argument advanced is that, contrary to claims by the European
   Commission and Germany's elite, growth and austerity are incompatible
   policy magnitudes. The Euro-zone constitutes the worst form of Gold
   Standard from which countries cannot escape and advance the imperative
   of domestic growth as external devaluation and import-substitution are
   no-options. The article shows neo-liberal austerity is currently being
   implemented via authoritarian forms of governance from above while
   cultivating racist and xenophobic movements from below.},
