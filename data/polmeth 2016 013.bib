@article{ WOS:000395217200018,
Author = {Osterberg-Kaufmann, Norma},
Title = {``How People View Democracy{''}: Measurement problems and possible
   alternatives},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {343+},
Abstract = {The following article discusses methodological challenges in surveying
   support for democracy in countries beyond the West. Considering the
   traveling premise of this book, a universal understanding of democracy
   cannot be assumed. As traditional methods mostly ignore the variety of
   how people view democracy worldwide, they produce data with limited
   meaning, especially for countries beyond the West. The article reviews
   the empirical and methodological literature on the understanding of
   democracy and discusses methodologically innovative and differentiated
   survey methods. As a continuation of this relatively `young' debate, the
   article presents the Repertory Grid-Method as an approach to-survey
   abstract concepts and terms like democracy and illustrates this method
   with an interview example.},
