@article{ WOS:000383107900003,
Author = {Johnson, Janet Elise},
Title = {Fast-Tracked or Boxed In? Informal Politics, Gender, and Women's
   Representation in Putin's Russia},
Journal = {PERSPECTIVES ON POLITICS},
