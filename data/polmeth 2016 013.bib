@article{ WOS:000380925600001,
Author = {Cayton, Adam},
Title = {Why Are Some Institutions Replaced while Others Persist? Evidence from
   State Constitutions},
Journal = {STATE POLITICS \& POLICY QUARTERLY},
Year = {2016},
Volume = {16},
Number = {3},
Pages = {267-289},
Month = {SEP},
Abstract = {Why are some institutions quickly replaced while others endure for more
   than a century? Majority cycling over institutions is theoretically
   unavoidable, but politics provides few opportunities to study it
   empirically. Using data on state constitutional characteristics and
   legislative composition from 1834 to 2012, this article advances the
   theory that institutions are more likely to be endogenously replaced
   when the society of actors differs from the time of enactment, and that
   institutional characteristics can exacerbate or mitigate these risks.
   Results show that political change interacts with institutional
   particularism to preserve or undermine state constitutions. More
   particularistic constitutions have shorter life spans because they are
   more vulnerable to changes in the political environment.},
