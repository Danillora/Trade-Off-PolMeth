@article{ WOS:000407676600008,
Author = {Puma Crespo, Jorge Ivan},
Title = {Maoists of Northern Mexico: A Short History of Politica Popular-Linea
   Proletaria, 1969-1979},
Journal = {IZQUIERDAS},
Year = {2016},
Volume = {27},
Pages = {200-229},
Month = {APR},
Abstract = {This essay studies evolution of Politica Popular, a Maoist group
   operating from 1968 to 1979. It presents its history in the context of
   the development of an alternative to armed struggle and party politics.
   The research is grounded in the testimonies of former militants,
   internal documents of the organization and a comprehensive literature
   review with a focus in regional history studies. Delving into Politica
   Popular's history we find that political context and the group fixation
   with mass line resulted in union and social activism.},
