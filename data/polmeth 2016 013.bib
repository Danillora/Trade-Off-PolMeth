Affiliation = {Ramanzini, H (Corresponding Author), Univ Fed Uberlandia, Int Relat, Uberlandia, MG, Brazil.
   Ramanzini Junior, Haroldo, Univ Fed Uberlandia, Int Relat, Uberlandia, MG, Brazil.
   Farias, Rogerio de Souza, Univ Brasilia UNB, Brasilia, DF, Brazil.},
DOI = {10.1111/lamp.12092},
ISSN = {2041-7365},
EISSN = {2041-7373},
Keywords = {Brazil; public policy; democracy; citizen participation},
Research-Areas = {Government \& Law},
Web-of-Science-Categories  = {Political Science},
