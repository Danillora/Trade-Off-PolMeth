@article{ WOS:000366281900002,
Author = {Wallace, Jeremy L.},
Title = {Juking the Stats? Authoritarian Information Problems in China},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2016},
Volume = {46},
Number = {1},
Pages = {11-29},
Month = {JAN},
Abstract = {Economic statistics inform citizens of general conditions, while central
   leaders use them to evaluate local officials. Are economic data
   systematically manipulated? After establishing discrepancies in economic
   data series cross-nationally, this article examines Chinese sub-national
   growth data. It leverages variation in the likelihood of manipulation
   over two dimensions, arguing that politically sensitive data are more
   likely to be manipulated at politically sensitive times. Gross domestic
   product (GDP) releases generate headlines, while highly correlated
   electricity production and consumption data are relatively unnoticed. In
   Chinese provinces, the difference between GDP and electricity growth
   increases in years with leadership turnover, which is consistent with
   juking the stats for political reasons. The analysis points to the
   political role of information and the limits of non-electoral
   accountability mechanisms in authoritarian regimes.},
