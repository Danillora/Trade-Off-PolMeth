@article{ WOS:000395217200011,
Author = {Holbig, Heike},
Title = {Democracy as a Translinguistic Praxis: Chinese appropriations of
   `deliberative democracy'},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {188+},
Abstract = {This article focuses on the appropriation of the concept of
   `deliberative democracy' among Chinese intellectual and political elites
   since the turn of the 21st century. Based on Lydia Liu's approach of
   `translingual practice', it examines the process of translation,
   selective adoption and gradual assertion of the concept's interpretation
   by the socialist party regime. The analysis finds that a gradual shift
   of meaning has taken place from `deliberative' to `consultative
   democracy' that is interpreted in a more vertical fashion of soliciting
   popular views from above. At the same time, this semantic shift
   constitutes specific legitimacy claims of the party-state. Against this
   backdrop, the seeming paradox of an authoritarian party regime framing
   itself as `democratic' can be resolved by looking at the ongoing
   ideological contestations among Chinese elites about regime legitimacy.},
