@article{ WOS:000384445800018,
Author = {Khosravi, Jamal and Kalhori, Jalal and Hamehmorad, Loghman},
Title = {The Presence of Israel in Iraqi Kurdistan and its Security Challenges
   for Iran's National Security},
Journal = {JOURNAL OF POLITICS AND LAW},
