@article{ WOS:000387531200005,
Author = {Formisano, Ron},
Title = {The Populist Tsunami of the Second Gilded Age},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
Year = {2016},
Volume = {14},
Number = {3},
Pages = {281-294},
Month = {OCT},
Abstract = {The candidacies of Donald Trump and Bernie Sanders typify populist
   protests launched in the name of the people going back to before the
   American Revolution. They express, however, very different populist
   impulses: Trump rode a mostly reactionary populism to the Republican
   nomination, echoing earlier reactionaries by exploiting nativist and
   racial resentments and prejudice among whites while inconsistently
   embracing progressive policies that unsettled traditional Republicans.
   Sanders launched a strong challenge for the Democratic nomination but
   with a consistently progressive populist message. Both campaigns
   appealed to their supporters by coming across as
   ``anti-establishment{''} and unrepresentative of politics-as-usual.},
