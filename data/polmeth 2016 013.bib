@article{ WOS:000395217200019,
Author = {Amirpur, Katajun},
Title = {Democracy as a circulating norm: From the West to the East and back
   again},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Number = {51},
Pages = {366+},
Abstract = {``Is democracy compatible with Islam?{''} Even if democracy could not be
   established across the entire region after the alleged Arab spring,
   there seems to be little doubt about the fact that ``Islam is able to do
   democracy{''} in the mind of the populations of Arab countries and Iran.
   Is there a reason for assuming that the widely travelled ideas of
   democracy and human rights have turned into a norm? Under the heading
   ``circulating norms{''} the article is about the discourse of democracy
   and human rights in Iran. The paper starts with a description of the
   negative and critical attitudes towards democracy taken by some
   illustrious Iranian intellectuals in the 60s and 70s. Then, hoWever, at
   the beginning of the 90s some outstanding thinkers turned towards
   democracy. Liberal thinkers such as Shabestari and Soroush grounded the
   concept of democracy in traditional views of justification. This
   indicates that democracy was apparently recognized as a norm.},
