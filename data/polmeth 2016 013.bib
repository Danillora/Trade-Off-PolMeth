@article{ WOS:000382490100020,
Author = {Coles, Romand},
Title = {Walt Whitman, Jane Bennett, and the Paradox of Antagonistic Sympathy},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2016},
Volume = {69},
Number = {3},
Pages = {621-625},
Month = {SEP},
Abstract = {This essay critically engages ontological, rhetorical, ethical, and
   political themes pertinent to the concept of sympathy as it appears in
   the poetry and prose of Walt Whitman and Jane Bennett's writing on him.
   I suggest that antagonism is immanent in the ecology of sympathies that
   Bennett theorizes, and that this partly explains why one frequently
   finds antagonistic articulations deeply intertwined with Whitman's most
   sympathetic expressions. I propose that we use the paradoxicaleven
   oxymoronic soundingtrope antagonistic sympathy to evoke this immanent
   relationship between affiliative and antagonistic flows, energies, and
   conditions for ethical and political cultivation. The concept of
   antagonistic sympathy helps us better understand Whitman, the ethical
   and political qualities, pulls, and implications of sympathy, and it
   enables us to theorize entanglements of sympathy and antagonism in ways
