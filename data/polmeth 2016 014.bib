@article{ WOS:000367170700001,
Author = {Coleman, Stephen and Moss, Giles},
Title = {Rethinking Election Debates: What Citizens Are Entitled to Expect},
Journal = {INTERNATIONAL JOURNAL OF PRESS-POLITICS},
