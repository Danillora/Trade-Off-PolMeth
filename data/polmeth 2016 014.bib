@article{ WOS:000390592200007,
Author = {Arrington, Theodore S.},
Title = {A Practical Procedure for Detecting a Partisan Gerrymander},
Journal = {ELECTION LAW JOURNAL},
Year = {2016},
Volume = {15},
Number = {4},
Pages = {385-402},
Month = {DEC},
Abstract = {The United States Supreme Court may be open to reconsidering the
   standards for judging the constitutionality of partisan gerrymanders.
   This article presents a workable criteria for determining when
   districting arrangements so distort the process of translating votes
   into seats in a legislature that the process or the redistricting plan
   rises to a constitutional violation. The procedure uses an adjusted
   normal partisan vote (ANPV) measure to determine the number of seats
   that the preferred party would receive when the vote is equally divided
   between the parties and how that distribution of seats would change as
   the ANPV is adjusted up or down. This procedure would show that a
   gerrymander is long lasting, severe, and intentional.},
Publisher = {MARY ANN LIEBERT, INC},
Address = {140 HUGUENOT STREET, 3RD FL, NEW ROCHELLE, NY 10801 USA},
Type = {Article},
Language = {English},
Affiliation = {Arrington, TS (Corresponding Author), 13015 Sandia Point Rd NE, Albuquerque, NM 87111 USA.
   Arrington, Theodore S., Univ N Carolina, Polit Sci, Charlotte, NC USA.},
DOI = {10.1089/elj.2016.0383},
ISSN = {1533-1296},
EISSN = {1557-8062},
Keywords = {redistricting; gerrymander; normal vote; seat; vote relationship; 14th
   Amendment; partisanship},
