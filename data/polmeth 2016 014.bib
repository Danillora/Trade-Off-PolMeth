@incollection{ WOS:000387122500009,
Author = {Barber, Michael},
Editor = {Jung, HY and Embree, L},
Title = {Liberation Ethics and Transcendental Phenomenology},
Booktitle = {POLITICAL PHENOMENOLOGY: ESSAYS IN MEMORY OF PETEE JUNG},
Series = {Contributions to Phenomenology},
Year = {2016},
Volume = {84},
Pages = {123-144},
Abstract = {Enrique Dussel's Etica de la Liberacion en la Edad de la Globalization y
   de la Exclusion seeks to correct the Eurocentric understanding of the
   history of philosophy, particularly modernity, and to develop an ethics
   that can play a role in transforming the present economic and political
   structures that oppress the majority of humanity. In his Etica, Dussel
   dismisses Husserlian transcendental phenomenology as an inadequate
   approach to understanding the subject in favor of Heidegger's concrete
   Being-in-the-World. Despite the seeming disconnection between Dussel and
   transcendental phenomenology, I will make use of aspects of Husserlian
   transcendental phenomenology to criticize Dussel's theory of modernity,
   his objection to formalistic ethics, and his approach to economics. I
   will show that Dussel's work could profit from explicitly relying upon
   transcendental phenomenological underpinnings and that without such
   phenomenological bases, his own work would be imperiled.},
Publisher = {SPRINGER INT PUBLISHING AG},
