@incollection{ WOS:000443931800004,
Author = {Robinson, Simon},
Book-Author = {Robinson, S},
Title = {Integrity and Accountability: Being True to Others},
Booktitle = {PRACTICE OF INTEGRITY IN BUSINESS},
Series = {Palgrave Studies in Governance Leadership and Responsibility},
Year = {2016},
Volume = {1},
Pages = {63-96},
Abstract = {This chapter sets out the meaning of accountability, arguing that it is
   central to integrity. Focusing on the case of the Mid Staffs Hospital
   Trust, it explores mutual accountability for meaning and practice and
   plural accountability (including to colleagues, profession, client,
   institution and so on). Central to this is responsibility for
   relationships, being true to them. Breakdown of the practice of this
   integrity is characterized as analogous to a breakdown of health. This
   view of accountability is critically contrasted with the corporate
   capture of accountability, where one contract is dominant. Narrow
   perspectives are then contrasted with the complexity of businesses
   relationship to the social environment, inside and outside the
   organization.
   The chapter, then, focuses on the practice of dialogue as the means of
   engaging complexity and practising accountability, illustrated by the
   example of Henry V at Agincourt, and ends by noting the connection
   between the first two modes of responsibility.},
