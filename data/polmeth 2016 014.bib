@article{ WOS:000369361500015,
Author = {Mackin, Anna E.},
Title = {Executive Constraints and Repression in Democratic Contexts: The Case of
   Land Protests in Brazil},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2016},
Volume = {69},
Number = {1},
Pages = {175-188},
Month = {MAR},
Abstract = {This paper uses Brazilian subnational panel data for 1997-2005 to
   examine how electoral and partisan pressures affect the ways in which
   state governors respond to protests over agrarian reform. The results
   are broadly consistent with the theoretical prediction that governors
   with precarious political positions are less likely to promote
   repressive policing strategies. A governor's party's share of seats in
   the state legislature is a significant positive predictor of both harsh
   and mild forms of repression. The percentage of votes attained in the
   first round of the previous election is an additional significant
   positive predictor of mild repression only.},
