@article{ WOS:000380088500005,
Author = {Woniak, Katarzyna},
Title = {Local Remembrance of Nazi Forced Labour},
Journal = {OSTEUROPA},
Year = {2016},
Volume = {66},
Number = {3},
Pages = {51+},
Abstract = {For decades, the issue of forced labour played a subordinate role in
   German confrontation with the Nazi past. Only in the 1990s did it begin
   to make its way into the public consciousness. The impetus for this
   often came from interested laymen studying local history. In the
   Bavarian town of Affing, a seminar paper on the fate of a forced
   labourer led not only to the construction of a memorial, but also to the
   establishment of a German-Polish city partnership. To keep communal
   memory alive, however, ritual commemoration and contacts between the
   communities are not enough.},
