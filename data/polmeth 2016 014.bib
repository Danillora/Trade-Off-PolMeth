@article{ WOS:000398285800003,
Author = {Goldman, Ogen},
Title = {Do Democracies Support Violent Non-governmental Organizations Less Than
   Autocracies Do?},
Journal = {CIVIL WARS},
Year = {2016},
Volume = {18},
Number = {4},
Pages = {439-466},
Abstract = {This study draws attention to the question: Do democracies fight
   indirectly through material support to violent non-governmental
   organizations (VNGOs) which wage intrastate war against other states in
   general and against democracies in particular, or are democracies less
   warlike by proxy? The main conclusions are: democracies are less warlike
   by proxy than non-democratic states, and the more democratic the regime
   the lower the probability that it will support VNGOs waging war against
   other states, both in general and against other democracies in
   particular. The results do not unequivocally support both the monadic or
   dyadic argument of democratic peaceful behaviour},
