@article{ WOS:000390288300002,
Author = {Dormal, Michel},
Title = {Voting without Citizenship? Noncitizen Voting Rights in Democratic
   Theory},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2016},
Volume = {57},
Number = {3},
Pages = {378-402},
Abstract = {In the face of growing migration, the idea that voting rights should be
   independent of formal citizenship has become more and more attractive.
   However, critics object that the dissociation of political rights from
   formal membership in the polity undermines the foundations of democracy.
   This article examines the theoretical assumptions underpinning both
   views. While supporters of alien suffrage rely on liberal conceptions of
   equality and representation, critics draw on republican understandings
   of politics. The article concludes by sketching a third perspective,
   described as conflictive republicanism, which tries to reconcile the
   republican idea that political rights should not unnecessarily be
   dissociated from political membership with the liberal concern for
   heterogeneous interests and broad inclusion.},
Publisher = {SPRINGER},
Address = {233 SPRING ST, NEW YORK, NY 10013 USA},
