@article{ WOS:000391517700004,
Author = {Delbos, Jean-Brieux},
Title = {Once in the Elite, Always in the Elite? Changing Wealth in a Changing
   City (Paris, France, 1845-1859)},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2016},
Volume = {126},
Number = {5},
Pages = {683-719},
