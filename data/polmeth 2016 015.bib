@article{ WOS:000393284700003,
Author = {Himelein, Kristen},
Title = {Interviewer Effects in Subjective Survey Questions: Evidence From
   Timor-Leste},
Journal = {INTERNATIONAL JOURNAL OF PUBLIC OPINION RESEARCH},
Year = {2016},
Volume = {28},
Number = {4},
Pages = {511-533},
Month = {WIN},
Abstract = {In quantitative data collection, there is an inherent tension between
   traditional norms and survey protocols, particularly in the case of
   subjective or sensitive questions, which can lead to interviewer
   effects. This article makes use of a household survey in Timor-Leste to
   study these effects. In addition to their age and gender, data were
   collected from the interviewers regarding their opinions on the
   subjective questions before fieldwork. Fixed effects and mixed effects
   logit models examine the main effects and interactions between
   interviewer and respondent characteristics. The article finds
   interviewer effects in both subjective and objective data, but the
   magnitude is stronger for subjective questions. It also finds female
   respondents appear more susceptible to influence based on the
   interviewer's beliefs.},
