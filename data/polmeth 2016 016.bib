@article{ WOS:000375021600006,
Author = {Chytil, Zdenek and Maslo, Lukas},
Title = {Revision of Marc Lavoie's Monetary Model by the Endogenization of the
   Parameter γ},
Journal = {POLITICKA EKONOMIE},
Year = {2016},
Volume = {64},
Number = {2},
Pages = {209-217},
Abstract = {This paper tries to point out a fallacy in Lavoie's (2006) conclusion
   that incorporation of deep endogeneity of parameters into the PKA model
   suffices to bring about irreversibility of such a system. The arguments
   of the authors are supported by Setterfield's (1998a, 2008) concept of
   hysteresis as path-dependence in the long-run outcome of systems with
   direct shocks. The paper shows that extension of Lavoie's PKA model by
   deep endogeneity of the parameter gamma causes the fulfillment of the
   necessary condition of hysteresis Setterfield's condition (a) ensuring
   deep endogeneity of the alleged exogenous parameter and, by means of
   this, path dependence in the long-run output but not the fulfillment of
   the sufficient condition of hysteresis - condition (c) ensuring the
   presence of adjustment asymmetry and, by means of this, irreversibility.
   Thus, deep-endogenisation of the parameter gamma does not bring about
   Setterfield's hysteresis and, in effect, does not cause irreversibility.},
