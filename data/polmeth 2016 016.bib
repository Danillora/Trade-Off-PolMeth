@article{ WOS:000376795000002,
Author = {Valat, Emmanuel},
Title = {Job access inequality according to the foreign origin of workers and
   social relations: what do we learn from recent researches?},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2016},
Volume = {126},
Number = {2},
Pages = {213-256},
Month = {MAR-APR},
Abstract = {Many works, both theoretical and empirical, have recently considered the
   role of social relations in the labor market. A number of them points
   out that job access through contacts sometimes considerably varies
   according to the foreign origin of workers. Differences in the use and
   the efficacy of social relations according to workers' origin result
   from the variation of individual's socio-demographic characteristics of
   each group, from the behavior of employers and employees transmitting
   job offers as well as from some contextual factors. We propose a
   literature review on the main findings of these recent researches. When
   it is possible, we illustrate our comments through the original French
   Trajectories and Origins (2008) dataset.},
