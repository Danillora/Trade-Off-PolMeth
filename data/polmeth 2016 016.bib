@article{ WOS:000379174300006,
Author = {Burdekin, Richard C. K. and Laney, Leroy O.},
Title = {Fiscal policymaking and the central bank institutional constraint Una
   Vez Mas: New Latin American evidence},
Journal = {PUBLIC CHOICE},
Year = {2016},
Volume = {167},
Number = {3-4},
Pages = {277-289},
Month = {JUN},
Abstract = {Evidence on the degree to which central bank independence can constrain
   government budget deficits remains surprisingly scarce. This paper finds
   empirical support for the importance of the central institutional
   constraint for a 14-country sample of Latin American countries over
   1990-2012. These results suggest that greater central bank autonomy has
   indeed helped reign in fiscal excesses in a region that has been plagued
   by inflationary deficit expansion for much of the post-war period. The
   ability to potentially discipline government policy adds to an
   independent central bank's inherent role in adding to the checks and
   balances essential to a well-functioning democracy.},
