@article{ WOS:000381776100004,
Author = {Solilova, Veronika and Nerudova, Danuse},
Title = {The Proposal of Safe Harbours in the Area of Transfer Prices for Small
   and Medium Sized Enterprises},
Journal = {POLITICKA EKONOMIE},
Year = {2016},
Volume = {64},
Number = {5},
Pages = {559-572},
Abstract = {In this paper we focus on transfer pricing issues of SMEs which impose
   relatively higher compliance costs of taxation on SMEs than on large
   entities which can benefit-from economies of scale. Therefore, the
   international platforms recommend introducing the simplified transfer
   pricing approaches mainly for SMEs. Currently, 62\% of EU Member States
   have implemented simplified transfer pricing measurements for them. The
   aim of the paper is to propose the methodical tool for SMEs in the form
   of safe harbours based on the arm's length profit margins in the
   selected industry sectors in order to reflect sectoral and size-classes
   differences. Further, we try to answer on the question, how wide margin
   is possible to apply in case of distributors if the proposed methodical
   tool is used and manufacturers generate different operating costs.},
