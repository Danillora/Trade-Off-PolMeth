@article{ WOS:000407412500004,
Author = {Nikolaev, Boris and Bennett, Daniel L.},
Title = {Give me liberty and give me control: Economic freedom, control
   perceptions and the paradox of choice},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2016},
Volume = {45},
Number = {S},
Pages = {39-52},
Month = {DEC},
Abstract = {We explore the relationship between individual control perceptions and
   the degree to which a country's institutions and policies are consistent
   with the principles of economic freedom. Using data from the World
   Values Surveys (WVS) and the Economic Freedom of the World (EFW) index,
   we find that people living in more economically free countries are more
   likely to perceive greater control over their lives. This effect is not
   diminishing at higher levels of economic freedom. One possible channel
   that explains this relationship is the perception of procedural fairness
   and social mobility. Decomposing the EFW index, we further find that the
   area of sound money is what drives the results. (C) 2016 The Authors.
   Published by Elsevier B.V.},
Publisher = {ELSEVIER SCIENCE INC},
Address = {STE 800, 230 PARK AVE, NEW YORK, NY 10169 USA},
Type = {Article},
