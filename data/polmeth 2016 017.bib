@article{ WOS:000379559800001,
Author = {Chortareas, Georgios and Logothetis, Vasileios and Papandreou, Andreas
   A.},
Title = {Political budget cycles and reelection prospects in Greece's
   municipalities},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2016},
Volume = {43},
Pages = {1-13},
Month = {JUN},
Abstract = {This paper considers the presence of political budget cycles in Greece's
   municipalities. We construct a new dataset from primary sources and we
   find strong evidence of pre-electoral manipulation through increased
   expenditures and excessive borrowing. We use a dynamic panel data
   approach producing evidence of opportunistic behavior in local
   government finances. Our results are robust in the face of a series of
   controls including mayors running for reelection, their political
   alignment with the central government, and prolonged terms. Moreover,
   the results are robust to the exclusion of small sized municipalities
   and to the restriction of the time range of our investigation to the
   post-Maastricht period. We also consider whether opportunistic policies
   influence incumbents' reelection prospects finding that increased
   expenditures and election year opportunistic excesses are electorally
   rewarding. Our findings provide a characterization of opportunistic
   public finance management in Greek municipalities where electorally
   motivated budgetary decisions appear impervious to the various municipal
   reform attempts. (C) 2016 Elsevier B.V. All rights reserved.},
