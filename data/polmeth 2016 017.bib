@article{ WOS:000379174300004,
Author = {Heckelman, Jac C.},
Title = {A note on majority rule and neutrality with an application to state
   votes at the Constitutional Convention of 1787},
Journal = {PUBLIC CHOICE},
Year = {2016},
Volume = {167},
Number = {3-4},
Pages = {245-255},
Month = {JUN},
Abstract = {Majority rule used in the legislative process has a bias toward the
   status quo. This implies that proposals are less likely to pass when the
   number of voters casting either ``yes{''} or ``no{''} votes sums to an
   even number rather than an odd number. The implication is weakly
   supported by examining state votes of 552 motions made at the 1787
   Constitutional Convention. A difference is found in the expected
   direction but is not statistically significant at traditional levels.},
