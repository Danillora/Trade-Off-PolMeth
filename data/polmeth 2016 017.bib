@article{ WOS:000390592200004,
Author = {Chamberlain, Adam and Klarner, Carl},
Title = {Spoilers? Evaluating the Logic Behind Partisan Disaffiliation
   Requirements for Independent and Third-Party Candidates},
Journal = {ELECTION LAW JOURNAL},
