@article{ WOS:000390519800006,
Author = {Lafrance, Xavier and Sears, Alan},
Title = {Beating Time in the Slow Movements: Bensaid's Revolutionary Rhythms},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2016},
Volume = {24},
Number = {4},
Pages = {129-149},
Abstract = {Daniel Bensa'id was prominent among the revolutionary thinkers and
   activists who emerged from the mass insurgency of the 1960s, a period in
   which anti-capitalist organisers had genuine social weight grounded in
   connections to broad layers of the working class and radical movements.
   As the neoliberal offensive developed, working-class and allied
   movements experienced crucial defeats that marginalised anti-capitalist
   theory and practice. Bensaid developed a unique theoretical analysis of
   radical mobilising during the neoliberal period, at once grounded in the
   history of revolutionary organising and audaciously open-ended in
   assessing the impact of capitalist restructuring and the employers'
   offensive. The basis of his theoretical renewal lay in a new approach to
   understanding temporality that undercut any sense of socialist
   inevitability, and a commitment to revolutionary pluralism that was
   crucially located in the political sphere.},
