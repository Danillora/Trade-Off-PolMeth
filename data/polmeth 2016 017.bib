@article{ WOS:000372408600006,
Author = {Huang, Sainan and Terra, Cristina},
Title = {Exchange Rate Populism},
Journal = {ECONOMICS \& POLITICS},
Year = {2016},
Volume = {28},
Number = {1},
Pages = {105-132},
Month = {MAR},
Abstract = {East Asian and Latin American economies present opposite exchange rate
   electoral cycles: exchange rates tend to be more depreciated before and
   appreciated after elections among East Asian economies, while the
   opposite is true in Latin America. We propose an explanation for these
   empirical findings where the driving force of the opposite exchange rate
   populism in these two regions is their difference in the relative size
   of tradable and non-tradable sectors, coupled with the distributive
   effect of exchange rates. In a setup where policy-makers differ in their
   preference bias toward non-tradable and tradable sectors, the exchange
   rate is used a noisy signal of the incumbent's type in an uncertain
   economic environment. The mechanism behind the cycle is engendered by
   the incumbent trying to signal he is median voter's type, biasing his
   policy in favor of the majority of the population before elections.},
