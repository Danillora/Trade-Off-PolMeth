@incollection{ WOS:000398574800012,
Author = {Abu Zayd, Nasr},
Editor = {Benhabib, S and Kaul, V},
Title = {The `Others' in the Qur'an: A Hermeneutical Approach},
Booktitle = {TOWARD NEW DEMOCRATIC IMAGINARIES - ISTANBUL SEMINARS ON ISLAM, CULTURE
   AND POLITICS},
Series = {Philosophy and Politics-Critical Explorations},
Year = {2016},
Volume = {2},
Pages = {97-109},
Abstract = {First, I argue for historical contextualization of the Qur'an as a given
   historical collections of discourses propagated by Muhammad as divine
   inspiration. Secondly, I argue for a distinction between the Qur'an and
   Islam, since the latter is the outcome of human efforts to construct
   their lives in accordance with what they understood to be the teachings
   of the Qur'an. The last point is to show how the role of Muhammad in his
   interaction with the communities of his time in Hijaz shaped the Qur'an.
   So, the article is organized as (1) introduction; (2) the Qur'an and
   lslam; (3) the Qur'an and history: open hermeneutics; (4) Muhammad and
   the Qur'an; (5) the divine-human communication; (6) Muhammad: the first
   recipient; (7) Muhammad in the Qur'an; and (8) the community of
   believers and the need for legal regulations; followed by (9)
   conclusion.},
