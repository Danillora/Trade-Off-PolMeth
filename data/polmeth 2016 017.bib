@article{ WOS:000371596900007,
Author = {Kokushkin, Maksim and Pettys, Alicia M.},
Title = {`Shock Therapy' as a Neo-Liberal Response to an Economic Crisis},
Journal = {TERRITORY POLITICS GOVERNANCE},
