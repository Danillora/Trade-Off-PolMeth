@article{ WOS:000386713100007,
Author = {Banda, Kevin K.},
Title = {Issue Ownership, Issue Positions, and Candidate Assessment},
Journal = {POLITICAL COMMUNICATION},
Year = {2016},
Volume = {33},
Number = {4},
Pages = {651-666},
Abstract = {I argue that citizens alter their views of candidates' ideological and
   issue positions in response to two kinds of information cues: issue
   ownership and issue position cues. Issue ownership cues associate a
   candidate with the party that owns the issue discussed by a candidate.
   Issue position cues associate a candidate with the party that is linked
   to the position that the candidate discusses. These cues can either lead
   citizens to view the candidate as more or less extremeboth in terms of
   ideological and issue position assessmentsthan that candidate's party.
   When both types of cues are present, citizens should ignore the issue
   ownership cues in favor of the easier-to-process issue position cues.
   Evidence from a survey experiment embedded in the 2010 Cooperative
   Congressional Election Study provides strong support for this theory and
   suggests that issue ownership can convey positional information.},
Publisher = {TAYLOR \& FRANCIS INC},
