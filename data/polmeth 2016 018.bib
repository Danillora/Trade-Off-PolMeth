@article{ WOS:000399178200004,
Author = {Rogstad, Ingrid},
Title = {Is Twitter just rehashing? Intermedia agenda setting between Twitter and
   mainstream media},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2016},
Volume = {13},
Number = {2},
Pages = {142-158},
Abstract = {This paper investigates intermedia agenda setting between old and new
   media platforms through a study of the Norwegian Twitter and mainstream
   media agendas. The study tracks top tweets and headline news, asking
   whether Twitter mainly rehashes mainstream media content or actually
   produces original content. The study finds that Twitter and mainstream
   media are consistent on the salience of many issues, but also that
   Twitter gives attention to issues that are overlooked by mainstream
   media, such as news about environmental challenges and gender equality.
   The paper also suggests that Twitter contributes to an expansion of the
   elite, meaning that it has become an important platform for eloquent and
   media-savvy people outside the traditional political, economic, or
   academic elites.},
