@article{ WOS:000383705000003,
Author = {Zhao, Xinyan},
Title = {Effects of Perceived Media Diversity and Media Reliance on Public
   Opinion Expression},
Journal = {INTERNATIONAL JOURNAL OF PUBLIC OPINION RESEARCH},
Year = {2016},
Volume = {28},
Number = {3},
Pages = {355-375},
Month = {FAL},
Abstract = {This study investigates the effects of different types of perceived
   media diversity and media reliance on individuals' public opinion
   expressions in China. Using data from a survey of 327 undergraduates in
   Shanghai, it was found that: (1) respondents perceived more diverse
   media contents on the Internet than on the traditional media; (2)
   people's perceived Internet diversity directly predicted their opinion
   expression in public; (3) people's perceived traditional media diversity
   moderated the relationship between opinion congruency with the majority
   position and opinion expression in public; and (4) people's reliance on
   newspapers, rather than their reliance on TV or the Internet, predicted
   their opinion expression in public. These findings suggest that people's
   perceptions (but not use) of the Internet may promote public
   deliberation in an authoritative country.},
