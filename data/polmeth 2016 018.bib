@article{ WOS:000383022500003,
Author = {Heuer, Jennifer Ngaire},
Title = {Celibacy, Courage, and Hungry Wives: Debating Military Marriage and
   Citizenship in Pre-Revolutionary France},
Journal = {EUROPEAN HISTORY QUARTERLY},
Year = {2016},
Volume = {46},
Number = {4},
Pages = {647-667},
Month = {OCT},
Abstract = {Enlightenment reformers in France had very different visions about
   whether military men should marry. In one view, acting as a good soldier
   and citizen drew on new models of masculine virility, and required
   separating oneself entirely from the restraints of domestic life.
   Conversely, being a soldier-citizen could mean building on changing
   ideas of domestic virtue and respectability, combining a patriotic
   obligation to fight with responsibility to family. The marital status of
   soldiers provided a potent flash point for negotiating these tensions,
   while also engaging with pragmatic questions of providing for
   dependents, promoting military efficiency, and reinforcing population
   and state power.},
