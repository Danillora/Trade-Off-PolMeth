
@article{ WOS:000371476000012,
Author = {Doss, Erika},
Title = {Commemorating Disaster and Disobedience: National Park Service
   Initiatives in the 21st Century},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2016},
Volume = {97},
Number = {1, SI},
Pages = {105-114},
Month = {MAR},
Abstract = {ObjectiveThis essay explores the ways in which memorial culture reflects
   contemporary attitudes about the United States and its history.
   MethodAn examination of the role of the U.S. National Park Service,
   which manages and maintains many American memorials, illustrates the
   evolving relationship between official memory and history. Memorials to
   dissent and disaster are considered as part of an emerging trend in
   American memorial culture in which trauma, conflict, and loss are
   commemorated.
   ResultsAmerican memorial culture is increasingly driven by ethical
   imperatives to remember those believed to be forgotten or ignored in
   history.},
