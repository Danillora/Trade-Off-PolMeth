@article{ WOS:000392733900005,
Author = {Artola Blanco, Miguel},
Title = {THE ARISTOCRATIC IMAGINARY DURING THE SECOND REPUBLIC AND EARLY
   FRANCOISM (1931-1950)},
Journal = {HISTORIA Y POLITICA},
Year = {2016},
Number = {36},
Pages = {103-127},
Month = {JUL-DEC},
Abstract = {Since the fall of the monarchy in 1931 and until the late 1940s, the
   Spanish aristocracy actively participated in monarchist circles that
   sought to restore the Bourbon dynasty on the throne. As part of this
   policy, the nobility articulated a coherent program with their nature as
   a status group, which sought to preserve the old liberal order,
   safeguard elites versus mass society and eliminate the double menace of
   democracy and ``social demagogy{''}. However, aristocratic politics went
   beyond a simple counter revolutionary formula and saw in corporatism a
   perfect solution to bring harmony among social actors.},
