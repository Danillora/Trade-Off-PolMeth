@article{ WOS:000379174300002,
Author = {Fauvet, Pierre and Rouillon, Sebastien},
Title = {Would you trust lobbies?},
Journal = {PUBLIC CHOICE},
Year = {2016},
Volume = {167},
Number = {3-4},
Pages = {201-219},
Month = {JUN},
Abstract = {We consider the regulatory problem to approve or to ban a new
   product/technology in the context of scientific controversy about its
   detrimental environmental and/or health effects. We formalize the
   regulator's decision-making process as a Tullock contest (Towards a
   Theory of the Rent-Seeking Society, Texas A\&M University Press, Texas,
   1980), the contestants being an industrial lobby, representing the
   economic agents who have developed the new product/technology, and an
   environmental lobby, representing the economic agents who will be harmed
   by the product or technology. Assuming that the industrial lobby has
   private information about the unfavorable environmental and/or health
   effects, but can be held liable for damage ex post, we derive the
   equilibrium properties of the contest. In particular, we derive
   conditions under which it is socially preferable for the regulator to
   decide according to the contest's outcome, rather than according to an
   ex ante cost-benefit analysis, using his prior beliefs. We find that the
   contest outperforms the ex ante cost-benefit analysis only if the risk
   of judgment-proofness is not too high.},
