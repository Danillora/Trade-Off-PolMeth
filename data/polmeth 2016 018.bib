@article{ WOS:000371307300001,
Author = {Binkley, John},
Title = {Clausewitz and Subjective Civilian Control: An Analysis of Clausewitz's
   Views on the Role of the Military Advisor in the Development of National
   Policy},
Journal = {ARMED FORCES \& SOCIETY},
Year = {2016},
Volume = {42},
Number = {2},
Pages = {251-275},
Month = {APR},
Abstract = {This article focuses on Carl von Clausewitz's ideas regarding
   civil-military relations and in particular how those ideas relate to
   Samuel Huntington's models of objective and subjective civilian control.
   Huntington believed that Clausewitz supplied the foundation for his
   concept of objective control. Yet an examination of Clausewitz's own
   experiences, as well as his theoretical writings, rejects the basic
   tenants of objective control: a politically neutral military, the
   separation of political from military considerations during the
   professional officer's analytical processes, and the reliance on the
   professional military as opposed to the citizen soldier. Instead,
   Clausewitz embraced something similar to Huntington's concept of
   subjective control and with it a fusionist model of civil-military
   relations.},
