@article{ WOS:000395354100007,
Author = {Binet, Marie-Estelle and Carlevaro, Fabrizio and Paul, Michel},
Title = {The Demand for Potable Water in Reunion Island: Estimate with Survey
   Data},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2016},
Volume = {126},
Number = {1},
Pages = {155-191},
Month = {JAN-FEB},
Abstract = {Using data from a household survey carried out in the French overseas
   territory of Reunion, we specify and estimate, in this paper, a micro
   econometric demand function for potable water with the purpose of
   identifying and quantifying the impact of the determinants of household
   consumption of this resource which is presently under heavy operating
   stress. We model this demand by means of a generalization of the linear
   expenditure system allowing to differentiate within the residential
   water consumption a captive component independent of prices and
   household income, and a variable component as a function of prices and
   household income, which can be influenced by a relevant water pricing
   policy. Our estimates carried out by taking into account possible
   selection bias reveal the importance of captive consumption resulting
   both from household basic needs and from the outdoor uses of water.
   Therefore, it is advisable to complement pricing policies with other
   measures, such as information campaigns focusing on the outdoor uses of
   water.},
