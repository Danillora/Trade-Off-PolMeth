@article{ WOS:000380947300001,
Author = {Baderin, Alice},
Title = {Political theory and public opinion: Against democratic restraint},
Journal = {POLITICS PHILOSOPHY \& ECONOMICS},
Year = {2016},
Volume = {15},
Number = {3},
Pages = {209-233},
Month = {AUG},
Abstract = {How should political theorists go about their work if they are
   democrats? Given their democratic commitments, should they develop
   theories that are responsive to the views and concerns of their fellow
   citizens at large? Is there a balance to be struck, within political
   theory, between truth seeking and democratic responsiveness? The article
   addresses this question about the relationship between political theory,
   public opinion and democracy. I criticize the way in which some
   political theorists have appealed to the value of democratic legitimacy
   in an attempt to justify a more opinion-sensitive approach to their
   work. Specifically, I identify a problematic model in the existing
   literature, which I term democratic restraint': an approach on which the
   theorist moderates her normative principles in response to evidence
   about public attitudes in order to enhance the legitimacy of her
   account. This model renders the discipline newly vulnerable to an
   otherwise misguided objection that political theory seeks to pre-empt
   democratic politics. I trace the problem with the democratic restraint
   model to its flawed underlying conception of democratic legitimacy. The
   article then outlines a more appealing democratic underlabourer' view of
   the status of political theory and draws out the implications of this
   alternative account for the role of public opinion.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Baderin, A (Corresponding Author), Univ Oxford, Nuffield Coll, New Rd, Oxford OX1 1NF, England.
   Baderin, Alice, Univ Oxford, Nuffield Coll, Polit, Oxford OX1 2JD, England.},
DOI = {10.1177/1470594X15621044},
ISSN = {1470-594X},
EISSN = {1741-3060},
Keywords = {democracy; democratic legitimacy; democratic restraint; democratic
   underlabourer; public opinion; political theory},
Keywords-Plus = {DELIBERATIVE DEMOCRACY; JUSTIFICATION; PHILOSOPHY; LISTS},
Research-Areas = {Social Sciences - Other Topics; Government \& Law},
