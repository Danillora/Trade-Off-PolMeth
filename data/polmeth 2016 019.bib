@article{ WOS:000383390200003,
Author = {Cushion, Stephen and Thomas, Richard and Kilby, Allaina and Morani,
   Marina and Sambrook, Richard},
Title = {Interpreting the Media Logic behind Editorial Decisions: Television News
   Coverage of the 2015 UK General Election Campaign},
Journal = {INTERNATIONAL JOURNAL OF PRESS-POLITICS},
Year = {2016},
Volume = {21},
Number = {4},
Pages = {472-489},
Month = {OCT},
Abstract = {This article enters into debates about media logic in political coverage
   by way of a case study of the 2015 U.K. General Election. We
   quantitatively and qualitatively examine two dominant themes of
   coveragenews about campaign rallies and horse-race reportingas both are
   widely seen in political communication scholarship as symptomatic of a
   media logic. We draw on a content analysis of BBC, ITV, Sky News,
   Channel 4, and Channel 5 U.K. national television newscasts and
   semi-structured interviews with their heads of news and/or senior
   editors to help interpret how far a media logic was the editorial
   driving force behind coverage. At face value, our content analysis
   appears to support the media logic thesis, with all broadcastersin
   particular commercial television newscastscovering more process than
   policy issues. But our case study questions the antecedents of media
   logic and shines a light on a political logic that may have remained in
   the dark in large-scale content analysis studies. In following a
   political logic, we argue that this promoted the horse-race narrative,
   and naturalized the parties' highly stage-managed rallies and
   walkabouts.},
