@incollection{ WOS:000449144600019,
Author = {Abresch, William},
Editor = {Cerna, CM},
Title = {A Human Rights Law of Internal Armed Conflict: The European Court of
   Human Rights in Chechnya},
Booktitle = {REGIONAL HUMAN RIGHTS SYSTEMS, VOL V},
Series = {Library of Essays on International Human Rights},
Year = {2016},
Pages = {351-377},
Abstract = {The cases on Chechnya recently decided by the European Court of Human
   Rights force us to re-evaluate the relationship between human rights law
   and humanitarian law. Since the International Court of lustice held that
   humanitarian law is lex specialis to human rights law in 1996 - if not
   since the Tehran Conference of 1968 - it has been widely accepted that
   `human rights in armed conflictf refers to humanitarian law. The ECtHR
   has directly applied human rights law to the conduct of hostilities in
   internal armed conflicts. The rules it has applied may prove
   controversial, but humanitarian law's limited substantive scope and poor
   record of achieving compliance in internal armed conflicts suggest the
   importance of this new approach.},
