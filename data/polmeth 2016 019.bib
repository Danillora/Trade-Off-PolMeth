@article{ WOS:000385516900002,
Author = {Day, Creina},
Title = {Non-Scale Endogenous Growth with R\&D and Human Capital},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2016},
Volume = {63},
Number = {5},
Pages = {443-467},
Month = {NOV},
Abstract = {This paper examines the conditions under which increasing knowledge,
   encapsulated in ideas for new technology through R\&D and embodied in
   human capital through education, sustains economic growth. A general
   model is developed where, consistent with recent literature, growth is
   non-scale (not increasing in population size) and endogenous (generated
   by factors within R\&D and education). Recent models feature the
   counterfactual assumption of constant returns to existing knowledge and
   restrict the substitutability of inputs within R\&D and education. We
   find that non-scale endogenous growth is possible under less stringent
   conditions. The findings reconcile sustained economic growth with
   evidence of diminishing marginal returns in education and R\&D, which
   suggests an ambiguous role for R\&D policy.},
