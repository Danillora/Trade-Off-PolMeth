@article{ WOS:000384955900006,
Author = {Le Bris, David},
Title = {What Governance for Real Estate banking? The Credit Foncier de France
   during the 19th century},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2016},
Volume = {126},
Number = {4},
Pages = {593-616},
Month = {JUL},
Abstract = {The Foncier benefits from unusual governance, which is a source of
   inspiration since it has amply fulfilled its real-estate financing
   mission. The strong agency conflict between private shareholders and
   state-appointed managers is resolved by a highly involved board and the
   need to buy a substantial number of shares held as guarantee for good
   management. Conflicts between bond holders and both managers and
   shareholders about the risk level taken by the bank are resolved by the
   legal prohibition on issuing bonds without quality loans as collateral,
   monthly publication of its balance sheet and imposition of a ``Basel
   ratio{''} of 5\%.},
