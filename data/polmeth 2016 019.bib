@article{ WOS:000390519800004,
Author = {Maria Antentas, Josep},
Title = {Daniel Bensaid, Melancholic Strategist},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2016},
Volume = {24},
Number = {4},
Pages = {51-106},
Abstract = {Daniel Bensaid was a Marxist philosopher and author of an extensive body
   of works about political strategy. His writings combine a diversity of
   singular influences, such as Marx, Lenin, Trotsky and Che Guevara on the
   one hand, and Benjamin, Peguy and Blanqui on the other. In his work,
   religious heresies, Marranos, moles and emblematic figures of the
   resistance to oppression such as Joan of Arc meet with the classic
   figures of Marxism. The non-linear concept of time and messianic reason
   support a strategic reading of history and an understanding of political
   commitment, following Goldmann's interpretation of Pascal's Wager as a
   wager of uncertain outcome.},
