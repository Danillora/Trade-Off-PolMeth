@article{ WOS:000377779000004,
Author = {Layman, Geoffrey C. and Weaver, Christopher L.},
Title = {Religion and Secularism among American Party Activists},
Journal = {POLITICS AND RELIGION},
Year = {2016},
Volume = {9},
Number = {2},
Pages = {271-295},
Month = {JUN},
Note = {Symposium on Politics of Religious Alliances, Rothermere Amer Inst,
   Oxford, ENGLAND, MAR, 2015},
Organization = {Oxford Univ},
Abstract = {Prior work has shown party activists and religious divisions to be two
   of the leading causes of party polarization in American politics. Using
   the Convention Delegate Studies, we examine the interaction between
   these two culprits and their impact on party polarization. We leverage a
   novel measure of secularism in the latest wave of the Convention
   Delegate Studies to demonstrate that active secularism is distinct both
   conceptually and statistically from low religiosity. Furthermore, we
   show that both religiosity and secularism drive party activists to take
   more extreme policy positions, to identify themselves as more
   ideologically extreme, and to exhibit less support for compromise. As
   the Democratic and Republican Parties have become more secular and
   religious, respectively, these results suggest religious polarization
   may compound existing divisions between the two parties and exacerbate
   the partisan divide in American politics.},
