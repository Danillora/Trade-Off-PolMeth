@article{ WOS:000377829100001,
Author = {Pan, Wei-Hwa and Xuan-Thang Ngo},
Title = {Endogenous growth theory and regional performance: The moderating
   effects of special economic zones},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2016},
Volume = {49},
Number = {2},
Pages = {113-122},
Month = {JUN},
Abstract = {Previous studies have explored regional performance from the perspective
   of endogenous growth theory. This empirical investigation is conducted
   on a panel dataset of 64 Vietnamese provinces and integrates moderated
   regression analysis. Statistically, the empirical results did not fully
   support the endogenous growth model in cases in which regional per
   capita income tended to converge across different regions, while
   improved foreign direct investment (FDI) inflow, capital investment, and
   degree of openness did have significant positive impacts on GDP growth.
   This study questions the validity of endogenous growth theory in the
   early stage of a less developed country. Furthermore, results indicate
   that internationalization activities positively affect regional
   performance for provinces that have established special economic zones
   (SEZs) through liberal state regulation. (C) 2016 The Regents of the
   University of California. Published by Elsevier Ltd. All rights
   reserved.},
