@article{ WOS:000410402300006,
Author = {Strelkov, Alexander},
Title = {The EU and rule of law promotion in Western Balkans - a new role for
   candidate states' parliaments},
Journal = {EAST EUROPEAN POLITICS},
Year = {2016},
Volume = {32},
Number = {4},
Pages = {505-524},
Abstract = {The article explores the recent shift in the European Union's approach
   towards candidate states in the Western Balkans.(1) It argues that the
   European Commission has started to pay greater attention to parliaments
   in candidate states in order to promote and secure accession-related
   reforms. As a result, national parliaments in candidate states have
   greater opportunities to shape the content of these reforms, including
   those in the rule of law sector. Consequently, the article elaborates on
   the factors that could potentially affect Balkan parliaments'
   involvement in the accession process.},
