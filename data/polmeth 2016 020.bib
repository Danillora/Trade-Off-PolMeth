@article{ WOS:000390887800007,
Author = {Wagner, Wolfgang and Anholt, Rosanne},
Title = {Resilience as the EU Global Strategy's new leitmotif: pragmatic,
   problematic or promising?},
Journal = {CONTEMPORARY SECURITY POLICY},
Year = {2016},
Volume = {37},
Number = {3},
Pages = {414-430},
Abstract = {A striking difference between the EU's 2016 Global Strategy and its 2003
   predecessor is the ubiquity of resilience as a new leitmotif, understood
   as the ability of states and societies to reform, thus withstanding and
   recovering from internal and external crisis. Resilience provides a
   middle ground between over-ambitious liberal peace-building and
   under-ambitious stability, (re) directs attention to local resources and
   practices, and is ambiguous enough to be acceptable to everyone. The
   Global Strategy's leitmotif is an example of the rise and spread of
   resilience in international discourses about crisis management and
   humanitarian emergencies. Although there are risks inherent to the way
   in which resilience reframes risks and crises, its added value lies in
   its power as convening concept, opening up international organizations
   to new ways of thinking and working, and providing a common ground for
   engagement.},
