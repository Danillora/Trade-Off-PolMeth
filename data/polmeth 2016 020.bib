@article{ WOS:000387583300010,
Author = {Clayton, Govinda and Thomson, Andrew},
Title = {Civilianizing Civil Conflict: Civilian Defense Militias and the Logic of
   Violence in Intrastate Conflict},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
Year = {2016},
Volume = {60},
Number = {3},
Pages = {499-510},
Month = {SEP},
Abstract = {This article examines how civilian defense militias shape violence
   during civil war. We define civilian defense forces as a sedentary and
   defensive form of pro-government militia that incumbents often use to
   harness the participation of civilians during a counterinsurgency
   campaign. We argue that civilian defense forces reduce the problem of
   insurgent identification. This leads to a reduction in state violence
   against civilians. However, we also claim that these actors undermine
   civilian support for insurgents, which leads to an increase in rebel
   violence against civilians and overall intensification of conflict. A
   statistical analysis of government and rebel violence against civilians
   from 1981 to 2005 and a qualitative assessment of a civilian defense
   force operating in Iraq from 2005 to 2009 offer strong support for our
   theoretical claims. These findings provide further insight into
   pro-government militias and their effects on violence. They also have
   wider ethical implications for the use of civilian collaborators during
   civil war.},
