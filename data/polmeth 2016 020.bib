@article{ WOS:000382765900006,
Author = {Ross, Karen},
Title = {Peacebuilding through inter-group encounters: rethinking the
   contributions of `mainstream' and `politicised' approaches},
Journal = {PEACEBUILDING},
Year = {2016},
Volume = {4},
Number = {3},
Pages = {317-330},
Abstract = {Scholars have long examined the role inter-group encounters can play in
   contributing to the creation of more peaceful and just societies in
   existing conflict contexts. Within Israel, these encounters provide a
   unique opportunity for Jewish and Palestinian citizens to counteract the
   segregation present in most aspects of their lives and to interact in a
   positive manner. Yet despite an abundance of scholarship on these
   programmes, it remains unclear how different approaches used in
   encounters between Jewish and Palestinian youth matter in contributing
   to societal peacebuilding. This manuscript focuses on approaches
   utilised by two veteran Israeli encounter organisations to highlight the
   benefits and drawbacks of different approaches, suggesting that both
   have important roles to play in building peace.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
