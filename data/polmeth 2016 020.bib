@article{ WOS:000372406900005,
Author = {Dafoe, Allan and Caughey, Devin},
Title = {HONOR AND WAR Southern US Presidents and the Effects of Concern for
   Reputation},
Journal = {WORLD POLITICS},
Year = {2016},
