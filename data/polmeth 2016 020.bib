@article{ WOS:000378876000007,
Author = {Hicks, Kathryn and Fabricant, Nicole},
Title = {The Bolivian Climate Justice Movement: Mobilizing Indigeneity in Climate
   Change Negotiations},
Journal = {LATIN AMERICAN PERSPECTIVES},
