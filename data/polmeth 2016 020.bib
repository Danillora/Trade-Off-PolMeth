@incollection{ WOS:000557691200005,
Author = {Priebus, Sonja},
Editor = {Fruhstorfer, A and Hein, M},
Title = {Hungary},
Booktitle = {CONSTITUTIONAL POLITICS IN CENTRAL AND EASTERN EUROPE: FROM
   POST-SOCIALIST TRANSITION TO THE REFORM OF POLITICAL SYSTEMS},
Series = {Vergleichende Politikwissenschaft},
Year = {2016},
Pages = {101-143},
Abstract = {Constitutional politics in Hungary has been very eventful throughout the
   previous 25 years. The reformed constitution of 1989/1990 had been
   revised 31 times from 1991 until 2011. After an unsuccessful attempt at
   constitution-making in the mid-1990s, the old constitution was finally
   replaced in 2012 by the `Fundamental Law', which was crafted
   unilaterally by the conservative Fidesz-KDNP majority. This constitution
   had already been amended five times by the end of 2014. The year 2010,
   when the alliance of the Fidesz and the KDNP came to power, marked a
   clear paradigm shift in constitutional politics. Before 2010, actors
   played by the rules set by the constitution. After 2010, the ruling
   majority increasingly played with the rules, thus destabilizing
   democracy in Hungary.},
