@article{ WOS:000381804800002,
Author = {O'Leary, Brendan},
Title = {Power-Sharing and Partition amid Israel-Palestine},
Journal = {ETHNOPOLITICS},
Year = {2016},
Volume = {15},
Number = {4, SI},
Pages = {345-365},
Abstract = {This paper reviews the deep difficulties entailed in achieving a
   two-state solution to the conflict between Israelis and Palestinians. It
   shows that there are equally deep difficulties in seeking to achieve
   standard power-sharing prescriptions in the region. The status quo,
   however, is neither acceptable nor likely to be stable in the medium
   term. A power-sharing perspective may, however, productively assist in
   making a two-state solution work better, and, paradoxically, may also
   assist the credibility of those who advocate a one-state solution.},
