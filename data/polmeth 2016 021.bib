@article{ WOS:000392925300016,
Author = {Li, Xiaojun and Wang, Jianwei and Chen, Dingding},
Title = {Chinese Citizens' Trust in Japan and South Korea: Findings from a
   Four-City Survey},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
Year = {2016},
Volume = {60},
Number = {4},
Pages = {778-789},
Month = {DEC},
Abstract = {Many observers see mutual distrust as a major reason for increasing
   tensions in East Asia, but few studies systematically explore exactly
   how trust, or the lack of it, influences international cooperation in
   the region. We examine the microfoundations of international trust in
   East Asia by analyzing an original survey of Chinese urban residents'
   attitudes toward Japan and South Korea. We find that generalized trust,
   or the belief that other nations have benign intentions, provides the
   most important driving force of trust toward Japan and South Korea.
   This, in turn, has a positive effect on preferences for interstate
   cooperation. We also find that, contrary to conventional wisdom,
   nationalism and historical memory have little or no effect on
   international trust. Our analysis enhances our broader understanding of
   international trust by clarifying its sources and consequences. It also
   suggests some important policy implications for Sino-Japanese relations
   and Chinese foreign policy.},
