@article{ WOS:000387583300004,
Author = {de Mesquita, Bruce Bueno and Smith, Alastair},
Title = {Competition and Collaboration in Aid-for-Policy Deals},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
Year = {2016},
Volume = {60},
Number = {3},
Pages = {413-426},
Month = {SEP},
Abstract = {Despite the extensive empirical and theoretical research into foreign
   aid, there remains little or no formal analysis of aid giving in a
   competitive donor environment. We endeavor to fill this lacuna with both
   a model and empirical analysis of aid-for-policy deals with rival aid
   donors. The model indicates that a dominant donor captures all the
   surplus from any deal. We test several hypotheses that follow from the
   model. We demonstrate that the United States paid less (in constant
   dollars) and gained more in policy terms through aid before the Soviet
   Union became a significant aid player. Once the Soviet Union became a
   player in the international aid arena, the United States paid more for
   aid and got less by way of security concessions from recipients.},
Publisher = {OXFORD UNIV PRESS},
