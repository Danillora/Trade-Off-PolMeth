@incollection{ WOS:000448172300014,
Author = {Kramer, Matthew H. and Steiner, Hillel},
Editor = {Bix, BH and Spector, H},
Title = {Theories of Rights: Is There a Third Way?},
Booktitle = {RIGHTS: CONCEPTS AND CONTEXTS},
Series = {International Library of Essays on Rights},
Year = {2016},
Pages = {243-272},
Abstract = {Some important recent articles, including one in this journal, have
   sought to devise theories of rights that can transcend the longstanding
   debate between the Interest Theory and the Will Theory. The present
   essay argues that those efforts fail and that the Interest Theory and
   the Will Theory withstand the criticisms that have been levelled against
   them. To be sure, the criticisms have been valuable in that they have
   prompted the amplification and clarification of the two dominant
   theories of rights; but their upshot has been to reveal the need for the
   improvement, rather than the abandonment, of those theories.},
