@article{ WOS:000381707300009,
Author = {Lequesne, Christian},
Title = {French foreign and security challenges after the Paris terrorist attacks},
Journal = {CONTEMPORARY SECURITY POLICY},
Year = {2016},
Volume = {37},
Number = {2},
Pages = {306-318},
Abstract = {The Paris terrorist attacks in January and November 2015 have changed
   the relationship between French society and security. For the first time
   since the end of the Second World War, the assumption that France is
   experiencing a new form of territorial war is explicit in the public
   debate. It has reinforced the strong conviction among the French
   politicians and diplomats that security requires close cooperation with
   the USA and a renouncement of the Gaullist paradigm of exceptionalism.
   This paper analyses why the terrorist attacks have been perceived in
   France as a form of territorial war. Second, it explains why terrorism
   contributes to a growing mistrust of the French public vis-a-vis the
   European Union. Finally, it shows the reasons but also the limits of
   French military activism outside Europe, in close connection with the
   US-led strategy.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
