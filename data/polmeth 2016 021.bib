@article{ WOS:000367635000004,
Author = {Onderco, Michal},
Title = {The provision of private goods and the emergence of armed rebellion: the
   case of the Slovak National Uprising 1944-1945},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2016},
Volume = {19},
Number = {1},
Pages = {76-100},
Month = {JAN},
Abstract = {Can authoritarian leaders maintain support for their rule by providing
   private goods to selected individuals? Current theories disagree
   regarding this. While the selectorate theory argues that, in
   undemocratic regimes, leaders should provide private goods selectively
   to remain in office, civil war research suggests the opposite. Through a
   case study of the Slovak National Uprising, using both qualitative and
   quantitative evidence, this paper shows that the provision of private
   goods may fuel resentment against the regime and thus increase the risk
   of armed rebellion. This finding suggests that reliance on the
   selectorate theory may be fatal for regimes.},
Publisher = {PALGRAVE MACMILLAN LTD},
