@article{ WOS:000383250500009,
Author = {Mkodzongi, Grasian},
Title = {``I am a paramount chief, this land belongs to my ancestors': the
   reconfiguration of rural authority after Zimbabwe's land reforms},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2016},
Volume = {43},
Number = {1, SI},
Pages = {99-114},
Month = {SEP},
Abstract = {This article explores the reconfiguration of rural authority in the
   aftermath of Zimbabwe's Fast Track Land Reform Programme, particularly
   the way the programme has allowed local chiefs to deploy ancestral
   autochthony as a way of contesting state hegemony over the countryside.
   It argues that chiefs cannot simply be viewed as undemocratic remnants
   of colonial rule; instead, a nuanced understanding of their role in
   rural governance is required.},
