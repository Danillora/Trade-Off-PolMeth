@article{ WOS:000382765900004,
Author = {Schuck, Christoph},
Title = {Peacebuilding through Militant Islamist Disengagement: conclusions drawn
   from the case of Al-Gama'a al-Islamiyya in Egypt},
Journal = {PEACEBUILDING},
Year = {2016},
Volume = {4},
Number = {3},
Pages = {282-296},
Abstract = {This article analyses the Militant Islamist Disengagement of Egypt's
   Al-Gama'a al-Islamiyya while anchoring the disengagement topic in the
   discourse on peacebuilding and security. In this context, we will first
   examine external and internal factors that destabilised the Gama'a to
   such an extent in the mid-1990s that the group was forced to find a new
   orientation in order to survive. Secondly, we will pay particular
   attention to the catalyst that finally led the group to reject violence
   after its destabilisation rather than intensifying its use of force.
   Here, we will attempt to establish whether the taqiyya principle might
   have played a role in the new orientation of the Gama'a. Third, we will
   systematically draw some conclusions, connecting them to current
   developments in Egypt. Here, the central question must be whether the
   extremely tense situation with regard to Islamist groups can be expected
   to lead the Gama'a to return to its former violent path.},
