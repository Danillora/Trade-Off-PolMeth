@article{ WOS:000387362300001,
Author = {Neville, Kate J. and Weinthal, Erika},
Title = {Mitigating Mistrust? Participation and Expertise in Hydraulic Fracturing
   Governance},
Journal = {REVIEW OF POLICY RESEARCH},
