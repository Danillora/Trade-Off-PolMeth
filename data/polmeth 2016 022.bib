@article{ WOS:000461300700002,
Author = {Lopes, Dawisson Belem and Pimenta de Faria, Carlos Aurelio and Santos,
   Manoel},
Title = {Foreign Policy Analysis in Latin American democracies: the case for a
   research protocol},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2016},
Volume = {59},
Number = {1},
Abstract = {In this article we propose an alternative theoretical path to study
   contemporary Latin American foreign policies, evoking the notion of
   `public policy cycle' to explain how democratic regimes in the region
   have been able to expand their autonomy over time. For that, we will
   first identify the sources of a given country's foreign policy, both at
