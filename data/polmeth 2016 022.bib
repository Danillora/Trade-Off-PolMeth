@article{ WOS:000368984800006,
Author = {Ojeda, Christopher},
Title = {The Effect of 9/11 on the Heritability of Political Trust},
Journal = {POLITICAL PSYCHOLOGY},
Year = {2016},
Volume = {37},
Number = {1},
Pages = {73-88},
Month = {FEB},
Abstract = {Following the attacks of September 11, 2001, a rally effect led to a
   precipitous rise in political trust. However, the increase in political
   trust concealed a simultaneous decline among a smaller portion of the
   population. This article examines the psychological mechanisms
   underlying these heterogeneous attitudes towards government and shows
   that a biosocial model best explains the observed patterns of response.
   The interplay of genetic and environmental factors of political trust
   reveals the stable but dynamic nature of heritability: genetic
   influences of political trust increased immediately following 9/11 but
