@incollection{ WOS:000400576100004,
Author = {Luszczuk, Michal},
Editor = {Heininen, L},
Title = {Military Cooperation and Enhanced Arctic Security in the Context of
   Climate Change and Growing Global Interest in the Arctic},
Booktitle = {FUTURE SECURITY OF THE GLOBAL ARCTIC: STATE POLICY, ECONOMIC SECURITY
   AND CLIMATE},
Series = {Palgrave Pivot},
Year = {2016},
Pages = {35-54},
Abstract = {Transformation occurring in the Arctic raises many questions about the
   specificity and prospects of the regional security, including the multi-
   and bilateral military relations among the Arctic states. It is
   significant that though the majority of security challenges and threats
   to the region are of a nonmilitary character, the view that the
   projection of military power is indispensable in the Arctic seems to be
   well founded. This chapter provides an overview of selected forms of
   military cooperation developed in the Arctic during the past decade and
   their impacts on the regional security. It concludes that this
   cooperation will have to prove its effectiveness in very new
   circumstances framed by the tensions growing in the other parts of the
   globe.},
