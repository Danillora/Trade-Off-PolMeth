@article{ WOS:000381441300002,
Author = {Primiano, Christopher B. and Xiang, Jun},
Title = {Voting in the UN: a Second Image of China's Human Rights},
Journal = {JOURNAL OF CHINESE POLITICAL SCIENCE},
Year = {2016},
Volume = {21},
Number = {3},
Pages = {301-319},
Month = {SEP},
Abstract = {It is generally agreed that China has a poor domestic human rights
   practice. In contrast, China has a better-than-average voting record on
   human rights in the United Nations General Assembly (UNGA). Based on a
   new UNGA human rights dataset that we identified, we found that since
   joining the United Nations (UN) in 1971, China voted in the affirmative
   on human rights resolutions 79 \% of the time, compared to the world
   average favoring ratio of 75 \%. This positive image of consistently
   voting in the affirmative on UNGA human rights resolutions suggests a
   second image of China's human rights. However, while the Chinese
   government is willing to accept the international human rights regime,
   it also strategically alters its endorsement of UNGA human rights
   resolutions based on changing levels of domestic threats.},
