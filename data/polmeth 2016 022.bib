@article{ WOS:000384625000005,
Author = {Schneider, Monica C. and Holman, Mirya R. and Diekman, Amanda B. and
   McAndrew, Thomas},
Title = {Power, Conflict, and Community: How Gendered Views of Political Power
   Influence Women's Political Ambition},
Journal = {POLITICAL PSYCHOLOGY},
Year = {2016},
Volume = {37},
Number = {4},
Pages = {515-531},
Month = {AUG},
Abstract = {We provide a novel approach to understanding the political ambition gap
   between men and women by examining perceptions of the role of
   politician. Across three studies, we find that political careers are
   viewed as fulfilling power-related goals, such as self-promotion and
   competition. We connect these goals to a tolerance for interpersonal
   conflict and both of these factors to political ambition. Women's lack
   of interest in conflict and power-related activities mediates the
   relationship between gender and political ambition. In an experiment, we
   show that framing a political career as fulfilling communal goals- and
   not power-related goals-reduces the ambition gap.},
