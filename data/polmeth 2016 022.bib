@incollection{ WOS:000400576100006,
Author = {Lamy, Steven L.},
Editor = {Heininen, L},
Title = {The US Arctic Policy Agenda: The State Trumps Other Interests},
Booktitle = {FUTURE SECURITY OF THE GLOBAL ARCTIC: STATE POLICY, ECONOMIC SECURITY
   AND CLIMATE},
Series = {Palgrave Pivot},
Year = {2016},
Pages = {77-98},
Abstract = {This chapter explores the recent national debates within the Arctic
   states about the economic, political and human security challenges
   presented by climate change, growing competition for resources and new
   political and economic realities. The study identifies how the Arctic
   arena fits with the larger foreign policy approach of the state in
   question. Utilizing an analytical framework developed by Stephen Krasner
   to explain U.S. raw materials investment policy the study suggests that
   U.S. policy is best explained as being statist meaning the state and its
   central decision makers are autonomous actors and their choices serve
   the interests of those actors. The study interprets that the issue
   leaders are the key actors who define the debates about military,
   environmental and human security issues that define the Arctic policy
   agenda.},
