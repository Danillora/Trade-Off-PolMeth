@article{ WOS:000375272800006,
Author = {Taylor, Rebecca and Rees, James and Damm, Christopher},
Title = {UK employment services: understanding provider strategies in a dynamic
   strategic action field},
Journal = {POLICY AND POLITICS},
Year = {2016},
Volume = {44},
Number = {2},
Pages = {253-267},
Month = {APR},
Abstract = {UK employment services are increasingly delivered by public, private and
   third sector organisations in quasi-markets that can be viewed as fields
   with actors (providers) competing for resources and position. The
   commissioning of the Work Programme produced an `episode of contention'
   as fewer resources, shifting policy priorities and new contractual
   arrangements restructured relationships within the field. Drawing on
   empirical research the paper demonstrates how providers with different
   resources have navigated this period, employing strategies to manage
   challenger and incumbent roles and maintain their position in the field.
   The findings contribute to both field theory and our theoretical
   understanding of employment services.},
