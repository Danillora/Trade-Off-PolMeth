@article{ WOS:000387362300005,
Author = {Liang, Jiaqi},
Title = {Defining Environmental Justice Communities for Regulatory Enforcement:
   Implications from a Block-Group-Level Analysis of New York State},
Journal = {REVIEW OF POLICY RESEARCH},
