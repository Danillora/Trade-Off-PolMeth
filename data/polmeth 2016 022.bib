@article{ WOS:000374416600007,
Author = {Angel Mansilla, Miguel and Alberto Orellana, Luis},
Title = {Political Participation of Pentecostal Minorities in Chile, 1937-1989},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2016},
Volume = {43},
Number = {3},
Pages = {104-115},
Month = {MAY},
Abstract = {Chilean Pentecostals have been described as passive and politically
   conformist in their relations with the military government. Instead,
   there is historical evidence that they have been an active and
   interactive minority. Pentecostal denominations have participated in
   political projects associated with leftist political parties.
   Pentecostal leaders have associated themselves with various political
   parties as a form of political struggle for recognition, and active
   Pentecostal organizations resisted and protested during the military
   government. Se han calificado a los pentecostales chilenos de pasivos y
   conformistas politicamente en sus relaciones con el gobierno militar. En
   cambio, hay evidencia historica que han sido una minoria activa e
   interactiva. Las denominaciones pentecostales han participado en
   proyectos politicos asociados con partidos politicos de izquierda. Los
   dirigentes pentecostales se han plegado a varios partidos politicos como
   forma de lucha politica por el reconocimiento, y organizaciones
   pentecostales activas han resistido y protestado durante el gobierno
   militar.},
