@article{ WOS:000382504600005,
Author = {Socrates, Maria Celine Anastasia P.},
Title = {Frame alignments between the press and political actors: the Corona
   impeachment trial},
Journal = {PHILIPPINE POLITICAL SCIENCE JOURNAL},
Year = {2016},
Volume = {37},
Number = {1},
Pages = {43-62},
Abstract = {This qualitative study examines the alignments between the frames used
   by the press and those used by political actors in the Corona
   impeachment trial. This is considered from a social constructionist
   theoretical framework. The political actors included in the study are
   the senators who served as jurors during the trial. On the side of the
   press, this study focuses on two of the leading broadsheets in the
   Philippines: The Philippine Daily Inquirer and The Philippine Star.
   Through a content analysis of textual data as well as key informant
   interviews, the main finding of this study is that frame alignments do
   exist between the press and the political actors, but in differing
   degrees of resonance. The clearest point of convergence between the two
   is the framing of the impeachment as an issue of Corona's lack of
   transparency. Placed within the context of a ``politics of
   representation,{''} these findings illustrate how discourse is indeed a
   platform for contestation and competition for meaning.},
