@article{ WOS:000372350600006,
Author = {Pedersen, Mogens Jin},
Title = {A `HEART OF GOAL' AND THE WILL TO SUCCEED: GOAL COMMITMENT AND TASK
   PERFORMANCE AMONG TEACHERS IN PUBLIC SCHOOLS},
Journal = {PUBLIC ADMINISTRATION},
Year = {2016},
Volume = {94},
Number = {1, SI},
Pages = {75-88},
Month = {MAR},
Abstract = {Much research suggests that employee goal commitment is predictive of
   work behaviour and performance. However, public administration research
   has yet to establish the linkage between goal commitment and task
   performance in the context of public service employees. Using survey
   data on school teachers with administrative data on students, this
   article employs within-student between-teachers fixed effects and shows
   how teachers' goal commitment affects their task performance. Moreover,
   the article shows how task difficulty moderates this relationship. The
   findings suggest that higher goal commitment promotes teacher task
   performance, in particular, at moderate to high levels of task
   difficulty.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
