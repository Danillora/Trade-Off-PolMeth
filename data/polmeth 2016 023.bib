@article{ WOS:000374491100006,
Author = {McAllister, Ian and White, Stephen},
Title = {Lukashenka and His Voters},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
Year = {2016},
Volume = {30},
Number = {2},
Pages = {360-380},
Month = {MAY},
Abstract = {Various explanations have been offered for the commanding electoral
   support that has been enjoyed by the Belarusian president Alyaksandr
   Lukashenka since his original election in 1994. On the evidence of the
   authors' surveys, in particular post-election opinion surveys conducted
   in 2006 and 2011, there is an association between social structural
   variables and electoral support, and between perceptions of government
   economic performance and electoral support, but both largely disappear
   when appropriate controls are introduced. It is personal approval of
   Lukashenka on the part of voters, over and above these other
   circumstances, that makes by far the largest contribution.},
