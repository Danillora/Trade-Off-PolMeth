@article{ WOS:000376661600004,
Author = {Kaplow, Jeffrey M.},
Title = {The Negotiation Calculus: Why Parties to Civil Conflict Refuse to Talk},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
Year = {2016},
Volume = {60},
Number = {1},
Pages = {38-46},
Month = {MAR},
Abstract = {Why do some parties to intrastate conflict refuse to negotiate? I
   propose a simple theory of civil conflict in which the act of
   negotiation itself carries costs and benefits. Several hypotheses
   follow: parties to civil conflict will avoid negotiation when they (1)
   fear alienating external supporters or internal constituencies, (2) risk
   granting legitimacy to their opponents or signaling weakness to other
   potential claimants, or (3) find it difficult to identify reliable
   negotiating partners. Empirical tests find support for my argument. My
   findings suggest that cases exist in which the parties would reach an
   agreement if only they could overcome the costs of negotiation and
   engage in talks. Diplomats and mediators should consider the costs and
   benefits of talks when planning the timing and form of interventions
   designed to bring parties to the table.},
