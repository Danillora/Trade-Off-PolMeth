@article{ WOS:000371083700006,
Author = {Bain, Mervyn and Fernandez Mollinedo, Liliana},
Title = {Cuba: Trapped by history - Still?},
Journal = {INTERNATIONAL POLITICS},
Year = {2016},
Volume = {53},
Number = {2},
Pages = {260-276},
Month = {MAR},
Abstract = {Throughout its history Cuba has had an intimate relationship with
   outside powers and since January 1959 Cuban foreign policy has attracted
   much academic attention. Subsequently, a number of different theories
   regarding Cuban foreign policy have evolved. These are undoubtedly
   important, but this article will posit that it is only by examining
   legacies from the past that a fuller understanding of the island's
   bilateral relationships is achieved. In order to do this, the article
   will examine Cuba's relationship with the United States, European Union
   and Russian Federation. Significant change is taking place, but legacies
   from the past remain important in all three, which have had very
   different results for Havana's contemporary relationships with
   Washington, Brussels and Moscow. Areas of contestation from the past
   remain unresolved and will continue to be important for the foreseeable
   future.},
