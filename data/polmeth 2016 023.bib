@article{ WOS:000380340200002,
Author = {Lybecker, Donna L. and McBeth, Mark K. and Stoutenborough, James W.},
Title = {Do We Understand What the Public Hears? Stakeholders' Preferred
   Communication Choices for Discussing River Issues with the Public},
Journal = {REVIEW OF POLICY RESEARCH},
Year = {2016},
Volume = {33},
Number = {4},
Pages = {376-392},
Month = {JUL},
Abstract = {Communication between stakeholders and the larger public is an
   increasingly important issue in today's policy world. This communication
   often takes the form of policy narratives; however, few studies have
   empirically examined what type of communication style stakeholders
   prefer for communication with the public. This study uses a survey of 87
   river stakeholders to determine the preferred communication choices of
   stakeholders. The study finds that 39\% of the stakeholders chose a
   duty-based narrative, 28\% choose a science statement, 23\% choose an
   engaged citizen narrative, and 10\% choose a do nothing statement. Some
   of these preferences were related to issues of trust, other policy
   preferences, and demographic background. Based on previous research, the
   findings demonstrate that stakeholders can make decisions about how to
   communicate with the public that are not necessarily reflective of the
   own values.},
