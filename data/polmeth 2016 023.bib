@incollection{ WOS:000398075200003,
Author = {Murphy, Susan P.},
Book-Author = {Murphy, SP},
Title = {The Assistance Industry - Crisis and Change},
Booktitle = {RESPONSIBILITY IN AN INTERCONNECTED WORLD: INTERNATIONAL ASSISTANCE,
   DUTY, AND ACTION},
