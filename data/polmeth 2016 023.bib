@article{ WOS:000378936900006,
Author = {Sobol, Mor},
Title = {Principal-Agent Analysis and Pathological Delegation: The (Almost)
   Untold Story},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2016},
Volume = {29},
Number = {3},
Pages = {335-350},
Month = {JUL},
Abstract = {Principal-agent (PA) has come a long way since it was introduced to the
   political science sphere. Nowadays, PA has established itself as an
   institutional midrange theoretical framework that encompasses various
   methodological as well as theoretical approaches. This article argues,
   however, that scholars still assume, a priori, that the agent is an
   opportunistic and disloyal actor. This article seeks to question this
   theoretical assumption by demonstrating how principals could be as much
   problematic as their agents, a phenomenon that it terms pathological
   delegation. In so doing, it offers a first analysis of the small strand
   of the literature that examines problems embedded in the principals'
   side as well as provides new empirical evidence in the context of the
   European Neighbourhood Policy. Finally, the article posits that scholars
   should not treat pathological delegation as an anomaly but rather
   attempt to integrate, test, and develop new theoretical assumptions on
   this phenomenon.},
