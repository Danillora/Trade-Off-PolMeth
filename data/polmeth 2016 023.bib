@article{ WOS:000379259300003,
Author = {Santana-Pereira, Jose and Raimundo, Filipa and Pinto, Antonio Costa},
Title = {An Ever-Shadowed Past? Citizens' Attitudes towards the Dictatorship in
   Twenty-First Century Portugal},
Journal = {SOUTH EUROPEAN SOCIETY AND POLITICS},
Year = {2016},
Volume = {21},
Number = {2, SI},
Pages = {197-210},
Month = {JUN},
Abstract = {Several years after the transition to democracy, positive attitudes
   towards the authoritarian past are still observable in Portugal: the
   belief that the previous regime had more good things than bad things is
   expressed by about one-fifthof the Portuguese. What explains this
   nostalgic sentiment? Are factors such as socialisation under the regime,
   party identification or religiosity more important than satisfaction
   with democracy and the state of the economy? The empirical analysis
   suggests that the relevance of these factors varies considerably, but
   socialisation phases lead to different stances on the past both in
   routine times and in times of economic crisis.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Santana-Pereira, J (Corresponding Author), Univ Lisbon, Inst Social Sci ICS, P-1699 Lisbon, Portugal.
