@article{ WOS:000385440800002,
Author = {DeLeo, Rob A.},
Title = {Time and the management of policy conflict: issues, actors and
   institutions in the Boston biolaboratory controversy},
Journal = {CRITICAL POLICY STUDIES},
Year = {2016},
Volume = {10},
Number = {1},
Pages = {3-20},
Abstract = {Public policy theoreticians and practitioners have increasingly
   considered the implications of time on interest group conflict. A number
   of scholars have explored the evolution of interest group conflict and
   policy frames across time, noting that, as conflicts mature, groups
   often adopt a more dynamical `conflict management' approach. Instead of
   solely expanding or containing the scope of debate, conflict management
   sees the two sides strategically engage one another's narratives, appeal
   to the same types of allies and compete within the same venues. Others
   argue time itself is an explanatory variable that can alter interest
   group strategy and undermine the possibility of compromise. This
   literature straddles policy theory as well as the interdisciplinary
   study of the social psychological dynamics of conflict escalation, which
   has long construed time constraints as a roadblock to negotiation. Using
   the debate over the siting of a high-level security biolaboratory in
   downtown Boston as a case study, this article considers the dynamics of
   interest group conflict in the face of acute temporal constraints. The
   biolab case offers a useful context for exploring the implications of
   time on the trajectory of interest group strategies and as a potential
   `stressor' that can cause groups to adopt intractable - uncompromising
   and polarized - positions.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
