@article{ WOS:000372792200005,
Author = {Martin, Susanne and Weinberg, Leonard B.},
Title = {Terrorism in an Era of Unconventional Warfare},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2016},
Volume = {28},
Number = {2},
Pages = {236-253},
Abstract = {Academic studies of terrorism and mass political violence have developed
   largely independently of one another. Insurgents, in contrast, have
   tended to incorporate terrorism tactics along with other types of
   unconventional warfare in their repertoire of action. This tendency has
   become more apparent among insurgents engaging in armed confrontations
   in the twenty-first century. In order to take account of this
   development, scholars and others interested in contemporary warfare need
   to incorporate terrorism studies within the broader subject of
   insurgencies and small warspolitical violence, in other words.},
