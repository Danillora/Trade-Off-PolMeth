@article{ WOS:000368178600006,
Author = {Smith, Alastair},
Title = {Leader Turnover, Institutions, and Voting at the UN General Assembly},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2016},
Volume = {60},
Number = {1},
Pages = {143-163},
Month = {FEB},
Abstract = {Using evidence from voting in the UN General Assembly (UNGA), this
   article shows that leader turnover, especially in small coalition,
   nondemocratic systems, increases the likelihood of policy realignment.
   Autocrats who are beholden to only a small proportion of the population
   represent the foreign policy interests of their small number of
   supporters. When leader turnover occurs, the interests represented often
   shift too and this results in an increased volatility and regression
   toward a neutral position of a nation's alignment at the United Nations
