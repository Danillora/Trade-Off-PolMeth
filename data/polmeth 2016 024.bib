@article{ WOS:000377803900003,
Author = {Clegg, Peter and Dunwoodie-Stirton, Fred and Cole, Phillip},
Title = {Human rights in the overseas territories: in policy but not in practice?},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2016},
Volume = {54},
Number = {1},
Pages = {46-68},
Abstract = {Since the late 1990s emphasis has been placed by the UK government on
   enhancing human rights in its Overseas Territories. Some early changes
   were enforced, but more recently persuasion and capacity building have
   been prioritised. However, due to the complexity of the bilateral
   relationships and the cultural diversity that exists, fostering and
   embedding reform is difficult. These challenges are seen most clearly in
   two examples: the rights of the child to be protected from sexual
   exploitation, and the securing of equality in relation to sexual
   orientation with reference to LGBT rights. The article analyses the
   constitutional and legal changes that have been made in regard to these
   two issues, and whether the creation of stronger human rights principles
   has led to enhanced rights in practice.},
