@article{ WOS:000374416600006,
Author = {Smiderle, Carlos Gustavo Sarmet Moreira and Mesquita, Wania Amelia
   Belchior},
Title = {Political Conflict and Spiritual Battle: Intersections between Religion
   and Politics among Brazilian Pentecostals},
