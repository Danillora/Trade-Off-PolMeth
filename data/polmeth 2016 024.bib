@article{ WOS:000385809900009,
Author = {Karmazin, Ales},
Title = {Rethinking the individual through Chinese ontology: Implications for
   International Relations theory and humanitarian intervention},
Journal = {POLITICS},
Year = {2016},
Volume = {36},
Number = {4},
Pages = {413-427},
Month = {NOV},
Abstract = {Through utilizing correlative ontology, which is derived from ancient
   Chinese philosophy, this article offers a redefinition of the individual
   (subject), which subsequently helps to problematize the particular
   versus universal' distinction. It is then shown how correlative ontology
   might be useful for criticizing and stimulating the English School of
   International Relations and its two main views (i.e. solidarism and
   pluralism) on humanitarian intervention. The significance is that
   correlative ontology helps to challenge solidarism qua universalism' and
   pluralism qua particularism'.},
