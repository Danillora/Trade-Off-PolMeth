@article{ WOS:000390887800008,
Author = {Biscop, Sven},
Title = {All or nothing? The EU Global Strategy and defence policy after the
   Brexit},
Journal = {CONTEMPORARY SECURITY POLICY},
Year = {2016},
Volume = {37},
Number = {3},
Pages = {431-445},
Abstract = {The public expects European governments and the European Union (EU) to
   deal with the security challenges in and around Europe. So does the US,
   whose strategic focus has pivoted to the Pacific. Washington, DC has
   made it clear that it will not, and cannot, solve all of Europe's
   problems. The call for `strategic autonomy' in the new EU Global
   Strategy of June 2016 does not come a moment too soon. But should the
   aim be EU strategic autonomy, without the UK, or can the aspiration
   still be European strategic autonomy, with the UK? Can nothing be
   achieved unless all are fully involved? Or are intermediate solutions
   possible? How EU Member States and the UK answer these questions will
   determine which degree of strategic autonomy the EU can achieve. With
   which degree of British involvement. And whether the UK itself will be
   left with any measure of strategic autonomy.},
