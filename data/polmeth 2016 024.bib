@incollection{ WOS:000557691200017,
Author = {Urdze, Sigita},
Editor = {Fruhstorfer, A and Hein, M},
Title = {Latvia},
Booktitle = {CONSTITUTIONAL POLITICS IN CENTRAL AND EASTERN EUROPE: FROM
   POST-SOCIALIST TRANSITION TO THE REFORM OF POLITICAL SYSTEMS},
Series = {Vergleichende Politikwissenschaft},
Year = {2016},
Pages = {411-437},
Abstract = {Latvia is the only post-Communist country in Central and Eastern Europe
   that reinstated its old interwar constitution. From 1993, when this
   occurred, until 2014, the constitution was amended twelve times and many
   more proposals to do so were presented to the parliament. Among the most
   important topics in this regard are the introduction of a constitutional
   court and a chapter on fundamental human rights, adapting the 1922
   constitution to contemporary circumstances. However, the most
   intensively debated amendments-nationally and internationally-were those
   on ethnic questions. Several amendments on this topic were adopted, and
   several more proposed. Debates on ethnic matters develop along a
   pro-Latvian nationalistic versus a pro-Russian nationalistic cleavage.},
