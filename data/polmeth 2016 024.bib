@article{ WOS:000374491100010,
Author = {Lampland, Martha and Nadkarni, Maya},
Title = {``What Happened to Jokes?{''} The Shifting Landscape of Humor in Hungary},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
