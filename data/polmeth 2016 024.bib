@article{ WOS:000372102800007,
Author = {Nattrass, Nicoli and Seekings, Jeremy},
Title = {Trade unions, the state and `casino capitalism' in South Africa's
   clothing industry},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2016},
Volume = {43},
Number = {147},
Pages = {89-106},
Month = {JAN 2},
Abstract = {Relationships between trade unions, the state and capital in South
   Africa have changed dramatically, especially in the clothing sector. The
   clothing workers' union became heavily dependent on its political
   alliances with the governing party, not only for the regulation of wages
   and industrial policies, but also for Black Economic Empowerment
   policies that helped it to acquire massive shareholdings, including in
   the largest clothing manufacturer. In terms of both its exposure to
   capitalist risk and its investments in the casino industry specifically,
   the union acquired a stake in `casino capitalism', whilst relying on
   government to stack the odds in its favour.},
