@article{ WOS:000390887800003,
Author = {Dijkstra, Hylke},
Title = {Introduction: one-and-a-half cheers for the EU Global Strategy},
Journal = {CONTEMPORARY SECURITY POLICY},
Year = {2016},
Volume = {37},
Number = {3},
Pages = {369-373},
Abstract = {EU High Representative Federica Mogherini presented her EU Global
   Strategy (EUGS) in June 2016. Encircled by security crises, it is
   difficult to think of something more important for Europe than
   collective action with the aim of weathering the storm. The EUGS, in
   this respect, seeks to define common ends and identify means. So what do
   we make of the EUGS? What does the EUGS tell us about the current role
   of the EU in global affairs? And how will the withdrawal of the UK from
   the EU affect foreign and security policy? As a way of introduction to
   the forum, this article notes that the EUGS focuses on the
   neighbourhood, puts the interests of European citizens first, identifies
   civilian means, and has created momentum on security policy. The key
   question, however, remains whether there is any interest in the EUGS
   beyond the foreign policy elites.},
