   Jones, Michael D., Oregon States Sch Publ Policy, Corvallis, OR USA.},
DOI = {10.1111/psj.12111},
ISSN = {0190-292X},
EISSN = {1541-0072},
Keywords = {Kingdon; multiple streams analysis; policy theory},
