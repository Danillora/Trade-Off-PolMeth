@article{ WOS:000375862000006,
Author = {Duwell, Marcus and Bos, Gerhard},
Title = {Human rights and future people Possibilities of argumentation},
Journal = {JOURNAL OF HUMAN RIGHTS},
Year = {2016},
Volume = {15},
Number = {2},
Pages = {231-250},
Abstract = {Questions of sustainability will be of crucial importance for the
   twenty-first century. But do we have to think about questions of
   responsibilities regarding future people in terms of human rights? And
   if duties regarding sustainability fall outside the scope of human
   rights, what would this imply for the moral and political importance of
   human rights in general? This article investigates conceptually how we
   should see the relationship between human rights and long-term global
   ecological challenges. We will discuss how a human rights approach to
   questions of sustainability would be different from other approaches and
   what would be required to see those ecological challenges as human
   rights questions. We will discuss the possibilities for conceptualizing
   the relationship between human rights and sustainability. And we will
   briefly draw some conclusions in terms of topics for further debate.},
