@article{ WOS:000405222200003,
Author = {Lopes, Dawisson Belem},
Title = {Polyarchies, competitive oligarchies or inclusive hegemonies? A
   comparison of 23 global intergovernmental organizations based on Robert
   Dahl's political theory},
Journal = {CAMBRIDGE REVIEW OF INTERNATIONAL AFFAIRS},
Year = {2016},
Volume = {29},
Number = {4},
Pages = {1233-1258},
Abstract = {In this article, I assume that global intergovernmental organizations
   (GIGOs) function as enablers of interstate liberal politics by way of
   their multilateral institutional frameworks. To support this view, I
   recall and adapt the classical concept of polyarchy,' coined in the
   early 1950s by Robert A. Dahl. It consists of a two-dimensional
   theoretical construct applicable for measuring the level of
   liberalization in modern political societies. It follows that the more
   actors who take part in politics, and the more such institutions allow
   political opposition, the more open a society (of states) is likely to
