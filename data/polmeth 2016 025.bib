@article{ WOS:000383572300015,
Author = {Osipovic, Dorota and Allen, Pauline and Shepherd, Elizabeth and Coleman,
   Anna and Perkins, Neil and Williams, Lorraine and Sanderson, Marie and
   Checkland, Katherine},
Title = {INTERROGATING INSTITUTIONAL CHANGE: ACTORS' ATTITUDES TO COMPETITION AND
   COOPERATION IN COMMISSIONING HEALTH SERVICES IN ENGLAND},
Journal = {PUBLIC ADMINISTRATION},
Year = {2016},
Volume = {94},
Number = {3},
Pages = {823-838},
Month = {SEP},
Abstract = {Since the beginning of the 1990s the public healthcare system in England
   has been subject to reforms. This has resulted in a structurally hybrid
   system of public service with elements of the market. Utilizing a theory
   of new institutionalism, this article explores National Health Service
   (NHS) managers' views on competition and cooperation as mechanisms for
   commissioning health services. We interrogate the extent of
   institutional change in the NHS by examining managers' understanding of
   the formal rules, normative positions and frameworks for action under
   the regime of the Health and Social Care Act 2012. Interviews with
   managers showed an overall preference for cooperative approaches, but
   also evidence of marketization in the normative outlook and actions.
   This suggests that hybridity in the NHS has already spread from
   structure and rules to other institutional pillars. The study showed
   that managers were adept at navigating the complex policy environment
   despite its inherent contradictions.},
