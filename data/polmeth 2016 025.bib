@article{ WOS:000385441600007,
Author = {Li, Ya and He, Jianzi},
Title = {Exploring deliberative policy analysis in an authoritarian country},
Journal = {CRITICAL POLICY STUDIES},
Year = {2016},
Volume = {10},
Number = {2},
Pages = {235-246},
Abstract = {Deliberative policy analysis (DPA), which embraces increased public
   involvement and rejects technocracy, seems naturally to be connected to
   democratic political systems. Why, then, and how has DPA developed in
   China, an authoritarian country? This forum piece takes experimental
   policy research methodology (EPRM), a localized version of DPA in China,
   as an embodiment of this and depicts the political, institutional, and
   disciplinary landscape in which DPA has been conducted in China from a
   historical perspective. The essay introduces EPRM's basic concepts,
   outlines its evolutionary trajectory from a specific systems methodology
   to a localized DPA method, and discusses the implications of the Chinese
   experience.},
