@article{ WOS:000390755900006,
Author = {Stone, John},
Title = {A Proxemic Account of Bayonet Fighting},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2016},
Volume = {39},
Number = {7},
Pages = {1023-1043},
Month = {DEC},
Abstract = {The bayonet has long been regarded as an obsolete piece of military
   technology. This article advances a proxemic account of close combat,
   which points to its continuing utility under modern battlefield
   conditions. It is argued that bayonet training remains an effective way
   of preparing soldiers to psychologically dominate contested spaces on
   the battlefield. Thus, although other weapons and techniques can fulfil
   the same role, it can nevertheless be argued that the bayonet's enduring
   presence is not simply the result of some atavistic military mind-set.},
