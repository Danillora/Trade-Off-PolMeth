@article{ WOS:000398001800003,
Author = {Tsatsanis, Emmanouil and Teperoglou, Eftichia},
Title = {Realignment under Stress: The July 2015 Referendum and the September
   Parliamentary Election in Greece},
Journal = {SOUTH EUROPEAN SOCIETY AND POLITICS},
Year = {2016},
Volume = {21},
Number = {4},
Pages = {427-450},
Abstract = {The victory of the radical-left SYRIZA in the September 2015 election
   confounded expectations given the failure of the SYRIZA-ANEL government
   formed in January either to deliver on its central promise of reversing
   austerity policies or to capitalise on its major victory in the July
   referendum. The article examines both the election and the referendum
   that preceded it, offering an explanation for SYRIZA's victory. It also
   attempts to trace the trajectory of the current party system in Greece
