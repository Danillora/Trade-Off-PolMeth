@article{ WOS:000384446100019,
Author = {Shahi, Deepshikha},
Title = {Engaging with the post-secular moment in post-9/11 Afghanistan: the
   search for a ``humanist' political discourse},
Journal = {CAMBRIDGE REVIEW OF INTERNATIONAL AFFAIRS},
Year = {2016},
Volume = {29},
Number = {2},
Pages = {718-739},
Month = {JUN},
Abstract = {9/11 and the subsequent war on terror in Afghanistan have severely
   challenged the idea of a world politics based on secular modernity.
   While the post-9/11 Afghan society remains troubled with the
   post-secular conflict between the so-called Islamic-terrorist and
   secular-democratic forces, the need for a humanist' political discourse
   that could pave the way for peace has become paramount. This paper
   explores the viability of post-Islamism' as an alternative humanist
   political discourse. It sets out to demonstrate how a post-Islamic
   humanist discourse, which is defined by the dialogic process of
   developing a hermeneutical understanding of Islamic philosophy, has the
   potential to not only carve the way for peace amidst perilous
   entanglement between politics and religion in post-secular Afghanistan,
   but also vindicate Islam of its unjustified denigration in the
   contemporary world.},
