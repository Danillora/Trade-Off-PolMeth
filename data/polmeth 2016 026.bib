@article{ WOS:000370546400005,
Author = {Malkki, Leena},
Title = {International Pressure to Perform: Counterterrorism Policy Development
   in Finland},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2016},
Volume = {39},
Number = {4, SI},
Pages = {342-362},
Month = {APR 2},
Abstract = {The major terrorist attacks in Western countries during the last fifteen
   years have had consequences way beyond the countries in which they have
   happened. The article provides a primary source-based account of the
   development of counterterrorism policy in Finland, which is one of those
   countries with a low national threat level. The article demonstrates the
   significant role that international pressure, through obligations,
   recommendations, and social learning, plays in developing national
   counterterrorist policies. The article calls also into question whether
   the pressure to comply with international pressure always contributes
   toward sound national counterterrorism policies that foster political
   resilience to terrorism.},
