@article{ WOS:000372845900001,
Author = {Suttmoeller, Michael J. and Chermak, Steven M. and Freilich, Joshua D.},
Title = {Only the Bad Die Young: The Correlates of Organizational Death for
   Far-Right Extremist Groups},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2016},
Volume = {39},
Number = {6},
Pages = {477-499},
Month = {JUN 2},
Abstract = {The domestic far-right movement has existed in the United States for
   many years. During that time, groups have appeared, disappeared, and
   reappeared. Unfortunately, very little is known about what causes these
   groups to disband. Prior research has focused on long-lived groups, but
   the majority of extremist or terrorist groups fail to survive for an
   extended period of time. This study examined a variety of external and
   internal correlates of organizational death identified from both
   terrorism and organizational literature, to empirically test which
   correlates lead to a group dying young.},
