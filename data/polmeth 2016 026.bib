@article{ WOS:000384625000009,
Author = {Reilly, Jonathan Joseph},
Title = {No Postmaterialists in Foxholes: Postmaterialist Values, Nationalism,
   and National Threat in the People's Republic of China},
Journal = {POLITICAL PSYCHOLOGY},
Year = {2016},
Volume = {37},
Number = {4},
Pages = {565-572},
Month = {AUG},
Abstract = {In this article, I present findings from a survey experiment in which
   Chinese university students exposed to a treatment designed to increase
   feelings of national threat were-based on their responses to the
   four-item postmaterialism values-priority battery-significantly more
   likely to be classified as ``pure materialists.{''} These findings are
   presented in support of the proposition that perception of a hostile
   international environment may tend to exaggerate citizens' authoritarian
   and nationalistic sentiments at the expense of more democratically
   favorable value orientations. Media and political figures in the West
   who rail against the evils of China's authoritarian leadership might
   believe that they are championing and encouraging democratic aspirations
   among the Chinese people, but might instead be inciting impulses and
   attitudes that are far less ``democracy-friendly.{''}},
