@article{ WOS:000384026800006,
Author = {Whitten-Woodring, Jenifer},
Title = {News about her: The effects of media freedom and internet access on
   women's rights},
Journal = {JOURNAL OF HUMAN RIGHTS},
Year = {2016},
Volume = {15},
Number = {3, SI},
Pages = {383-407},
Abstract = {Human rights organizations have long heralded media freedom as critical
   to holding government accountable and thereby improving a wide range of
   human rights. Similarly the Internet and social media are assumed to
   empower citizens by enabling them to document repression and thereby
   discourage future abuse. So what does this mean for women's rights? I
   propose that, when it comes to women's rights, the combination of media
   freedom and Internet access will make a difference and that the effect
   of media freedom will depend on Internet access. I test my hypotheses
   across countries and over time and find that the interaction of Internet
   access and media freedom has positive effects on women's rights
   regardless of regime type.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
