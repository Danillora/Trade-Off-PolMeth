@article{ WOS:000393323600002,
Author = {Wietzke, Frank-Borge},
Title = {Kicking away the ladder? Poverty reduction and public support for
   redistribution in emerging economies},
Journal = {SOCIO-ECONOMIC REVIEW},
Year = {2016},
Volume = {14},
Number = {3},
Pages = {419-442},
Month = {JUL},
Abstract = {By recent estimates, the developing world's share of people in extreme
   deprivation has been reduced by half in the past two decades. In the
   meantime, the population just above the absolute poverty line has grown
   by about 1.4 billion and will soon represent the majority of the
   developing world's population. This article analyses the consequences of
   these dramatic transformations for public attitudes about
   redistribution. Contrary to the predictions of standard economic models,
   it finds that countries with faster rates of poverty reduction tend to
   report growing support for redistribution. In addition, there are signs
   that changing poverty levels and public redistribution preferences were
   accompanied by changes in public spending on basic social services.},
Publisher = {OXFORD UNIV PRESS},
Address = {GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND},
Type = {Article},
Language = {English},
