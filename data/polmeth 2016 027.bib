@article{ WOS:000374191800001,
Author = {Ura, Joseph Daniel and Flink, Carla M.},
Title = {Managing the Supreme Court: The Chief Justice, Management, and Consensus},
Journal = {JOURNAL OF PUBLIC ADMINISTRATION RESEARCH AND THEORY},
Year = {2016},
Volume = {26},
Number = {2},
Pages = {185-196},
Month = {APR},
Abstract = {In this article, we draw on a prominent model of public management to
   develop a preliminary theoretical approach to understanding the role of
   the chief justice in Supreme Court decision-making. In particular, we
   argue that the Court may seek legitimacy through greater unanimity and
   discuss how the leadership of the chief justice can facilitate that
   effort. We assess a hypothesis derived from this theory, showing greater
   agreement among the justices as the incumbent chief justice's tenure in
   office increases. We argue that these results provide support for
   further attention to and development of a public administration-based
   approach to the study of Supreme Court decision-making. The application
   of public administration to judicial politics provides further evidence
   of management dynamics in American institutions.},
