@article{ WOS:000394726900003,
Author = {Markussen, Havard Rustad},
Title = {On fiction and reality ⟪Okkupert⟫ as naturalizing for Norwegian
   Russia-representations},
Journal = {INTERNASJONAL POLITIKK},
Year = {2016},
Volume = {74},
Number = {4},
Abstract = {Responding to criticism from the Russian embassy in Oslo, the man behind
   the idea of the TV-show << Okkupert >>, Jo Nesbo, argued that the show
   could not impact relations between Norway and Russia because of its
   fictional character. This statement is leaning on the interesting
   philosophical assumption that fiction and reality are of different
   ontological qualities, and therefore completely detached from one
   another. This article will challenge Nesbo's statement and the
   assumption he makes, by applying a poststructuralist philosophy of
   science in arguing that what we consider << reality >> is, just like
   fiction, only a representation of reality rather than reality itself.
   This way, a case can be made that popular culture has naturalizing
   effects on social reality. Accordingly, there should be room for it in
   the social sciences generally and in the International
   Relations-discipline specifically. As empirical support for this
   theoretical argument, the article will demonstrate how << Okkupert >>
   reproduces and reinforces Norwegian representations of Russia and this
   way also how it contributes to constituting Norwegian identity.},
