@article{ WOS:000368524000003,
Author = {Detzi, Daniel and Winkleman, Steven},
Title = {Hitting Them Where it Hurts: A Joint Interagency Network to Disrupt
   Terrorist Financing in West Africa},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2016},
Volume = {39},
Number = {3},
Pages = {227-239},
Month = {MAR 3},
Abstract = {Illicit trafficking networks in West Africa are a major funding source
   for violent extremist groups in the region. Although the United States
   has formed interagency constructs in the Caribbean and in Europe to
   counter illicit trafficking, there is a gap in West Africa. The corridor
   of smuggling routes from the shores of West Africa into North Africa
   provides extremist groups with the financing they need to continue their
   activities. By forming a third interagency organization in West Africa,
   the United States and its partners could strike a significant blow to
   violent extremist groups, which continue to threaten U.S. interests in
   the region.},
