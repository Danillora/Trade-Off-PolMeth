@article{ WOS:000382980200004,
Author = {Bove, Vincenzo and Gleditsch, Kristian Skrede and Sekeris, Petros G.},
Title = {``Oil above Water{''}: Economic Interdependence and Third-party
   Intervention},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2016},
Volume = {60},
Number = {7},
Pages = {1251-1277},
Month = {OCT},
Abstract = {We explore economic incentives for third parties to intervene in ongoing
   internal wars. We develop a three-party model of the decision to
   intervene in conflict that highlights the role of the economic benefits
   accruing from the intervention and the potential costs. We present novel
   empirical results on the role of oil in motivating third-party military
   intervention. We find that the likelihood of a third-party intervention
   increases when (a) the country at war has large reserves of oil, (b) the
   relative competition in the sector is limited, and (c) the potential
   intervener has a higher demand for oil.},
