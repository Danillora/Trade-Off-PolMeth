@article{ WOS:000372837500003,
Author = {McConnell, Allan},
Title = {A public policy approach to understanding the nature and causes of
   foreign policy failure},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2016},
Volume = {23},
Number = {5, SI},
Pages = {667-684},
Month = {MAY 27},
Abstract = {All governments are vulnerable to policy failure but our understanding
   of the nature and causes of policy failure is highly underdeveloped.
   This contribution, written from a public policy perspective, sets out a
   framework for understanding these issues as applied to foreign policy.
   In doing so, it seeks a cross-disciplinary fertilization of thinking
   that uses the messy and contested reality of policy failure as
   fundamentally a key - rather than a barrier - to advancing our
   understanding of a phenomenon referred to variously as policy fiascos,
   policy disasters, policy blunders and policy failures.},
