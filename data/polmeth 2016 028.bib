@article{ WOS:000381282700006,
Author = {Imbrogno, Anthony F.},
Title = {The founding of the European Council: economic reform and the mechanism
   of continuous negotiation},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2016},
Volume = {38},
Number = {6},
Pages = {719-736},
Month = {SEP},
Abstract = {By comparing two eras of European heads-of-state-or-government (HoSG)
   summitry - the summits of 1961 and those between 1967 and 1974 - an
   under-studied factor in the European Council's institutionalisation is
   revealed. At the 1961 summits, political integration dominated HoSG
   discussions, whereas from 1967 onward, economic reform - in the form of
   the common market, common agriculture policy, economic and monetary
   union and regional development fund - was the dominant topic of
   discussions. The institutionalisation of summitry was a product not only
   of personalities and international events but also of the mechanism of
   continuous negotiation on economic reform.},
