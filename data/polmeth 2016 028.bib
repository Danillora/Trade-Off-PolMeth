@article{ WOS:000388717400005,
Author = {Mulligan, William},
Title = {Capitalism returns},
Journal = {INTERNATIONAL POLITICS},
Year = {2016},
Volume = {53},
Number = {5},
Pages = {618-627},
Month = {SEP},
Abstract = {Since the 1980s, historians have tended to downplay the significance of
   capitalist economics in shaping great power politics, favouring
   political and cultural approaches to classic topics, such as the origins
   of the First World War, peace-making, and appeasement policy in the
   1930s. Anievas's book is invitation to scholars to rethink the
   connections between international economic relations, social
   instability, and great power politics. This review suggests two ways, in
   which Anievas's contribution can stimulate further debate and research.
   First, it argues that historians should consider the relationship
   between instruments of economic and military power. It offers the
   hypothesis that states, which had few effective instruments of economic
   power, were more willing to consider using military force to achieve
   their interests. Second, we need a more differentiated debate on the
   relationship between global economic interdepedence and the
   possibilities of war and peace. Trading relationships and financial
   interdependence could produce different interests. We need to show how
   different business groups considered international politics and how
   politicians interpreted economic interdependence.},
