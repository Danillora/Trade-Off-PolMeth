@article{ WOS:000394725500007,
Author = {Svendsen, Oyvind},
Title = {Stigma in international politics: Norway, EU and the search for
   ``normal{''} in a post-Soviet security context},
Journal = {INTERNASJONAL POLITIKK},
Year = {2016},
Volume = {74},
Number = {1},
Abstract = {Why has Norway developed to become an active agent in EU security
   policy? This article questions an apparent anomaly in Norway's
   relationship with the EU since 1990: on the one side the country is not
   a member of the union, on the other, it has sought close ties in with
   the EU in relation to security since the Soviet Union fell. The
   sanctioning regime following Russia's annexation of the Crimean
   peninsula is a recent example of this. By utilising the insights of
   social theorists Pierre Bourdieu and Erving Goffman, as brought into IR
   by Rebecca Adler-Nissen, I argue that the practice of convergence was,
   and is, driven by (1) the integrationist habitus of Norwegian elites,
   (2) The EUs symbolic power in its relationship with Norway and (3) a
   process of stigmatization in the European field of security. The latter
   point is treated with particular attention, and involves how Norwegian
   actors finds it uncomfortable to be << outsiders >> in Europe. The
   response has been stigma recognition in an attempt to pass as << normal
   >>.},
