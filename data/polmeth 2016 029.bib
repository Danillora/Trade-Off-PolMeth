@article{ WOS:000371328500014,
Author = {Aviram, Hadar},
Title = {The Correctional Hunger Games: Understanding Realignment in the Context
   of the Great Recession},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2016},
Volume = {664},
Number = {1},
Pages = {260-279},
Month = {MAR},
Abstract = {The California Criminal Justice Realignment is often seen as a sui
   generis penal experiment in response to a court mandate. However, when
   examined in a broader context, it can be understood as part of a
   recession-era institutional effort to reduce prison costs, fueled by a
   discourse of austerity and financial prudence, which I refer to as
   humonetarian. This article examines Realignment, its predecessor, Senate
   Bill 18, and its successor, Proposition 47, as examples of humonetarian
   policy, characterized by a rhetoric of costs and savings; bipartisan
   support; inmate transference practices; and a focus on nonviolent,
   low-risk offenders, whose incarceration expenses exceed the risk they
   pose. The analysis yields two insights: one, the financial context of
   reform; and the other, the unique, neopopulist California flavor of this
   financial context, which stands in the way of even more effective
   reform. I end with thoughts about the promise and perils of
   humonetarianism in the California context.},
