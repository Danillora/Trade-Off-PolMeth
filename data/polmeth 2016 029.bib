@article{ WOS:000382206100002,
Author = {Hopkin, Jonathan and Shaw, Kate Alexander},
Title = {Organized Combat or Structural Advantage? The Politics of Inequality and
   the Winner-Take-All Economy in the United Kingdom},
Journal = {POLITICS \& SOCIETY},
Year = {2016},
Volume = {44},
Number = {3, SI},
Pages = {345-371},
Month = {SEP},
Note = {Biennial Conference of the European-Union-Studies-Association (EUSA),
   Boston, MA, MAR, 2015},
Organization = {European Union Studies Assoc},
Abstract = {Since 1970 the United Kingdom, like the United States, has developed a
   winner-take-all political economy characterized by widening inequality
   and spectacular income growth at the top of the distribution. However,
   Britain's centralized executive branch and relatively insulated
   policymaking process are less amenable to the kind of organized combat
   that Hacker and Pierson describe for the United States. Britain's
   winner-take-all politics is better explained by the rise of political
   ideas favoring unfettered markets that, over time, produce a
   self-perpetuating structural advantage for the richest. That advantage
   is, in turn, justified and sustained by reference to the same ideas.
   Inequality growth in the United Kingdom has been primarily driven by the
   financialization of the economy that began under the Thatcher government
   and continued under New Labour. The survival of pro-finance policies
   through the financial crisis provides further evidence that lobbying by
   a weakened City of London was less decisive in shaping policy than the
   financial sector's continuing structural advantage and the tenacity of
   its supporting political consensus.},
