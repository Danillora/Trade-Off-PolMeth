@article{ WOS:000380686100004,
Author = {Bush, Sarah Sunn},
Title = {When and why is civil society support ``made-in-America{''}? Delegation
   to non-state actors in American democracy promotion},
Journal = {REVIEW OF INTERNATIONAL ORGANIZATIONS},
Year = {2016},
Volume = {11},
Number = {3},
Pages = {361-385},
Month = {SEP},
Abstract = {One of the United States' main strategies of democracy promotion
   involves supporting civil society abroad. According to original data,
   most of the money spent by the United States on that task supports
   American NGOs working abroad rather than local NGOs in transitioning and
   non-democratic countries. Yet there are also significant variations
   across countries in donor officials' reliance on American NGOs. Why do
   American donor officials fund American NGOs as a strategy of aiding
   democracy abroad more in some cases than in others? This paper argues
   that donor officials find it easier to observe American NGOs than other
   NGOs and that American NGOs are more likely to share donor officials'
   preferences. Donor officials are therefore more likely to pursue a
   strategy of ``made-in-America{''} democracy support in countries that
   are salient for U.S. foreign policy. Evidence from a new data set of
   democracy assistance programs supports the argument. The findings have
   implications for the study of American foreign policy, foreign aid
   effectiveness, and NGOs in world politics.},
Publisher = {SPRINGER},
