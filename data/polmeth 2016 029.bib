@article{ WOS:000381366100001,
Author = {Crowley, Stephen},
Title = {Monotowns and the political economy of industrial restructuring in
   Russia},
Journal = {POST-SOVIET AFFAIRS},
Year = {2016},
Volume = {32},
Number = {5},
Pages = {397-422},
Abstract = {During the 2008-2009 economic crisis, Russia's monotowns - one-industry
   towns left from the Soviet era - gained widespread attention as
   potential sources of social protest and unrest. Will such worries
   resurface under current economic conditions? While fears about monotowns
   were exaggerated during the last economic crisis, Russia's leadership
   has reason to remain concerned. Despite the dramatic transformations of
   the last two decades, Russia's post-Soviet industrial landscape has
   largely survived intact, leaving a significant number of monotowns with
   unprofitable enterprises in a precarious position. Yet given its
   emphasis on social stability, we can expect the government to continue
   subsidies, both explicit and hidden, that seek to maintain employment
   and avoid social conflict, but that preserve the country's inefficient
   industrial geography.},
