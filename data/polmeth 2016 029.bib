@article{ WOS:000381412800002,
Author = {Falleti, Tulia G.},
Title = {Process tracing of extensive and intensive processes},
Journal = {NEW POLITICAL ECONOMY},
Year = {2016},
Volume = {21},
Number = {5, SI},
Pages = {455-462},
Abstract = {This essay briefly analyzes the introduction of process tracing to the
   political science and political economy methodological toolkit. It then
   proposes a theory-guided variant of process tracing and distinguishes
   between its application in extensive and intensive type of processes. It
   argues that the comparative advantage of process tracing vis-a-vis other
   social sciences research methods lies in its potential to uncover the
   causal mechanisms that link the constitutive events of intensive type of
   processes. It shows that theory-guided process tracing of intensive
   processes can be successfully applied to illustrate, test and produce
   theory.},
