@article{ WOS:000377779300004,
Author = {Fedorenko, Kostyantyn and Rybiy, Olena and Umland, Andreas},
Title = {The Ukrainian Party System before and after the 2013-2014 Euromaidan},
Journal = {EUROPE-ASIA STUDIES},
Year = {2016},
Volume = {68},
Number = {4, SI},
Pages = {609-630},
Month = {JUN},
Abstract = {The formation of a party system is widely regarded as a key to
   successful and sustainable patterns of democratisation. In this essay we
   examine the evolution of the party system in Ukraine, focusing on the
   extent to which the Euromaidan has addressed previous problems and
   weaknesses. So far the post-Soviet Ukrainian party system has been
   exceptionally unstable as electoral legislation, the factional
   composition of Ukraine's parliament, and the dominant parties in Ukraine
   underwent frequent changes. We argue that, despite advances in
   democratisation as a result of the revolutionary events of 2013-2014,
   the essential features of Ukraine's party system have not changed. The
   legislative, ideological and organisational foundations of the Ukrainian
   party-political landscape and parliamentary politics have remained
   fragile.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
