@article{ WOS:000398282600005,
Author = {Tate, Tony and Kabia, John and Nair, Seema},
Title = {Balancing Respect While Ensuring Accountability in the Funder-Grantee
   Relationship: Lessons from Central Africa, Liberia and India},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2016},
Volume = {8},
Number = {3},
Pages = {378-392},
Month = {NOV},
Abstract = {How can human rights funders ensure accountability from the
   organizations they fund while respecting the autonomy and agency of
   grantees? How can funder requirements for reporting support genuine
   processes of critical assessment without overburdening grant recipients?
   In this policy and practice note, the authors examine these and other
   dilemmas drawing on experiences and examples from funding human rights
   organizations in India, Liberia, and Central Africa. Exploring issues of
   agency, transparency, and power, one comprehensive message emanating
   from grant-making in these largely disparate areas of the world is the
   imperative need for funders to establish long-term and transparent
   relationships with grantees. Lessons learned from over a decade of
   funding local and national human rights organizations suggest that from
   this solid foundation built on mutual trust and respect can grow over
   time more open communication, accountability, and critical assessment.
   This policy and practice note analyses tools used to navigate
   funder-grantee relations and tests hypotheses and assumptions in
   situations where grantees are facing various internal and external
   challenges. It also provides some recommendations and further thinking
   for other funders when similarly confronted by power imbalances in the
   funder-grantee relationship.},
Publisher = {OXFORD UNIV PRESS},
Address = {GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND},
