@article{ WOS:000369427200001,
Author = {Biglaiser, Glen and Lee, Hoon and Staats, Joseph L.},
Title = {The effects of the IMF on expropriation of foreign firms},
Journal = {REVIEW OF INTERNATIONAL ORGANIZATIONS},
Year = {2016},
Volume = {11},
Number = {1},
Pages = {1-23},
Month = {MAR},
Abstract = {This paper seeks to explain the determinants of foreign expropriation in
   the developing world. We argue that the International Monetary Fund
   (IMF) helps to reduce the likelihood of nationalization because of the
   direct leverage the Fund holds over borrowers, especially as
   expropriation is a blatant violation of international property rights.
   Using expropriation data from 1961 to 2006, and several different
   measures for the Fund, we find that countries under IMF agreements are
   less likely to nationalize foreign firms. We also show that the Fund's
   influence is greatest when the IMF loan represents a larger share of the
   borrower country's gross domestic product (GDP) as well as in countries
   with weaker political institutions. The takeaway is that IMF continues
   to influence policy choices in the developing world.},
