@article{ WOS:000388838500003,
Author = {Kochenov, Dimitry and Pech, Laurent},
Title = {Better Late than Never? On the European Commission's Rule of Law
   Framework and its First Activation},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2016},
Volume = {54},
Number = {5},
Pages = {1062-1074},
Month = {SEP},
Abstract = {This article first offers an overview of the European Commission's Rule
   of Law Framework, which was adopted in March 2014. The mechanism's
   potential effectiveness and the Commission's reasoning to justify its
   first activation against Poland in January 2016, when it has failed to
   do so against Hungary, are subsequently analyzed. While the Commission
   should be commended for seeking to address increasing rule of law
   backsliding at Member State level, our main submission is that reliance
   on the Rule of Law Framework alone, if only because of its soft and
   discursive nature, will not remedy a situation where systemic violations
   of EU values form part of a governmental plan to set up an `illiberal'
   regime.},
