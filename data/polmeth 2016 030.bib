@article{ WOS:000381520400001,
Author = {Glaurdic, Josip and Vukovic, Vuk},
Title = {Proxy Politics, Economic Protest, or Traditionalist Backlash: Croatia's
   Referendum on the Constitutional Definition of Marriage},
Journal = {EUROPE-ASIA STUDIES},
Year = {2016},
Volume = {68},
Number = {5},
Pages = {803-825},
Month = {JUL},
Abstract = {This article sheds light on the popular sources of opposition to the
   extension of gay rights in Eastern Europe by analysing the results of
   the 2013 Croatian referendum on the constitutional definition of
   marriagethe first referendum of this kind in Europe. Contrary to popular
   interpretations, our aggregate-level analysis reveals that the
   referendum results primarily reflected the pattern of support for the
   two principal electoral blocs, rather than communities' traditionalist
   characteristics or grievances stemming from economic adversity. The
   article thereby stresses the importance of embedding the issue of
   contention over gay rights in Eastern Europe into the context of
   conventional political competition.},
