@article{ WOS:000384662200004,
Author = {Cesari, Jocelyne and Fox, Jonathan},
Title = {Institutional Relations Rather Than Clashes of Civilizations: When and
   How Is Religion Compatible with Democracy?},
Journal = {INTERNATIONAL POLITICAL SOCIOLOGY},
Year = {2016},
Volume = {10},
Number = {3},
Pages = {241-257},
Month = {SEP},
Abstract = {This study develops and examines the concept of hegemonic religion and
   its relationship with democracy. A religion is hegemonic not only when
   the state grants that religion exclusive material and political
   privileges and benefits, but also when the religion is a core element of
   national identity and citizenship. We empirically examine the link
   between hegemonic religion and democracy using the Religion and State
   round 2 (RAS2), Polity, and CIRI datasets. We specifically use religious
   education policy, financing of religion, and religiously based laws as
   measures of the extent of religious hegemony in a state. We find that
   the presence of these religiously hegemonic traits, especially in
   combination, is strongly associated with a lack of democracy. However,
   it is possible for democracies to have some hegemonic features but not
   all of them.},
