@article{ WOS:000383445100007,
Author = {Gentry, Caron E.},
Title = {Feminist Christian Realism: VULNERABILITY, OBLIGATION AND POWER POLITICS},
Journal = {INTERNATIONAL FEMINIST JOURNAL OF POLITICS},
Year = {2016},
Volume = {18},
Number = {3},
Pages = {449-467},
Month = {SEP},
Abstract = {While Christianity and feminism may seem at odds with one another, both
   make normative claims about justice and addressing the needs of those on
   the margins of power. This article explores what feminism contributes to
   Christian realism. The current revival of Niebuhrian Christian realism
   highlights how much it still has to offer as a theoretical underpinning
   for policy and governance. However, Christian realism remains wedded to
   masculinist abstractions and power structures, such as the balance of
   power, that are ultimately harmful to those on the margins. Thus, this
   article uses feminism to argue for a greater acceptance of vulnerability
   and obligation in Christian realism.},
