@article{ WOS:000383996400004,
Author = {Bange, Oliver},
Title = {Onto the Slippery Slope: East Germany and East-West Detente under
   Ulbricht and Honecker, 1965-1975},
Journal = {JOURNAL OF COLD WAR STUDIES},
Year = {2016},
Volume = {18},
Number = {3},
Pages = {60-94},
Month = {SUM},
Abstract = {This article identifies and explains the fundamental shift of political
   and ideological paradigms that drew the Soviet Union's close ally, East
   Germany, into the detente process. Although economic and political
   influences and pressures, including from the Soviet Union itself, pushed
   the East German Communist regime to participate in this era of peaceful
   coexistence, officials in East Berlin were well aware of the dangers
   this posed to the Communist society in the German Democratic Republic
   (GDR) in the long term. Even at this early stage of East-West
   interaction, detente left the GDR with the unenviable task of squaring
   ideology with realpolitika task that East German leaders found
   increasingly hard to cope with.},
