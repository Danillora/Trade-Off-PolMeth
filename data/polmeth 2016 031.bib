@incollection{ WOS:000385037300010,
Author = {Rozenboym, Anna V.},
Editor = {Schnee, E and Better, A and Cummings, MC},
Title = {Incorporating Civic Engagement in the Human Anatomy and Physiology
   Course},
Booktitle = {CIVIC ENGAGEMENT PEDAGOGY IN THE COMMUNITY COLLEGE: THEORY AND PRACTICE},
Series = {Education Equity Economy},
Year = {2016},
Volume = {3},
Pages = {115-132},
Abstract = {Incorporating civic engagement in science courses often presents a
   challenge of seamless integration of real-life issues and course
   content. This chapter offers an overview and a model for contextualizing
   human anatomy and physiology concepts, making them relevant to students'
   lives and therefore encouraging a student-centered and student-driven
   inquiry into causes, consequences, and potential solutions to
   health-related issues faced by society. A series of assignments ranging
   from rather simple investigations into students' perceptions of the
   usefulness and meaningfulness of civic engagement in a biology course to
   group presentations on food-related social issues and the
   pathophysiological basis of disease are illustrated and supplemented
   with examples of student work. Not only were these assignments designed
   to engage and motivate students to study the subject matter of the
   course, but also, perhaps more importantly, to empower students to
   become educated and literate consumers of the health-care industry,
   thoughtful and compassionate health-care providers, and engaged citizens
   aware of social issues as well as capable of advocacy.},
