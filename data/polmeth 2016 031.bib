@article{ WOS:000385611700004,
Author = {Tsiapa, Maria},
Title = {Spatial Heterogeneity and Economic Dualism in European Neighbourhood
   Policy Countries},
Journal = {EUROPE-ASIA STUDIES},
Year = {2016},
Volume = {68},
Number = {7},
Pages = {1197-1219},
Abstract = {This article studies how spatial heterogeneity (by urban concentration)
   and economic heterogeneity (by economic dualism), which are being
   affected by trade integration with the EU, influence the growth of
   European Neighbourhood Policy countries (ENPs). It is proved that urban
   concentration is not always related to growth. Furthermore, the eastern
   and southern areas of the ENPs represent two different cases of
   heterogeneity. In the eastern ENPs the pattern of heterogeneity seems to
   generate more benefits for the metropolitan region and the country in
   comparison with the southern ENPs in which population accumulations do
   not function as critical foundations of their development process.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
