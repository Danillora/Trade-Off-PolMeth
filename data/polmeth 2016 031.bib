@article{ WOS:000388643200006,
Author = {Carnaghan, Ellen},
Title = {From Balcony to Barricade: Nationalism and Popular Mobilisation in
   Georgia, Ukraine, and Russia},
Journal = {EUROPE-ASIA STUDIES},
Year = {2016},
Volume = {68},
Number = {9},
Pages = {1579-1607},
Month = {NOV},
Abstract = {This article examines the dynamics of popular mobilisation in autocratic
   post-communist regimes, focusing on how opposition groups manoeuvre
   around cultural and structural constraints to inspire ordinary citizens
   to join street protests, despite the real dangers associated with
   political action. Using evidence from original interviews with activists
   in Georgia, Ukraine, and Russia, the article analyses the comparative
   challenges facing Russian activists in constructing new collective
   identities for the people they hope will become their followers, through
   the use of an inclusive, civic nationalism and a narrative that links
   action in the present to an ongoing, shared historical process.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
