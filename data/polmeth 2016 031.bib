@article{ WOS:000377048000002,
Author = {Waterbury, Myra A.},
Title = {National Minorities and Intra-Ethnic Coordination in the European
   Parliament: Evidence from Central and Eastern Europe},
Journal = {EUROPE-ASIA STUDIES},
