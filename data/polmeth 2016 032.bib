@article{ WOS:000390024800004,
Author = {Evans, Allison D.},
Title = {Protest Patterns in Provincial Russia: a Paired Comparison of Company
   Towns},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
