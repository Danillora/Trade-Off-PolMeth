@article{ WOS:000374335700006,
Author = {Bolton, Sharon and Laaser, Knut and Mcguire, Darren},
Title = {Quality Work and the Moral Economy of European Employment Policy},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
