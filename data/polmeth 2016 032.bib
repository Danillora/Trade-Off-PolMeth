@article{ WOS:000381162200005,
Author = {Schiff, Jennifer S.},
Title = {Masquerading as Compliance: Tracing Canada's Policy Implementation of
   the Human Right to Water},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2016},
Volume = {8},
Number = {2},
Pages = {264-283},
Month = {JUL},
Abstract = {A number of international agreements support the legitimacy of the human
   right to water, and the `fulfilment' requirement of that right compels
   states to provide equal water access for all citizens. Canada's policy
   evolution in this respect is instructive for practitioners, as it allows
   a study of the challenges in providing marginalized groups with clean
   water. Through a case study approach, this article examines Canada's
   implementation of the human right to water and finds that Canada appears
   to be complying with its international water rights commitments, as
   almost concurrently to its acceptance of the international norm in 2012,
   Canada created a domestic law to address the deficiencies of water
   provision within its aboriginal communities. Tracing Canada's actual
   practice of the human right to water reveals, however, that the
   government's compliance with that right has had a much more ambiguous
   outcome. In this particular case, Canada agreed to an international
   human rights norm with which its decentralized governance structure was
   incompatible, suggesting that while international pressure to adopt a
   norm is a powerful force, that pressure may be misplaced if the state
   has limited capacity to enforce the norm domestically. Ultimately,
   Canadian implementation of the human right to water suggests that while
   the boomerang model of norm diffusion may be successful in pressuring
   states to make a tactical concession and adopt norms internationally,
   such acceptance can then masquerade as compliance and instead serve as
   an evasive manoeuvre on the domestic level, thus allowing the state to
   continue to marginalize the human right to water within its own internal
   policies.},
