@article{ WOS:000373614600002,
Author = {Debuysere, Loes},
Title = {Tunisian Women at the Crossroads: Antagonism and Agonism between Secular
   and Islamist Women's Rights Movements in Tunisia},
Journal = {MEDITERRANEAN POLITICS},
Year = {2016},
Volume = {21},
Number = {2},
Pages = {226-245},
Month = {MAY 3},
Abstract = {The recent rise in Islamist-inspired women's activism is posing
   challenges to the longstanding secular women's movements in post-Ben Ali
   Tunisia. Starting from the conviction that cohesive, cross-class women's
   coalitions are better suited to achieve gender justice for women of all
   walks of life, this article draws on the concept of `agonistic
   pluralism' (Chantal Mouffe) to understand how Tunisia's women's
   movements can deal with the new, multifaceted conflict in their ranks.
   Through a discussion of the `Dialogue of Tunisian Women', the grounds
   for strategic coalition-building and `agonistic' engagement between
   secular and Islamist women's rights actors are illustrated.},
