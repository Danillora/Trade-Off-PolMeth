@article{ WOS:000387694700004,
Author = {Sallaz, Jeffrey J. and Wang, Chi Phoenix},
Title = {Sumptuary Labor: How Liberal Market Economies Regulate Consumption},
Journal = {POLITICS \& SOCIETY},
Year = {2016},
Volume = {44},
Number = {4},
Pages = {551-572},
Month = {DEC},
Abstract = {Liberal market states promote the responsible consumption of potentially
   dangerous commodities. But the work of enforcing sumptuary law is in
   fact delegated to service employees in the private sector. In this
   article such work is termed sumptuary labor. Although the ability of
   states to privatize sumptuary enforcement is a remarkable
   accomplishment, it is by no means a seamless one. Drawing on
   ethnographic fieldwork among bartenders and casino dealers, the article
   elaborates patterned conflicts of interest that arise during the
   performance of sumptuary labor. Both firms and workers may fail to
   ensure eligibility for consumption, prevent overindulgence, or guarantee
   the purity of the problematic commodity. The article concludes by
   theorizing how liberal market states manage the potential decoupling of
   sumptuary policy and practice.},
