@article{ WOS:000382206100005,
Author = {Cioffi, John W. and Dubin, Kenneth A.},
Title = {Commandeering Crisis: Partisan Labor Repression in Spain under the Guise
   of Economic Reform},
Journal = {POLITICS \& SOCIETY},
