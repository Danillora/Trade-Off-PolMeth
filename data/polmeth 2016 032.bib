@article{ WOS:000394356900004,
Author = {Braat, Eleni},
Title = {Recurring Tensions between Secrecy and Democracy: Arguments about the
   Security Service in the Dutch Parliament, 1975-1995},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2016},
Volume = {31},
Number = {4},
Pages = {532-555},
Abstract = {There is a recurring tension between secrecy and democracy. This article
   analyzes the continually ambiguous relations between intelligence and
   security agencies and their parliamentary principals. I present a novel
   conceptual framework to analyze political relations influenced by
   secrecy. I draw on Albert Hirschman's concepts of exit, voice and
   loyalty and Max Weber's ideal types of the ethics of conviction and
   responsibility. The focus is a case study of the Dutch parliament and
   Security Service between 1975 and 1995. The analysis demonstrates how
   parliament can deal constructively with the secret services. This
   depends both on party-political responses to secrecy and strategic
   responses on the part of the secret services to deteriorating
   relationships with parliament.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
