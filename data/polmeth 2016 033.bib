Author = {Beckett, Megan K. and Elliott, Marc N. and Gaillot, Sarah and Haas, Ann
   and Dembosky, Jacob W. and Giordano, Laura A. and Brown, Julie},
Title = {ESTABLISHING LIMITS FOR SUPPLEMENTAL ITEMS ON A STANDARDIZED NATIONAL
   SURVEY},
