@article{ WOS:000378140700007,
Author = {Fabrykant, Marharyta and Buhr, Renee},
Title = {Small state imperialism: the place of empire in contemporary nationalist
   discourse},
Journal = {NATIONS AND NATIONALISM},
Year = {2016},
Volume = {22},
Number = {1},
Pages = {103-122},
Month = {JAN},
Abstract = {Many modern European nations can trace their heritage back to one of the
   large multinational empires that once encompassed much of the European
   landscape, and nationalising elites often refer back to their place in
   these empires for the materials upon which their nation was purportedly
   built. In this article we examine some Belarusian nationalising elites
   and their references to the Grand Duchy of Lithuania in order to
   demonstrate a recent trend in East European small-state national
   identity construction, which we refer to as `small state imperialism.'
   Small state imperialism exhibits realist characteristics and paints the
   small nation's place in empires of the past as privileged and
   aggressive, and in this way deviates from the oppressed but morally
   superior image one typically expects of a small nation. This
   interpretation is not limited to Belarus; in a number of East European
   states a similar imperialist turn has taken root in nationalist
   discourses.},
