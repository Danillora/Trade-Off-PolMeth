@article{ WOS:000379544200008,
Author = {Schmeier, Susanne and Gerlak, Andrea K. and Blumstein, Sabine},
Title = {Clearing the muddy waters of shared watercourses governance:
   conceptualizing international River Basin Organizations},
Journal = {INTERNATIONAL ENVIRONMENTAL AGREEMENTS-POLITICS LAW AND ECONOMICS},
