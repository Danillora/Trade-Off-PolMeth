@article{ WOS:000387242900004,
Author = {Tudoroiu, Theodor},
Title = {Unfreezing Failed Frozen Conflicts: A Post-Soviet Case Study},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2016},
Volume = {24},
Number = {3},
Pages = {375-396},
Abstract = {The dispute for regional influence between Russia and the European Union
   has resulted in dramatic events in Ukraine. Less visibly, a similar
   situation has developed in the southern Moldovan autonomous region of
   Gagauzia. In the context of Moldova's signing of an EU Association
   Agreement in June 2014, calls were made to take up arms and erect
   barricades. Independence was to be proclaimed one day after - and
   because of - that signing. This was avoided, but the crisis is far from
   coming to an end. Using the concept of failed frozen conflict, this
   article argues that tensions mainly are due to local leaders trying to
   take advantage of the new international situation in order to promote
   their own group and private interests. This endangers seriously the
   stability and security of both Gagauzia and the Republic of Moldova.},
