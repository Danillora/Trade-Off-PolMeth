@article{ WOS:000382848600005,
Author = {Arjona, Ana},
Title = {Institutions, Civilian Resistance, and Wartime Social Order: A
   Process-driven Natural Experiment in the Colombian Civil War},
Journal = {LATIN AMERICAN POLITICS AND SOCIETY},
Year = {2016},
Volume = {58},
Number = {3},
Pages = {99-122},
Month = {FAL},
Abstract = {Why do armed groups fighting in civil wars establish different
   institutions in territories where they operate? This article tests the
   mechanisms of a theory that posits that different forms of wartime
   social order are the outcome of a process in which an aspiring ruleran
   armed groupexpands the scope of its rule as much as possible unless
   civilians push back. Instead of being always at the mercy of armed
   actors, civilians arguably have bargaining power if they can credibly
   threaten combatants with collective resistance. Such resistance, in
   turn, is a function of the quality of preexisting local institutions.
   Using a process-driven natural experiment in three villages in Central
   Colombia, this article traces the effects of institutional quality on
   wartime social order.},
