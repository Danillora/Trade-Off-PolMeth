@article{ WOS:000387240800006,
Author = {Tzortzis, Ioannis},
Title = {Mirror Images: The Greek (1973) and the Spanish (1977) Reformas
   Pactadas in Comparison},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2016},
Volume = {24},
Number = {1},
Pages = {101-116},
Abstract = {Departing from a literature comparing the Spanish reforma pactada of
   1977 with the Greek transition by regime collapse of 1974, the article
   compares for the first time the Spanish transition with what might have
   been its Greek counterpart: the failed regime transformation attempt of
   1973. Based on the comparison of the natures and structures of the two
   dictatorial regimes, the conjunctures at the start of the transitions,
   and the tactics of actors (regime elites and counter-elites) during the
   transition time, it concludes that the Greek case is the `mirror image'
   of the Spanish reforma in that all above factors worked exactly the
   opposite way from Spain.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
