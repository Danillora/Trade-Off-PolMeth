@article{ WOS:000387321300004,
Author = {Freudenreich, Johannes},
Title = {The Formation of Cabinet Coalitions in Presidential Systems},
Journal = {LATIN AMERICAN POLITICS AND SOCIETY},
Year = {2016},
Volume = {58},
Number = {4},
Pages = {80-102},
Month = {WIN},
Abstract = {Cabinet coalitions are central to the functioning of Latin American
   presidential systems. However, the reasons for their formation remain
   unclear. While recent studies suggest that presidents invite parties to
   the cabinet to facilitate governability and lawmaking, this study argues
   that the composition of cabinet coalitions is largely predetermined by
   commitments made before presidential elections. To analyze this
   argument, the study introduces the conditional logit model as a new
   empirical strategy for modeling cabinet choice under this type of
   regime. Based on a new dataset of 107 cabinets in 13 Latin American
   democracies, the study shows that pre-electoral commitments strongly
   affect cabinet formation and thereby also confound the relationship
   between cabinet formation and governability.},
