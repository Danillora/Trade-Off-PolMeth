@incollection{ WOS:000411729200010,
Author = {Wan, Hooi Lai},
Book-Author = {Wan, HL},
Title = {Organisational Justice-Job Satisfaction-Organisational Citizenship
   Behaviour},
Booktitle = {ORGANISATIONAL JUSTICE AND CITIZENSHIP BEHAVIOUR IN MALAYSIA},
