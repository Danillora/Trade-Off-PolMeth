@incollection{ WOS:000401334000001,
Author = {Bates, Robert H.},
Book-Group-Author = {Annual Review},
Title = {Politics, Academics, and Africa},
Booktitle = {ANNUAL REVIEW OF POLITICAL SCIENCE, VOL 20},
Series = {Annual Review of Political Science},
Year = {2017},
Volume = {20},
Pages = {1-14},
Abstract = {The roots of my fascination with politics and Africa run deep; so too
   does my need for clarity. The combination drove me into the
   professoriate. My research in Africa convinced me that modernization
   theory was wrong: The people I came to know in the field were
