@article{ WOS:000404718600005,
Author = {Lisoni, Carlos M.},
Title = {Contributions to a Typology of Clientelistic Brokers},
Journal = {JOURNAL OF POLITICS IN LATIN AMERICA},
Year = {2017},
Volume = {9},
Number = {1},
Pages = {125-147},
Abstract = {This research note uses two case studies of Argentine provinces to
   develop a typology of clientelistic brokers. I identify a dirigente-type
   broker, who acts as an advocate of voters, and a puntero-type broker,
   who acts as a delegate of the patron. The clear differences between
   these two broker types point to greater diversity among clientelistic
   practices (such as monitoring and enforcing clientelistic exchanges)
   than is commonly acknowledged. I provide original evidence from 34
   open-ended interviews with local- and provincial-level elected officials
   that accounts for patron-broker dynamics in small, poor, and peripheral
   Argentine communities.},
