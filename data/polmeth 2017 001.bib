@article{ WOS:000413973300003,
Author = {Harrison, Laura S.},
Title = {`THAT FAMOUS MANIFESTO': THE DECLARATION OF ARBROATH, DECLARATION OF
   INDEPENDENCE, AND THE POWER OF LANGUAGE},
Journal = {SCOTTISH AFFAIRS},
