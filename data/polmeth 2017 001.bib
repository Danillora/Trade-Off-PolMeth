@article{ WOS:000405144100002,
Author = {Saati, Abrak},
Title = {Constitution-Building Bodies and the Sequencing of Public Participation
   A Comparison of Seven Empirical Cases},
Journal = {JOURNAL OF POLITICS AND LAW},
