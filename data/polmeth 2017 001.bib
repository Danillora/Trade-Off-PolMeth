@article{ WOS:000419290300004,
Author = {Finseraas, Henning and Roed, Marianne and Schone, Pal},
Title = {Labor Market Competition with Immigrants and Political Polarization},
Journal = {QUARTERLY JOURNAL OF POLITICAL SCIENCE},
Year = {2017},
Volume = {12},
Number = {3},
Pages = {347-373},
Abstract = {The political consequences of economic globalization has lately been
   fiercely debated across Europe and the United States, including the role
   of labor immigration. In this paper we study the party choices of voters
   facing labor market competition from immigration. To identify the effect
   of labor market competition we introduce the national skill cell
   approach, which is designed to isolate a direct partial effect of
   immigrant competition. By access to detailed, population-wide,
   administrative data, we get precise measures of Norwegian voters'
   exposure to competition, and we relate this measure to voting behavior
   in five national elections. We find a polarizing effect of immigration
   among voters experiencing negative wage effects of immigration. The
   polarization points to the existence of a protectionist and a
   compensatory response, and we propose that predetermined ideological
   convictions determine the response.},
Publisher = {NOW PUBLISHERS INC},
