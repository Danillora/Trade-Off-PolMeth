@article{ WOS:000419196300005,
Author = {Carcamo Hun, Rodrigo},
Title = {Chile: considerations on civil control of military intelligence},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
Year = {2017},
Number = {21},
Pages = {70-86},
Month = {DEC},
Abstract = {Intelligence constitutes a key aspect in the security policy of the
   States, both for the civil controls to which it must be subject, which
   will ensure the stability of democracy; as for its technical and
   operational contribution to other areas that have taken relevance in the
   globalized world. This paper is an exploratory review of the concepts
   that apply to the subject of control that the civil power should apply
   on the military intelligence function, focused on the case of Chile and
   its recent State Intelligence System, established and regulated from the
   year 2004 by Law 19.974. Also, it is intended to make explicit some
   deficiencies in the intelligence model, in general, and in military
   intelligence, in particular, focused on the poor articulation of the
   system, parliamentary control and its development against new threats.},
