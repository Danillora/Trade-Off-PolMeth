@article{ WOS:000417507700006,
Author = {Antonopoulos, Paul},
Title = {The Kangaroo, the Bear, and the Dragon Australia-Russia-China Relations
   in the ``Asian Century{''}},
Journal = {CHINA QUARTERLY OF INTERNATIONAL STRATEGIC STUDIES},
