@article{ WOS:000422892700012,
Author = {Heupel, Monika},
Title = {Contested responsibility attribution: Extraterritorial human rights
   violations in the US campaign against terrorism},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Number = {52},
Pages = {244+},
Abstract = {The article develops a simple model of contested responsibility
   attribution for extraterritorial human rights violations which is based
   on mechanisms of responsibility attribution and corresponding defense
   mechanisms. The article applies the model to the conflict on two
   extraterritorial rights violations that have occurred in the context of
   the US campaign against terrorism, namely torture and mass surveillance
   of foreigners abroad. The article shows that the model can both explain
   why states gradually take on responsibility in a new issue area (right
   to privacy) and why states anew take on responsibility for the
   protection of a human right that they have temporarily de facto
   denounced (right to freedom from torture).},
