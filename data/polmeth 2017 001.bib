@article{ WOS:000419320200006,
Author = {Nelson, Paul},
Title = {Citizens, consumers, workers, and activists: civil society during and
   after water privatization struggles},
Journal = {JOURNAL OF CIVIL SOCIETY},
Year = {2017},
Volume = {13},
Number = {2},
Pages = {202-221},
Abstract = {Water privatization battles and their aftermath in seven Latin American,
   African, and Asian countries feature diverse and often complementary
   forms of action by unions, popular movements, neighbourhood
   associations, and non-governmental organizations (NGOs). NGOs cooperate
   with popular movements for water rights more than the literature would
   suggest, and after mobilizations against privatization, activists have
   had some success maintaining pressure for better service to poor
   neighbourhoods through advocacy and local self-provision arrangements.
   Public employees' unions play important roles in water policy
   coalitions, often practising a distinctive form of social movement
   unionism. These experiences argue for a more flexible understanding of
   categories and organizational forms in civil society.},
