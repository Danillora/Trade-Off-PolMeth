@article{ WOS:000396653400005,
Author = {Arnold, Samuel and Harris, John R.},
Title = {What is arbitrary power?},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2017},
Volume = {10},
Number = {1},
Pages = {55-70},
Abstract = {Republicans define domination as subjection to arbitrary power. But what
   is arbitrary power? We consider three views. According to the first,
   championed recently by Frank Lovett, power is arbitrary insofar as it is
   unconstrained. According to the second, advanced most prominently by
   Philip Pettit in his recent work, power is arbitrary insofar as it is
   uncontrolled by those subject to it. According to the third, found
   (among other places) in Pettit's early work, power is arbitrary insofar
   as it is not forced to track the interests of those subject to it. We
   advance several objections against each of the first two views and offer
   support for the third. Pettit, we might say, got it right the first
   time.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
