@article{ WOS:000398866000002,
Author = {Martinez-Palacios, Jone},
Title = {Democratizing participation through feminism. The role of feminist
   subaltern counterpublics in the expansion of the Basque public sphere},
Journal = {REVISTA ESPANOLA DE CIENCIA POLITICA-RECP},
Year = {2017},
Number = {43},
Pages = {37-59},
Month = {MAR},
Abstract = {This article aims to understand the limits on the expansion of the
   public space that is occurring through democratic innovations, and to
   investigate strategies for overcoming these limits. With an approach
   rooted in standpoint epistemology, this article studies the
