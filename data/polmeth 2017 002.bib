@article{ WOS:000403933800002,
Author = {Pruitt, Lesley and Hamilton, Gemma and Heydon, Georgina and Spark,
   Ceridwen},
Title = {Abbott's ``budget crisis', CALD women's loss? Service providers explore
   the impact of funding cuts},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2017},
Volume = {52},
Number = {3},
Pages = {335-350},
Abstract = {Women have been disproportionately affected by funding cuts to services
   following the 2008 global financial crisis. Using a feminist
   intersectional analysis of austerity measures applied to family violence
   (FV) services in Victoria, Australia, including 11 service provider
   interviews, we find that: the Australian government drew on global
   narratives of austerity in constructing a budget crisis', with
   subsequent cuts to funds addressing FV; budget cuts negatively impacted
   services' abilities to address the needs of culturally and
   linguistically diverse (CALD) women; there is a lack of qualified
   interpreters and multilingual services, and; there are missed
   opportunities to engage and support young CALD people in FV services.
   Policy recommendations to support FV services and their abilities to
   help CALD women and young people are provided.},
