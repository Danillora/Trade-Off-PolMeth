@article{ WOS:000408559500002,
Author = {Ridolfi, Maurizio},
Title = {Republic of the future ! Projects and republican horizons in Italy from
   the Risorgimento to the decline of Monarchy},
Journal = {LABORATOIRE ITALIEN-POLITIQUE ET SOCIETE},
Year = {2017},
Number = {19},
Abstract = {Looking at the history of the Italian republicanism and his
   political-institutional representations at the time of the Monarchy
   (liberal and fascist), means to think to it as a civil and political
   passion. The republican tradition had a great importance in the
   political thought and in the social action. Through the thought and the
   inheritance of Giuseppe Mazzini and Carlo Cattaneo, the republican
   concepts are reconsidered, so the models and the principal public
   narrations, from the Republic of Rome and Venice (1848-1849) to the
   birth of the Republic of the Italians (1946).},
