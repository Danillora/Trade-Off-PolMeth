@article{ WOS:000402780000010,
Author = {Schuster, Dirk},
Title = {``European Culture{''} and its ``Eastern Borders{''}},
Journal = {OSTEUROPA},
Year = {2017},
Volume = {67},
Number = {1-2},
Pages = {121+},
Abstract = {Hans Heinrich Schaeder is considered an important Iranist and historian
   of religion. For reasons of opportunism, careerism, and anti-Semitic
   resentment, he used the chance afforded him after the National
   Socialists seized power in Germany: he combined his historical and
   philological knowledge with National-Socialist racial ideology. Drawing
   on the superiority of ``Aryanism{''} he derived from this merger,
   Schaeder tried to redefine the ``Eastern Borders{''} of ``European
