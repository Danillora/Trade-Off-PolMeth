@article{ WOS:000422892700005,
Author = {Neuhaeuser, Christian},
Title = {Collective Responsibility as a Political Term in the Debate on
   Historical Injustice},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Number = {52},
Pages = {81+},
Abstract = {The article carves out the deeply political character of the concept of
   responsibility in the context of questions of historical justice. The
   starting point for the inquiry is the assumption that according to the
   use theory of meaning any conceptualization of responsibility can have
   considerable consequences for its practical use. I engage the very
   different conceptions of responsibility for historical injustice
   provided by Julian Nida-Rumeln, Margaret Gilbert and Michael Schefczyk
   and discuss their positions on collective guilt. It thereby becomes
   visible that the differences between their positions cannot be resolved
   analytically, because those differences are political in nature.},
