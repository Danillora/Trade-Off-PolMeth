@article{ WOS:000396406700029,
Author = {Ghasemi, Omid and Aghamiri, Seyed Mohammad},
Title = {Dispute Settlement in Iran's Stock Market},
Journal = {JOURNAL OF POLITICS AND LAW},
Year = {2017},
Volume = {10},
Number = {1},
Pages = {245-251},
Month = {JAN},
Abstract = {Arbitration has long been considered by people to settle disputes.
   Moreover, due to the specific advantages of this method, it has a
   particular place in different legal systems and it has been attempted to
   use it in important contexts, even family disputes. Nowadays,
   arbitration is a significant alternative to settle disputes in a
   business context. Exchange is one of the crucial fields of domestic and
   foreign trade. In this regard, the legislature with the goal of using
   arbitration advantages such as speed, accuracy and expertise, has
   considered this method to settle exchanged disputes and introduced a
   committee called ``jury{''} in Securities Market Act enacted in 2005.
   But it has imposed criteria and rules that made it distinctive from
   arbitration. This study aimed to study this committee and clarify its
   legal nature and jurisdiction.},
