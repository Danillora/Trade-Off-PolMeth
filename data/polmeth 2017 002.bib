@article{ WOS:000413859200002,
Author = {Weyland, Kurt},
Title = {Fascism's missionary ideology and the autocratic wave of the interwar
   years},
Journal = {DEMOCRATIZATION},
Year = {2017},
Volume = {24},
Number = {7, SI},
Pages = {1253-1270},
Abstract = {Because fascism embodied a missionary creed, it exemplifies the
   ideology-driven cooperation and diffusion among autocratic regimes that
   the Introduction to this special issue discussed. The messianic promise
   that forceful charismatic leadership sustained by fervent mass support
   would guarantee domestic order and restore national grandeur was
   attractive to innumerable citizens, important political elites, and even
   to prominent intellectuals across the globe. Consequently, fascism
   exuded strong, wide-ranging contagion and demonstration effects.
   Moreover, Mussolini and Hitler actively promoted the new credo and
   sought support for their expansionary tendencies.},
