@article{ WOS:000401730900007,
Author = {Legrenzi, Matteo and Lawson, Fred H.},
Title = {The changing role of the Gulf in the international political economy},
Journal = {GLOBAL CHANGE PEACE \& SECURITY},
Year = {2017},
Volume = {29},
Number = {2},
Pages = {189-199},
Abstract = {In the past half-decade, the role of the Gulf in the international
   political economy has changed dramatically. The region's position as a
   supplier of world hydrocarbons has slipped, even as local consumption of
   oil and gas continues to expand. Gulf investments have shifted from the
   industrialized countries to the Middle East and North Africa. Saudi
   Arabia no longer exercises disproportionate influence in the Group of
   20. Finally, relations with the People's Republic of China and India
   have become truly interdependent, which gives the Gulf the capacity to
   exercise leverage over these two rising powers, despite its diminished
   position in global affairs.},
