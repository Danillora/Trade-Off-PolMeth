@article{ WOS:000438275400001,
Author = {Aytac, S. Erdem},
Title = {Policy Priorities in the Election Manifestos of Political Parties in
   Turkey, 2002-2015},
Journal = {SIYASAL-JOURNAL OF POLITICAL SCIENCES},
Year = {2017},
Volume = {26},
Number = {2},
Pages = {7-26},
Abstract = {The policy areas prioritized by the major Turkish political parties
   during the election campaigns between 2002 and 2015 are examined in this
   study. The main questions to be addressed are as follows: Which policy
   areas do the political parties prioritize? Is there a significant
   divergence in the prioritization of different policy areas or are there
   common themes that are emphasized by all parties? Are there trends
   regarding the parties' policy priorities over time? And finally, do the
   policy areas, as prioritized by the parties, correspond to what the
   public considers as important issues? To address these questions, the
   election manifestos of the four major Turkish political parties are
   analyzed. The analyses show that policy areas related to the economy,
   welfare, and quality of life are the most recurring themes in the
   election manifestos of the four major Turkish parties between 2002 and
   2015.},
