@article{ WOS:000405667700005,
Author = {Lopez Guerra, Luis},
Title = {The European Court of Human Rights, the Court of Justice of the European
   Union and ``le mouvement necessaire des choses{''}},
Journal = {TEORIA Y REALIDAD CONSTITUCIONAL},
Year = {2017},
Volume = {39},
Pages = {163-188},
Abstract = {The progressive development of two systems of international protection
   of Human Rights in Europe, one derived from the European Convention on
   Human Rights and the other from the Charter of Fundamental Rights of the
   European Union, requires coordination, particularly between the two main
   jurisdictional bodies of those systems, the Strasbourg and Luxembourg
   Courts. This article asserts that in spite of the lack of formal
   mechanisms of coordination due to the EU's non-accession (for the time
   being) to the Convention, coordination is nevertheless achieved through
   a practical concordance between the case law of both courts, which in a
   process of both formal and informal dialogue is defining the scope of
   the two Courts' respective jurisdictions.},
