@article{ WOS:000398137300001,
Author = {Sauer, Birgit},
Title = {Social-theoretical reflections on European right-wing populism. The
   analytical potential of the category gende},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Volume = {58},
Number = {1},
Pages = {3-22},
Abstract = {The article analyses the causes for the recent success of right-wing
   populist parties and movements from a gender perspective. The article
   aims first at a deeper understanding of right-wing populism and second
   it wants to demonstrate the analytical potential of gender as a
   political science category. While gender ideologies have always been
   crucial for extreme right parties and groups, only recently a
   gender-based strategy moved to the forefront of (European) right-wing
   populist actors. Based on an `anti-gender' strategy right-wing populists
   aim at contributing to the resolution of a `crisis of masculinity' in
   western societies as well as antidiscrimination policies, implemented
   since they prepare their anti-hegemonic project against equal
   opportunity and anti-discrimination since the 1960s and 1970s.},
