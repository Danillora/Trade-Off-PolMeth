@article{ WOS:000400252500011,
Author = {Angell, Kim and Huseby, Robert},
Title = {Should Irregular Immigrants Be (Rapidly) Enfranchised?},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2017},
Volume = {70},
Number = {2},
Pages = {363-373},
Month = {JUN},
Abstract = {Theories of voting rights differ quite sharply with regard to whether or
   not they support (rapid) enfranchisement of irregular immigrants. In
   this paper, we first outline these theories and their implications. We
   then assess a number of reasons against rapidly enfranchising irregular
   immigrants. We find, on reflection, that none of these reasons are
   persuasive. While this result is not in itself sufficient to draw strong
   conclusions, it does offer some support to the more inclusive theories
   of voting rights, and poses a challenge to the less inclusive ones.},
