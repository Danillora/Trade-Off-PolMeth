@article{ WOS:000416491700007,
Author = {Kelly, Roger},
Title = {ACHIEVING SUSTAINABLE GROWTH THROUGH ECONOMIC INCLUSION IN TURKEY},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2017},
Volume = {16},
Number = {2},
Pages = {67-75},
Month = {SUM},
Abstract = {Turkey has faced a number of challenges in the recent past, but the
   government has acted swiftly and successfully to limit the impact on the
   economy. However, in order to generate sustainable growth in the
   country, structural reforms are required to enhance the competitiveness
   of the economy. This article looks at how Turkey can take advantage of
   its significant human capital potential in order to improve
   competitiveness and attract foreign direct investment (FDI), and details
   activities the European Bank of Reconstruction and Development (EBRD) is
   undertaking in the area of inclusion to help the country build a
   sustainable growth model.},
