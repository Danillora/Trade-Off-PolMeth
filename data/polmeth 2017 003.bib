@article{ WOS:000424980200001,
Author = {Enns, Peter K. and Kelly, Nathan J. and Masaki, Takaaki and Wohlfarth,
   Patrick C.},
Title = {Moving forward with time series analysis},
Journal = {RESEARCH \& POLITICS},
Year = {2017},
Volume = {4},
Number = {4},
Month = {OCT 4},
Abstract = {In a recent Research and Politics article, we showed that for many types
   of time series data, concerns about spurious relationships can be
   overcome by following standard procedures associated with cointegration
   tests and the general error correction model (GECM). Matthew Lebo and
   Patrick Kraft (LK) incorrectly argue that our recommended approach will
   lead researchers to identify false (i.e., spurious) relationships. In
   this article, we show how LK's response is incorrect or misleading in
   multiple ways. Most importantly, when we correct their simulations,
   their results reinforce our previous findings, highlighting the utility
   of the GECM when estimated and interpreted correctly.},
