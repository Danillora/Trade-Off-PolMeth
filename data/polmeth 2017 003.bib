@article{ WOS:000398203900011,
Author = {Schwarz, Daniel and Traber, Denise and Benoit, Kenneth},
Title = {Estimating Intra-Party Preferences: Comparing Speeches to Votes},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
