@article{ WOS:000424170800002,
Author = {Bartu, Peter},
Title = {What Mandate? Mediating During Warfighting in the Libyan Revolution
   (2011)},
Journal = {AFRICAN SECURITY},
Year = {2017},
Volume = {10},
Number = {3-4},
Pages = {176-191},
Abstract = {The United Nation's mediation mandate evolved during the Libyan
   revolution and was hampered by inconsistent support for the mediation,
   by other mediation initiatives, including that of the African Union, and
   by a proliferation of envoys and transition plans. When a battlefield
   stalemate obliged the international community to agree on a political
   mandate for mediation led by United Nations Special Envoy Abdelelah
   al-Khatib, the challenge was to persuade Gaddafi to accept a transition
   in which he would have no role. Gaddafi would not consent to this, and
   the rebels had little interest in a mediated transition that included
   him.},
