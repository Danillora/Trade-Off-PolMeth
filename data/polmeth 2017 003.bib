@article{ WOS:000413898500005,
Author = {Villanueva Rivas, Cesar},
Title = {Mexico's Public Diplomacy Approach to the Indo-Pacific: A Thin Soft
   Power?},
Journal = {POLITICS \& POLICY},
Year = {2017},
Volume = {45},
Number = {5, SI},
Pages = {793-812},
Month = {OCT},
Abstract = {This article argues that Mexico has a thin public diplomacy approach to
   the Indo-Pacific. The reasons are twofold: first, the great weight of
   the United States poses limits on Mexico's foreign policy outside North
   America; and second, the scarce resources have been channeled to soft
   power strategies elsewhere, particularly toward the Indo-Pacific.
   However, China, Japan, South Korea, India, and Australia have better
   chances to engage productively in public diplomacy, and the MIKTA
   groupMexico, Indonesia, the Republic of Korea, Turkey, and
   Australiaoffers certain venues to expand common interests. To develop a
   better appreciation of Mexico's public diplomacy, a brief summary of its
   main features is offered as well as some comparative analysis of
   Mexico's image abroad.},
