@article{ WOS:000423272800003,
Author = {Morissette, Benoit},
Title = {``The Foundations of Freedom and Civilization{''}: The Durham Report,
   the municipal institutions and liberalism},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2017},
Volume = {50},
