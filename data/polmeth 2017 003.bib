@article{ WOS:000406159400003,
Author = {Yenel, Selim},
Title = {RESETTING THE TURKEY-EU RELATIONSHIP},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2017},
Volume = {16},
Number = {1},
Pages = {29-35},
Month = {SPR},
Abstract = {Turkey-EU relations are going through a difficult period, which is not a
   new feature per se as they have been through many different stages.
   However, the present situation is unsustainable. The accession process
   continues to stall due to political obstacles and can proceed only if
   the Cyprus issue is resolved. As accession is the dominant feature, this
   situation poisons the overall relationship. While the migration crisis
   brought the two sides closer, the July 15th coup attempt in Turkey and
   its aftermath again caused consternation. In a world with a multitude of
   challenges, the two sides need each other and should build their
   relationship on trust and realistic expectations.},
