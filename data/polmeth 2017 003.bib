@article{ WOS:000423499000006,
Author = {Hoehmann, Daniel},
Title = {Delegation Problems in Coalition Governments. Committee Chairs as an
   Instrument of Mutual Control of Government Parties in the German Federal
   States},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Volume = {58},
Number = {4},
Pages = {593-617},
Abstract = {As in coalition governments policy-authority for specific policy areas
   is delegated to the cabinet ministers, ministers have incentives to
   implement policies which oppose the preferences of the coalition
   partner. However, government parties can monitor partners by appointing
   committee chairs for those policy areas that are under the
   responsibility of a minister from the coalition partner. For the first
   time, this paper tests the use of this control mechanism for the
   governments of the German federal states. The results show that one
   third of all ministers are controlled by a committee chair from the
   coalition partner. Control takes place in Grand Coalitions between the
   SPD and the CDU, when parties are governing together for the first time,
   and when the number of committee members is small.},
Publisher = {NOMOS},
