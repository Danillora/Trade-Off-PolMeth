@article{ WOS:000397449400004,
Author = {Enroth, Henrik},
Title = {Cartelization versus representation? On a misconception in contemporary
   party theory},
Journal = {PARTY POLITICS},
Year = {2017},
Volume = {23},
Number = {2},
Pages = {124-134},
Month = {MAR},
Abstract = {According to a popular but controversial view in contemporary party
   research, political parties increasingly put a premium on governing
   within the institutions of the state rather than on representation of
   interests and identities in society. This critique has been phrased most
   uncompromisingly in Richard Katz and Peter Mair's theory of the cartel
   party, according to which the cartel party governs but does not
   represent, and thus fails to do what we expect of parties in a modern
   democracy. This article is an analysis and critique of this
   presupposition, which has largely escaped the attention of commentators.
   The idea that the cartel party governs but does not represent rests on
   an untenable view of political representation. As I argue, the normative
   problem with the cartel party is not so much that this type of party
   does not represent, as to decide when and how it does or does not
   represent.},
