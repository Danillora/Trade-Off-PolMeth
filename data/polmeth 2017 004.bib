@article{ WOS:000418715300011,
Author = {Hamidi, Ibrahim},
Title = {SYRIA AT A CROSSROADS: ``A PEACE TO END ALL PEACE{''}?},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2017},
Volume = {16},
Number = {3},
Pages = {113-124},
Month = {FAL},
Abstract = {For the first time in almost seven years, the Syrian conflict is
   approaching what seems to be the start of its conclusion. Syria is
   presently at a crossroads; what happens in the weeks and months ahead
   will determine the Syrian endgame. If change is deep, serious, and
   committed, it might bring hope for a new Syria. If superficial, however,
   it can - and will - lead to an even more explosive situation. In this
   article, the author argues that the outcome will be determined by a
   basket of regional and international influences - particularly Russia
   and the US. Lingering issues include territorial control in the Syrian
   theater, ejecting Iran and Hezbollah from the Syrian battlefield, and
   counter-terrorism.},
