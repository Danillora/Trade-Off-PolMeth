@article{ WOS:000403933800006,
Author = {Malloy, Jonathan},
Title = {Political opportunity structures, evangelical Christians and morality
   politics in Canada, Australia and New Zealand},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2017},
Volume = {52},
Number = {3},
Pages = {402-418},
Abstract = {Politically active evangelical Christian populations are found in
   Canada, Australia and New Zealand. Yet their patterns of activism differ
   not only from the exceptional United States experience, but also one
   another. This paper applies a political opportunity approach to explain
   these variations, demonstrating how differences in denominational
   identities and linkages and political institutions shape evangelical
   activism, and illustrating these opportunity structures through an
   examination of the struggles over same-sex marriage in each country.
   This approach offers a variety of further avenues for the comparative
   study of morality politics' in different countries, moving beyond
   bilateral comparisons with the United States.},
