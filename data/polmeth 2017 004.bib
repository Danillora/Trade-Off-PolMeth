@article{ WOS:000418031300012,
Author = {Miranda, Rafael},
Title = {What has become of the Democracies of the Second Wave? Historical Study
   Compared between Costa Rica, Colombia and Venezuela},
Journal = {CIENCIA POLITICA},
Year = {2017},
Volume = {12},
Number = {24},
Pages = {281-304},
Month = {JUL-DEC},
Abstract = {The aim of this article is to compare the political changes of Costa
   Rica, Colombia and Venezuela from the so-called ``second wave{''} of
   democracy to nowadays. The article states that the condition of
   democratic solidity can be detrimental to democratization, that a
   quality democracy is not necessarily the most stable rather than the one
   that has the greatest adaptive capacity to incorporate institutionally
   the social tension, and that one of the key factors to avoid
   undemocratic phenomena such as populism and political violence is the
   left-to-right coverage of the democratic political spectrum by the
   parties.},
