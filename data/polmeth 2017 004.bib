@article{ WOS:000390976100001,
Author = {Bush, Sarah Sunn and Gao, Eleanor},
Title = {Small Tribes, Big Gains The Strategic Uses of Gender Quotas in the
   Middle East},
Journal = {COMPARATIVE POLITICS},
Year = {2017},
Volume = {49},
Number = {2},
Pages = {149+},
Month = {JAN},
Abstract = {Why do some political actors nominate women more than others in the
   Muslim world? This article argues that certain social groups have an
   instrumental demand for female candidates because they believe such
   candidates will enhance their electoral chances in the wake of gender
   quotas' adoption. Looking at Jordan, it hypothesizes that small tribes
   can make big gains by nominating women due to the design of the
   country's reserved seat quota. This argument complements existing
   perspectives on women's (under-)representation in the Muslim world,
   which emphasize the role of features of the culture, economy, or
   religion. The analysis of original data on Jordan's local elections and
   tribes supports the argument. The article's findings have implications
   for our understanding of women's representation, tribal politics, and
   authoritarian elections.},
Publisher = {SHERIDAN PRESS},
Address = {COMPARATIVE POLITICS, PO BOX 465, HANOVER, PA 17331 USA},
Type = {Article},
Language = {English},
Affiliation = {Bush, SS (Corresponding Author), Temple Univ, Polit Sci, Philadelphia, PA 19122 USA.
