@article{ WOS:000414154200003,
Author = {Chen, Jim Q.},
Title = {A New Dynamic Cyber Defense Framework},
Journal = {INTERNATIONAL JOURNAL OF CYBER WARFARE AND TERRORISM},
Year = {2017},
Volume = {7},
Number = {4, SI},
Pages = {14-22},
Month = {OCT-DEC},
Abstract = {Current approaches in cyber defense are flawed as they are
   fortress-based and generally static in nature. They are not flexible in
   dealing with variations of attacks, especially zero-day attacks. To
   address this issue, researchers have looked into dynamic cyber defense.
   However, the available approaches are either only about high-level
   strategies or only about specific tactics. There is no integrated
   approach that brings both levels together in a systematic way. This
   research article intends to address this challenge by proposing a new
   dynamic cyber defense framework that is systematic and cohesive, and
   that integrates strategic, operational, and tactical levels. It improves
   the research in dynamic cyber defense by employing game-changing
   elements such as a contextual analysis system and an intelligent
   decision-making system.},
