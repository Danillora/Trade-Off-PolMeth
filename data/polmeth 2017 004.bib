@article{ WOS:000419283800002,
Author = {Ricciardi, Maurizio},
Title = {Time, Order, Power. On Some Conceptual Presuppositions of Neoliberal
   Program},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
Year = {2017},
Volume = {29},
Number = {57},
Pages = {11-30},
Abstract = {The essay maintains that, despite the significant successive
   transformations, a ordoliberal moment operates within the neoliberal
   program. Ordoliberalism establishes a specific political anthropology
   based on the centrality of economic action and on an a-revolutionary
   temporality that enhances the normative continuity of the tradition.
   Even against the specific configurations that the State can assume,
   especially the democratic one, it also aims at the constant reactivation
   of a political as a fundamental decision in favor of the economic.},
