@article{ WOS:000397496000005,
Author = {Lubbers, Marcel and Coenders, Marcel},
Title = {Nationalistic attitudes and voting for the radical right in Europe},
Journal = {EUROPEAN UNION POLITICS},
Year = {2017},
Volume = {18},
Number = {1, SI},
Pages = {98-118},
Month = {MAR},
Abstract = {Voting for radical right-wing parties has been associated most strongly
   with national identity threats. In Europe, this has been framed by the
   radical right in terms of mass-migration and European integration, or
   other politicians bargaining away national interests. Perhaps
   surprisingly given the radical right's nationalist ideology,
   nationalistic attitudes are hardly included in empirical research on the
   voting behaviour. In this contribution, we test to what extent various
   dimensions of nationalistic attitudes affect radical right voting, next
   to the earlier and new assessed effects of perceived ethnic threat,
   social distance to Muslims, Euroscepticism and political distrust. The
