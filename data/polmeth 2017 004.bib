@article{ WOS:000423214800002,
Author = {Hoch, Tomas and Kopecek, Vincenc and Baar, Vladimir},
Title = {Civil Society and Conflict Transformation in De Facto States The Case of
   Abkhazia},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2017},
Volume = {64},
Number = {6},
Pages = {329-341},
Abstract = {De facto states are considered to be highly specific entities, thus
   meriting a detailed analysis of how conflict transformation functions
   within such states and what role is played in them by civil society
   organizations (CSOs), which are traditionally considered one of the key
   actors in conflict transformation. The authors discuss the factors that
   limit the activities of these CSOs as a result of the restricted
   sovereignty of de facto states. The study is based on three field
   research projects focusing on CSOs in Abkhazia, carried out in 2009,
   2014, and 2015.},
