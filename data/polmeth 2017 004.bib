@article{ WOS:000422892700010,
Author = {Hauswedell, Corinna},
Title = {``New responsibility{''} in German Foreign Politics: Between Handling
   Migration and Military Missions},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Number = {52},
Pages = {196+},
Abstract = {The latest public discourse in Germany about adopting more
   responsibility (Verantwortung) in foreign affairs, primarily shaped by
   international expectations for a larger German role in military
   missions, became a different humanitarian slant during 2015 when the
   admission of about a million refugees stimulated all kinds of ethical
   convictions and political dilemmas. With recourse to German philosophers
   like Picht, Jonas, Weber, and Arendt the article explores the actual
   normative scope of the term ``Verantwortung{''}, and comes up on a
   sceptical note: Both foreign policies, waging war and managing
   migration, cross frontiers where human rights violations are of high
   concern. Globalization increasingly blurring domestic and foreign
   affairs will require a new ethic.},
