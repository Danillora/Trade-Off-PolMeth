@article{ WOS:000423265500006,
Author = {Rosa, Paolo and Foradori, Paolo},
Title = {Politics does not stop at the `nuclear edge': neoclassical realism and
   the making of China's military doctrine},
Journal = {ITALIAN POLITICAL SCIENCE REVIEW-RIVISTA ITALIANA DI SCIENZA POLITICA},
Year = {2017},
Volume = {47},
Number = {3},
Pages = {359-384},
Month = {NOV},
Abstract = {The article tries to explain why China, after having launched a crash
   programme in the mid-1950s to develop a nuclear deterrent, did not
   formulate a clear operational doctrine with respect to the targeting and
   employment of atomic weapons until the mid-1980s. Propositions derived
   from neoclassical realism are used to shed some light on this puzzling
   aspect of China's nuclear doctrine. The general hypothesis of the study
   is that, international predicaments notwithstanding, China's domestic
   politics prevented the possibility of articulating a clear and detailed
   nuclear doctrine during the period following the first nuclear test,
   when such a doctrine was more necessary.},
