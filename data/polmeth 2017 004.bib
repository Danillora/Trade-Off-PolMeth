@article{ WOS:000417174500012,
Author = {Mikhailov, R. V.},
Title = {THE RUSSIAN-FRENCH DIALOGUE ON THE CRISIS OF EUROPEAN CIVILIZATION (THE
   5th BERDYAEV READINGS IN PARIS)},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2017},
Number = {3},
Pages = {179-191},
Abstract = {The article is devoted to the analysis of the modern crisis of European
   civilization, of the degradation of its basic values, as well as of the
   actual consequences brought about by the conscious refusal of Western
   Europe from its roots. The author summarizes the discussions at the
   Fifth Forum Berdyaev Readings, held under the auspices of the
   Noncommercial Foundation - Institute of Socio-Economic and Political
   Researches (ISEPR Foundation) in October 2016 in Paris. Russian and West
   European experts discussed the tragic processes of the destruction of
   European civilizational identity, their origins, and also sought ways to
   overcome them. The author of the article, making use of speeches and
   discussions, makes an attempt to analyze the emergence and establishment
   of the ideology of political conservatism in continental Europe,
   including the ``Russian factor{''} as well as Russia's mission to
   protect the entire spiritual heritage, centuries-old values, and
   traditional institutions of European civilization. The return of Russia
   to its natural historical path of development against the backdrop of
   the decline of the traditional foundations of European culture in
   Western Europe leads to the conclusion that Russia now plays
   increasingly important role of the embodiment of primordial European
   values. Russia turns from the Eurasian frontier of the European
   civilization into its spiritual and cultural citadel. Thus, Russia is
   returning to its historic European mission as a guardian of the
   spiritual principles of the European Christian civilization.},
