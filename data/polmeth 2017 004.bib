@article{ WOS:000399328200011,
Author = {Onderco, Michal},
Title = {Public support for coercive diplomacy: Exploring public opinion data
   from ten European countries},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
Year = {2017},
Volume = {56},
Number = {2},
Pages = {401-418},
Month = {MAY},
Abstract = {Scholarship has increasingly acknowledged the importance of public
   attitudes for shaping the European Union's Common Foreign and Security
   Policy. Economic sanctions emerged as one of CFSP's central tools. Yet
   despite the emergence of sanctions as a popular instrument in the EU
   foreign policy toolbox, public attitudes towards sanctions are yet to be
   studied in depth. This article explains public support for EU sanctions,
   using the empirical example of sanctions against Russia. It looks at
   geopolitical attitudes, economic motivations and ideational factors to
   explain the variation in public support for sanctions. The conclusion
   suggests that geopolitical factors are the most important, and that
   economic factors matter very little. Euroscepticism and anti-Americanism
   play an important role in explaining the support for sanctions at the
   individual level.},
