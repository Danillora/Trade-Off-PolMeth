@article{ WOS:000410827800004,
Author = {Xydias, Christina},
Title = {Citizens, Domestic Institutions, and International Organizations: Recent
   Financial Crises in Greece and Iceland},
Journal = {POLITY},
Year = {2017},
Volume = {49},
Number = {4},
Pages = {518-547},
Month = {OCT},
Abstract = {Attempts to repair a democratic deficit are typically undertaken at the
   level of government understood to have the deficit. However, this
   qualitative comparative study of Greece's and Iceland's recent financial
   crises illustrates that domestic institutional arrangements have the
   potential to amplify citizens' voices at the international level,
   increasing the inclusive and participatory nature of decision making and
   thereby addressing some features of a democratic deficit in
   non-majoritarian, international institutions. Greece and Iceland are the
   only two countries to have held referendums related to the recent debt
   crises. At the same time, they differ in key domestic political
   institutions, which I argue produce different pathways for the
   transmission of public opinion to the international level. By tracing
   the institutional barriers and opportunities for citizen protesters in
   each case, this article demonstrates that the presence in Iceland of a
   dual executive and the availability of a specific form of referendum
   amplified citizens' voices, while these voices were blocked in Greece.},
