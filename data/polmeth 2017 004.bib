
@article{ WOS:000414547800002,
Author = {Merz, Nicolas},
Title = {Gaining voice in the mass media: The effect of parties' strategies on
   party-issue linkages in election news coverage},
Journal = {ACTA POLITICA},
Year = {2017},
Volume = {52},
Number = {4, SI},
Pages = {436-460},
