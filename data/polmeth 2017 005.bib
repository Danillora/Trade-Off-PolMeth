@incollection{ WOS:000387846700012,
Author = {Liu Zuokui},
Editor = {Zhou, H},
Title = {The Chinese Perception of the EU},
Booktitle = {CHINA-EU RELATIONS: REASSESSING THE CHINA-EU COMPREHENSIVE STRATEGIC
   PARTNERSHIP},
Series = {Research Series on the Chinese Dream and Chinas Development Path},
Year = {2017},
Pages = {189-200},
Abstract = {The Chinese people's Perception of the EU can be roughly divided into
   two groups: that of the general public and that of the elite, which has
   been surveyed from three perspectives including their degree of
   favorability towards the EU, their evaluation of the EU's international
   influence and their opinions concerning the status quo and the prospects
   for China-EU relations. Both the elite and the general public view the
   EU relatively optimistically and positively in general terms; however,
   in the last two years the percentage of those being positive has
   increasingly decreased. The elite's perception of the EU is more
   rational than that of the general public. The general public is more
   positive than the elite with regards to the status quo of China-EU
   relations, and vice versa with regards to the degree of favorability
   towards the EU and the future development of the bilateral relations. As
   a conclusion, the Chinese people's perception of the EU has been
   influenced by the general atmosphere of China-EU relations, China's
   strategy towards the EU and the changing mentality of the Chinese people
   in the context of globalization.},
