@article{ WOS:000422892700011,
Author = {Geis, Anna and Pfeifer, Hanna},
Title = {Negotiating German Responsibility ``at the Heart of Society{''}? On the
   Politicization and Depoliticization of German Foreign Policy},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Number = {52},
Pages = {218+},
Abstract = {This article analyzes the participatory process ``Review 2014 -
   Aussenpolitik weiter den ken{''} which was initiated by the German
   Federal Foreign Office in May 2014. Drawing on Iris Marion Young's works
   on deliberative democracy and political responsibility, we point to the
   tensions that emerge between the attempt to politicize foreign policy on
   the one hand, and the political elites' strive for a more active German
   role in global politics. If processes of self-reflection in the field of
   foreign policy are taken seriously, they do not necessarily end up in an
   enhanced commitment of Germany in world politics, nor in a normatively
   demanding program of political responsibility. Moreover, failing
   attempts at politicization which are initiated top-down by political
   elites, such as the ``Review{''} process, may indeed contribute to a
   fake legitimization of a foreign policy agenda which had ``more
