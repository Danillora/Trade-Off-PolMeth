@article{ WOS:000408222000002,
Author = {Lobato, Luisa Cruz},
Title = {The brazilian cybersecurity policy as a strategy of regional leadership},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
Year = {2017},
Number = {20},
Pages = {16-30},
Month = {JUN},
Abstract = {The article analyzes the structuration of Brazil's cybersecurity policy
   between the years of 2003 and 2016 as a component of its strategy of
   international insertion and projection of leadership in the Global
   South. The Internet governance field, of which cybersecurity is a part,
   offers the country a relatively low-cost opportunity of protagonism in
   the elaboration of international norms. It analyzes cornerstone
   documents of this policy and argues that it is a part of the country's
   efforts to project its soft power in the field of international
   security, but that its incoherencies can affect and even compromise the
   strategy. Finally, it draws brief projections to this policy in face of
   political changes in Brazil.},
