@article{ WOS:000422892700009,
Author = {Magnin, Chantal},
Title = {Participatory Democracy and Diffusion of Responsibility in Urban
   Planning Projects},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Number = {52},
Pages = {170+},
Abstract = {Exemplified by the informal participation of citizens in urban planning
   projects we address the question of whether and how the implementation
   of participatory events contribute to the diffusion of political
   responsibility. Although the citizens hope to directly take part in the
   political decision-making, their experiences in urban planning projects
   show that a delegation of political responsibility by means of
   deliberative techniques almost never occurs. Instead, investors and
   other private-sector stakeholders nowadays often turn out to be the
   decisive partners of the authorities. From citizen's perspective, those
   kinds of partnership impede the accountability of political
   responsibility.},
