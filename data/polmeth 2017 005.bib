@article{ WOS:000422892700003,
Author = {Schmelzle, Cord},
Title = {Second Order Responsibility and the Concept of the State},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Number = {52},
Pages = {36+},
Abstract = {In light of increasing globalization and denationalization, the
   organization of the world into nation-states is more and more
   questioned. This article contributes to this debate by developing a
   novel and normatively relevant concept of statehood that conceives of
   states as bearers of second order responsibilities. According to this
   conceptualization, states have two types of second order
   responsibilities that arise from their monopoly of authority and their
   role as public actors: A meta-responsibility to create a fair and
   effective societal system of first order responsibilities and ultimate
   accountability for incomplete, unjust or ineffective first order
   systems. The paper develops this account through detailed discussions},
