Title = {Poor young men without sex and brain. Manners and Models of
   Student Life Under the Left-Wing Reformist Critic in the University of
   Buenos Aires (1917-1921)},
Journal = {IZQUIERDAS},
Year = {2017},
Volume = {32},
Pages = {79-106},
Month = {FEB},
Abstract = {The work at hand focuses on the reconstruction of student sociabilities
   at the University of Buenos Aires (UBA) between 1917-1921. It aims to
   inquire into the implementation of the University Reform and the effect
   it had on the capital's collegial student body, which concentrated more
   than half of the universitary population in the country. To achieve this
   purpose, the present paper will resort to analyzing publications written
   by left-wing reformists who, by means of their reviews, interpellated
   university youths to reformulate their practices, their ways of
   associating, and to socialize a new student profile.},
