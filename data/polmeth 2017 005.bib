@article{ WOS:000406159400010,
Author = {Tekmen, Hilal},
Title = {POPULISM AND CLOSING CIVIC SPACE: A POST-TRUTH CHALLENGE},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2017},
Volume = {16},
Number = {1},
Pages = {105-112},
Month = {SPR},
Abstract = {The ongoing surge of right-wing populism and the post-truth environment
   in Europe and the US have cast a global threat to civic space. Recent
   developments in national and international politics render us, citizens
   of the world, incapable of fully enjoying our rights to free assembly,
   protest, travel, or simply to speak out. This narrowing trend revives
   discriminatory debates over refugees, minorities, and racism, and is
   reinforced by populist leaders speaking ``for the people.{''} The danger
   of populism asserts itself in ways ranging from restricting civil
   society through limiting foreign funding for civil society organizations
   to closing space for peaceful assembly. The future is concerning.
   However, it can be saved through sustainable support to civil society
   actors.},
