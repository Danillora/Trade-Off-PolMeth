@article{ WOS:000408222000005,
Author = {Antonio Rodriguez, Juan and Oduber, Jesus and Mora, Endira},
Title = {Routine activities and cyber-victimization in Venezuela},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
Year = {2017},
Number = {20},
Pages = {63-79},
Month = {JUN},
Abstract = {Cybercrime has significantly increased worldwide in the last decades.
   There has been very little research done regarding cybercrimes in
   Venezuela in comparison to other places, more specifically in regards to
   the factors related to online victimization. Thus, this study aims to
   encourage the analysis on the factors of crime and online victimization
   in the region. Therefore, a group of variables derived from the routine
   activity theory (Cohen and Felson 1979), is analyzed. The purpose is to
   observe its relation to hacking and online harassment in a sample of 308
   individuals. This study found a group of variables that could be related
   to the likelihood of cyber victimization, which will be discussed in
   empirical, theoretical and practical terms.},
