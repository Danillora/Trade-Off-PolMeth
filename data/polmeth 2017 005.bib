@article{ WOS:000413977600011,
Author = {Duong, Kevin},
Title = {The People as a Natural Disaster: Redemptive Violence in Jacobin
   Political Thought},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2017},
Volume = {111},
Number = {4},
Pages = {786-800},
Month = {NOV},
Abstract = {The trial and execution of Louis XVI served as a founding act of French
   republican democracy. It was also a scene of irregular justice: no legal
   warrants or procedural precedents existed for bringing a king to justice
   before the law. This essay describes how Jacobins crafted a new language
   of popular agency to overcome that obstaclethe language of redemptive
   violence. Although redemptive violence had roots in prerevolutionary
   notions of penal justice and social cohesion, its philosophical
   ambitions were revolutionary and modern. Analyzing that language
   illuminates how republican democracy weaponized a distinctive ideology
   of extralegal violence at its origins. It also helps explain redemptive
   violence's enduring appeal during and after the French Revolution.},
