@article{ WOS:000419266200001,
Author = {Heroux-Legault, Maxime},
Title = {Public opinion in Quebec under the Harper Conservatives},
Journal = {CANADIAN POLITICAL SCIENCE REVIEW},
Year = {2017},
Volume = {11},
Number = {1},
Pages = {1-36},
Abstract = {An important part of Stephen Harper's early strategy to form a majority
   government was to appeal to Quebec voters. Open federalism, the
   recognition of the Quebec nation in the House of Commons, and
   international representation at the UNESCO were all measures aimed at
   increasing support for the Conservatives in the province. This article
   reviews Quebecers' reception to several key policy decisions taken by
   Stephen Harper's government in the 2006-2011 time period. It also
   estimates the impact of these policies on satisfaction towards the
   federal government, appreciation of Stephen Harper, and voting
   intentions towards the Conservatives in Quebec.},
