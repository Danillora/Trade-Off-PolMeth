@article{ WOS:000411382900004,
Author = {Oehmer, Franziska},
Title = {It's the communication that matters, too! Determinants of interest
   groups' media exposure in German newspapers},
Journal = {INTEREST GROUPS \& ADVOCACY},
Year = {2017},
Volume = {6},
Number = {2},
Pages = {179-194},
Month = {MAY},
Abstract = {In modern societies, the media functions as the most important source of
   information about current political or societal events. Given this media
   power, political organizations are eager to participate in the public
   discourse. In addition to political parties, interest groups also seem
   to rely more and more on media exposure to strengthen their position
   within the political decision-making process, to inform their members or
   even to court new members. The limited capacity of the media, however,
   does not allow for every interest group to be heard. The purpose of the
   present study is to identify and explain the amount of interest groups'
   media exposure. The results mainly indicate that the quantity and the
   quality of the public relations activities of an interest group can
   account for a large part of the amount of media exposure; the more
   frequent and the more professional their communications, the more likely
   they are to be covered.},
