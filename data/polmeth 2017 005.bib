@article{ WOS:000419763300001,
Author = {Li Shenming},
Title = {The October Revolution: A New Epoch in the World History},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2017},
Volume = {7},
Number = {3, SI},
Pages = {279-288},
Abstract = {Since the October Revolution in Russia in 1917, debates on its role as
   well as its advantages and disadvantages have been unceasing. At a time
   when the one-hundredth anniversary of the October Revolution is
   approaching, it is necessary to sum up its experience, review the
   development of world socialism, and learn from the collapse of the
   former Soviet Union, which is of great significance to the promotion of
   China's sustainable development, the implementation of the government's
   harmonious development strategies, the enforcement of strict party
   disciplines, and the realization of China's all round prosperity. The
   October Revolution was firmly based on theory; the October Revolution
   created a powerful former Soviet Union which developed rapidly; the
   reflection of Russian people from all walks of life in recent years has
   demonstrated convincingly that the October Revolution was correct. A
   series of facts reveal that the October Revolution ushered in a new
   epoch in world history.},
