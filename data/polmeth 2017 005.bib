@article{ WOS:000417399800001,
Author = {Garcia Linera, Alvaro},
Title = {National Space and Global Space under Capitalism},
Journal = {GEOPOLITICAS-REVISTA DE ESTUDIOS SOBRE ESPACIO Y PODER},
Year = {2017},
Volume = {8},
Number = {2},
Pages = {159-176},
Abstract = {Drawing on the Marxist notion of commodity value, this article seeks to
   establish the difference between national space and global space. The
   idea of national space is anchored in the notion of use-value, whereas
   the idea of global space is bound to the exchange of commodities
   (exchange -use). Alternating between protectionism and free market, the
   cyclical nature of capitalism introduces a tendency to accentuate either
   the national space or the global space. Linked to cyclical crises in the
   free market dynamics, the crisis of neoliberalism enables the
   development of effective resistances to the system in the weak areas of
   the capitalist system such as Latin America.},
