@article{ WOS:000413768100003,
Author = {Mondak, Jeffery J. and Hurwitz, Jon and Peffley, Mark and Testa, Paul},
Title = {The Vicarious Bases of Perceived Injustice},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
Year = {2017},
Volume = {61},
Number = {4},
Pages = {804-819},
Month = {OCT},
Abstract = {Profound differences exist in how Americans from various racial and
   ethnic groups view police and court officials. We argue that vicarious
   experiences contribute to this racial and ethnic divide. Drawing on
   research on social communication, social network composition, and
   negativity biases in perception and judgment, we devise a theoretical
   framework to articulate why vicarious experiences magnify racial and
   ethnic disparities in evaluations of judicial actors. Four hypotheses
   are tested using original survey data from the state of Washington.
   Results provide strong evidence that vicarious experiences influence
   citizens' evaluations of both police and courts, and they do so in a
   manner that widens racial divides in how those actors are perceived.},
