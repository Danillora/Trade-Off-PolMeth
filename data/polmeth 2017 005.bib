@article{ WOS:000446296100010,
Author = {Ediger, Volkan S. and Durmaz, Duygu},
Title = {Energy in Turkey and Russia's Roller-Coaster Relationship},
Journal = {INSIGHT TURKEY},
Year = {2017},
Volume = {19},
Number = {1},
Pages = {135-155},
Month = {WIN},
Abstract = {Energy relations between Turkey and Russia provide an excellent example
   of how energy and politics interrelate in countries with a historically
   up-and-down relationship. Having started in the 1960s, the two
   countries' energy relations gained a new dimension after 1991 with the
   intensification of pipeline politics. In the 2000s, energy relations
   gained impetus owing to the leadership of Erdogan and Putin, and reached
   an apex with the establishment of a cooperation council in 2010. Since
   2011, Turkey's demands for gas price reduction and volume increase have
   dominated the agenda of the countries' energy talks. While
   Turkish-Russian relations have remained on shaky ground lately, the
   signing of the Turkish Stream agreement in 2016 might be perceived as a
   positive step for closer energy relations.},
