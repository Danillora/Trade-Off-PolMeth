@article{ WOS:000395115900008,
Author = {Wilkins, Thomas},
Title = {Australia and middle power approaches to Asia Pacific regionalism},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2017},
Volume = {52},
Number = {1},
Pages = {110-125},
Month = {MAR},
Abstract = {This article presents a liberal-institutionalist conceptual framework
   drawn from Middle Power theory to analyse Australian foreign policy
   approaches towards Asia Pacific regionalism. Building on precedents set
   by the former Keating administration, the Labor government of
   Rudd/Gillard (2007-13) undertook high-profile efforts not only to
   engage, but to champion, the regionalism process in the Asia Pacific.
   This enterprise became fused with a self-proclaimed identity as a
   creative middle power'. Through an analysis of regional community
   building, regional security architecture and regional order, the article
   identifies the strong linkages between the theory and practice of middle
   power' diplomacy, and the concept of regionalism' itself, in Australian
   foreign policy. The article thus contributes to the theoretical
   literature by exposing the important intersections between the two
   concepts and concludes that despite Rudd's prolific attempts to harness
   Australia's middle power credentials, Canberra was not able to
