Language = {English},
Affiliation = {Kavwahirehi, K (Corresponding Author), Univ Ottawa, Dept French Studies, Ottawa, ON, Canada.
   Kavwahirehi, Kasereka, Univ Ottawa, Dept French Studies, Ottawa, ON, Canada.},
DOI = {10.1080/13621025.2017.1279799},
ISSN = {1362-1025},
EISSN = {1469-3593},
Keywords = {Great Lake regions; violence; Christianity; nation state},
Research-Areas = {Government \& Law},
Web-of-Science-Categories  = {Political Science},
