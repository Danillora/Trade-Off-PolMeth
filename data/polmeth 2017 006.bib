@article{ WOS:000418715300004,
Author = {Debeuf, Koert},
Title = {THE ARAB SPRING SEVEN YEARS ON: A FAILURE OR JUST THE BEGINNING?},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2017},
Volume = {16},
Number = {3},
Pages = {41-48},
Month = {FAL},
Abstract = {Using the 1978 French Revolution as a blueprint, scholar Crane Briton
   observed a pattern in four historic revolutions and identified four
   distinct phases: the rule of the moderates; the reign of terror and
   virtue; Thermidor; and the end of the revolution. Although seemingly
   incomparable at first, in this article the author identifies a number of
   similarities between the French Revolution and the Arab Revolutions:
   both stemmed from a younger generation aspiring for more political
   freedom and rights and both proceeded in line with each of Briton's
   phases. The author argues that as it took France 86 years to establish
   its first stable democracy, not all hope is lost for the Arab world.},
