@article{ WOS:000404849500006,
Author = {Cunningham, Kathleen Gallagher and Dahl, Marianne and Fruge, Anne},
Title = {Strategies of Resistance: Diversification and Diffusion},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
Year = {2017},
Volume = {61},
Number = {3},
Pages = {591-605},
Month = {JUL},
Abstract = {Why do organizations choose to use nonviolence? Why do they choose
   specific nonviolent tactics? Existing quantitative work centers on mass
   nonviolent campaign, but much of the nonviolence employed in contentious
   politics is smaller-scale nonviolent direct action. In this article, we
   explore the determinants of nonviolence with new data at the
   organization level in self-determination disputes from 1960 to 2005. We
   present a novel argument about the interdependence of tactical choices
   among nonviolent options in self-determination movements. Given
   limitations on their capabilities, competition among organizations in a
   shared movement, and different resource requirements for nonviolent
   strategies, we show that organizations have incentives to diversify
   tactics rather than just copy other organizations. The empirical
   analysis reveals a rich picture of varied organizational resistance
   choices, and a complex web of interdependence among tactics.},
