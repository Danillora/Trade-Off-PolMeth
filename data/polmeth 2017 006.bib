@article{ WOS:000405254200012,
Author = {Behrends, Jan Claas},
Title = {``Ostpolitik{''} is Security Policy for the Whole of Europe A Response
   to Dembinski \& Spanger},
Journal = {OSTEUROPA},
Year = {2017},
Volume = {67},
Number = {3-4},
Pages = {135+},
Abstract = {The concept of ``plural peace{''} is misleading. It will bring neither
   security nor peace, but will instead again divide Europe and create
   greater instability. We do not need a Russia policy that rewards
   Russia's flouting of the law, but a European and Atlantic-based
   ``Ostpolitik{''}, a policy towards the east, which promotes security in
   the region and contains Russia without isolating it. The
   ``constrainment{''} approach will defend international law and the
   sovereignty of states, including those in Eastern Europe.},
