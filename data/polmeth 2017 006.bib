@article{ WOS:000395115900002,
Author = {Maddison, Sarah},
Title = {Recognise what? The limitations of settler colonial constitutional
   reform},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2017},
Volume = {52},
Number = {1},
Pages = {3-18},
Month = {MAR},
Abstract = {In settler colonial societies such as Australia, both Indigenous and
   non-Indigenous people have turned to constitutional reform as a means of
   addressing historical exclusions and colonial injustice. In practice,
   however, the promise of constitutionalism has revealed clear limits.
   This article explores these limits in the context of the current
   Australian campaign for the constitutional recognition' of Aboriginal
   and Torres Strait Islander peoples, where the loudest dissenting voices
   have come from Indigenous people themselves. In light of this, this
   article proposes a more agonistic engagement of diverse and dissenting
   opinions, with a view to opening up a more radical, decolonising space
   for constitutional politics.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
