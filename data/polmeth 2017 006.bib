@article{ WOS:000449596100010,
Author = {Gabardo, Emerson and de Moura Rezende, Mauricio Correa},
Title = {The concept of public interest in brazilian administrative law},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2017},
Volume = {115},
Pages = {267-318},
Month = {JUL-DEC},
Abstract = {This article seeks to situate the contemporary discussion about the
   legal concept of ``public interest{''} in Administrative Law. Without
   forgetting that this concept has several approaches (sociological,
   philosophical, political), it is sought, initially, to think about its
   legal aspect. For this, it promotes the analysis of the concept
   initially in its legal nature - which reveals itself as an indeterminate
   legal concept. Following, it observes the legal structure of the concept
   according to the doctrine. Finally, it seeks to observe what the
   doctrine has to say about the legal content of the concept, not in order
   to establish a priori what is or not public interest, but to delineate
   what kind of objects can be framed in this conceptualization.},
