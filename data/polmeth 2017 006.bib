@article{ WOS:000416638300007,
Author = {Bronk, Justin},
Title = {THE RISE OF THE FIGHTER-BOMBER IN THE WESTERN DESERT},
Journal = {RUSI JOURNAL},
Year = {2017},
Volume = {162},
Number = {4},
Pages = {50-59},
Abstract = {The fighter-bomber rose from obscurity in January 1941 to become the
   major component of the RAF's Desert Air Force strike power by late 1942.
   This was a major shift in the way the RAF employed its aircraft. The
   tactical and technical factors that led to this change are crucial not
   only to understanding the course of the Second World War, but also to
   the history of airpower itself. In this article, Justin Bronk discusses
   how the fighter-bomber became the most important and practical component
   in the arsenal of a modern air force in terms of capability to project
   power on the battlefield.},
