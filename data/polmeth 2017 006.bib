@article{ WOS:000404497700007,
Author = {Corduwener, Pepijn},
Title = {research note: integrating contemporary populism with the history of
   democracy in Western Europe},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2017},
Volume = {16},
Number = {2},
Pages = {206-216},
Month = {JUN},
Abstract = {Populism has become a resilient political phenomenon. Much of the
   normative political science on this topic is concerned with the
   relationship between democracy and populism. At the same time, the
   characterisation of post-war democracy has emerged as a key focal point
   in recent contributions to political history. This research note
   explores how both these developments and their disciplines might benefit
   from closer collaboration. It highlights, therefore, some of the
   distinctive features of populism's relationship with democracy and shows
   how these might be accounted for by incorporating insights from history.
   At the same time, it argues that historiography has largely ignored the
   populist question in the history of post-war democracy and makes some
   suggestions as to how the history of populism might be included in this
   research.},
