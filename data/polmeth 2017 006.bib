@article{ WOS:000423499000004,
Author = {Hillen, Sven},
Title = {Do only Voters Count? An Analysis of the Relationship Between Electoral
   Turnout and Left Partisan Effects on Labour Market-Related Social
   Policies in OECD-Countries},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Volume = {58},
Number = {4},
Pages = {533-559},
Abstract = {The effects of left governing parties on the welfare state are contested
   in the literature. To date no study considered voter turnout as a
   moderator of partisan effects. Low turnout rates generally mean
   socio-economically unequal turnout rates, since low income citizens have
   a higher probability of non-voting. At the same time, low income groups
   are among the core constituency of left parties. This article
   investigates whether left governing parties expand labour market-related
