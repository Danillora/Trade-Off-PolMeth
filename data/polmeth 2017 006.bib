@article{ WOS:000398509700002,
Author = {Claassen, Rutger},
Title = {Markets as Mere Means},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2017},
Volume = {47},
Number = {2},
Pages = {263-281},
Month = {APR},
Abstract = {There has been a remarkable shift in the relationship between market and
   state responsibilities for public services like health care and
   education. While these services continue to be financed publicly, they
   are now often provided through the market. The main argument for this
   new institutional division of labor is economic: while (public) ends
   stay the same, (private) means are more efficient. Markets function as
   mere means' under the continued responsibility of the state. This
   article investigates and rejects currently existing egalitarian liberal
   theories about this division of labor and it presents and defends a new
   theory of marketization, in which social rights and democratic
   decision-making occupy center-stage.},
