@article{ WOS:000396653400007,
Author = {Murphy, Michael P. A.},
Title = {Pouvoir constituant betrayed: a model of abjection in power
   relations},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2017},
Volume = {10},
Number = {1},
Pages = {85-93},
Abstract = {Examination of the limit serves as a powerful tool for revealing the
   hidden characteristics of concepts, and also their relationship with
   other concepts. This article follows the processes of sovereign
   exceptionalism from Marx to the capitalist estrangement of labour from
   Marx to their limit figures. The paper builds on comparisons between the
   proletarian and the homo sacer; however, the focal point is not on the
   figures themselves, but their importance in understanding the effect of
   biopolitics on power relations. Building on the concept of pouvoir
   constituant as discussed by Carl Schmitt, this paper addresses the ways
   in which different types of constituent power form structures that can
   then be used against the constituents themselves. The limit figures
   suggest a process of abjection is co-created in the establishment of
   power structures, and that overcoming this process requires a conscious
   dis-agreement with the politics of policing.},
