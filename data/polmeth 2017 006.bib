@article{ WOS:000405101900008,
Author = {Arrighi, Jean-Thomas and Baubock, Rainer},
Title = {A multilevel puzzle: Migrants' voting rights in national and local
   elections},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
