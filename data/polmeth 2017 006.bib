@incollection{ WOS:000429066500004,
Author = {Wekker, Fenneke},
Book-Author = {Wekker, F},
Title = {Community Building Practices},
Booktitle = {TOP-DOWN COMMUNITY BUILDING AND THE POLITICS OF INCLUSION},
Series = {Europe in Transition-The NYU European Studies Series},
Year = {2017},
Pages = {19-40},
Abstract = {This empirical chapter focuses on community building practices that were
   deployed during the dinners at this particular restaurant. There
   appeared to be a complex range of practices, organized and unorganized,
   intended and unintended, foreseen and unforeseen, that gave shape to the
   construction and constitutive boundaries of the restaurant community and
   its several subcommunities. By distinguishing three types of practices,
   and drawing on theories regarding rituals, ceremonies, and social
   solidarity, this chapter shows to what extent and how community building
   practices evoke and reinforce modes of collective thinking, feeling, and
   acting and thereby create (immaterial) boundaries to outsiders.},
