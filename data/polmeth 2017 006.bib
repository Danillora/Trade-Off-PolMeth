@article{ WOS:000399615200002,
Author = {Apakan, Ertugrul and Sporrer, Wolfgang},
Title = {THE UKRAINIAN CRISIS: THE OSCE'S SPECIAL MONITORING MISSION},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2017},
Volume = {15},
Number = {4},
Pages = {17-25},
Month = {WIN},
Abstract = {The article describes the case of the OSCE's Special Monitoring Mission
   to Ukraine (SMM)as a civilian mission operating in an environment of
   armed crisis. It describes its tasks, achievements, and main challenges,
   observing that this mission has been entrusted with functions that in
   the past have been performed by peacekeeping operations. At the same
   time, the authors highlight the fact that the SMM not only serves as an
   instrument of conflict management and resolution, but has an important
   preventative and deterrent role as well. The article then examines what
   lessons could be drawn from this example for other civilian peace
   operations.},
