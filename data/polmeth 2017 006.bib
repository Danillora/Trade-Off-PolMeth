@article{ WOS:000426418500001,
Author = {Keenan, Jeremy H.},
Title = {UK FOREIGN POLICY AND INTELLIGENCE IN THE POST-TRUTH ERA: RADICAL
   VIOLENT EXTREMISM AND ``BLOW-BACK{''}},
