@article{ WOS:000419512800001,
Author = {Muzzopappa, Eva},
Title = {Military intelligence in Argentina. Reflections since a naval archive},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
