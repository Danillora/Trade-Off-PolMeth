@article{ WOS:000412503300005,
Author = {Alvarez Tardio, Manuel},
Title = {Mobilization and political violence following the Spanish general
   elections of 1936},
Journal = {REVISTA DE ESTUDIOS POLITICOS},
Year = {2017},
Number = {177},
Pages = {147-179},
Month = {JUL-SEP},
Abstract = {This paper provides the results of a detailed investigation, mainly
   based on primary sources, regarding the characteristics of the political
   mobilization that followed the decisive Spanish general elections of
   1936. This mobilization occurred five months before the Civil War,
   during the last period of the Second Republic. The main objective of the
   paper is to contribute to a richer and better-informed understanding of
   the role played by political violence, by analyzing its presence and
   impact. It studies the specific circumstances in which the transfer of
   powers took place after the elections. The paper focuses on a short
   period of time, allowing a detailed analysis of events. In addition,
   this paper provides an answer to an important question: did the sudden
   change of government contribute to the demobilization of the extremist
   groups during this period?},
