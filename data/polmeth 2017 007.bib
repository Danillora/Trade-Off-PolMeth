DOI = {10.18611/2221-3279-2017-8-2-8-18},
ISSN = {2221-3279},
EISSN = {2412-4990},
Keywords = {World Politics; ``End of history{''}; ``Clash of civilizations{''};
   migration crisis; ``End of liberalism{''}},
Research-Areas = {Government \& Law},
