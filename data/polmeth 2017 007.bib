@article{ WOS:000399478000007,
Author = {Baumgartner, Frank R. and Epp, Derek A. and Shoub, Kelsey and Love,
   Bayard},
Title = {Targeting young men of color for search and arrest during traffic stops:
   evidence from North Carolina, 2002-2013},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2017},
Volume = {5},
Number = {1},
Pages = {107-131},
Abstract = {North Carolina mandated the first collection of demographic data on all
   traffic stops during a surge of attention to the phenomenon of driving
   while black in the late 1990s. Based on analysis of over 18 million
   traffic stops, we show dramatic disparities in the rates at which black
   drivers, particularly young males, are searched and arrested as compared
   to similarly situated whites, women, or older drivers. Further, the
   degree of racial disparity is growing over time. Finally, the rate at
   which searches lead to the discovery of contraband is consistently lower
   for blacks than for whites, providing strong evidence that the empirical
   disparities we uncover are in fact evidence of racial bias. The findings
   are robust to a variety of statistical specifications and consistent
   with findings in other jurisdictions.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
