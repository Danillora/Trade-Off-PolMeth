@article{ WOS:000416491700004,
Author = {Bostan, Mehmet and Ergen, Mustafa},
Title = {THE ROLE OF WEALTH FUNDS IN AN ENTREPRENEURIAL ECONOMY},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2017},
Volume = {16},
Number = {2},
Pages = {39-46},
Month = {SUM},
Abstract = {Over the last decade, the role of the state in global economic policy
   has deepened, which particularly has implications for sovereign wealth
   funds (SWFs). SWFs government-owned investment vehicles established with
   a purpose to boost the national economy - originated in the 1950s but
   have grown significantly since then, now possessing huge amounts of
   investment capital. In this article, the authors argue that while each
   SWF has its own unique objective, on a whole SWFs are becoming key
   drivers of the entrepreneurial economy. Furthermore, they argue that the
   success of SWFs is contingent on building trust, establishing a
   reputable brand, inhabiting a start-up mindset, and subjective
   decision-making.},
