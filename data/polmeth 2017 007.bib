@article{ WOS:000411912900001,
Author = {Cantillana Pena, Carlos and Contreras Aguirre, Gonzalo and Morales
   Quiroga, Mauricio and Oliva, Daniela and Pere, Lucas},
Title = {The Malaise with Democratic Representation in Latin America},
Journal = {POLITICA Y GOBIERNO},
Year = {2017},
Volume = {24},
Number = {2},
Pages = {245-274},
Abstract = {Though there is a wide literature on the crisis of representation in
   Latin America, less is known about the political processes that precede
   the aforementioned crisiswhich translates into the feeling of malaise
   with democratic representation. Here we operationalize malaise as a
   combination of political disaffection, distrust with political
   institutions and government disapproval. We conclude that the malaise
   with democracy is not a systematic outcome of either the quality of
   democracy or the human development index. Rather, it is associated to
   inequality. At the individual level, malaise is more intense among the
   youth and in the lower-income segments of the population.},
Publisher = {CENTRO DE INVESTIGACION Y DOCENCIA ECONOMICAS},
