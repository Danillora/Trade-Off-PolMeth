@article{ WOS:000418292200003,
Author = {Planinc, Emma},
Title = {Catching Up with Wells: The Political Theory of H. G. Wells's Science
   Fiction},
Journal = {POLITICAL THEORY},
Year = {2017},
Volume = {45},
Number = {5},
Pages = {637-658},
Month = {OCT},
Abstract = {H. G. Wells's The Rights of Man (1940)-which provided the groundwork for
   the 1948 UN Declaration of Human Rights-has been re-released with a new
   Introduction by novelist Ali Smith, who reminds us of Wells's political
   prophetic call for ``a real federation of mankind,{''} and of the fact
   that we have still failed to meet the future he envisioned. If we are to
   catch up with Wells, we must, however, examine the foundations of
   Wells's ``cosmopolitan{''} vision, which requires examining both his
   scientific non-fiction and his scientific romances. Looking to Wells's
   The Island of Dr. Moreau in particular, and the influence of Wells's
   early scientific essays on Moreau's narrative, we get a picture of Wells
   as a writer and a man who is anxious about the identity and future of
   the human species, but who nevertheless puts his faith in the
   ``apparatuses{''} of ``education and moral suggestion,{''} which are
   held together by ``common faith.{''} Much like Charles Taylor and Simon
   Critchley, Wells calls for more than a political reconstitution, or
   institution, of right: he calls for a new cosmic imaginary, or supreme
   fiction, that has the potential to redeem and preserve the human
   species.},
