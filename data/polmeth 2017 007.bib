@article{ WOS:000426573000005,
Author = {Hove, Mediel and Chenzi, Vincent},
Title = {Terrorism: A Reflection on the Dynamics of Contemporary Suicide
   Terrorism},
Journal = {CONFLICT STUDIES QUARTERLY},
Year = {2017},
Number = {20},
Pages = {67-89},
Month = {JUL},
Abstract = {Suicide terrorism has evaded understanding and the war on terror is
   failing in its attempt to counter and even control it. This article
   argues that suicide terrorism is largely caused by indoctrination and,
   therefore, the key to understand and defeat it is through weakening and
   even conquering indoctrination carried out by terror organizations. It
   further asserts that although other factors such as finance, weapons,
   religion, political environment, structured organization, infrastructure
   and sponsors contribute to suicide operations, they are not central. To
   this end, efforts to counter suicide terrorism should focus on ending
   radicalisation and/or indoctrination of individuals and communities.},
