Title = {Presidents, Parties, and Inflation, 1957-2014},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2017},
Volume = {47},
Number = {2},
Pages = {245-254},
Month = {JUN},
Abstract = {Several studies have reported that the U.S. economy has experienced
   higher growth rates and/or lower unemployment under Democratic
   presidents than under their Republican counterparts. But only one study
   has examined whether the apparently more prosperous economy during
   Democratic administrations has come at the cost of higher inflation, and
   that study reached inconclusive results. We reexamine that question and
   find a positive association between inflation and Democratic
   presidencies only in the period from the 1950s to 1972. The contribution
   of Democratic presidencies to the rate of U.S. consumer price inflation
   was, however, quantitatively small even in that period.},
