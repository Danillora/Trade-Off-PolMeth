@article{ WOS:000418715300008,
Author = {Salem, Ola},
Title = {THE GULF CRISIS AND A PARIAH'S FLING WITH ISLAMISM},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2017},
Volume = {16},
Number = {3},
Pages = {81-91},
Month = {FAL},
Abstract = {For years, the Gulf states have been at war through proxies each side
   betting on the revolution it wants to drive, with Qatar leaning heavily
   toward Islamists in every case. For the anti-Qatar quartet, the Gulf
   crisis is a direct result of Doha's support for Islamist movements
   across the region. The determination of the countries opposed to Qatar
   is that only by addressing issues at home can the Gulf Cooperation
   Council (GCC) unite under one front and tackle larger regional issues,
   including terrorism. In this article, the author argues that resolving
   the issue with Qatar is the first step in uniting the GCC and ending
   years of proxy wars in the region.},
