@article{ WOS:000402631400006,
Author = {Kailitz, Steffen and Stockemer, Daniel},
Title = {Regime legitimation, elite cohesion and the durability of autocratic
   regime types},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2017},
Volume = {38},
Number = {3},
Pages = {332-348},
Month = {JUN},
Abstract = {We present a theory that addresses the question of why autocracies with
   a regime legitimation which ties the destiny of the members of the
   ruling elite, namely the nobility or ideocratic elite, to the survival
   of the autocracy, namely (ruling) monarchies and communist ideocracies,
   are more durable than other kinds of autocracies. Using logistic
   regression analysis and event history analysis on a dataset on
   autocratic regimes in the period 1946 to 2009, we are able to show that
   ruling monarchies and communist ideocracies are indeed the most durable
   autocratic regime types.},
