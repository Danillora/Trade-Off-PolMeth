@article{ WOS:000424567800007,
Author = {Martinez Olguin, Juan Jose},
Title = {The Community of Writing. The Dialogue between Blanchot and Nancy},
Journal = {ANACRONISMO E IRRUPCION},
Year = {2017},
Volume = {7},
Number = {13},
Pages = {173-188},
Month = {NOV-MAY},
Abstract = {The theme of the community was, in the 80's, the object of a fruitful
   exchange of texts between Jean-Luc Nancy and Maurice Blanchot. However,
   not only Blanchot and Nancy were involved in this exchange: both of them
   recognized themselves as heirs of a reflection that had begun, with
   different shades and at different times, with Nietzsche and Bataille.
   Several decades later, in The revoked community, Nancy returns to the
   debate that had himself as a protagonist: How far did Blanchot intend to
   go with the arguments he opposed against Nancy's own text? From that
   question, which opens one of the sections of Nancy's last essays, this
   paper tries to renew that debate with the purpose of rethinking the
   concept of community as community of writing.},
