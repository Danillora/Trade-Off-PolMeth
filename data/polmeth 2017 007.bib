@article{ WOS:000417438800002,
Author = {Sanchez Capdequi, Celso},
Title = {The Creative Ethos: Debates and Diagnoses about the New Modern
   Imperative},
Journal = {POLITICA Y SOCIEDAD},
Year = {2017},
Volume = {54},
Number = {3},
Pages = {621-640},
Abstract = {The text intends to explain the emergence of creativity as idea and
   ideal of late modernity. If the vision of classical sociology defines
   modernity as the hegemony of the instrumental rationality, in the late
   forty years the art is the core of the representation of reality. Art
   and science coexist in different fields of the social life. The artist
   has replaced the specialist as model of social actor and the creative
   action has become principle of social normality. The actors not only
   want to create, they must to do it and to be original. The creativity
   defines the daily life and introduces new expressions of domination. The
