@article{ WOS:000416123700002,
Author = {Post, Alison E. and Bronsoler, Vivian and Salman, Lana},
Title = {Hybrid Regimes for Local Public Goods Provision: A Framework for
   Analysis},
Journal = {PERSPECTIVES ON POLITICS},
Year = {2017},
Volume = {15},
Number = {4},
Pages = {952-966},
Month = {DEC},
Abstract = {There is a growing recognition that the state is not the sole provider
   of local public goods such as water and education in the developing
   world. Mainstream approaches to the study of local public goods
   provision, however, have yet to incorporate these insights. We offer a
   descriptive typology of hybrid local public goods regimes, or systems in
   which both state and non-state actors contribute to provision. It
   emphasizes two dimensions: the type of state involvement (direct versus
   indirect provision), and the degree of formal state penetration. The
   politics of producing local public goods, we argue, takes on distinct
   forms in each cell. The framework allows scholars to develop more
   accurate and precise explanations of variation in service quality and
   access, and to choose more appropriate outcome measures. We illustrate
   the utility of this framework by analyzing distinct hybrid regimes for
   water and sanitation, and mass transit in Africa, Asia, and Latin
   America.},
