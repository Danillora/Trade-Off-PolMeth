@article{ WOS:000405143200005,
Author = {Schnabel, Johanna},
Title = {Committed to Coordination? Intergovernmental Councils as a Federal
   Safeguard},
Journal = {SWISS POLITICAL SCIENCE REVIEW},
Year = {2017},
Volume = {23},
Number = {2},
Pages = {191-206},
Month = {JUN},
Abstract = {In this research note, I suggest that the design of intergovernmental
   councils (IGC) accounts for the extent to which they are able to prevent
   the federal government from encroaching on subnational jurisdictions.
   IGC operate in areas of interdependence where the federal government
   faces incentives to restore to hierarchical coordination. The effect of
   the intergovernmental safeguard is measured by the absence or presence
