@article{ WOS:000402143100007,
Author = {Burns, Kevin J.},
Title = {The Law: A Constitutionalist's Defense of Prerogative: Taft's Our
   Chief Magistrate and His Powers},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2017},
Volume = {47},
Number = {2},
Pages = {336-353},
Month = {JUN},
Abstract = {Traditional discussions of presidential prerogative power have grouped
   scholars into Jeffersonian and Hamiltonian camps, both of which seek to
   justify the broad-ranging use of executive power in an emergency while
   also admitting that the prerogative power is technically illegal or
   extralegal. This article explores William Howard Taft's Our Chief
   Magistrate and His Powers, which lays out a theory of Article II that
   permits the president to employ expansive executive means to attain
   limited constitutional ends. By both legalizing and limiting the
   president's use of prerogative, Taft's understanding of the Constitution
   and prerogative may contribute to the scholarly debate on executive
   power.},
