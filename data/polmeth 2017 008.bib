@article{ WOS:000418368000008,
Author = {Bethke, Felix S.},
Title = {Nonviolent Resistance and Peaceful Turnover of Power},
Journal = {PEACE ECONOMICS PEACE SCIENCE AND PUBLIC POLICY},
Year = {2017},
Volume = {23},
Number = {4, SI},
Month = {DEC},
Abstract = {Previous research has shown that nonviolent resistance (NVR) campaigns
   are beneficial for democratization. However, research to date has not
   considered whether nonviolent revolutions succeed long term in bringing
   about democratic consolidation. In this paper, I address this gap by
   analyzing the effect of NVR on democratic consolidation, using data on
   Huntington's consolidation criteria of two peaceful turnovers of power.
   The results suggest that initiating a democratic transition through NVR
   is not necessarily beneficial for achieving the first peaceful turnover
   of power. However, given that a democratic regime achieved a successful
   first turnover of power, NVR substantially increases the probability of
   completing democratic consolidation through a second peaceful turnover
   of power.},
