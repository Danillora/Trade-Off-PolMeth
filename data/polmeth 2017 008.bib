@article{ WOS:000404602000005,
Author = {Ross, Robert E.},
Title = {Recreating the House: The 1842 Apportionment Act and the Whig Party's
   Reconstruction of Representation},
Journal = {POLITY},
Year = {2017},
Volume = {49},
Number = {3},
Pages = {408-433},
Month = {JUL},
Abstract = {This article analyzes early debates over the size of the United States
   House of Representatives and the requirement to elect representatives
   using single-member districts. It focuses on early theories of
   representation during the 1790s and later developments during debates
   over the 1842 Apportionment Act, when Congress, for the first time,
   reduced the size of the House and required single-member districts. It
   was in the course of the 1842 debates that the Whig Party legitimized
   these electoral changes by articulating a theory of representation that
   synthesized the Federalists' and the Anti-Federalists' theories. The
   Whigs argued that the reduced House size provided for a more efficient
   deliberative body, and the single-member district requirement provided
   for better constituent representation, particularly given the
   development of political parties. This historical analysis enhances our
   understanding of representation by addressing how the Whigs legitimized
   electoral change, aiming to balance legislative efficiency with adequate
   representation in a way that became the foundation for how we currently
   understand representation.},
