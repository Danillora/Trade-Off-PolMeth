@article{ WOS:000416638300004,
Author = {Thornton, Rod},
Title = {THE RUSSIAN MILITARY'S NEW `MAIN EMPHASIS' ASYMMETRIC WARFARE},
Journal = {RUSI JOURNAL},
Year = {2017},
Volume = {162},
Number = {4},
Pages = {18-28},
Abstract = {The nature of the Russian military threat to Western interests needs to
   be reassessed. An examination of articles in Russian military journals
   shows that its senior ranks now see `asymmetric means and methods' as
   their new `main emphasis' in modern peer-state wars. Rod Thornton
   explains that for them, winning such wars through asymmetric means has
   become a key way to impose Moscow's will on other states. Remarkably,
   they seek to avoid the use of military violence.},
