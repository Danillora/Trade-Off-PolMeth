@article{ WOS:000409433000004,
Author = {Lane, Lottie and Hesselman, Marlies},
Title = {Governing Disasters: Embracing Human Rights in a Multi-Level, Multi-Duty
   Bearer, Disaster Governance Landscape},
Journal = {POLITICS AND GOVERNANCE},
Year = {2017},
Volume = {5},
Number = {2},
Pages = {93-104},
Abstract = {International and national disaster governance faces multiple challenges
   given the large variety and amounts of resources, skills and expertise
   that adequate disaster response commands. Moreover, disasters do not
   necessarily respect territorial boundaries, or may overwhelm the
   capacity of any one nation. They may therefore need a truly collective,
   joint, or even global effort to be overcome. Not seldom, reducing
   disaster risks and responding to disasters as they occur requires a
   sustained, concerted and coordinated effort of a broad range of actors,
   both public and private, acting nationally and internationally, and
   across the full `disaster cycle'. Unfortunately, disaster governance is
   commonly characterized as patchy, fragmented and inadequate, leading to
   essential protection gaps for affected communities. In order to
   strengthen disaster governance, this article first aims to further
   conceptualize the practice and challenges of `disaster governance',
   mostly through the lens of `Multi-Level Governance'. Secondly, it
   proposes that disaster governance will greatly benefit from relevant
   actors more firmly embracing human rights-based approaches, particularly
   in the context of so-called, emerging `multi-duty bearer human rights
   regimes'.},
