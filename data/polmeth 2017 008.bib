@article{ WOS:000423766600004,
Author = {Tellidis, Ioannis and Baek, Buhm-Suk},
Title = {Democracy and the War on Terror in South Korea},
Journal = {EAST ASIAN POLICY},
Year = {2017},
Volume = {9},
Number = {4},
Pages = {41-52},
Month = {OCT-DEC},
Abstract = {Although South Korea has had minimal exposure to terrorism, it recently
   adopted a controversial Anti-Terrorism Act that is characterised by
   arbitrary and vague definitions of `terrorism' and `terrorism'-related
   crimes. The Act risks manufacturing the `terrorism' phenomenon with the
   unnecessary curtailing of civil liberties and stifling of political
   dissent. This article argues that the legislation's objectives are more
   a rhetorical device to solidify power than to sustain the vibrancy of
   democratic politics and provide effective human security.},
