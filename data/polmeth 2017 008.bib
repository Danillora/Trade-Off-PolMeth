@article{ WOS:000392882800001,
Author = {Duggan, John},
Title = {May's theorem in one dimension},
Journal = {JOURNAL OF THEORETICAL POLITICS},
Year = {2017},
Volume = {29},
Number = {1},
Pages = {3-21},
Month = {JAN},
Abstract = {This paper provides three versions of May's theorem on majority rule,
   adapted to the one-dimensional model common in formal political modeling
   applications. The key contribution is that single peakedness of voter
   preferences allows us to drop May's restrictive positive responsiveness
   axiom. The simplest statement of the result holds when voter preferences
   are single peaked and linear (no indifferences), in which case a voting
   rule satisfies anonymity, neutrality, Pareto, and transitivity of weak
   social preference if and only if the number of individuals is odd and
   the rule is majority rule.},
