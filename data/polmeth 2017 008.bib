@article{ WOS:000419283800005,
Author = {Settis, Bruno},
Title = {The Constitution in the Shopfloor. Some notes on Fordist Social Contract
   and Constituent Compromise},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
Year = {2017},
Volume = {29},
Number = {57},
Pages = {69-83},
Abstract = {The Italian ``constitutional compromise{''} between the liberal,
   catholic, and socialist-communist traditions and the ``fordist-keynesian
   compromise{''} between capital and labor are two frequent set
   expressions. They are often put together, and sometimes overlapped,
   insofar as they share an emphasis on the central role of labor, the
   former putting it at the very foundation of the Republic, the latter at
   the core of the virtuous circle of mass production and mass consumption.
   Starting from a discussion of these formulas, the essay will propose
   some lines of research for the history of the Italian Republic, not
   without significance for contemporary political discourse.},
