@article{ WOS:000430363500002,
Author = {Li Jiacheng},
Title = {Developing China's Indian Ocean Strategy Rationale and Prospects},
Journal = {CHINA QUARTERLY OF INTERNATIONAL STRATEGIC STUDIES},
