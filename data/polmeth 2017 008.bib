@article{ WOS:000416581000003,
Author = {Patton, David F.},
Title = {Monday, Monday: Eastern Protest Movements and German Party Politics
   since 1989},
Journal = {GERMAN POLITICS},
Year = {2017},
Volume = {26},
Number = {4, SI},
Pages = {480-497},
Abstract = {Since 1989-90, sporadic but intense protest movements, centred in the
   east, have transformed German political parties, even those with West
   German origins. This article examines four major eastern protest
   movements: (a) the peaceful demonstrations of autumn 1989; (b) the
   anti-Hartz IV protests of summer 2004; (c) the burgeoning neo-Nazi free
   fellowships (freie Kamer-dschaften) of the mid-to-late 1990s and early
   2000s; and (d) Pegida rallies in 2014-16. It addresses the following
   questions. First, in what ways did eastern protest shape German parties
   and party systems? Second, why did the protest movements have such a
   varying impact on party politics?},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
