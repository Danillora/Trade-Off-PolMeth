@article{ WOS:000416433600002,
Author = {Vittori, Davide},
Title = {Re-conceptualizing populism: Bringing a multifaceted concept within
   stricter borders},
Journal = {REVISTA ESPANOLA DE CIENCIA POLITICA-RECP},
Year = {2017},
Number = {44},
Pages = {43-65},
Month = {JUL},
Abstract = {The word populism has been associated to (very) different meanings in
   the last years. The ``populist{''} label is still used to describe
   parties, leaders, movements, attitudes and political regimes, too.
   Moreover, the adjective ``populist{''} is used in a normative fashion in
   the public debate to denigrate those movements or parties which contrast
   the mainstream views. The aim of this paper is twofold: on the one hand,
   I conduct a non-normative analysis to avoid a biased vision of the
   concept. On the other hand, I advocate the understanding of populism as
   a thin-centered ideology, according to which it is based on two
   necessary features, namely, (a) an anti-elite(s) mindset and (b) the
   criticism of representative politics.},
Publisher = {ASOC ESPANOLA CIENCIA POLITICA \& ADM-AECPA},
