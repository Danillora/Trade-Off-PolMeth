@article{ WOS:000406823700006,
Author = {Adomeit, Hannes},
Title = {USA-Russia relations under Trump and Putin},
Journal = {OSTEUROPA},
Year = {2017},
Volume = {67},
Number = {5},
Pages = {115+},
Abstract = {At the beginning of his period in office, Donald Trump announced the
   start of better relations with Russia. Similar intentions had also been
   expressed by several US presidents but without result. Hopes in Moscow
   that Trump might be prepared to agree to Putin's demands to the
   disadvantage of the EU have proven groundless. While Russia may benefit
   from the chaos surrounding domestic policy in the US and from Trump's
   erratic foreign policy, there is no indication that the structural
   conflict between the US and Russia can be resolved. Moscow also
   continues to pursue its anti-western confrontation course.},
