@article{ WOS:000406050200007,
Author = {Sabl, Andrew},
Title = {Realist liberalism: an agenda},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
