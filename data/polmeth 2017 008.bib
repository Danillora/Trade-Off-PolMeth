@article{ WOS:000423969600008,
Author = {Murphy, Michael P. A.},
Title = {Using Active-Learning Pedagogy to Develop Essay-Writing Skills in
   Introductory Political Theory Tutorials},
Journal = {JOURNAL OF POLITICAL SCIENCE EDUCATION},
Year = {2017},
Volume = {13},
Number = {3},
Pages = {346-354},
Abstract = {Building on prior research into active learning pedagogy in political
   science, I discuss the development of a new active learning strategy
   called the ``thesis-building carousel,{''} designed for use in political
   theory tutorials. This use of active learning pedagogy in a graduate
   student-led political theory tutorial represents the overlap of several
   currents of political science education research. First, the graduate
   teaching assistant-led tutorial is a common complement to large
   lecture-based introductory courses in political theory. Second, where
   International Relations has led the expansion of active learning in
   political science, political theory has lagged behind other subfields
   (Archer \& Miller 2011). The thesis-building carousel is designed to
   develop skills necessary for political theory classes-essay writing and
   peer review. The article concludes with a discussion of how active
   learning activities designed for specialized spaces can be modified for
   use in traditional classroom settings, with the example of three forms
   of ``summary carousels.{''}},
