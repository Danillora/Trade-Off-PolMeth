@article{ WOS:000431730500002,
Author = {Raineri, Luca and Martini, Alice},
Title = {ISIS and Al-Qaeda as Strategies and Political Imaginaries in Africa: A
   Comparison between Boko Haram and Al-Qaeda in the Islamic Maghreb},
Journal = {CIVIL WARS},
Year = {2017},
Volume = {19},
Number = {4},
Pages = {425-447},
Abstract = {By analysing Boko Haram and Al-Qaeda in the Islamic Maghreb, this
   article argues that ascriptions to international jihadist brands are
   linked to local movements' political economy and geopolitical
   imaginaries, and, therefore, driven more by contingent strategic
   considerations rather than by ideological motives. Consequently, three
   sets of evidence are discussed, by drawing also on fieldwork conducted
   in Mali and Niger from 2013 to 2016: the discourses of these actors;
   their political economies; their use of political violence. In
