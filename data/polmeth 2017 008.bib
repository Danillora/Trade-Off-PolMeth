@article{ WOS:000417507700001,
Author = {Brown, Kerry},
Title = {China's Foreign Policy Since 2012 A Question of Communication and
   Clarity},
Journal = {CHINA QUARTERLY OF INTERNATIONAL STRATEGIC STUDIES},
Year = {2017},
Volume = {3},
Number = {3},
Pages = {325-339},
Month = {FAL},
Abstract = {China's success in developing its economy since 1978 has been
   accompanied by an increasing geopolitical role. In recent years, this
   has been accentuated by the increasing confusion about the resilience of
   its own systems and global visions by the United States and its allies.
   China has never had a larger stage to exercise its influence and present
   its values and vision. It has created a set of narratives for its
   foreign policy that conveys this vision in ways which attempt to avoid
   normativeness, but assert China's legitimate interests. Nevertheless, a
   number of challenges have emerged for China in the past five years and
   these will continue shaping China's self-image and global role in the
   future.},
