@article{ WOS:000422892700013,
Author = {Junk, Julian and Kroll, Stefan},
Title = {The Relationship between Sovereignty and Responsibility in International
   Law: The Responsibility to Protect and State Responsibility},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Number = {52},
Pages = {268+},
Abstract = {The article aims at tracing the tension between sovereignty and
   responsibility in both international law and international politics. It
   does so by scrutinizing two cases: First, the emerging norm of a
   Responsibility to Protect and second, the State Responsibility which is
   illustrated by the debate on investment protection. By analyzing these
   two cases, the article demonstrates that conceptualizing sovereignty as
   responsibility is necessarily linked to only one type of responsibility.
   This is the responsibility to take over a particular task in the future,
   and which is discussed mainly in the political realm. However, another
   type of responsibility, which is the accountability and which is
   connected to processes of juridification, is characterized by the
   traditional tension of sovereignty and responsibility.},
