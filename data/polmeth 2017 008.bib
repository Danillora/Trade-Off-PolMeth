@article{ WOS:000397502800006,
Author = {Stegman, Casey},
Title = {Remembering Atlantis: Plato's Timaeus-Critias, the Ancestral
   Constitution, and the Democracy of the Gods},
Journal = {POLITICAL THEORY},
Year = {2017},
Volume = {45},
Number = {2},
Pages = {240-260},
Month = {APR},
Abstract = {There has been much scholarly disagreement concerning Plato's
   participation in the mid-fourth century debates over Athens's ancestral
   constitution (patrios politeia). This disunity stems from contrasting
   views about the relationship between philosophy and Athenian politics in
   Plato's writings. Recently, several political theorists have reoriented
   our general understanding about Plato's complex involvement with
   Athenian politics. However, these discussions do not discuss Plato's
   specific relationship with patrios politeia. In order to bridge this
   gap, I turn to two dialogues within the later Platonic corpus: Timaeus
   and Critias. By examining the Atlantis myth that spans both dialogues, I
   discuss how Plato uses the story both to comment on and critique the
   democratic Athenian constitution. At the same time, however, Plato also
   advances a unique veneration of democracy by asserting that it is the
   politeia of the gods. In this way, I argue that Timaeus-Critias
   contributes a valuable new perspective in the ongoing debate regarding
   the relationship between Plato's philosophy and democracy.},
