@article{ WOS:000407501700003,
Author = {Simas, Elizabeth N.},
Title = {The effects of electability on US primary voters},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2017},
Volume = {27},
Number = {3},
Pages = {274-290},
Abstract = {The general consensus of the research on US primary contests is that
   voters consider candidates' potential for a general election victory
   when choosing their party's nominee. Yet, at the individual level, this
   literature has failed to (1) clearly isolate the effects of electability
   from the money and media attention that they generate; and (2) properly
   control for the potential effects of ideology. Using an original
   experimental design, I find that electability can increase the
   likelihood of a voter supporting a more ideologically distant candidate.
   I also show that when faced with a tradeoff, a large percentage of
   subjects from both parties choose electability over ideology. The
   resulting implication is that there is potential for moderates to be
   successful in primaries, as even ideologically extreme voters appear to
   be willing to compromise on policy representation when confronted with a
   more distant but electable candidate.},
