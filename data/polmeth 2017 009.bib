@article{ WOS:000407688000006,
Author = {Lida, Miranda},
Title = {Between audacity and moderation. The Russian Revolution, the Argentine
   magaYine Nosotros and Roberto Giusti (1917-1927)},
Journal = {IZQUIERDAS},
Year = {2017},
Volume = {33},
Pages = {111-134},
Month = {MAY},
Abstract = {The cultural journal Nosotros edited in Buenos Aires between 1907 and
   1943 attained a prestigious place in the Argentine culture. This article
   analyzes the impact of the Russian revolution in the magazine and in the
