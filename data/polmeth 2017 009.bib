@article{ WOS:000392647600005,
Author = {Bolleyer, Nicole and Weeks, Liam},
Title = {From cartel party to traditional membership organisation: the
   organisational evolution of Fianna Fail},
Journal = {IRISH POLITICAL STUDIES},
