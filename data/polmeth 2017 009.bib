@article{ WOS:000407107300001,
Author = {Krimmel, Katherine and Rader, Kelly},
Title = {The Federal Spending Paradox: Economic Self-Interest and Symbolic Racism
   in Contemporary Fiscal Politics},
Journal = {AMERICAN POLITICS RESEARCH},
Year = {2017},
Volume = {45},
Number = {5},
Pages = {727-754},
Month = {SEP},
Abstract = {We show how symbolic politics condition public opinion on federal
   spending and how this helps to explain an important puzzle in
   contemporary American politics. Using multilevel regression and
   poststratification to estimate state-level opinion on federal spending,
   we show that, curiously, opposition to federal spending is higher in
   states receiving more federal money, per tax dollar paid. Belying the
   popular narrative surrounding so-called red state socialism, we find
   that simple hypocrisy does not explain this paradoxindividuals who are
   likely to benefit from spending tend to support it. But, income is a
   more powerful predictor of opinion on spending in taker states than
   giver states, heightening state-level opposition in the former. There is
   also more to the story than economic self-interest. Symbolic racism is
   four times more powerful than income in explaining opposition to
   spending, and there are more people with such attitudes in states
   receiving more federal money.},
Publisher = {SAGE PUBLICATIONS INC},
