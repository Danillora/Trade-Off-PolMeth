@article{ WOS:000431730500005,
Author = {Tarin Sanz, Adrian},
Title = {The Ethnic Russian as an Enemy of Islam: Frame Analysis of the Kavkaz
   Center News Agency (2001-2004)},
Journal = {CIVIL WARS},
Year = {2017},
Volume = {19},
Number = {4},
Pages = {495-516},
Abstract = {Typically, when analysing contemporary Russian-Chechen conflicts, the
   relegation of the nationalist struggle to a secondary role by the
   religious battle waged by the North Caucasus insurgency is pinpointed as
   one of the fundamental differences between the First and Second War in
   Chechnya. This article discusses how it was reflected in one of the most
