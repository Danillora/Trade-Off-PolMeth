@article{ WOS:000413322600002,
Author = {Uzzell, Lynn},
Title = {A New Investigation into Madison's Notes of the Convention: Solving the
   Mystery of His June 6 Speech},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2017},
Volume = {6},
Number = {4},
Pages = {517-549},
Month = {FAL},
Abstract = {James Madison's Notes of the Constitutional Convention have long been
   the gold standard for understanding how the Constitution was formed.
   Nevertheless, there are a series of troubling discrepancies within the
   extant records that give contradictory accounts of Madison's June 6
   speech, which is Madison's first semipublic expression of the argument
   later immortalized in Federalist no. 10. According to a provocative new
   theory advanced by the 2015 book Madison's Hand, these various
   discrepancies point to a larger scandal of Madison's routine negligence,
   frauds, forgeries, and cover-ups when he was writing his Notes. This
   article will demonstrate that this new hypothesis fails on several
   levels to provide a plausible solution. Furthermore, a fresh look at the
   clues, including ones that have never been noticed before, will finally
   point us in the direction of a satisfactory solution to this
   long-standing mystery. More importantly, this new solution reestablishes
   the reliability of Madison's Notes.},
