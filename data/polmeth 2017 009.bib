@article{ WOS:000416746800008,
Author = {Franceschet, Susan and Annesley, Claire and Beckwith, Karen},
Title = {What do women symbolize? Symbolic representation and cabinet
   appointments},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2017},
Volume = {5},
Number = {3},
Pages = {488-493},
Abstract = {Women's presence in cabinets around the world has expanded considerably
   in the past two decades. All male cabinets are all but unthinkable in
   most democracies today. Yet existing studies of cabinet formation pay
   little systematic attention to the representative dimensions of
   cabinets. We argue that the concept of symbolic representation is useful
   for analyzing ministerial recruitment because cabinets are not mere
   collections of individuals with political skills or policy expertise.
   Cabinets are sites of representation, and ministers are often chosen in
   light of informal rules about which groups must be represented in
   cabinet. The concept of symbolic representation draws attention to both
   the symbol maker (in this case, the chief executive who selects
   ministers) and the multiple audiences to whom the symbol is directed. As
   such, the concept is well suited to reveal how gender plays out in
   cabinet formation, particularly when symbolic representation is
   contested by audiences.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
