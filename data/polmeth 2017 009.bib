@article{ WOS:000403273800003,
Author = {Brant, Hanna K. and Myers, Nathan and Runge, Katherine L.},
Title = {Promotion, Protection, and Entrepreneurship: Stakeholder Participation
   and Policy Change in the 21st Century Cures Initiative},
Journal = {POLITICS \& POLICY},
