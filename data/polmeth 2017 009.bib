@article{ WOS:000414596600010,
Author = {Brasil, Felipe Goncalves and de Mattos e Silva, Ivan Henrique and
   Zambello, Aline Vanessa},
Title = {The Effects of Policy Change on Brazil's Public Higher Education System},
Journal = {LATIN AMERICAN POLICY},
Year = {2017},
