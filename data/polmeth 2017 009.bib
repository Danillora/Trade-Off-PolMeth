Language = {English},
Affiliation = {Leblang, D (Corresponding Author), Univ Virginia, 185 Gibson Hall, Charlottesville, VA 22904 USA.
   Leblang, David, Univ Virginia, Polit \& Publ Policy, Charlottesville, VA USA.},
DOI = {10.1177/0010414015606736},
ISSN = {0010-4140},
EISSN = {1552-3829},
Keywords = {migration; political economy; remittances; dual citizenship},
Keywords-Plus = {NETWORKS; TRANSNATIONALISM; NATURALIZATION; MIGRATION; POLITICS; MEXICO;
