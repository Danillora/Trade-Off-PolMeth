@article{ WOS:000407685400005,
Author = {Haroche, Pierre},
Title = {Transposing the Fifth Republic: The National Political Regime as a
   Source of France's European Policy},
Journal = {POLITIQUE EUROPEENNE},
Year = {2017},
Volume = {55},
Number = {1},
Pages = {116-141},
Abstract = {This article contends that the logic of the regime of the Fifth Republic
   influenced France's European policy, especially towards the European
   Parliament. This argument is illustrated by two case studies: the 1965
   Empty Chair crisis and the 1986 Single European Act. In both cases,
   France's position was defined by the executive, which sought to
   transpose to the European level its practice of subordinating the French
   Parliament, giving Council powers priority over those of the European
   Parliament.},
