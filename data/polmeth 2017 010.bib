Affiliation = {Harvey, CJ (Corresponding Author), Univ N Carolina, Dept Polit Sci, Chapel Hill, NC 27599 USA.
   Greenstein, Claire; Harvey, Cole J., Univ N Carolina, Dept Polit Sci, Chapel Hill, NC 27599 USA.},
DOI = {10.1080/13510347.2017.1304380},
ISSN = {1351-0347},
EISSN = {1743-890X},
Keywords = {Transitional justice; electoral manipulation; trials; lustration;
   democratization},
