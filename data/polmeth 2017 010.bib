@article{ WOS:000417175400006,
Author = {Smirnov, V. A.},
Title = {THEORETICAL AND METHODOLOGICAL ASPECTS OF POLITICAL ELITES'
   INSTITUTIONALIZATION IN THE BALTIC STATES},
