@article{ WOS:000437685300004,
Author = {Bogoya Caviedes, Giovanny Eduardo},
Title = {The role of collective memory: an experience with women victims of the
   conflict in the municipality of Granada, Cundinamarca},
Journal = {CIUDAD PAZ-ANDO},
