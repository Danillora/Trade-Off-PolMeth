@article{ WOS:000394733300003,
Author = {Parasher, Tejas},
Title = {Inequality and Turnulti in Machiavelli's Aristocratic Republics},
Journal = {POLITY},
Year = {2017},
Volume = {49},
Number = {1},
Pages = {42-68},
Month = {JAN},
Abstract = {This article reassesses the portrayal of aristocratic government in
   Machiavelli's Discourses on Livy. Political theorists working on
   Machiavelli's analysis of regime types have interpreted aristocratic
   republics in the Discourses to be internally stable and isolationist,
   and popular republics to be tumultuous and expansionist. Focusing on the
   issue of stability, I argue that such a clear distinction between the
   two regimes does not in fact exist for Machiavelli. Rather, it is by
   emphasizing close similarities in class relations within aristocracies
   and popular governments that Machiavelli formulates a new understanding
   of the relationship between constitutional form and inequality. Viewing
   the disciplinary laws and redistributive mechanisms of aristocratic
   republics to be insufficient in preventing class conflict, Machiavelli
   overturns a core argument of Renaissance political theory: that a
   politically hierarchical regime could remain just under a constitution
   committed to controlling the effects of private wealth. I draw out
   Machiavelli's critique of aristocratic constitutionalism by re -reading
   his accounts of Sparta and Venice, the two exemplary elite -ruled
   republics in the Discourses.},
