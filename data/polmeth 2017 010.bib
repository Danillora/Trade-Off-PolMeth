@article{ WOS:000397496000006,
Author = {Steenbergen, Marco R. and Siczek, Tomasz},
Title = {Better the devil you know? Risk-taking, globalization and populism in
   Great Britain},
Journal = {EUROPEAN UNION POLITICS},
Year = {2017},
Volume = {18},
Number = {1, SI},
Pages = {119-136},
Month = {MAR},
Abstract = {Right-wing populist parties in European democracies appeal to citizens'
   feelings of uncertainty related to globalization by promoting tough
   immigration laws and curbing the power of the European Union. This
   article adds to our understanding of how individuals' risk propensity
   relates to support for right-wing populist parties and their ideas in
   the context of globalization. In particular, by drawing on survey data
   from the United Kingdom we investigate how this personality trait
   relates to support for the United Kingdom Independence Party and the
   vote for a British exit from the European Union. The article explores
   the complex interplay between risk propensity and right-wing populist
   appeals by dissecting the direct, indirect and total effects of this
   trait.},
