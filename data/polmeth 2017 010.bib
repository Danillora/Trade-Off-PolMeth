@article{ WOS:000395435100007,
Author = {Wei, Judy Chia Yin},
Title = {The Strategic Coordination under Quasi-SNTV: A Case Study of Hong Kong},
Journal = {JAPANESE JOURNAL OF POLITICAL SCIENCE},
