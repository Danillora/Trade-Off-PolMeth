@article{ WOS:000398817800005,
Author = {Bukow, Sebastian and Switek, Niko},
Title = {Does the economy trump the environment? Programmatic changes of green
   parties in the wake of the global financial and economic crisis},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2017},
Volume = {11},
Number = {1},
Pages = {104-128},
Month = {MAR},
Abstract = {Until today, the global financial crisis that commenced in 2007
   continues to cast its shadow beyond the economic realm. This paper
   analyzes in how far political parties in Europe have adapted or modified
   their election manifestos as a reaction to this external shock. In doing
   so, the analysis focuses on members of the green party family with their
   distinct postmaterialist heritage. In the tradition of party family
   research, we test whether these political parties reacted coherently or
   whether the programmatic changes were largely due to electoral
   competition within the respective national context. The results show
   that the effect of party family membership on changes in green parties'
   election manifestos was secondary to the influence of changes in the
   respective party system as well as the extent to which the respective
   countries were afflicted by the crisis. Thus, our paper contributes to
   research on party families and provides insights on (green) parties'
   responsiveness and programmatic changes in times of crisis.},
