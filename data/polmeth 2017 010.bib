@article{ WOS:000407501000004,
Author = {Plescia, Carolina and Kritzinger, Sylvia},
Title = {Retrospective voting and party support at elections: credit and blame
   for government and opposition},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2017},
Volume = {27},
Number = {2},
Pages = {156-171},
Abstract = {Retrospective voting is arguably one of the most important mechanisms of
   representative democracy, and whether or not the public holds the
   government accountable for its policy performance has been extensively
   studied. In this paper, we test whether retrospective voting extends to
   parties in the opposition, that is whether and how parties' past
   performance evaluations affect their vote, regardless of whether they
   were in government or in opposition. Taking advantage of a rich set of
   questions embedded in a representative German national elections panel,
   we update our knowledge on the retrospective voting mechanism by
   modeling retrospective voting at the party level. The findings indicate
