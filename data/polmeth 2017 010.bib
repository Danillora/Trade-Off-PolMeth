@article{ WOS:000458033900002,
Author = {Gel'man, Vladimir},
Title = {POLITICS VERSUS POLICY: TECHNOCRATIC TRAPS OF POST-SOVIET REFORMS},
Journal = {POLITEIA-JOURNAL OF POLITICAL THEORY POLITICAL PHILOSOPHY AND SOCIOLOGY
   OF POLITICS},
Year = {2017},
Volume = {85},
Number = {2},
Pages = {32+},
Abstract = {After the collapse of the Soviet Union, the post-Soviet countries
   carried out policy reforms largely within the framework of the
   technocratic model. The transformation projects were elaborated and
   partly implemented by the groups of professionals under the mandate
   granted to them by the political leadership. The latter sought to
   consolidate its monopoly over decision-making and assessing reform
   projects realization, ``isolating{''} their content from the impact of
   public opinion. The article is devoted to a critical analysis of the
   technocratic model of policy in the post-Soviet Eurasia of the
   1990-2010s. The author focuses on the political and institutional
   constraints associated with the influence of vested interests and
   mechanisms of functioning of the state apparatus. The final part of the
   article discusses alternatives to the technocratic model, the prospects
   for their implementation and the potential effect.},
