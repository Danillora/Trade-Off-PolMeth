@article{ WOS:000417991800006,
Author = {Tormey, Simon and Gagnon, Jean-Paul},
Title = {The End of Representative Politics, Revisited},
Journal = {DEMOCRATIC THEORY-AN INTERDISCIPLINARY JOURNAL},
Year = {2017},
Volume = {4},
Number = {2},
Pages = {82-101},
Month = {WIN},
Abstract = {In reinterrogating core concepts from his 2015 book, The End of
   Representative Politics, Simon Tormey explains the nature of emergent,
   evanescent, and contrarian forms of political practice. He sheds light
   on what is driving the political disruption transpiring now through a
   series of engaging comments from the field on well-known initiatives
   like Occupy, \#15M, and Zapatistas and also lesser-known experiments
   such as the creation of new political parties like Castello en Moviment,
   among others. Post-representative representation, it is argued, is not
   an oxymoron; it, like the term antipolitical politics, is rather a
   provocative concept designed to capture the radically new swarming
   politics underway in countries like Spain, Italy, Greece, Portugal, and
   Iceland. Citizens are tooling up with ICTs, and this has led to resonant
   political movements like \#15M in Spain or Occupy more broadly. Key
   takeaways from this interview include the double-edged nature of
   representation and the fact that new forms of political representation
   are breaking the mould.},
