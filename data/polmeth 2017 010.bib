@article{ WOS:000417655500002,
Author = {Newman, Stephen L.},
Title = {Finding the Harm in Hate Speech: An Argument against Censorship},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2017},
Volume = {50},
Number = {3},
Pages = {679-697},
Month = {SEP},
Abstract = {The liberal justification for censorship equates the harm in hate speech
   with the sort of tangible injury that would justify state intervention
   under J.S. Mill's harm principle. Recently, Jeremy Waldron has suggested
   that the real harm perpetuated by hate speech is less tangible, taking
   it to be a variety of moral pollution which undermines both the public
   good of inclusiveness and the minority's assurance of personal dignity.
   This paper scrutinizes Waldron's conception of the harm in hate speech,
   arguing that it lacks the specificity and gravity Mill's principle
   requires in order to justify censorship. The paper also questions the
   categorical distinction between hate speech and speech that is ``merely
   offensive,{''} arguing that Waldron's reasons for censoring the one also
   apply to the other. The result is a censorship regime that liberals
   ought not to accept.},
