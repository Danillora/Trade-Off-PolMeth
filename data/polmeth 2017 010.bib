@article{ WOS:000407687300001,
Author = {Lauria Monteiro, Marcio},
Title = {The forgotten history of Trotskyism in the Spanish Revolution: the POUM
   and the ``Bolshevik-Leninists{''}},
Journal = {IZQUIERDAS},
Year = {2017},
Volume = {32},
Pages = {1-17},
Month = {FEB},
Abstract = {This article deals with the history of Trotskyism in the Spanish
   Revolution. It aims at clarifying the actual relationship of the POUM
   with this political current -by attempting to deconstruct the frequent
   and erroneous association of the two - and at detailing the practical
   activity and the political analyses of Leon Trotsky and his allies in
   Spain at the time. This effort of a historical rescue is based upon
   mainly in secondary sources works of historical analysis that are little
   known of the general public- and, in a smaller portion, also on some
   primary sources - especially Trotsky's writings on the subject.},
