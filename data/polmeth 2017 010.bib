@article{ WOS:000423946900007,
Author = {Lavin, Chad},
Title = {Escaping the Spatial Imaginary, or, Politics as an Occupation},
Journal = {NEW POLITICAL SCIENCE},
Year = {2017},
Volume = {39},
Number = {4, SI},
Pages = {538-553},
Abstract = {This article examines the persistence of spatial metaphors in modern
   political thought, from the ``public sphere{''} that has defined liberal
   and republican thinking for the past few centuries to the high-profile
   ``occupations{''} that have animated political action of the past few
   years. The article argues that these metaphors reflect a ``spatial
   imaginary{''} that corresponds to an ideal of politics as the forcible
   control of space, and then explores the possibility of a ``temporal
   imaginary{''} more compatible with the imagination and aspiration
   required of progressive politics. The article explores how the dual
   meaning of the term ``occupation{''} - the filling of space and the
   filling of time reflects the dialectic of time and space, and considers
   what happens when a spatial imaginary overshadows the temporal dimension
   of politics. Ultimately, the article argues that a spatial imaginary
   tends to inform an idea of politics as a functional use of force, while
   a temporal imaginary forwards a greater appreciation for history, civic
   work, and creativity.},
