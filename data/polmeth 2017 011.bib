@article{ WOS:000405983700009,
Author = {Chapman, Michael},
Title = {Against the Wall: Ideology and Form in Mies van der Rohe's Monument to
   Rosa Luxemburg and Karl Liebknecht},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2017},
Volume = {29},
Number = {1},
Pages = {199-213},
Abstract = {This essay looks at Mies van der Rohe's 1926 Monument to Rosa Luxemburg
   and Karl Liebknecht from the perspective of contemporaneous theories of
   Marxism, and particularly those of Luxemburg herself. The essay sets out
   to explore the political backdrop against which the monument was made,
   Mies's own political allegiances at the time, and the symbolic and
   spatial implications of this influential architectural monument. The
   essay reflects on the relationship between avant-garde practices and
   Marxism, and the role of ideology in framing certain practices within
   architectural modernism.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
