@article{ WOS:000411585200010,
Author = {Cannataro, Italia Maria},
Title = {Reshaping the State. Cuba from Rectificacion to Periodo
   Especial},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
Year = {2017},
Volume = {29},
Number = {56},
Pages = {207-222},
Abstract = {The crisis of communism and the collapse of the Soviet Union, and the
   accompanying shock waves felt in Cuba starting in 1991, intensified
   internal divisions within the regime. This article has a twofold
   purpose. First, it seeks to present and assess critically the changes
   that took place in Cuban society, economy, politics, and culture as Cuba
   emerged from the rectificacion and the crisis of the 1990s. Second, it
   analyzes the Constitution of 1992 connected with the new dynamic between
   state and society that is shaped by the characteristics of Cuba's
   national history, as well as the interactions between exogenous and
   endogenous factors during the ``Special Period{''}.},
