@article{ WOS:000405254200016,
Author = {Eichwede, Wolfgang},
Title = {The Art of Seizing the Moment Restitution between Expertise and
   Diplomacy: from the Engine Room of German-Russian Cultural Relations},
Journal = {OSTEUROPA},
Year = {2017},
Volume = {67},
Number = {3-4},
Pages = {181+},
Abstract = {In art restitution, traumatic history and dramatic present are densely
   intertwined. This applies in particular to the relationship between
   Germany and Russia. A look back over 30 years of cooperation and
   conflicting actions in relation to looted and stolen art reveals a
   history of opening and gift -giving, struggle and failure. Academia and
   society have shown (sic) settlement was missed. However, time and again,
   in individual cases, it has been possible, through skill and commitment,
   to circumnavigate the legal barriers. Thus, in culture, social relations
   remain that have not been destroyed by the current political upheaval.
   These relations cannot overcome the conflict between Russia and Germany,
   but are at least able to resist the process of drifting apart.},
