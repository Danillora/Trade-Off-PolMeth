@article{ WOS:000402780000007,
Author = {Conrad, Benjamin},
Title = {The Geographer and His Borders},
Journal = {OSTEUROPA},
Year = {2017},
Volume = {67},
Number = {1-2},
Pages = {89+},
Abstract = {Eugeniusz Romer is considered the founder of modern Polish geography. As
   a geographer, he also participated in the peace negotiations after the
   First World War. He did not see himself as a neutral expert, but
   committed himself to the restoration of the Polish state. The Polish
   delegation's successes at the 1919 Paris conference are in no small part
   attributed to his preliminary cartographic work. At the Riga conference
   in 1920-21, Romer's primary objective was no longer the largest national
   territory possible, but rather the most ethnically homogenous population
   possible. Here, however, his influence was already waning. Military
   interests gained the upper hand.},
