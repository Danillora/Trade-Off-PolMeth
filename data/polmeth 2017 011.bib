@article{ WOS:000395185500002,
Author = {Fisk, Jonathan M. and Davis, Charles and Cole, Benjamin},
Title = {``Who Is at ``Fault?'{''} The Media and the Stories of Induced
   Seismicity},
Journal = {POLITICS \& POLICY},
Year = {2017},
Volume = {45},
Number = {1},
Pages = {31-50},
Month = {FEB},
Abstract = {Media attention to earthquake risks linked to the underground injection
   of produced waters from oil and gas fracking operations has increased
   over the past few years. However, little scholarly attention has been
   devoted to newspaper coverage of events and state policy actions that
   are associated with induced seismicity. A key concern addressed in this
   article is whether newspaper coverage is largely unrestrained or is
   restricted by other factors. Is coverage by differing newspapers
