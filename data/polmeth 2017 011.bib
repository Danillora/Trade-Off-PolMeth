@article{ WOS:000396412200008,
Author = {Crewe, Emma},
Title = {Ethnography of Parliament: Finding Culture and Politics Entangled in the
   Commons and the Lords},
Journal = {PARLIAMENTARY AFFAIRS},
Year = {2017},
Volume = {70},
Number = {1},
Pages = {155-172},
Month = {JAN},
Abstract = {Ethnographic approaches are beginning to percolate through political
   science, but are often taken up as a `method', rather than an approach
   to methodology and theory. I describe my experience of doing ethnography
   in the Houses of Lords and Commons. Through the themes of whipping and
   gender, I explain how theory and method were interwoven and how
   reflexivity improves rigour. Dealing with the methodological challenge
   of disjunctions and contradictions is explored through the case studies
   of constituency work, law-making and ceremonies. Finally, I remark on
   how ethnographic approaches to ethics entail attention to process and
   relationships rather than compliance with rules.},
