@article{ WOS:000404761500004,
Author = {Riley, John and Pearson, Kristin and Schneider, Mary Kate and Stimeling,
   Lindsey},
Title = {Escaping the LRA: Examining the Decision to Disengage from Militarized
   Dissident Groups},
Journal = {AFRICAN SECURITY},
Year = {2017},
Volume = {10},
Number = {2},
Pages = {80-102},
Abstract = {This study seeks to understand the conditions that led individuals to
   accept the risks associated with leaving the Lord's Resistance Army. To
   this end, a large participant survey of former rebels was conducted.
   Results from the survey suggest that the investment model offers a
   useful tool for capturing the factors that weigh into an individual's
   decision-making process as he or she considers disengaging from a group.
   However, the data also suggest that lack of investment does not equate
   to disengagement. The perceived opportunity for escape and the costs
   associated with the risk of escape play critical roles. Consequently,
   counterinsurgency campaigns and postconflict reconciliation efforts in
   Africa ought to be jointly calibrated to maximize the likelihood of
   disengagement.},
