@article{ WOS:000405063100002,
Author = {Blaette, Andreas and Wuest, Andreas M.},
Title = {Migration-specific Effects on Parliamentary Action: hypothesis testing
   on the content of speeches held in Germany's Bundestag between 1996 and
   2013},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Volume = {58},
Number = {2},
Pages = {205-233},
Abstract = {The rise in the parliamentary presence of politicians of immigrant
   origin raises the question of whether they represent citizens
   substantially different from native politicians. For the first time, a
   content analysis of all speeches given in the Deutscher Bundestag
   between 1996 and 2013 analyzes this research question systematically.
   There are two main results': First, in their speeches, parliamentarians
   of immigrant origin use some words more often, especially pertaining to
   matters of migration and integration. They put more emphasis on
   migration-related issues. Second, the analysis shows that parliamentary
   and mandate-specific factors do also structure discussions on migration
   and integration issues in the Bundestag.},
