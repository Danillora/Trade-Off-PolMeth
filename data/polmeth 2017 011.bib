@article{ WOS:000424066600011,
Author = {Halbach, Uwe},
Title = {Russia and the Caucasus Two books about a difficult relationship},
Journal = {OSTEUROPA},
Year = {2017},
Volume = {67},
Number = {9-10},
Pages = {153+},
Abstract = {In Russia, it has been alleged for some years now that western
   historiography is distorting the history of the Russian Empire and the
   Soviet Union. Two books about Russian rule in the Caucasus disprove this
   assertion. Jeronim Perovic presents a nuanced picture of rebellion and
   adaptation in the multi-ethnic region of the Northern Caucasus. Philipp
   Ammon looks to the Southern Caucasus and illustrates how in Georgia,
   Russia has been perceived sometimes as a protector and sometimes as a
   colonial power.},
