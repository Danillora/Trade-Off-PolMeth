@article{ WOS:000406050200003,
Author = {McQueen, Alison},
Title = {Political realism and the realist `Tradition'},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2017},
Volume = {20},
Number = {3},
Pages = {296-313},
Abstract = {Appeals to a `tradition' stretching back to Thucydides have been central
   to the recent emergence of realism in political theory. This article
   asks what work these appeals to tradition are doing and whether they are
   consistent with contemporary political realism's contextualist
   commitments. I argue that they are not and that realists also have
   independent epistemic reasons to attend to contextualist worries.
   Ultimately, I make the case for an account of the realist tradition that
   is at once consistent with moderate contextualist commitments and that
   preserves the classificatory and analytical value of tradition-building.},
