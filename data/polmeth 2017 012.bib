   Bolleyer, Nicole, Univ Exeter, Comparat Polit, Exeter, Devon, England.
   Bytzek, Evelyn, Univ Koblenz Landau, Dept Polit Sci, Mainz, Germany.},
DOI = {10.1177/1354068815626604},
ISSN = {1354-0688},
EISSN = {1460-3683},
Keywords = {cross-national analysis; new parties; party building; party
   organization; party performance},
