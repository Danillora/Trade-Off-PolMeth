@article{ WOS:000406159400007,
Author = {Suchkov, Maxim A.},
Title = {RUSSIA'S{''} POST-WEST WORLD ORDER{''}: WHY TURKEY MATTERS},
Journal = {TURKISH POLICY QUARTERLY},
