@article{ WOS:000415091200011,
Author = {Millard, Andrew S. and Yi, Chae-Deug},
Title = {The EU's Potential Role in the Six Party Talks and the North Korean
   Nuclear Crisis},
Journal = {BALTIC JOURNAL OF EUROPEAN STUDIES},
Year = {2017},
Volume = {7},
Number = {2},
Pages = {247-283},
Month = {OCT},
Abstract = {Discourse on the Six Party Talks has focused solely on denuclearisation.
   Through the power struggles of the members and the refusal of the
   People's Democratic Republic of Korea (DPRK) to return to negotiations,
   the Six Party Talks have been stalled since 2008. Due to current
   increased tensions and the use of brinkmanship tactics the Talks must be
   restarted, albeit under a reformed shape. This paper analyses the
   potential role of the EU in furthering the peace process in Northeast
   Asia. This paper suggests that the EU needs to be more assertive and the
   Talks should focus on building trust and cooperation, not on the DPRK's
   nuclear program. With its impartiality, experience in integration and
   use of soft power, the EU can act as an effective mediator building
   trust.},
