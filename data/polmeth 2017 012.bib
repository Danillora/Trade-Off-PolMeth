@article{ WOS:000424805500001,
Author = {Unzue, Martin and Emiliozzi, Sergio},
Title = {Public policies of Science and Technology in Argentina: a balance in the
   period 2003-2015},
Journal = {TEMAS Y DEBATES},
Year = {2017},
Number = {33},
Pages = {13-33},
Month = {JAN-JUN},
Abstract = {This paper presents an analysis of the innovations and the continuities
   that were implemented in the field of public policies of Science and
   Technology throughout the cycle of Kirchner governments in Argentina.
   It will be proposed to distinguish between the initial meanings of S \&
   T policies (2003-2006) that were deployed in the midst of a complex
   context, and where priorities appeared to be in other areas of
   government, followed by a cycle of growth of investment and expansion of
   the area, concomitant with the decision to give greater visibility to
   the link between S \& T and economic development (period 2007-2010).
   Finally, the cycle of adjustments on this idea of growth, which are
   deployed from 2010 to the end of the presidential term, and which seek
   to explore ways of building governance on the research community.},
