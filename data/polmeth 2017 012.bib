@article{ WOS:000396662700002,
Author = {Managhan, Tina},
Title = {We all dreamed it: the politics of knowing and un-knowing the
   ``war on terror{''}},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2017},
Volume = {10},
Number = {1},
Pages = {22-43},
Abstract = {This article begins via an exploration of Jean Baudrillard's provocative
   claim that we dreamed of the `events of 9/11' prior to their occurrence.
   Baudrillard's particular quote is introduced to raise questions about
   the politics of knowing and un-knowing in International Relations, with
   specific reference to risk and the ``war on terror{''}. Building on
   postcolonial scholarship, this article points to the limits of
   contemporary approaches to risk and offers an alternative methodological
   approach -one it argues better identifies the power relations that
   structure the daily forms of knowing and un-knowing that give meaning to
   and invigorate articulations of risk.},
