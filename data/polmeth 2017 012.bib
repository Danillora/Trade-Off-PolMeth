@article{ WOS:000424155000004,
Author = {Plank, Friedrich},
Title = {When Peace Leads to Divorce: The Splintering of Rebel Groups in
   Powersharing Agreements},
Journal = {CIVIL WARS},
Year = {2017},
Volume = {19},
Number = {2},
Pages = {176-197},
Abstract = {While research has already focused on power-sharing agreements by
   assessing specific effects of political, economic, territorial and
   military provisions, some provisions might be more important than
   others. This paper argues that the content of an agreement is crucial
   for the cohesion of a signatory rebel group since these are strongly
   affected by resource-distribution and the way in which the underlying
   conflict is managed. Investigating on the Free Aceh Movement and the
   Moro National Liberation Front, this study concludes that the
   implementation of power-sharing is decisive. When inner-core provisions
   and single factions are addressed, groups tend to stay united.},
