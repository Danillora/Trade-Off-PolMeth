@article{ WOS:000458543100004,
Author = {Lira, Erygeanny and Araujo, Cicero},
Title = {The Formation of the Brazilian State and the Question of Sovereignty in
   the Period of Crisis (1822-1824)},
Journal = {REVISTA ESTUDOS POLITICOS},
Year = {2017},
Volume = {8},
Number = {2},
Pages = {50-66},
Abstract = {This study analyzes how the concept of sovereignty was mobilized in
   Brazil between the years of 1822 and 1824. The theoretical starting
   point of article consists in the affirmation that it is necessary to
   ensure a sovereignty principle to think politics and political changes
   in the modernity. For this, initially, the essay approaches the
   metamorphosis that occurred in the notion of sovereignty in the early
   years of the 1820s. Then, it studies the discourses around the concept
   of national sovereignty in the context of Independence, of the
   Constituent Assembly of 1823 and of the Imposed Letter of 1824. Finally,
   it analyzes the uses of the concept in relation to the problem of the
   limitation of popular power. From this path, it is understood that it is
   possible to comprehend the operative value of the notion of sovereignty
   in the formation of the Brazilian State.},
