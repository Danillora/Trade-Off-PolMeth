@article{ WOS:000398137300003,
Author = {Kern, Anna},
Title = {Party Closeness and Satisfaction with Democracy: A Longitudinal Analysis
   on the effects of partisanship in Germany},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Volume = {58},
Number = {1},
Pages = {51-76},
Abstract = {Political parties provide an attitudinal linkage between citizen and the
   state, but in Germany, partisan ties are weakening. We know little about
   what this decline implies for citizens' attitudes towards the party
   system and democracy more generally. Relying on data from the German
   Socio-Economic Panel (SOEP), a longitudinal multilevel analysis was
   conducted to investigate whether the effect of partisanship on
   satisfaction with democracy has changed over a period of 23 years
   (1987-2010). The results show that the effect of party attachments on
   satisfaction with the functioning of democracy has strengthened.
   However, it also shows that the link between partisan ties and
   satisfaction with democracy depends on whether citizens support winners
   of the electoral competition.},
Publisher = {SPRINGER},
