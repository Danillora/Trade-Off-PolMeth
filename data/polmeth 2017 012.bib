@article{ WOS:000414304000009,
Author = {Petrov, Eduard Yu. and Semercov, Mikhail S.},
Title = {THE GEOPOLITICAL RISKS OF CHINA'S INFLUENCE ON CENTRAL ASIAN COUNTRIES},
Journal = {SRAVNITELNAYA POLITIKA-COMPARATIVE POLITICS},
Year = {2017},
Volume = {8},
Number = {3},
Pages = {114-119},
Abstract = {The article analyzes the Central Asian region and the influence of China
   on countries of the region from a position of geopolitical risks. For
   the analysis of the classification of risks taken and on the basis of
   the theoretical knowledge in this area the article examines relations of
