@article{ WOS:000411289200005,
Author = {Waisova, Sarka},
Title = {TAIWANESE CIVIL SOCIETY ORGANISATIONS AND THEIR ROLE IN CROSS-STRAIT
   RELATIONS},
Journal = {POLITICKE VEDY},
Year = {2017},
Volume = {20},
Number = {3},
Pages = {95-115},
Abstract = {Substantial part of the academic world as well as politics recognizes
   civil society organizations as important positive actors in political
   change, in democratization as well as in peacebuilding. But the
   empirical evidence from some conflict prone areas show that civil
   society organizations can have substantial conflict boosting potential.
   This article investigates the role of Taiwanese civil society
   organizations in shaping the relationship between Mainland China and
   Taiwan. It shows that Taiwanese CSOs hold a very complicated position
   which has in fact added to tensions in cross-strait relations. The
   conflict potential of Taiwanese CSOs in cross-strait relations grows not
   only from the difference in political regimes between China and Taiwan,
   but also from the special relationship of Taiwanese CSOs with political
   elites, the incompatibility of building peace in cross-strait relations
   and democratization in Taiwan, from the Chinese concentrated action
   against Taiwanese CSOs in international arena and from the deep division
   of the Taiwanese society about the political future of the island.},
