@article{ WOS:000416735300002,
Author = {Weststeijn, Arthur},
Title = {Machiavelli in Dutch colonial ideology Caspar Barlaeus, Johan Maurits of
   Nassau, and the imperial prince},
Journal = {STORIA DEL PENSIERO POLITICO},
Year = {2017},
Volume = {6},
Number = {2},
Pages = {177-196},
Month = {MAY-AUG},
Abstract = {What was the impact of Machiavelli's political thought on early-modern
   Dutch colonialism? This article zooms in on Dutch Brazil between 1630
   and 1654 to argue that the Dutch employed a distinctive notion of
   Machiavellian reason of state to theorize and legitimize their expansion
   overseas. Discussing the work of the prominent humanist scholar Caspar
   Barlaeus and his presentation of governor-general Johan Maurits of
   Nassau as a colonial prince, the article shows that the period of Dutch
   rule in Brazil provided the setting where Machiavelli was adapted to the
   concerns of a modern commercial empire.},
