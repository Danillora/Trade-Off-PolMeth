@article{ WOS:000404782300001,
Author = {Bermudez, Sandra and Cordero, Guillermo},
Title = {Who is recruiting our crew? Contextual determinants of MPS' selection},
Journal = {ACTA POLITICA},
Year = {2017},
Volume = {52},
Number = {3},
Pages = {265-285},
Month = {JUL},
Abstract = {This article analyses the contextual determinants of parliamentary
   elites' methods of selection. Using survey data from a representative
   sample of 500 Spanish MPs, we empirically demonstrate that different
   district characteristics generate different ways of MPs' selection.
   Specifically, parties implement more exclusive ways of candidate
   selection in more competitive districts and in regional chambers. On the
   contrary, selection processes are more participative at the national
   level and where electoral competition is low.},
Publisher = {PALGRAVE MACMILLAN LTD},
Address = {BRUNEL RD BLDG, HOUNDMILLS, BASINGSTOKE RG21 6XS, HANTS, ENGLAND},
Type = {Article},
