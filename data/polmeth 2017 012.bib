@article{ WOS:000431730500003,
Author = {Ouaiss, Makram and Rowayheb, Marwan},
Title = {Ex-combatants Working for Peace and the Lebanese Civil Society: A Case
   Study in Non-communal Reintegration},
Journal = {CIVIL WARS},
Year = {2017},
Volume = {19},
Number = {4},
Pages = {448-469},
Abstract = {This article investigates the factors that made few Lebanese
   ex-combatants, who fought each other during the Lebanese civil war of
   1975 and who believed they were fighting for a just cause, able to work
   together to establish the Fighters for Peace (FFP) association to preach
   peace and peaceful coexistence. It demonstrates that ex-combatants, who
   are cause-oriented individuals, can overcome their differences and
   hatred towards each other and work together for the purpose of civil
   peace. This can happen, if ex-combatants gradually reintegrate back into
   society following a two-phase model proposed by the authors.},
