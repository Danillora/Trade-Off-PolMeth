@article{ WOS:000391225900009,
Author = {Whitten-Woodring, Jenifer and Van Belle, Douglas A.},
Title = {The Correlates of Media Freedom: An Introduction of the Global Media
   Freedom Dataset},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2017},
Volume = {5},
Number = {1},
Pages = {179-188},
Month = {JAN},
Abstract = {Media freedom has motivated substantial political activism, yet there
   are surprising gaps in the related academic literature, in part because
   of a lack of historic and consistent data. We introduce the newly
