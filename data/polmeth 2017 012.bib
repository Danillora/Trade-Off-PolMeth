@article{ WOS:000422892700015,
Author = {Neyer, Juergen and Sienknecht, Mitja and Martin-Russu, Luana},
Title = {Cosmopolitan Responsibility: between Normative Claims and Political
   Practice},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Number = {52},
Pages = {311+},
Abstract = {Responsibility is an important norm in all contexts of policy-making.
   Today, it must be understood as a cosmopolitan principle to respect the
   concerns of affected parties of own decisions and to give them voice in
   own decision-making processes even if they have no formal right to be
   represented. This cosmopolitan principle is already institutionalized in
   a number of international, supranational and transnational contexts. All
   of these contexts lack important pre-conditions for a full-blown
   application of the principle. The fact that the principle has
   nevertheless been institutionalized to some degree in all three contexts
   can be interpreted as evidence for its broad acceptance as a new global
   norm.},
