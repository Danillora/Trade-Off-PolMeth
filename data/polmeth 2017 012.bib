Author = {Valkovicova, Veronika},
Title = {``Regrettably, it seems that breaking one border causes others to
   tumble{''}. Nationalism and Homonegativity in the 2015 Slovak Referendum},
Journal = {POLITIQUE EUROPEENNE},
Year = {2017},
Volume = {55},
Number = {1},
Pages = {86-115},
Abstract = {The paper focuses on the interplay of nationalist and homonegative
   discourses applied by anti-LGBT activists during the Slovak Referendum
   on the Protection of Family in 2015. The study applies a conceptual
   framework of Europeanisation as a cognitive resource, which activists on
   the national level apply in order to campaign against same-sex marriage
   and adoption rights of LGBT people. Via frame content analysis, the
   study contributes to diverse literature on the discursive
   cleavage-building of homonegativity and ethno-nationalism in Central and
   Eastern European countries.},
