   Piattoni, Simona, Univ Trento, Polit Sci, Trento, Italy.},
DOI = {10.1017/gov.2016.48},
ISSN = {0017-257X},
EISSN = {1477-7053},
Keywords = {European Union; intergovernmentalism; democracy; domination; hapticity},
Keywords-Plus = {EU; CRISIS; GOVERNANCE; INTEGRATION; POLITICS},
Research-Areas = {Government \& Law},
Web-of-Science-Categories  = {Political Science},
Author-Email = {simona.piattoni@unitn.it},
