@article{ WOS:000424709500001,
Author = {Hlousek, Vit and Kopecek, Lubomir},
Title = {Entrepreneurial Parties: A Basic Conceptual Framework},
Journal = {POLITOLOGICKY CASOPIS-CZECH JOURNAL OF POLITICAL SCIENCE},
Year = {2017},
Number = {2},
Pages = {83-91},
Abstract = {The paper offers a conceptual framework to investigate the phenomenon of
   entrepreneurial parties in the countries of East-Central Europe. The
   authors evaluate existing concepts related to increasing interdependence
   of party politics and the business sector, such as business-firm
   parties. These concepts are typically much too bound to a particular
   example of historically emerging entrepreneurial party and therefore the
   paper offers a more general concept of entrepreneurial party. The
   concept is based on five basic features: (1) the central role of the
   leader and his private initiative, (2) the party as a personal vehicle,
   (3) the crucial formative influence of a leader over the political
   project. Two other features are related to the social and institutional
   un-rootedness of the entrepreneurial party which is neither a product of
   (4) a promoter/sponsor organisation or social movement, nor it is an
   outcome of (5) a parliamentary split or emergence of a new faction.},
