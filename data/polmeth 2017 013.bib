Title = {Legal Advisors in the Armed Forces: Military Lawyers in the Israel
   Defense Forces as Mediators, Interpreters, and Arbiters of Meaning
   during Operations},
Booktitle = {POLITICAL AND MILITARY SOCIOLOGY: MILITARY PERCEPTIONS AND PERCEPTIONS
   OF THE MILITARY},
Series = {Political and Military Sociology-An Annual Review},
Year = {2017},
Volume = {42},
Number = {2014},
Pages = {125-148},
Abstract = {This article deals with lawyers in the Israeli military in the context
   of military operations. Based on an organizational analysis of military
   lawyers' hybrid roles, it argues that (1) military legal advisors belong
   to two professional communities that are highly partisan; (2) they
   fulfill a variety of roles that include mediation between the
   professions, the interpretation of military law, and the final
   arbitration of operational decisions; and (3) they represent the only
   hybrid military role that deals with the core of military expertise: the
   organized management and deployment of violence.},
