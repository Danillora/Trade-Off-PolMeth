@article{ WOS:000402408900008,
Author = {Elena Gronemeyer, Maria and Porath, William},
Title = {Trends of the editorial viewpoints in reference newspapers in Chile: The
   art of balancing criticism of the performance of political actors},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2017},
Volume = {37},
Number = {1},
Pages = {177-202},
Abstract = {The study analyzes whether the press, in the context of increasingly
   concentrated ownership, has shown a corresponding trend toward a
   uniformity of editorial viewpoints when discussing the actions of the
   government, political parties, or civil society. Prior findings reveal
   that editorials tend to assume a position by identifying an actor that
   is responsible for the topic being discussed. This facilitates a content
   analysis that uses the editorial positions of major media outlets
   regarding the acceptance or rejection of a specific actor. The results
   suggest a tendency towards an increasingly homogeneous view by the five
   media outlets studied, especially in the two leading newspapers, El
   Mercurio and La Tercera. Chilean newspapers tend to be very similar when
   judging those responsible, balancing acceptance and rejection in a way
   that is generally less critical of the government than it is of civil
   actors.},
Publisher = {PONTIFICIA UNIV CATOLICA CHILE, INST CIENCIA POLITICA},
Address = {AV VICUNA MACKENNA 4860, SANTIAGO, 00000, CHILE},
