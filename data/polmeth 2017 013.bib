@article{ WOS:000422892700016,
Author = {Mueller, Franziska},
Title = {Productive forests: Diffusion of carbon responsibilities in REDD plus
   projects},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Number = {52},
Pages = {334+},
Abstract = {The article analyses the diffusion of carbon responsibilities on a
   global/local level with regards to the policy instrument REDD+, which
   finances carbon reduction projects in the Global South. This goes in
   line with the localisation of a formely universal carbon responsibility.
   The roles and responsibilites of marginalized and subaltern actors
   (small farmers, indigenous peoples) are thereby empowered; however this
