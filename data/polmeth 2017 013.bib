@article{ WOS:000396407100008,
Author = {Southwell, Priscilla L.},
Title = {Campaign Contribution Limits in California, Oregon \& Washington: What a
   Difference It Makes},
Journal = {JOURNAL OF POLITICS AND LAW},
Year = {2017},
Volume = {10},
Number = {2},
Pages = {82-87},
Month = {MAR},
Abstract = {This research examines the current restrictions on campaign
   contributions to nonfederal candidates in the states of Washington,
   Oregon, and California, as well as the legal challenges in the latter
   two states. The impact of unrestricted contributions in Oregon had the
   effect of larger campaign coffers, per registered voter, for state
