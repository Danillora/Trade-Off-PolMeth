@article{ WOS:000418715300013,
Author = {Robinson, Neil},
Title = {CYBER DEFENSE AT NATO: FROM WALES TO WARSAW, AND BEYOND},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2017},
Volume = {16},
Number = {3},
Pages = {133-143},
Month = {FAL},
Abstract = {Euro-Atlantic security continues to confront the challenges of an
   emergent and unstable cyberspace, characterized by a diverse landscape
   of risks. From its technical beginnings, NATO's evolutionary approach to
   addressing this challenge remains in motion, shaped by an increasingly
   challenging threat landscape, diverse political climate, and evolving
   technology. This article seeks to describe the salient features of
   NATO's approach to tackling the emerging security challenge of cyber
   attacks. After a description of some of the key aspects of the Enhanced
   NATO Policy on Cyber Defence endorsed at the Wales Summit in 2014 which
   remains NATO's extant policy, the two major cyber defense decisions from
   the Warsaw Summit in 2016 are reflected upon before the conclusion
   offers an indication of a possible future direction for cyber defense at
   NATO.},
