@article{ WOS:000422892700021,
Author = {Hempel, Leon},
Title = {Policies of Responsibility in the context of IT security: Responsibility
   as contingency shift.},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2017},
Number = {52},
Pages = {454+},
Abstract = {The paper analyses the idea of responsibility in the context of IT
   security. In the debates about cybersecurity, it is often said that the
   `nature of the network' does not allow to attribute actions to an actor.
   However, to ascribe the so called attribution problem merely to the
   technical structure implies to follow a technical determinism, which
   relieves from responsibility, and also disguises the permanent
   incantation of the attribution problem as part of a responsibility
   policy. With regard to the sociological theory of action, on the other
