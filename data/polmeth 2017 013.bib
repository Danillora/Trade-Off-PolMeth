@article{ WOS:000424811800004,
Author = {Soledad Llovera, Maria and Scialla, Marina},
Title = {Drug Policies in Argentina (2003-2015): Reflections on the Struggle
   Between the Inherited and the Reformulated},
Journal = {TEMAS Y DEBATES},
Year = {2017},
Number = {34},
Pages = {77-99},
Month = {JUL-DEC},
Abstract = {In this article we propose to analyze the way in which public policies
   for people on situacion of drug use and adiction have been structured in
   the long decade between the years 2003 and 2015. We will make a tour of
   the main milestones which allow us to talk about the ``drug
   problema{''}. We will review public documents, current legislation,
   journalistic articles and specialized bibliography, in order to reflect
   on the tensions, contradictions and heterogeneities that arise in
   argentine national legislation to address the consumption of ilegal
   substances. We will characterize the validity of two regulations as
   dissimilar as the Drug Law No 23.737 and the Law of Mental Health No
   26.657 as the result of a long struggle between the inherited and the
   reformulated, not yet resolved.},
