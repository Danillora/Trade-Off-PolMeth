@article{ WOS:000395185500004,
Author = {Kim, Hae S.},
Title = {Patterns of Economic Development: Correlations Affecting Economic Growth
   and Quality of Life in 222 Countries},
Journal = {POLITICS \& POLICY},
Year = {2017},
Volume = {45},
Number = {1},
Pages = {83-104},
Month = {FEB},
Abstract = {The purpose of this article is twofold. First, it identifies different
   patterns of economic growth among the 222 countries/territories in the
   world. Second, it assesses how each of the patterns of economic growth
   affects quality of life, including human development, poverty level,
   unemployment, international peace/conflict, and relative deprivation,
   among others. Several broad patterns of economic growth were found:
   knowledge-based, trade-based, savings-based, and ethnic-religiously
   affected. The knowledge-based economy was the most predominant pattern,
   followed by trade-based. Although the dominance of the knowledge-based
   economy does diminish the effect of the traditional pattern of the free
   trade on economic growth, these two patterns are significant in
   enhancing not only economic growth but also quality of life across the
   world. This pattern-analysis aims to group insights from diverse
   theories of economic growth and variables in a systematic way, and
   identifies causal relations between the variables grouped under each
   pattern.},
