@article{ WOS:000418267500029,
Author = {Gordon, Sanford C. and Landa, Dimitri},
Title = {Common Problems (or, What's Missing from the Conventional Wisdom on
   Polarization and Gridlock)},
Journal = {JOURNAL OF POLITICS},
Year = {2017},
Volume = {79},
Number = {4},
Pages = {1433-1437},
Month = {OCT},
Abstract = {We examine the intuition that in supermajoritarian settings,
   polarization and policy-making gridlock are fundamentally linked but
   that a pressing common problem can reduce both. When actors' individual
   costs from a policy addressing such a problem differ, their preferences
   over the appropriate policy respond asymmetrically to increases in the
   magnitude of the problem. In a broad range of circumstances such
   increases can give rise to increased polarization but may also
   simultaneously yield net welfare-enhancing policy adjustments rather
   than entrenchment of gridlock. The association of polarization and
   gridlock is contingent on how the problem responds to the policy
   solution, institutional structure, and the location of the status quo
   policy when the extent of the problem changes. We illustrate the model's
   logic by comparing US national policy making in the Progressive Era and
   the present.},
