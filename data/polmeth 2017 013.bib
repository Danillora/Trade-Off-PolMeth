@article{ WOS:000406460000008,
Author = {Monroy C, Daniel A.},
Title = {Nudges and Unaware Choices: Statu Quo Bias and Public Policies in
   Colombia},
Journal = {DESAFIOS},
Year = {2017},
Volume = {29},
Number = {1},
Pages = {211-247},
Abstract = {The article presents, first, a synthesis of the cognitive anomaly called
   ``statu quo bias{''} and shows how this bias could influence individual
   decision-making and therefore also the effectiveness of public policies
   in some relevant contexts, among them: (i) people's bad eating habits;
   and (ii) the quality of savings for retirement. Second, the article
   suggests some strategies (nudges) about how the prevalence of ``statu
   quo bias{''} could be used by policymakers as a tool to design or to
   improve the effectiveness of public policies in each of those contexts.
   The article ends with a brief consideration of the ethics and the use of
   nudges by policymakers.},
