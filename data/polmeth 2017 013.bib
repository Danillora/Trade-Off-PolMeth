@article{ WOS:000418941800009,
Author = {Campbell, Colin and Gaddis, S. Michael},
Title = {``I Don't Agree with Giving Cash{''}: A Survey Experiment Examining
   Support for Public Assistance},
Journal = {SOCIAL SCIENCE QUARTERLY},
