   Kaiser, Wolfram, Univ Portsmouth, Sch Social Hist \& Literary Studies, Portsmouth, Hants, England.},
DOI = {10.1080/14608944.2016.1265491},
ISSN = {1460-8944},
EISSN = {1469-9907},
Keywords = {Cultural committee; European Commission; European identity; European
   Parliament; integration narratives; narrative pluralism},
