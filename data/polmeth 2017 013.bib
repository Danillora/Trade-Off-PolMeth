@article{ WOS:000423473300006,
Author = {Kolers, Avery},
Title = {LATIN AMERICA IN THEORIES OF TERRITORIAL RIGHTS},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2017},
Volume = {37},
Number = {3},
Pages = {737-753},
Abstract = {``Who owns it?{''} is a surprisingly confusing question when applied to
   territory. Each word opens up puzzles: who can ``own{''} territory? What
   is ``ownership{''} in this context? How can it be justified in a way
   that could convince an outsider? These questions are particularly
   salient in the Latin American context, where multiple distinct kinds of
   land disputes converge. This paper canvasses two familiar approaches to
   these questions: the Kantian autochthony view, and the Lockean
   efficiency view. Neither view answers the question as to ``who owns
   it{''} in all its complexity. The paper then defends an alternative
   approach grounded in recognition of diverse conceptions of land and the
   ways that distinct groups achieve plenitude in particular places.},
