@incollection{ WOS:000428537900003,
Author = {Chelaru, Eugen},
Editor = {Arnold, R and MartinezEstay, JI},
Title = {On the Jurisdictional Control of the Acts of the Government of Romania},
Booktitle = {RULE OF LAW, HUMAN RIGHTS AND JUDICIAL CONTROL OF POWER: SOME
   REFLECTIONS FROM NATIONAL AND INTERNATIONAL LAW},
Series = {Ius Gentium-Comparative Perspectives on Law and Justice},
Year = {2017},
Volume = {61},
Pages = {29-52},
Abstract = {The executive power in Romania is made up of two branches: the
   Presidency and the Government. In its status as executive authority, the
   Government issues decisions by means of which the execution of laws is
   organized. Based on legislative delegation, the Government also issues
   legal acts. These are represented by ordinances and emergency
   ordinances. The external control over the two categories of documents
   that the Government issues is exercised in different ways: control of
   legality and control of constitutionality. For ordinances and emergency
   ordinances; there is only control of legality for decisions. Because
   government decisions have the juridical nature of administrative acts,
   the contentious control over them may be exercised either directly or
   indirectly. Direct control is achieved by bringing about an action for
   annulment and thus constitutes a full-jurisdiction contentious control
   that is exercised by contentious-administrative courts, which are part
   of the judiciary. However, the injured party may opt for dispute
   settlement by certain administrative jurisdictions. The judge may
   examine the lawfulness and, within certain boundaries, the opportunity
   of Government decisions with an individual nature and only the
   lawfulness of decisions with a normative nature. Indirect control is
   performed by invoking the plea of illegality, which is to be settled by
   the court vested with the merits of the case, whether it is a
   contentious-administrative court or a common law one.},
