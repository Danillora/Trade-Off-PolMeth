@article{ WOS:000405823300006,
Author = {Fair, C. Christine and Hamza, Ali and Heller, Rebecca},
Title = {Who Supports Suicide Terrorism in Bangladesh? What the Data Say},
Journal = {POLITICS AND RELIGION},
Year = {2017},
Volume = {10},
Number = {3},
Pages = {622-661},
Month = {SEP},
Abstract = {Bangladesh, one of the world's largest Muslim countries, is generally
   viewed as a success story with a strong tradition of secular democracy.
   Unfortunately, this assertion rests on a weak empirical foundation.
   Since becoming independent from Pakistan in 1971, democracy and
   secularism have been consistently undermined. Moreover, since 2000
   Bangladesh has experienced more than 100 incidents of Islamist
