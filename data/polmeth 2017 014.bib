@article{ WOS:000400881800004,
Author = {Gohin, Alexandre},
Title = {On the Economic Costs of the Russian Embargos on Food Products},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2017},
Volume = {127},
Number = {1},
Pages = {71-91},
Month = {JAN-FEB},
Abstract = {Russia decides two trade embargoes in 2014: a first sanitary embargo on
   European pig meat and a second political embargo on fruits, vegetables,
   dairy and meat products from many developed countries. We assess the
   economic costs of these trade sanctions using a computable general
   framework detailing food trade flows. Our simulations show that the
   negative impacts of the sanitary embargo are larger than those of the
   political embargo. We also show that these economic costs are highly
   dependent on the labor market functioning. When the nominal wages are
   supposed fixed, our French cost estimate increases by a factor of 20.},
