@article{ WOS:000408110600004,
Author = {Skomorovsky, Alla and Bullock, Amanda},
Title = {The Impact of Deployment on Children From Canadian Military Families},
Journal = {ARMED FORCES \& SOCIETY},
Year = {2017},
Volume = {43},
Number = {4},
Pages = {654-673},
Month = {OCT},
Abstract = {Children in military families experience various stressors associated
   with the demands of military life, such as parental absences due to
   deployments. However, there is a limited understanding of children's
   well-being to parental deployment from Canadian military families. This
   study was conducted to examine the impact of deployment on the
   well-being of school age children from Canadian Armed Forces families
   and to consider the resilience factors in their well-being. Focus groups
   with children (N = 85) showed that deployment negatively impacted
   children's well-being, routines, and family dynamics. Active distraction
   and social support seeking served as the most effective protective
   factors against deployment stress. Recommendations for mitigating the
   impact of deployment are offered.},
