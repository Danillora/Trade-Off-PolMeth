@article{ WOS:000417738800004,
Author = {Birch, Sarah and ElSafoury, Fatma},
Title = {Fraud, Plot, or Collective Delusion? Social Media and Perceptions of
   Electoral Misconduct in the 2014 Scottish Independence Referendum},
Journal = {ELECTION LAW JOURNAL},
Year = {2017},
Volume = {16},
Number = {4},
Pages = {470-484},
Month = {DEC},
Abstract = {Online discussions of electoral fraud are becoming an increasingly
   important aspect of the electoral landscape in many contexts, as
   cyberspace is one of the few places where concerns about electoral
   conduct can be aired openly and freely. But it is often difficult to
   assess what this online activity tells us about actual electoral
   processes. This article analyzes a surge of tweets about electoral fraud
   at the time of the Scottish independence referendum of 2014 in order to
   ascertain whether this online activity reflected: (a) actual offline
   fraud observed by the social media users, (b) a concerted effort to
   undermine confidence in electoral administration, or (c) a collective
   delusion. Data mining and machine learning techniques are deployed in
   this analysis, which comes out strongly in favor of the collective
   delusion hypothesis.},
