@incollection{ WOS:000441390100001,
Author = {Braender, Morten and Andersen, Lotte Bogh},
Editor = {Karakatsanis, NM and Swarts, J},
Title = {Soldier and Civilian Motivation-Different or Similar? A Comparison of
   Public Service Motivation for Civilians and Military Personnel},
Booktitle = {POLITICAL AND MILITARY SOCIOLOGY: MILITARY PERCEPTIONS AND PERCEPTIONS
   OF THE MILITARY},
Series = {Political and Military Sociology-An Annual Review},
Year = {2017},
Volume = {42},
Number = {2014},
Pages = {1-30},
Abstract = {Any country using volunteer forces must ask whether their soldiers are
   representative of the general population. While many studies to this end
   have focused on gender race, and socioeconomic status, this article
   compares the motivations of soldiers and civilians. Its main purpose is
   to contribute to the existing toolbox used in soldier motivation studies
   by introducing a measurement tool called public service motivation that
   is applicable across the civilian-military gap. Using this tool,
   developed within the field of public administration, to compare Danish
   combat troops with Danish civilians, we find that soldier motivation is
   unique in two important respects: whereas soldiers are more normatively
   motivated than their civilian compatriots, their affectively based
   motivation is lower},
