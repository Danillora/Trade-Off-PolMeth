@article{ WOS:000419031600002,
Author = {Eggert, Wolfgang and Minter, Steffen and Stephan, Maximilian and von
   Ungern-Sternberg, Handirk},
Title = {SANCTIONS FOR REPEAT OFFENDERS: A QUESTION OF WEALTH?},
