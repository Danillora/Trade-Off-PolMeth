@article{ WOS:000412260400029,
Author = {McConnell, Fiona and Kuus, Merje and Jeffrey, Alex and Crawley, Heaven
   and Vaughan-Williams, Nick and Smith, Adrian},
Title = {Interventions on Europe's political futures},
Journal = {POLITICAL GEOGRAPHY},
Year = {2017},
Volume = {60},
Pages = {261-271},
Month = {SEP},
Abstract = {Europe is facing challenging times. The so-called `migration crisis' has
   seen the hardening and militarisation of Europe's borders. Nationalist
   politicians are framing European states as being under siege from
   Islamist terrorists and economic migrants, which has led to a rise in
   xenophobia and casual racism on the streets of European cities.
   Meanwhile the Euro-zone has seen a series of employment crises and
   economic bailouts. Alongside such political and economic turmoil, the
   European Union is facing unprecedented pressures, not least from the
   `Brexit' result of the UK's referendum on EU membership in June 2016. In
   reflecting on these manifold challenges to the idea and space of Europe
   these interventions focus on three themes that have long animated
   political geography scholarship: borders, power and crises.
   Cross-cutting the interventions are two calls to action: to rethink our
   analytical approaches to Europe, and to reframe our role as critical
   scholars. (C) 2017 Elsevier Ltd. All rights reserved.},
