@incollection{ WOS:000441391500007,
Author = {Wald, Kenneth D. and Feinstein, Danielle},
Editor = {Karakatsanis, NM and Swarts, J},
Title = {Higher Education and Political Tolerance: The ``Difficult{''} Case of
   Israel},
Booktitle = {POLITICAL AND MILITARY SOCIOLOGY: POLITICAL ATTITUDES, PERCEPTIONS, AND
   CULTURE},
Series = {Political and Military Sociology-An Annual Review},
Year = {2017},
Volume = {43},
Number = {2015},
Pages = {149-171},
Abstract = {Prior research has documented a robust link between higher education and
   political tolerance. Israel might be an exception to this rule because
   many Jewish Israelis perceive the country's Arab citizens as a fifth
   column hostile to the state's Jewish character. During their formative
   years, young Israelis are exposed to powerful messages reinforcing
   Israel's ethnonational character from both the ``religion of national
   security{''} and compulsory military service that typically occurs
   before higher education. These unique experiences raise the possibility
   that young Israelis may be essentially fixed in their orientations to
   Israel's Arab citizens at an early age, preempting the influence of
   university experience on attitudes to political tolerance. Using a
   survey of Israeli university students who entered higher education after
   military service, we test whether exposure to higher education changes
   attitudes to the rights of Israeli Arabs and the Israeli-Arab conflict.},
