@article{ WOS:000418941800025,
Author = {Easterly, Bianca},
Title = {The Ties that Bind Beyond the Battlefield: An Examination of the
   Diffusion Patterns of Veterans Treatment Courts},
Journal = {SOCIAL SCIENCE QUARTERLY},
