@article{ WOS:000410857200006,
Author = {Rossi, Federico M.},
Title = {Compulsion mechanisms: state-movement dynamics in Buenos Aires},
Journal = {SOCIAL MOVEMENT STUDIES},
Year = {2017},
Volume = {16},
Number = {5},
Pages = {578-594},
Abstract = {The article reveals and explains the workings of generally ignored
   mechanism of state-movement interaction proposed by Charles Tilly,
   namely the compulsion mechanism. Specifically, two types of compulsion
   mechanisms will be defined: compulsive support and compulsive control.
   In both types, without using physical repression, the state's
   institutions reinforce the movement's identity while also prompting it
   to adapt its repertoire of strategies to the state institutions'
   requirements. Empirically, this article focuses on the interaction of
   the assembly movement with the state in the City of Buenos Aires. This
   movement emerged as a result of the socioeconomic and political crises
   of 2001-2002 in Argentina. Based on ethnographic fieldwork and archival
   research, the purpose is to unpack how the assembly movement's
   identities and strategies were built and how its interaction with the
   state evolved.},
