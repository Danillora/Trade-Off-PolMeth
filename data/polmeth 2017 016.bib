@article{ WOS:000405329500008,
Author = {Richards, Robert},
Title = {The Role of Interest Groups and Group Interests on Gun Legislation in
   the US House},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2017},
Volume = {98},
Number = {2, SI},
Pages = {471-484},
Month = {JUN},
Abstract = {Objective. I examine the differential effects on legislative behavior of
   organized and unorganized subgroups related to gun policy. Methods.
   Using various data sources on campaign contributions, voter
   characteristics, and legislative behaviors, I estimate logit models of
   the decision to co-sponsor or vote for gun bills in the U.S. House of
   Representatives. Results. Both interest group contributions and the
   percentage of hunters in a district are independently predictive of
   legislative behavior, above and beyond district ideology and party
   affiliation. These findings are robust and substantively meaningful.
   Conclusions. Those involved in the politics surrounding gun policy
   should take into account the difference between the influence of
   organized interests like the National Rifle Association (NRA) and the
   unorganized interests of voter subgroups.},
