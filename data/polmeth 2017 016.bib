@article{ WOS:000405678900002,
Author = {Smrcka, Lubos and Camska, Dagmar and Arltova, Marketa and Placek, Jan},
Title = {Characteristics of Insolvency Proceedings of Enterprises with Virtual
   Address},
Journal = {POLITICKA EKONOMIE},
Year = {2017},
Volume = {65},
Number = {3},
Pages = {287-300},
Abstract = {The aim of the research is to show differences between characteristics
   of insolvency proceedings of traditional and virtual enterprises. This
   paper focuses on results of insolvency proceedings in the case of
   enterprises which do not have a traditional headquarter. Instead of the
   traditional headquarter these enterprises are registered at virtual
   addresses for a purpose of business register. Among the professional
   public this institute enables discussions about lower transparency and
   even functioning of grey-area. First the paper analyzes results for
   enterprises with virtual address and second these results are compared
   with traditional enterprises. Research questions are based on
   probability of insolvency proposal, kind of bankruptcy (with default and
   without default) and level of creditors' satisfaction. The used data are
   extracted from business as well as insolvency register. The gained
   results show that there are significant differences between the
   insolvency proceedings of traditional and virtual enterprises which have
   in this case higher probability of insolvency, lower level of creditor's
   satisfaction and therefore many cases finish without default due to
   enterprises' property emptiness.},
