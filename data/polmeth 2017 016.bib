@article{ WOS:000482619600002,
Author = {Tolosa Villabona, Luis Armando},
Title = {On the Principles of the Law of Obligations and the Contemporary
   Contractual Law},
Journal = {REVISTA ESTUDIOS SOCIO-JURIDICOS},
Year = {2017},
Volume = {19},
Number = {2},
Pages = {13-61},
Month = {JUL-DEC},
Abstract = {This paper analyses some hermeneutic principles whose normative scope
   defines the rational exercise of mandatory relationships and, as a
   consequence, contractual and extracontractual relationships. This
   analysis is built from a constitutional view of private law, which
   emphasizes in postulates of the Constitutional and Social Law State and
