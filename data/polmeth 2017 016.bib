@incollection{ WOS:000413334500010,
Author = {Giovagnoli, Raffaela and Larrey, Philip},
Editor = {Beziau, JY and Basti, G},
Title = {Aristotle, Frege and ``Second Nature{''}},
Booktitle = {SQUARE OF OPPOSITION: A CORNERSTONE OF THOUGHT},
Series = {Studies in Universal Logic},
Year = {2017},
Pages = {169-178},
Abstract = {Aristotle proposed a ``naturalistic{''} epistemological perspective that
   rests on some fundamental notions:
   - Perceptual judgment (passivity and activity),
   - Simple propositions (subject and predicate),
   - Complex propositions (syllogisms).
   As is well known, the ``Square of Opposition{''} provides the
   possibility of a fruitful classification of reality that is made of
   things, species and genus. Frege introduced a new form of notation that
   is exemplified in his Begriffschrift and changed the Aristotelian
   square. He introduced a conception of judgment that entails a
   fundamental relationship with a ``second nature{''}. Starting from this
   background, McDowell and Brandom present two original views of the
   ``second nature{''} which are subject to some criticisms.},
Publisher = {BIRKHAUSER BOSTON},
