@incollection{ WOS:000441389200007,
Author = {Knopf, Christina M.},
Editor = {Karakatsanis, NM and Swarts, J},
Title = {Relational Dialectics in the Civil-Military Relationship: Lessons from
   Veterans' Transition Narratives},
Booktitle = {POLITICAL AND MILITARY SOCIOLOGY},
Series = {Political and Military Sociology-An Annual Review},
Year = {2017},
Volume = {40},
Number = {2012},
Pages = {171-191},
Abstract = {This article examines U.S. veteran narratives of transition from Iraq
   combat to civilian life, using relational dialectics to explore the
   under-studied aspects of communication and interpersonal interactions in
   civil-military relations theory and studies. A close reading of eight
   veteran narratives at SupportYourVet.org extrapolates common themes
   regarding veteran interaction with civilian and military communities.
   The lens of relational dialectics, particularly the tensions of
   integration-separation, stability-change, and expression-non-expression,
   is used to offer possible explanations for divisions between civilian
   and military spheres, suggesting lessons to be learned about the
   civil-military relationship as enacted and as studied.},
