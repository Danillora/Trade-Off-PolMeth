@article{ WOS:000399506400008,
Author = {Canofari, Paolo and Di Bartolomeo, Giovanni},
Title = {Regime switches under policy uncertainty in monetary unions},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2017},
Volume = {47},
Pages = {124-132},
Month = {MAR},
Note = {14th Annual Conference on Political Economy of Exchange Rate Regimes and
   Policies, European Econ \& Finance soc, Brussels, BELGIUM, JUN 20-24,
   2015},
Organization = {Ctr European Policy Studies},
Abstract = {This paper analyzes the effects of policy uncertainty on the stability
   of a monetary union. Focusing on peripheral countries, we study how
   uncertainty over the consequences of a possible exit affects regime
   switches. Applying game theory and a cost-benefit analysis, we model a
   regime switch as the endogenous result of a two-stage policy game. We
   find that the effects of uncertainty are not trivial. Unilateral exits
   are less probable, but contagion is more likely to be observed. Our
   results are driven by two opposite forces: a traditional conservative
   effect induced by policy uncertainty in a single policymaker framework,
   which calls for more stability, and a strategic effect, arising from the
   strategic interaction, which may undermine the monetary union's
   foundation and strengthen incentives for contagion. (c) 2016 Elsevier
   B.V. All rights reserved.},
