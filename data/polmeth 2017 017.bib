@incollection{ WOS:000441390100004,
Author = {Johansen, Rino Bandlitz and Laberg, Jon Christian and Martinussen,
   Monica},
Editor = {Karakatsanis, NM and Swarts, J},
Title = {The Impact of Military Identity on Norwegian Junior Officer Students},
Booktitle = {POLITICAL AND MILITARY SOCIOLOGY: MILITARY PERCEPTIONS AND PERCEPTIONS
   OF THE MILITARY},
Series = {Political and Military Sociology-An Annual Review},
Year = {2017},
Volume = {42},
Number = {2014},
Pages = {75-98},
Abstract = {The present study examines the influence of military identity on
   military performance, and the potential of military identity to predict
   military performance beyond military ethos, organizational commitment,
   and hardiness in Norwegian petty officer students. Dependent variables
   were (1) overall performance, (2) petty officer potential, and (3)
   leader performance. Hierarchical regression analyses showed that (1)
   overall performance was predicted by organizational commitment,
   hardiness, and military identity (professionalism); (2) petty officer
   potential was predicted by organizational commitment and hardiness; and
   (3) leader performance was predicted by hardiness. Military identity
   (professionalism) was thus found to explain overall military performance
   among Norwegian petty officer students. Additionally, results confirmed
   the predictive value of hardiness on military performance. This is the
   first study to examine the unique influence of military identity on
   military performance in the Norwegian armed forces.},
