@incollection{ WOS:000468636600016,
Author = {Ji, Qihao},
Book-Author = {Adria, M
   Mao, Y},
Title = {A Comparative View of Censored and Uncensored Political Discussion: The
   Case of Chinese Social Media Users},
Booktitle = {HANDBOOK OF RESEARCH ON CITIZEN ENGAGEMENT AND PUBLIC PARTICIPATION IN
   THE ERA OF NEW MEDIA},
Series = {Advances in Public Policy and Administration},
Year = {2017},
Pages = {269-282},
Abstract = {Through a content analysis on Chinese online dissidents' social media
   discourses, this study examines the impact of Internet censorship on
   Chinese dissidents' political discourse in two social media platforms:
   Weibo and Twitter. Data was collected during a time period when China's
   Internet censorship was tightened. Results revealed that Chinese online
   dissidents are more likely to post critical opinions and direct
   criticism towards the Chinese government on Twitter. In addition,
   dissidents on Twitter are more likely to engage in discussing with
   others, while Weibo dissidents tend to adopt linguistic skills more
   often to bypass censorship. No difference was found in terms of
   dissidents' civility and rationality across the two platforms.
   Implications and future research are discussed in detail.},
