@incollection{ WOS:000428537900014,
Author = {Banaszak, Boguslaw},
Editor = {Arnold, R and MartinezEstay, JI},
Title = {Constitutional Tribunals' Judicial Review of Public Power in Poland},
Booktitle = {RULE OF LAW, HUMAN RIGHTS AND JUDICIAL CONTROL OF POWER: SOME
   REFLECTIONS FROM NATIONAL AND INTERNATIONAL LAW},
Series = {Ius Gentium-Comparative Perspectives on Law and Justice},
Year = {2017},
Volume = {61},
Pages = {243-257},
Abstract = {During the first phase of Poland's transformation (1989-1997) the
   advantage of the Constitutional Tribunal's activism was that the
   Constitution was not an obstacle to democratic reforms, even if its
   structure dated back to the times of real socialism. Thus, socially
   desirable reforms could be implemented relatively quickly, without
   working on the revision of the Constitution. After the adoption of the
   new Polish Constitution in light of the judicial practice of the
   Constitutional Tribunal, we can observe a number of worrying cases of
   excessive control. The Constitutional Tribunal specifically interprets
   the constitution and in its case law introduces its own system of values
   as control criteria. Currently, the Constitutional Tribunal ``pushes{''}
   the literal interpretation to the margins, precisely in those situations
   that enable the Tribunal to give explicit content to a phrase of
   considerable importance for the matter in question. The Constitutional
   Tribunal plays in some cases the role not so much of ``negative
   legislator{''}, but rather of a positive `` substitute legislator{''}.
   The Constitutional Tribunal also exceeds its index of power as conferred
   by the Constitution. The activism of the Constitutional Tribunal in the
   area of the control of public power leads inevitably to the ``
   politicization{''} of its jurisprudence. However, the Polish
   Constitutional Court in its case law has repeatedly emphasized that
   aspect of the principle of judicial restraint that approaches the review
   of statutes with a presumption of conformity of the examined norms with
   the Constitution.},
