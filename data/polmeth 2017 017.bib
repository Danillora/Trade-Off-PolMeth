@article{ WOS:000411212700006,
Author = {Wagner, Kevin M. and Gray, Tricia J. and Gainous, Jason},
Title = {Digital information consumption and external political efficacy in Latin
   America: Does institutional context matter?},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2017},
Volume = {14},
Number = {3},
Pages = {277-291},
Abstract = {Digital information consumption has a positive effect on external
   political efficacy in that the Internet provides factual support for the
   belief that governments are responsive to the demands of citizens. The
   existence of the digital forum creates the perception of greater
   openness and transparency. This effect is likely conditional on
   institutional context. Specifically, we expected the relationship
   between digital information consumption and efficacy to be stronger in
   more democratic countries. We use survey data from 18 Latin American
   countries to demonstrate that consumption is a strong predictor of
   external efficacy, and that this effect is modestly stronger in more
   democratic countries.},
