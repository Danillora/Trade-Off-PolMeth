Author = {Fregonese, Sara},
Title = {Affective atmospheres, urban geopolitics and conflict (de)escalation in
   Beirut},
Journal = {POLITICAL GEOGRAPHY},
Year = {2017},
Volume = {61},
Pages = {1-10},
