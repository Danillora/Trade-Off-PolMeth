@article{ WOS:000392679700007,
Author = {Merriman, Peter and Peters, Kimberley and Adey, Peter and Cresswell, Tim
   and Forsyth, Isla and Woodward, Rachel},
Title = {Interventions on military mobilities},
Journal = {POLITICAL GEOGRAPHY},
Year = {2017},
Volume = {56},
Pages = {44-52},
Month = {JAN},
Abstract = {Mobility and movement are central to military actions and military life,
   and yet despite an increasing concern with military geographies and the
   geographies of mobility, little consideration has been given by scholars
   to the political geographies of military mobilities and movements, past
   or present. In these interventions, we examine how these different
   bodies of work might intersect, focusing on social media, methods for
   tracing military mobilities, the role of military technologies in
   facilitating everyday mobilities, and the more-than-human dimensions of
   military mobilities. (C) 2016 Elsevier Ltd. All rights reserved.},
