@article{ WOS:000418989300012,
Author = {de Haan, Jakob and Sturm, Jan-Egbert},
Title = {Finance and income inequality: A review and new evidence},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2017},
Volume = {50},
Number = {SI},
Pages = {171-195},
Month = {DEC},
Note = {CESifo Venice Summer Institute Workshop on Political Economy Aspects of
   Income (Re-)Distribution, Venice, ITALY, JUL 20-21, 2016},
Organization = {CESifo},
Abstract = {Using a panel fixed effects model for a sample of 121 countries covering
   1975-2005, we examine how financial development, financial
   liberalization and banking crises are related to income inequality. In
   contrast with most previous work, our results suggest that all finance
   variables increase income inequality. The level of financial development
   conditions the impact of financial liberalization on inequality. Also
   the quality of political institutions conditions the impact of financial
   liberalisation on income inequality, in contrast to the quality of
   economic institutions. Our main findings are robust for using random
   effects, cross-country regressions and legal origin as instrument for
   financial development.},
