@article{ WOS:000403782200013,
Author = {Quarfoot, David and von Kohorn, Douglas and Slavin, Kevin and
   Sutherland, Rory and Goldstein, David and Konar, Ellen},
Title = {Quadratic voting in the wild: real people, real votes},
