@article{ WOS:000415227100003,
Author = {Tadros, Victor},
Title = {The persistence of the right of return},
Journal = {POLITICS PHILOSOPHY \& ECONOMICS},
Year = {2017},
Volume = {16},
Number = {4},
Pages = {375-399},
Month = {NOV},
Abstract = {This article defends the right that Palestinians have to return to the
   territory governed by Israel. However, it does not defend the duty on
   Israel to permit return. Whether there is such a duty depends on whether
   the economic, social and security costs override that right. In order to
   defend the right of return, it is shown both that the current generation
   of Palestinians retain a significant interest in return, and that
   insofar as their interests are diminished, their rights are not
   diminished proportionally. The interests of Jewish Israelis in excluding
   the Palestinians are then considered. Their rights of
   self-determination, it is argued, do not powerfully favour excluding the
   Palestinians. The economic, social and security costs may do so.
   Overall, I conclude that either the Israel should grant return to the
   Palestinians or it should properly acknowledge the right of return and
   respond appropriately through a powerful effort to compensate and
   resettle them.},
