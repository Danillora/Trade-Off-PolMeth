@article{ WOS:000409361200002,
Author = {Amann, Juergen and Middleditch, Paul},
Title = {GROWTH IN A TIME OF AUSTERITY: EVIDENCE FROM THE UK},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2017},
Volume = {64},
Number = {4},
Pages = {349-375},
Month = {SEP},
Abstract = {This paper uses an empirical approach to test the specific causal
   relationship between debt and growth in the UK, in the context of the
   debate surrounding the use of a policy known as austerity measures. This
   time series perspective makes use of more recent Granger causality and
   cointegration tests that allow for nonstationarity in macroeconomic time
   series data in the presence of structural breaks. Controlling for
   exogenous shocks associated with the period around the financial crisis,
   we find no evidence of a causal relationship between economic growth and
   public debt for the UK.},
