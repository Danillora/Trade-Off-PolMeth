@article{ WOS:000408110600002,
Author = {Bury, Patrick},
Title = {Recruitment and Retention in British Army Reserve Logistics Units},
Journal = {ARMED FORCES \& SOCIETY},
Year = {2017},
Volume = {43},
Number = {4},
Pages = {608-631},
Month = {OCT},
Abstract = {The British Army Reserve (AR), and in particular its logistics
   component, is undergoing profound changes. The Future Reserves 2020
   policy aims to expand the AR and make it more deployable on operations.
   However, to date, FR20 has struggled to attract the recruits required to
   man this more deployable reserve force, despite recruitment campaigns
   offering increased monetary benefits. This study sampled AR logistics
   soldiers' reasons for joining, remaining in service, and mobilizing when
   deployed. Consistent with the previous research, the study found that
   soldiers who joined for institutional reasons were more associated with
   longer career intentions and mobilizing for intrinsic reasons. Soldiers
   who joined for occupational reasons were less satisfied with all
   elements of reserve service and deployed in order to fulfill their
   contracts. These trends suggest that recruitment campaigns that stress
   the pecuniary benefits of reserve service may attract soldiers less
   committed to reserve service and deployments and who are harder to
   retain.},
