@incollection{ WOS:000441389200009,
Author = {Davis, Aaron K.},
Editor = {Karakatsanis, NM and Swarts, J},
Title = {The Illinois State Chapter of the Moral Majority: A Study of a Religious
   Right Organization at the State Level and the Perception of Power,
   1980-1988},
Booktitle = {POLITICAL AND MILITARY SOCIOLOGY},
Series = {Political and Military Sociology-An Annual Review},
Year = {2017},
Volume = {40},
Number = {2012},
Pages = {217-241},
Abstract = {Over the course of the 1980s, the emergence of the voting bloc commonly
   referred to as the ``religious right{''} has attracted a great deal of
   scholarly attention. In recent years, a handful of studies have narrowed
   their scope to focus on demographics, behavior, and values at the local
   and state level. This article discusses the activities of the Illinois
   state chapter of the Moral Majority and considers the actual power that
   it wielded. Many would consider the national parent organization to have
   wielded a considerable amount of political influence throughout the
   1980s. This article was driven by a central question: How much power did
   this state chapter hold? By examining the actions and claims of the
   Illinois state chapter, this article also considers further questions
   that are crucial for gauging the legitimacy of the organization. Was the
