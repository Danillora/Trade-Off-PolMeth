@article{ WOS:000399179200005,
Author = {Lee, Shin Haeng},
Title = {Digital democracy in Asia: The impact of the Asian internet on political
   participation},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2017},
Volume = {14},
Number = {1},
Pages = {62-82},
Abstract = {This study examines the effects of Internet use on political
   participation in East and Southeast Asia. Data came from the Asian
   Barometer Survey, gathered in two waves: 2005-07 and 2010-11. The
   results show that Internet use is positively associated with
   unconventional political participation, while it is not with
   institutionalized action. Also, the data reveal the mechanism by which
   the technology influences interpersonal discussion and self-efficacy, in
   turn leading to participation. Furthermore, Internet use is found to
   provide mobilizing capacities for those who lack organizational
   membership as a traditional agent of political action.},
