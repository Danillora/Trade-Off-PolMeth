@incollection{ WOS:000441390100002,
Author = {Franke, Volker C. and Markley, Eliza M.},
Editor = {Karakatsanis, NM and Swarts, J},
Title = {International Military Education and the Propensity for Building Social
   Capital},
Booktitle = {POLITICAL AND MILITARY SOCIOLOGY: MILITARY PERCEPTIONS AND PERCEPTIONS
   OF THE MILITARY},
Series = {Political and Military Sociology-An Annual Review},
Year = {2017},
Volume = {42},
Number = {2014},
Pages = {31-50},
Abstract = {International security today increasingly depends on good relations and
   close cooperation among and between actual and potential military allies
   based on shared values, norms, and procedures. The purpose of this
   research is to explore the extent to which international military
   education (IME) might contribute to the building of social capital in
   the military and foster a common understanding of operational contexts,
   performance, and behaviors. Drawing on established theories of social
   capital, this article analyzes the perceptions and values of
   international officers in residence at the U.S. Army War College
   (USAWC), and it examines the influence of these values on several
   measures of trust and, by extension, on the development of social
   capital. Our results show that attitudes of international officers are
   similar to those of American officers obtained in previous research and
   that their levels of patriotism and support of international cooperation
   strengthened levels of institutional trust and trust in the UN, while
   their levels of Machiavellianism reduced both types of trust. Overall,
   the results show that international military education (IME) impacts the
   propensity to build military social capital.},
