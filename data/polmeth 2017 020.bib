@article{ WOS:000418865200002,
Author = {Thomas, Owen D.},
Title = {Good faith and (dis)honest mistakes? Learning from Britain's Iraq War
   Inquiry},
Journal = {POLITICS},
Year = {2017},
Volume = {37},
Number = {4},
Pages = {371-385},
Month = {NOV},
Abstract = {The recent `Chilcot' Inquiry judged that British participation in the
   2003 Iraq War was neither right nor necessary. When reading the final
   report of over 2.6 million words, I warn against seeking accountability
   solely in terms of intent and individual culpability, such as
   questioning whether the government deceived the public. There also needs
   to be an examination of the rationalities and power relations that
   allowed figures such as Tony Blair to believe, and still believe, that
   the war was for the common good. Doing so reveals how the preemptive
   logics behind the war endure today.},
