@article{ WOS:000404252300002,
Author = {Donatiello, Davide and Ramella, Francesco},
Title = {The Innovation Paradox in Southern Europe. Unexpected Performance During
   the Economic Crisis},
Journal = {SOUTH EUROPEAN SOCIETY AND POLITICS},
Year = {2017},
Volume = {22},
Number = {2},
Pages = {157-177},
Abstract = {The international crisis has exerted a strong impact on the National
   Innovation Systems (NIS) of three Southern European economies: Italy,
   Portugal and Spain. These countries represent interesting cases for
   analysing responses to the crisis because they show a sort of innovation
   paradox: despite the weakness of their institutional systems and the
   defensive policies developed by governments, some of their companies
   have nevertheless been able to innovate even during the hardest years of
   the recession. In order to shed light on this paradox, three matters are
   taken into account: the distinctive features of the South European
   National Innovation Systems; the behaviour of governments and companies;
   the generative dynamics' and the creative processes' that have taken
   place during the crisis.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
