@article{ WOS:000403201000006,
Author = {Hassan, Oz},
Title = {The \$74 billion problem: US-Egyptian relations after the `Arab
   Awakening'},
Journal = {INTERNATIONAL POLITICS},
Year = {2017},
Volume = {54},
Number = {3, SI},
Pages = {322-337},
Month = {MAY},
Abstract = {Adopting an epistemic communities approach, this article outlines how US
   foreign policy elites have constructed their response to Egypt's 2011
   revolution. It argues that through the discursive deployment of elite
   power a neoliberal-security policy paradigm has been constructed and
   institutionalised. This policy seeks to promote a democratic transition
   in the long term whilst also allowing US elites to pursue more immediate
   security interests. However, tensions in the policy are evident as a
   result of continued flows of US foreign aid to Egypt that are
   contributing to the continuation of an Egyptian
   military-industrial-commercial complex that threatens the likelihood of
   any democratic transition.},
