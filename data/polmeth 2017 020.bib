@article{ WOS:000410411700003,
Author = {Kostanyan, Hrant},
Title = {How ``the political{''} can make the European external action service
   more effective in the eastern region},
Journal = {EAST EUROPEAN POLITICS},
Year = {2017},
Volume = {33},
Number = {3, SI},
Pages = {355-370},
Abstract = {Following the enactment of the Lisbon Treaty, the European External
   Action Service (EEAS) became a key player in the EU's foreign policy.
   This article assesses and explains variations in the EEAS' approach to
   the Eastern Partnership (EaP) region. It contends that EU Member States
   (MS) limit the discretion of the EEAS more in a bilateral than in a
   multilateral framework, thus accounting for varying degrees of political
   sensitivity. By restricting EEAS discretion to the level of
   bureaucratised politics especially in sensitive policy areas, MS miss
   the opportunity to bring ``the political{''} back in and to make EEAS
   policy leadership more tangible.},
