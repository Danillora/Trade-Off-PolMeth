@incollection{ WOS:000424275800017,
Author = {Mathias, Megan},
Editor = {Balakrishnan, MS and Moonesar, IA and Awamleh, R and RowlandJones, R},
Title = {Inspiring Science and Research Careers: Outreach at the Masdar Institute},
Booktitle = {UAE: PUBLIC POLICY PERSPECTIVES},
Series = {Actions and Insights-Middle East North Africa},
Year = {2017},
Volume = {6},
Pages = {317-332},
Abstract = {This case study focuses on the policy challenge of increasing the number
   of Emirati nationals with postgraduate qualifications in science and
   technology, as a pillar of the UAE's ambition to become a knowledge
   economy. Policy-makers must meet this challenge by working in
   partnership with institutions across the higher education sector. A
   snapshot of one high-profile institution's contribution to this policy
   is presented: the Masdar Institute of Science and Technology's out-reach
   program, as it stands in 2016. The program comprises three initiatives,
   with a shared goal of attracting elite students and young professionals
   into the fields of alternative energy and sustainability research,
   policy and leadership. The case prompts students to imagine themselves
   in the role of Asma, a Federal government policy analyst, tasked
   consider how they might assess the contribution of Masdar Institute's
   outreach program to national policy, and to reflect on the potential
   policy levers available in the pursuit of higher education outcomes.},
