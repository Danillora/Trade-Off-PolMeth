@article{ WOS:000418213800007,
Author = {Resende Cierco Gomes, Teresa Maria},
Title = {RETRACTED: Promoting the rule of law in Serbia. What is hindering the
   reforms in the justice sector? (Retracted Article)},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2017},
Volume = {50},
Number = {4},
Pages = {331-337},
Month = {DEC},
Abstract = {EU rule of law assistance relies on supplying institutional ties as well
   as economic and technical support to candidate states, complemented by a
   demand to comply with certain political conditions. But, the
   establishment of the rule of law is a complex and often long-term
   process that includes both different facilitating and inhibiting
   conditions. Since 2006 Serbian government has adopted the National
   judicial Reform Strategy aiming to establish a legal system based on
   legal security and respect for the rule of law. Nevertheless, its
   judiciary sector still reveals serious difficulties, especially in what
   concerns the independence and efficiency of judiciary.
   Looking at Serbian efforts to reform the rule of law and promote an
   independent judiciary, we analyse how the EU has been influencing these
   reforms and try to identify the weaknesses and strengths of EU rule of
   law assistance. Finally, we aim to contribute to know why judicial
   reforms, introduced by legislation, are not resulting in a truly
   judicial independence in Serbia, and what are the main obstacles to its
   effective implementation. (C) 2017 The Regents of the University of
   California. Published by Elsevier Ltd. All rights reserved.},
