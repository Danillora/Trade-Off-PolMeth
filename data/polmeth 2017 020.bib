@article{ WOS:000394952100009,
Author = {Scott, Tyler A. and Thomas, Craig W.},
Title = {Unpacking the Collaborative Toolbox: Why and When Do Public Managers
   Choose Collaborative Governance Strategies?},
Journal = {POLICY STUDIES JOURNAL},
