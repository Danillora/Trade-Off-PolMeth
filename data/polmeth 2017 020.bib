@article{ WOS:000399602800003,
Author = {Field, Antony},
Title = {The Dynamics of Terrorism and Counterterrorism: Understanding the
   Domestic Security Dilemma},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2017},
Volume = {40},
Number = {6},
Pages = {470-483},
Abstract = {This article argues that the concept of the domestic security dilemma
   can help us to better understand public opposition to government
   counterterrorism policies. It examines the concept of the security
   dilemma in international relations theory and argues that this concept
   can also be applied to the analysis of domestic security politics. The
   article explains that when the government takes actions intended to make
   people safer from terrorist threats, it often has the unintended
   consequence of heightening concerns about government oppression. Thus,
   counterterrorism represents a domestic security dilemmaa situation where
   security tradeoffs have consistently undermined anticipated security
   gains.},
