@article{ WOS:000415755800006,
Author = {Garcia-Juanatey, Ana and Jordana, Jacint and Sancho, David},
Title = {Administrative Innovations and Accountability Failures: The Termination
   of the Spanish Telecommunications Agency},
Journal = {SOUTH EUROPEAN SOCIETY AND POLITICS},
Year = {2017},
Volume = {22},
Number = {3},
Pages = {385-404},
Abstract = {We discuss how far accountability practices may be relevant to the
   success of independent regulators in countries without a tradition of
   administrative autonomy. We develop a conceptualisation of agency
   accountability failures and develop an analytical framework to examine
   the life and termination of the Spanish regulatory agency for
   telecommunications, CMT (Comision del Mercado de las Telecomunicaciones
   - Telecommunications Market Commission). We argue that the CMT's
   termination was related, among other reasons, to accountability failures
   in its institutional design. The paper relies on a variety of sources,
   including legislation, literature, media coverage, and semistructured
   interviews with agency officials and several stakeholders.},
