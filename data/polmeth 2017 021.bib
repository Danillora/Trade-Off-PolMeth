@article{ WOS:000398128000012,
Author = {Blair, Robert A. and Blattman, Christopher and Hartman, Alexandra},
Title = {Predicting local violence: Evidence from a panel survey in Liberia},
Journal = {JOURNAL OF PEACE RESEARCH},
