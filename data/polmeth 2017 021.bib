@incollection{ WOS:000438076000006,
Author = {Bedi, Ibrahim},
Editor = {Mensah, K},
Title = {Political Financing and Fund-Raising in Ghana},
Booktitle = {POLITICAL MARKETING AND MANAGEMENT IN GHANA: A NEW ARCHITECTURE},
Series = {Palgrave Studies in Political Marketing and Management},
Year = {2017},
Pages = {97-109},
Abstract = {Political party financing and fund-raising and its influence on
   corruption and securing political office and contracts have occupied the
   public space over the last decade. The study is an attempt to understand
   political financing in Ghana and its effects on parties. The
   interpretative approach was employed by conducting interviews with
   officers of political parties involved in financial management.
   Political financing has been left in the hands of a few `rich' members
   and `friends' of the parties. Political parties use indirect means of
