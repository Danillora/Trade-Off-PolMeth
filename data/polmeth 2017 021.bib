@article{ WOS:000412039900005,
Author = {Stewart, Susan and Dollbaum, Jan Matti},
Title = {Civil society development in Russia and Ukraine: Diverging paths},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
