@article{ WOS:000399844600001,
Author = {Gugushvili, Dimitri},
Title = {Lessons from Georgia's neoliberal experiment: A rising tide does not
   necessarily lift all boats},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2017},
Volume = {50},
Number = {1},
Pages = {1-14},
Month = {MAR},
Abstract = {Between 2004 and 2012, Georgia implemented one of the most comprehensive
   packages of neoliberal economic reforms ever. These reforms have
   certainly helped to spur growth, but their social effects remain
   under-researched. To narrow this gap, this article investigates the
   effects of growth on poverty in Georgia using the official household
   survey data. The analysis shows that contrary to popular expectations,
   poverty has decreased only slightly throughout this period and remains
   high despite a number of progressive measures adopted by a successor
   coalition government. These findings provide further evidence on the
   inappropriateness of the neoliberal model as a poverty reduction
   strategy. (c) 2016 The Regents of the University of California.
   Published by Elsevier Ltd. All rights reserved.},
