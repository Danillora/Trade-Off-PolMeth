@article{ WOS:000408660300004,
Author = {Dian, Matteo},
Title = {The strategic value of the Trans-Pacific Partnership and the
   consequences of abandoning it for the US role in Asia},
Journal = {INTERNATIONAL POLITICS},
Year = {2017},
Volume = {54},
Number = {5},
Pages = {583-597},
Month = {SEP},
Abstract = {The promotion of the Trans-Pacific Partnership (TPP) represented a major
   component of the strategy of rebalancing. Through the TPP, the Obama
   administration aimed at achieving several objectives: (1) `locking in'
   an America-friendly form of free market capitalism as the main normative
   platform for economic integration in the Asia Pacific region; (2)
   reaffirming Washington's capacity to shape the rules and norms of the
