@article{ WOS:000407912300001,
Author = {Monsod, Toby C.},
Title = {Political appointees in the Philippine bureaucracy: do increasing
   numbers help or harm?},
Journal = {PHILIPPINE POLITICAL SCIENCE JOURNAL},
Year = {2017},
Volume = {38},
Number = {1},
Pages = {1-27},
Abstract = {This essay looks at where the line is drawn between political appointees
   and careerists in the Philippine government and its effects on agency
   performance. Using the period 1986 to 2010 as a case study, the study
   unpacks the Philippine civil service to reveal the location and scale of
   political appointees in the bureaucracy, describes common techniques
   used by presidents to increase the number and penetration of appointees,
   and provides a quantitative evaluation of effects. Of specific interest
   is the thesis from Lewis that while political appointees in the
   bureaucracy can be a short term strategy for improving performance, the
   long term consequences are pernicious even when appointees are of
   consistently high quality. The available data permits only the simplest
   of regressions but results indicate that political appointees may indeed
   help in the short term but ultimately cause harm in the longer run.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
