@article{ WOS:000409203600006,
Author = {Fouilleux, Eve and Bricas, Nicolas and Alpha, Arlene},
Title = {`Feeding 9 billion people': global food security debates and the
   productionist trap},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2017},
Volume = {24},
Number = {11, SI},
Pages = {1658-1677},
Abstract = {Food security, a long-established item on the international agenda,
   raises many issues including production, consumption, poverty,
   inequalities, healthcare and conflicts. However, in 2007/2008 the global
   food security debate was relaunched with a single dominant focus which
   continues to the present day: increasing agricultural production. This
   paper explains this productionist bias - which may translate into
   inadequate policies - by combining insights from institutionalist and
   cognitive analyses. We show that, despite recent reforms, the global
   food security field remains dominated by macro- and micro-institutions
   that put food availability and agricultural production at the heart of
   the problem and solutions. The political and discursive strategies
   developed by transnational corporations and private foundations to
   promote their productivist interests are also key, along with the
   demands of dominant farmers' unions in exporting countries. Although
   advocating opposite development patterns, civil society actors
   implicitly reinforce the productionist perspective through their focus
   on family agriculture.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
