@article{ WOS:000405316900003,
Author = {Boon, Hoo Tiang},
Title = {Hardening the Hard, Softening the Soft: Assertiveness and China's
   Regional Strategy},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2017},
Volume = {40},
Number = {5},
Pages = {639-662},
Abstract = {There is a growing view that the emerging brand of Chinese regional
   diplomacy in recent years is increasingly assertive. This article
   attempts to make better sense of this perceived more forceful Chinese
   diplomacy. It argues that Chinese regional behavior is more profitably
   understood through the lens of a two-pronged foreign policy strategy
   that combines two particular aspects. One is a tougher and more
   uncompromising approach toward issues that China regards as concerning
   its core interests. The other is a more flexible and cooperative
   position toward interests that, while significant, are of secondary
   importance.},
