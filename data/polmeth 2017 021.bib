@article{ WOS:000396534100006,
Author = {van den Broek, Hans-Peter},
Title = {Labelling and Legitimization: Justifying Political Violence in the
   Basque Country},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2017},
Volume = {29},
Number = {1},
Pages = {119-136},
Abstract = {This article focuses on the role of labelling in the discourse employed
   by the Left-Wing Nationalist movement in the Basque Country to
   legitimize the use of violence for political ends. The approach in this
   article goes beyond classic labelling theory. I demonstrate that radical
   Nationalists do not passively undergo their being labelled as deviants
   (fanatics, terrorists) by society, but develop counter-labels instead to
   define their opponents and re-label themselves.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
