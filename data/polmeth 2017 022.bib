@article{ WOS:000407182900004,
Author = {Kovac, Polonca and Jukic, Tina},
Title = {Recent Regional Developments in Red Tape Reduction A Content Analysis of
   Web of Science Journals},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2017},
Volume = {15},
Number = {3},
Pages = {433-457},
Month = {JUL},
Note = {Lex Localis - Journal of Local Self-Government Annual Conference, Brno,
   CZECH REPUBLIC, JUN 15-16, 2017},
Abstract = {The economic crisis has led to slowdowns in both national and wider
   regional economies. A comparatively successful approach to mitigate its
   impacts is red tape or administrative burden reduction through
   procedural and other simplifications. This paper explores the relevant
   public policies examined in 70 scientific articles, classified in the
   Web of Science under ``red tape{''} or ``administrative burden{''} and
   related terms and published between 2013 and 2015. The study focuses on
   the understanding of red tape in terms of regional relevance, dominant
   doctrinal and disciplinary framework and types of cooperation in
   research, in an attempt to identify inter-boundaries dialogue as a means
   of cross-regional learning. The research fmdings show a prevailing field
   role of economic, managerial and procedural measures since red tape
