@article{ WOS:000407306300001,
Author = {Hunt, Paul},
Title = {Configuring the UN Human Rights System in the ``Era of
   Implementation{''}: Mainland and Archipelago},
Journal = {HUMAN RIGHTS QUARTERLY},
