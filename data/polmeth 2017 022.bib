@article{ WOS:000409934000011,
Author = {Hustedt, Thurid and Danken, Thomas},
Title = {Institutional logics in inter-departmental coordination: Why actors
   agree on a joint policy output},
Journal = {PUBLIC ADMINISTRATION},
Year = {2017},
Volume = {95},
Number = {3},
Pages = {730-743},
Month = {SEP},
Abstract = {By investigating two German inter-departmental committees, this article
   shows that the policy output of these coordination bodies depends on the
   specific institutional logic evoked throughout the coordination process.
   While in one of the groups a policy logic prevailed and a joint
   coordination output was achieved, the other was dominated by a political
   logic and proved unable to achieve agreement. The article contributes to
   research on government coordination by showing that actor orientations
   are crucial for explaining inter-organizational coordination. The
   results direct attention to the behavioural implications of coordination
   structures.},
