@article{ WOS:000404047200014,
Author = {Becker, Jordan and Malesky, Edmund},
Title = {The Continent or the ``Grand Large{''}? Strategic Culture and
   Operational Burden-Sharing in NATO},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
