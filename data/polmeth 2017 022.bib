@article{ WOS:000424615900004,
Author = {Perea Ozerin, Iratxe},
Title = {Collective Action by Women and Processes of Emancipation in Latin
   America and the Caribbean. An Examination of the Cases of Cuba, Bolivia
   and Ecuador},
Journal = {FORO INTERNACIONAL},
Year = {2017},
Volume = {57},
Number = {4},
Pages = {915+},
Month = {OCT-DEC},
Abstract = {The participation of women's movements in processes of emancipation has
   been dealt with from the perspective of international relations,
   emphasizing analysis of the Latin American context. Following this line
   of research, this paper proposes a study of the effectiveness of this
   participation in processes that present different characteristics with
   regard to the incorporation of this social participation and to the
   nature of the social actors incorporated, in this case women's
   movements. The paper focuses on the Cuban Revolution and the recent
   constituent processes in Bolivia and Ecuador.},
