@article{ WOS:000401553700006,
Author = {Langer, Ana Ines and Sagarzazu, Inaki},
Title = {Are All Policy Decisions Equal? Explaining the Variation in Media
   Coverage of the UK Budget},
Journal = {POLICY STUDIES JOURNAL},
Year = {2017},
Volume = {45},
Number = {2},
Pages = {337-358},
Month = {MAY},
Abstract = {A country's budget is one of the most important public policy
   instruments, as it establishes the government's policy priorities and
   has the potential to determine winners and losers. The budget, however,
   is a mixture of different components and these get varying degrees of
   attention in the media. Drawing on sociology of news research, this
   paper seeks to explain this heterogeneous coverage of a budget's policy
   decisions. To do so, it uses a unique data set of over 5,000 articles of
   press coverage of six UK budgets (2008-2012). These articles are coded
   for the presence/absence of each of the budget's policy decision, via
   automated content analysis. On the basis of a multivariate negative
   binomial model, we find that the salience of a policy decision in the
   coverage is determined by its cost, whether it is negative (i.e., tax
   hikes and spending cuts) or positive, the income group that is the most
   affected by it, and the level of attention given to it by the
   government.},
Publisher = {WILEY},
