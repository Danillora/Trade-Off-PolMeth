@article{ WOS:000403483400003,
Author = {Li, Huiping and Wang, Qingfang and Zheng, Chunrong},
Title = {Interjurisdictional Competition and Intracity Fiscal Disparity across
   Chinese Prefectural Cities},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2017},
Volume = {30},
Number = {3},
Pages = {365-385},
Month = {JUL},
Abstract = {Fierce competition among county-level governments has substantially
   increased disparities in public service provision and housing markets
   across urban China. The role of the changing intracity administrative
   structure deserves scholarly, attention in this process. Building on an
   interdisciplinary literature of public choice and cadre promotion
   theory, we propose that interjurisdictional competition enlarges the
   intracity fiscal disparity under China's transitional administrative and
   fiscal systems. Results from fixed-effect panel data modeling confirm
   that cities with higher level of interjurisdictional competition are
   associated with higher intracity fiscal disparity. This study
   demonstrates how transition and decentralization of the centralized
   fiscal system have long-term, intended and unintended, impacts on fiscal
   inequality and urban stratification. With continuous urban sprawl,
   jurisdictional annexation, and large-scale urban-to-rural migration, our
   study suggests that policies attempting to equalize public service
   distribution within cities need to consider the administrative structure
   at the local and regional levels.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Li, HP (Corresponding Author), Shanghai Univ Finance \& Econ, Shanghai, Peoples R China.
   Li, Huiping; Zheng, Chunrong, Shanghai Univ Finance \& Econ, Shanghai, Peoples R China.
