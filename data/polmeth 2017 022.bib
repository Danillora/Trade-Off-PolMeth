Author = {Vega Canovas, Gustavo and Campos Ortiz, Francisco E.},
Title = {Economic Integration in North America and the Election of Donald Trump:
   A Prospective Analysis on the Future of NAFTA},
Journal = {FORO INTERNACIONAL},
Year = {2017},
Volume = {57},
Number = {4},
Pages = {781+},
Month = {OCT-DEC},
Abstract = {This paper evaluates the international economic policy of the United
   States in the context of the administration of President Donald Trump.
   The paper analyzes the dissatisfaction with the process of globalization
   in the United States, the consequences of its abandonment of the
   Trans-Pacific Partnership Agreement and the potential scenarios raised
   by the imminent renegotiation of the North American Free Trade
   Agreement. The main spheres and disciplines that need to be updated or
   added to the NAFTA are analyzed on the basis of international trends in
   trade and investment regulation in the twenty-first century.},
