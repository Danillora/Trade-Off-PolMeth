@article{ WOS:000211791000006,
Author = {Fruehling, Stephan and O'Neil, Andrew},
Title = {Nuclear weapons and alliance institutions in the era of President Trump},
Journal = {CONTEMPORARY SECURITY POLICY},
Year = {2017},
Volume = {38},
Number = {1},
Pages = {47-53},
Abstract = {The swearing in of Donald Trump as the 45th President of the United
   States presages, at the very least, a period of flux in American
   strategy with respect to the relationship between nuclear weapons and
   alliances. In this response to three thoughtful rejoinders to our
   article, ``Nuclear weapons, the United States and alliances in Europe
   and Asia: Toward an institutional perspective,{''}we clarify key aspects
