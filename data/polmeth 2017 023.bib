@article{ WOS:000393720100002,
Author = {Oh, Youngmin and Park, Jongsun},
Title = {The Impact of Different Public Contracting Management Models on the
   Inputs and Outcomes of Local Public Services},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2017},
Volume = {15},
Number = {1},
Pages = {19-41},
Month = {JAN},
Abstract = {Many empirical studies have found that local public contracting reduces
   the inputs and outcomes of public services. However, few studies have
   examined what factors are associated with such inputs and outcomes. To
   address the limitation, this study conceptualizes two competing
   contracting management models (competitive and cooperative) and
   empirically tests the associations between these contracting approaches
   and service inputs and outcomes. The findings report that while local
   governments using competitive contracting elements have lower service
   inputs through reduced local operating expenditures and part-time
   employment; cooperative contracting elements do not necessarily produce
   enhanced service outcomes.},
