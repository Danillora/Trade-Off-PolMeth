@article{ WOS:000424614800005,
Author = {Giaccaglia, Clarisa},
Title = {Traditional, Emerging and Re-Emerging Powers: Ambiguous, yet Pragmatic
   Relationships},
Journal = {FORO INTERNACIONAL},
Year = {2017},
Volume = {57},
Number = {2},
Pages = {422+},
Month = {APR-JUN},
Abstract = {This paper reflects on the role that the emerging powers have developed
   in different spheres of global decision-making. Firstly, it describes
   the current international order, and conceptualizes the category of
   emerging power, as derived from that of middle power. Next, the paper
   focuses on three fields of global governance: commercial sphere (WTO),
   financial sphere (World Bank, FMI, G-20), and environmental sphere
   (summits on climate change). In this way it aims to establish whether
   the role of the emerging powers has led to modifications in
   international forums for decision-making, and how the relationships
   between traditional, emerging and re-emerging powers have been
   reconfigured.},
