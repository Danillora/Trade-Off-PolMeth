@article{ WOS:000400593000003,
Author = {Lowry, William R. and Krummenacher, W. Scott},
Title = {Coalitions and Conservation: Conditional Impacts of Coalitions on Ballot
   Measures for Open Space},
Journal = {REVIEW OF POLICY RESEARCH},
Year = {2017},
Volume = {34},
Number = {3},
Pages = {357-377},
Month = {MAY},
Abstract = {Social scientists have long debated the impact of interest group
   coalitions on public policies. While views on coalition impacts range
   from dominance to impotence, an emerging perspective suggests that
   coalitions have impacts under certain conditions. In this paper, we join
   and expand that perspective by arguing that coalitions have a
   conditional impact on public policies through ballot measures.
   Specifically, we argue that coalitions will have greater impact on
   ballot measure outcomes in non-presidential election years, when the
   stakes involved are high, and when the goals of the measure are diverse.
   We test these hypotheses with analyses of a dataset of over 2,400 ballot
   measures on spending for open space at the state and local level between
   1988 and 2014. We find strong support for our hypotheses. The findings
   have implications for scholarly debates on interest groups and
   coalitions, for the role of ballot measures in American public policy,
   and for assessments of open space and conservation in American society.},
