@article{ WOS:000416573300003,
Author = {Avdan, Nazli and Uzonyi, Gary},
Title = {V for Vendetta: Government Mass Killing and Domestic Terrorism},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2017},
Volume = {40},
Number = {11},
Pages = {934-965},
Abstract = {Scholarship explores the impact of human rights abuse and state
   repression on terrorism. Heretofore, scholarship has ignored the impact
   of government-sponsored killings on domestic terrorism. This article
   proposes that mass killings create a focal point for terrorist
   mobilization. The vendetta agenda fuels violence by animating
   retributory violence. Additionally, mass atrocities create a permissive
   environment for violent nonstate activity. A spiral of violence ensues
   whereby groups resort to terrorism. Utilizing data from the Global
   Terrorism Database, 1971-2011, the study shows that mass killings
   significantly increase domestic terrorism. It contributes to emerging
   scholarship examining how state policies influence terrorist activity.},
