@article{ WOS:000403473700010,
Author = {Koch, Florian and Sanchez Steiner, Lina Maria},
Title = {Participation without Power The Failure of Citizen Participation in
   Barranquilla},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2017},
Volume = {44},
Number = {2},
Pages = {168-183},
Month = {MAR},
Abstract = {Citizen participation has become an important political strategy. A case
   study of the Barranquilla land-use plan employing Fung and Wright's
   theories on countervailing power reveals that the citizen participation
   promoted to comply with legal requirements was a failure because it was
   manipulated by the dominant public actor. In societies such as that of
   Barranquilla, which employs traditional modes of politics, there is
   little chance of fostering successful participatory processes because
   there is no countervailing power to offset the established one. The
   violent context that surrounds the political sphere, low confidence in
   state institutions, and the convergence of economic and political power
   prevent the creation of strong and independent countervailing powers.},
