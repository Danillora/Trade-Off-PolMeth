@article{ WOS:000403473700004,
Author = {Franz, Tobias},
Title = {Urban Governance and Economic Development in Medellin An ``Urban
   Miracle{''}?},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2017},
Volume = {44},
Number = {2},
Pages = {52-70},
Month = {MAR},
Abstract = {Many of the academic analyses calling Medellin's development an ``urban
   miracle{''} fall short with regard to discussion of the political
   economic implications of institutional shifts. An emerging transnational
   capitalist class promoted `good governance' reforms and the embedding of
   neoliberalism in the urban context. Medellin's neoliberal development
   agenda is not only a market-led strategy but also a particular form of
   hierarchic rule and distribution of power. Increased economic activities
   in the tertiary sector, the promotion of flexible labor markets, and the
   incorporation of the city into the global economy at the lower end of
   the value chain have not served as sustainable growth escalators for
   Medellin's economy. The city continues to have high rates of un- and
   underemployment and is still the country's most unequal city. These
   developments can by no means be described as miraculous.},
