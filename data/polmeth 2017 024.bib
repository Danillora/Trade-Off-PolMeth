@article{ WOS:000416572500001,
Author = {Phillips, Vaughan},
Title = {The Islamic State's Strategy: Bureaucratizing the Apocalypse through
   Strategic Communications},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2017},
Volume = {40},
Number = {9},
Pages = {731-757},
Abstract = {To understand the Islamic State's strategy, we have to look at the way
   the group twins Maoist and post-Maoist strategies, previously considered
   strategically incompatible. By establishing a state that it claims to
   embody the Caliphate, it not only gains revenue and resources, but also
   generates a seductive ``brand{''} with a compelling message that it
   ``sells{''} via the Internet. This brand, based on Propaganda of the
   Deed, synergizes its physical and digital activities to create a
   virtuous circle that is very close to being self-sustaining. As such it
   represents a new insurgent model that corroborates the News Wars thesis.},
