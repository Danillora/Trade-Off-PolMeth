@article{ WOS:000406599400007,
Author = {Rivera Hernandez, Raul Diego},
Title = {Making Absence Visible: The Caravan of Central American Mothers in
   Search of Disappeared Migrants},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2017},
Volume = {44},
Number = {5},
Pages = {108-126},
Month = {SEP},
Abstract = {The touring model employed by the Caravan of Central American Mothers in
   search of disappeared migrants in transit through Mexico creates
   processes of political empowerment for poor, indigenous peasant women
   who have no previous experience as activists and human rights defenders.
   Interviews and participant observation with members and organizers of
   the ninth caravan (held in December 2013) reveal three key moments that
   anchor the mothers' transformation into political subjects and human
   rights activists: the creation of a collective identity based on
   maternal activism, the forging of an alliance with transnational human
   rights networks, and the emergence of a budding politics of visibility
   based on public acts of grieving that candidly lament the vulnerability
   of migrants caught in the drug war and border securitization across the
   hemisphere.},
