@article{ WOS:000419815900002,
Author = {Heidelberg, Roy L.},
Title = {Public administration and the logic of resolution},
Journal = {CRITICAL POLICY STUDIES},
Year = {2017},
Volume = {11},
Number = {3},
Pages = {272-290},
Abstract = {Through a critical reading of Wilson's 1887 article, I propose a
   political logic of public administration, what I call the logic of
   resolution. A central but neglected aspect of Wilson's article is the
   focus and importance of closure and creation, both of which underscore
   how public administration emerged not only as an area of study, but also
   as a political project. I discuss three central considerations of the
   logic of resolution: problem framing, solutionism, and optics. This
   logic conditions how problems can be viewed and, consequently, limits
   available actions. By developing the logic of resolution, I make
   explicit the hegemonic discourse of public administration.},
