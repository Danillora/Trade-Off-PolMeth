@article{ WOS:000418340500006,
Author = {Nadege Compaore, W. R.},
Title = {``Here is the young woman who wants to ask questions{''} To deal with
   gender and changing posture during the field survey},
Journal = {ETUDES INTERNATIONALES},
Year = {2017},
Volume = {48},
Number = {1},
Pages = {105-116},
Month = {WIN},
Abstract = {My research examined transparency measures in Gabon and Ghana's oil and
   gas industry and involved three months of field work in each country.
   Being a woman had an effect on my work in the field, but the fact that I
   am young, a researcher, black, African, and based in Canada also
   influenced respondents' attitudes towards me and the responses they
   offered. Reactions to my being an African based in the West were
   especially contradictory - ranging from friendly to suspicious - which
   complicated my interactions with respondents. For this research note I
   draw on Butz and Besio's concept of ``autoethnographic space{''} (2009)
