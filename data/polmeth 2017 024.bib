@article{ WOS:000390142000003,
Author = {Bayulgen, Oksan and Ladewig, Jeffrey W.},
Title = {Vetoing the future: political constraints and renewable energy},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2017},
Volume = {26},
Number = {1},
Pages = {49-70},
Abstract = {Despite the increased importance of and attention to renewable energy,
   its share in the overall energy mix has varied significantly across
   countries and over time. There are many determinants of clean energy
   transitions; this study focuses on political constraints. Here it is
   argued that political systems that have fewer political constraints have
   fewer access points through which powerful status quo veto players can
   slow the progress of clean energy reforms. To test the theory, a
   hierarchical model is applied on a dataset of 125 countries over four
   decades. The results provide significant support for the theory.
   Furthermore, the effects for political constraints hold even when we
   distinguish between hydro and non-hydro renewable sources and control
   for regime type. This study builds on research that recognizes the
   importance of politics in understanding the challenges and opportunities
   of clean energy reform.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
