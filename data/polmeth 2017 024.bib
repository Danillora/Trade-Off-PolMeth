@article{ WOS:000407609300003,
Author = {Mastenbroek, Ellen},
Title = {Guardians of EU law? Analysing roles and behaviour of Dutch legislative
   drafters involved in EU compliance},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2017},
Volume = {24},
Number = {9},
Pages = {1289-1307},
Abstract = {By drafting statutes and delegated acts, national legislative drafters
   play a crucial role in European Union (EU) compliance. Given their
   extensive legal training, they can be expected to operate as guardians
   of EU law' and thus correct national non-compliant tendencies. Yet, they
   also have a role as politically loyal civil servants, responsive to
   national political demands. This conntribution answers the question of
   to what extent Dutch legislative drafters fulfil a role of guardian of
   EU law'. Using in-depth interviews, the paper analyses legislative
   drafters' role conceptions and their strategies in case national
   political demands prove incompatible with EU legal requirements. It
   finds that most Dutch legislative drafters try to reconcile EU law with
   their ministers' political demands, if necessary by reinterpreting EU
   law. When this proves unfeasible, most respondents prioritize political
   loyalty over EU legality. Ultimately, therefore, legislative drafters do
   not form an insurmountable normative factor in EU compliance.},
