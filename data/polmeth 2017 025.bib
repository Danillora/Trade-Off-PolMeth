@article{ WOS:000423948600003,
Author = {Batta, Anna},
Title = {Patterns of Ethnic Marginalization and Inclusion in New States},
Journal = {ETHNOPOLITICS},
Year = {2017},
Volume = {16},
Number = {5},
Pages = {471-487},
Abstract = {Many new states have formed in the international system as a result of
   secession. However, such states are rarely entirely homogenous and
   nationalist elites face the challenge of creating a brand new state
   while ensuring minority rights and avoiding future conflict. Why do some
   secessionist states protect minority rights and create inclusive
   political institutions while others marginalize or discriminate against
   minorities? In this paper, the author examines patterns of
   marginalization after secession and provides an overarching theory of
   inclusion in all secessionist cases and all ethnic groups that reside in
   newly established countries.},
