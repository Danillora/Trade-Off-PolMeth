@article{ WOS:000395401900001,
Author = {da Cruz, Nuno F. and Marques, Rui Cunha},
Title = {Structuring composite local governance indicators},
Journal = {POLICY STUDIES},
Year = {2017},
Volume = {38},
Number = {2},
Pages = {109-129},
Month = {MAR},
Abstract = {The importance of good governance is praised by many academics and
   practitioners. The prominence of the subject suggests that measurement
   is important. However, setting out to measure the quality of governance
   empirically is controversial. Doubts regarding the feasibility and
   meaningfulness of this undertaking are widespread in the literature.
   Recognizing the potential caveats, the current article discusses a set
   of guidelines for structuring a theoretically sound local governance
   assessment tool based on the Decision Analysis and Operational Research
   literatures. The authors argue that using a multi-criteria model which
   employs several objective (quantitative and qualitative) indicators and
   relies on a participatory method to aggregate them is a suitable way of
   developing sensible Local Governance Indicators. The purpose here is to
   provide a detailed roadmap for any country (or region, or locality)
   willing to engage in the assessment of the quality of local governance.
   The real-world implementation of a model developed according to these
   guidelines could help raise awareness, promote good practices and
   increase the governance literacy' of citizens. By operationalizing good
   governance, analysts may also be able to further investigate the
   relationships between local governance practices and several
   socio-economic factors.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {da Cruz, NF (Corresponding Author), London Sch Econ \& Polit Sci, LSE Cities, London, England.
