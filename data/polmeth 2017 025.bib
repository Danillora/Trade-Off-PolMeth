@article{ WOS:000396596300004,
Author = {Koneska, Cvete},
Title = {On peace negotiations and institutional design in Macedonia: social
   learning and lessons learned from Bosnia and Herzegovina},
Journal = {PEACEBUILDING},
Year = {2017},
Volume = {5},
Number = {1, SI},
Pages = {36-50},
Abstract = {Twenty years after the end of the inter-ethnic war in Bosnia and
   Herzegovina, it appears the Dayton Peace Agreement has been successful
   in ensuring peace and stability in the country. However, while violence
   has not recurred since 1995, the track record of the Dayton agreement in
   discouraging secession threats and enabling a relatively sustainable
   decision-making political process has been less successful. Reflecting
   on how the peace and post-conflict institutions were negotiated in
   Macedonia in 2001, this paper examines if any lessons learned from
   Bosnia and Herzegovina were applied in the Macedonian case. It argues
   that both external and domestic political actors sought to avoid some of
   the key negative outcomes from the Bosnian case, opting for a
   centralised instead of federal territorial structure and a much more
   flexible power-sharing deal. Still, important limitations remain to the
   lessons learned from the Bosnian case and their wider applicability to
   contemporary conflicts.},
