@article{ WOS:000423254100003,
Author = {Bjorn, Fagersten},
Title = {Sweden, EU and Brexit- effects, distress and choices},
Journal = {INTERNASJONAL POLITIKK},
Year = {2017},
Volume = {75},
Number = {2},
Pages = {131-141},
Abstract = {The British exit from the European Union puts Sweden in a difficult
   position. The UK has been a close partner and has offered protection in
   areas where Sweden's interests have been challenged. This article looks
   at Sweden's relation to the EU and examines Swedish-British cooperation
   and the effects that Sweden can expect following Brexit. The Swedish
   Brexit-debate is scrutinized as are the choices that are now facing the
   country in a shifting strategic environment. It is suggested that the
   political cost of taking on the UK's role as a sceptic in the area of EU
   security and defense policy or as a watchdog in relation to the Eurozone
   will be difficult for Sweden to bear. Consequently, Sweden could come
   out as a more loyal EU member and might with time also find its own role
   within the European integration project.},
