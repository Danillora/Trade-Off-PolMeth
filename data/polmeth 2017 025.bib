@article{ WOS:000394161700001,
Author = {Truong-Minh Vu},
Title = {International Leadership as a Process: The case of China in Southeast
   Asia},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2017},
Volume = {60},
Number = {1},
Abstract = {Leadership theory in IR still lacks a coherent approach, and it is
   analytically useful to use eclectic lenses by combining all factors
   related to power and the usage of power to gain leadership status. I
   define the term ``international leadership{''} as a process in which a
   state mobilizes its resources to influence a group of other states
   (followership) in order to achieve a common goal. In the empirical
   investigation, I will focus on China's abilities to lead in Southeast
   Asia. Despite the fact that there are many advantages for China, the
   mechanism of transforming power resources into regional leadership is
   still questionable.},
