@article{ WOS:000211791000003,
Author = {Knopf, Jeffrey W.},
Title = {Security assurances and proliferation risks in the Trump administration},
Journal = {CONTEMPORARY SECURITY POLICY},
Year = {2017},
Volume = {38},
Number = {1},
Pages = {26-34},
Abstract = {Comments that Donald Trump made while campaigning to be U.S. president
   have raised concerns that his administration will pull back from U.S.
   alliance commitments and encourage countries such as Japan and South
   Korea to acquire nuclear arms. The new article by Fruhling and O'Neil
   outlines an institutional framework that can be helpful in assessing the
   risks that Trump administration policies will lead to nuclear
   proliferation. An institutional perspective shows that important
   elements of U.S. security assurances will continue to function, and this
   reduces the chances that President Trump's actions or statements will
   trigger proliferation by U.S. allies. The greatest risk to global
   non-proliferation efforts posed by a Trump administration in fact lies
   elsewhere, in the possibility that President Trump will seek to abrogate
   the Iran nuclear deal.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
