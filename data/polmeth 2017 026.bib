@article{ WOS:000391002300013,
Author = {Lake, David A.},
Title = {Why Does Ethnicity Increase in Salience as Political Order Decays?},
Journal = {ETHNOPOLITICS},
Year = {2017},
Volume = {16},
Number = {1},
Pages = {82-88},
Abstract = {We know more about the sources of political violence than we sometimes
   admit. Conflict scholars now have a reasonably well-developed, if still
   largely informal, spiral model of ethnic mobilization, fear, and
   escalation that accounts for the breakdown of bargaining and the turn to
   violence. The key question for future research, however, is Why does
   ethnicity increase in salience when political order decays or is
   threatened? Addressing this puzzle will likely require delving into the
   social psychology of identity formation.},
