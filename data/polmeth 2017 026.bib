@article{ WOS:000395313200005,
Author = {Clark, Nicholas},
Title = {Explaining Political Knowledge: The Role of Procedural Quality in an
   Informed Citizenry},
Journal = {POLITICAL STUDIES},
Year = {2017},
Volume = {65},
Number = {1},
Pages = {61-80},
Month = {MAR},
Abstract = {While much is known about the micro-level predictors of political
   knowledge, there have been relatively few efforts to study the potential
   macro-level causes of knowledge. Seeking to improve our understanding of
   country-based variation in knowledge, this article demonstrates that
   individuals have an easier time finding and interpreting information in
   political environments that provide the public with greater
   opportunities to engage, observe, and learn about the political process.
   To investigate that possibility, the article analyzes how the procedural
   quality of the political process affects political knowledge. Using data
   from the Comparative Study of Electoral Systems and the Worldwide
   Governance Indicators Project, survey analyses show that the
   transparency and responsiveness of a political system indeed influence
   the public's information about political parties and, to a lesser
   extent, the amount of factual knowledge retained by survey respondents.
   In other words, the quality of democratic governance affects how much
   individuals know about the political process.},
Publisher = {SAGE PUBLICATIONS LTD},
Address = {1 OLIVERS YARD, 55 CITY ROAD, LONDON EC1Y 1SP, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Clark, N (Corresponding Author), Susquehanna Univ, Dept Polit Sci, 514 Univ Ave, Selinsgrove, PA 17870 USA.
   Clark, Nicholas, Susquehanna Univ, Dept Polit Sci, 514 Univ Ave, Selinsgrove, PA 17870 USA.},
DOI = {10.1177/0032321716632258},
ISSN = {0032-3217},
EISSN = {1467-9248},
Keywords = {political knowledge; political sophistication; procedural quality;
