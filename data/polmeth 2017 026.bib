@article{ WOS:000405004400004,
Author = {Hopkin, Jonathan},
Title = {When Polanyi met Farage: Market fundamentalism, economic nationalism,
   and Britain's exit from the European Union},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
