@article{ WOS:000416582500005,
Author = {Wells, Luke Benjamin},
Title = {The `bomber gap': British intelligence and an American delusion},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2017},
Volume = {40},
Number = {7},
Pages = {963-989},
Abstract = {For most of the 1950s, manned aircraft were the prime nuclear bomb
   delivery method, and were therefore a vital metric for British and
   American intelligence when calculating the Soviet threat. Each community
   reached very different conclusions from the same raw intelligence,
   generating the `bomber gap' myth in the US but not in the UK. The
   information available was inconclusive, so in order to understand it
   estimators had to rely on their assumptions, which were different.
   Contrasting scopes for parochial capitalisation drew their conclusions
   further apart. Contrary to orthodox accounts of this episode, Soviet
   deception did not play a central role.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
