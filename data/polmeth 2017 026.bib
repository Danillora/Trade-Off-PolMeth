@article{ WOS:000394911500008,
Author = {Decevic, Mehmed and Vukovic-Calasan, Danijela and Knezevic, Sasa},
Title = {Re-designation of Ethnic Muslims as Bosniaks in Montenegro: Local
   Specificities and Dynamics of This Process},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
