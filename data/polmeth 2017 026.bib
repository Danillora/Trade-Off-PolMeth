@article{ WOS:000390111100006,
Author = {Pedersen, Morten Jarlbaek},
Title = {Real-world Europeanisation: the silent turning of small gears},
Journal = {POLICY STUDIES},
Year = {2017},
Volume = {38},
Number = {1},
Pages = {91-107},
Month = {JAN},
Abstract = {Several studies of the impacts of the EU on modern policies and polities
   evade investigation of the most detailed level of Europeanisation, that
   is, gradual legal changes. This seems odd as it is exactly these small
   gears turning that constitute perhaps the most real and concrete of all
   types of Europeanisation. This paper looks at a range of cash benefits
   in Denmark. Applying a novel framework of silent versus loud
   Europeanisation, it investigates how legal changes since 1972 came into
   being by digging into both the legal details and the preparatory and
   parliamentary work. The conclusion is that Denmark - often mentioned as
   an archetype universalist welfare state at odds with the European logics
   of free movement and non-discrimination - has indeed altered the very
   nature of its most universalist cash benefits, but these changes have
   been very gradual and have often not even been recognised as
   Europeanisation.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
