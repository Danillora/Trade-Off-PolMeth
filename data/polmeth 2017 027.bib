@article{ WOS:000393814000008,
Author = {Moelders, Christina and Van Quaquebeke, Niels and Paladino, Maria Paola},
Title = {Consequences of Politicians' Disrespectful Communication Depend on
   Social Judgment Dimensions and Voters' Moral Identity},
Journal = {POLITICAL PSYCHOLOGY},
Year = {2017},
Volume = {38},
Number = {1},
Pages = {119-135},
Month = {FEB},
Abstract = {The present study investigates the consequences of respectful versus
   disrespectful communication in political debates on voters' social
   judgments and voting decisions. Reconciling previously mixed results, we
   argue that the consequences of disrespect vary with the judgment
   dimension (communion vs. agency) and voters' moral identity. An initial
   study (N5197) finds that a political candidate's disrespect towards his
   or her opponent affects voting decision through voting intention. A
   second study (N5327) shows that disrespect influences voting intention
   through communion but not through agency ratings. Qualifying the
   previous finding, a third study (N5329) shows that both communion and
   agency judgments act as mediators, but in different ways depending on
   the level of moral identity. Overall, communion judgments played a more
   prominent part in explaining the consequences of disrespectful
   communication. Our findings thus present a nuanced picture of respect
   and disrespect in political communication and shed light on their
   ramifications.},
