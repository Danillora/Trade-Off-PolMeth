@article{ WOS:000396233200001,
Author = {Freeland, Valerie},
Title = {Beyond Robert Jackson: post-colonial realities and the patronage state},
Journal = {INTERNATIONAL POLITICS},
Year = {2017},
Volume = {54},
Number = {2},
Pages = {125-143},
Month = {MAR},
Abstract = {Why do fundamentally un-statelike units, governed through patronage
   networks, persist in the modern state system? And why has the myth of
   the universal territorial state remained so robust despite these
   alternate political units? I argue, first, that the state system can be
   divided into bureaucratic and patronage states. While the former derive
   authority from and exert control over defined territory, the latter use
   defined populations for the same purposes. Second, I argue that
   powerful, Weberian states have incentives to bolster patronage states.
   Rather than allowing alternate modes of rule to threaten the sovereign
   territorial state's primacy, they encouraged the pretence of territorial
   statehood within the post-colonial world. The trappings of sovereignty
   gave patronage states the rights, resources and incentives to uphold the
   fac, ade and subdue non-state challengers within their territories. This
   suggests that the state system rests on a deeper hierarchy that excludes
   non-sovereign units from the system's benefits.},
