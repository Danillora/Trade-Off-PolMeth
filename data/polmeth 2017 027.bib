@article{ WOS:000413896300009,
Author = {Hill, Eleanor and Sobolewska, Maria and Wilks-Heeg, Stuart and
   Borkowska, Magda},
Title = {Explaining electoral fraud in an advanced democracy: Fraud
   vulnerabilities, opportunities and facilitating mechanisms in British
   elections},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
Year = {2017},
Volume = {19},
Number = {4, SI},
Pages = {772-789},
Month = {NOV},
Abstract = {Explanatory theories of electoral fraud are usually developed for new
   and failing democracies. However, while rarer, electoral fraud does
   happen in advanced democracies. Because data on fraud in advanced
   democracies are scarce, single instances of fraud are studied in
   isolation and offer very little generalisability. This study uses a
   unique comparative dataset of 35 in-depth, semi-structured interviews
   from eight locations, only half of which experienced allegations of
   fraud. We show that theories of why and how fraud happens in developing
   democracies can be extended to an advanced democracy. We also provide a
   detailed description of two micro-mechanisms, which facilitate fraud
   taking place and thus provide a causal link between the structural
   vulnerability to fraud and the direct opportunities for fraud to take
   place. The case study of Britain focuses mostly on the biraderi
   structures within the British South Asian communities, but we offer ways
   in which these structures generalise more broadly.},
