@article{ WOS:000416427200002,
Author = {Park, Yung Chul},
Title = {The Role of the IMF in Managing the Euro Area Sovereign Debt and Banking
   Crises: Perspectives from East Asia},
Journal = {GLOBAL POLICY},
Year = {2017},
Volume = {8},
Number = {4},
Pages = {443-454},
Month = {NOV},
Abstract = {This paper analyzes strategies and policy conditionality of the troika -
   European Commission, European Central Bank, and IMF - in resolving the
   euro area crisis from Asia's perspective with the 1997 IMF Asian crisis
   management as a possible counterfactual and a yard stick for their
   effectiveness. The IMF entered into collaboration with the EU mainly to
   provide the technical expertise in and share with its European partners
   the lessons it learned from managing past crises. But many of these
   lessons have been unheeded - in particular limitations of the export-led
   recovery through internal devaluation and the need to move forward with
   financial reform at an early stage of the crisis and calibrate supply
   side reform with the implementation capacity of the crisis countries.},
