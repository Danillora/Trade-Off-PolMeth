@article{ WOS:000417636600005,
Author = {Park, Sunghoon},
Title = {The new politics of trade negotiations: the case of the EU-Korea FTA},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2017},
Volume = {39},
Number = {7, SI},
Pages = {827-841},
Abstract = {The European Union (EU) has successfully negotiated with the Republic of
   Korea its first free trade agreement (FTA) under the scheme of the 2006
   Global Europe strategy. The EU-Korea FTA has triggered a number of
   changes, including the emergence of a new paradigm of
   trade-policy-making. While traditional negotiation was dominated by
   governments and/or executive agencies, a new political constellation has
   emerged with stronger involvement by other stakeholders, including
   parliamentary actors and civil society organizations (CSOs). Through a
   case study of FTA negotiations between the EU and Korea, this paper
   shows that the new players have gained weight in the process of
   negotiation both by increasing their influence in regulatory activities
   (Contentious Market Regulation) and creating effective channels for
   their involvement in the policy-making process (Feedback Loop). This
   paper also argues that though the changes that took place in both
   parties were similar, they have had different roots and dimensions.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
