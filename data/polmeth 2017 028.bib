@article{ WOS:000393720100006,
Author = {Vanleene, Daphne and Voets, Joris and Verschuere, Bram},
Title = {Co-Producing a Nicer Neighbourhood: Why do People Participate in Local
   Community Development Projects?},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2017},
Volume = {15},
Number = {1},
Pages = {111-132},
Month = {JAN},
Abstract = {Co-production is often considered a solution to improve service quality
   and production efficiency, yet research on the reasons why citizens
   participate in the creation and/or implementation of public services is
   still limited to specific sectors. In this article, we study the
   different citizens' motivations discussed in the literature. We
   distinguished two categories of motivations, personal and
   circumstantial. By means of a guided survey, we collected data on these
   different motivations in a Belgian case in the unstudied setting of
   community development. The results show that even in a community
   development case, where material incentives are used to entice citizens
   in a vulnerable socio-economic position to participate, the reasons
   behind co-production are still more diverse and complex than simple
   benefit maximization},
