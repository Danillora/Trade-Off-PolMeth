@article{ WOS:000400550300003,
Author = {Malamud, Andres and Alcaniz, Isabella},
Title = {Managing Security in a Zone of Peace: Brazil ` s Soft Approach to
   Regional Governance},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2017},
Volume = {60},
Number = {1},
Abstract = {Given Brazil's regional prevalence, its low, late and soft investment in
   regional security governance appears puzzling. We approach the puzzle
   through an analysis of contextual features, institutional overlap and
   policy networks, especially regarding nuclear energy and the
   environment. Our findings show that Brazil's behavior is explained by a
   combination of low regional risks, scarce domestic resources, a
   legalistic regional culture of dispute settlement, and transgovernmental
   networks that substitute for formal interstate cooperation and deep
   regional institutions.},
