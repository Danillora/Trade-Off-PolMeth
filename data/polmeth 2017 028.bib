@article{ WOS:000410411700005,
Author = {van Gils, Eske},
Title = {Differentiation through bargaining power in EU-Azerbaijan relations:
   Baku as a tough negotiator},
Journal = {EAST EUROPEAN POLITICS},
Year = {2017},
Volume = {33},
Number = {3, SI},
Pages = {388-405},
Abstract = {Using the case of democracy and human rights promotion, this article
   examines the concept of ``differentiation{''} in relations between
   Azerbaijan and the European Union (EU). Post-independence, Azerbaijan
   increasingly positions itself as a strong and influential actor,
   demanding more discretion from the EU, based on equal input and interest
   representation. This article argues that the EU approach remains too
   unilateral, causing the Azerbaijani government to resist and gain
   influence through different routes, including lobbying activities. The
   article concludes that EU policy-making mechanisms could become more
   differentiated to reflect the political reality of changing power
   dynamics between the EU and Azerbaijan.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
