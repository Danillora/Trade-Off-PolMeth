@article{ WOS:000403489000001,
Author = {Metternich, Nils W. and Minhas, Shahryar and Ward, Michael D.},
Title = {Firewall? or Wall on Fire? A Unified Framework of Conflict Contagion and
   the Role of Ethnic Exclusion},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2017},
Volume = {61},
Number = {6},
Pages = {1151-1173},
Month = {JUL},
Abstract = {While some borders are real firewalls against conflicts, others appear
   like tinder just waiting for the smallest spark. Only recently has
   research focused on the transnational perspective of conflict and
   current research has focused mostly on isolated aspects of this
   phenomenon. In this article, we provide a unified framework for conflict
   contagion that takes into account receiver, sender, dyad, and network
   effects. This is a novel perspective on conflict contagion, and our
   empirical results suggest that distinguishing between sender and
   receiver effects allows for a better understanding of spillover effects.
   We provide insights that especially excluded ethnic groups impact the
