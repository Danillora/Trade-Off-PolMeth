@article{ WOS:000403319400012,
Author = {Rice, Deborah},
Title = {How governance conditions affect the individualization of active labour
   market services: An exploratory vignette study},
Journal = {PUBLIC ADMINISTRATION},
Year = {2017},
Volume = {95},
Number = {2, SI},
Pages = {468-481},
Month = {JUN},
Abstract = {Service individualization is high on the agenda of active welfare
   states. Especially for the long-term unemployed and other people with
   complex problems, individualized employment and social services are seen
   as a crucial precondition for social and labour market integration. To
   achieve tailor-made service delivery, welfare states have introduced new
   governance mechanisms such as client profiling, performance indicators,
   game-theoretical incentives or institutionalized discretion. However,
   comparative research on how these governance mechanisms affect service
   individualization is still rare. To fill this gap, this article presents
   an exploratory vignette study on Denmark and the Netherlands conducted
   in 2010-12. The main finding of this study is that institutionalized
   discretion, as it is implemented in the Dutch minimum income system,
   increases the variety and innovativeness of employment services, makes
   service provision more holistic (especially for vulnerable clients), and
   increases the targeting capacity of service allocation between
   vulnerable and work-ready clients.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Rice, D (Corresponding Author), Carl von Ossietzky Univ Oldenburg, Dept Social Sci, Oldenburg, Germany.
   Rice, Deborah, Carl von Ossietzky Univ Oldenburg, Dept Social Sci, Oldenburg, Germany.},
