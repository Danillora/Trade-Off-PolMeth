@incollection{ WOS:000424275800007,
Author = {Salem, Fadi},
Editor = {Balakrishnan, MS and Moonesar, IA and Awamleh, R and RowlandJones, R},
Title = {Building a Smart City: Overcoming the Challenges of Digital
   Transformation - The Case of ``Smart Dubai{''}},
Booktitle = {UAE: PUBLIC POLICY PERSPECTIVES},
