@article{ WOS:000431136600002,
Author = {Deblock, Christian and Wells, Guy-Philippe},
Title = {Regulatory cooperation and trade agreements},
Journal = {ETUDES INTERNATIONALES},
Year = {2017},
Volume = {48},
Number = {3-4},
Pages = {319-345},
Month = {SUM-FAL},
Abstract = {One of the main characteristics of third-generation regionalism is the
   inclusion of institutional arrangements aimed at bringing the regulatory
   systems of participants closer into line. Two agreements, CETA and the
   TPP, have recently been signed, and both include a special section on
   regulatory cooperation, This topic is also on the agenda of trade
   negotiations between the United States and the European Union, where it
   is both the main focus and the biggest stumbling block. This article
   shows how regulatory provisions in these new agreements are linked to
   the debate over regulatory cooperation at other levels, especially
   within the WTO and the OECD. It also looks at the new parameters framing
   such cooperation and compares the regulatory provisions in the CETA and
   TPP texts we have access to.},
