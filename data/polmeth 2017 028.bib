@article{ WOS:000413896300003,
Author = {Chalmers, Damian},
Title = {Brexit and the renaissance of parliamentary authority},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
Year = {2017},
Volume = {19},
Number = {4, SI},
Pages = {663-679},
Month = {NOV},
Abstract = {With the promised return of Parliamentary powers, Brexit is supposed to
   be a fillip for representative democracy. There is a significant danger
   that will not be so. EU membership weakened the conditions for
   representative democracy within those fields governed by the EU.
   Whitehall will almost be the central player in the reform of EU-derived
   law. And policy styles are being developed which marginalise rather than
   consolidate UK legislatures in these fields. Restoration of
   representative democracy will require a realistic re-evaluation of its
   virtues so that these can be protected and a re-imagination of the
   possibilities and limits of parliaments within such a landscape.},
