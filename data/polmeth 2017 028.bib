@article{ WOS:000423948600005,
Author = {Graff-Mcrae, Rebecca L.},
Title = {Ghosts of Gender: Memory, Legacy and Spectrality in Northern Ireland's
   Post-Conflict Commemorative Politics},
Journal = {ETHNOPOLITICS},
Year = {2017},
Volume = {16},
Number = {5},
Pages = {500-518},
Abstract = {The spectrality of gender in conflict narratives and post-conflict
   politics has implications for how the conflict is framed, and therefore
   managed and normalized. This paper adapts Marysia Zalewski's concept of
   `Gender Ghosts' to analyse and deconstruct the dominant memory
   narratives surrounding the legacy of the Troubles in Northern Ireland.
   This approach underscores the precarious-and often marginalized-role of
   women, femininity and masculinity, and gender in the writing and
   rewriting of the conflictual past. Conceptualized as spectrality, this
   precarity can, however, disrupt and challenge exclusively
   ethno-nationalist constructions of the past.},
