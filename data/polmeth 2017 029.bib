@article{ WOS:000398041900005,
Author = {Harring, Niklas and Sohlberg, Jacob},
Title = {The varying effects of left-right ideology on support for the
   environment: Evidence from a Swedish survey experiment},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2017},
Volume = {26},
Number = {2},
Pages = {278-300},
Abstract = {Years of research show that left-leaning individuals are more supportive
   of environmental policies than right-leaning individuals. The
   explanation for the lower level of support among right-leaning
   individuals is their stronger preference for economic growth and lower
   acceptance of intervention in markets. However, recent cross-national
   studies have questioned whether the effect of ideology on environmental
   support is universal. A Swedish survey experiment shows that the effect
   of ideology varies greatly depending on how individuals think about the
   environment. Specifically, it demonstrates that if environmental support
   is contrasted with economic growth, then the effect of ideology is
   stronger as opposed to when environmental support is not juxtaposed with
   economic growth. Furthermore, among people who strongly perceive the
   environment as a left-right issue, there is a larger divide between left
   and right.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
