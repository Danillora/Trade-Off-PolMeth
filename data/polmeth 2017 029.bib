@incollection{ WOS:000419723900028,
Author = {Getenet, Belachew and Tefera, Berihun},
Editor = {Stave, K and Goshu, G and Aynalem, S},
Title = {Institutional Analysis of Environmental Resource Management in Lake Tana
   Sub-basin},
Booktitle = {SOCIAL AND ECOLOGICAL SYSTEM DYNAMICS: CHARACTERISTICS, TRENDS, AND
   INTEGRATION IN THE LAKE TANA BASIN, ETHIOPIA},
Series = {AESS Interdisciplinary Environmental Studies and Sciences Series},
Year = {2017},
Pages = {453-477},
Abstract = {Natural resource degradation in the form of soil erosion, deforestation,
   and wetland resource depletion, is a major problem in Lake Tana
   Sub-Basin, Ethiopia. Various measures have been taken by the government
   and its development partners in response. This study investigates
   whether measures such as formulation of policies, strategies, laws and
   proclamations as well as establishment of organizations contribute to
   the protection and/or management of natural resource management of the
   Lake Tana Sub-Basin or not. Government reports, journals, articles and
   other relevant sources were used to examine whether the measures taken
   fulfil their objectives. The findings of the study indicate that the
   policy measures and the efforts of translating them into practice do
   support conservation of natural resources in Lake Tana Sub-Basin.
   Various sustainable land management (SLM) technologies introduced by the
   government and development partners helped to address soil degradation.
   The remnants offorests around the lake and in the upper sub basin have
   been preserved and protected. Yet, land degradation in various forms
   still outweighs the rehabilitation of degraded lands and conservation of
   the natural resources in the sub basin. Lack of genuine participation of
   the local people, technical and policy failure, lack of enforcement of
   some policies/laws, overlapping and conflicting responsibilities of
   different government institutions and conflict of interests accounted to
   failure of achieve the desired outcomes. The findings suggest ways
   policy makers and development actors can address the multifaceted
   barriers to improve policy effectiveness.},
