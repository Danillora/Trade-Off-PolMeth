   Harinck, Fieke, Leiden Univ, Dept Social \& Org Psychol, Leiden, Netherlands.
   Druckman, Daniel, George Mason Univ, Sch Policy Govt \& Int Affairs, Fairfax, VA 22030 USA.
   Druckman, Daniel, Macquarie Univ, Soft Power Advocacy \& Res Ctr, Sydney, NSW, Australia.
   Druckman, Daniel, Macquarie Univ, Dept Polit \& Int Relat, Sydney, NSW, Australia.
   Druckman, Daniel, Univ Queensland, Sch Polit Sci \& Int Studies, Brisbane, Qld, Australia.},
DOI = {10.1177/0022002715569774},
ISSN = {0022-0027},
EISSN = {1552-8766},
