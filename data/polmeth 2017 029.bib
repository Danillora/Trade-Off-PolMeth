@article{ WOS:000402059900003,
Author = {Kumah-Abiwu, Felix},
Title = {Issue framing and electoral violence in Ghana: a conceptual analysis},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2017},
Volume = {55},
Number = {2},
Pages = {165-186},
Abstract = {Ghana has conducted several successful elections since 1992, but the
   country continues to face many threats of widespread violence due to the
   recurrent nature of micro-level electoral violence and the existence of
   vulnerabilities such as political patronage, politics of exclusion,
   winner-takes-all electoral system and ethnic cleavages. While these
   factors have been used to explain the causes of electoral violence,
   issue framing by political elites and its connection to electoral
   violence have not been adequately examined. To better understand this
   phenomenon, this article draws on the concept of framing to underscore
   the argument that issue framing and reframing by political elites tend
   to shape micro-level electoral violence in Ghana.},
