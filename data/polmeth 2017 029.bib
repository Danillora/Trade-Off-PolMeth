@article{ WOS:000394161700002,
Author = {Schleicher, Rafael Tavares and Platiau, Ana Flavia},
Title = {What is the relation between Brazilian Foreign Policy and the
   implementation of bilateral technical cooperation projects in African
   Countries? Lessons from a South-South cooperation project implemented by
   the Brazilian National School of Public Administration - ENAP
   (2009-2012)},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2017},
Volume = {60},
Number = {1},
Abstract = {Based on the ``street-level{''} and the ``theory of the bureaucracy{''}
   approaches, the article contends that national bureaucracies may play an
   important role in explaining the relation between Brazilian foreign
   policy and international technical cooperation. Drawing lessons from a
