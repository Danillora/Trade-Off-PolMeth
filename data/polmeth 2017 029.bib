@article{ WOS:000417409300004,
Author = {Rho, Sungmin and Tomz, Michael},
Title = {Why Don't Trade Preferences Reflect Economic Self-Interest?},
Journal = {INTERNATIONAL ORGANIZATION},
Year = {2017},
Volume = {71},
