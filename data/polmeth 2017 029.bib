@article{ WOS:000406086300007,
Author = {Montgomery, Tom and Mazzei, Micaela and Baglioni, Simone and Sinclair,
   Stephen},
Title = {Who cares? The social care sector and the future of youth employment},
Journal = {POLICY AND POLITICS},
Year = {2017},
Volume = {45},
Number = {3},
Pages = {413-429},
Month = {JUL},
Abstract = {The social care sector has been identified as a potential source of job
   creation for young people by policymakers, however the quality of such
   employment opportunities requires closer scrutiny. This study reveals
   how job creation strategies focused upon sectors such as social care
   which have high levels of employment insecurity can contribute towards
   reinforcing patterns of precarious work across Europe. Drawing upon
   research involving young people and representatives from the sector,
   this study argues that while attracting new employees is a worthy
   objective, the capacity of the social care sector to offer secure
   employment for young people is questionable.},
