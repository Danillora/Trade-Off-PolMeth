Affiliation = {McCauley, JF (Corresponding Author), Univ Maryland, Dept Govt \& Polit, 3140 Tydings Hall, College Pk, MD 20742 USA.
   McCauley, John F., Univ Maryland, College Pk, MD USA.},
DOI = {10.1080/17449057.2016.1235348},
ISSN = {1744-9057},
