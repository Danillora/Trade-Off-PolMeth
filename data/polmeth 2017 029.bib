@article{ WOS:000418865200008,
Author = {Bentley, Michelle},
Title = {Trigger warnings and the student experience},
Journal = {POLITICS},
Year = {2017},
Volume = {37},
Number = {4},
Pages = {470-485},
Month = {NOV},
Abstract = {The use of trigger warnings (TW) in higher education (HE) is a point of
   significant contention. Discussion has centred on academic perspectives.
   This study turns that around to ask what the student experiences of TW
   are, how they are perceived, and what this contributes to the existing
   debate. Based on survey and focus group data from two undergraduate
   modules, this article demonstrates that students are similarly divided
   on TW. It also re-assesses the current debate with respect to
   positive/negative effects on students, in-class applications of TW, how
   far students believe their learning environment should be `controlled',
   and perceived censorship.},
