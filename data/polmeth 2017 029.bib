@article{ WOS:000394161700005,
Author = {Duarte, Rubens de S. and Soares de Lima, Maria Regina},
Title = {Politicising financial foreign policy: an analysis of Brazilian foreign
   policy formulation for the financial sector (2003-2015)},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2017},
Volume = {60},
Number = {1},
Abstract = {At the start of the 21st century, there was a reconfiguration of the
   Brazilian domestic political dynamic, as its actors gradually became
   more interested in the formulation of foreign policy and tried to
   safeguard their interests. When analysing the country's foreign policy
   for the financial sector, it becomes quite clear that to consider
   foreign policy simply as one type of public policy is insufficient to
   assess its decision-making process. This paper seeks to contribute to
   the debate by arguing that the analysis of foreign policy should be
   carried out by taking into consideration specific issue-areas. This
   paper explores the movement to rearrange domestic decision power,
   seeking to gain leverage in international negotiations.},
