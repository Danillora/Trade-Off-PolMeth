@article{ WOS:000393361900007,
Author = {Hillebrecht, Courtney and Straus, Scott},
Title = {Who Pursues the Perpetrators? State Cooperation with the ICC},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2017},
Volume = {39},
Number = {1},
Pages = {162-188},
Month = {FEB},
Abstract = {Despite the International Criminal Court's increased prominence in
   international politics, there remains marked variation in states'
   cooperation with the ICC. This article asks why do states cooperate with
   the ICC following an indictment, arrest warrant or request for
   information, and how do these patterns of cooperation affect the Court's
   ability to constrain state behavior? Using comparative case studies from
   Kenya, Cote d'Ivoire, and Libya, we suggest that states' cooperation
   with the ICC is a function of domestic political calculations, tempered
   by states' international partners and ambitions and the ICC's own
   learning process.},
