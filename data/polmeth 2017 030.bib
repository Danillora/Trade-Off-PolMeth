@incollection{ WOS:000417799200006,
Author = {Cutler, A. Claire},
Editor = {Cutler, AC and Dietz, T},
Title = {Private transnational governance in global value chains Contract as a
   neglected dimension},
Booktitle = {POLITICS OF PRIVATE TRANSNATIONAL GOVERNANCE BY CONTRACT},
Series = {Politics of Transnational Law},
Year = {2017},
Pages = {79-96},
Abstract = {Governance through value chain contracting is an excellent example of
   private transnational governance by contract because value chains span
   national borders and link local and global producers, suppliers, and
   consumers of both a public and private nature. This chapter addresses
   the neglect of the contractual dimension in studies of value chain
   governance. It develops a critical political economy approach to
   understanding the power relations embedded in the contracts that
   regulate `who gets what' in global value chains. The analysis suggests
   that governance through global value chain contracting raises concerns
   about the legitimacy of this significant mechanism of governance in the
   global political economy.},
