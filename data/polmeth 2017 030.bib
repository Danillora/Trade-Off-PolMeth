@incollection{ WOS:000417799200008,
Author = {Dietz, Thomas},
Editor = {Cutler, AC and Dietz, T},
Title = {Relational contracts 2.0 Efficiency and power},
Booktitle = {POLITICS OF PRIVATE TRANSNATIONAL GOVERNANCE BY CONTRACT},
Series = {Politics of Transnational Law},
Year = {2017},
Pages = {115-129},
Abstract = {This article proposes that the rise of the internet and other
   information and communication technologies (ICTs) has facilitated the
   evolution of a new, virtual form of relational contracting. ICT enabled
   contractual governance mechanisms to reduce information asymmetries
   between transaction partners and facilitate the evolution of almost
   unrestricted virtual reputational networks. In the absence of a workable
   global legal order these privately generated governance mechanisms
   reduce transaction costs and facilitate the emergence of beneficial
   cross-border exchange relations. However, the rise of informal virtual
   governance mechanisms also has downsides. Formal contract laws that are
   embedded in a constitutional public order are increasingly replaced by
   informal mechanisms like ostracism or the threat of ending a business
   relation. This chapter discusses the extent to which such unregulated
   private power relations give power to stronger parties to control and
   exploit their weaker business partners.},
