@incollection{ WOS:000405353800011,
Author = {Beney, Terence},
Editor = {Podems, D},
Title = {PAYING FOR TROUBLEMAKING Strengthening Democracy by Institutionalizing
   Multiple Centers of Evaluation},
Booktitle = {DEMOCRATIC EVALUATION AND DEMOCRACY: EXPLORING THE REALITY},
Series = {Evaluation and Society},
Year = {2017},
Pages = {191-208},
Abstract = {This chapter explores the importance of having multiple centers of
   independent evaluation for strengthening South Africa's democracy, and
   considers how parliament, Institutions Supporting Democracy, and civil
   society represent the potential for such centers. The argument asserts
   that while embedding evaluation in government is necessary for
   normalizing evaluation as a governance practice, this development in
   itself does not constitute a strengthening of democracy. Evaluation
   strengthens democracy when it extends people's agency to hold the
   executive to account, not just for its policies, programs, and service
   delivery, but also for the version of democracy being established
   through them. For evaluation to be practiced to these degrees of freedom
   it must be woven into the fabric of institutionalized democracy, and
   prove valuable for civic action. The chapter goes on to describe how the
   potential of evaluation for strengthening democracy has not yet been
   realized because of lingering institutional inadequacies, and it's as
   yet nascent contribution to strengthening the hand of protest and direct
   action.},
