@article{ WOS:000416537800006,
Author = {Oatley, Thomas},
Title = {FORUM: OPEN ECONOMY REFLECTIONS: SYSTEMIC THEORY AND POLICY RELEVANCE
   Open economy politics and trade policy},
Journal = {REVIEW OF INTERNATIONAL POLITICAL ECONOMY},
Year = {2017},
Volume = {24},
Number = {4},
Pages = {699-717},
Abstract = {Has 25 years of accumulated empirical research informed by the open
   economy politics (OEP) perspective deepened our understanding of how
   politics influence trade policy? This paper offers an answer to this
   question based on a survey of empirical research on individual trade
   policy preferences, the impact of domestic political institutions on
   trade policy outcomes, and the relationship between international
   institutions and trade policy outcomes. I reach a rather disappointing
   conclusion: although every contribution to the OEP research program is
   rigorous and robust, the individual findings have not produced consensus
   on any of the major questions at the center of research. Consequently,
   we know no more about the politics of trade policy today than we knew
   three decades ago. This lack of progress in OEP's variable-based
   approach creates opportunities for research that relies on a
   pattern-based approach informed by the logic of causal complexity.},
