@incollection{ WOS:000411608900003,
Author = {Campos, Jr., Eddie},
Book-Author = {Cakirtas, O},
Title = {``Power Is Only a Word{''}: Language, Control, and the Orwellian
   Philosophy of Nineteen Eighty-Four},
Booktitle = {IDEOLOGICAL MESSAGING AND THE ROLE OF POLITICAL LITERATURE},
Series = {Advances in Public Policy and Administration},
Year = {2017},
Pages = {25-42},
Abstract = {This chapter aims to define and examine the role of language and
   political power as they appear in two of George Orwell's most
   influential works: Nineteen Eighty-Four and the essay ``Politics and the
   English Language.{''} These complimentary, though at times seemingly
   contradictory, works build a philosophy of language which sees the
   spoken and written word used as weapons for an authoritarian regime. By
   comparing Orwell's essay and novel, as well as explaining their
   connection to the Whorfian theory of Linguistic Relativity, the author
   hopes to track the development of Nineteen Eighty-Four's Newspeak as the
   ultimate language of control in a fictionalized world which seemed all
   too possible to its author.},
