@incollection{ WOS:000405353800010,
Author = {Troskie, Dirk and Kelly, Kevin and Mandondo, Shelton Kaba},
Editor = {Podems, D},
Title = {STRENGTHENING GOVERNMENT THROUGH EVALUATION The Evaluation Journey of a
   Provincial Agriculture Department},
Booktitle = {DEMOCRATIC EVALUATION AND DEMOCRACY: EXPLORING THE REALITY},
Series = {Evaluation and Society},
Year = {2017},
Pages = {175-189},
Abstract = {This chapter explores the evaluation experiences of a provincial
   government department in South Africa, the Western Cape Department of
   Agriculture. The Department has 17 evaluations completed or ongoing over
   a 3-year period. The authors describe the influence of the national
   level government in providing a policy framework and guidelines for
   institutionalizing evaluation, aimed at ensuring evaluation efficiency
   and use, with the aim of strengthening democratic governance and
   accountability, and improving service delivery. The chapter provides
   insight into the processes involved in successfully instituting the
   program and creating conditions that needed to be set in place for its
   success. The authors then discuss the Department's challenges and
   achievements in implementing the program, including: accommodating and
   developing capacity for evaluation alongside other professional duties;
   identifying the need for evaluations; implementing and managing
   evaluations; engaging with stakeholders; and sharing and using
   evaluation findings.},
