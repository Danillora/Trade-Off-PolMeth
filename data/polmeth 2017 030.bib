@article{ WOS:000418514600012,
Author = {Pearlman, Adam R.},
Title = {Collection `management by crisis': strategic targeting and interrogation
   at Guantanamo Bay},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2017},
Volume = {32},
Number = {6},
Pages = {849-866},
Abstract = {This article examines human intelligence collection in wartime, and
   offers a methodology to help determine the relative success or failure
   of the detainee interrogation mission at Guantanamo Bay, Cuba (GTMO). It
   surveys the relevant background on GTMO, a brief history on
   interrogations during World War II and Vietnam, and draws comparisons
   among them to be considered in potential future mass-interrogation
   missions. Drawing from documentary research and two dozen original
   interviews, it argues that, to the extent an intelligence mission was
   one of the purposes of transferring detainees to GTMO, the relevant
   metric of success is whether collectors were able to obtain more or
   `better' intelligence from the detainees than they would have been able
   to in-theater. At the strategic level, then, GTMO's value to
   intelligence collectors is to be assessed at the margins, rather than
   the absolute value of the information educed.},
