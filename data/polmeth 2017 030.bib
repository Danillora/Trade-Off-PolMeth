@article{ WOS:000399342900006,
Author = {Jessop, Bob and Sum, Ngai-Ling},
Title = {Putting the ``Amsterdam School' in its Rightful Place: A Reply to Juan
   Ignacio Staricco's Critique of Cultural Political Economy},
Journal = {NEW POLITICAL ECONOMY},
