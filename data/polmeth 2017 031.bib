@article{ WOS:000418514600002,
Author = {Karam, Jeffrey G.},
Title = {Missing revolution: the American intelligence failure in Iraq, 1958},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2017},
Volume = {32},
Number = {6},
Pages = {693-709},
Abstract = {Why were American officials caught by surprise with the military coup
   and later revolution in Iraq on 14 July 1958? Drawing on American
   intelligence and diplomatic records as well as multilingual sources,
   this article argues that the US intelligence failure is the product of
   two factors: the collection of information from too few and too similar
   human sources of intelligence in Iraq's ruling regime, and the
   unreceptivity of US officials to assessing new information and their
   unwillingness to update assessments of local Iraqi developments. It
   revisits America's intelligence failure in Iraq and suggests important
   lessons for the study of intelligence.},
