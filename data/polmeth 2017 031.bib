@article{ WOS:000416537800005,
Author = {Chaudoin, Stephen and Milner, Helen V.},
Title = {FORUM: OPEN ECONOMY REFLECTIONS: SYSTEMIC THEORY AND POLICY RELEVANCE
   Science and the system: IPE and international monetary politics},
Journal = {REVIEW OF INTERNATIONAL POLITICAL ECONOMY},
Year = {2017},
Volume = {24},
Number = {4},
Pages = {681-698},
Abstract = {A recent RIPE article by Jerry Cohen argues that current research on the
   political economy of money has stagnated because of its overemphasis on
   the scientific method and domestic variables. We argue that a wide array
   of scientifically oriented research on the IPE of money considers the
   system in many different ways. To help build a dialogue, we categorize
   each of these conceptions of `the system' and give examples of their
   application from recent research on the IPE of money. Our hope is that
   this typology will help scholars of different approaches recognize the
   similarities and differences in their research, beyond simply whether
   the research is scientific or heterodox.},
