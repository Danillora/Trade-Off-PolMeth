@article{ WOS:000415388800016,
Author = {Chapoulie, Jean-Michel},
Title = {Schooling in France: From Organizational to Informal Inequality},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
