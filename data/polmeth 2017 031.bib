@article{ WOS:000400245500002,
Author = {Abbott, Kenneth W. and Levi-faur, David and Snidal, Duncan},
Title = {Theorizing Regulatory Intermediaries: The RIT Model},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2017},
Volume = {670},
Number = {1},
Pages = {14-35},
Month = {MAR},
Abstract = {Regulation is typically conceived as a two-party relationship between a
   rule-maker or regulator (R) and a rule-taker or target (T). We set out
   an agenda for the study of regulation as a three- (or more) party
   relationship, with intermediaries (I) at the center of the analysis.
   Intermediaries play major and varied roles in regulation, from providing
   expertise and feedback to facilitating implementation, from monitoring
   the behavior of regulatory targets to building communities of assurance
   and trust. After developing the basic regulator-intermediary-target
   (RIT) model, we discuss important extensions and variations of the
   model. We then discuss the varieties of regulatory capture that may
   appear where intermediaries are involved.},
