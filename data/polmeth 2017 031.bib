@incollection{ WOS:000417799200005,
Author = {Fuchs, Doris and Hennings, Anne},
Editor = {Cutler, AC and Dietz, T},
Title = {Governance by contract from a perspective of power The case of land
   grabbing},
Booktitle = {POLITICS OF PRIVATE TRANSNATIONAL GOVERNANCE BY CONTRACT},
Series = {Politics of Transnational Law},
Year = {2017},
Pages = {57-75},
Abstract = {Drawing attention to the role of power, this chapter ponders how the
   move to governance by contract advantages certain interests vis-a-vis
   others, arguing that it affects the future distribution of power and
   resources. Focusing on the phenomenon of land grabbing, we use a power
   theoretical framework that differentiates between material and
   ideational as well as actor-specific and structural sources of power.
   Our findings suggest that governance by contract, which plays an
   increasing role in the context of land investments, highly impacts
   domestic and global power relations and undermines social equity.},
