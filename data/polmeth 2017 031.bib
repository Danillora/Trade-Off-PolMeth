   Boettner, Bethany and Smith, Anna L. and Haynie, Dana},
Title = {Understanding Racial Differences in Exposure to Violent Areas:
   Integrating Survey, Smartphone, and Administrative Data Resources},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
