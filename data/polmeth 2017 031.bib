@article{ WOS:000401370900004,
Author = {De Juan, Alexander and Krautwald, Fabian and Pierskalla, Jan Henryk},
Title = {Constructing the State: Macro Strategies, Micro Incentives, and the
   Creation of Police Forces in Colonial Namibia},
Journal = {POLITICS \& SOCIETY},
Year = {2017},
Volume = {45},
Number = {2, SI},
Pages = {269-299},
Month = {JUN},
Abstract = {How do states build a security apparatus after violent resistance
   against state rule? This article argues that in early periods of state
   building two main factors shape the process: the macro-strategic goals
   of the state and administrative challenges of personnel management.
   These dynamics are studied in the context of the establishment of police
   forces in the settler colony of German Southwest Africa, present-day
   Namibia. The empirical analysis relies on information about the location
   of police stations and a near full census of police forces, compiled
   from the German Federal Archives. A mismatch is found between the
   allocation of police presence and the allocation of police personnel.
   The first was driven by the strategic value of locations in terms of
   extractive potential, political importance, and the presence of critical
   infrastructure, whereas the allocation of individual officers was likely
   affected by adverse selection, which led to the assignment of
   low-quality recruits to strategically important locations.},
