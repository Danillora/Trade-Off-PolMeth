@article{ WOS:000425464100008,
Author = {Shonge, Maureen Sibanda},
Title = {Spinning the Web: Grassroots Human Rights Work Inspired by Peacebuilding
   Approaches},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2017},
Volume = {9},
Number = {3, SI},
Pages = {447-456},
Month = {NOV},
Abstract = {This practice note reflects on two years of conversations in Zimbabwe
   exploring the violent past(s) of various communities and how they
   prevented recurring violence. The paper primarily describes the
   collaborative efforts of a group of human rights lawyers in Zimbabwe who
   partnered with conflict transformation practitioners in South Africa. It
   also sheds light on how exposure to peacebuilding methodologies can
   strengthen human rights practices to support social change. The original
   intention of the project was that, once educated about human rights,
   community members could claim these rights using the justice system. But
   these conventional methods hinged on providing information without
   meaningfully engaging with the villagers. The lawyers shifted their
   approach and collaborated with peace practitioners to form a network of
   organizations that infused both human rights and peacebuilding
   methodologies to address causes of conflict and foster peaceful conflict
   resolution. Though they soon had more success with the communities,
   these lawyers faced scepticism from their own organizations. Only when
   the process enabled human rights practitioners to put issues on the
   political agenda in a way that was not previously possible did they
   elicit a broader interest. The peacebuilding language and methodology,
   however, was seen as more acceptable and less threatening. Members of
   Parliament (MPs) agreed to participate in so-called `fireside chats'
   with local civil society organizations (CSOs) and committed to making
   changes. As a result, this dialogical approach brought local community
   concerns on political violence forward in parliamentary committee
   debates and meetings.},
