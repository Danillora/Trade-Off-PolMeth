@incollection{ WOS:000405353800005,
Author = {Cloete, Fanie},
Editor = {Podems, D},
Title = {EVALUATION AND DEMOCRATIC GOVERNANCE The Public Management Perspective},
Booktitle = {DEMOCRATIC EVALUATION AND DEMOCRACY: EXPLORING THE REALITY},
Series = {Evaluation and Society},
Year = {2017},
Pages = {81-104},
Abstract = {This chapter assesses the link between democracy and program evaluation
   from a public management perspective. It commences with a succinct
   summary of the nature of government and governance and the origins and
   nature of democracy. It then explains the relevance of systematic
   evaluation of governmental programs for the consolidation of democracy
   from an evidence-informed perspective on public policymaking. It is
   concluded that evaluation is an integral higher order public management
   function that needs to be regularly undertaken in order to ensure the
   optimal efficiency and effectiveness of democratic policymaking. The
   democratic nature of the South African state is then summarized and the
   role of the Government Wide Monitoring and Evaluation System in
   supporting the consolidation of democracy in South Africa assessed. The
   final conclusion is that systematic program evaluation is increasingly
   seen as a crucial, intrinsic element of good democratic management and
   governance in 21st century governments.},
