@article{ WOS:000417371600001,
Author = {Abraham, Kavi Joseph},
Title = {Making Machines: Unlikely Resonances between Realist and Postcolonial
   Thought},
Journal = {INTERNATIONAL POLITICAL SOCIOLOGY},
