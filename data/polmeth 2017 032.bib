@article{ WOS:000400865400007,
Author = {Findley, Michael G. and Milner, Helen V. and Nielson, Daniel L.},
Title = {The choice among aid donors: The effects of multilateral vs. bilateral
   aid on recipient behavioral support},
Journal = {REVIEW OF INTERNATIONAL ORGANIZATIONS},
Year = {2017},
Volume = {12},
Number = {2, SI},
Pages = {307-334},
Month = {JUN},
Abstract = {Scholars studying foreign assistance differ over whether multilateral
   aid is preferable to bilateral aid for promoting development, but nearly
   all build their cases primarily on highly aggregated cross-national
   time-series data. We investigate this topic experimentally from the
   perspective of those whom the foreign aid directly affects: recipient
   citizens and elites. We thus report results of a survey experiment with
   behavioral outcomes on more than 3000 Ugandan citizens and over 300
   members of Uganda's Parliament. In spite of a large literature
   suggesting differences, the findings generally reveal few substantive
   differences in citizens' and elites' preferences and behavior toward the
   two types of aid. While no strong pattern of differences emerges,
   limited evidence suggests that the public evinces greater trust in
   multilateral institutions, and both masses and elites feel that
   multilateral aid is more transparent. Overall, these null results inform
   an ever-expanding literature, which is increasingly articulating
   distinctions between multilateral and bilateral aid. At least in the
   minds of the recipients, however, multilateral and bilateral aid may not
   in fact be all that different. This accords with the literature noting
   the strong overlap in aid organizations and bemoaning the fact that they
   do not specialize more. Our results raise the question about why have
   both multilateral and bilateral aid donors if they in effect do the same
   thing.},
