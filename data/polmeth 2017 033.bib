@article{ WOS:000423918300005,
Author = {Kagan, Michael},
Title = {(Avoiding) The End of Refugee Status Determination},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2017},
Volume = {9},
Number = {2},
Pages = {197-202},
Month = {JUL},
Abstract = {Increasing backlogs are preventing individual refugee status
   determination systems from functioning, even in some of the most well
   developed asylum systems. This means that asylum seekers must spend the
   first, most vulnerable, years in their countries of refuge without
   having been individually adjudicated to meet the international refugee
   definition. Decline in individual refugee status determination hastens
   an overall decline in refugee rights. It is essential to find means of
   maintaining viable individual refugee status determination systems, even
   as large refugee movements seem to make this impractical.},
