@article{ WOS:000393782400009,
Author = {Karolewski, Ireneusz Pawel and Cross, Mai'a K. Davis},
Title = {The EU's Power in the Russia-Ukraine Crisis: Enabled or Constrained?},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2017},
Volume = {55},
Number = {1, SI},
Pages = {137-152},
Month = {JAN},
Abstract = {The article presents conclusions of this Special Issue. It first
   summarizes the empirical findings of the individual contributions to
   this Special Issue in terms of whether EU power was enabled or
   constrained in response to the crisis. Second, the article synthesizes
   these findings to advance some overarching arguments about the EU as an
   actor in this crisis, and the types of power that were evident in this
   case. Third, it examines the significance of the study overall, and
   highlights some key policy implications. Finally, the article takes a
   broader perspective, and suggests that this framework is valuable more
   generally for understanding how various crises might impact the EU as a
   foreign policy actor.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
