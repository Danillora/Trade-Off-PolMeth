@incollection{ WOS:000417799200007,
Author = {LeBaron, Genevieve and Lister, Jane and Dauvergne, Peter},
Editor = {Cutler, AC and Dietz, T},
Title = {The new gatekeeper Ethical audits as a mechanism of global value chain
   governance},
Booktitle = {POLITICS OF PRIVATE TRANSNATIONAL GOVERNANCE BY CONTRACT},
Series = {POLITICS OF TRANSNATIONAL LAW},
Year = {2017},
Pages = {97-114},
Abstract = {The private `ethical audit' regime occupies significant, yet
   under-investigated power within global value chain (GVC) governance. We
   argue that while governments and civil society groups accept audits as a
   useful and legitimate extra-contractual tool of GVC governance, audits
   are not politically neutral. Rather, transnational corporations and
   audit firms have vested interests in the formal rules, norms, and
   expectations of the audit regime, and leverage strategic power over
   audit design, implementation, and enforcement. We conclude that the
   audit regime as a global governance mechanism disproportionately
   benefits corporations. Fundamentally, it guards the status quo of global
   production by deflecting attention away from challenging the underlying
   contractual conditions that generate and perpetuate the most egregious
   GVC environmental and social abuses.},
Publisher = {ROUTLEDGE},
