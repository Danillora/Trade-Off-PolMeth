@article{ WOS:000399561300007,
Author = {Kodila-Tedika, Oasis and Asongu, Simplice A.},
Title = {Women in power and power of women: the Liberian experience},
Journal = {INTERNATIONAL FEMINIST JOURNAL OF POLITICS},
Year = {2017},
Volume = {19},
Number = {1},
Pages = {86-101},
Month = {MAR},
Note = {4th Annual International Feminist Journal of Politics (IFJP) Conference,
   Univ Queensland, Brisbane, AUSTRALIA, JUN 18-19, 2015},
Abstract = {Do women really improve conditions for gender equality after becoming
   heads of states? This study investigates if having a woman at the helm
   of country's decision making processes leads to better indicators on
   women's conditions. Using time-series observations for the period
   2000-11, we test the hypothesis with the Liberian experience. We analyse
   six main gender indicators: gender equality, equality of representation
   in rural areas (basic community), economic rights of women,
   participation of women in active life, political rights of women and
   legislations for the protection of women against violence. Our findings
   do not show substantially significant changes between the first mandate
   of Ellen Johnson Sirleaf and the period before. In essence, we only
   notice positive and statistical ruptures for equality of representation
   in rural areas and the economic rights of women. Policy implications are
   discussed.},
