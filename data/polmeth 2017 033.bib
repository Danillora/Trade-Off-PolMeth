@article{ WOS:000417765900004,
Author = {Toplisek, Alen and Thomassen, Lasse},
Title = {From Protest to Party: Horizontality and Verticality on the Slovenian
   Left},
Journal = {EUROPE-ASIA STUDIES},
Year = {2017},
Volume = {69},
Number = {9},
Pages = {1383-1400},
Abstract = {This essay analyses the politics of horizontalitya key characteristic of
   recent forms of protest and activist citizenshipthrough the case of the
   2012-2013 protests in Slovenia. The Slovenian case is illustrative
   because we can trace the emergence of the Initiative for Democratic
   Socialism and, subsequently, the United Left from protest through
   movement to party. Since we believe that horizontality and verticality
   are present in both movements and parties, we argue against a simple
   opposition between movements and parties. In particular, we focus on the
   reasons for the move from horizontalist ways of political organising to
   vertical structures.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
