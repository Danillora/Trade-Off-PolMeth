@incollection{ WOS:000405353800007,
Author = {Amisi, Matodzi and Vawda, Ahmed},
Editor = {Podems, D},
Title = {STRENGTHENING DEMOCRATIC GOVERNANCE IN THE BUILDING OF INTEGRATED HUMAN
   SETTLEMENTS THROUGH EVALUATIONS},
Booktitle = {DEMOCRATIC EVALUATION AND DEMOCRACY: EXPLORING THE REALITY},
Series = {Evaluation and Society},
Year = {2017},
Pages = {127-138},
Abstract = {This chapter argues that in countries transitioning to democracy the
   embedding of democratic principles takes time. A key factor in
   consolidating democracy is the instrumental role played by a capable
   state administration applying the instruments of development to
   effectively redistribute wealth, meet basic needs, protect human rights
   and exercise democratic forms of governance. In this context evaluation
   contributes to strengthening democracy by developing a reflective and
   learning state. An illustrative case is presented: the government led
   evaluation of the Urban Settlements Development Grant (USDG). The
   authors describe the government's evaluation process and show how it
   promoted constructive dialogue across spheres of government, ultimately
   leading to a redesign of the program and a reform in human settlements
   policy. Both the program overhaul and the policy reforms enhance the
   rights-based delivery of services to poor communities, increase their
   access to economic opportunities, and in so doing, contribute to
   consolidating democracy beyond the introduction of universal suffrage.},
