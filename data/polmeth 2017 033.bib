   Reinsberg, Bernhard, Univ Zurich, Zurich, Switzerland.
   Reinsberg, Bernhard, Univ Cambridge, Cambridge, England.},
DOI = {10.1007/s11558-017-9268-1},
ISSN = {1559-7431},
EISSN = {1559-744X},
Keywords = {Trust funds; Multilateral agencies; Bureaucratic politics; Collective
   agents},
