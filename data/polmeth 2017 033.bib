@article{ WOS:000402863400006,
Author = {Gabor, Daniela and Brooks, Sally},
Title = {The digital revolution in financial inclusion: international development
   in the fintech era},
Journal = {NEW POLITICAL ECONOMY},
Year = {2017},
Volume = {22},
Number = {4, SI},
Pages = {423-436},
Abstract = {This paper examines the growing importance of digital-based financial
   inclusion as a form of organising development interventions through
   networks of state institutions, international development organisations,
   philanthropic investment and fintech companies. The
   fintech-philanthropy-development complex generates digital ecosystems
   that map, expand and monetise digital footprints. Its know thy
   (irrational) customer' vision combines behavioural economics with
   predictive algorithms to accelerate access to, and monitor engagement
   with, finance. The digital revolution adds new layers to the material
   cultures of financial(ised) inclusion, offering the state new ways of
   expanding the inclusion of the legible', and global finance new forms of
   profiling' poor households into generators of financial assets.},
