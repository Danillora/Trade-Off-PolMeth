@article{ WOS:000425464100003,
Author = {Simpson, Graeme},
Title = {`From the Normative to the Transformative': Defining and Promoting
   Justice and Human Rights as Part of Violent Conflict Prevention and
   Peacebuilding},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2017},
Volume = {9},
Number = {3, SI},
Pages = {379-400},
Month = {NOV},
Abstract = {The issue of justice and human rights in peacebuilding is often still
   controversial in practice, especially in dealing with criminal justice
   demands in the context of delicate politically negotiated peace
   processes. This article aims to help organizations resolve and optimize
   this creative tension in carving out a distinctive role for
   peacebuilding practitioners, scholars and policy makers within a
   justice-based approach to preventing violent conflict. The article seeks
   to support the development of a collaborative space at the intersection
   between the human rights and peacebuilding fields, challenges narrow,
   normative definitions of criminal justice in favour of a transformative
   approach to human rights, and endeavours to offer peacebuilders and
   human rights actors an alternative framework for `justice as
   prevention', rather than a retroactive approach in conflict-affected and
   post-conflict societies. The article offers a three-pronged critique of
   a `peace and justice dichotomy', including by reference to the lived
   experiences of injustice in the most affected communities, and offers an
   alternative way of thinking about a mutually beneficial `peacebuilding
   and justice continuum'. Thereafter, the article scans some of the
   thematic trends within the justice and human rights sector, as well as
   the gaps and opportunities that these present. It concludes by offering
   some concrete programming and policy approaches, and argues for
   addressing the causes rather than just the symptoms of violent conflict
   as part of a prevention-based approach, and to shift perspective-from a
   set of normative parameters to justice and human rights as a
   transformative influence in the processes of conflict transformation and
   peacebuilding.},
