@article{ WOS:000425464100005,
Author = {Sriram, Chandra Lekha},
Title = {Peace, Governance and Rule of Law, and Transitional Justice:
   Interrogating the Connections},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2017},
Volume = {9},
Number = {3, SI},
Pages = {424-430},
Month = {NOV},
Abstract = {In conflict-affected countries, significant political, peacekeeping and
   development resources are devoted to preventing future conflict,
   frequently through the use of transitional justice and rule of law
   promotion, often linked to promoting peace agreements with specific
   governance measures. However, despite the importance of the goals and
   the scale of resources committed, the evidence that such measures
   promote peace is mixed. Further, the way in which these measures
   interact, positively and negatively, has not been sufficiently analysed
   by academics or practitioners. Drawing on the author's years of
   fieldwork in Sierra Leone, Uganda, Colombia, Kenya, Sri Lanka and Sudan,
   among others, this policy note elaborates on the challenges of using law
   for peace through governance and rule of law measures, as well as
   through transitional justice.},
