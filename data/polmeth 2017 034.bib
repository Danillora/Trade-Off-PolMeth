@incollection{ WOS:000419731500008,
Author = {Pillow, Wanda S.},
Editor = {Parker, S and Gulson, KN and Gale, T},
Title = {Imagining Policy {[}Data] Differently},
Booktitle = {POLICY AND INEQUALITY IN EDUCATION},
Series = {Education Policy \& Social Inequality},
Year = {2017},
Volume = {1},
Pages = {133-151},
Abstract = {This chapter articulates a call to attend to how policy imagines, or
   cannot imagine, futures. Utilizing examples from a policy table focused
   on increasing Black young parents education success, Pillow demonstrates
   the necessity to trace how data matter in policy formation. Situating
   this work within a context of education debt and data deficits, Pillow
   takes seriously Webb and Gulson's petition to reinvigorate policy
   studies in ways that account for embodied materialities. Troubled by the
   incapacity to think Black youth futures outside of temporal and
   structural constraints, Pillow focuses on questions of how policy
   creates feelings about young parents and turns to Afrofuturism
   theorizing to identify and rethink policy construction limitations. The
   chapter concludes with consideration of what it might look like to
   imagine policy data and futures differently and the need for continued
   emphasis in this area.},
Publisher = {SPRINGER-VERLAG SINGAPORE PTE LTD},
