@article{ WOS:000397900100006,
Author = {Gros, Valentin and de Goede, Marieke and Isleyen, Beste},
Title = {The Snowden Files Made Public: A Material Politics of Contesting
   Surveillance},
Journal = {INTERNATIONAL POLITICAL SOCIOLOGY},
Year = {2017},
Volume = {11},
Number = {1},
Pages = {73-89},
Month = {MAR},
Abstract = {In the wake of the disclosures by Edward Snowden about NSA surveillance
   practices, a series of public hearings was held before the Civil
   Liberties, Justice, and Home Affairs (LIBE) Committee of the European
   Parliament in 2013-2014. These hearings offer a wealth of information
   concerning the details of Snowden's claims, their implications for
   privacy rights, and the way in which the transatlantic political
   dialogue on these issues is unfolding. However, they have yet to receive
   academic attention. This article suggests that the LIBE Hearings were an
   important platform that rendered the contested Snowden files into public
   evidence of contemporary surveillance practices. Drawing on the concept
   of ``material publics{''} proposed by Noortje Marres and others, we
   examine how the material setting of LIBE was crucial to the ways in
   which the Snowden files were made public in Europe. Valid evidence was
   produced, legal issues were identified, technological solutions were
   fostered, and responsibilities were enacted and denied.},
