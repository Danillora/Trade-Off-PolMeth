@incollection{ WOS:000405353800012,
Author = {Fraser, Dugan and Rogers, Patricia J.},
Editor = {Podems, D},
Title = {IS GOVERNMENT'S APPROACH TO EVALUATION DEEPENING DEMOCRACY IN SOUTH
   AFRICA?},
Booktitle = {DEMOCRATIC EVALUATION AND DEMOCRACY: EXPLORING THE REALITY},
Series = {Evaluation and Society},
Year = {2017},
Pages = {209-224},
Abstract = {Evaluations need to do more than produce reports if they are to go
   beyond just generating knowledge and start to help South Africans think
   evaluatively and reason effectively. The processes through which
   evaluations are undertaken can generate some of the most enduring
   effects, potentially felt long after findings and recommendations are no
   longer pertinent. South Africa is a violent and divided society and
   evaluative practice that is inclusive, deliberative, and characterized
   by dialogue has much to offer in helping to address these problems and
   deepen democracy. Better dissemination of evaluation results would help
   make Government evaluations more transparent, while the participation of
   more non-state actors in processes would also make government
   evaluations more inclusive. The national evaluation policy should
   provide its implementers with better guidance on how to undertake
   dialogues and deliberative processes. Overall, while good progress has
   been made in the implementation of the policy, it is due for a review.
   Certain revisions such as those proposed here are needed in order to
   enhance its contribution to democracy in South Africa.},
