@article{ WOS:000418515500010,
Author = {Lowenthal, Mark M.},
Title = {My take on teaching intelligence: why, what, and how},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2017},
Volume = {32},
Number = {7, SI},
Pages = {986-994},
Abstract = {This article is a summation of the emphases I place in my teaching about
   intelligence, both in academic settings and in courses presented to
   government and commercial clients. The main goal is to demystify and
   deromanticize intelligence, to present it as a normal function of
   government and one that has moral and ethical standards and can exist
   compatibly within a democratic government. The article also discusses
   suggested course readings, some successful term paper topics and the
   concept of graduate intelligence courses as a type of professional
   training. The article also notes some themes emphasized in courses on
   training new analysts in their required skill sets.},
