@article{ WOS:000417765900002,
Author = {Milan, Chiara},
Title = {Reshaping Citizenship through Collective Action: Performative and
   Prefigurative Practices in the 2013-2014 Cycle of Contention in Bosnia
   \& Hercegovina},
Journal = {EUROPE-ASIA STUDIES},
Year = {2017},
Volume = {69},
Number = {9},
Pages = {1346-1361},
Abstract = {This essay analyses the strategic practices adopted by social movement
   actors during the 2013 and 2014 mobilisations in Bosnia \& Hercegovina.
   By bridging critical citizenship studies with literature on social
   movements, it classifies them as belonging to the realm of activist
   citizenship, but also as having a performative and prefigurative
   dimension. While the strategies adopted during the 2013 wave had a
   performative dimension, as they disrupted routines and created
   opportunities for social change, the 2014 practices are to be considered
   prefigurative, as they developed modes of interaction embodying a new
   model of citizenship at odds with the existing one based on the
   institutionalisation of ethno-national categories.},
