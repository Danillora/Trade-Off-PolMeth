@incollection{ WOS:000417799200009,
Author = {Gelinas, Fabien},
Editor = {Cutler, AC and Dietz, T},
Title = {Arbitration as transnational governance Legitimacy beyond contract},
Booktitle = {POLITICS OF PRIVATE TRANSNATIONAL GOVERNANCE BY CONTRACT},
Series = {POLITICS OF TRANSNATIONAL LAW},
Year = {2017},
Pages = {133-150},
Abstract = {This chapter provides a unique consideration of the legitimacy of
   international arbitration as a form of transnational governance by
   contract. Following a brief outline of the historical development of the
   practice of international arbitration and the institutional actors,
   public and private, that have prompted and shaped its contemporary
   resurgence, the types of power seen in the practice and regime of
   international arbitration are noted and the potential and limits of
   consent as a source of legitimacy are explored. A detailed argument
   follows positing that the adjudicative function performed by arbitrators
   provides its own, distinct form of rule-of-law legitimation for
   arbitration, which supplements consent-based modes of legitimation.},
