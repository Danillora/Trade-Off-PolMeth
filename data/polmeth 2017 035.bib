@article{ WOS:000396678400001,
Author = {Vollmer, Bastian A.},
Title = {A Hermeneutical Approach to European Bordering},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2017},
Volume = {25},
Number = {1},
Pages = {1-15},
Abstract = {Post-Westphalia processes such as the increasing mobility of people, the
   rise of the international human rights framework, globalization,
   transnationalization and the political integration of Europe have
   `fundamentally changed the meaning of borders' (Guild 2001, Moving the
   Borders of Europe. Inaugural Lecture. Nijmegen: University of Nijmegen,
   3). Borders have a `polysemic nature' (Balibar 2002, Politics and the
   Other Scene. London: Verso, 81), they do not have the same meaning for
   everyone. This article claims, however, there is another, yet new,
   fundamental shift as regards the meaning of border: a
   socio-psychological shift. There is a drastic increase in the public
   profile of borders. This hypothesized shift requires a new approach on
   borders. In part it confirms Balibar's (2002) claim but the article
   considers the implications for research agendas (Politics and the Other
   Scene. London: Verso). The article posits a research agenda that goes
   back to basic questions. It suggests a hermeneutical approach to
   European bordering and border regimes.},
