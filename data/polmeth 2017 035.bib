@article{ WOS:000402615500002,
Author = {Kuru, Ozan and Pasek, Josh and Traugott, Michael W.},
Title = {MOTIVATED REASONING IN THE PERCEIVED CREDIBILITY OF PUBLIC OPINION POLLS},
Journal = {PUBLIC OPINION QUARTERLY},
