@article{ WOS:000398071200007,
Author = {Butler, Daniel M. and Homola, Jonathan},
Title = {An Empirical Justification for the Use of Racially Distinctive Names to
   Signal Race in Experiments},
Journal = {POLITICAL ANALYSIS},
Year = {2017},
Volume = {25},
Number = {1},
Pages = {122-130},
Month = {JAN},
Abstract = {Researchers studying discrimination and bias frequently conduct
   experiments that use racially distinctive names to signal race. The
   ability of these experiments to speak to racial discrimination depends
   on the excludability assumption that subjects' responses to these names
   are driven by their reaction to the individual's putative race and not
   some other factor. We use results from an audit study with a large
   number of aliases and data from detailed public records to empirically
   test the excludability assumption undergirding the use of racially
   distinctive names. The detailed public records allow us to measure the
   signals about socioeconomic status and political resources that each
   name used in the study possibly could send. We then reanalyze the audit
   study to see whether these signals predict legislators' likelihood of
   responding. We find no evidence that politicians respond to this other
   information, thus providing empirical support for the excludability
   assumption.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {EDINBURGH BLDG, SHAFTESBURY RD, CB2 8RU CAMBRIDGE, ENGLAND},
