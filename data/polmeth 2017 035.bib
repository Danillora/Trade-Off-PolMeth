@article{ WOS:000410711800001,
Author = {Nijar, Gurdial Singh and Louafi, Selim and Welch, Eric W.},
Title = {The implementation of the Nagoya ABS Protocol for the research sector:
   experience and challenges},
Journal = {INTERNATIONAL ENVIRONMENTAL AGREEMENTS-POLITICS LAW AND ECONOMICS},
