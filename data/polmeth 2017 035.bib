@incollection{ WOS:000416641900024,
Author = {Bell, Victoria and Pereira, Ana Leonor and Pita, Joao Rui},
Editor = {Valente, IMF},
Title = {Routes of science and health: Antibiotics Innovation in Europe and the
   United States (1940-1960)},
Booktitle = {EUROPA, ATLANTICO E O MUNDO: MOBILIDADES, CRISES, DINAMICAS CULTURAIS -
   PENSAR COM MARIA MANUELA TAVARES RIBEIRO},
Year = {2017},
Pages = {429-448},
Abstract = {In this study the authors present a brief history of the main
   antibiotics discovered between the 1940's and the 1960's. Penicillin is
   referred, by the authors, as the starting point for the discovery and
   development of other antibacterial products. It becomes clear how the
   research in this domain that began in England, during World War II, was
   transferred to the United States of America, involving the transference
   of knowledge and scientists and the screening of natural products to
   obtain new antibiotics. The authors also show how a new phase in
   antibacterial therapy, initiated by penicillin, resulted in profound
   changes in the assessment of infectious diseases and the way they were
   treated. The effect on demographic, social and cultural dynamics and
   mobility of this complex scientific, medical and pharmaceutical
   innovation remain visible today.},
