@article{ WOS:000395106600002,
Author = {Drevon, Jerome},
Title = {The Constrained Institutionalization of Diverging Islamist Strategies:
   The Jihadis, the Muslim Brotherhood, and the Salafis between Two Aborted
   Egyptian Revolutions},
Journal = {MEDITERRANEAN POLITICS},
Year = {2017},
Volume = {22},
Number = {1},
Pages = {16-34},
Month = {MAR},
Abstract = {This research analyses the comparative institutionalization of the
   strategies of three major components of the Egyptian Islamist social
   movement family: the jihadis, the Muslim Brotherhood and the salafis. It
   uses historical institutionalism to amend rational choice paradigms and
   to investigate the constraints and opportunities posed by these actors'
   past trajectories on their subsequent strategic choices. This article
   argues that 1981 and 2011 were two critical junctures that have shaped
   these actors' ideational and organizational construction through
   path-dependent causal mechanisms regulating their mobilization and
   socialization processes. It contends that these mechanisms have shaped
   these groups' evolution and mediated the institutionalization of their
   strategies.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
