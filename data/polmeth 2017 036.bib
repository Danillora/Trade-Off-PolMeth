@article{ WOS:000399315200004,
Author = {Dieckhoff, Alain},
Title = {The Jewish diaspora and Israel: belonging at distance?},
Journal = {NATIONS AND NATIONALISM},
Year = {2017},
Volume = {23},
Number = {2},
Pages = {271-288},
Month = {APR},
Abstract = {The involvement of diasporas in the advent of modern nationalism is not
   a new phenomenon: already in the 19th century some diasporas wanted to
   normalise' their national existence by building a state of their own.
   However, with the growing globalisation trend in the 199'0s , especially
   in the areas of transportation and communication, Benedict Anderson put
   forward the idea of long-distance nationalism (LDN), as a new way of
   linking diasporas and the national project and thus creating a more
   intense sense of belonging. LDN has been characterised by him as having
   two main features: its unaccountability which allows for intense
   political radicalism, and its instrumental function for strengthening
   ethnic identity in the diaspora and thus a sense of belonging. I will
   test those hypotheses in the case of the archetypal Jewish diaspora.},
