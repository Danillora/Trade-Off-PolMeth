@article{ WOS:000399366200002,
Author = {Roca, Beltran and Diaz-Parra, Iban},
Title = {Blurring the borders between old and new social movements: the M15
   movement and the radical unions in Spain},
Journal = {MEDITERRANEAN POLITICS},
Year = {2017},
Volume = {22},
Number = {2},
Pages = {218-237},
Month = {JUN},
Abstract = {This article analyses the relationships between the M15 movement and
   radical labour organizations in Spain. Based on semi-structured
   interviews and document analysis, it concludes that to the extent that
   the economic crisis has evolved, the relations between the M15 and the
   trade unions have moved from mistrust to convergence. This is especially
   evident in the case of radical trade unionism with which the M15 shares
   several features. Although the M15 has been studied as an example of a
   new social movement', recent changes suggest certain shifts in relation
   to the type of activated subject and to the motivating factors for
   collective action. One of the consequences of this is the closeness to
   the institutions of the workers' movement, which blurs the borders
   between old and new social movements.},
