@article{ WOS:000398071200001,
Author = {Ratkovic, Marc and Tingley, Dustin},
Title = {Sparse Estimation and Uncertainty with Application to Subgroup Analysis},
Journal = {POLITICAL ANALYSIS},
Year = {2017},
Volume = {25},
Number = {1},
Pages = {1-40},
Month = {JAN},
Abstract = {We introduce a Bayesian method, LASSOplus, that unifies recent
   contributions in the sparse modeling literatures, while substantially
   extending pre-existing estimators in terms of both performance and
   flexibility. Unlike existing Bayesian variable selection methods,
   LASSOplus both selects and estimates effects while returning estimated
   confidence intervals for discovered effects. Furthermore, we show how
   LASSOplus easily extends to modeling repeated observations and permits a
   simple Bonferroni correction to control coverage on confidence intervals
   among discovered effects. We situate LASSOplus in the literature on how
   to estimate subgroup effects, a topic that offen leads to a
   proliferation of estimation parameters. We also offer a simple
   preprocessing step that draws on recent theoretical work to estimate
   higher-order effects that can be interpreted independently of their
   lower-order terms. A simulation study illustrates the method's
   performance relative to several existing variable selection methods. In
   addition, we apply LASSOplus to an existing study on public support for
   climate treaties to illustrate the method's ability to discover
   substantive and relevant effects. Soffware implementing the method is
   publicly available in the R package sparsereg.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {EDINBURGH BLDG, SHAFTESBURY RD, CB2 8RU CAMBRIDGE, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Ratkovic, M (Corresponding Author), Princeton Univ, Dept Polit, Princeton, NJ 08544 USA.
   Ratkovic, Marc, Princeton Univ, Dept Polit, Princeton, NJ 08544 USA.
   Tingley, Dustin, Harvard Univ, Govt, Cambridge, MA 02138 USA.},
DOI = {10.1017/pan.2016.14},
