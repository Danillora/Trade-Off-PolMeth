@article{ WOS:000399315200009,
Author = {Trittler, Sabine},
Title = {Repertoires of national boundaries in France and Germanywithin-country
   cleavages and their political consequences},
Journal = {NATIONS AND NATIONALISM},
Year = {2017},
Volume = {23},
Number = {2},
Pages = {367-394},
Month = {APR},
Abstract = {This article analyses how members of the majority population in France
   and Germany define membership in the nation and how they relate to the
   various civic, cultural, or ethnic visions of national belonging
   available in the cultural repertoires of historical models,
   institutional arrangements, and elite discourses. To scrutinize
   within-country differences in the configurations of the symbolic
   boundaries of national belonging, this article applies cluster analysis
   techniques for each country separately using data from the International
   Social Survey Program (ISSP). Overall, the results suggest that people
   choose and arrange different criteria from cultural repertoires,
   resulting in various configurations of national boundaries. Furthermore,
   the number and types of symbolic boundaries used are decisive for
   explaining restrictive and hostile attitudes towards immigrants.
   Contrary to the civic and ethnic historical models, the national
   boundary configurations display very similar patterns across the two
   countries, especially attesting to the considerable process of
   liberalization of citizenship regulations in Germany.},
