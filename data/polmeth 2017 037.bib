@article{ WOS:000404993800007,
Author = {Axelrod, Mark},
Title = {Blocking change: facing the drag of status quo fisheries institutions},
Journal = {INTERNATIONAL ENVIRONMENTAL AGREEMENTS-POLITICS LAW AND ECONOMICS},
