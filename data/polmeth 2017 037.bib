@article{ WOS:000398001700008,
Author = {Schnabel, Landon and Sevell, Eric},
Title = {SHOULD MARY AND JANE BE LEGAL? AMERICANS' ATTITUDES TOWARD MARIJUANA AND
   SAME-SEX MARRIAGE LEGALIZATION, 1988-2014},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2017},
Volume = {81},
