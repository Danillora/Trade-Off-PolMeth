@article{ WOS:000398071200004,
Author = {Greene, Derek and Cross, James P.},
Title = {Exploring the Political Agenda of the European Parliament Using a
   Dynamic Topic Modeling Approach},
Journal = {POLITICAL ANALYSIS},
Year = {2017},
Volume = {25},
Number = {1},
Pages = {77-94},
Month = {JAN},
Abstract = {This study analyzes the political agenda of the European Parliament (EP)
   plenary, how it has evolved over time, and the manner in which Members
   of the European Parliament (MEPs) have reacted to external and internal
   stimuli when making plenary speeches. To unveil the plenary agenda and
   detect latent themes in legislative speeches over time, MEP speech
   content is analyzed using a new dynamic topic modeling method based on
   two layers of Non-negative Matrix Factorization (NMF). This method is
   applied to a new corpus of all English language legislative speeches in
   the EP plenary from the period 1999 to 2014. Our findings suggest that
   two-layer NMF is a valuable alternative to existing dynamic topic
   modeling approaches found in the literature, and can unveil niche topics
   and associated vocabularies not captured by existing methods.
   Substantively, our findings suggest that the political agenda of the EP
   evolves significantly over time and reacts to exogenous events such as
   EU Treaty referenda and the emergence of the Euro Crisis. MEP
   contributions to the plenary agenda are also found to be impacted upon
   by voting behavior and the committee structure of the Parliament.},
