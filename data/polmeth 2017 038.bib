@incollection{ WOS:000411499000019,
Author = {Romm-Livermore, Celia and Raisinghani, Mahesh and Rippa, Pierluigi},
Book-Author = {Ibrahim, Y},
Title = {eLearning Political Strategies: A Four Act Play},
Booktitle = {POLITICS, PROTEST, AND EMPOWERMENT IN DIGITAL SPACES},
Series = {Advances in Electronic Government Digital Divide and Regional
   Development},
Year = {2017},
Pages = {289-303},
Abstract = {The starting point for this paper is the eLearning Political Strategies
   (ELPoS) model. The model is based on two dimensions: 1) the direction of
   the political strategy (upward or downward), and 2) the scope of the
   political strategy (individual or group based). The interaction between
   the above dimensions defines four different types of eLearning political
   strategies, which result in different political outcomes. The model is
   accompanied with one case study that is divided into four parts
   ({''}acts{''}). Each of the acts provides an example of one of the four
   strategies in the model. The discussion and conclusions sections
   integrate the findings from the case study, outline the rules that
   govern the application of political strategies in the context of
   eLearning, and list some of the theoretical and practical implications
   from a better understating of the politics of eLearning.},
