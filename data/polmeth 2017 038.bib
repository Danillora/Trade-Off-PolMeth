@article{ WOS:000399341800007,
Author = {Shon, Jongmin},
Title = {State-local sales tax, spillover, and economic activity: examining
   county governments in the US},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2017},
Volume = {43},
Number = {3},
Pages = {429-450},
Month = {JUN},
Abstract = {The reliance on sales taxation has increased in both states and
   counties. This study aggregated county-level data and empirically
   explored the associations between state-local sales taxes and economic
   activity by industry types in county governments for the period of
   1990-2013. The results revealed negative associations of sales tax rates
   with economic activity, especially in the manufacturing industry.
   Further, the findings provided the spatial dependence of economic
   activity across counties as a form of possible spillover. This study
   suggests that policymakers should pay attention to how the manufacturing
   and retail industries respond to any changes in sales tax rates for
   business activity.},
