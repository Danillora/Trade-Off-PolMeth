@article{ WOS:000423965900003,
Author = {Stefoska, Irena and Stojanov, Darko},
Title = {A tale in stone and bronze: old/new strategies for political
   mobilization in the Republic of Macedonia},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2017},
Volume = {45},
Number = {3},
Pages = {356-369},
Abstract = {The redesign of Skopje's main square and the wider central area in the
   last six years has been a top priority of the Macedonian government. The
   project, called Skopje 2014, provoked intense domestic debate and
   controversy as well as international reaction and concern. Although
   officials say that project's aim is to unify ethnic Macedonians, it has
   produced several lines of political, intra-ethnic/interethnic as well as
   intracultural/intercultural divisions in the fragile Macedonian society.
   The aim of the paper is to offer reflections about its mobilizing
   potential among ethnic Macedonians in a set of social, economic, and
   political contexts. In that sense, four areas of mobilization are
   suggested: (1) around new identity markers; (2) around the name dispute
   and against threats (real or imagined) to the ethnic and national
   identity; (3) against the internal Other, that is, the ethnic Albanian
   community, as well as critics of these identity politics; and (4) in
   reaction to the global financial crisis and problems within the EU.},
