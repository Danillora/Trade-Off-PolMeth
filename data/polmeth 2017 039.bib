@article{ WOS:000409272600002,
Author = {Tavares, Antonio F.},
Title = {Ten years after: revisiting the determinants of the adoption of
   municipal corporations for local service delivery},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2017},
Volume = {43},
Number = {5},
Pages = {697-706},
Abstract = {Research on the use of municipal corporations to deliver local and
   regional public services has evolved significantly in the past decade.
   Most of this work addresses the performance of this service-delivery
   mode in relation to local bureaucracies. However, much less is known
   about the drivers for the adoption of municipal corporations in the
   first place. This article reviews the main determinants of adoption -
   service characteristics, institutional and regulatory settings,
   political constraints and financial conditions - and highlights the need
   for comparative research across countries.},
