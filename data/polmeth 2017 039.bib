@article{ WOS:000399341800009,
Author = {Nyhuis, Dominic},
Title = {Estimating policy positions of local parties in elections with
   multi-vote ballots},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2017},
Volume = {43},
Number = {3},
Pages = {475-498},
Month = {JUN},
Abstract = {This paper investigates the potential for estimating policy positions
   from electoral results in elections with multiple votes. When voters can
   spread their votes across multiple party lists in open list elections,
   they are more likely to select candidates from parties with similar
   policy positions. The electoral results can therefore be exploited to
   infer parties' preferences based on the structure of vote combinations.
   The proposed data provide a valuable tool for analysing party behaviour
   in circumstances where ordinary methods for estimating policy positions
   fail, most importantly in electoral contexts with local competitors.
   Applying an ideal point model for count data, party preferences are
   estimated for a German municipality.},
