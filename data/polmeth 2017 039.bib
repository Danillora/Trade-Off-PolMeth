@article{ WOS:000415670500003,
Author = {Morris, Jonathan and Harrison, John and Genovese, Andrea and Goucher,
   Liam and Koh, S. C. L.},
Title = {Energy policy under austerity localism: what role for local authorities?},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2017},
Volume = {43},
Number = {6},
Pages = {882-902},
Abstract = {In the UK, local authorities (LAs) have been placed at the forefront of
   domestic energy-reduction strategies as the responsible actors for
   coordinating policy in this sector. Yet, there has been little research
   regarding the role of LAs in this policy agenda, and their abilities to
   bring together stakeholders in the successful design and implementation
   of strategies to reduce energy demands. The paper aims to fill this gap
   by highlighting the relevance and importance of the energy policy sphere
   to local government studies, building on the idea of resilient LAs
   within the context of tensions between the localism agenda and the
   actual implementation of energy efficiency polices. This is achieved
   through multiple rounds of semi-structured interviews with LA officers.
   Our findings reveal that LAs, operating under a localism agenda, lack
   the freedoms and resources from central government to meet the needs of
   multiple stakeholders, resorting to short-term policies.},
