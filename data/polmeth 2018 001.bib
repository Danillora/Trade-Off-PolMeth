@article{ WOS:000425469900002,
Author = {Best, Robin E. and Budge, Ian and McDonald, Michael D.},
Title = {Jettisoning Illusions About the Median Mandate},
Journal = {LEGISLATIVE STUDIES QUARTERLY},
