@article{ WOS:000457133400007,
Author = {Concha Villanueva, Sebastian},
Title = {Proposals to regulate migrations in Chile and the obstinacy of
   securitism},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
Year = {2018},
Number = {23},
Pages = {110-126},
Month = {DEC},
Abstract = {This paper aims to analyze, in a comparative perspective, the projects
   to regulate migrations in Chile presented by the governments of Michelle
   Bachelet and Sebastian Pinera. It is argued that both share a common
   feature: despite having elements based on the promotion of rights, they
   maintain traits that correspond to a control logic. We summarize the
   Chilean phenomenon of immigration that motivates the creation of a new
   regulation, analyze the present law and, finally, establish a
   comparative framework that addresses the particularities of the proposed
   immigration laws made by the Michelle Bachelet and Sebastian Pinera
   governments.},
Publisher = {FLACSO-ECUADOR},
Address = {CALLE PRADERA E7-174 \& DIEGO ALMAGRO, QUITO, 00000, ECUADOR},
Type = {Article},
Language = {Spanish},
Affiliation = {Villanueva, SC (Corresponding Author), Univ Catolica Chile, Santiago, Chile.
