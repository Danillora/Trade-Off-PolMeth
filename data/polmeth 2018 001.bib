@article{ WOS:000451387500003,
Author = {Reus, Iris},
Title = {The journey is the destination? Conceptual considerations on the
   operationalization of ``federal independence{''} in Lander policy-making},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2018},
Volume = {12},
Number = {4, SI},
Pages = {643-661},
Month = {DEC},
Abstract = {The question of federal independence of Lander policy-making is
   discussed with regard to two aspects in the literature, independence in
   terms of content (differing laws) and in terms of process (without
   coordination). Referring to this, statements often imply adirect
   connection between both, i.e. one concludes from similar contents to the
   intention of harmonisation and from coordination to the consequence of
   uniform policies. However, without differentiated examination the risk
   of false conclusions is high. Therefore this contribution presents anew
   approach of measurement of the degree of independence of Lander
   policy-making, which systematically includes content and process of
   legislation. The results show that often similarities of laws were not
   the consequence of intentional harmonisation. Thus, the new approach
   offers ahuge added value and leads to amore positive evaluation of
   independent Lander policy-making than often depicted in the literature.},
