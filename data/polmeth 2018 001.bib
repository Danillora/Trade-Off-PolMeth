@article{ WOS:000451376900004,
Author = {Jacobs, Nicholas F.},
Title = {After the White House: The Politics of the Postpresidency},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2018},
Volume = {48},
Number = {4},
Pages = {711-740},
Month = {DEC},
Abstract = {As independent elder statesmen, former presidents command a unique
   authority and widespread ambitions. But as the experiences of Harry S.
   Truman and Dwight D. Eisenhower reveal, postpresidential goals are often
   in conflict with one another. Both men remained active and were forced
   to speak out on national issues they tried to ignore. Both were also
   adamant in condemning their presidential successors on policy, and as
   senior party officials helped to build their party organizations after
   leaving office. To the extent that they were successful, both relied on
   the peculiar form of political capital accrued in office, but they were
   often unable to escape the roles they carved out during their respective
   tenures. Truman's and Eisenhower's postpresidencies therefore suggest
   that despite lacking institutional power, ex-presidents can succeed in
   leveraging existing political capital to safeguard their legacies and
   protect the institutional reputation of the presidency.},
Publisher = {WILEY},
