@article{ WOS:000434315100001,
Author = {Curry, James M. and Haydon, Matthew R.},
Title = {Lawmaker Age, Issue Salience, and Senior Representation in Congress},
Journal = {AMERICAN POLITICS RESEARCH},
Year = {2018},
Volume = {46},
Number = {4},
Pages = {567-595},
Month = {JUL},
Abstract = {Political scientists have demonstrated the importance of lawmakers'
   identities, showing that race, gender, socioeconomic status, and sexual
   orientation affect legislative and representational behavior. Is the
   same true for age? We argue it is, but the effect is conditioned by the
   salience of different senior issues. Analyzing the bill introductions by
   members of Congress during the 109th and 110th Congresses, we show that
   older lawmakers are more likely to introduce legislation addressing
   lower salience senior issues than their younger colleagues. In contrast,
   sizeable senior constituencies in a district influence lawmaker
   attention to higher salience senior issues, regardless of a lawmaker's
   age. These findings have implications for our understanding of senior
   power and personal roots of representation in the United States.},
