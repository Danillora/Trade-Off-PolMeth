@article{ WOS:000450646900001,
Author = {Krickovic, Andrej and Weber, Yuval},
Title = {Commitment Issues The Syrian and Ukraine Crises as Bargaining Failures
   of the Post-Cold War International Order},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2018},
Volume = {65},
Number = {6},
Pages = {373-384},
Month = {NOV 2},
Abstract = {Russia's interventions in Ukraine and Syria are designed to force the
   U.S. to accept a new ``grand bargain{''} about the future of world
   order. Drawing on bargaining theory, the article argues that any such
   bargain suffers from acute commitment problems. Russia cannot convince
   the U.S. that there are limits to its revisionism, while the U.S. is
   unable to reassure Russia that it won't renege on any agreements as
   Russian power declines. President Donald Trump has hinted at detente.
   But as long as these underlying commitment problems remain unresolved,
   the two sides will continue their slide towards a new Cold War.},
