@article{ WOS:000446499900002,
Author = {Maull, Hanns W.},
Title = {Reflective, Hegemonic, Geo-economic, Civilian ... ? The Puzzle of German
   Power},
Journal = {GERMAN POLITICS},
Year = {2018},
Volume = {27},
Number = {4, SI},
Pages = {460-478},
Abstract = {Can German foreign policy still be understood as one approximating the
   foreign policy of an ideal type of civilian power? This article
   discusses recent efforts to offer alternative characterisations, such as
   `reflective', `shaping', or `geo-economic' power and concludes that
   `civilian power' may still offer the best fit to understand German
   foreign policy. Its problem may not consist so much in a shift towards a
   different trajectory and destination but the hollowing out of its
   existing foreign policy role concept.},
