@article{ WOS:000424493800002,
Author = {Allard-Tremblay, Yann},
Title = {The differentiation of citizenship; from negotiated differences to
   corrupted liability},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2018},
Volume = {6},
Number = {1},
Pages = {3-19},
Abstract = {Citizenship is presented as a mode of governance, independently from
   substantive conceptions of political community, and differentiated
   citizenship and citizenship removal are assessed in light of this
   approach. In the first part, I present citizenship as a relationship
   where the governors are liable to the governed. In the second part, I
   argue that differentiation should be the norm rather than the exception.
   In the third part, I present three forms of differentiated citizenship
   and I discuss their compatibility with civic governance. I specifically
   make a case against the regularization of the power to remove
   citizenship.},
