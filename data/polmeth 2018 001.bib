@article{ WOS:000444425400003,
Author = {Hjorth, Ronnie},
Title = {Civil association across borders: Law, morality and responsibility in
   the post-Brexit Era},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
