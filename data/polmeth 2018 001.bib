   Cortella, Lucio, Univ Ca Foscari Venezia, Dipartimento Filosofia \& Beni Culturali, Dorsoduro 3484-D, I-30123 Venice, Italy.},
DOI = {10.4476/92293},
ISSN = {2240-7901},
Keywords = {socialism; capitalism; ethical life; freedom; social freedom},
Research-Areas = {Government \& Law},
