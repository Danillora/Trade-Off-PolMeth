@article{ WOS:000458387700007,
Author = {Sharma, Buddhi Prasad},
Title = {China-Nepal Relations A Cooperative Partnership in Slow Motion},
Journal = {CHINA QUARTERLY OF INTERNATIONAL STRATEGIC STUDIES},
Year = {2018},
Volume = {4},
Number = {3},
Pages = {439-455},
Month = {FAL},
Abstract = {Despite the long-standing friendship between Nepal and China, their
   relationship has developed rather slowly since the reestablishment of
   democracy in Nepal in 1990, mainly due to the constant pressure from
   Western forces faced by Nepali political elite, as well as the ignorance
   of most democratic governments about balancing Nepal's foreign
   relations, especially between its two giant neighbors, India and China.
   In recent years, however, the Nepali government has been trying to
   promote pragmatic cooperation with China in order to garner support for
   its own development. With the signing of the memorandum of understanding
   (MoU) on cooperation in building the ``Belt and Road Initiative
   (BRI),{''} the China-Nepal relationship is likely to enter a new era.
   Based on a historical review of the development of China-Nepal
   relations, this article offers some suggestions on how to enhance their
   cooperative partnership in the future.},
