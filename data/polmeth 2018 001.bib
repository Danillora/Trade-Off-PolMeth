@article{ WOS:000456954600010,
Author = {Carkoglu, Ali and Yildirim, Kerem},
Title = {Change and Continuity in Turkey's June 2018 Elections},
Journal = {INSIGHT TURKEY},
Year = {2018},
Volume = {20},
Number = {4},
Pages = {153-182},
Month = {FAL},
Abstract = {In this paper, we first present a background to the watershed elections
   of 2018, describing the context preceding the elections. In order to
   explain the changes and continuities in the geographic patterns of the
   2018 elections, we present ballot-box and district level data from 2018
   and previous elections. We refer to long-term ideological/cultural
   differences us well us short-term evaluations about security and the
   economy in shaping the 2018 election results. Even in the face of
   various important developments, radical changes in the geographic voting
   patterns do not appear to have taken place. The incumbent AK Party and
   its leader, President Erdogan, continued to have an electoral edge over
   competitors in regions that mostly overlap with those observed in
   earlier elections.},
