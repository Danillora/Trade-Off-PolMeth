@article{ WOS:000451271900007,
Author = {Davydov, D. A.},
Title = {SOCIAL SUBJECT OF TRANSITION TOWARDS POST-CAPITALIST SOCIETY: WHO IS IT?},
Journal = {POLITEIA-JOURNAL OF POLITICAL THEORY POLITICAL PHILOSOPHY AND SOCIOLOGY
   OF POLITICS},
