@article{ WOS:000432925000011,
Author = {Voskresenskiy, A. D.},
Title = {19TH NATIONAL CONGRESS OF THE COMMUNIST PARTY OF CHINA: INTERNAL AND
   EXTERNAL EFFECTS AND PROSPECTS OF CHINA'S REFORMS},
