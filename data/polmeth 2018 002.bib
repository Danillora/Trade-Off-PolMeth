@article{ WOS:000424201600001,
Author = {Cox, Gary W. and Weingast, Barry R.},
Title = {Executive Constraint, Political Stability, and Economic Growth},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2018},
Volume = {51},
Number = {3},
Pages = {279-303},
Month = {MAR},
Abstract = {Previous studies have argued that democracy diminishes the extent to
   which leadership contests depress economic growth, by reducing the
   violence and uncertainty attendant on such contests. We reconsider the
   theoretical basis for this claim, highlighting the separate roles of
   executive constraint and electoral accountability. Exploiting panel data
   from 1850 to 2005, we show that the executive's horizontal
   accountability to the legislature significantly moderates the economic
   downturns associated with leadership turnover, while its vertical
   accountability to the electorate does not. These results suggest that,
   in terms of moderating succession-related downturns and thereby
   promoting steadier economic growth, the health of legislatures is more
   important than the health of elections.},
Publisher = {SAGE PUBLICATIONS INC},
