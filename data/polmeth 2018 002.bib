@article{ WOS:000448160600021,
Author = {Boydstun, Amber E. and Highton, Benjamin and Linn, Suzanna},
Title = {Assessing the Relationship between Economic News Coverage and Mass
   Economic Attitudes},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2018},
Volume = {71},
Number = {4},
Pages = {989-1000},
Month = {DEC},
Abstract = {Do economic performance and economic news coverage influence public
   perceptions of the economy? Efforts to assess the effects are hampered
   by the interrelationships among the variables. In this paper, we bring
   to bear a more careful accounting of available economic variables than
   previous studies have used. We find that both media tone and economic
   attitudes are strongly related to actual economic performance. Moreover,
   after taking into account the economy itself, a substantial relationship
   between media tone and economic attitudes persists. Given that economic
   attitudes influence a wide variety of political outcomes, this finding
   carries important normative and political significance.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
