@article{ WOS:000462253400005,
Author = {Torbakov, Igor'},
Title = {Twins and competitors The limitations of the Russian-Turkish alliance},
Journal = {OSTEUROPA},
Year = {2018},
Volume = {68},
Number = {10-12},
Pages = {95+},
Abstract = {At first glance, Russia and Turkey resemble twins. They have an imperial
   heritage, the nation building process has not yet been completed, and
   modernisation has occurred through state mobilisation. Their
   relationship with Europe is also ambivalent. The authoritarian regimes
   appear to be ideal alliance partners. Yet in reality, this is far from
   true. Their current joint appearance belies the fact that they are
   pursuing different interests in their shared regional environment, where
   they have become competitors due to the ongoing imperialist nature of
   their strategic culture.},
