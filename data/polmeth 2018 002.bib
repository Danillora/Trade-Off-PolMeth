DOI = {10.1080/18918131.2018.1522765},
ISSN = {1891-8131},
EISSN = {1891-814X},
Keywords = {Denmark; human rights diplomacy; Universal Declaration of Human Rights;
   eugenics; 1953 Danish Constitution; internationalism},
