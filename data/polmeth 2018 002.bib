@article{ WOS:000444731900006,
Author = {Christiaens, Tim},
Title = {Neoliberalism and the Right to Be Lazy: Inactivity as Resistance in
   Lazzarato and Agamben},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2018},
Volume = {30},
Number = {2},
Pages = {256-274},
Abstract = {Neoliberalism has installed an unending competitive struggle in the
   economy. Within this context activists have pushed for a reappraisal of
   laziness and inactivity as forms of resistance. This idea has been
   picked up by Maurizio Lazzarato and Giorgio Agamben, but in different
   ways. This essay first explains the former's appraisal of laziness as a
   release of potentialities unrealizable under financial capitalism.
   However, Lazzarato's appraisal of laziness resembles neoliberal theories
   of innovation because it shares with neoliberalism the conceptual
   persona of a subject whose potentialities exceed the current status quo.
   Potentiality is thus not an unambiguous antagonist of capitalism, as
   Lazzarato suggests. In order to adequately oppose neoliberalism,
   Lazzarato should question the role of potentiality in capitalism.
   Agamben has undertaken such a project. The second part of the essay
   consequently argues that Agamben's philosophy of inactivity as
   impotentiality is able to circumvent neoliberalism and the society of
   the spectacle.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
