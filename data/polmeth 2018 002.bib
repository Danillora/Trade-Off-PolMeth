@article{ WOS:000413960200010,
Author = {Ceron, Andrea and Negri, Fedra},
Title = {March divided, fight united? Trade union cohesion and government appeal
   for concertation},
Journal = {WEST EUROPEAN POLITICS},
Year = {2018},
Volume = {41},
Number = {1},
Pages = {218-239},
Abstract = {Why does the government appeal for concertation? Starting from the
   principalagent framework and delegation theory, the article argues that
   the government is more willing to share decision-making power with trade
   unions when the policy preferences endorsed by the unions are closer to
   those of the cabinet. Furthermore, it maintains that government
   propensity to negotiate with trade unions increases as the heterogeneity
   of union policy preferences grows because the cabinet can exploit its
   agenda-setting power to divide the union front. The article tests these
   two hypotheses through a longitudinal analysis of the Italian case
   (1946-2014). In detail, it takes advantage of two original datasets
   built through content analysis that provide unique in-depth information
   on the policy preferences of parties and cabinets and measures the
   policy positions of the main Italian trade unions, thus allowing
   assessment of their reciprocal heterogeneity. The results confirm the
   expectations.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
