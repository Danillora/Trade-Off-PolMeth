@article{ WOS:000419812900005,
Author = {Letsa, Natalie Wenzell and Wilfahrt, Martha},
Title = {Popular Support for Democracy in Autocratic Regimes: A Micro-Level
   Analysis of Preferences},
Journal = {COMPARATIVE POLITICS},
Year = {2018},
Volume = {50},
Number = {2},
Pages = {231+},
Month = {JAN},
Abstract = {Scholarly opinions diverge as to which citizens in autocratic regimes
   actually prefer democracy to the status quo. While some argue that
   citizens with higher levels of socioeconomic status are more likely to
   prefer democracy because they desire political equality, others argue
   that the poor should prefer democracy most because they will have more
   relative power to affect redistributive policies. Analyzing public
   opinion data for tens of thousands of respondents living in autocracies
   around the world, we show that all types of citizens in authoritarian
   countries-rich and poor alike-prefer democracy to autocratic rule.
   Drawing on two case studies, we also show that political and economic
   factors are more likely to mobilize different types of people to protest
   for democracy.},
