@article{ WOS:000447678800012,
Author = {Perez, Domingo and Link, Sebastian},
Title = {Territorial control in the labour process dispute: Chilean trade
   unionism in mining companies versus supermarkets},
Journal = {POLITICA Y SOCIEDAD},
Year = {2018},
Volume = {55},
Number = {2},
Pages = {575-596},
Abstract = {Based on the thesis on marginal territorial control of workers in
   capitalist enterprise, we have investigated in Chile experiences of
   trade union territoriality in two paradigmatically opposed workplaces,
   namely, mining and supermarkets. Through a study of intro-sector
   comparative cases and extreme cases comparison between sectors, we have
   found three significant tactics of union territoriality: (i) production
   of safe spaces at the company, (ii) creation of unions in territorial
   rearguard regarding property, and (iii) conjuncture control in strikes
   mobilizations. We conclude by emphasizing the importance of safe spaces
   for emergence of trade unionism in front of anti-union labour regimes,
   providing protection and relative stimulation to working-class
   consciousness - and especially safe spaces external to company, which,
   politically, suggest as a thesis of contemporary revitalization the
   construction of ``labour districts{''}.},
