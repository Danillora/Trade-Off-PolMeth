@article{ WOS:000428675900005,
Author = {Wemheuer-Vogelaar, Wiebke and Risse, Thomas},
Title = {International Relations Scholars in Germany: Young, Internationalised,
   and Non-Paradigmatic},
Journal = {GERMAN POLITICS},
Year = {2018},
Volume = {27},
Number = {1},
Pages = {89-112},
Abstract = {This article describes the German International Relations (IR) community
   and their research and writing preferences based on data from the 2014
   Teaching, Research, and International Policy (TRIP) faculty survey.
   Germany has a relatively young IR community with a relatively large
   share of women, however, this changes once scholars reach tenured
   professorships. We found that German IR scholars are, on average, more
   oriented towards IR theory and the study of international
   organisation(s) than other national IR communities. The overall picture
   is one of paradigmatic as well as theoretical pluralism and a rejection
   of ontological warfare, despite there being more IR scholars in Germany
   than anywhere else in the world self-identifying as social
   constructivists. Their methodological orientations are overwhelmingly
   qualitative - again above average as compared to other IR communities in
   the world. At the same time, German respondents identified methodology
   and epistemology as two of the main factors causing division among IR
   scholars today. Moreover, German IR scholars are almost completely
   internationalised with a strong leaning toward the Anglo-American core
   of the discipline. Various beauty contests' reveal a still vividly
   accurate image of IR as an American social science'.},
