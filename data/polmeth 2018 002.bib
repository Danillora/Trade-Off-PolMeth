@article{ WOS:000429910600006,
Author = {Dacombe, Rod},
Title = {Systematic Reviews in Political Science: What Can the Approach
   Contribute to Political Research?},
Journal = {POLITICAL STUDIES REVIEW},
Year = {2018},
Volume = {16},
Number = {2},
Pages = {148-157},
Month = {MAY},
Abstract = {Recent years have seen the growing use of systematic literature reviews
   within the social sciences. Despite some reservations over the adoption
   of an approach originally popularised within clinical and health
   sciences, the literature in the area has contributed some clear benefits
   to accounts of existing research. It is surprising, therefore, that
   political scientists have tended to ignore, or at best marginalise,
   reviews of this kind. This article outlines a number of features of
   systematic reviews which might be suitable for the kinds of questions
   political scientists ask of their data. Throughout, it highlights both
   the value and the potential complications of the approach, raising a
   number of questions which should be considered if systematic reviews are
   to be adopted more widely.},
