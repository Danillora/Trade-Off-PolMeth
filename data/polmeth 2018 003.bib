@article{ WOS:000429442900003,
Author = {Norri-Sederholm, Teija and Huhtinen, Aki-Mauri and Paakkonen, Heikki},
Title = {Ensuring Public Safety Organisations' Information Flow and Situation
   Picture in Hybrid Environments},
Journal = {INTERNATIONAL JOURNAL OF CYBER WARFARE AND TERRORISM},
