@article{ WOS:000425118300013,
Author = {Stavrianakis, Anna},
Title = {When ``Anxious Scrutiny' of Arms Exports Facilitates Humanitarian
   Disaster},
Journal = {POLITICAL QUARTERLY},
Year = {2018},
Volume = {89},
Number = {1},
Pages = {92-99},
Month = {JAN-MAR},
Abstract = {How is it that the UK government continues to export weapons to Saudi
   Arabia for use in the war in Yemen, despite an explicit commitment to
   international humanitarian law (IHL)? And how is it that the High Court
   recently dismissed a case of judicial review, confirming that the
   government was rationally entitled to conclude' that arms exports pose
   no clear risk to IHL in Yemen? In what follows, I explain how a flexible
   interpretation of risk, reliance on secret information, and deference of
   the Court to the executive serve to facilitate rather than restrict arms
   exports. The judges' decision provides a stamp of approval to an arms
   export policy that has directly contributed to the deaths of thousands
   of civilians in Yemen. Attention to the Saudi/Yemen case shows the
   political and legal manoeuvring that goes into managing the
   contradictions in government arms export policy.},
