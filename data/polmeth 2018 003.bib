@article{ WOS:000446898800003,
Author = {Hassanzadeh, Navid},
Title = {Race, Internationalism, and Comparative Political Theory},
Journal = {POLITY},
Year = {2018},
Volume = {50},
Number = {4},
Pages = {519-546},
Month = {OCT},
Abstract = {Minority traditions of thought within the West have typically not been
   examined as objects of study within comparative political theory, which
   as a field has tended to focus mainly on non-Western thought in its
   interventions within the broader theory subfield. This article offers an
   illustration of the takeaways to be derived from placing black political
   thought into conversation with comparative theory by examining the work
   of W. E. B. Du Bois from the early twentieth century to the early 1940s.
   I contend that through his argument in support of black autonomy or
   separation among African Americans, which draws inspiration from a
   particular form of internationalism, Du Bois transforms Western values,
   making them sensitive to the particularities of black life and the
   enduring nature of racial injustice, and in the process, usefully
   demonstrates how universalism can be re-thought by dwelling on the
   points of contact between different traditions of thinking.},
