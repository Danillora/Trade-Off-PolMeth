@article{ WOS:000430961100003,
Author = {Jarvie, Grant and Birnbacher, Dominik},
Title = {SPORT, AUSTERITY OR CHOICE IN SCOTLAND: AN ANALYSIS OF DIRECT GOVERNMENT
   EXPENDITURE ON SPORT},
Journal = {SCOTTISH AFFAIRS},
