   4002, VENEZUELA},
Type = {Article},
Language = {Spanish},
Affiliation = {Rojas-Bahamon, Magda J., Univ Surcolombiana, Educ \& Cultura Ambiental, Neiva, Huila, Colombia.
   Fernanda Arbelaez-Encarnacion, Tanya, Univ Libre Colombia, Bogota, Colombia.},
ISSN = {0798-1406},
