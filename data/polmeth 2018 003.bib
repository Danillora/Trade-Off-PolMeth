@article{ WOS:000428280900007,
Author = {Africa, Cherrel and Mutizwa-Mangiza, Shingai},
Title = {The Need for a New Language? How Historically Disadvantaged Institutions
   Grapple with the Effects of Labelling in Higher Education: The Case of
   the University of the Western Cape},
Journal = {POLITIKON},
Year = {2018},
Volume = {45},
Number = {1, SI},
Pages = {81-93},
Note = {National Conference on Decolonization after Democracy, S African Assoc
   Polit Studies, Cape Town, SOUTH AFRICA, AUG 30-SEP 02, 2016},
Organization = {Univ Western Cape},
Abstract = {Tertiary institutions in South Africa have been dichotomised through the
   colonial structure and apartheid which sought to subjugate some
   institutions and elevate others. Not only have historically
   disadvantaged institutions (HDIs) faced a set of deep material
   difficulties, they have also had to grapple with the negative
   implications of naming. Using the work of Stryker and Burke as well as
   Bourdieu, this article interrogates the complexities of the HDI label
   for the University of the Western Cape. This is a complex issue because
   the same label can simultaneously provide access to state resources and
   inhibit opportunities from potential sponsors and employers. Unlike
   individuals who can hold multiple identities, institutions are expected
   to embrace a particular identity. Our conclusion is that we need to open
   the conversation about institutional labelling and develop a new
   language to describe former HDIs which should ideally acknowledge their
   difficult history without perpetuating stigma.},
