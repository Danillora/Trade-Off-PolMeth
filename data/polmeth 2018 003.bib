@article{ WOS:000433949600020,
Author = {Russell, Ellen D.},
Title = {Rethinking Marxism in Stephen Resnick's Classroom: Pedagogical
   Reflections on the Fatiguing and the Luminous},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2018},
Volume = {30},
Number = {1},
Pages = {142-153},
Abstract = {Marx famously described the learning of Marxism as both potentially
   fatiguing and luminous. Marxian pedagogy must navigate the perils
   encountered by students of Marxism, and encourage glimpses of the
   enlivening possibilities that this encounter may enable. To reflect on
   Stephen Resnick's pedagogical contributions to the teaching of Marxism,
   this essay employs insights from psychoanalysis and education theory to
   reconsider the author's experience of Marxism in Steve Resnick's
   classroom. It highlights the concepts of overdetermination and
   exploitation in order to reflect on the ways in which Resnick both
   provoked and supported struggles with what Cho calls ``knowledge of the
   disturbing kind.{''}},
