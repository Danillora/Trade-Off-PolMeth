@article{ WOS:000457455300007,
Author = {Enrique Tapia, Carlos},
Title = {Human mobility in Michoacan: an approach to institutional research},
Journal = {CIMEXUS},
Year = {2018},
Volume = {13},
Number = {2},
Pages = {113-133},
Month = {JUL-DEC},
Abstract = {Research on human mobility in Michoacan currently represent a vast
