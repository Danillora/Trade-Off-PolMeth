@article{ WOS:000446528800007,
Author = {Luis Thielemann H},
Title = {The Pagan Rudeness: On the Radicalization of the Labour 1Movement During
   the ``Long Sixties{''}. Chile, 1957 - 1970},
Journal = {IZQUIERDAS},
Year = {2018},
Volume = {44},
Number = {SI},
Pages = {114-133},
Month = {JUN},
Abstract = {During the ``Long Sixties{''} (1957-1973) in Chile, the labor movement
   experienced a process of political radicalization. This paper reviews
   the general dimensions and the concrete forms of this phenomenon,
   defined by a complex, class-based articulation of memory, identity and
   experience of the social conflict. Thus, it is argued that the
   radicalization of the labor movement did not start with the arrival of
   Allende and Popular Unity to the government in 1970. This process
   started several years before and it found in those three years
   (1970-1973) its ideal conditions for an overflowing deployment.},
