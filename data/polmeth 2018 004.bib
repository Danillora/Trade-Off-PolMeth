@article{ WOS:000439983500003,
Author = {Golosov, Grigorii V. and Tkacheva, Tatiana},
Title = {Let My People Run Pre-election Resignations of Russia's Governors,
   2013-2015},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2018},
Volume = {65},
Number = {4},
Pages = {243-252},
Abstract = {By permitting early resignations of the governors of Russia's regions,
   followed by their participation in premature elections, the federal
   center seeks to facilitate their long-term political survival. This
   study uses the data from the 2013-2015 gubernatorial elections in order
   to reveal the Kremlin's motivations for this strategy. The analysis
   demonstrates that in contrast to the previous periods of Russia's
   political development, when the federal center tended to reward the
   governors for electoral deference, the current strategy is aimed
   primarily at long-term risk aversion. This signifies a shift in the
   order of priorities of the Kremlin's policy toward the regions.},
