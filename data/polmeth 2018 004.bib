@article{ WOS:000452210300008,
Author = {Ugur, Agah},
Title = {THE ART OF MAKING CRITICAL DECISIONS},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2018},
Volume = {17},
Number = {2},
Pages = {73-77},
Month = {SUM},
Abstract = {The ability to make sound and critical decisions is a key determinant of
   leadership mettle. In this article, the author relays his personal
   experience with making prudent decisions, highlighting two rules of
   thumb that come particularly in handy The first rule is to focus on the
   big picture, which entails identifying what the aim and destination of
   your personal journey is. The second rule is to link your decisions to a
   reference point, by which the author means your values, ethics, and
   point of view. Finally, learning how to balance the forces of emotion
   and rationality, while dealing with uncertainty is crucial for sound
   decision making and creating meaningful change.},
