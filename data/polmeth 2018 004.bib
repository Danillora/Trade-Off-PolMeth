@article{ WOS:000444126600003,
Author = {Maloney, William A. and Hafner-Fink, Mitja and Fink-Hafner, Danica},
Title = {The impact of the EU accession process and EU funding on the
   professionalization of national interest groups: the Slovenian case},
Journal = {INTEREST GROUPS \& ADVOCACY},
Year = {2018},
Volume = {7},
Number = {1},
Pages = {41-60},
Month = {MAR},
Abstract = {This article investigates the extent to which European Union (EU)
   accession and EU funding contribute to the professionalization of
   interest groups in Slovenia. Have EU accession and funding had a more
   profound impact on the professionalization of interest groups in
   Slovenia than the shift from socialism to capitalism? Our novel
   empirical evidence is drawn from two surveys of interest groups in
   Slovenia-the first in 1996 eight years prior to Slovenia's membership of
   the EU (in 2004) and the second in 2012 eight years after its accession.
   Our findings show that EU accession and funding accentuate the
   professionalization of interest groups. However, these processes are
   intertwined with the partitocratic role of national political parties
   and parties mediate the shape and structure of interest group
   professionalization via their decisions on the allocation of EU funding
   to groups.},
