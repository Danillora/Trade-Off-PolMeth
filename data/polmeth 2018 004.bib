@article{ WOS:000438360000004,
Author = {Riegl, Martin and Dobos, Bohumil},
Title = {Power and Recognition: How (Super)Powers Decide the International
   Recognition Process},
Journal = {POLITICS \& POLICY},
Year = {2018},
Volume = {46},
Number = {3},
Pages = {442-471},
Month = {JUN},
Abstract = {The article offers a modified theoretical approach toward the issue of
   secession and international recognition. Based on the novel political
   theory of secession as presented by Milena Sterio and the measurement of
   external involvement in secessionist attempts as researched by Alexis
   Heraclides, the article provides a theoretical model of modified
   (super)power rule. The article, based on preliminary conclusions from an
   internal dataset of secessionist attempts between 1946 and 2016
   containing 63 items, argues that effective (super)power support is a
   sufficient condition for the de facto independence of any secessionist
   entity. The model is tested on cases of Somaliland, Sahrawi Arab
   Democratic Republic, Kosovo, and Transnistria.},
