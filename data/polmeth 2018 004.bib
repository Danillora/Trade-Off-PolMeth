@article{ WOS:000425788800014,
Author = {Hunger, Uwe and Krannich, Sascha},
Title = {Advantages and disadvantages of a point based immigration system.
   Lessons from classic immigration countries},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2018},
Volume = {12},
Number = {1},
Pages = {229-245},
Month = {MAR},
Abstract = {The paper analyzes different point based systems in Canada, Australia,
   and New Zealand, and attempts to draw conclusions from it for policy
   making in Germany. It becomes evident that none of the point based
   systems in these three countries is one to one applicable to Germany. In
   fact, each system has always to be justified to the social, economic,
   political as well as geographic characteristics of a country. However,
   basic advantages and risks of a point based system became clear in the
   descriptions of these different cases that should be considered for the
   creation of a point based system in Germany.},
