@article{ WOS:000437123000005,
Author = {Ahmed, Sayeed Iftekhar},
Title = {Can Islamists be secularized? Religion and politics in urban Bangladesh},
Journal = {ASIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2018},
Volume = {26},
Number = {2},
Pages = {238-256},
Abstract = {As in many other Muslim majority states, the Islamist parties play a
   significant role in the body politic of Bangladesh. Because of their
   counter-hegemonic modus operandi, the question arises regarding the
   ability of the Islamists to cohabit democratically in a secular or a
   quasi-secular political system. The literature on secularization,
   secularism, and Islam relies on a grand narrative of the reasons to
   address the question and provides us with polarized answers regarding
   the Islamists' ability to accommodate the principle of secularism.
   However, almost all of the works have been done on the abstract,
   theoretical level and do not address the interplay of social and
   political factors in Muslim societies. None of the research on Islamism
   in Bangladesh addresses Islamist parties' ability or inability to
   participate in a quasi-secular, democratic system through acclimating to
   secularism in the rubric of their political ideology, Islamism. This
   study therefore examines the grassroots-level socio-political interplays
   of the Islamist activists with the non-Islamists and analyzes their
   willingness and ability to accommodate secularism in order to
   democratically participate in a quasi-secular political system.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
