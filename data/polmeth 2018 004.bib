@article{ WOS:000432906700003,
Author = {Bures, Oldrich and Pernica, Bohuslav},
Title = {Pluralization of Security Education in the Czech Republic},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2018},
Volume = {65},
Number = {3},
Pages = {175-187},
Abstract = {In this article, we argue that although the Czech state may still be the
   dominant player in the Czech security field, its cultural power is
   continuously eroding due to the ongoing process of pluralization of
   security education. While in communist-era Czechoslovakia, only persons
   enlisted in service in the security forces could obtain
   military-oriented security education at state-run military and police
   schools, nowadays virtually anyone living in the Czech Republic has
   access to a wide variety of security studies programs at both public and
   private universities. This relatively quick and spontaneous transition
   has brought about both opportunities and dilemmas, which jointly
   highlight the importance of control over the institutional structures of
   security generation and the production of security knowledge when it
   comes to actors' ability to successfully operate in the security field.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
