@article{ WOS:000419167800006,
Author = {Scully, Mark A.},
Title = {Principled Rhetoric as Coalition Management: Speech in the
   Reconstructive Presidencies of Franklin Roosevelt and Ronald Reagan},
Journal = {POLITY},
Year = {2018},
Volume = {50},
Number = {1},
Pages = {129-157},
Month = {JAN},
Abstract = {This article examines the rhetorical leadership of reconstructive
   presidents: how they use political principles and ideology in an attempt
   to moderate and integrate diverse groups within their partisan regime.
   It analyzes the presidencies of Franklin Roosevelt and Ronald Reagan,
   who shared a method of rhetorical leadership that I call principled
   rhetoric. Principled rhetoric explains specific public policy positions
   by referring to broadly shared partisan values and aspirations. By using
   principled rhetoric, a president attempts to articulate common regime
   principles rather than factional motives to gain support for public
   policy. Principled rhetoric provides competing partisans with a shared
   rationale to support a given public policy, even if their specific
   policy preferences diverge. By developing and perpetuating a principled
   consensus on policy, these two presidents attempted to deepen partisans'
   identity through a common regime narrative. To illustrate the use of
   principled rhetoric, I focus on Roosevelt's and Reagan's advocacy for
   regime-defining public policies: Social Security and budget and tax
   cuts, respectively.},
