@article{ WOS:000431952000002,
Author = {Wagenaar, Hendrik and Wood, Matthew},
Title = {The Precarious Politics of Public Innovation},
Journal = {POLITICS AND GOVERNANCE},
Year = {2018},
Volume = {6},
Number = {1},
Pages = {150-160},
Abstract = {This article argues that debates about public innovation among
   governance scholars risk essentialising the concept. Rather than
   recognise the inherently normative content of public innovation, some
   scholars have created taxonomies that conflate very different forms of
   `innovation' in the public and private sectors, the latter of which is
   deeply contradictory to public values. We re-think public innovation as
   both a pragmatic process, a way of responding to developments in
   contemporary governance, and an inherently public and democratic
   practice. Our analysis addresses three points: who innovates; what is
   the object of innovation, and what are the effects of innovation? From
   this analysis we specify public innovation as both inescapable and
   democratically necessary to safeguard and promote the important values
   of public life.},
