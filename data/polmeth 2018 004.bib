@article{ WOS:000425644800003,
Author = {Liu, Huchen and Jacobson, Gary C.},
Title = {Republican Candidates' Positions on Donald Trump in the 2016
   Congressional Elections: Strategies and Consequences},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2018},
Volume = {48},
Number = {1},
Pages = {49-71},
Month = {MAR},
Abstract = {Donald Trump's nomination posed a challenge for Republican congressional
   candidates running on the ticket with him in 2016. The nominee's intense
   base of support within the party combined with his unprecedented
   unpopularity more broadly required these candidates to think
   strategically about supporting or opposing him. This article examines
   how they responded to that challenge, what explains their different
   responses, and what electoral consequences ensued from their choices.
   The data show that candidates' positions on Trump primarily reflected
   the partisanship of their districts and secondarily their gender and
   incumbency status. Their strategic choices had little general impact in
   an election almost completely dominated by partisanship, but in at least
   a few instances refusal to support Trump may have been necessary for the
   Republican incumbent to win reelection.},
