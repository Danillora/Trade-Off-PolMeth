@article{ WOS:000447466600005,
Author = {Lasnier, Virginie},
Title = {Russia's Opposition Movement Five Years After Bolotnaia: The Electoral
   Trap?},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2018},
Volume = {65},
Number = {5},
Pages = {359-371},
Abstract = {Russia's 2016 parliamentary elections offered an occasion to reassess
   the disappointing outcomes of the movement ``For Fair Elections.{''} I
   argue that to do so we need to look beyond the fact of increasing regime
   repression to examine the choices of the movement's leaders as well.
   After Bolotnaia, internal struggles and ineffective issue framing by
   movement leaders led to failures to attract new sympathizers. Moreover,
   their repeated focus on elections not only made it more difficult for
   key social movement organizations to maintain cooperative ties, but also
   contributed to further legitimizing the Putin regime. These choices
   served to limit the lasting impact of the movement.},
