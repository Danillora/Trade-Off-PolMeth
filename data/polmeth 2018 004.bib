@article{ WOS:000439948200009,
Author = {Bozoki, Andras and Hegedus, Daniel},
Title = {Hybrid regimes under external control On the nature of Hungarian
   politics},
Journal = {OSTEUROPA},
Year = {2018},
Volume = {68},
Number = {3-5},
Pages = {149+},
Abstract = {Hungary's political system is neither a democracy nor a dictatorship. It
   is instead a specific hybrid regime. The government under Viktor Orban
   has largely put paid to the separation of powers that characterised the
   liberal constitutional state. However, to date, the basic rights of
   Hungarian citizens have remained intact. This is due to Hungary's
   membership of the European Union and its seat on the Council of Europe.
   However, the role of the EU is ambivalent. Its legal system is designed
   to prevent the establishment of an authoritarian system. At the same
   time, EU funds are helping to stabilise the regime in Budapest. Since
   the EU is regarded as an association of democratic states, Orban is also
   using Hungary's membership of the EU to legitimise his regime.},
