@article{ WOS:000432225100014,
Author = {Kahan, Alan S.},
Title = {AND WHAT IF TOCQUEVILLE WAS WRONG?},
Journal = {TOCQUEVILLE REVIEW},
Year = {2018},
Volume = {39},
Number = {1},
Pages = {235-244},
Abstract = {What if Tocqueville was wrong and the coming of democracy was not a
   Providential Fact but a mere blip in humanity's aristocratic history?
   What if the idea that all people are basically equal is already on its
   way out? Education was supposed to be the great equalizer, but now it
   divides the gentry from the plebs. The revolution in gender roles was
   supposed to be about greater equality, but the result of birth control
   pills and professional women marrying professional men has been a
   concentration of wealth in small families. The great future struggle
   will be between the new international aristocracy of education and
   wealth and the nation-state. Liberal democracy will be the loser.},
