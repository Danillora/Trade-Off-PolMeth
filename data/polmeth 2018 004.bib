@article{ WOS:000443917500007,
Author = {Stringer, Johnny},
Title = {Changing Character, Changing Context: Enhancing Operational Airpower
   Employment in the Royal Air Force's Second Century},
Journal = {RUSI JOURNAL},
Year = {2018},
Volume = {163},
Number = {3},
Pages = {36-42},
Abstract = {As the RAF celebrates its centenary, the changing character of conflict
   and a rapidly changing context within which airpower can, is and will be
   employed offers profound challenges and opportunities. Established
   technologies and capabilities - many associated with and honed since the
   first Gulf War of 1990- 91 - have been joined by exponential development
   in space, cyber, artificial intelligence and the information
   environment. Recent operations in Afghanistan, Libya and the Gulf have
   reflected this, yet have also highlighted the competitive nature of the
   geopolitical, societal, legal, ethical and reputational contexts within
   which airpower is employed. In this article, Johnny Stringer draws on
   his own experience at the tactical and operational levels of UK, joint
   and coalition operations. He examines those contexts to provide pointers
   to the rapid evolutionary changes required to enhance what integrated
   airpower can provide, and the potential consequences of inaction or
   timidity. In particular, he articulates changing ways in air warfare -
   access, targeting, non- kinetic and information/ influence operations -
   and the need to move beyond technology and tactics to fully exploit the
   conceptual component of air and space fighting power to the full.},
