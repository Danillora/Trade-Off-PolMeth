@article{ WOS:000443917500008,
Author = {Blount, Clive},
Title = {Useful for the Next Hundred Years? Maintaining the Future Utility of
   Airpower},
Journal = {RUSI JOURNAL},
Year = {2018},
Volume = {163},
Number = {3},
Pages = {44-51},
Abstract = {In order to investigate how airpower might maintain its position as
   central in the minds of strategic decision-makers, Clive Blount examines
   the fundamental attributes and roles of airpower, looking into the
   future, and discusses the continuing strategic utility of airpower.
   After considering potential technological advances, he then turns to the
   strategic interface and, in the face of enemies that are likely to adopt
   a hybrid warfare' strategy, he proposes three new fundamental properties
   to which airpower must aspire if it is to retain its historical
   strategic utility - those of availability, affordability and
   acceptability.},
