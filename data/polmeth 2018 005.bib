@article{ WOS:000482710300003,
Author = {Ramirez Macias, Jose de Jesus and Hernandez Martinez, Cuauhtemoc N.},
Title = {Spatial approaches to biopolitics and governmentality in territorial key},
Journal = {REVISTA MEXICANA DE ANALISIS POLITICO Y ADMINISTRACION PUBLICA},
Year = {2018},
Volume = {7},
Number = {2},
Pages = {35-57},
Month = {JUL-DEC},
Abstract = {In this paper we propose to carry out an exploratory review of the
   biopolitical and governmental concepts formulated by Michel Foucault,
   based on a spatial and territorial key reading. In particular, space
   management is approached in the thread of Foucault's analysis, to
   understand how space becomes a core element in the deployment of
   ``security devices{''} in rights and social events such as leprosy, the
   plague and smallpox, which have been seen and evidenced the different
   ways of managing the spaces.
   This mark of reading reminds us of the centrality of space management in
   the contemporary exercise of power in state and economic configurations.
   We take on the task of probing and advancing some reflections on the
   architecture of land management in contemporary times.},
