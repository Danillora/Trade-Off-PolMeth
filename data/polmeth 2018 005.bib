@article{ WOS:000436854700006,
Author = {Tomini, Luca and Wagemann, Claudius},
Title = {Varieties of contemporary democratic breakdown and regression: A
   comparative analysis},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
Year = {2018},
Volume = {57},
Number = {3},
Pages = {687-716},
Month = {AUG},
Abstract = {The goal of this article is to understand which combinations of
   explanatory conditions account for the qualitative differences within
   forms of democratic breakdown (i.e., transition from democracy to a
   hybrid or authoritarian regime) and democratic regression (i.e.,
   transition within democracy through a loss of democratic quality). The
   analysis focuses particularly on the specific features of those
   processes of change ending up with a transition from democratic rule,
   compared to those producing a simple loss of democratic quality within
   the democratic regime. Applying two-step fuzzy-set Qualitative
   Comparative Analysis (fsQCA), the study aims to integrate different
   types of explanatory factors, offering a fresh and comprehensive
   perspective on this phenomenon.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
