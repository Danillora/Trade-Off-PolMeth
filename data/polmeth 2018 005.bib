@article{ WOS:000433182500009,
Author = {Megret, Frederic},
Title = {Bin Laden: Tale of a Death Foretold},
Journal = {JOURNAL OF GENOCIDE RESEARCH},
Year = {2018},
Volume = {20},
Number = {2, SI},
Pages = {290-304},
Abstract = {The death of Bin Laden was the highly intentional result not only of US
   efforts, but also of an international legal regime that could not
   tolerate what he stood for. What legally justified the execution of Bin
   Laden for US authorities? International human rights law and
   international humanitarian law have failed to fully answer this
   question. Bin Laden was neither ``just{''} a criminal nor merely an
   adversary in the ``war on terror.{''} Rather, the Al-Qaida leader was
   constructed as the ultimate enemy of sovereignty, a scourge that only
   the sovereign's sword could purge. From there, a biopolitical logic
   paved the way for his execution: a spectacular reassertion of statehood
   through violence, couched in a novel form of legal normativity.},
