@article{ WOS:000438330400002,
Author = {Marszalek-Kawa, Joanna and Plecka, Danuta},
Title = {POLITICAL KNOWLEDGE AS THE BASIS FOR BUILDING MUTUAL RELATIONS IN THE
   INTERNATIONAL ARENA. THE CASE OF POLAND AND AZERBAIJAN},
Journal = {EUROPEAN JOURNAL OF TRANSFORMATION STUDIES},
Year = {2018},
Volume = {6},
Number = {1},
Pages = {20-29},
Abstract = {Knowledge of politics is the basic determinant of political civic
   competences. In the contemporary world, citizens do not know a lot about
   politics, which affects their decisions, e.g. when voting in elections.
   The acquisition of political knowledge shapes political thinking, based
   on a specific set of views, which make up the so called mind map or mind
   model.
   The aim of our research is to identify the influence of political
   knowledge on the quality of Polish-Azerbaijani relations. The study was
   carried out between October 2017 and March 2018 and is one of the
   outcomes of our research internship at Baku Slavic University.
   The research shows that the respondents agree only on a few specific
   issues, such as trust in politicians or the importance of family in
   life, but demonstrate considerable differences when it comes to basic
   knowledge of their partner country and in their attitude to tradition or
   gender roles. Therefore, the incompatibility of answers in this sphere
   makes it very difficult to build good social and political relations
   between Azerbaijan and Poland.},
