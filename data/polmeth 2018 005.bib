@article{ WOS:000438918400007,
Author = {Feola, Giuseppe},
Title = {Against indifference: a call for civil engagement in postconflict in
   Colombia},
Journal = {CIUDAD PAZ-ANDO},
Year = {2018},
Volume = {11},
Number = {1},
Pages = {51-61},
Month = {JUN},
Abstract = {The peace agreement of 2016 was received with substantial indifference
   by a large part of the Colombian population. This article suggests that
   the aforementioned indifference is related to rooted fractures in the
   Colombian Society. Four thesis against indifference are put as follows:
   (i) the limitations of the peace agreement do not alter its potential
   for change; (ii) pressure from the civil society and the public can hold
   those in power accountable for the realisation of the peace agreement;
   (iii) the civil society and the public can appropriate the peace process
   and have a positive incidence in the peacebuilding; (iv) peacebuilding
   is the opportunity of not only to build peace, but also to newly shape a
   more diverse, inclusive, democratic, dignifying and
   environmentally-conscious Colombia.},
