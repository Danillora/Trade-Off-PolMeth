@article{ WOS:000433949600003,
Author = {Bruschi, Fabio},
Title = {Splitting Science: The Althusserian Interpretation of Capital's
   Multiple Orders of Exposition},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2018},
Volume = {30},
Number = {1},
Pages = {25-43},
Abstract = {In his 1978 preface to Gerard Dumenil's Le concept de loi economique
   dans ``Le Capital,{''} Louis Althusser rectified the conception of
   Capital's revolutionary form of scientificity that he had put forth in
   For Marx and Reading ``Capital.{''} According to these works, Capital's
   form of scientificity rests on the systematicity of its order of
   exposition. This systematicity allows Marx to transform ideological
   notions in order to understand how they foreclose the contingent and
   conflictual structuring of the capitalist mode of production. In 1978,
   Althusser found the condition of possibility of this theoretical
   operation not in Capital's systematicity but in the coexistence of other
   orders of exposition along with Capital's ``major{''} order of
   exposition. These other orders of exposition, by interrupting the
   systematicity of the major order of exposition and forcing its
   constitution to take place through a series of discontinuities,
   continuously detach it from the ideology that constitutes its starting
   point.},
