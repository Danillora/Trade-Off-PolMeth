@article{ WOS:000419564000003,
Author = {Matthews, Felicity},
Title = {Majoritarianism Reinterpreted: Effective Representation and the Quality
   of Westminster Democracy},
Journal = {PARLIAMENTARY AFFAIRS},
Year = {2018},
Volume = {71},
Number = {1},
Pages = {50-72},
Month = {JAN},
Abstract = {Comparative analyses have frequently cast the UK as a paradigm of
   majoritarianism, wherein a power-hoarding executive dominates parliament
   and policymaking. Yet, this article contends that existing studies have
   paid insufficient attention to the opportunities for opposition parties
   to affect policymaking via the legislative arena; and applies a refined
   version of Powell's index of effective representation to map the
   institutional conditions that structure policy payoffs. This analysis
   demonstrates that reforms to shift the balance between government and
   parliament have served to offset the declining vote basis of government,
   and have in turn ensured that Westminster remains effectively responsive
   to a majority of the electorate.},
