@article{ WOS:000451166300006,
Author = {Oyarzun Serrano, Lorena},
Title = {The Pacific in Chile's Foreign Policy: A Tool to Reinforce Open
   Regionalism},
Journal = {LATIN AMERICAN POLICY},
Year = {2018},
Volume = {9},
Number = {2},
Pages = {282-303},
Month = {DEC},
Abstract = {The reconfiguration of power hierarchies has transformed the Pacific
   Rim, mainly China, into one of the most-dynamic and fastest-growing
   regions. In line and consistent with its international-insertion
   strategy based on open regionalism, Chile has developed commercial links
   with Pacific countries, including the United States, China, Japan, South
   Korea, Australia, Canada, and Vietnam. It also participates fully in the
   Pacific Alliance-a regional cooperation mechanism that includes
   Colombia, Mexico, and Peru, and whose objective is to promote the
   development of its members while expanding and consolidating ties to the
   Pacific. In this article, I analyze the meaning of ``The Pacific{''}
   from a historical perspective, and of the Pacific Alliance, focusing on
   Chile's foreign policy. I argue that ``The Pacific{''} is a strategic
   construct created to strengthen the opening, diversification, and
   economic liberalization of the country. In the commercial sphere, it
   becomes a soft-balancing mechanism in response to protectionist ideas.
   In this argumentative line, and in the face of an uncertain commercial
   scenario, the Pacific Alliance is also a useful platform to defend the
   idea that free trade strengthens collective economic security.},
Publisher = {WILEY},
