@article{ WOS:000436182200005,
Author = {Mufti, Mariam},
Title = {What Do We Know about Hybrid Regimes after Two Decades of Scholarship?},
Journal = {POLITICS AND GOVERNANCE},
Year = {2018},
Volume = {6},
Number = {2},
Pages = {112-119},
Abstract = {In two decades of scholarship on hybrid regimes two significant
   advancements have been made. First, scholars have emphasized that the
   hybrid regimes that emerged in the post-Cold War era should not be
   treated as diminished sub-types of democracy, and second, regime type is
   a multi-dimensional concept. This review essay further contends that
   losing the lexicon of hybridity and focusing on a single dimension of
   regime type-flawed electoral competition-has prevented an examination of
   extra-electoral factors that are necessary for understanding how regimes
   are differently hybrid, why there is such immense variation in the
   outcome of elections and why these regimes are constantly in flux.
   Therefore, a key recommendation emerging from this review of the
   scholarship is that to achieve a more thorough, multi-dimensional
   assessment of hybrid regimes, further research ought to be driven by
   nested research designs in which qualitative and quantitative approaches
   can be used to advance mid-range theory building.},
