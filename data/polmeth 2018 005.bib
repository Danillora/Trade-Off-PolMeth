@article{ WOS:000433182500006,
Author = {Patin, Nicolas},
Title = {Atoning for the Murder of Millions? The Execution of High-Ranking Nazis
   after the Second World War},
Journal = {JOURNAL OF GENOCIDE RESEARCH},
Year = {2018},
Volume = {20},
Number = {2, SI},
Pages = {247-260},
Abstract = {At the end of the Second World War, the Nazi atrocities raised some very
   tangible problems. How should those responsible for crimes on this scale
   be judged? What means should be employed to execute those criminals who
   were sentenced to death? What should be done subsequently with their
   bodies? Those high-ranking Nazis who stood trial were for the most part
   executed in public, despite this form of public spectacle having largely
   disappeared in Europe. Taking Foucault's notion of the ``political
   technology of bodies{''} as its main starting-point, this article
   analyses the mechanisms put in place after 1945 that were intended to
   create the feeling that a new order had been founded and the crimes of
   these perpetrators atoned for.},
