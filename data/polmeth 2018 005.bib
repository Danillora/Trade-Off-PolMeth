@article{ WOS:000448682000003,
Author = {Malik, Ali},
Title = {POLICE GOVERNANCE AND ACCOUNTABILITY IN SCOTLAND FOLLOWING REFORM:
   REVISITING THE POLICY RATIONALE FOR THE CREATION OF THE SCOTTISH POLICE
   AUTHORITY},
Journal = {SCOTTISH AFFAIRS},
