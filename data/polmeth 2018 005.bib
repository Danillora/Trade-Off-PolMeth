@article{ WOS:000438648100002,
Author = {Moskop, Wynne Walker},
Title = {Jane Addams and Possibilities for Transnational Political Friendship},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2018},
Volume = {7},
Number = {3},
Pages = {400-431},
Month = {SUM},
Abstract = {Jane Addams was a first-rate practitioner of political friendship whose
   model is useful for exploring possibilities for transnational political
   friendships. She and other residents of the Hull House settlement worked
   for more than 40 years in their diverse immigrant neighborhood on
   Chicago's South Side to move beyond class divisions and build friendly
   cosmopolitan communities to address common civic problems. Her
   experiential accounts detail the processthe epistemological and
   structural changesthrough which friendships develop among diverse
   persons and groups that are unequal in power and resources. These
   accounts expand notions of political friendship beyond current
   applications to democratic citizens and provide insights into an area
   neglected by democratic theoristspossibilities for political friendship
   among unequal parties in transnational market relations.},
