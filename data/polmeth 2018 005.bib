@article{ WOS:000451783500002,
Author = {Van den Heede, Pieter and Ribbens, Kees and Jansz, Jeroen},
Title = {Replaying Today's Wars? A Study of the Conceptualization of Post-1989
   Conflict in Digital ``War{''} Games},
