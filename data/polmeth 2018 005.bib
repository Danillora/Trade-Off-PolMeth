@article{ WOS:000446846200006,
Author = {Hopkins, Daniel J. and Pettingill, Lindsay M.},
Title = {Retrospective Voting in Big-City US Mayoral Elections},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2018},
Volume = {6},
Number = {4},
Pages = {697-714},
Month = {OCT},
Abstract = {Retrospective voting is a central explanation for voters' support of
   incumbents. Yet, despite the variety of conditions facing American
   cities, past research has devoted little attention to retrospective
   voting for mayors. This paper first develops hypotheses about how local
   retrospective voting might differ from its national analog, due to both
   differing information sources and the presence of national benchmarks.
   It then analyzes retrospective voting using the largest data set on
   big-city mayoral elections between 1990 and 2011 to date. Neither crime
   rates nor property values consistently influence incumbent mayors' vote
   shares, nor do changes in local conditions. However, low city-level
   unemployment relative to national unemployment correlates with higher
   incumbent support. The urban voter is a particular type of retrospective
   voter, one who compares local economic performance to conditions
   elsewhere. Moreover, these effects appear to be present only in cities
