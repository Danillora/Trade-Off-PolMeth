@article{ WOS:000445101800006,
Author = {Mattila, Mikko and Wass, Hanna and Lahtinen, Hannu and Martikainen,
   Pekka},
Title = {Sick leave from work and the voting booth? A register-based study on
   health and turnout},
Journal = {ACTA POLITICA},
Year = {2018},
Volume = {53},
Number = {3},
Pages = {429-447},
Month = {JUL},
Abstract = {Previous studies show that people with poor health have a lower
   propensity to vote. With individual-level register data on sickness
   allowance episodes and voting in three Finnish elections, we address the
   following questions: (1) What degree of sickness allowance days
   negatively influences turnout? (2) Are sickness absences on election day
   more harmful than absences that occur before the elections? (3) What is
   the effect of cumulative sickness allowance spells before the elections
   over a period of several years? We use a threefold categorisation
   approach, which differentiates between immediate, short-term and
   long-term health effects on voting. The results show that multiple
   sickness allowance spells over several years are more strongly connected
   to turnout than health problems experienced only in the year prior to
   the elections. Falling ill at the time of the elections had no
   consistent additional negative relationship with voting. We suggest that
   the demobilising effects of immediate health problems are associated
   with tangible factors, while long-term effects are related to lowered
   levels of political efficacy, interest and social connectedness.},
