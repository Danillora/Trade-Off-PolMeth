@article{ WOS:000453432300012,
Author = {Meloni Gonzalez, Carolina Natalia},
Title = {The limits of the thinkable: Subject, Grief and Melancholy in Judith
   Butler},
Journal = {POLITICA Y SOCIEDAD},
Year = {2018},
Volume = {55},
Number = {3},
Pages = {893-911},
Abstract = {The text analyses Judith Butler's political theory through the
   intersection of the concepts of the subject, the political community and
   the grievance process. In order to do so, the political ontology of
   vulnerability will be analyzed, focusing on Butler's thesis on the
   uneven distribution of the possibility to grief. For Butler, this
   inequality will enable us to analyze and understand the dimension live
   acquires in a political community, as well as its limits and emptiness.
   The political related proposal outlined in Butler's later texts that
   focus on rethinking the concept of melancholy as well as its
   politization will also be analyzed.},
