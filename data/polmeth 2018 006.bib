@article{ WOS:000428280900004,
Author = {Pillay, Suren},
Title = {Thinking the State from Africa: Political Theory, Eurocentrism and
   Concrete Politics},
Journal = {POLITIKON},
Year = {2018},
Volume = {45},
Number = {1, SI},
Pages = {32-47},
Note = {National Conference on Decolonization after Democracy, S African Assoc
   Polit Studies, Cape Town, SOUTH AFRICA, AUG 30-SEP 02, 2016},
Organization = {Univ Western Cape},
Abstract = {This paper is divided into two parts. In the first section, I describe a
   project that we have underway at University of Western Cape, to rethink
   Political Theory and Philosophy. It is a project partly responsive to
   the questions that have been raised over the last two years in South
   African universities, about rethinking the curriculum and decolonizing
   knowledge'. The second part of this paper will offer a description of a
   course that I have been teaching at Honours and Masters level on
   Political Violence and the Modern State. In recounting these two
   projects I wish to contribute to possible ways we might reconstitute the
   genealogy of the modern state from an African vantage point, and
   therefore to think about how this might be done in a way that is not
   Eurocentric.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
