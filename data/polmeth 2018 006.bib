@article{ WOS:000452845800003,
Author = {Fedorov, G. M.},
Title = {RUSSIAN FEDERATION IN THE BALTIC REGION: POLITICAL RELATIONS AND
   ECONOMIC DEVELOPMENT IN 1992-2017},
