@article{ WOS:000430490000011,
Author = {Pickett, Brent},
Title = {The New Paternalists},
Journal = {POLITY},
Year = {2018},
Volume = {50},
Number = {2},
Pages = {300-329},
Month = {APR},
Abstract = {A common theoretical framework underpins the work of the recent wave of
   paternalism defenders. While unacknowledged, authors who invoke
   libertarian paternalism, coercive paternalism, asymmetric paternalism,
   and other positions share fundamental, common assumptions and approaches
   that break with earlier defenses of paternalism, which were grounded
   primarily in natural law theory and liberal perfectionism. This break is
   sharp enough that it makes sense to speak of a new paternalism: a
   framework that avoids any comprehensive account of the human good;
   relies strongly on behavioral economics and psychology to justify its
   favored interventions; and uses a utilitarian calculus to evaluate when
   regulations, or rather nudges, are appropriate. After describing this
   framework, this article examines whether the new paternalism marks any
   advance over the old.},
