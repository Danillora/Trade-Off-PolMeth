@article{ WOS:000468863000011,
Author = {Villaplana Jimenez, F. Ramon},
Title = {A theoretical proposal for analyzing contemporary political parties'
   behavior},
Journal = {REFLEXION POLITICA},
Year = {2018},
Volume = {20},
Number = {40},
Pages = {138-151},
Month = {JUL-DEC},
Abstract = {To explain political parties behavior solely based on external factors,
   or exclusively on internal dynamics, isn't enough to understand the real
   causes of their actions. The current proposal considers the political
   party as a microsystem inside of the party system, which in turn is to
   be considered as a part of the political system as a whole. These three
   systems are constantly changing as a result of the feedback that happens
   between themselves and the internal dynamics that happen in each one of
   them. Thanks to this analisis model integrating pre-existing theories,
   we can come to a more satisfying understanding of party behaviour in the
   multiple competitive contexts of contemporary politics. This proposal
   includes identifying the multiple systemic elements to be considered.},
