@article{ WOS:000440698800001,
Author = {Novkov, Julie},
Title = {How Do We Solve a Problem Like the Donald? The Democratic Challenge of
   Trump Supporters and the Politics of Presidential Removal},
Journal = {NEW POLITICAL SCIENCE},
Year = {2018},
Volume = {40},
Number = {3},
Pages = {439-458},
Abstract = {Political observers have debated whether and how to remove Donald J.
   Trump from the office of the presidency. This article explains the
   difficulties associated with both the Twenty-Fifth Amendment's
   incapacity route and impeachment. These difficulties illuminate a larger
   underlying problem with American democracy that the Trump presidency
   both crystallizes and reinforces: the emergence of an energized core of
   political participants who unite around racialized identity and reject
