@article{ WOS:000444731900007,
Author = {Azzara, Stefano G.},
Title = {Left-Wing Nietzscheanism in Italy: Gianni Vattimo},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2018},
Volume = {30},
Number = {2},
Pages = {275-290},
Abstract = {In 1974, Vattimo's ``Subject and the Mask{''} proposed a
   ``revolutionary{''} reading of Nietzsche, presented as an
   anarcho-libertarian prophet who intended to break down the totalitarian
   ratio of Platonic and modern/Cartesian metaphysics and, above all, the
   hierarchical social order stabilized in the capitalist state. Hence came
   the idea of combining Nietzsche with Marx, soliciting a dialogue between
   bourgeois intelligentsia (students and radicalized professors) and a new
   worker movement, postlaborist and post-Hegelian (Antonio Negri's mass
   worker). In the 1980s, however, the development of ``weak thought{''}
   coincided with an understanding of the absurdity of any
   ``revolutionary{''} and ``Marxist{''} reading of Nietzsche. This
   understanding is above all the recognition of the end of every project
   of political transformation in a period in which terrorism was revealing
   a strict relationship between dialectics and violence. This essay
   proposes a reading of the evolution of Vattimo's thought.},
