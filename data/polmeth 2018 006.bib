@article{ WOS:000444731900009,
Author = {Day, James},
Title = {On Broken Ground},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2018},
Volume = {30},
Number = {2},
Pages = {307-323},
Abstract = {Following a brief review of dissident Marxist art historian T. J.
   Clark's Picasso and Truth, this essay asks whether the techniques
   developed by certain visual artists and writers over the course of the
   last century might be applied in Marxist critique. By returning to
   debates on realism in Europe in the mid 1930s and subsequent work by
   Marxist art historians, the essay makes the case for the use of
   modernist-like deformation in contemporary Marxist critique. Realist
   engagement is seen as compatible with-indeed, as
   necessitating-experimentation with form, something recognized in the
   recent revival of interest in Gyorgy Lukacs.},
