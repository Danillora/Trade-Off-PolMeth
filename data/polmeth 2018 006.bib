@article{ WOS:000442596300014,
Author = {Griggs, Steven and Howarth, David},
Title = {The Airports Commission, the Dilemmas of Political Leadership and the
   Third Runway at Heathrow Airport},
Journal = {POLITICAL QUARTERLY},
Year = {2018},
Volume = {89},
Number = {3},
Pages = {434-445},
Month = {JUL-SEP},
Abstract = {This article assesses the character, role and outcomes of the Airports
   Commission. Analysing its workings from September 2012, it evaluates the
   final recommendations and then charts their subsequent public reception.
   The article claims that the Airports Commission's endeavours to
   depoliticise aviation by using reasonable' methods and impartial
   judgementsoften embodied in Howard Davies himselfhave been met with
   local resistance and political opposition, focussed on the proposal to
   expand either Heathrow or Gatwick. It exposes how the recourse to expert
   commissions offers only temporary respite for government responsibility
   and accountability in the making of hard decisions. It concludes that
   the inability to secure a binding and acceptable agreement does not just
   reside at the door of the Airports Commission, but rests also on the
   failures of political leadership and the missed opportunity' to
   articulate a sustainable vision for aviation after the 2010 moratorium
   on airport expansion in the south-east of England.},
