@article{ WOS:000579878900021,
Author = {Senatus, Kenel},
Title = {THE DOMINICAN REPUBLICA AND THE ARBITRARY DEPRIVATION OF THE RIGHT TO
   NATIONALITY OF PERSONS OF HAITIAN ORIGIN IN THE JUDGMENT TC/0168/13},
Journal = {REVISTA INTERNACIONAL DE PENSAMIENTO POLITICO},
Year = {2018},
Number = {13},
Pages = {399-422},
Abstract = {In the Dominican Republic, the right to nationality of individuals that
   are born in that country and that are of Haitian origin is treated with
   discrimination. By means of a large reinterpretation of the expression
   aliens ``in transit{''}, the Constitutional Court judgment in case
   TC/0168/13 deprived arbitrarily a great number of persons of Haitian
   origin from their nationality, that they acquired by the principle of
   ius soli. They did so just because their parents did not have a legal
   migratory status. Considered incompatible with the standards of the
   international human rights law, this decision makes prevail the state
   sovereignty over the right to nationality. As a result, it gave rise to
   a crisis that is very difficult to solve.},
