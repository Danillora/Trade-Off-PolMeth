@article{ WOS:000455206300003,
Author = {Hogstrom, John},
Title = {does closeness matter for voter turnout in proportional systems? an
   examination at the local level in Sweden},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2018},
Volume = {17},
Number = {4},
Pages = {571-586},
Month = {DEC},
Abstract = {The purpose of this study is to examine whether citizens are more likely
   to vote in a political system that uses a proportional electoral system
   if the election is close. The results show that citizens who live in
   Swedish municipalities in which the competition between the two leading
   parties is close are more likely to vote. However, the results also show
   that the vote gap between the two major traditional blocs, the left and
   the right, is not important for citizens when they are considering
   whether or not to vote in municipal elections. In the study, a
   theoretical argument has been formulated which suggests that voters are
   confused and discouraged when there are many aspects to consider
   regarding the form of an upcoming coalition government. Accordingly,
   potential voters see the issue of how the coalition will be formed as
   too complex, and therefore the vote gap between the two blocs is not an
   issue that is prioritised by potential voters.},
Publisher = {PALGRAVE MACMILLAN LTD},
Address = {BRUNEL RD BLDG, HOUNDMILLS, BASINGSTOKE RG21 6XS, HANTS, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Högström, J (Corresponding Author), Mid Sweden Univ, Dept Social Sci, S-83125 Ostersund, Sweden.
