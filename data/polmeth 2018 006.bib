@article{ WOS:000451387500006,
Author = {Wagschal, Uwe},
Title = {Party politics and budget consolidation in german states (1992-2016)},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2018},
Volume = {12},
Number = {4, SI},
Pages = {703-723},
Month = {DEC},
Abstract = {The paper analyses the fiscal consolidation after the German
   unification. It focusses on the determinants for the different budget
   consolidation performance of the Lander since 1992. Different factors
   based on public policy theories are examined. Anew multidimensional
   consolidation indicator based on the primary balance and public debt is
   the dependent variable. Special focus is attributed on the
   parties-do-matter-theory. According to this theory different partisan
   complexion of governments should make adifference for budget
   consolidation. However, the empirical findings are rather weak for the
   partisan variables. The findings indicate in particular the importance
   of socio-economic causes. The higher the unemployment and the more
   welfare recipients in afederal state, the worse the consolidation
   balance. Acentral factor are interest rates, whose reductions over the
   past years contributed substantially to the consolidation success.
   Direct contributions from the Federal Government also played
   asignificant role.},
