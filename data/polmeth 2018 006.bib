@article{ WOS:000425469900006,
Author = {Bertelli, Anthony M. and McCann, Pamela J. Clouser},
Title = {Decentralizing Pork: Congressional Roll-Call Voting, Decentralized
   Administration, and Distributive Politics},
Journal = {LEGISLATIVE STUDIES QUARTERLY},
Year = {2018},
Volume = {43},
Number = {1},
Pages = {69-100},
Month = {FEB},
Abstract = {Congress packages pork-barrel spending in complicated proposals that
   belie theories of distributive politics. We theorize that roll-call
   voting on such bills depends on grant programs' administrative
   centralization, party ties with presidents or home-state governors, and
   differences in geographic representation between chambers. Analyzing
   votes between 1973 and 2010 using a within-legislator strategy reveals
   that House members are less likely to support decentralized spending
   when they are copartisans with presidents, while senators support
   decentralization regardless of such party ties. When House members or
   senators share affiliation with only governors or with neither chief
   executive, the likelihood of support rises with decentralization.},
