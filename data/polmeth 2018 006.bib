@article{ WOS:000456106000009,
Author = {Lebovic, Nitzan},
Title = {The History of Political Melancholy as an Alternative History of Zionism},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2018},
Volume = {30},
Number = {3, SI},
Pages = {462-483},
Abstract = {Understanding political melancholy as central to the crisis of modernity
   and democracy implies a growing realization that melancholy teaches us
   something essential about different forms of political crisis and their
   affective modes. This essay contends that the relationship between
   political melancholy in Weimar Germany and its repurposing by German
   Jews for Zionist thought reveals how political melancholy was and
   remains at the heart of Zionism. The essay offers both a historical and
   theoretical consideration of political melancholy. Its purpose is to
   question how a political affect of melancholy helps us grasp Zionism,
   offering a new way to think through its failures. More specifically, the
   growing attention, both critical and affirmative, paid to ``left-wing
   melancholy{''} is used to examine a general sense of loss and crisis in
   the West and the more concrete expression of this sense in the history
   of Zionism.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
