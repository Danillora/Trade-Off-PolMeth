@article{ WOS:000431598000006,
Author = {Knutson, Katherine},
Title = {From identity to issue: policy agenda and framing shifts within
   long-term coalitions},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2018},
Volume = {6},
Number = {2},
Pages = {281-302},
Abstract = {Recent research calls for increased attention to the development of
   interest group policy agendas, the effects of agenda shifts, and the
   process of issue framing. Some scholars argue that identity-based groups
   and coalitions have a particularly difficult time surviving policy
   agenda shifts. Drawing upon historical archives, interview data, and
   media coverage of a long-term, identity-based coalition, the Joint
   Religious Legislative Coalition (JRLC), this article examines how and
   why the group's policy agenda and framing of issues changed over time.
   The study finds that the JRLC's policy agenda and framing strategy
   changed from emphasizing policies identifying a specific target
   population to emphasizing policies with a broad or undefined target
   population. These shifts suggest a strategic response by the coalition
   both to changes in the external environment and to organizational
   challenges related to coalition maintenance.},
