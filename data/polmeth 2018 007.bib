@article{ WOS:000427492300007,
Author = {Moury, Catherine and Fernandes, Jorge M.},
Title = {Minority Governments and Pledge Fulfilment: Evidence from Portugal},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2018},
Volume = {53},
Number = {2},
Pages = {335-355},
Month = {APR},
Abstract = {In an age of rampant distrust and disaffection, pledge fulfilment is
   important for the quality of delegation between voters and elected
   officials. In this article, we make an empirical appraisal of pledge
   fulfilment in Portugal. Do Portuguese minority governments fulfil their
   pledges? How do they fulfil those pledges? What is the role of
   opposition parties? Using an original data set with over 3,000 electoral
   pledges for three Socialist governments, as well as interviews with
   former ministers and party leaders, our evidence suggests that: (1)
   minority governments fulfil at least as many pledges as their majority
   counterparts; (2) the main opposition party manages to extract the most
   policy benefits; and (3) economic conditions and cohabitation situations
   matter for pledge fulfilment.},
