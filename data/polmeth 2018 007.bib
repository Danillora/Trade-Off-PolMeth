@article{ WOS:000457607000001,
Author = {Yusupova, Guzel},
Title = {Making ethnic boundaries in the society of religious renaissance: Islam
   and everyday ethnicity in post-Soviet Tatarstan},
Journal = {NATIONAL IDENTITIES},
Year = {2018},
Volume = {20},
Number = {4},
Pages = {345-360},
Abstract = {This article investigates the conflict between closely intertwined
   specific ethnic and universal religious practices that affect the
   formation and maintenance of ethnic group boundaries in the society of
   simultaneous Islamic and ethnic renaissances: contemporary post-Soviet
   Tatarstan. I argue that the negotiation of this conflict produces both
   reinforcement and erosion of the titular ethnic group boundaries. I pay
   special attention on ethnicity performance and ethnicity consumption
   practices. Thus, I conclude that practices of performing and consuming
   ethnicity serve as effective mechanism of boundary formation not just
   between various ethnic groups in the multi-ethnic republic but also
   inside the group itself.},
