@article{ WOS:000438944800005,
Author = {Karakas, Oznur},
Title = {Gezi Assemblages: Embodied Encounters in the Making of an Alternative
   Space},
Journal = {STUDIES IN SOCIAL JUSTICE},
Year = {2018},
