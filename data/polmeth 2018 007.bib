@article{ WOS:000439983500005,
Author = {Engvall, Johan},
Title = {From Monopoly to Competition Constitutions and Rent Seeking in
   Kyrgyzstan},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2018},
Volume = {65},
Number = {4},
Pages = {271-283},
Abstract = {This article examines the relationship between constitutional design and
   the organization of corruption in Kyrgyzstan. It argues that
   constitutions shape patterns of corruption, particularly with regard to
   top appointments in lucrative state sectors. Under the presidential
   systems of former leaders Akaev and Bakiev the organization of public
   corruption took a monopolistic order, concentrated primarily around the
   presidential families. The constitutional changes of 2010 curbed
   presidential powers and introduced an empowered legislature. This led
   corruption to gravitate from the executive to political parties.
   Consequently, corruption has become more dispersed and competitive while
   concomitantly requiring brittle coalition deals to be made in order to
   handle the allocation of profitable public offices.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
