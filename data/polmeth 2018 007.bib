@article{ WOS:000454210900009,
Author = {Gizelis, Theodora-Ismene},
Title = {Systematic Study of Gender, Conflict, and Peace},
Journal = {PEACE ECONOMICS PEACE SCIENCE AND PUBLIC POLICY},
Year = {2018},
Volume = {24},
Number = {4, SI},
Month = {DEC},
Abstract = {This article reviews the literature on gender, conflict, and peace. In
   traditional security studies there was not much room for gender or
   gender equality, while feminist theorists have claimed most of the
   research on war and peace. The empirical research on gender, conflict,
   and peace is a relatively new sub-field that brings together diverse
   traditions from sociology, feminist theory, international relations, and
   economic development. The common ground of all researchers included in
   this short review is the effort to systematically understand the role of
   gender in shaping outcomes of conflict and peace. Despite the increasing
   number of articles and new datasets, I identify four areas that scholars
   must address for the research agenda to further grow, deepen, and
   develop as part of the mainstream study of peace and conflict: women's
   status and quality of peace, women's participation, sexual violence, and
   gender mainstreaming to promote gender equality in development and
   peace.},
