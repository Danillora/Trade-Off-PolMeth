@article{ WOS:000468863000004,
Author = {Arias Henao, Diana Patricia and Idrobo Velasco, Jhon Alexander},
Title = {Social crime, transitional justice and territory. Critical notes on the
   intervention of the ``Bronx{''} in Bogota},
Journal = {REFLEXION POLITICA},
Year = {2018},
Volume = {20},
Number = {40},
Pages = {40-49},
Month = {JUL-DEC},
Abstract = {Justice, transition, territory and crime. These four words have begun to
   demand new frames of understanding in Colombia. The article tries to
   discuss the possibilities and limits of its links from the perspective
   of the social sciences to think about the case of the Bronx intervention
   in Bogota, so that in this scenario it is possible to suggest what are
   the challenges that arise when thinking about scenarios of transition
   and post-war rehabilitation. Through some reflections of Foucault and
   some reports on the intervention of the Bronx, the article problematizes
   the way in which justice is thought, its relation with the territory and
   the way in which the concept of crime circulates, all this to produce a
   critical reflection on the models in which those conflicts are treated,
   others that derive from an uninterrupted war for five decades like the
   Colombian one.},
