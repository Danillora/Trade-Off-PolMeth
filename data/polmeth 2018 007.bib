Keywords = {Social policy; Welfare state; Retrenchment; Pensions; Unemployment;
   Partisan politics},
Keywords-Plus = {POLITICAL-PARTIES; SOCIAL-POLICIES; LABOR-MARKET; COUNTRIES;
