@incollection{ WOS:000441861700003,
Author = {Hussain, Imtiaz},
Book-Author = {Hussain, I},
Title = {Southern Transatlantic Flows: Silver Bullets},
Booktitle = {TRANSATLANTIC TRANSITIONS: BACK TO THE GLOBAL FUTURE?},
Series = {Global Political Transitions},
Year = {2018},
Pages = {29-49},
Abstract = {Using colonial silver trade across the transatlantic, this chapter
   validates an earlier argument of how much was happening across the ocean
   far before the World War II starting point in the relevant literatures.
   This temporal transition exposes a geographical counterpart: the heavier
   central Atlantic flows overshadowing the north Atlantic counterparts,
   necessitating an analytical shift. All three Pollack-Shaffer-Peterson
   indicators measuring transatlantic flows profited from this: even with
   their intimate global interlinks, transnational actors began facing
   newer governance forms; though an inter-governmental presence was
   absent, capturing how they evolved reinforces our analytical usage of
   the term, while trans-governmental action was also captured in many of
   its prototype forms. Ultimately, a-transatlantic sequence transgressing
   the Atlantic revealed (a) the invisible impacts of visible flows, (b)
   state genesis, (c) a rationale for delayed Latin democratization, and
   (d) the growth of intra-American relations.},
