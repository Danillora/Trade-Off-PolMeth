@incollection{ WOS:000441861700007,
Author = {Hussain, Imtiaz},
Book-Author = {Hussain, I},
Title = {Muslims in the `Transatlantic' Zone: Differing Stripes?},
Booktitle = {TRANSATLANTIC TRANSITIONS: BACK TO THE GLOBAL FUTURE?},
Series = {Global Political Transitions},
Year = {2018},
Pages = {157-184},
Abstract = {Testing ideas as a transatlantic flow, this chapter compares the
   mind-set over Muslims on both shores. After assessing how transatlantic
   countries established relations with Muslim countries/ colonies, such
   concurrent social policies as the treatment of the `hijab', as well as
   public opinion on policy issues pitting the United States with various
   West European countries, receive attention. The chapter's salient
   findings accent transitions as follows: (a) competition shifting from
   real estate (territory) to the belief system; (b) from legitimate
   actors, like the state, to non-legitimate counterparts, such as
   terrorist organizations; (c) dwarfing the transatlantic zone to the
   global; and from the psyche geared to facing predictable threats to one
   confronting the unpredictable. Identity is examined theoretically,
   elevating `constructivist' thinking, but without destroying the
   self-help instincts `realists' constantly find.},
