@article{ WOS:000430240600004,
Author = {Bartmann, Christoph},
Title = {``Surrounded by voices{''} Alexievich's literary monuments and the Nobel
   Prize},
Journal = {OSTEUROPA},
Year = {2018},
Volume = {68},
Number = {1-2},
Pages = {45-54},
Abstract = {When Svetlana Alexievich was awarded the Nobel Prize for Literature, a
   debate was ignited regarding the status of her works, with critics
   doubting that they could be classed as literature. The extended concept
   of literature postulated by the Swedish Academy in making its decision
   was in general not new. In this way, it served the purpose of the
   sponsor of the prize, whose aim was above all to honour ``idealistic{''}
   literature. Alexievich also represents a trend towards the softening of
   genre boundaries, towards ``autofiction{''} and ``life writing{''}. Her
   methods are journalistic, while the effect of her texts is literary. She
   takes collected testimonies and uses them to create monuments to an
   epoch and its people.},
