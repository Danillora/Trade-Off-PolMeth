@article{ WOS:000427490900002,
Author = {Schleiter, Petra and Voznaya, Alisa},
Title = {Party System Institutionalization, Accountability and Governmental
   Corruption},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2018},
Volume = {48},
Number = {2},
Pages = {315-342},
Month = {APR},
Abstract = {Why do repeated elections often fail to curb governmental corruption,
   even in full democracies? While much of the comparative literature on
   corruption focuses on the institutional features of democracies, this
   article argues that party system institutionalization is an additional
   and neglected factor in explaining why corruption may persist in the
   context of democratic elections. Under-institutionalized party systems
   impede accountability. They compromise the capacity of voters to
   attribute responsibility and undermine electoral co-ordination to punish
   incumbents for corruption. These expectations are tested by combining a
   controlled comparative study of eighty democracies around the world with
   an examination of the causal process in a case study of Panama. The
   findings suggest that party system institutionalization powerfully
   shapes the scope for governmental corruption.},
