@article{ WOS:000457244700007,
Author = {Canosa Usera, Raul},
Title = {The prohibition of torture and inhuman treatments in the ECHR},
Journal = {TEORIA Y REALIDAD CONSTITUCIONAL},
Year = {2018},
Volume = {42},
Pages = {247-271},
Abstract = {This is an analysis of one of the crucial articles of the ECHR that
   takes part of the core of non-derogable rights in the terms of Article
   15.2 of the Treaty. We have to admit that this provision does not
   completely protect personal integrity (for protecting the rest of the
   integrity the ECtHR uses Article 8), but a part of it in the face of the
   most serious attacks, and that it does so, not through the explicit
   recognition of a right but by establishing a prohibition from which is
   inferred, by reflection, a right that holders can finally defend in the
   European Court of Human Rights.},
