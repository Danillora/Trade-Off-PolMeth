@article{ WOS:000445660100009,
Author = {Blaydes, Lisa and Grimmer, Justin and McQueen, Alison},
Title = {Mirrors for Princes and Sultans: Advice on the Art of Governance in the
   Medieval Christian and Islamic Worlds},
Journal = {JOURNAL OF POLITICS},
Year = {2018},
Volume = {80},
Number = {4},
Pages = {1150-1167},
Month = {OCT},
Abstract = {When did European modes of political thought diverge from those that
   existed in other world regions? We compare Muslim and Christian
   political advice texts from the medieval period using automated text
   analysis to identify four major and 60 granular themes common to Muslim
   and Christian polities, and examine how emphasis on these topics evolves
   over time. For Muslim texts, we identify an inflection point in
   political discourse between the eleventh to thirteenth centuries, a
   juncture that historians suggest is an ideational watershed brought
   about by the Turkic and Mongol invaders. For Christian texts, we
   identify a decline in the relevance of religious appeals from the Middle
   Ages to the Renaissance. Our findings also suggest that Machiavelli's
   Prince was less a turn away from religious discourse on statecraft than
   the culmination of centuries-long developments in European advice
   literature.},
Publisher = {UNIV CHICAGO PRESS},
Address = {1427 E 60TH ST, CHICAGO, IL 60637-2954 USA},
Type = {Article},
Language = {English},
Affiliation = {Blaydes, L (Corresponding Author), Stanford Univ, Dept Polit Sci, Stanford, CA 94305 USA.
   Blaydes, Lisa; Grimmer, Justin; McQueen, Alison, Stanford Univ, Dept Polit Sci, Stanford, CA 94305 USA.},
DOI = {10.1086/699246},
ISSN = {0022-3816},
EISSN = {1468-2508},
Keywords = {comparative political theory; mirrors for princes; institutional
   divergence; Islamic political thought; text as data},
Keywords-Plus = {EUROPE; RELIGION; STATE; RISE},
Research-Areas = {Government \& Law},
