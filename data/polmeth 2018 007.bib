@article{ WOS:000437898600004,
Author = {Arevalo Hernandez, Jose Luis and Sosa Marquez, Jose Hernando},
Title = {Autonomy, Governability and Ethicity in the University: a Reflection
   from the Rational Reality of Hegel},
Journal = {ANALECTA POLITICA},
Year = {2018},
Volume = {8},
Number = {14},
Pages = {61-82},
Month = {JAN-JUN},
Abstract = {In the last decades, the changes of policies in Latin America have
   provoked alterations in the managing of universities, reducing their
   autonomy and leaving a governability deficit. These challenges generate
   a double legitimacy crisis: inner (foundations), and outer (State and
   organizations). In other words, disempowerment. This article is about
   the notion of university as an idea in its reality, and takes as an
   interpretative reference the objective spirit's dialectics exposed by
   Hegel in his philosophical system, proposing a critical-comprehensive
   approach of the idea developing in the autonomy, governability and
   ethicity moments, spotting the autoconsciousness of ethics in the game
   of governability.},
