@article{ WOS:000428671400007,
Author = {Zhang Yang and Tong, Sarah Y.},
Title = {Strong Domestic Consumption Key to Hong Kong's Robust Economic Growth in
   2017},
Journal = {EAST ASIAN POLICY},
