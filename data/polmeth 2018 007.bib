@article{ WOS:000453579400003,
Author = {Pardow Lorenzo, Diego and Carbonell Bellolio, Flavia},
Title = {Searching for the ``Median Judge{''}: A Study of Coalition Formation in
   the Third Chamber of the Chilean Supreme Court},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2018},
Volume = {38},
Number = {3},
Pages = {485-505},
Abstract = {This work adapts the traditional methodology of dissent analysis for
   approaching to the judicial behavior of the Chilean Supreme Court
   between 2009 and 2013, particularly its Third Section. Although the high
   proportion of unanimous decisions (90\% of the total) and the huge
   workload (500 cases per judge) generate limitations, our analysis finds
   robust coalitions in an environment of variability with over 10
   different compositions per year in the same court. Within this period,
   the behavior of the court seems dominated by the coalition of justices
   Pierry, Carreno and Araneda, who form the majority when the court
   splits. Regarding the dissenting justices, the first part of the period
   features an opposition with Brito, and the second with Munoz. Whereas
   both justices appear in a solitary position as the minority, Pierry
   seems to behave as a ``median justice.{''}},
