@article{ WOS:000452786300004,
Author = {Steinmetz-Jenkins, Daniel},
Title = {Stanley Hoffmann's Critique of Hans Morgenthau's Political Realism},
Journal = {TOCQUEVILLE REVIEW},
Year = {2018},
Volume = {39},
Number = {2},
Pages = {63-76},
Abstract = {This article begins by arguing that Raymond Aron's interest in the
   burgeoning field of international relations theory was fundamentally
   motivated by the attempt to provide an alternative to Hans Morgenthau's
   Politics Among Nations. Throughout his book Paix et guerre Aron
   expressed a consistent worry that Morgenthau's political realism was too
   influenced by a dangerous strain of German power politics which he
   identified with the thought of Carl Schmitt. The article, then, suggests
   that Morgenthau, frustrated by Aron's accusations, perhaps played a key
