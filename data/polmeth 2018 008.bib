@article{ WOS:000426004800003,
Author = {Chamberlain, Adam},
Title = {Moral Suasion and Political Action},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2018},
Volume = {7},
Number = {1},
Pages = {57-85},
Month = {WIN},
Abstract = {Moral suasion played an important role in the abolition and temperance
   movements, yet scholarship provides no guidance on how to systematically
   evaluate its place in nineteenth- and early twentieth-century reform
   efforts. Here this is addressed by studying moral suasion's use in
   justifying social and political actions. By using the perspectives of
   prominent groups and leaders in the two movements to illuminate the
   nonpolitical and political motivations behind moral suasion, this
   article displays how its application varied from an end in itself, with
   no political goal, to a political tool, used in various ways to justify
   political reforms. Overall, this article places moral suasion squarely
   amid these two movements, revealing its variegated use by activists and
   how these different uses of it reflect liberal and republican thinking
   in the American polity.},
