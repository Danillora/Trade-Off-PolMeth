@article{ WOS:000468863000016,
Author = {Nunez Prado, Christian David},
Title = {Political Romanticism: The irony as a form of subjectivity and the
   harmful effects of the aestheticisation of politics},
Journal = {REFLEXION POLITICA},
Year = {2018},
Volume = {20},
Number = {40},
Pages = {209-219},
Month = {JUL-DEC},
Abstract = {Schmitt applied the method of political theology to demonstrate how
   romanticism moved the structure of the Kantian aesthetic judgment to all
   practical spheres, which enhances the role of the imagination as a
   creator of reality. This develops a kind of ironic subjectivity that is
   not committed with no ethical, moral or political value, for this
   reason, it distorts speeches in terms of truth and enduring commitments
   of ``truly{''} political subjectivity, which seeks through volitional
   affective membership a value, to transform the concrete reality. This
   ironic subjectivity is the prefiguration of a political activity
   associated with individualism, creativity, rejection of the whole fixed
   rule and institutional rigidity.},
