@article{ WOS:000423054100006,
Author = {Burzo, Stefano and Li, Xiaojun},
Title = {Public Perceptions of International Leadership in China and the United
   States},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Year = {2018},
Volume = {3},
Number = {1, SI},
Pages = {81-99},
Month = {MAR},
Abstract = {Many fear that with Trump taking the helm, the United States will scale
   back its international leadership role in global governance, leaving a
   void that is too big for any single country to fill. Others are hopeful
   that emerging powers such as China will be able to step in and provide
   international leadership to solve global governance challenges, from
   climate change to nuclear nonproliferation. In this study, we explore
   the Chinese and American publics' perceptions and views on international
   leadership in the Trump era. Results from two parallel surveys conducted
   in China and the United States shed light on how ordinary citizens in
   these two countries conceptualize international leadership and how their
   views contrast with conventional wisdom and with each other. Given the
   increasingly larger role played by public opinion in the foreign
   policies of both democratic and authoritarian countries, the findings of
