@article{ WOS:000449778500005,
Author = {Johnson, Austin P. and Meier, Kenneth J. and Carroll, Kristen M.},
Title = {Forty acres and a mule: housing programs and policy feedback for
   African-Americans},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2018},
Volume = {6},
Number = {4},
Pages = {612-630},
Abstract = {Inequality and a historical pattern of discrimination in housing policy
   have contributed to the level of trust between the U.S. government and
   the black community. Drawing upon the literature on policy feedback, we
   propose that policy effects can be race specific and that improved
   housing policies have a secondary effect of encouraging political
   participation at the local level in the black community. Using a
   supplement to the 2013 American Housing Survey, our findings provide
   evidence in favor of this theory.},
