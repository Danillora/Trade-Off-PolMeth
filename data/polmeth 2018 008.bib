@article{ WOS:000452053000004,
Author = {Brathagen, Kjersti},
Title = {From Global Ambition to Local Reality: Initiatives for the Dissemination
   of the Universal Declaration of Human Rights in Norway, 1948-1952},
Journal = {NORDIC JOURNAL OF HUMAN RIGHTS},
Year = {2018},
Volume = {36},
Number = {3, SI},
Pages = {237-251},
Abstract = {Dissemination of the Universal Declaration of Human Rights and its
   content started immediately upon its adoption in December 1948. This
   article is an exploration into how an early UNESCO initiative for what
   later was termed human rights education was central to dissemination
   efforts in Norway in the years 1948 to 1952. An underlying argument is
   that by shifting the scale of observation from the international level
   and onto the individual, we can gain new insights into early efforts to
   spread knowledge about human rights during the early Cold War. Based on
   archival material and open sources, it provides a glance into a rather
   understudied field of research for historians.},
