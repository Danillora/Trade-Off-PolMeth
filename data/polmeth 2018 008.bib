@article{ WOS:000450651500031,
Author = {Crafts, Nicholas},
Title = {Industrial Strategy: Policy Like It's 1964},
Journal = {POLITICAL QUARTERLY},
Year = {2018},
Volume = {89},
Number = {4},
Pages = {725-727},
Month = {OCT-DEC},
Abstract = {The remedies provided in Prosperity and Justice: a Plan for the New
   Economy are less convincing than its diagnosis. It entails a substantial
   shift back towards selective industrial policies and resource allocation
   by the state rather than the market, with the ambition of changing the
   structure of the economy. Many economists are likely to be sceptical of
   some of the arguments put forward in the report. An obvious issue is the
   huge policy discretion to be given to government without effective
   constraints, and the significance of government failure is overlooked.
   It is a triumph of hope over experience.},
