@article{ WOS:000423741800008,
Author = {Caligaris, Gaston},
Title = {Revisiting the Marxist debate over the `breakdown' of capitalism. A
   methodological criticism},
Journal = {IZQUIERDAS},
Year = {2018},
Volume = {39},
Pages = {182-208},
Month = {APR},
Abstract = {This paper offers a critical reconstruction of the Marxist controversy
   over the `breakdown of capitalism' developed between the last decades of
   the nineteenth century and the decade of 1930. In particular, it focuses
   on how each position in debate conceived the connection between
   capitalist economic development and the revolutionary action of the
   working class. Then, it is argued that the recovery of some modern
   investigations on the dialectical method allows to offer a novel
   alternative position to the debate.},
