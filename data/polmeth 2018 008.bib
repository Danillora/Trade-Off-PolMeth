@article{ WOS:000442466100005,
Author = {Jankowski, Michael and Lewandowsky, Marcel},
Title = {The AfD in the eighth European Parliament: an analysis of position
   shifts based on roll-call voting behavior between 2014-2016},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2018},
Volume = {12},
Number = {3},
Pages = {567-589},
Month = {SEP},
Abstract = {This article analyzes the voting patterns in the current 8th European
   Parliament (EP) and addresses two research questions. First, which
   dimensions structure the voting behavior in the EP? Second, how do the
   legislators of the AfD position themselves in this political space and
   how was their position affected by the internal split of the party?
   Based on spatial models, we find that the 8th EP is mainly structured by
   a divide on European Integration preferences and to a lesser extent by
   the left-right dimension. Legislators of the AfD clearly moved towards a
   more right-wing populist position over time, but this change mainly took
   place after the EPG change in 2016 and not after the internal split in
   May 2015. We discuss this finding.},
