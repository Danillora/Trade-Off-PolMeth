@article{ WOS:000432620200007,
Author = {Colonomos, Ariel},
Title = {Hostageship: What can we learn from Mauss?},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
Year = {2018},
Volume = {14},
