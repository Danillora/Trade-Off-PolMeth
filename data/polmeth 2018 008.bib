@article{ WOS:000459799400003,
Author = {Dufek, Pavel},
Title = {Democracy as Intellectual Taste? Pluralism in Democratic Theory},
Journal = {CRITICAL REVIEW},
Year = {2018},
Volume = {30},
Number = {3-4},
Pages = {219-255},
Abstract = {The normative and metanormative pluralism that figure among core
   self-descriptions of democratic theory, which seem incompatible with
   democratic theorists' practical ambitions, may stem from the internal
   logic of research traditions in the social sciences and humanities and
   from the conceptual structure of political theory itself. One way to
   deal productively with intradisciplinary diversity is to appeal to the
   idea of a meta-consensus; another is to appeal to the argument from
   cognitive diversity that fuels recent work on epistemic democracy. For
   different reasons, both strategies fail, such that a metatheoretical
   step-aside may be desirable, one that entails modeling democratic theory
   after the public justification approach.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
