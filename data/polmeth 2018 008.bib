@article{ WOS:000435215400003,
Author = {Amat, Dolores},
Title = {Leo Strauss and the Political Role of Knowledge in Modernity},
Journal = {ANACRONISMO E IRRUPCION},
Year = {2018},
Volume = {8},
Number = {14},
Pages = {69-91},
Month = {MAY-NOV},
Abstract = {The issue of the political role of knowledge is a fundamental issue in
   contemporary debates on political theory. All sources of authority and
   certainty called into question in our Time, knowledge becomes
   problematic and a traditional question re-emerges: What are the possible
   links between philosophy, science or academia, and public debate or
   politics in general? This paper analyzes Leo Strauss' understanding of
   the development of this question in Modernity and seeks to demonstrate
   that the author's work points out a pendular movement, an oscillation
   between omnipotence and impotence, between excessive confidence on the
   usefulness of science for political communities, and relativism, or the
   affirmation of the absolute incapacity of reason to produce any good for
   humanity.},
