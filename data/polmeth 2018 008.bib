@article{ WOS:000460088800002,
Author = {Blitstein, Pablo A. and Lemieux, Cyril},
Title = {How to reopen the question of modernity? Some proposals},
Journal = {POLITIX},
Year = {2018},
Volume = {31},
Number = {123},
Pages = {9-33},
Abstract = {Often considered in connection to evolutionism and sometimes accused of
   maintaining neocolonial domination, the field of studies on modernity
   and the processes of modernization has been progressively discredited in
   the social sciences over the last forty years. However, for some time
   now, a number of historians and sociologists have sought to reinvest
   this field on new bases. Rejecting both the miserabilist and populist
   approaches that have held modernity in their grip for too long, their
   work outlines a research program, whose consistency and main arguments
   are brought out by this article. We show that this program proposes a
   properly methodological definition of modernity-as the present of
   humanity-and imposes on researchers four major shifts: the refusal of
   stato-centrism; an attention to the practical foundations of different
   conceptions of ``modernity{''}; an adherence to the principle of
   contemporaneity; and finally, situating the generalizing ambition of
   social sciences within a rigorous search of processual analogies.},
