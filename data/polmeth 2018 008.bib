@article{ WOS:000468863000009,
Author = {Celis Estupinan, Carlos German and Pinzon Arteaga, Iris Aleida and
   Guevara Parra, America Celeste},
Title = {On the political dimension of memory, mourning and forgetfulness. An
   approach to peace construction},
Journal = {REFLEXION POLITICA},
