@article{ WOS:000438138900002,
Author = {Raimondi, Fabio},
Title = {⟪Accustomed to live in freedom⟫. War and Religion in the Machiavellian
   Constitution of Freedom},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
Year = {2018},
Volume = {30},
Number = {58},
Pages = {15-32},
Abstract = {This essay intends to fill some gaps in my L'ordinamento della liberty
   (2013) concerning, in particular, the role played by ``one's own
   arms{''} and ``religion{''} in the Machiavellian conception of freedom
   and its reality in the Florentine republic. The analysis confirms that
   freedom, according to Machiavelli, does not coincide with the
   absoluteness of the will, but with the ability to fight both with
   weapons and with religious symbols, also bringing into play their lives
   if necessary, within a defined context in order to transform it.},
