@article{ WOS:000436182200003,
Author = {Kennedy, John James and Liu, Hongyan and Nagao, Haruka},
Title = {Voting and Values: Grassroots Elections in Rural and Urban China},
Journal = {POLITICS AND GOVERNANCE},
Year = {2018},
Volume = {6},
Number = {2},
Pages = {90-102},
Abstract = {Authoritarian leaders often claim that they promote democratic
   institutions such as elections and democratic values. In China, the
   central propaganda often promotes the right and duty of citizens to vote
   in local elections as well as the importance of citizens' input into the
   policy making process. However, there is often a gap between government
   rhetoric and reality. In this article, we use the China General Social
   Survey (CGSS) 2013 to evaluate the determinants of voting in local
   elections and democratic values (attitudes) in rural and urban China.
   The results show that respondents with higher education tend to have
   lower levels of democratic values and participate less in local
   elections, but respondents with only compulsory education are more
   likely to display democratic orientations and vote. This suggests the
   relative success as well as the limits of authoritarian democratic
   propaganda.},
