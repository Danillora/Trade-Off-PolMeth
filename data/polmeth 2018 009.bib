@article{ WOS:000443851300002,
Author = {Langdridge, Darren and Parchev, Ofer},
Title = {Transgression and (sexual) citizenship: the political struggle for
   self-determination within BDSM communities},
Journal = {CITIZENSHIP STUDIES},
Year = {2018},
Volume = {22},
Number = {7},
Pages = {667-684},
Abstract = {There has been - and continues to be - a tension within the political
   strategies of sexual minority communities claiming citizenship. Whilst
   attempting to forge a political self-determination based on being
   (dissident) sexual subjects, members of sexually diverse communities
   have frequently engaged in political practices that normalize their
   diversity to accord with wider socio-cultural conventions. In this
   article, we address this issue in relation to the political strategies
   of one of the most marginalized sexual identities/practices: BDSM. By
   drawing on the work of Foucault, Rose, Rabinow and Bahktin, we advance a
   case for how it may be possible for dissident sexual communities to
   resist the normalizing effects of citizenship whilst still making claims
   for legal recognition and wider social acknowledgment. Key to the
   argument is the theorization of a position wherein carnival
   transgression operates within a dialectical integration of ideology and
   utopia as a mode of citizenship.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
