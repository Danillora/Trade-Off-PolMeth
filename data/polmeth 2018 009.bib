@article{ WOS:000451387500009,
Author = {Boehm, Katharina and Klinnert, Dorothea and Weidtmann, Jonas},
Title = {The Lander and the health insurances as negotiating partners: The
   variance in the implementation of the prevention act on the state level
   in Germany},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2018},
Volume = {12},
Number = {4, SI},
Pages = {763-781},
Month = {DEC},
Abstract = {The German Prevention Act of 2015 obliges the statutory health insurance
   funds to contract framework agreements with the Lander and the other
   social insurances. The agreements display ahigh variance in respect to
   the degree of commitment for binding joint action as well as in regard
   to the role of the Lander ministries. Using aframework based on the
   actor-centred institutionalism, we analyse actors' resources and
   orientations, and the interaction between health funds and ministries
   during the negotiation process in five Lander. The analysis reveals that
   the differences between the Lander primarily result from differing
   orientations for action of the ministries. Furthermore, we found
   differences in the interaction that also had an impact on negotiation
   outcomes. Ministries' orientations towards action and interaction were
   both influenced by their (missing) cooperation experiences with health
   funds. The ministries' orientations for action, furthermore, mirror the
   relevance of preventive policies in the respective Lander.},
