@article{ WOS:000417897800007,
Author = {Mendez, Fernando and Germann, Micha},
Title = {Contested Sovereignty: Mapping Referendums on Sovereignty over Time and
   Space},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2018},
Volume = {48},
Number = {1},
Pages = {141-165},
Month = {JAN},
Abstract = {The recent proliferation of referendums on sovereignty matters has
   fuelled growing scholarly interest. However, comparative research is
   hindered by the weaknesses of current compilations, which tend to suffer
   from conceptual vagueness, varied coding decisions, incomplete coverage
   and ad hoc categorizations. Based on an improved conceptualization and
   theory-driven typology, this article presents a new dataset of 602
   sovereignty referendums from 1776-2012, more than double the number in
   existing lists. In an exploratory analysis, it uncovers eight
   distinctive clusters of sovereignty referendums and identifies patterns
   of activity over time and space as well as outcomes produced.},
