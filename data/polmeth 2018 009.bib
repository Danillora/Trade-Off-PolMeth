@article{ WOS:000417897800003,
Author = {Crisp, Brian F. and Demirkaya, Betul and Schwindt-Bayer, Leslie A. and
   Millian, Courtney},
Title = {The Role of Rules in Representation: Group Membership and Electoral
   Incentives},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2018},
Volume = {48},
Number = {1},
Pages = {47-67},
Month = {JAN},
Abstract = {Existing research shows that the election of members of previously
   underrepresented groups can have significant consequences for
   policymaking. Yet, quotas, reserved seats, communal rolls, and
   race-conscious districting make it difficult to distinguish whether it
   is group membership, electoral incentives, or a combination of the two
   that matters. It is argued here that lawmakers who are members of
   underrepresented groups will stand out as defenders of their group's
   interests only when electoral rules incentivize them to do so. This is
   demonstrated empirically using data from New Zealand, showing that Mori
   Members of Parliament systematically vary in the extent to which they
   represent their ethnic group as a function of the three different sets
   of rules under which they were elected.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {32 AVENUE OF THE AMERICAS, NEW YORK, NY 10013-2473 USA},
Type = {Article},
Language = {English},
Affiliation = {Crisp, BF (Corresponding Author), Washington Univ, Dept Polit Sci, St Louis, MO 63130 USA.
   Crisp, Brian F.; Demirkaya, Betul; Millian, Courtney, Washington Univ, Dept Polit Sci, St Louis, MO 63130 USA.
   Schwindt-Bayer, Leslie A., Rice Univ, Dept Polit Sci, Houston, TX 77251 USA.},
DOI = {10.1017/S0007123415000691},
ISSN = {0007-1234},
EISSN = {1469-2112},
Keywords-Plus = {SUBSTANTIVE REPRESENTATION; POLITICAL REPRESENTATION; PROPORTIONAL
