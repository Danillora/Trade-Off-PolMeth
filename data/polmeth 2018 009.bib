@article{ WOS:000452198300011,
Author = {Allusson, Guillaume},
Title = {SOCIAL REPRESSION IN A DEMOCRACY: THE CASE OF TUNISIA},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2018},
Volume = {16},
Number = {4},
Pages = {131-142},
Month = {WIN},
Abstract = {Tunisia's remarkable democratization process in the wake of the Jasmine
   Revolution has not been followed by an improvement of civil liberties:
   Tunisia is still home to harsh repression on particular social
   behaviors, namely homosexuality and actions or speeches deemed insulting
   to religion. This article seeks to account for the apparent
   contradiction between the Tunisian newly free political environment and
   the socially repressive measures that persist in the country by arguing
   that the democratization of Tunisia further entrenches the repression of
   irreligious speech and of homosexuality. It highlights the coalition
   government's political disincentives in advocating against these
   restrictive measures and presents the rise of Salafi Islamism in the
   country as a factor of the unwillingness of the government to stop or
   even alleviate the violent repression of homosexuals and the severe
   sanctions against blasphemy.},
