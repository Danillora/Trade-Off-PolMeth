@article{ WOS:000430150900005,
Author = {Areshidze, Giorgi},
Title = {A Faith-Friendly Form of Rawls's ``Public Reason{''}? Barack Obama's
   Civic Faith and the Challenge of Religion in Deliberative Politics},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2018},
Volume = {7},
Number = {2},
Pages = {304-336},
Month = {SPR},
Abstract = {Through a case study of Barack Obama, this article evaluates the
   inclusive form of public reason that John Rawls developed in his later
   writings to accommodate religion. Rawls anticipated recent attempts in
   liberal theory and practice to reopen liberalism to religion, and Obama
   contributes to this effort through his engagement with the civic role of
   Christianity in America's reform movements. Opening liberalism to
   religion may lead us to three conclusions that challenge even this more
   inclusive form of public reason: (i) translation from religious to
   secular reasons may not be the most productive way to accommodate
   religion in democracy, (ii) it may be difficult to distinguish between
   acceptable and unacceptable religious contributions without recourse to
   their underlying theological perspectives, and (iii) a more modest
   constitutional consensus may do a more effective job of accommodating
   religion in democracy than the overlapping consensus that Rawls and
   Obama recommend.},
