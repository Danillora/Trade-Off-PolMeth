@article{ WOS:000426004800005,
Author = {Patterson, James M.},
Title = {A Covenant of the Heart: Martin Luther King Jr., Civil Disobedience, and
   the Beloved Community},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2018},
Volume = {7},
Number = {1},
Pages = {124-151},
Month = {WIN},
Abstract = {Dr. Martin Luther King Jr. articulated a covenant foundation for the
   United States. He called this covenant the ``Beloved Community.{''} In
   it, King described citizen obligations as the experience of divine love
   in the sacrifice of individual interests for the common good and
   personal atone. The Beloved Community operated outside the broader
   covenant tradition in American politics. Grounded in personalist
   theology, King conceived of the Beloved Community as the manifestation
   of individual persons engaging in acts of loving self-sacrifice, or
   agape. These acts provoked the national conscience to begin the legal
   and constitutional steps toward racial reconciliation. King's concept of
   agape was fundamentally Christian, as it grounded his understanding of
   the divine law that set firm moral limits to democracy. God in the
   person of Jesus was ``Lord and Master{''} in a way that renders the
   Beloved Community incompatible with contemporary efforts to fit King
   into secular democratic theory.},
