@article{ WOS:000435008100001,
Author = {Ariely, Gal},
Title = {Globalization and global identification: a comparative multilevel
   analysis},
Journal = {NATIONAL IDENTITIES},
Year = {2018},
Volume = {20},
Number = {2},
Pages = {125-141},
Abstract = {This paper focuses on the relation between national and global
   identification and the impact of globalization on this relation. The
   multilevel approach drew survey data from 89 countries, country-level
   measures of globalization enabling an examination of globalization
   effects on the relation between national and global identification. A
   negative link obtained between national and global identification in
   most of the countries, the cross-country variation of this link is
   related to globalization. Countries characterized by rapid globalization
   exhibit a lower negative relation between national and global
   identification than those marked by lower levels of globalization
   change.},
