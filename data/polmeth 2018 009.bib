@article{ WOS:000443232700002,
Author = {Kriner, Douglas and Schickler, Eric},
Title = {The Resilience of Separation of Powers? Congress and the Russia
   Investigation},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2018},
Volume = {48},
Number = {3},
Pages = {436-455},
Month = {SEP},
Abstract = {The return of unified government following President Trump's election in
   2016 triggered fears that separation of powers could fail to rein in
   executive misconduct. However, Congress' handling of the Russia probe
   largely accords with lessons derived from analysis of more than a
   century of investigative activity. Unified government greatly blunted
   the force of the probe in the House; however, the Senate investigation
   has been more productive and comprehensive. The investigation directly
   led to sanctions legislation and contributed to the pressure to appoint
   a special counsel. More generally, it has significantly weakened
   President Trump's political position by eroding his support among the
   public.},
