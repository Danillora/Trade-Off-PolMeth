@article{ WOS:000434431300004,
Author = {Garding, Sarah},
Title = {Weak by design? Diaspora engagement and institutional change in Croatia
   and Serbia},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2018},
Volume = {39},
Number = {3, SI},
Pages = {353-368},
Month = {JUN},
Abstract = {Diaspora engagement has increasingly been adopted as a strategy for
   leveraging migration's development potential. While a rich literature
   accounts for the emergence of diaspora engagement institutions, there is
   less research on how these institutions perform in practice and why they
   change over time. This article compares change across diaspora
