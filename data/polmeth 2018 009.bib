@article{ WOS:000443300600001,
Author = {Parker, David C. W.},
Title = {Following Fenno: Learning from Senate Candidates in the Age of Social
   Media and Party Polarization},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
