@article{ WOS:000430490000010,
Author = {Ghosh, Cyril},
Title = {Marriage Equality and the Injunction to Assimilate: Romantic Love,
   Children, Monogamy, and Parenting in Obergefell v. Hodges},
Journal = {POLITY},
Year = {2018},
Volume = {50},
Number = {2},
Pages = {275-299},
Month = {APR},
Abstract = {This article offers a critique of the conception of the good intimate
   life in Justice Kennedy's majority opinion in Obergefell v. Hodges
   (2015). In so doing, it draws upon insights from both queer theory and
   liberal theory, each of which repudiates the endorsement of any
   particular conception of the good life. Queer theorists point out the
   dangers of normifying any particular way of life because it demeans and
   marginalizes those who will not, or cannot, assimilate into the dominant
   culture, while liberal theorists point out that neutrality with regard
   to competing conceptions of the good life can lead to stability and the
   mitigation of strife, while expanding the range of human freedoms. The
   Court's opinion in Obergefell, however, is deeply ensconced in a
   specific conception of the good intimate life-one that positions
   marriage, romantic love, monogamy, parenting, and the nuclear family as
   a standard to which all Americans ought to aspire. Consequently, it
   demeans queer-identified and other non-assimilationist lesbian, gay,
   bisexual, and straight individuals, and it potentially exacerbates,
   rather than tempers or resolves, the conflict surrounding LGBT + rights
   in the United States.},
