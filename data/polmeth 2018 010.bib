@article{ WOS:000428594200002,
Author = {Arnold, Richard and Veth, Karl Manuel},
Title = {Racism and Russian Football Supporters' Culture A Case for Concern?},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2018},
Volume = {65},
Number = {2, SI},
Pages = {88-100},
Abstract = {Why is Russian football culture so racist and xenophobic? While many
   countries have problems with racism, abundant evidence shows this is
   particularly bad in Russia and, indeed, a major concern ahead of the
   2018 World Cup. We argue that, while a host of contributing factors in
   some sense ``overdetermine{''} high levels of racism among Russian fans,
   the main reason is the deep penetration of the international far right.
   In the 1990s, the under-institutionalized nature of Russian fan culture
   was unable to organize resistance to the encroachment of the far right.},
