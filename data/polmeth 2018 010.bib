@article{ WOS:000426093900009,
Author = {Rico, Guillem and Lineira, Robert},
Title = {Pass the Buck If You Can: How Partisan Competition Triggers Attribution
   Bias in Multilevel Democracies},
Journal = {POLITICAL BEHAVIOR},
