@article{ WOS:000456100100004,
Author = {Rameez, Aboobacker},
Title = {Resurgence of Ethno-Religious Sentiment against Muslims in Sri Lanka:
   Recent Anti-Muslim Violence in Ampara and Kandy},
Journal = {JOURNAL OF POLITICS AND LAW},
Year = {2018},
Volume = {11},
