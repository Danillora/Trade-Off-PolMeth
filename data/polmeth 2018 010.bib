@article{ WOS:000460821200004,
Author = {Brian Duche-Perez, Aleixandre and Gutierrez Aguilar, Olger Albino and
   Blaz Sialer, David Isaac},
Title = {THE RAWLSIAN NOTION OF DELIBERATIVE RATIONALITY},
