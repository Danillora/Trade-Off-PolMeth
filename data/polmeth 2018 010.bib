@article{ WOS:000425118300016,
Author = {Richardson, Jeremy},
Title = {Brexit: The EU Policy-Making State Hits the Populist Buffers},
Journal = {POLITICAL QUARTERLY},
Year = {2018},
Volume = {89},
Number = {1},
Pages = {118-126},
Month = {JAN-MAR},
Abstract = {There seems to be an emerging conventional wisdom that the Brexit vote
   resulted from specific domestic factors in Britain, such as divisions
   within the ruling Conservative party, the rise of UKIP, strong reaction
   to increased immigration, all set against the backdrop of globalisation
   and its adverse effects. The end result was a populist revolt. The
   argument presented here is somewhat different. Whilst accepting that the
   above factors were certainly very important, it is argued that it is
   important to examine the key role of the EU itself in the creation of
   the current crisis within the EU. The construction of an EU
   policy-making state has run far ahead of what voters at the national
   level want, leading to a central paradox within the EU, namely that the
   European elite which runs the EU has introduced some very beneficial
   public policies, yet that elite has become increasingly out of touch
   with its peoples.},
