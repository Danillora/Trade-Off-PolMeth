@article{ WOS:000435022600006,
Author = {Tatsiy, Vasyl and Serohina, Svitlana},
Title = {Bicameralism: European Tendencies and Perspectives for Ukraine},
Journal = {BALTIC JOURNAL OF EUROPEAN STUDIES},
Year = {2018},
Volume = {8},
Number = {1},
Pages = {101-122},
Month = {JUN},
Abstract = {The authors of the article continue the scientific discourse on the role
   and significance of bicameralism for building a modern democratic legal
   state. The main arguments of supporters and opponents of bicameralism
   have been analysed, given the complicated development of Ukrainian
   institutional framework and discussions over the European Committee of
   Regions evolution. The authors of the article attempt to supplement the
   list of arguments that exist in modern legal literature in favor of
   Ukraine's transition to bicameralism. The challenges of European
   integration for Ukrainian parliament as a unitary actor have been
   highlighted. The authors substantiate the thesis that the formation of
   the second (upper) chamber does not threaten the Ukrainian unitarianism,
   but will contribute to further diversification of state power and at the
