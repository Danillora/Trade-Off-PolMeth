@article{ WOS:000453459900006,
Author = {Prochazka, Josef and Novotny, Antonin and Stojar, Richard and Frank,
   Libor},
Title = {THE LONG TERM PERSPECTIVE FOR DEFENCE 2030 COMPARATIVE ANALYSIS},
Journal = {POLITICKE VEDY},
