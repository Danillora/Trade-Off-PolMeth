@article{ WOS:000452845600004,
Author = {Zaykov, K. S. and Maksimov, A. M. and Tamitskiy, A. M. and Troshina, I,
   T.},
Title = {ETHNOSOCIAL SITUATION IN ARCTIC REGIONS OF RUSSIA AND THE STATE NATIONAL
   POLICY},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2018},
Number = {2},
Pages = {57-67},
Abstract = {The article presents the results of the sociological studies of the
   interethnic relations in the Arkhangelsk region and the Nenets
   Autonomous District, that are a part of the Arctic zone of the Russian
   Federation. We held an opinion poll and identified a statistically
   significant group of respondents that negatively evaluated the state of
   international relations, as well as the prevalence of a stable negative
   attitude towards the certain ethnic minorities in the population. The
   mass survey was supplemented by the materials of the series of expert
   interviews and statistical data. Comparison and interpretation of
   empirical values have led to the conclusion about the absence of
