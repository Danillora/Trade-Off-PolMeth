@article{ WOS:000444897600005,
Author = {Prando, Patrizia},
Title = {FASCIST ANTI-SEMITISM IN THE ITALIAN REVIEW DEFENCE OF RACE
   (1938-1943)},
Journal = {POLITICKE VEDY},
Year = {2018},
Volume = {21},
Number = {3},
Pages = {95-116},
Abstract = {The bimonthly review The Defence of Race (1938-1943) was considered as
   symbol of official racist orientation of the Italian Mussolini's
   dictatorial regime and its racist propaganda. In this review the Italian
   readers of the fascist period could find a lot of pseudo-scientific
   articles and studies, concerning the principal doctrines of race and
   nation as fundamental pillars of Italian racist and anti-Semitic
   ideology. The aim of this study is to characterise the fascist
   anti-Semitic doctrine trought the pages of this racist review. In this
   study we have chosen selected articles, that have better demonstrated
   and clarified the real purpose of realisation of totalitarian society in
   Italy, so called new Italian empire{''}. We have had the opportunity of
   consult all numbers of review in Italian libraries and archives. Using
   these authentic materials we would like to explain the relevant
   importance of the racial purity question and the negative strength of
   masses manipulation. The Defence of Race offers in the current era an
   interesting example for deepest analysis of racial and anti-Semitic
   ideology in the European society as a whole. Throught this study we
   would like to introduce a significant aspect of racial and anti-Semitic
   propaganda of Mussolini's dictatorial regime from the perspective of
   spread of chosen races and nations ideologies and the existance of
   racist thought in the Italian society.},
