@article{ WOS:000417897800009,
Author = {Shineman, Victoria Anne},
Title = {If You Mobilize Them, They Will Become Informed: Experimental Evidence
   that Information Acquisition Is Endogenous to Costs and Incentives to
   Participate},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2018},
Volume = {48},
Number = {1},
Pages = {189-211},
Month = {JAN},
Abstract = {Because non-voters are less politically informed than voters, some
   propose that increasing voter turnout would reduce the quality of
   information among the active voting population, damaging electoral
   outcomes. However, the proposed tradeoff between increased participation
   and informed participation is a false dichotomy. This article
   demonstrates that political information is endogenous to participation.
   A field experiment integrates an intensive mobilization treatment into a
   panel survey conducted before and after a city-wide election. Subjects
   who were mobilized to vote also became more informed about the content
   of the election. The results suggest institutions that encourage
   participation not only increase voter turnout - mobilizing electoral
   participation also motivates citizens to become more politically
   informed.},
