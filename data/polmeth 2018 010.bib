@article{ WOS:000472627600010,
Author = {Paulose, Regina Menachery and Rogo, Ronald Gordon},
Title = {ADDRESSING COLONIAL CRIMES THROUGH REPARATIONS: THE MAU MAU, HERERO AND
   NAMA},
Journal = {STATE CRIME},
Year = {2018},
Volume = {7},
Number = {2},
Pages = {369-388},
Month = {FAL},
Abstract = {Britain and Germany had expansive colonial agendas in Africa from the
   mid to late 1800s, and both the British authorities in Kenya and the
   German authorities in Namibia were responsible for the commission of
   state crimes. This article explores reparations for state crimes
   committed during the colonial period and specifically examines how
   reparations should be afforded for crimes committed during colonization
   given the length of time that has passed. The authors focus on the cases
   of the Mau Mau and the Herero and Nama peoples and discuss present-day
   litigation and movements that have arisen from the commission of state
   crimes against these groups. The current framework for reparations is
   discussed, and a holistic and balanced formula that should be applied
   for those seeking justice for these state crimes is outlined.},
