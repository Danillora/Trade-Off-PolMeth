@article{ WOS:000426905400005,
Author = {Fawcett, Louise},
Title = {MENA and the EU: contrasting approaches to region, power and order in a
   shared neighbourhood},
Journal = {CONTEMPORARY POLITICS},
Year = {2018},
Volume = {24},
Number = {1, SI},
Pages = {65-80},
Abstract = {This article aims to analyse the different power relationships and
   resulting interactions between the major Middle Eastern powers and the
   European Union in their shared neighbourhood. After examining some
   background conditions relating to the local and international power
   dynamics in the Middle East and North Africa (MENA), it considers first
   the perspective of the major MENA powers, then moves to a discussion of
   the different interactions that have characterized relations between
   MENA and EU states over time. It argues that major power relations in
   the MENA neighbourhood have been both dynamic and competitive and
   primarily reflective of the interests of dominant actors. Leadership is
   fragmented and institutions have had limited effects. Regional order
   remains elusive and depends upon developing shared and improved
   understandings of the region among its multiple stakeholders.},
