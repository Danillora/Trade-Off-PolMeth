@article{ WOS:000436084100025,
Author = {Diament, Sean M. and Howat, Adam J. and Lacombe, Matthew J.},
Title = {Gender Representation in the American Politics Canon: An Analysis of
   Core Graduate Syllabi},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
