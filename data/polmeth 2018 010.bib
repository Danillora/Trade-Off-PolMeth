@article{ WOS:000416776500005,
Author = {Matejova, Miriam},
Title = {WHY WAS `COMMUNISM BETTER'? RE-THINKING INEQUALITY AND THE COMMUNIST
   NOSTALGIA IN CENTRAL EUROPE},
Journal = {JOURNAL OF COMPARATIVE POLITICS},
Year = {2018},
Volume = {11},
Number = {1},
Pages = {66-83},
Month = {JAN},
Abstract = {More than twenty years after the fall of the Soviet Union, Central
   Europeans are past their transitions and have well-functioning
   economies. Yet, judging by their lingering communist nostalgia, they
   have failed to notice. Neo-liberal economists argue that transition
   results in higher inequality, and that economic liberalization widens
   the gap between the losers and the winners. Through a comparative
   analysis of Czechoslovakia and Slovakia, this article suggests that it
   is not inequality per se but a perception of inequality - whether
   accurate or skewed - that may aggravate domestic tensions in
   post-communist Central Europe. I propose two factors through which such
   perception may arise: 1) the nature of domestic political institutions,
   and 2) the growth of information and communication technology associated
   with globalization.},
Publisher = {UNIV LJUBLJANA},
