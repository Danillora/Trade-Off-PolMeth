@article{ WOS:000430112300002,
Author = {Fisk, Jonathan M. and Vonasek, Joseph A. and Davis, Elvis},
Title = {``Pot{''}reneurial Politics: The Budgetary Highs and Lows of
   Recreational Marijuana Policy Innovation},
Journal = {POLITICS \& POLICY},
Year = {2018},
Volume = {46},
Number = {2},
Pages = {189-208},
Month = {APR},
Abstract = {Public- and elite-level interest concerning recreational marijuana is
   increasing, thanks in large part to the successful legalization
   campaigns in four states. Scholars, however, have yet to systemically
   assess how states allocate and manage marijuana-related dollars. Some
   states likely prefer tight budgetary control, suggesting that remitted
   marijuana dollars are largely earmarked to specific purposes. Other
   states might opt for looser budgetary mechanisms, giving law makers more
   freedom in how they spend marijuana revenues. States may also vary in
   the tax rates applied to marijuana purchases with some states preferring
   to charge higher rates. Using information drawn from primary sources, we
   assess which factors are most helpful in accounting for the budgetary
   innovations related to recreational marijuana revenues.},
