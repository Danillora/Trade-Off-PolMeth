@article{ WOS:000425118300005,
Author = {Moran, Michael},
Title = {Whatever Happened to Overloaded Government?},
Journal = {POLITICAL QUARTERLY},
Year = {2018},
Volume = {89},
Number = {1},
Pages = {29-37},
Month = {JAN-MAR},
Abstract = {Anthony King was, among other things, an outstandingly acute public
   intellectual. His work on overload and policy fiasco exemplifies this.
   His original diagnosis in his famous paper of 1975 was deeply
   pessimistic in character, but subsequent adaptations by government
   solved many of the original problems arising from dependency relations.
   These adaptations amounted to the creation of a Madisonian system of
   regulation that insulated policy makers from democratic pressures. But
   these adaptations in turn created new difficulties that lie at the root
   of the analysis in King and Crewe's The Blunders of our Governments,
   published nearly four decades after King's original classic paper.},
