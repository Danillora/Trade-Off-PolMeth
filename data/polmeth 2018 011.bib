@article{ WOS:000508596600004,
Author = {Goulart, Mayra},
Title = {Populism beyond Laclau: between the expansion of the demos and
   the disfigurement of liberalism},
Journal = {REVISTA ESTUDOS POLITICOS},
Year = {2018},
Volume = {9},
Number = {1},
Pages = {49+},
Abstract = {The object of this article is the Laclaunian concept of populism. After
   presenting a brief historiography of its multiple mobilizations in Latin
   American, I will present an argument that highlights the link between
   the polysemic use of the concept and the polymorphous nature of its main
   element: the idea of people (demos) and its ability to designate, at the
   same time, the whole and one of its parts (the plebs). From the
   demonstration of this hypothesis, which claims a historical and
   axiological connection between populism and the defense of subalterns, I
   will present a proposal for a reformulation of this category. Thus, by
   incorporating the idea of demos expansion as part of the semantic
   nucleus of the concept, I hope to reduce its degree of indeterminacy
   and, consequently, the spectrum of phenomena that can be typified by it
   - denying, for example, its applicability to discourses of a
   conservative and / or neoliberal nature.},
Publisher = {UNIV FEDERAL FLUMINENSE},
Address = {CAMPUS DE PRAIA VERMELHA, AVE MILTON TAVARES SOUZA S-N, RIO DE JANEIRO,
   24210-340, BRAZIL},
Type = {Article},
Language = {Portuguese},
Affiliation = {Goulart, M (Corresponding Author), Univ Fed Rural Rio de Janeiro, Teoria Polit \& Polit Int, Rio De Janeiro, Brazil.
   Goulart, Mayra, Univ Fed Rural Rio de Janeiro, Teoria Polit \& Polit Int, Rio De Janeiro, Brazil.},
ISSN = {2177-2851},
Keywords = {Populism, Contemporary Democratic Theory; Latin America; New
