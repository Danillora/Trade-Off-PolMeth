@article{ WOS:000442002500010,
Author = {Cassese, Erin C. and Holman, Mirya R.},
Title = {Party and Gender Stereotypes in Campaign Attacks},
Journal = {POLITICAL BEHAVIOR},
Year = {2018},
Volume = {40},
