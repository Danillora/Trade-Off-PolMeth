@article{ WOS:000439648800001,
Author = {Pavlakovic, Vjeran and Brentin, Dario and Paukovic, Davor},
Title = {The Controversial Commemoration: Transnational Approaches to Remembering
   Bleiburg},
Journal = {POLITICKA MISAO-CROATIAN POLITICAL SCIENCE REVIEW},
Year = {2018},
Volume = {55},
Number = {2},
Pages = {7-32},
Month = {JUN},
Abstract = {This article provides an overview on some of the key issues related to
   the Bleiburg commemoration and more broadly the cultural memory of
   Partisan crimes at the end of the Second World War. Drawing upon four
   years of fieldwork, media analysis, and recent historiographical
   debates, the authors take a transnational approach in examining why
   Bleiburg remains one of the most controversial commemorations not just
   in Croatia but in the region. The article focuses on historical
   narratives in the commemorative speeches, the role of space in shaping
   memory politics, symbols and monuments present at Bleiburg Field, and
   the broader context of how Austrian politics affects the commemoration
   and its public perception.},
