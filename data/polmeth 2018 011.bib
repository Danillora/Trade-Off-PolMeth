@article{ WOS:000448984500009,
Author = {Wight, Aldwin and Spencer, James},
Title = {Rehabilitating the Yemeni Defence and Security Establishment Then and
   Now},
Journal = {RUSI JOURNAL},
Year = {2018},
Volume = {163},
Number = {4},
Pages = {72-83},
Abstract = {In Yemen, military effectiveness has often been secondary to political
   or financial advantage. State or military `capture' by self-serving
   cliques increases corruption and discontent. However. Aldwin Wight and
   James Spencer argue that viable and equable military organisations and
   dispositions can be created and maintained, and that a combination of
   transparency and technology can help to identify and inhibit corruption.
   The key to achieving a capable military is political will, and this
   article shows how external sticks and carrots may help.},
