@article{ WOS:000439000900007,
Author = {Hameleers, Michael and Bos, Linda and de Vreese, Claes},
Title = {Framing blame: toward a better understanding of the effects of populist
   communication on populist party preferences},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2018},
Volume = {28},
Number = {3},
Pages = {380-398},
Abstract = {Although previous research has argued that the media play a crucial role
   in populism's success, we know too little about how populist messages
   affect preferences for populist parties. To advance this knowledge, we
   conducted an experiment in which the core of populist rhetoric -
   constructing the people as innocent in-group opposed to the
   establishment as culprit out-group - was manipulated in news articles.
   The findings indicate that when political elites are blamed for a
   salient national problem, people are more likely to vote for a populist
   party and less likely to vote for the largest party in government.
   Populist vote intentions are indirectly affected via blame perceptions.
   These findings offer important insights into the media's role in the
   electoral success of populism.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
