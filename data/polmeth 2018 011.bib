@article{ WOS:000438216100001,
Author = {Hodzi, Obert and Chen Yu-Wen},
Title = {Following the Flow China's Approach to Global Leadership},
Journal = {CHINA QUARTERLY OF INTERNATIONAL STRATEGIC STUDIES},
Year = {2018},
Volume = {4},
Number = {1},
Pages = {1-21},
Month = {SPR},
Abstract = {The liberal international order has been under heavy strain in recent
   years. From Brexit to Trump's presidency, its tenacity is being put to
   the test. The resultant commotion should be a window of opportunity for
   China to materially revise the existing order or establish an
   alternative order under its leadership. However, the puzzle that is
   unfolding but not yet significantly examined is that China is seemingly
   not taking advantage of the upheaval in the liberal international order
   to substantially revise the order or set up a new one. Premised on the
   metaphor of ``following the flow,{''} this paper argues that there is
   understandable rationale as to why China is appearing to ignore the
   opportunity to establish a China-led global order. It then explores the
   approach in China's posture as protector of globalization and the
   liberal international order.},
