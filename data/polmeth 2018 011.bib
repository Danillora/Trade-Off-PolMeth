@article{ WOS:000419167800004,
Author = {Avgousti, Andreas},
Title = {A Text for the City: Plato's Menexenus and the Legacy of Pericles},
Journal = {POLITY},
Year = {2018},
Volume = {50},
Number = {1},
Pages = {72-100},
Month = {JAN},
Note = {Workshop on Plato's Menexenus, Univ Penn, Philadelphia, PA, JAN, 2013},
Abstract = {Many readers have been reluctant to take Plato's Menexenus as evidence
   for its author's fruitful engagement with the institution of the funeral
   oration and have overlooked the value of the dialogue for Plato's
   political thought. In contrast, I argue that in the Menexenus, Plato
   theorizes the Athenian regime (politeia) as an intergenerational
   multitude to which reputation is key. I draw attention to the
   conversation between Menexenus and Socrates, two men who stand for
   different generations of Athenians. Socrates is skeptical about whether
   Menexenus is sufficiently civic-minded in his eagerness for the
   ancestral custom of the funeral oration, and, to Menexenus's irritation,
   belittles the skills required to give such a speech. The funeral oration
   in the dialogue addresses the tension between Menexenus and Socrates by
   casting the city as a ground upon which the generations can meet. The
   war deadwho are made to speak during the orationmake the pursuit of a
   good reputation and the avoidance of a bad one a normative goal for the
   living, since both children and parents are to be judged by the
   multitude. Understood as a capacity or power of the multitude,
   reputation puts the rulers and the ruled into a mutual relationship that
   sustains the existence of the regime. I demonstrate the explanatory
   force of these claims in what I call the Socrates-Aspasia fusion, which
   symbolizes Plato's capacious understanding of the multitude and which
   indicates how reputation ought to be understood. It emerges that the
   Athenian general-orator Pericles, selected by the city to give the
   funeral oration recorded by Thucydides, threatens the intergenerational
   operation of reputation.},
