@article{ WOS:000432111900004,
Author = {Swaine, Lucas},
Title = {Freedom of Thought as a Basic Liberty},
Journal = {POLITICAL THEORY},
Year = {2018},
Volume = {46},
Number = {3},
Pages = {405-425},
Month = {JUN},
Abstract = {Freedom of thought has been lauded in political theory and celebrated in
   human rights discourse. But what kind of freedom is it? I propose that
   freedom of thought deserves status as a basic liberty, given the
   significance of thought to human life, the fundamental importance of
   freedom of thought in establishing and sustaining crucial rights and
   freedoms, and the value of being able to develop and experience one's
   thoughts without undue influence from others.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
