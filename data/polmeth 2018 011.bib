@article{ WOS:000446898800014,
Author = {Wylie, Lana},
Title = {Assessing Howard J. Wiarda's Contributions to Our Understanding of US
   Foreign Policy in Latin America and the Caribbean},
Journal = {POLITY},
Year = {2018},
Volume = {50},
Number = {4},
Pages = {684-696},
Month = {OCT},
Abstract = {Howard J. Wiarda's scholarship advanced our understanding of Latin
   America and the Caribbean by emphasizing culture and revealing that U.S.
   foreign policy often failed to understand the cultures of those regions.
   Through an analysis of Wiarda's writing on Cuba, this article
   demonstrates that his formative experience in the Dominican Republic
   influenced his later analyses of U.S. foreign policy and his comparative
   research on Latin American and Caribbean states, and also led him to
   underestimate the uniqueness of the societies in the region.},
