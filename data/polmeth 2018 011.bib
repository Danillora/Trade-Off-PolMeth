@article{ WOS:000433218800003,
Author = {Koenig, Pascal D.},
Title = {Positions on digital policy in the German party system An analysis of
   party positions at the Bundestag elections of 2009, 2013, and 2017},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2018},
Volume = {12},
Number = {2},
Pages = {399-427},
Month = {JUN},
Abstract = {The article examines how German political parties position themselves on
   issues concerning digital technologies and their societal implications.
   In order to do so, it analyzes the programmatic space at the elections
   to the German Bundestag in 2009, 2013, and 2017 by means of scaling
   analysis. The findings show that, overall, the parties dedicated
   considerably more attention to digitalization issues in 2017 compared to
   previous years. Moreover, the programmatic relations between parties'
   positions have changed in structure with regard to different aspects of
   the digital transformation. In 2009, parties' positions mainly addressed
   socio-cultural issues, with varying emphases on security and
   surveillance, privacy and data protection as well as open content and
   licenses. Toward 2017, the parties generally emphasized issues related
   to economic development, productivity and efficiency, whereas the
   relative emphasis on open content, data protection and privacy as well
   as digital inclusion through net neutrality declined. The increased
   importance of digital technologies for economic development is thus
   reflected in parties' changing programmatic priorities.},
