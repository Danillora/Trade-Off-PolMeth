@article{ WOS:000445171100005,
Author = {Davoliute, Violeta},
Title = {Painful pluralisation Lithuania's handling of the past},
Journal = {OSTEUROPA},
Year = {2018},
Volume = {68},
Number = {6},
Pages = {91+},
Abstract = {Nearly three decades after the collapse of the Soviet Union, Lithuania's
   collective memory is still dominated by the deportation of hundreds of
   thousands of Lithuanians to the Gulag and by armed resistance against
   Soviet rule. The collaboration of individual partisans with the National
   Socialist regime is ignored. As a result of Russia's annexation of
   Crimea and the armed aggression in eastern Ukraine, the policies of
   remembrance in Lithuania are increasingly being defined by security
   policy. However, in historical research and in the public debate,
   pluralistic versions of history and international perspectives on
   Lithuania's past are gaining ground.},
