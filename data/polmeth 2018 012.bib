@article{ WOS:000447722500001,
Author = {Gerring, John and Maguire, Matthew and Jaeger, Jillian},
Title = {A general theory of power concentration: demographic influences on
   political organization},
Journal = {EUROPEAN POLITICAL SCIENCE REVIEW},
Year = {2018},
Volume = {10},
Number = {4},
Month = {NOV},
Abstract = {Why is the exercise of political power highly concentrated in some
   polities and widely dispersed in others? We argue that one important
   causal factor is demographic. Populous polities are characterized by
   less concentrated structures of authority. To explain this relationship,
   we invoke two mechanisms: heterogeneity and trust. The theory is
   demonstrated with a wide variety of empirical measures in cross-country
   analyses including most sovereign states and extending back to the 19th
   century. The result suggests the possibility of a ubiquitous `law' of
   politics.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {32 AVENUE OF THE AMERICAS, NEW YORK, NY 10013-2473 USA},
Type = {Article},
Language = {English},
