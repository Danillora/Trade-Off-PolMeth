@article{ WOS:000452053000006,
Author = {Kjaersgaard, Kristine},
Title = {International Arenas and Domestic Institution Formation: The Impact of
   the UN Women's Conferences in Denmark, 1975-1985},
Journal = {NORDIC JOURNAL OF HUMAN RIGHTS},
Year = {2018},
Volume = {36},
Number = {3, SI},
Pages = {271-286},
Abstract = {During the United Nations (UN) International Women's Decade 1975-1985,
   three UN women's conferences were held in respectively Mexico (1975),
   Copenhagen (1980), and Nairobi (1985). All three women's conferences
   adopted World Action Plans with (non-legally binding) guidelines for the
   advancement of women. Denmark was internationally perceived as a front
   runner in the field of gender equality and human rights. However, the
   Danish women's movement and several female parliamentarians continuously
   advocated increased equality for women in Denmark. Hence, the UN women's
   conferences were highly relevant also to Danish women. This article
   analyses the impact of the three UN women's conferences on the
   development of equality for women in Denmark. It argues that the UN
   women's conferences contributed in two ways to facilitate an increase in
   the so-called `opportunity structure' for equality of women in Denmark.
   First, the conferences contributed to the formation of new institutions
   and action plans for gender equality that came to form part of an
   `opportunity structure' in which equality for women could be advanced.
   Second, the UN conferences contributed to the formation of an informal
   network among female parliamentarians. This network was central to the
   adoption of the new institutions and action plans.},
