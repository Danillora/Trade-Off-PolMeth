@article{ WOS:000437909900001,
Author = {Adeney, Katharine and Bhattacharyya, Harihar},
Title = {Current challenges to multinational federalism in India},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2018},
Volume = {28},
Number = {4},
Pages = {409-425},
Abstract = {India's multinational federation has experienced multiple challenges in
   the last 25 years, relating to the rise of coalition politics and the
   process of economic liberalization, both of which have increased the
   power of some of the states of the federation at the expense of others.
   The internal borders of India continue to be restructured, with the
   latest state, Telangana, created in 2014. India is often seen as a
   successful multinational federation, but it is important to recognize
   the limitations of this success, as well as the areas where the rise of
   an aggressive Hindu nationalism poses a powerful threat to India's
   multinational federal democracy.},
