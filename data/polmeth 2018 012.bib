@article{ WOS:000437945800005,
Author = {Uche, Jasper C. and Iwuamadi, Chijioke K.},
Title = {Nigeria: Rural Banditry and Community Resilience in the Nimbo Community},
Journal = {CONFLICT STUDIES QUARTERLY},
Year = {2018},
Number = {24},
Pages = {71-82},
Month = {JUL},
Abstract = {This paper examines the upsurge in rural violence in Nigeria occasioned
   by the activities of armed herdsmen. Focusing on the 25 April 2016
   invasion of Nimbo Community in Enugu State, the paper argues that rural
   governance deficit and absence of effective security tending mechanisms
   took a turn for the worse. The major finding is that, beyond the common
   conflict triggers, the factionalization of a local vigilante in Nimbo
   bred criminal activities that escalated the tensed milieu. Traditional
   qualitative method of research was applied. The paper suggests
   prioritization of rural security in terms of intelligence gathering,
   response to early conflict warning signals and combat operations, as
   well as a multi-stakeholder consensus in mapping out mutually-rewarding
   terms of engagements for the pastoralists and sedentary farmers.},
