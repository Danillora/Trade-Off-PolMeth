@article{ WOS:000444731900008,
Author = {Roche, Julian},
Title = {Can Biography Benefit from a Marxist Theory of Individuality? Lucien
   Seve's Contribution to Biographical Theory and Practice},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2018},
Volume = {30},
Number = {2},
Pages = {291-306},
Abstract = {This essay examines the theory of personality compatible with Marxism
   developed by the French Marxist philosopher Lucien Seve, including his
   view of how a Marxist biography should be written and how actual
   biographies should be evaluated. The conclusion is that Marxist
   biography should be distinctive and that Seve has presented a plausible
   sketch of what a Marxist biography should look like. But he has also
   laid down an incredibly difficult set of criteria for success that even
   Marxists fail to satisfy in their biographical writing. Unsurprisingly,
   no one fulfills, or perhaps ever could, all the criteria of such a
   biography, however potentially worthwhile the effort.},
