@article{ WOS:000437437900006,
Author = {Medir, Lluis and Magre, Jaume and Tomas, Mariona},
Title = {Mayors' perceptions on local government reforms and decentralization in
   Spain},
Journal = {REVISTA ESPANOLA DE CIENCIA POLITICA-RECP},
Year = {2018},
Number = {46},
Pages = {129-155},
Month = {MAR},
Abstract = {This article analyses Spanish Mayors' perceptions on three areas of
   possible reforms that are currently on the local government agenda:
   re-scaling, amalgamations and metropolitanization. This study shows two
   main features: On the one hand, a relative homogeneity regarding mayors'
   perceptions of reforms and, on the other hand, a consistent difference
   in the mayors' orientations from two groups of autonomous communities.
   The first ones acceded to the ``fast track{''} decentralization process
   that unfolded in Spain since 1978, due to the pressure exerted by their
   political leaders; the second group acceded to autonomy in a later wave,
   equating the distribution of power in all the territories of the state.
   Specifically, it is found that homogeneity in responses is only apparent
