@incollection{ WOS:000441861700005,
Author = {Hussain, Imtiaz},
Book-Author = {Hussain, I},
Title = {Central America in Transatlantic Transactions: Global Commodity Central?},
Booktitle = {TRANSATLANTIC TRANSITIONS: BACK TO THE GLOBAL FUTURE?},
Series = {Global Political Transitions},
Year = {2018},
Pages = {75-120},
Abstract = {To bring the historical central Atlantic flows discussed in the previous
   two chapters to the present time, attention shifts to an analysis of
   three of its dominant exports: bananas, coffee, and sugar. Reiterating
   several prior hypotheses (the presence of strong east-west and
   north-south transatlantic traffic; the emergence of strong
   intra-American transatlantic networks, given the rise of the United
   States and Latin independence from Portugal and Spain, and the
   indispensable transatlantic-global historical linkage evolving into
   concurrent national-multilateral arrangements, and on the evolution of
   inter-governmental, and trans-governmental functions and capacities),
   this chapter (a) thickens the notion of transnational dynamics; (b)
   exposes the transition of transnationalism-fueled colonialism into its
   multilateral and neoliberal arrangements today; and (c) captures the
   transition of central agencies into independence demands, state
   building, nationalism, and import substitution industrialization.},
