@article{ WOS:000438351900008,
Author = {Bragagnolo, Manuela},
Title = {Physiognomy and prophecy between 16th and 17th century. Some
   considerations},
Journal = {LABORATOIRE ITALIEN-POLITIQUE ET SOCIETE},
Year = {2018},
Number = {21},
Abstract = {My contribution widens the horizon to the political importance of
   ``other{''} forms of knowledge aiming to know the future in the age of
   the Counter-Reformation. In the definition and classification of them,
   prophecy plays an important role. At the end of the 16th century, in
   fact, prophecy becomes a comparison element in the classification of the
   forms of knowledge linked to the forecast of future events and in the
   definition of their lawfulness. The enlargement of the notion of heresy
   to magic, goes hand in hand with the condemnation of the divinatory arts
   and that both in the Indexes of forbidden books, in the debates within
   the Roman congregations as well in theological-juridical treatises.
   Nevertheless, some of these disciplines - often in use in courts - are
   saved. Trying to achieve an appropriate categorization, the juridical
   literature examined in this article starts from prophecy and shows the
   need to distinguish the last one from the first ones. In this context,
   amongst the divinatory arts, physiognomy is particularly interesting,
   especially for the political and juridical use of it.},
