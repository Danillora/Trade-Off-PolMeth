@article{ WOS:000445171100006,
Author = {Suziedelis, Saulius},
Title = {Difficult remembrance Lithuania: the Holocaust and the competition among
   victims},
Journal = {OSTEUROPA},
Year = {2018},
Volume = {68},
Number = {6},
Pages = {101+},
Abstract = {In Lithuania, competing remembrances of the victims of the totalitarian
   regimes and the Second World War are widespread. Ethnic Lithuanians
   emphasise their own suffering under Soviet rule. For many years, the
   Holocaust had no place in Lithuania's collective memory. This has
   changed since Lithuania's independence. A part of society is openly
   reflecting on and discussing the Holocaust. However, to date, no
   consensus has formed that Jewish life is a constitutive part of
   Lithuania and the Holocaust was a key event in the more recent history
   of the country.},
