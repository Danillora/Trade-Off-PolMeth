@article{ WOS:000579878900022,
Author = {Terol Orozco, Pablo Jose},
Title = {THE INSTITUTION OF REFERENDUM IN EUROPEAN CONSTUTIONAL LAW: A
   COMPARATIVE STUDY},
Journal = {REVISTA INTERNACIONAL DE PENSAMIENTO POLITICO},
Year = {2018},
Number = {13},
Pages = {423-451},
Abstract = {the Referendum stands as one of the most traditional institutions of
   direct democracy in western constitutionalism. This article aims to lay
   out a comparative study on how that institution is nowadays regulated in
   european States, as well as its role in Spanish law. This text shall
