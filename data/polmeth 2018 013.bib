@article{ WOS:000454467500020,
Author = {Friehe, Tim and Mueller, Helge and Neumeier, Florian},
Title = {The effect of Western TV on crime: Evidence from East Germany},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2018},
Volume = {55},
Pages = {346-372},
Month = {DEC},
Abstract = {This paper explores the potential causal influence of Western television
   programming on crime rates. We exploit a natural experiment involving
   access to West German TV within the German Democratic Republic (GDR) in
   which only geography and topography determined the allocation of
   individuals to treatment and control groups. Focusing on violent and
   property crime (as these domains were most likely to be affected by the
   marked differences in TV content), we find that in the
   post-reunification decade in which TV content was harmonized, regions
   that had access to Western TV broadcasts prior to the reunification
   experienced lower rates of violent crime, sex crime, and theft, but more
   fraud.},
