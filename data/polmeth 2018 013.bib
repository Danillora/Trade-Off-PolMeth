@article{ WOS:000457607000004,
Author = {Williams, Sophie},
Title = {Purebloods and mudbloods: the continuum of Welshness and Basqueness and
   its implications for the development of a National consciousness},
Journal = {NATIONAL IDENTITIES},
Year = {2018},
Volume = {20},
Number = {4},
Pages = {401-418},
Abstract = {The contentious nature of national identity has been welldocumented, and
   the difficulties and challenges associated with self-understanding,
   particularly in a `stateless nation' context, have undergone profound
   scholarly exploration. As part of this exploration, the idea of
   `multiple identities' is prominent; one nation member can simultaneously
   hold a number of ideas, for example, Welsh and British. This had also
   led scholars to suggest that the same nominal identity, for example
   Welsh, may in itself have multiple meanings. Yet this accepted
   multiplicity of the same nominal identity often masks the hard truths:
   it is plausible that certain nation members may rank each other on a
   continuum, or sliding scale, of national identity, wherein some are
   considered more or less worthy of claiming that identity than others.
   This paper explores this concept in relation to Wales and the Basque
   Country. It begins by situating this phenomenon within the relevant
   theoretical context, subsequently exploring its analytical purchase
   through examination of focus group discussion in both nations. Finally,
   it considers the consequences of a continuum, or sliding scale, of
   identity for national unity, in particular its implications for the
   on-going development of a coherent national consciousness.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
