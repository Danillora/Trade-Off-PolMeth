@article{ WOS:000427490900001,
Author = {Gerring, John and Hoffman, Michael and Zarecki, Dominic},
Title = {The Diverse Effects of Diversity on Democracy},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2018},
Volume = {48},
Number = {2},
Pages = {283-314},
Month = {APR},
Abstract = {Diverse identities coexisting within the same society are often viewed
   as problematic for economic and political development. This article
   argues that different types of social diversity have differential
   effects on regime type. Specifically, ethno-linguistic diversity
   increases prospects for democracy while religious diversity decreases
   prospects for democracy. The article presents a variety of reasons why
   diversity might have divergent causal effects on regime type.
   Cross-national regressions in a variety of econometric formats -
   including instrumental variables - provide corroborating evidence for
   the argument.},
