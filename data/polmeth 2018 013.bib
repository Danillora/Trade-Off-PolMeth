@article{ WOS:000445191600009,
Author = {Brandle, Shawna M.},
Title = {Opening up to OERs: Electronic Original Sourcebook Versus Traditional
   Textbook in the Introduction to American Government Course},
Journal = {JOURNAL OF POLITICAL SCIENCE EDUCATION},
Year = {2018},
Volume = {14},
Number = {4},
Pages = {535-554},
Abstract = {Traditional American Government textbooks are expensive and often
   unpopular with students. New technologies and Open Educational Resources
   (OERs) open up the potential for change, but questions of quality are
   ever present: can OERs really help students learn better, or are they
   just cheaper? I developed an OER based on original sources and compared
   student learning outcomes with the OER section to those in a free
   digital textbook section. While the OER I created did not work as well
   as I had hoped, I nonetheless developed a redesign of my course and my
   approach to teaching, which is the true benefit of adopting OERs.},
