@article{ WOS:000436308500019,
Author = {Gradwohl, Ronen and Feddersen, Timothy},
Title = {Persuasion and Transparency},
Journal = {JOURNAL OF POLITICS},
Year = {2018},
Volume = {80},
Number = {3},
Pages = {903-915},
Month = {JUL},
Abstract = {An advisory committee with common values and asymmetric information
   provides a recommendation to a decision maker facing a binary choice. We
   investigate the effect of a transparency requirement-requiring committee
   members' actions to be observable-on the committee's ability to
   influence the decision maker. We show that unless the preferences of the
   committee and decision maker are sufficiently close, requiring
   transparency eliminates the committee's ability to provide any useful
   information. In contrast, if preferences are very close or if committee
   members are able to verifiably reveal their signals then transparency is
   beneficial.},
Publisher = {UNIV CHICAGO PRESS},
Address = {1427 E 60TH ST, CHICAGO, IL 60637-2954 USA},
Type = {Article},
Language = {English},
