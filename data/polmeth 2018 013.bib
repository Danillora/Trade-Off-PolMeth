@article{ WOS:000450373600003,
Author = {Canuto Coelho, Andre Felipe and Borba, Bruna Estima},
Title = {Waiting for an ideal tax: the categorical imperative of the ability to
   pay principle},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2018},
Volume = {117},
Pages = {55-96},
Month = {JUL-DEC},
Abstract = {This paper intends to verify the possibilities and the features of the
   ideal taxation trying to associate it with a fair one. Using the
   deductive method as well as making references to doctrines,
   jurisprudences and legal norms, this study aims to associate the fair
   taxation to the ability to pay principle. Through a historic perspective
   trying to link the taxation phenomena to several forms of economic
   capacity, it's proposed that, in fiscal capitalistic States, a fair
   taxation could be achieved if it's goes hand in hand with an
   individual's taxable expression of wealth.},
