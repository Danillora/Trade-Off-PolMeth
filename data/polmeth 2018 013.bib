@article{ WOS:000443814300008,
Author = {Arunachalam, Raj and Watson, Sara},
Title = {Height, Income and Voting},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2018},
Volume = {48},
Number = {4},
Pages = {1027-1051},
Month = {OCT},
Abstract = {The claim that income drives political preferences is at the core of
   political economy theory, yet empirical estimates of income's effect on
   political behavior range widely. Drawing on traditions in economic
   history and anthropology, we propose using height as a proxy for
   economic well-being. Using data from the British Household Panel Study,
   this article finds that taller individuals are more likely to support
   the Conservative Party, support conservative policies and vote
   Conservative; a one-inch increase in height increases support for
   Conservatives by 0.6 per cent. As an extension, the study employs height
   as an instrumental variable for income, and finds that each additional
   thousand pounds of annual income translates into a 2-3 percentage point
   increase in the probability of supporting the Conservatives, and that
   income drives political beliefs and voting in the same direction.},
