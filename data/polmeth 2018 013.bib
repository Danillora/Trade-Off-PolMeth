@article{ WOS:000433949600017,
Author = {Callari, Antonio},
Title = {Was It Simple to Be a Marxist in Economics? A Tribute to Stephen Resnick},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2018},
Volume = {30},
Number = {1},
Pages = {99-116},
Abstract = {This essay was written in honor of Stephen Resnick. It places his work
   in the tradition of activism and performativity of theory associated
   with Luis Althusser, and it situates his work in a particular political
   and theoretical conjuncture (as it situates Althusser's work in its own
   conjuncture). It argues that Resnick's theoretical practice was
   designed, as Althusser's had been, to give class-struggles an epistemic
   value, that this practice is best understood in terms of the Gramscian
   notion of a war of position (applied to theory), and that this war of
   position entailed a rhetoric of confrontation and an acceptance of risk.
   It argues finally that the legacy of Stephen Resnick can be secured only
   by the continuation of a politically grounded practice of theory.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
