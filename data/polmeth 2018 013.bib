@article{ WOS:000458387700002,
Author = {Tillman, Henry and Yang Jian and Nielsson, Egill Thor},
Title = {The Polar Silk Road China's New Frontier of International Cooperation},
Journal = {CHINA QUARTERLY OF INTERNATIONAL STRATEGIC STUDIES},
Year = {2018},
Volume = {4},
Number = {3},
Pages = {345-362},
Month = {FAL},
Abstract = {The introduction of the ``Polar Silk Road (PSR){''} into the first
   comprehensive white paper on Arctic policy is a historic step for
   China's Arctic engagement. Over the past few years, China has achieved
   policy synergies and launched industrial, scientific and technological
   cooperation with Russia and Nordic countries. With enlarged interests
   and enhanced capabilities, China is becoming a preferred partner for
   Russia and Nordic countries in a number of infrastructure, energy and
   transportation projects within the Arctic region. The PSR framework to
