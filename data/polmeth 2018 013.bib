@article{ WOS:000456954600011,
Author = {Sobaci, Mehmet Zahid and Koseoglu, Ozer and Mis, Nebi},
Title = {Reforming the Policymaking Process in Turkey's New Presidential System},
Journal = {INSIGHT TURKEY},
Year = {2018},
Volume = {20},
Number = {4},
Pages = {183-210},
Month = {FAL},
Abstract = {The system of government in Turkey shifted from a parliamentary to an
   intrinsic presidential system, in which the president solely employs the
   executive power granted from the constitution, after the elections held
   on June 26, 2018. Following the elections, the central government was
   reorganized in quite a short time. While the reorganization process will
   continue for a certain period, it seems that the main policymaking
   actors and their role in the new system have substantially emerged. This
   study provides a legal and institutional analysis of how the public
   policy process and the roles and responsibilities of policy actors
   changed as a result of the restructuring of Turkey's central government
   under the new presidential system.},
