@article{ WOS:000447181200005,
Author = {Gonzalez, Sara and Oosterlynck, Stijn and Ribera-Fumaz, Ramon and Rossi,
   Ugo},
Title = {Locating the global financial crisis: variegated neoliberalization in
   four European cities},
Journal = {TERRITORY POLITICS GOVERNANCE},
