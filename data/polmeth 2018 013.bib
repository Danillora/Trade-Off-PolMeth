@article{ WOS:000438351900004,
Author = {Baggioni, Laurent},
Title = {Palmieri and the civic uses of prophecy},
Journal = {LABORATOIRE ITALIEN-POLITIQUE ET SOCIETE},
Year = {2018},
Number = {21},
Abstract = {The study of the prophetic motives in the Vita civile by Matteo Palmieri
   highlights the religious and mystical foundations of this text, which is
   a significant product of Florentine political humanism of the first half
   of the 15th century. This prophetic basis allows the humanist to
   interlace the temporality of a ``rebirth{''} and the patriotic
   temporality. This intention, both political and cultural, is undoubtedly
   linked with Cosimo de' Medici's access to power, but it also reveals a
   structural dimension of Florentine humanism: its tendency to recover a
   remote and idealized past in order to break with a closer past, thus
   redefining and shaping the collective expectations of the political
   community. In that sense, Palmieri's civic uses of prophecy are
   testimony to how political humanism is involved in shaping a thought of
   civic cohesion which appears as a common experience of time. This
   experience is not only based on the exaltation of Antiquity but also on
   a lay prophetism elaborated with multiple elements of the Christian
   prophetic tradition.},
