@article{ WOS:000450409200011,
Author = {Cabo, Miguel},
Title = {The four leaf clover. The International Peasant Union and its role
   during the Cold War},
Journal = {HISTORIA Y POLITICA},
Year = {2018},
Number = {40},
Pages = {299-329},
Month = {JUL-DEC},
Abstract = {The International Peasant Union was founded in Washington in 1947 as an
   umbrella organization for the agrarian parties of the countries under
   Communist rule in Eastern and Central Europe. It developed a remarkable
   range of activities for three decades, keeping alive the peasantist
   ideology of the so-called Prague Green International between the wars
   and at the same time mobilizing every possible ressource for the triumph
   of the West against the Soviet Union. The exiled agrarian leaders
   provided a valuable, although biased by their hard personal and
   political experiences, insight into the reality behind the Iron Curtain,
   especially in fields related to their expertise (collectivization,
   agrarian policies and the like). The organization, ideology and range of
   activities of the IPU are explored at lenght using printed (particularly
   the IPU Bulletin), archival and bibliographical sources.},
