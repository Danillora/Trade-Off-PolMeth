@article{ WOS:000491235200002,
Author = {Deffains, Bruno and Rouillon, Sebastien},
Title = {Economics of Liability Precaution versus Avoidance},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2018},
Volume = {128},
Number = {1},
Pages = {41-58},
Month = {JAN-FEB},
Abstract = {This paper analyses the behaviour of an agent under strict liability and
   negligence when he can influence both the probability of the accident
   and the probability of being found liable in case of accident. We aim to
   compare the normative properties of strict liability versus fault-based
   liability under such circumstances. The model demonstrates that only a
   negligence rule is efficient and that the optimal damages under strict
   liability is less than what is usually admitted in the literature, i.e.
   the expected fine, given the probability to escape the liability, should
   be less than the level of harm. Finally, the paper highlights the role
   played by the technologies of precaution and evasion in the results.},
