@article{ WOS:000449698700009,
Author = {Valdez, Danny and Pickett, Andrew C. and Goodson, Patricia},
Title = {Topic Modeling: Latent Semantic Analysis for the Social Sciences},
Journal = {SOCIAL SCIENCE QUARTERLY},
