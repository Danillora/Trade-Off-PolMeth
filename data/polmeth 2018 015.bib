@incollection{ WOS:000431979000013,
Author = {Bishop, Matthew},
Editor = {Hay, C and Hunt, T},
Title = {China Crisis?},
Booktitle = {COMING CRISIS},
Series = {Building a Sustainable Political Economy-SPERI Research \& Policy},
Year = {2018},
Pages = {103-111},
Abstract = {Since the onset of China's massive industrial expansion in the 1980s,
   predictions of a coming crisis have consistently been proven wrong as
   its growth trajectory has continued, decade after decade, seemingly
   unabated. But are things different this time? To answer this question,
   the contemporary moment must be set within the wider context of China's
   substantial developmental transformation. A range of challenges lend
   weight to the notion of an impending crisis, but there is significant
   uncertainty about the likely extent of adjustment that will be required.
   How Chinese policymakers grapple with these imperatives, and the way
   that their costs and consequences are distributed, will ultimately shape
   the next stage of China's spectacular development process.},
