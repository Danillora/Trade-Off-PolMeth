@article{ WOS:000514097900005,
Author = {Chareyron, Sylvain and Gray, David and L'Horty, Yannick},
Title = {Raising Take-Up of Social Assistance Benefits through a Simple Mailing:
   Evidence from a French Field Experiment},
Journal = {REVUE D ECONOMIE POLITIQUE},
