@article{ WOS:000439683100010,
Author = {Fuchs, Andreas and Richert, Katharina},
Title = {Development minister characteristics and aid giving},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2018},
