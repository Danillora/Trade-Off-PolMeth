@article{ WOS:000428100100006,
Author = {Abraham, Martin and Lorek, Kerstin and Richter, Friedemann and Wrede,
   Matthias},
Title = {Breaking the norms: When is evading inheritance taxes socially
   acceptable?},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2018},
Volume = {52},
Pages = {85-102},
Month = {MAR},
Abstract = {Using the example of inheritance tax, we examine the relationship
   between the structure of the tax and the strength of the social tax
   compliance norm. Based on the assumption that deviation from a legal
   norm is the more accepted the less its underlying principles are
   jeopardized, we argue that violating the tax compliance norm is socially
   justifiable if revenue requirements as well as equity and efficiency tax
   principles are not infringed upon by the evasion. Building on the
   literature on tax evasion, optimal inheritance taxation, and family
   economics, we hypothesize that a large non-declared amount of transfer
   and a high liquidity of the transferred asset decrease the acceptability
   of inheritance tax evasion and that both a high degree of kinship and
   young age of the heir increase the acceptance of evasion. Utilizing an
   experimental design embedded in a survey on the acceptance of
   inheritance tax evasion conducted in Germany in 2012, we confirm these
   hypotheses empirically.},
Publisher = {ELSEVIER SCIENCE INC},
Address = {STE 800, 230 PARK AVE, NEW YORK, NY 10169 USA},
