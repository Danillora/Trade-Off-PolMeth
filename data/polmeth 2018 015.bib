@article{ WOS:000448096400013,
Author = {Wiese, Rasmus and Jong-A-Pin, Richard and de Haan, Jakob},
Title = {Can successful fiscal adjustments only be achieved by spending cuts?},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2018},
Volume = {54},
Number = {SI},
Pages = {145-166},
Month = {SEP},
Note = {Conference on Political Economy of Public Policy, Ariel Univ, Ariel,
   ISRAEL, SEP 10-12, 2017},
Abstract = {We re-examine the conventional view that to be successful, fiscal
   adjustments should rely on spending cuts and not on tax increases. We
   apply the Bai-Perron structural break filter to identify fiscal
   adjustments and their successfulness in 20 OECD countries. Our results
   suggest that the composition of fiscal adjustments is not related to
   their success. Furthermore, we find that political-economy variables
   considered are not robustly related to successful fiscal adjustments
   with one exception: the probability of a successful fiscal adjustment
   increases if left-wing goverments rely on spending cuts and right-wing
   governments rely on tax increases.},
