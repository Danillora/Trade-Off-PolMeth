@article{ WOS:000445884100004,
Author = {Post, Charles},
Title = {The Use and Misuse of Uneven and Combined Development: A Critique of
   Anievas and Nisancioglu},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2018},
Volume = {26},
Number = {3},
Pages = {79-98},
Abstract = {Aneivas and Nisancioglu's provocative book, How the West Came to Rule,
   attempts to provide an alternative account of the origins of capitalism
   to both `Political Marxism' and `World-Systems Theory'. By making uneven
   and combined development a universal dynamic of human history and by
   utilising a flawed concept of `Eurocentrism', however, they introduce a
   high degree of causal pluralism into their analysis. Despite important
   insights into the specific dynamics of different pre-capitalist forms of
   social labour, their account of the origins of capitalism in How the
   West Came to Rule suffers from causal indeterminacy and historical
   inaccuracies.},
