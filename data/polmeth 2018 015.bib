@article{ WOS:000436447200002,
Author = {Huang, Vincent Guangsheng},
Title = {Floating control: examining factors affecting the management of the
   civil society sector in authoritarian China},
Journal = {SOCIAL MOVEMENT STUDIES},
Year = {2018},
Volume = {17},
Number = {4},
Pages = {378-392},
Abstract = {Civil society organisations can both assist and threaten autocracies'
   monopoly of power. Pre-emptive measures have thus been strategically
   adopted by nondemocratic regimes to harness and manage the civil society
   sector. One tacit sanctioning strategy observable from a
   macro-institutional perspective is to mediate the institutional space
   for civil society organisations. This study explores how the Chinese
   authoritarian state manipulates such institutional space according to
   the changing internal and external environments, especially domestic
   crises and international movement waves. The study uses a 31x17 panel
   data-set about China's social organisations covering 31 administrative
   units (excluding Hong Kong, Macau and Taiwan) during the period of 1998
   to 2014 to gain insight into the management pattern. The results show
   that economic performance, natural disaster, social instability and
   political instability are critical factors affecting regulatory
   dynamics.},
