@article{ WOS:000454467500027,
Author = {Banerjee, Ritwik},
Title = {On the interpretation of World Values Survey trust question Global
   expectations vs. local beliefs},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2018},
Volume = {55},
Pages = {491-510},
Month = {DEC},
Abstract = {How should we interpret the World Values Survey (WVS) trust question? We
   conduct an experiment in India - a low trust country - to correlate the
   WVS-trust question with trust decision in an incentivized Trust Game.
   Evidence supports findings from one strand of the fractured literature:
   the WVS-trust question captures expectations about others'
   trustworthiness. We further show that the WVS-trust question correlates
   with stable beliefs about how trustworthy people in general are.
   However, when subjects go through a treatment induced negative
   experience, WVS-trust question does not capture the immediate
