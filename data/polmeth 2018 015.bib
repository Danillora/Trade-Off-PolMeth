@article{ WOS:000424223300004,
Author = {Kappel, Klemens},
Title = {How moral disagreement may ground principled moral compromise},
Journal = {POLITICS PHILOSOPHY \& ECONOMICS},
Year = {2018},
Volume = {17},
Number = {1},
Pages = {75-96},
Month = {FEB},
Abstract = {In an influential article, Simon C. May forcefully argued that, properly
   understood, there can never be principled reasons for moral compromise
   (May, 2005). While there may be pragmatic reasons for compromising that
   involve, for instance, concern for political expediency or for
   stability, there are properly speaking no principled reasons to
   compromise. My aim in the article is to show how principled moral
   compromise in the context of moral disagreements over policy options is
   possible. I argue that when we disagree, principled reasons favoring
   compromises or compromising can assume a more significant part of what
   makes a position all things considered best, and in this way
   disagreement can ground moral compromise.},
