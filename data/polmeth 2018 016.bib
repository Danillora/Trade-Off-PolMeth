@article{ WOS:000456889300006,
Author = {Szalai, Julia and Svensson, Sara},
Title = {On Civil Society and the Social Economy in Hungary},
Journal = {INTERSECTIONS-EAST EUROPEAN JOURNAL OF SOCIETY AND POLITICS},
Year = {2018},
Volume = {4},
Number = {4},
Pages = {107-124},
Abstract = {While the development of civil society organisations in Hungary has been
   impressive in terms of number and diversity, its influence has remained
   limited on policy-making. Administrative attempts to draw civil society
   under tight regulation and control have produced a blurring of the
   boundaries between the civil and the public spheres that, in turn, has
   impaired the independent voice and criticism of civil society.
   Therefore, economic acts based on solidarity and originating from civil
   society do not automatically form or increase a `social economy' but
   become as contested by and as intermingled with political developments
   as other acts of civil society. This development also has affected the
   profile of civil activities: against the earlier impressive weight of
   anti-poverty, anti-racist and human rights engagements, the `non-risky'
   activities of sports and leisure services have come to domination. A
   turn toward declining participation is a warning sign of the decreasing
   contribution of civil society to everyday democracy.},
