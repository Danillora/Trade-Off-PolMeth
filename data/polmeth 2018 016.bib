   Su, Xiaobo, Univ Oregon, Dept Geog, Eugene, OR 97403 USA.},
DOI = {10.1016/j.polgeo.2017.12.005},
ISSN = {0962-6298},
EISSN = {1873-5096},
Keywords = {Crisis of hegemony; Fragmented sovereignty; Northern Burma; Illicit
   drugs; Territorial fragmentation},
