@article{ WOS:000482620300007,
Author = {Buchely, Lina and Solano, Diana and Recalde, Gabriela C.},
Title = {Listen! Alternative Experiences of Access to Justice in Santiago de
   Cali. Justice-State Tension in Multi-Door Courthouses, Non-Profit
   Organization and Community Kitchens},
Journal = {REVISTA ESTUDIOS SOCIO-JURIDICOS},
Year = {2018},
Volume = {20},
Number = {1},
Pages = {179-206},
Month = {JAN-JUN},
Abstract = {The Distrito de Aguablanca in Cali was chosen in 1994 by government and
   multilateral organizations to develop a pioneering project of the second
   movement of Right and Development: the Casas de Justicia. It integrates
   two apparently differing objectives. One of them relates with the
   reduction of the resource expenditures and the devoted time to the
   provision of justice. The other one attempts to narrowing the
   relationship gaps between community and State, in order to the
   prevention of new conflicts. We are trying a deeper examination into
   this dichotomy, by highlighting the relevance of dialogue and listening
   in the conception of a perspective of justice involving the stances
   prevailing at the scenarios where the State intervenes. We will carry
   out a comparative analysis between the measurement logics arrayed by the
   central government at the Casa de Justicia, the Amor Foundation, and the
   Tia Paula community dinning service, situated in the neighborhoods of
   Marroquin II and Potrero Grande. We will use the referents developed by
   the philosopher Jean-Luc Nancy in his text Listening, such as silence,
   present time, transformation, and noise.},
