@article{ WOS:000428457000009,
Author = {Byeon, Jaewook and Kim, Iljoong and Lee, Dongwon},
Title = {Protest and property crime: political use of police resources and the
   deterrence of crime},
Journal = {PUBLIC CHOICE},
Year = {2018},
Volume = {175},
Number = {1-2},
Pages = {181-196},
Month = {APR},
Abstract = {This article investigates the claim that the political use of police
   resources promotes crime. Using a panel of South Korean metropolitan
   areas, we show that (1) the reallocation of police resources toward the
   control of political protests reduces arrest rates for crime and (2) the
   resulting reduction in criminal arrests significantly increases the
   incidence of crime. Overall, the impact of the reallocation of police
   resources works mainly through tradeoffs with arrest rates. Our findings
   imply that it is not the size of the police per se, but the allocation
   of police resources toward crime control that deters crime.},
