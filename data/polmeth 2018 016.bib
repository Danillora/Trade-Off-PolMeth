@article{ WOS:000444491900003,
Author = {Bobba, Giuliano and Cremonesi, Cristina and Mancosu, Moreno and Seddone,
   Antonella},
Title = {Populism and the Gender Gap: Comparing Digital Engagement with Populist
   and Non-populist Facebook Pages in France, Italy, and Spain},
Journal = {INTERNATIONAL JOURNAL OF PRESS-POLITICS},
Year = {2018},
Volume = {23},
Number = {4, SI},
Pages = {458-475},
Month = {OCT},
Abstract = {This paper clarifies whether and to what extent populist communication
   could drive different gender-oriented reactions. We adopted an original
   research design intending Facebook as a natural environment where
   investigating the interaction between social media users and populist
   and non-populist parties. Our case selection considers three countries
   falling into the pluralist polarized media system: France, Italy, and
   Spain. A human content analysis was carried out on a sample of 2,235
   Facebook posts published during thirty days in 2016 by the four main
   parties/leaders in each country. An original algorithm allowed to
   identify the gender of users liking each message. We tested whether men
   tend more to provide likes to messages posted by populist parties,
   messages published by radical populists, messages containing populist
   contents, and different components of populist messages. Findings
   confirm the existence of a gender-oriented reaction to populism: Men
   tend to support populist actors and parties on Facebook more than women
   do, by providing likes to their content. Yet the difference in gender
   gap between radical and moderate parties is not significant. We also
   found that the antielite component of populist discourse obtains more
   likes by male Facebook users. This pattern is common for both populist
   and non-populist parties.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Bobba, G (Corresponding Author), Univ Turin, Dept Cultures Polit \& Soc, Lungo Dora 100-A, I-10153 Turin, Italy.
   Bobba, Giuliano; Cremonesi, Cristina; Seddone, Antonella, Univ Turin, Dept Cultures Polit \& Soc, Lungo Dora 100-A, I-10153 Turin, Italy.
