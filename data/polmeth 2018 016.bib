@incollection{ WOS:000424969400005,
Author = {Richardson, Martin and Wilkie, Simon},
Book-Author = {Richardson, M},
Title = {Faddists, Enthusiasts and Canadian Divas: Broadcasting Quotas and the
   Supply Response},
Booktitle = {DIMENSIONS OF TRADE POLICY},
Series = {World Scientific Studies in International Economics},
Year = {2018},
Volume = {63},
Pages = {73-104},
Abstract = {This chapter constructs a model of the recorded music market to
   investigate the consequences of local content requirements in
   broadcasting for the ``internationalization{''} of domestic music. It
   models the entry decisions of bands, the contracting decisions of record
   companies, the airplay decisions of radio stations and the radio
   listening and recording purchasing decisions of consumers. The chapter
   shows that a local content quota leads, perversely, to the increased
   internationalization of domestic music. A quota that also requires
   increased broadcasting of ``new{''} music yields an additional welfare
   loss but does nothing to a record company's incentives to sign up new
   bands.},
