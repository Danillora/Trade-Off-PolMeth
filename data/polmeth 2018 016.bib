@article{ WOS:000453903200005,
Author = {McDonald, Michael D. and Magleby, Daniel B. and Krasno, Jonathan and
   Donahue, Shawn J. and Best, Robin},
Title = {Making a Case for Two Paths Forward in Light of Gill v. Whitford},
Journal = {ELECTION LAW JOURNAL},
Year = {2018},
Volume = {17},
Number = {4},
Pages = {315-327},
Month = {DEC 1},
Abstract = {In light of the Supreme Court's ruling in Gill v. Whitford (2018), we
   propose two paths forward for establishing manageable standards to
   identify partisan gerrymanders. One is aimed most particularly at
   exclusionary gerrymanders and follows the Court's directive to
   demonstrate personal and district-specific harms to representational
   rights. The second challenges mapmakers in the first instance and
   courts, if needed later, to recognize that entrenchment gerrymanders
   offend not just representational rights but also the right for all votes
   to carry the same weight. We put our proposals to a series of tests
   through applications to post-2010 suspected gerrymanders of
   congressional districts in Maryland and Ohio along with applications to
   Massachusetts and Illinois to show that the approaches hold safe
   district plans that are not gerrymanders.},
