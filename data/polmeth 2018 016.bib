@article{ WOS:000419032100005,
Author = {Hallett, Andrew Hughes and Proske, Lorian D.},
Title = {Conservative central banks: how conservative should a central bank be?},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2018},
Volume = {65},
Number = {1},
Pages = {97-104},
Month = {FEB},
Abstract = {Using Rogoff's, 1985 model, we determine how inflation averse a central
   banker should be, given the level of volatility and projected output gap
   in the economy. We confirm a strong degree of conservatism, almost twice
   what society would have chosen. But, for a range of developing countries
   and the OECD, economies that systematically experience higher levels of
   output volatility would do best to hire a central banker who is more
   inflation averse than society, but less so than in stable developed
   economies. Thus, while a conservative central banker remains desirable,
   the trade-off is with output volatility rather than with the output gap
   itself.},
