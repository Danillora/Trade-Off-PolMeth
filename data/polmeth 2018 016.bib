@incollection{ WOS:000424969400012,
Author = {Richardson, Martin},
Book-Author = {Richardson, M},
Title = {Trade and Competition Policies: Concordia Discors?},
Booktitle = {DIMENSIONS OF TRADE POLICY},
Series = {World Scientific Studies in International Economics},
Year = {2018},
Volume = {63},
Pages = {221-242},
Abstract = {In a simple model of trade and competition policies we show that the
   abolition of trade restrictions may lead to governments independently
   choosing more competitive competition policies. Subsequent co-ordination
   of competition policy involves encouraging less competitive behaviour
   than when such policies are not co-ordinated and may involve
   disharmonisation, in a sense made clear in the paper. Adding a third
   country, simulation results indicate that non-member concerns about
   customs union formation may be well-founded, particularly when the union
   goes beyond mere trade policy co-ordination (which may be needed to make
   it attractive to members in the first place).},
