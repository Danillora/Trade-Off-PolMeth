@incollection{ WOS:000440272100003,
Author = {Lundstrom, Markus},
Book-Author = {Lundstrom, M},
Title = {Democratic Conflict},
Booktitle = {ANARCHIST CRITIQUE OF RADICAL DEMOCRACY: THE IMPOSSIBLE ARGUMENT},
Series = {Theories Concepts and Practices of Democracy},
Year = {2018},
Pages = {11-25},
Abstract = {This chapter analyses democratic conflict in Husby, a Stockholm city
   district, located at Sweden's socio-political periphery. The so-called
   Husby Riots, enacted in the Swedish spring of 2013, mark the analytical
   entry point from which the author traces, through ethnographic
   interviews, its local-historical roots. The chapter conveys, guided by
   the radical-democratic theory of Jacques Ranciere, how Husby's
   democratic life triggers resistance from the democratic state; Husby
   residents recount being continuously ignored, disqualified, or
   repressed. Through the Rancierian lens, the Husby case exposes how
   democratic conflict-between governors and governed-became intensified in
   the 2013 events, challenging the grounded social division of democracy
   itself.},
Publisher = {PALGRAVE},
Address = {HOUNDMILLS, BASINGSTOKE RG21 6XS, ENGLAND},
