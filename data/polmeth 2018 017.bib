@article{ WOS:000449992200012,
Author = {Davis, Nicholas T.},
Title = {Religion and Partisan-Ideological Sorting, 1984-2016},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2018},
Volume = {99},
Number = {4},
Pages = {1446-1466},
Month = {DEC},
Abstract = {Objective Method This article explores how religion affects the extent
   to which individuals connect their ideological to partisan identities-a
   process termed partisan-ideological sorting. To explore this
   relationship, I analyze data from the American National Election Studies
   (ANES) Time-Series surveys and the Youth-Parent Socialization (YPS)
   panel study. Results Conclusion I find that a matrix of belief,
   belonging, and behavior constrains the convergence between citizens'
   political identities, with one important caveat: evangelical affiliation
   functions as the primary conduit through which religiosity shapes this
   sorting. Building on these results, I then estimate the direct impact of
   religion on sorting over time. These findings show that religion has
   produced asymmetric sorting in the mass public among persons with
   right-leaning identities. Further, they provide a social explanation for
   partisan-ideological sorting that complements extant institutional ones.},
