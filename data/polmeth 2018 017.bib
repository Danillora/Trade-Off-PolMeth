@article{ WOS:000424223300003,
Author = {Wintein, Stefan and Heilmann, Conrad},
Title = {Dividing the indivisible: Apportionment and philosophical theories of
   fairness},
Journal = {POLITICS PHILOSOPHY \& ECONOMICS},
Year = {2018},
Volume = {17},
Number = {1},
Pages = {51-74},
Month = {FEB},
Abstract = {Philosophical theories of fairness propose to divide a good that several
   individuals have a claim to in proportion to the strength of their
   respective claims. We suggest that currently, these theories face a
   dilemma when dealing with a good that is indivisible. On the one hand,
   theories of fairness that use weighted lotteries are either of limited
   applicability or fall prey to an objection by Brad Hooker. On the other
   hand, accounts that do without weighted lotteries fall prey to three
   fairness paradoxes. We demonstrate that division methods from
   apportionment theory, which has hitherto been ignored by philosophical
   theories of fairness, can be used to provide fair division for
   indivisible goods without weighted lotteries and without fairness
   paradoxes.},
