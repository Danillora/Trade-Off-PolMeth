@article{ WOS:000449484200003,
Author = {Glazier, Rebecca A.},
Title = {Acting for God? Types and Motivations of Clergy Political Activity},
Journal = {POLITICS AND RELIGION},
Year = {2018},
Volume = {11},
Number = {4},
Pages = {760-797},
Month = {DEC},
Abstract = {Clergy members are often important political actors. Yet, scholars
   rarely distinguish among different types of clergy political activities.
   Here, I argue for three disaggregated categories of clergy political
   activity: personal, general congregation level, and election-specific
   congregation level. Data from two sources-the Cooperative Clergy Study
   and the Little Rock Congregations Study-demonstrate that important
   differences exist across these categories, with the majority of model
   variables significantly influencing different clergy political
   activities in different directions. For instance, a conservative
   ideology and affiliation with a Black Protestant church both negatively
   influence personal political activities, like donating to a campaign,
   while also positively influencing election-related political activities
   in the congregation, like distributing voter guides. Similarly,
   providential religious beliefs increase general congregation-level
   political activities, while decreasing personal and electoral
   activities. These relationships are obscured when political activity is
   considered in the aggregate, suggesting that clergy political activities
   are nuanced; different activities are driven by different motivations.},
