@article{ WOS:000442882000012,
Author = {Shi, Chi-Chi},
Title = {Defining My Own Oppression: Neoliberalism and the Demands of Victimhood},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2018},
Volume = {26},
Number = {2, SI},
Pages = {271-295},
Abstract = {In this article I explore a central paradox of contemporary identity
   politics: why do we look for recognition from the very institutions we
   reject as oppressive? I argue that neoliberalism's continued assault on
   the bases for collectivity has led to a suspicion that `the collective'
   is an essentialising concept. The assault on the collective coupled with
   the neoliberal imperative to create an `authentic' self has led to
   trauma and victim-hood becoming the only bases on which people can
   unite. This manifests discursively and theoretically in the primary
   trope of contemporary activism: `intersectionality'. Mobilising around
   this analytical concept has led to an analysis of oppression that. even
   as it claims to be systemic, is totally dematerialised and relentlessly
   individualised. Instead of building collective power, we are left with a
   politics of individual demand coming from a coalition of dispersed
   subject positions.},
