@article{ WOS:000432112500004,
Author = {Kolodny, Niko},
Title = {Standing and the sources of liberalism},
Journal = {POLITICS PHILOSOPHY \& ECONOMICS},
Year = {2018},
Volume = {17},
Number = {2, SI},
Pages = {169-191},
Month = {MAY},
Note = {Symposium on Contemporary Moral and Political Philosophy, New Orleans,
   LA, FEB 19-20, 2016},
Organization = {Polit, Philosophy \& Econ},
Abstract = {Whatever else liberalism involves, it involves the idea that it is
   objectionable, and often wrong, for the state, or anyone else, to
   intervene, in certain ways, in certain choices. This article aims to
   evaluate different possible sources of support for this core liberal
   idea. The result is a pluralistic view. It defends, but also stresses
   the limits of, some familiar elements: that some illiberal interventions
   impair valuable activities and that some violate rights against certain
   kinds of invasion. More speculatively, it points to two further sources
   of support for liberalism, each of which represents a certain kind of
   social standing: a self-sovereignty compromised simply by being subject
   to certain kinds of commands and a relational equality compromised by
   the condemnation of choices with which one's group is identified.},
