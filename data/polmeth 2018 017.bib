@article{ WOS:000457579200001,
Author = {Nitschke, Paula and Donges, Patrick},
Title = {Online Repertoires of NGOs in the International Arena},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2018},
Volume = {15},
Number = {4},
Pages = {301-318},
Abstract = {This study examines the influence of structural conditions on the online
   activities of non-governmental organizations (NGOs) in the international
   arena. We propose the concept of an online repertoire, defined as a set
   of platforms and communication modes that an NGO employs to communicate
   with the greater public on the web. We tested the influence of four
   structural conditions on that repertoire (position in a political
   process, organizational capacity, membership type, and type of
   interest). The findings suggest that organizational capacity has the
   most substantial influence and that NGOs with higher organizational
   capacities communicate more actively on the web.},
