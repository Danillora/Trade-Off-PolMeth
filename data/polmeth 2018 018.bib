@article{ WOS:000443417400002,
Author = {Ley, Aaron J. and Clayton, Cornell W.},
Title = {Constitutional Choices: Political Parties, Groups, and Prohibition
   Politics in the United States},
Journal = {JOURNAL OF POLICY HISTORY},
Year = {2018},
Volume = {30},
Number = {4},
Pages = {609-634},
Month = {OCT},
Abstract = {Traditional accounts of the Eighteenth and Twenty-first Amendments to
   the U.S. Constitution largely ignore the role of the major political
   parties. We argue that partisan politics was an integral part of the
   constitutional politics of this period. The need to manage divisions
   within both parties' electoral coalitions during the transition from the
   third to the fourth-party systems led to the enactment of the Eighteenth
   Amendment without support from either national party. While most
   accounts trace prohibition's demise to widespread noncompliance and the
   graft it generated, we argue that elite congressional support for
   prohibition gave way when civil service reforms removed federal
   prohibition agents as patronage resources. We also argue that by giving
   states control of designing state conventions, and thereby risking state
   malapportionment of conventions, Democrats succeeded in overcoming the
   traditional fissures that divided their southern and northern wings.},
Publisher = {CAMBRIDGE UNIV PRESS},
