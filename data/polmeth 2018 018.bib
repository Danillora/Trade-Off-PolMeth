@article{ WOS:000482623100002,
Author = {Baird, Adam},
Title = {Becoming the Baddest: Masculine Trajectories of Gang Violence in
   Medellin},
Journal = {REVISTA ESTUDIOS SOCIO-JURIDICOS},
Year = {2018},
Volume = {20},
Number = {2},
Pages = {9-48},
Month = {JUL-DEC},
Abstract = {Drawing upon forty life-history interviews with gang members in
   Medellin, Colombia, this paper argues that many young men join gangs to
   emulate and reproduce `successful' local male identities. The
   accumulation by the gang of ``masculine capital{''}, the material and
   symbolic signifiers of manhood, and accompanying stylistic and timely
   displays, means that youths often perceive them to be spaces of male
   success, driving the social reproduction of the gang. Once in the gang,
   they become increasingly ``bad{''} using of violence to defend the
   gang's interests in exchange for masculine capital. Gang leaders,
   colloquially known as duros or ``hard men{''}, tend to be the mas malo,
   the ``baddest{''}. The ``ganging process{''} should not be understood in
   terms of aberrant youth behaviour, rather there is practical logic to
   joining the gang as a site of identity formation for aspirational young
   men who are coming-of age when conditions of structural exclusion
   conspire against them.},
