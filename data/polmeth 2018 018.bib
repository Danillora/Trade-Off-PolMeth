@article{ WOS:000434636400004,
Author = {Colley, Linda},
Title = {For Better or for Worse: Fifty Years Since the Removal of the Marriage
   Bar in the Australian Public Service},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2018},
Volume = {64},
Number = {2},
Pages = {227-240},
Month = {JUN},
Abstract = {Fifty years ago, in late 1966, the Australian Public Service officially
   removed the marriage bar policy that had prevented the recruitment or
   continued employment of married women. The anniversary of this historic
   milestone is an opportunity to reflect on the introduction and removal
   of the marriage bar and to reflect on the policy change process and
   lessons that can be applied to the continuing struggle for gender
   equality at work. The research uses John Kingdon's framework for policy
   change to examine the delays and obstacles to the removal of the
   marriage bar. It highlights a number of occasions when the window of
   opportunity for policy change was briefly open but rapidly closed again
   due to changing political and economic circumstances.},
