@article{ WOS:000446104000006,
Author = {Esposito, Giovanni and Ferlie, Ewan and Gaeta, Giuseppe Lucio},
Title = {The European public sectors in the age of managerialism},
Journal = {POLITICS},
Year = {2018},
Volume = {38},
Number = {4},
Pages = {480-499},
Month = {NOV},
Abstract = {During the past 50years, European public sectors have undergone a
   profound process of organizational change, where managerial tools and
   principles from the private sector have permeated through governments
   and administrations of many countries. A substantial amount of academic
   literature has now been devoted to public management reforms. Many
   scholars have associated them with the diffusion of a managerialist
   ideology. However, the relationship between public management reforms,
   political ideology, and public expressions of support for these reforms
   by political parties has been a relatively under-explored topic within
   the literature and is the gap we address in this article. Using a
   longitudinal framework of study, our analysis shows how issues
   surrounding managerialist reforms have evolved across the electoral
   manifestos of European parties during the past 50years. Our findings
   reveal that these reforms have enjoyed a growing political profile over
   time in many countries within Western and Eastern Europe. Furthermore,
   we also examine and discuss the differences and similarities of these
   reforms across countries.},
