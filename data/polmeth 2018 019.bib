@article{ WOS:000440562700013,
Author = {Baumgartner, Jody C. and Lockerbie, Brad},
Title = {Maybe it Is More Than a Joke: Satire, Mobilization, and Political
   Participation},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2018},
Volume = {99},
Number = {3},
Pages = {1060-1074},
Month = {SEP},
Abstract = {Objective. The objective of this study was to examine the relationship
   between viewing late night political humor and political participation.
   Methods. We used various measures of viewership of late night talk shows
   and political participation in the 2012 American National Election
   Studies (ANES) data set. Results. We show that viewership of ``Late
   Night with David Letterman,{''} a simple form of political comedy, seems
   to be unrelated to political participation. However, viewership of
   Comedy Central's ``The Daily Show with Jon Stewart{''} and ``The Colbert
   Report,{''} considered by most to be genuine political satire, is
   associated with higher levels of political participation. Conclusion.
   The results suggest that advocates of political satire may be correct
   when they suggest that satire mobilizes viewers to political action.},
