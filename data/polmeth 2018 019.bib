@incollection{ WOS:000472929000010,
Author = {Becker, Stefan},
Editor = {Ege, J and Bauer, MW and Becker, S},
Title = {Steering Soft Economic Governance - The Commission and Country-specific
   Recommendations in the European Semester},
Booktitle = {EUROPEAN COMMISSION IN TURBULENT TIMES: ASSESSING ORGANIZATIONAL CHANGE
   AND POLICY IMPACT},
Series = {Schriftenreihe des Arbeitskreises Europaische Integration e.V.},
Year = {2018},
Volume = {105},
Pages = {207-231},
Abstract = {The European Semester, the EU's framework for economic policy
   coordination, features country-specific recommendations to stimulate
   reform at national level. Despite being soft law, this advice causes
   frequent controversy. This chapter shows that the Commission has been a
   driving force behind the growing significance of country-specific
   recommendations during the Euro crisis in that it has moved from
   administrating towards steering soft economic governance. It first
   promoted the integration of the Europe 2020 strategy into the European
   Semester and then, during its implementation, coupled the nonbinding
   country-specific recommendations with the harder strands of economic
   governance. On this institutional basis, it shifted the thematic
   portfolio of advice towards structural reforms and employment and social
   policies and, in the delivery of recommendations, curtailed the focus on
   austerity. The Commission thus realized its own preferences, both
   institutional and substantive, and made itself heard in matters at the
   core of national welfare systems. As a result, soft economic governance
   has become harder and more supranational during the Euro crisis.},
