@article{ WOS:000432152900006,
Author = {Gin, Willie},
Title = {Big data and labor: What baseball can tell us about information and
   inequality},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2018},
Volume = {15},
Number = {1},
Pages = {66-79},
Abstract = {Big data, enhanced behavioral profiling, and information analytics will
   tend to have a leveling-down effect on the negotiating power of workers
   against management, and against other more privileged workers. The
   article looks specifically at the bellwether case of baseball in the
   wake of the Moneyball revolution-the increasingly sophisticated
   quantification of baseball players' contributions. The analysis shows
   that although major league teams are increasingly relying on the lower
   paid to produce baseball wins, the share of salary the lower paid has
   commanded relative to their contribution has declined in the era of
   Moneyball. Information has allowed baseball teams to wring more out of
   lower-paid baseball players more efficiently, by identifying to
   management highly productive substitutes. The article compares the
   situation in baseball to other labor markets, and concludes that
   transparency of data analytics is an insufficient solution to the
   ``leveling down{''} effect of big data on wages.},
