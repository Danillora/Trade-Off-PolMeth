@article{ WOS:000430627200009,
Author = {Menzani, Tito and Medina-Albaladejo, Francisco J.},
Title = {BETWEEN LEADER-WORSHIP AND MEMBER'S DEMOCRACY: THE CONSUMER
   CO-OPERATIVES IN THE FASCIST ITALY},
Journal = {HISTORIA Y POLITICA},
Year = {2018},
Number = {39},
Pages = {235-260},
Month = {JAN-JUN},
Abstract = {We analyse the impact of the Italian fascist regime on the consumer
   co-operative movement. This is of great interest because of the clash
   between an anti-democratic political culture and a democratic economic
   culture. We intend to demonstrate that the fascist regime penalized the
   consumer co-operative movement in various ways. However, a small number
   of organizations were an exception to this state of events, and
   paradoxically succeeded, from an entrepreneurial point of view. Some
   problems-small size, excessive social investments, a more political than
   entrepreneurial vision - were often resolved by the new administrators.
   This distinction is absolutely essential also in terms of the
   ``legacy{''} for the consumer co-operative movement after 1945 in Italy.},
