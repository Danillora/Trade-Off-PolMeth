@article{ WOS:000419032100004,
Author = {McCannon, Bryan C.},
Title = {Leadership and motivation for public goods contributions},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2018},
Volume = {65},
Number = {1},
Pages = {68-96},
Month = {FEB},
Abstract = {Results from a leader-follower public goods game are presented. An
   individual, when randomly selected to make a contribution knowing others
   will observe the selection, gives more than in the simultaneous-move
   public goods game. Followers adopt a quasi-matching strategy where they
   systematically donate less than the leader, but contribute more when the
   leader does and contribute less when the leader free rides. The net
   result is increased provision of a public good when contributions are
   sequential. The results highlight that psychological preferences, rather
   than solely social preferences, can explain behavior.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
