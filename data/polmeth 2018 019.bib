@article{ WOS:000429391700002,
Author = {Ishiyama, John and Mezvrishvili, Lia and Zhgenti, Nina},
Title = {An oasis of democracy in an authoritarian sea? Civil society, social,
   and institutional trust in Georgia},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2018},
Volume = {51},
Number = {1},
Pages = {19-26},
Month = {MAR},
Abstract = {We examine whether the existence of civil society in Georgia has
   promoted social and institutional trust in the population. As much of
   the literature suggests, Georgia is different from its neighbors in that
   civil society development is more advanced. Does participation in civil
   society lead to more social and institutional trust? Using survey data
   from the Caucasus Barometer and the World Values Survey, we find that
   attitudes regarding social and institutional trust are more developed in
   Georgia than in its neighbors, and that activity in voluntary
   organizations is positively associated with social and institutional
   trust in Georgia. (C) 2018 The Regents of the University of California.
   Published by Elsevier Ltd. All rights reserved.},
