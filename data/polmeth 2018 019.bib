@article{ WOS:000451685000006,
Author = {Kraft, Herman Joseph S.},
Title = {Beyond the transmission of ideas: Pedro L. Baldoria and the field of
   geopolitics in the Philippines},
Journal = {PHILIPPINE POLITICAL SCIENCE JOURNAL},
Year = {2018},
Volume = {39},
Number = {2},
Pages = {132-135},
Abstract = {This article looks into the significance of the contribution made by
   Pedro L. Baldoria to the development of International Relations as a
   field of study in the Philippines. More specifically, his work was a key
   factor in the introduction of Geopolitics to foreign policy thinking in
   the Philippines in the years following the end of the Second World War.
   More than simply being an agent for the transmission into the
   Philippines of an approach that had become increasingly popular in the
   Cold War context of thinking about international relations, Baldoria had
   been instrumental in adapting Geopolitics as an approach to foreign
   policy in the specific context of the Philippines. He was responsible
   for introducing a course on Geopolitics in the Political Science program
   of the University of the Philippines, advising students who worked with
   him using this approach in their theses, and encouraging practitioners
   and academics in the realm of foreign policy to think beyond legal
   institutionalist ways of appreciating international relations. Beyond
   merely transmitting ideas, he was able to make Geopolitics relevant to
   the Philippines and the way it conducted its foreign policy.},
