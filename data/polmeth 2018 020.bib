@article{ WOS:000440675700010,
Author = {Velasquez, Teresa A.},
Title = {Tracing the Political Life of Kimsacocha: Conflicts over Water and
   Mining in Ecuador's Southern Andes},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2018},
Volume = {45},
Number = {5},
Pages = {154-169},
Month = {SEP},
Abstract = {In 2009, farmers in the highlands of Ecuador challenged a proposed water
   law by staging public rituals to venerate their watershed, called
   Kimsacocha, as the embodiment of the Pachamama (Mother Earth). They
   rejected the proposed law because it allowed for mineral extraction in
   communal watersheds. They argued that human and nonhuman entities are
   interconnected and that the state should designate communal watersheds
   as no-mining zones to defend the right to life. While some scholars have
   argued that indigenous ontologies decolonize the political realm, in
   fact they have uneven outcomes when mobilized in contentious mining
   politics. Indigenous ontologies enabled farmers to build a multiethnic
   movement in defense of life, but this did not lead to the implementation
   of their demands. Instead, the state appropriated the language of the
   Pachamama to produce a revised water law that promoted piecemeal
   environmental conservation.},
