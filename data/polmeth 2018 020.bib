   Lee, Kelley, Simon Fraser Univ, Burnaby, BC, Canada.
   Lee, Kelley, Simon Fraser Univ, Fac Hlth Sci, Burnaby, BC, Canada.},
DOI = {10.1111/1758-5899.12612},
ISSN = {1758-5880},
EISSN = {1758-5899},
Keywords-Plus = {PRODUCTION NETWORKS; POLITICAL-ECONOMY; IMPACT; POLICY; RULES},
