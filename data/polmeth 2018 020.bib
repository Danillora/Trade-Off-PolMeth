@article{ WOS:000448057400001,
Author = {Leira, Halvard},
Title = {Foreign Policy - a conceptual history},
Journal = {INTERNASJONAL POLITIKK},
Year = {2018},
Volume = {76},
Number = {1},
Pages = {1-23},
Abstract = {This article deals with the emergence of Norwegian foreign policy,
   providing dating through a conceptual history of the phenomenon. Earlier
   research has suggested that Norwegian foreign policy emerged during the
   Middle Ages, with the establishment of relations between Norwegian kings
   and other kings, towards the end of the 18th century, with the
   establishment of a dedicated ministry for foreign affairs in Copenhagen,
   or in 1905 with full external sovereignty. Approaching foreign policy as
   a practice-concept, which emerged at a specific time for a specific
   purpose, yields another answer. The emergence of foreign policy in
   Norway should be dated to the years around 1860, when the Storting
   started to pay attention to the world outside of Norway, and desired
   more control over what was conceptualised specifically as foreign
   policy.},
Publisher = {NORSK UTENRIKSPOLITISK INST},
