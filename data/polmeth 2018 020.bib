@article{ WOS:000438374000017,
Author = {Koliev, Faradj and Lebovic, James H.},
Title = {Selecting for Shame: The Monitoring of Workers' Rights by the
   International Labour Organization, 1989 to 2011},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
