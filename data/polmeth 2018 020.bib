@article{ WOS:000446247800001,
Author = {Fernanda Somuano, Ma.},
Title = {APPROVING THE PRESIDENT. A COMPARISON BETWEEN FELIPE CALDERON AND
   ENRIQUE PENA NIETO},
Journal = {FORO INTERNACIONAL},
Year = {2018},
Volume = {58},
