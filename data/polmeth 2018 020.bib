@article{ WOS:000430810100005,
Author = {Rutelione, Ausra and Hopeniene, Rimante and Zalimiene, Kristina},
Title = {Linking Destination Brand Personality, Self-congruity and Tourist
   Behaviour: A Local Spa Resort Case Study},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2018},
Volume = {16},
Number = {2},
Pages = {293-310},
Month = {APR},
Abstract = {This study was designed to explore the relationship between destination
   brand personality, self-congruity and the behavioural intentions of
   tourists at a local spa resort. The findings indicate that domestic
   tourists (N=232) of the spa resort attribute personal characteristics to
   the destination, and the brand personality of this resort is a
   combination of five dimensions which positively correlate with the
   tourists' intentions to return to and recommend the location. The
   results of the study also confirm that the self-congruity theory can be
   applied to a spa destination, as positive relationships between all four
   self-congruity dimensions and intentions to recommend were found.
   However, our research reveals that there is negative though
   insignificant correlation between social self-congruity and intention to
   return.},
