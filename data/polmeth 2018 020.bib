@incollection{ WOS:000432431100005,
Author = {Reilly, Michael},
Book-Author = {Reilly, M},
Title = {What Would a Good Investment Agreement Between the EU and Taiwan Look
   Like?},
Booktitle = {TOWARDS AN EU-TAIWAN INVESTMENT AGREEMENT: PROSPECTS AND PITFALLS},
Series = {European Union in International Affairs Series},
Year = {2018},
Pages = {49-64},
Abstract = {Taiwan's top trade policy priority is membership of the Trans-Pacific
   Partnership. If it is to join, it will have to commit to domestic
   reforms and market-opening measures. Similar commitments would
   facilitate negotiations and progress with the EU. But this would be the
   minimum acceptable outcome and Taiwan policymakers should be more
   ambitious, using negotiations both to pursue wider domestic reforms and
   to seek more concessions from the EU. Such an agreement might be
   modelled on aspects of the Free Trade Agreement between the EU and
   Korea, with further specific agreements on investment. This would make
   Taiwan more attractive to European business, ensure its exporters were
   not at a disadvantage to Korean competitors and provide a foundation on
   which to build a broader relationship.},
