@article{ WOS:000427075400008,
Author = {Stivachtis, Yannis A.},
Title = {`International society' versus' `world society': Europe and the Greek
   War of Independence},
Journal = {INTERNATIONAL POLITICS},
Year = {2018},
Volume = {55},
Number = {1},
Pages = {108-124},
Month = {JAN},
Abstract = {This paper examines the interplay between international society and
   world society during the Greek War of Independence. It argues that the
   Philhellenist movement represented a European world society, which
   politically engaged with European international society in an effort to
   influence the latter's policies pertaining to the Greek case. As a
   result, a tension between the two emerged. The independence of Greece
   constituted a major illustration of the European Concert in action.
   However, European world society played a major role in the final
   outcome. Not only did European world society provided the means to the
   Greeks to pursue their cause but, most importantly, kept the hope of
   independence alive even at times when the Greek revolt appeared to be
   dead. Moreover, European world society served as an additional factor in
   the calculations of European powers, thereby exercising considerable
   influence over the decisions and actions of the governments of the
   European powers.},
