@article{ WOS:000433590900004,
Author = {Simmons, Richard},
Title = {Using cultural theory to navigate the policy process},
Journal = {POLICY AND POLITICS},
Year = {2018},
Volume = {46},
Number = {2, SI},
Pages = {235-253},
Month = {APR},
Abstract = {Cultural Theory's (CT) four `cultural biases' hold an intuitive appeal
   and empirical resonance for policy scholars as they seek to make sense
   of complex policy contexts. Yet research applications of CT are often
   hampered by superficial classifications and a lack of operationalisable
   measures. This article solves these problems, presenting valid measures
   in a `compass' and `map' to show how cultural patterns are both
   `internalised' in individuals and `institutionalised' in policy
   contexts. Key tensions here provide researchers with new interpretive
   tools to promote better questioning, learning and adaptation within
   particular policy environments, including opportunities for research
   impact through practical, `institutional work'.},
