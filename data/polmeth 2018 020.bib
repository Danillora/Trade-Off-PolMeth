   Elcheroth, Guy, Univ Lausanne, LINES, Lausanne, Switzerland.
   Spini, Dario, Univ Lausanne, NCCR LIVES, LINES, Lausanne, Switzerland.},
DOI = {10.1177/0022002716645388},
ISSN = {0022-0027},
EISSN = {1552-8766},
Keywords = {collective guilt; collective victimization; collective vulnerability;
   multilevel approach},
