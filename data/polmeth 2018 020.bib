@article{ WOS:000427726500008,
Author = {Oestreich, Joel E.},
Title = {Sexual orientation and gender identity in Nepal: Rights promotion
   through UN development assistance},
Journal = {JOURNAL OF HUMAN RIGHTS},
Year = {2018},
Volume = {17},
Number = {2},
Pages = {265-279},
Abstract = {United Nations (UN) development agencies have been actively working to
   protect lesbian, gay, bisexual, transgender, and intersex (LGBTI) rights
   in Nepal, despite having no official mandate to work on these rights.
   This presents an important example of how such agencies are able to act
   independently to set their own agenda and illustrates the open system
   approach to international bureaucracies. It also suggests that these
   agencies have the potential to be important instruments of LGBTI rights
   promotion outside the traditional human rights machinery such as the
   Human Rights Council and various committees. Based on extensive
   interview research as well as documentary evidence, this article traces
   the origins of the UN's engagement with LGBTI rights. It then discusses
   the work of UN agencies in South Asia, and Nepal in particular, focusing
   on the UN Development Programme, the UN Children's Fund, UNAIDS, and UN
   Women. Political changes in Nepal since 2006 have opened it up for
   change in its approach to these rights, and UN agencies have worked
   actively to change both legal norms and social attitudes. The conclusion
   considers whether these lessons are applicable to other states and
   whether the UN development machinery must be considered an important
   ally in pursuing LGBTI rights worldwide.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
