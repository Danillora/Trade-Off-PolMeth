@article{ WOS:000423102600005,
Author = {Dunwoody, Philip T. and McFarland, Sam G.},
Title = {Support for Anti-Muslim Policies: The Role of Political Traits and
   Threat Perception},
Journal = {POLITICAL PSYCHOLOGY},
