@article{ WOS:000436615700006,
Author = {Schulzke, Marcus and Carroll, Amanda Cortney},
Title = {Corrective justice for the civilian victims of war: compensation and the
   right to life},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2018},
Volume = {21},
Number = {2},
Pages = {372-395},
Month = {APR},
Abstract = {The civilian right to life is a central concept in just war theory and
   international law. It plays an important role in restraining wars, as it
   is the basis for the principle of civilian immunity and the jus in bello
   principle of discrimination; yet, it is generally interpreted as only
   establishing a first-order duty, according to which belligerents must
   avoid harming civilians. In this essay, we argue that, in addition to
   this duty, the right to life also entails a second-order duty to
   compensate civilians whose right to life has been breached. We propose
   that states should have to pay compensatory damages to individual
   civilians harmed by their military actions and we discuss the standards
   of liability that can be used to determine when payment is owed.},
Publisher = {PALGRAVE MACMILLAN LTD},
