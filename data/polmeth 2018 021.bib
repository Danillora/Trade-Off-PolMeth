   Carroll, Amanda Cortney, Univ Leeds, Lifelongjoints Project, 2-46 Mech Engn Bldg, Leeds, W Yorkshire, England.},
DOI = {10.1057/jird.2015.34},
ISSN = {1408-6980},
EISSN = {1581-1980},
Keywords = {civilians; civilian immunity; jus post bellum; just war theory; justice},
