@article{ WOS:000433979700005,
Author = {Fromage, Diane and Ibrido, Renato},
Title = {The `Banking Dialogue' as a model to improve parliamentary involvement
   in the Monetary Dialogue?},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2018},
Volume = {40},
Number = {3, SI},
Pages = {295-308},
Abstract = {The newly established European Banking Union has introduced a unique
   accountability framework of the European Central Bank vis-a-vis the
   European Parliament (EP), but also vis-a-vis national parliaments, in
   the form of Banking Dialogue. The ECB was thus far held to account for
   its monetary policy actions by the EP in the framework of the
   long-existing Monetary Dialogue. However, some shortcomings have become
   apparent in this procedure, especially with the economic crisis. Beyond
   this, the recent attribution of new competences to the ECB calls for a
   reflection as to the accountability mechanisms in place to control its
   actions. Based on an empirical and normative analysis of both types of
   dialogues, we contend that the Banking Dialogue could serve as a model
   to reform the Monetary Dialogue to enhance the legitimacy of the ECB's
   actions and of the EU as a whole.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Ibrido, R (Corresponding Author), Univ LUISS Guido Carli, Dept Polit Sci, Rome, Italy.
   Fromage, Diane, Maastricht Univ, Law Fac, Maastricht, Netherlands.
   Ibrido, Renato, Univ LUISS Guido Carli, Dept Polit Sci, Rome, Italy.},
DOI = {10.1080/07036337.2018.1450406},
