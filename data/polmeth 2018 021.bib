@article{ WOS:000439366200004,
Author = {Irvin-Erickson, Douglas},
Title = {Prosecuting Sexual Violence at the Cambodian War Crimes Tribunal:
   Challenges, Limitations, and Implications},
Journal = {HUMAN RIGHTS QUARTERLY},
