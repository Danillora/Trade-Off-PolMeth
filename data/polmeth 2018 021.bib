@article{ WOS:000429730100001,
Author = {Gratius, Susanne},
Title = {Brazil and the European Union: from liberal inter-regionalism to realist
   bilateralism},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2018},
Volume = {61},
Number = {1},
Abstract = {Brazil-European Union relations punch below their weight. Cooperation
   takes place at three levels: relations with European Union (EU) member
   states, Brazil's partnership with Brussels, and EU-MERCOSUR
   negotiations. This multilevel governance contrasts with poor results:
   there is no free trade agreement, development cooperation became
   irrelevant, and international positions rarely converge. The article
   explores the reasons for the underperformance by comparing foreign
   policy shifts in Brazil and the EU, and analyzing multilevel governance
   in selected sectors of cooperation. It is based on four assumptions:
   multilevel relations are uncoordinated, idealist inter-regionalism
   doesn't work, and crisis-driven, liberal realist foreign policies in
   Brazil and the EU facilitate bilateralism.},
