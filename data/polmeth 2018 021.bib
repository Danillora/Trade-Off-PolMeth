   Osborne, Matthew; D'Exelle, Ben; Verschoor, Arjan, Univ East Anglia, Sch Int Dev, Norwich, Norfolk, England.
   Osborne, Matthew, Stockholm Environm Inst, Stockholm, Sweden.},
DOI = {10.1177/0022343317729011},
ISSN = {0022-3433},
EISSN = {1460-3578},
Keywords = {behavioural experiments; ex-combatants; northern Uganda; post-conflict
   social reintegration; social tie survey},
Keywords-Plus = {PUBLIC-GOODS EXPERIMENTS; CIVIL-WAR; ECONOMIC-GROWTH; RECIPROCITY;
