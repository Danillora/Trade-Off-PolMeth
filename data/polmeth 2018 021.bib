Web-of-Science-Categories  = {Economics; Political Science; Sociology},
Author-Email = {marento@sosgeo.uio.no},
Affiliations = {University of Oslo},
Funding-Acknowledgement = {Socio-Economic Review},
Funding-Text = {I am grateful to Marianne Nordli Hansen and Johs Hjellbrekke as well as
   two anonymous reviewers from the Socio-Economic Review for critical
