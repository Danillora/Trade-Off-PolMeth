@article{ WOS:000438583400003,
Author = {De Bruin, Erica},
Title = {Preventing Coups d'etat: How Counterbalancing Works},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2018},
Volume = {62},
Number = {7},
Pages = {1433-1458},
Month = {AUG},
Abstract = {Although coups remain a pressing concern for rulers across the globe,
   the mechanisms through which common coup-prevention strategies operate
   have not been well theorized or rigorously tested. This article analyzes
   how counterbalancing the military with other security forces prevents
   coups. Using new cross-national time-series data on state security
   forces along with evidence from sixteen case studies, the article
   demonstrates that counterbalancing reduces the likelihood that coup
   attempts will succeed and that it does so primarily by creating
   incentives for some soldiers to resist the coup, rather than by creating
   barriers to coordination between forces. However, counterbalancing is
   not associated with fewer coup attempts. In fact, the creation of a new
   security force increases the odds of a coup attempt in the following
   year. These findings highlight potential costs associated with
   counterbalancing and explain why it is not more widespread.},
