@article{ WOS:000440173700010,
Author = {Podlipnik, Jernej and Ilic-Popov, Gordana},
Title = {Self-imposed Contribution in Slovenian and Serbian Legislation: Concept
   and Legal Nature},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2018},
Volume = {16},
Number = {3},
Pages = {613-630},
Month = {JUL},
Abstract = {Sometimes, in order to provide themselves better living conditions, the
   residents of a certain municipality may be willing to pay an additional
   fiscal levy besides ordinary taxes. In this paper, the authors deal with
   a self-imposed contribution as a possible resource of local budgets,
   comparing the regulation and concept of such a contribution in Slovenia
   and Serbia. By analysing the characteristics of such a levy in these two
   former Yugoslav states, the authors based their main hypothesis on
   proving that self-imposed contribution is, according to its legal
   nature, also a type of a tax, even if it is introduced under a
   referendum and the funds are earmarked for specific local purposes.},
