@article{ WOS:000446247800003,
Author = {Souto Olmedo, Veronica S.},
Title = {HUMANITY IN LIMBO. TURKEY'S INTERNATIONAL PROTECTION SYSTEM IN THE FACE
   OF THE HUMANITARIAN CRISIS ARISING FROM THE CONFLICT IN SYRIA},
Journal = {FORO INTERNACIONAL},
Year = {2018},
Volume = {58},
Number = {4},
Pages = {719-754},
Month = {OCT-DEC},
Abstract = {The Syrian conflict has caused a humanitarian catastrophe with 6.1
   million internally displaced persons and more than 5 million refugees.
   Turkey is a key player in the conflict because of its military
   involvement and because it hosts 3.5 million registered Syrian refugees.
   This paper focuses on the humanitarian crisis and seeks to verify the
   relationship between the Turkish international protection system and the
   strategy to manage this crisis, which has been based on two premises: on
   the one hand, the temporary character of the refugees and, on the other
   hand, the provision of services, rather than the recognition of rights.},
