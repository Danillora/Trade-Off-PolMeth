@incollection{ WOS:000441825100015,
Author = {Zaluski, Wojciech},
Editor = {HuppesCluysenaer, L and Coelho, NMMS},
Title = {Virtue as a Synthesis of Extremes Versus Virtue as a Mean Between
   Extremes: A Comparison of Chesterton's Account of Virtue with
   Aristotle's},
Booktitle = {ARISTOTLE ON EMOTIONS IN LAW AND POLITICS},
Series = {Law and Philosophy Library},
Year = {2018},
Volume = {121},
Pages = {297-311},
Abstract = {Both Aristotle and Gilbert Keith Chesterton asserted that virtue can be
   analyzed by means of a `tripartite' scheme, viz. as being opposed to its
   two `correlated' extremes (the excess of or the deficiency in a passion
   or action to which the virtue refers). However, while according to
   Aristotle virtue is radically different from its `correlated' extremes
   (is `the mean' between them), according to Chesterton, virtue, rather
   than being radically different from extremes, is a paradoxical synthesis
   of them, holding them in an ethically creative tension. The paper aims
   at elucidating this-Chestertonian-account of virtue, providing examples
   of virtues which can be plausibly analyzed in accordance with it
   (including, for instance, courage, modesty, charity), and comparing it
   to Aristotle's account.},
