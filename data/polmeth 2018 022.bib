Author = {Lilli, Eugenio},
Title = {Debating US Military Strategy in the Persian Gulf: What is the Way
   Forward?},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2018},
Volume = {61},
Number = {1},
Abstract = {Should the US strategy toward the Gulf be one of offshore balancing or
   one of deep engagement? The debate on US grand strategy lacks solid
   empirical ground. I address this issue by providing a study of the US'
   role as the Gulf's security provider. I investigate the extent to which
   distinct military strategies have affected the stability of the region.
   My findings show no clear correlation between increased US military
   presence and a reduction in either the incidence or the intensity of
   regional armed conflict, possibly lending credibility to the arguments
   of the advocates of a strategy of offshore balancing.},
