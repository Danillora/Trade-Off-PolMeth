@article{ WOS:000440754500001,
Author = {Ricart, Carlos A. Perez},
Title = {THE ROLE OF THE DEA IN THE EMERGENCE OF THE FIELD OF COUNTERNARCOTICS
   POLICING IN LATIN AMERICA},
Journal = {FORO INTERNACIONAL},
Year = {2018},
Volume = {58},
Number = {1},
Pages = {5-48},
Month = {JAN-MAR},
Abstract = {In this paper we examine the institutional responses that emerged in
   Latin America to drug trafficking in the 1970s. It explores, in
   particular, the emergence of the field of counternarcotics policing at
