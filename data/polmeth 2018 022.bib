DOI = {10.1177/0010836717728539},
ISSN = {0010-8367},
EISSN = {1460-3691},
Keywords = {International relations theory; just war theory; pacifism; realism},
Keywords-Plus = {INTERNATIONAL-POLITICS; REFLEXIVE REALISM; SELF-DEFENSE; WAR THEORY;
   RESPONSIBILITY},
