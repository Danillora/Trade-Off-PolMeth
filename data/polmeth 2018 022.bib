@article{ WOS:000432961300002,
Author = {Cook, Rebecca},
Title = {Sir Nigel Rodley's Insights on the Feminist Transformation of the Right
   of Conscience},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2018},
Volume = {40},
Number = {2},
Pages = {255-259},
Month = {MAY},
Abstract = {This article explains Sir Nigel Rodley's ``feminist transformation{''}
   through his spirited concurring judgment in the case of Amanda Mellet v.
   Ireland decided by the UN Human Rights Committee. In his concurrence, he
   emphasized that the state party's refusal to allow abortions in cases of
   fatal fetal anomaly cannot be justified even as protecting potential
   life. He was especially critical of the state for violating Mellet's
   right to be free from inhuman and degrading treatment by subjecting her
   to the anguish of carrying a doomed foetus to term. Noting his life's
   work with prisoners of conscience, the author wonders how Sir Nigel
   might have provided a feminist perspective in finding a violation of the
   right of conscience, had that right been argued in the Mellet case.},
