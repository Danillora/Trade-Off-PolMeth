@incollection{ WOS:000472929000008,
Author = {Eckert, Sandra},
Editor = {Ege, J and Bauer, MW and Becker, S},
Title = {The European Commission as a Negotiator - Evidence from the
   Disintegration Talks with the United Kingdom and Switzerland},
Booktitle = {EUROPEAN COMMISSION IN TURBULENT TIMES: ASSESSING ORGANIZATIONAL CHANGE
   AND POLICY IMPACT},
Series = {Schriftenreihe des Arbeitskreises Europaische Integration e.V.},
Year = {2018},
Volume = {105},
Pages = {159-180},
Abstract = {This chapter analyses the role of the European Commission in the
   disintegration talks with the United Kingdom and Switzerland. It
   systematically compares three negotiation processes: first, the
   intergovernmental negotiations with the UK as a member state granting a
   special deal in the run-up to the June 2016 referendum; second, the
   attempts by Switzerland to redefine its bilateral relations with the EU
   between 2014 and 2017; third, the first round of Brexit negotiations
   launched in June 2017 and concluded in early December 2017. The case
   study findings illustrate how the Commission makes use of its various
   powers in a negotiation process. Although negotiations with the UK prior
   to the Brexit vote were an intergovernmental bargain, the Commission
   nevertheless did successfully draw on its resources as legal guardian
   and policy initiator. By contrast, the Commission was the chief
   negotiator in the Schwexit and Brexit talks. In this role the Commission
   exerted considerable pressure vis-a-vis the negotiation partners, and
   effectively posited the indivisibility of the four freedoms as
   non-negotiable. The chapter concludes that the European Commission plays
   the lead as chief negotiator when all member states can align their
   interests and agree on a common cause.},
