@article{ WOS:000455328900003,
Author = {Xu, Guoqi},
Title = {The year 1919 and the question of ``what is China?{''}},
Journal = {INTERNATIONAL POLITICS},
Year = {2018},
Volume = {55},
Number = {6, SI},
Pages = {752-764},
Month = {NOV},
Abstract = {For the Chinese, the Great War might have been principally a war among
   white people, a European war, and a war between Western powers. But to
   the degree that China got involved in the war, the experience of the war
   and its aftermath forced Chinese to think about who they were and what
   their position in the world was and could be. In 1919, they reconsidered
   the relationship between Asia and the West, between Eastern and Western
   civilizations, with an eye to deciding how best to protect and advance
   themselves in the wake of the war. Given their collective disappointment
   with outcomes of the Paris Peace Conference, Chinese people came away
   with vastly diminished expectations of and respect for the Western
   powers. The resulting May Fourth Movement of 1919 has already been
   studied from many different angles, so this paper will instead address
   the cultural effects and civilizational significance of the Great War
   for China and the Chinese.},
