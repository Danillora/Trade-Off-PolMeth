@article{ WOS:000428469000001,
Author = {Vivekanandan, Jayashree},
Title = {Indianisation or indigenisation? Greater India and the politics of
   cultural diffusionism},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2018},
Volume = {56},
Number = {1},
Pages = {1-21},
Abstract = {The paper focuses on the notion of Greater India that celebrated the
   diffusion of Indian cultural practices in Asia. Although prominent in
   the early twentieth century, it continued to fascinate Indian statesmen
   in the post-independence period. Juxtaposing its cultural frame with
   alternative conceptions of India's self-identity, the paper argues that
