ISSN = {1053-1858},
EISSN = {1477-9803},
Keywords-Plus = {ORGANIZATIONAL CITIZENSHIP BEHAVIOR; LEADER-MEMBER EXCHANGE; EMOTIONAL
   LABOR; EMPIRICAL-EXAMINATION; JOB-SATISFACTION; CUSTOMER SATISFACTION;
