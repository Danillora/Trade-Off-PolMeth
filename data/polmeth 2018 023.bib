@article{ WOS:000428127800002,
Author = {Asal, Victor and Ayres, R. William},
Title = {Attention Getters: Diaspora Support for Ethno-Political Organizations in
   the Middle East},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2018},
Volume = {41},
Number = {1},
Pages = {24-38},
Abstract = {Why do some ethno-political organizations get support from their
   diaspora while others do not? There is little analysis that examines why
   some organizations (both violent and nonviolent) get support. Using data
   on 112 organizations in the Middle East we examine how factors like the
   power of the organization, ideology, political behavior, and government
   treatment might impact the likelihood of an organization getting support
   from its diaspora. We argue that contentious political behavior should
   have the largest impact on such support. We find that those that do the
   best job of getting attention through visible action get the most
   support.},
