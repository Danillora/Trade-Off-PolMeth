@article{ WOS:000434164000004,
Author = {Vasilopoulos, Pavlos and Marcus, George E. and Foucault, Martial},
Title = {Emotional Responses to the Charlie Hebdo Attacks: Addressing the
   Authoritarianism Puzzle},
