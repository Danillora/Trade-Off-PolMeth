@article{ WOS:000424059900003,
Author = {Fisk, Kerstin},
Title = {One-sided Violence in Refugee-hosting Areas},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2018},
Volume = {62},
Number = {3},
Pages = {529-556},
Month = {MAR},
Abstract = {Previous research demonstrates that refugee populations can threaten the
   security of receiving countries. This study, in contrast, seeks to
   examine the physical security challenges refugees face in host states.
   It utilizes a new, geographically referenced data set on subcountry
   refugee demographics to test the hypothesis that locations home to
   larger refugee populations are more likely to experience one-sided
   attacks by conflict actors. Results demonstrate that refugee
   accommodation is a significant predictor of one-sided violence in
   Africa. In particular, combatants commit significantly more acts of
   violence against civilians in locations home to larger numbers of
   self-settled refugees compared to other locations. These findings
   suggest that scholars and practitioners account for possible dangers
   presented by refugee flows and threats to refugees simultaneously.},
