   Van de Graaf, Thijs; Haesebrouck, Tim; Debaere, Peter, Univ Ghent, Dept Polit Sci, Ghent, Belgium.},
DOI = {10.1080/13501763.2017.1301985},
ISSN = {1350-1763},
EISSN = {1466-4429},
Keywords = {Comparative public policy; hydraulic fracturing ({''}fracking'); (risk)
   regulation; shale gas; qualitative comparative analysis (QCA)},
Keywords-Plus = {ECONOMIC-GROWTH; PUBLIC-OPINION; POLICY; RESPONSES; FRACKING;
   INSTITUTIONS; STATES},
Research-Areas = {Government \& Law; Public Administration},
