@article{ WOS:000449714200003,
Author = {Carter, Neil and Childs, Mike},
Title = {Friends of the Earth as a policy entrepreneur: `The Big Ask' campaign
   for a UK Climate Change Act},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2018},
