@article{ WOS:000447801200009,
Author = {Joy, Meghan and Shields, John},
Title = {Austerity in the making: reconfiguring social policy through social
   impact bonds},
Journal = {POLICY AND POLITICS},
Year = {2018},
Volume = {46},
Number = {4},
Pages = {681-695},
Month = {OCT},
Abstract = {Social impact bonds (SIBs) are spreading worldwide in the absence of
   public debate and proof that they improve social outcomes, despite
   claims to advance evidence-based policymaking and transparency in
   government. A critical policy reading of SIBs elucidates this
   conundrum.We argue that SIBs are part of a neoliberal political project
   that uses austerity as an economic and moral symbol to manufacture
   consent for social service reform.The intent of this research
   provocation is to identify future avenues for empirical research on SIBs
   to further assess how the tool reconfigures social policy in a
   profoundly neoliberal direction.},
