@article{ WOS:000442666300007,
Author = {Hanson, Marianne},
Title = {Normalizing zero nuclear weapons: The humanitarian road to the
   Prohibition Treaty},
Journal = {CONTEMPORARY SECURITY POLICY},
Year = {2018},
Volume = {39},
Number = {3, SI},
Pages = {464-486},
Abstract = {The nuclear age has come to be seen as ``normal,{''} marked by a process
   of ``nuclearism{''} whereby nuclear weapons and deterrence are seen as
   inevitable and acceptable elements of international security. Factors
   which have allowed this to flourish include the relative absence of
   humanitarian considerations, nuclear decision-making by a select few,
   and the unequal nature of the Non-Proliferation Treaty (NPT), where the
   P5 states have shaped the nuclear order on their own terms. The
   ``humanitarian initiative{''} and Treaty on the Prohibition of Nuclear
   Weapons challenge this ``normal{''} nature of nuclear weapons,
   re-casting them as incompatible with humanitarian law, and
   delegitimizing them for all states. This shift away from the structural
   constraints of the NPT allows non-nuclear states a degree of agency they
   did not previously possess. Nonetheless, the Treaty faces difficulty in
   dislodging the practices of the nuclear-weapon states, suggesting that
   its value lies in its long-term normative influence.},
