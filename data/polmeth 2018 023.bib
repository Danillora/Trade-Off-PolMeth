@article{ WOS:000435449500002,
Author = {Gourlay, William},
Title = {Oppression, Solidarity, Resistance: The Forging of Kurdish Identity in
   Turkey},
Journal = {ETHNOPOLITICS},
Year = {2018},
Volume = {17},
Number = {2},
Pages = {130-146},
Abstract = {This paper examines the intersection of oppression and Kurdish
   resistance to the state in Turkey and the impacts these have on the
   formation of ethnic identity amongst the Kurds of Diyarbakir. It
   examines how repressive state measures imposed upon the Kurds,
   ostensibly to crush the PKK, rallied Kurdish political sentiment such
   that resistance to state hegemony expanded to encompass a much broader
   `popular resistance'. Resistance by `everyday' Kurds to what they
   perceive as hegemonic projects, whether instigated by Kemalists or the
   AKP, continues to forge internal cohesion and highlight their
   differences from the majority Turks. In this way, resistance becomes a
   central pillar of Kurdish identity.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
