@article{ WOS:000428342100008,
Author = {van der Bles, Anne Marthe and Postmes, Tom and LeKander-Kanis, Babet and
   Otjes, Simon},
Title = {The Consequences of Collective Discontent: A New Measure of Zeitgeist
   Predicts Voting for Extreme Parties},
Journal = {POLITICAL PSYCHOLOGY},
