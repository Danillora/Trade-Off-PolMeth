@article{ WOS:000433985000001,
Author = {Tan, Netina and Grofman, Bernard},
Title = {Electoral rules and manufacturing legislative supermajority: evidence
   from Singapore},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2018},
Volume = {56},
Number = {3},
Pages = {273-297},
Abstract = {Electoral authoritarian regimes usually preserve the dominance of the
   ruling party through electoral fraud, violence and intimidation. This
   paper focuses on the subtler forms of manipulation that undermine the
   electoral integrity and democratic outcomes. Specifically, we examine
   how an unusual electoral rule, involving multimember districts elected
   through plurality bloc voting for party slates, exaggerates the
   legislative seat shares of the People's Action Party (PAP) in Singapore.
   This rule, used also by other electoral authoritarian regimes,
   facilitates the manipulation of district magnitude and gerrymandering,
   especially the `stacking' form, to produce a large disproportionality
   which distorts the seats-votes linkage. It operates in an undemocratic
   fashion by precluding the opposition from gaining anything but token
   seats as long as the PAP remains the plurality-winning party. The
   importance of this electoral rule and its manipulation has been
   overlooked in current work that emphasises redistributive strategies or
   coercion to repress electoral competition.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Tan, N (Corresponding Author), McMaster Univ, Dept Polit Sci, Hamilton, ON, Canada.
   Tan, Netina, McMaster Univ, Dept Polit Sci, Hamilton, ON, Canada.
   Grofman, Bernard, Univ Calif Irvine, Ctr Study Democracy, Dept Polit Sci, Irvine, CA USA.},
