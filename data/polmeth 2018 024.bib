@article{ WOS:000443996800009,
Author = {Ahuja, Amit and Ostermann, Susan L.},
Title = {From quiescent bureaucracy to ``undocumented wonder{''}: Explaining the
   Indian Election Commission's expanding mandate},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2018},
Volume = {31},
Number = {4},
Pages = {759-776},
Month = {OCT},
Abstract = {Public institutions in the developing world are often characterized by
   institutional capture or decay. Yet, India's Election Commission (EC)
   has become one of its most powerful regulatory bodies. We use a
   process-tracing approach to explain the EC's surprising expansion of
   mandate, arguing that in a federal democracy: (a) when institutional
   constraints are weakened, (b) when state-based actors demand a competent
   and neutral arbiter, and (c) when entrepreneurial bureaucratic actors
   take advantage of moments of political opportunity, those aspects of the
   bureaucracy that can credibly meet these demands are able to
   successfully expand their powers. Changes in Model Code Implementation
   and Election Duration attest to the EC's broader role. The EC's
   experience suggests that a weak executive can facilitate the
   strengthening of state institutions and that, under federalism,
   state-based electoral forces can produce strong national institutions.},
