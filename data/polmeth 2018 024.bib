@article{ WOS:000427192800006,
Author = {Feraru, Atena S.},
Title = {Regime Security and Regional Cooperation Among Weak States},
Journal = {INTERNATIONAL STUDIES REVIEW},
Year = {2018},
Volume = {20},
Number = {1},
Pages = {101-126},
Month = {MAR},
Abstract = {This article focuses on a category of states, referred to as weak,
   proven to evade entrenched international relations theoretical
   assumptions relating to sovereignty and government interests. It
   renounces these premises and builds on the argument that state weakness
   generates a peculiar security predicament that shapes governments'
   internal agenda. It focuses on Migdal's (1988) findings of a regime
   security-centered mindset shared by weak states' leaders and asks the
   following: does this common mindset generate a common approach to
   regional cooperation? It answers this question by inferring a set of
   strategies toward external cooperation and proceeds by testing their
   empirical validity in relation to three of the most integrated regional
   organizations in Southeast Asia, Africa, and the American continent.},
Publisher = {OXFORD UNIV PRESS},
