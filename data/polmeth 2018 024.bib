@incollection{ WOS:000504215900011,
Author = {Condello, Angela},
Editor = {Huygebaert, S and Condello, A and Marusek, S and Antaki, M},
Title = {Aesthetic Mediation: Towards Legitimate Power},
Booktitle = {SENSING THE NATION'S LAW: HISTORICAL INQUIRIES INTO THE AESTHETICS OF
   DEMOCRATIC LEGITIMACY},
Series = {Studies in the History of Law and Justice},
Year = {2018},
Volume = {13},
Pages = {265-284},
Abstract = {In this chapter I try to explain why the nation, its unity and
   democratic legitimacy at a certain moment in history had to become
   visible, tangible, and more generally perceivable. There were indeed
