@article{ WOS:000424916000004,
Author = {Currie, Shannon and Choma, Becky},
Title = {Sociopolitical ideology and the morality of green behaviour},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2018},
Volume = {27},
Number = {2},
Pages = {247-266},
Abstract = {Combating climate change and protecting the environment is a significant
   topic amongst political leaders and concerned citizens across the globe.
   The degree to which individuals perceive green issues as important is
   connected to their politics, those subscribing to conservative
   ideologies being less supportive of pro-environmental initiatives and
   less concerned about climate change than those endsorsing liberal
   ideologies. Examining political ideology as a uni- and bi-dimensional
   construct, the present research investigated whether political liberals
   (vs. conservatives), those higher (vs. lower) in right-wing
   authoritarianism, and those higher (vs. lower) in social dominance
   orientation differ in their perceptions of the morality of green
   behaviours. In Studies 1 (n = 200) and 2 (n = 150), liberal (vs.
   conservative) ideologies related to perceiving pro-environmental
   behaviours as more moral. Further, in Study 2, the relation between
   ideology and perceptions of green behaviours as moral was explained by
   positive feelings associated with the self engaging in green behaviours.},
