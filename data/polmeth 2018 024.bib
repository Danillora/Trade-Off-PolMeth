@article{ WOS:000429052700002,
Author = {El Machnouk, Saleh},
Title = {Electoral System Reform in Lebanon: Dilemmas of a Consociational State},
Journal = {ETHNOPOLITICS},
Year = {2018},
Volume = {17},
Number = {1},
Pages = {1-20},
Abstract = {Lebanon's complex consociational democracy has been the subject of
   intense scholarly debate, especially with regard to its outdated and
   controversial electoral system. This paper argues that the current
   majoritarian electoral lawsserves to undermine democracy and exacerbate
   sectarian tensions, as it now runs counter to the original
   integrationist goals for which it was designed. After exploring the
   numerous existing reform proposals within the broader debate on power
   sharing in divided states, an alternative electoral formula that
   accounts for Lebanon's political geography and reform objectives shall
   be proposed. This multi-phased approach could in turn provide a
   framework for electoral engineering in other conflict-ridden,
   multi-ethnic states.},
