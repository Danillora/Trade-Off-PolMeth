@article{ WOS:000427279100006,
Author = {Shield, Ralph},
Title = {The Saudi air war in Yemen: A case for coercive success through
   battlefield denial},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2018},
Volume = {41},
Number = {3},
Pages = {461-489},
Abstract = {The present-day conflict in Yemen is a valuable case study in the
   coercive use of airpower. The Saudi-led bombing campaign demonstrates
   the challenges inherent in applying a punishment-based strategy in a
   civil war, where coercive mechanisms operate differently than in
   interstate conflict. The audit from Yemen vindicates the effectiveness
   of a denial-based strategy and offers insights on the relative utility
   of interdiction and close air support in advancing that strategy. This
   examination dissects the campaign, assesses its effectiveness, and
   extracts insights useful both to the analysis of similar conflicts and
   to the evaluation of U.S. foreign policy alternatives.},
