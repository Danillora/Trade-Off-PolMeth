@article{ WOS:000443340400003,
Author = {Dressler, Matteo},
Title = {Decentralising power: building inclusive peace? The European Union's
   support to governance reform in Eastern Ukraine},
Journal = {PEACEBUILDING},
Year = {2018},
Volume = {6},
Number = {3, SI},
Pages = {201-217},
Abstract = {This article contributes to the ongoing debate on what role
   decentralisation processes can play in building sustainable peace. More
   specifically, the article reflects upon the European Union's (EU's)
   support to the implementation of decentralisation reform in
   government-controlled areas of Eastern Ukraine, examining the EU's
   support to civil society and local authorities in areas affected by the
   armed conflict. A Whole-of-Society lens is employed to analyse the
   topic, paying special attention to connections between multiple levels
   of relevant actors in the reform process. The final discussion reveals
   multiple challenges for decentralisation reform in Eastern Ukraine such
   as insufficient communication by the national government with local
   authorities and lack of participation of broader segments of society in
   the process. However, it also shows the reform's potential for
   empowering local actors to take on issues such as social cohesion and
   gender violence.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
