@article{ WOS:000430100500001,
Author = {Fleming, Jenny and Rhodes, Rod},
Title = {Can experience be evidence? Craft knowledge and evidence-based policing},
Journal = {POLICY AND POLITICS},
Year = {2018},
Volume = {46},
Number = {1},
Pages = {3-26},
Month = {JAN},
Abstract = {This article explores the use of evidence and varieties of knowledge in
   police decision making. It surveys official government policy,
   demonstrating that evidence-based policymaking is the dominant
   policy-making paradigm in the United Kingdom. It discusses the limits to
   social science knowledge in policymaking. The article explores four
   ideas associated with the notion of `experience': occupational culture,
   institutional memory, local knowledge and craft, drawing on data from
   four UK police forces. We discuss the limits to experiential knowledge
   and conclude that experience is crucial to evidence-based policing and
   decision-making because it is the key to weaving the varieties of
   knowledge together.},
Publisher = {POLICY PRESS},
