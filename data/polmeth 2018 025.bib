@incollection{ WOS:000472929000004,
Author = {Maurer, Andreas and Wolf, Michael C.},
Editor = {Ege, J and Bauer, MW and Becker, S},
Title = {Agenda-Shaping in the European Parliament and the European Commission's
   right of legislative initiative},
Booktitle = {EUROPEAN COMMISSION IN TURBULENT TIMES: ASSESSING ORGANIZATIONAL CHANGE
   AND POLICY IMPACT},
Series = {Schriftenreihe des Arbeitskreises Europaische Integration e.V.},
Year = {2018},
Volume = {105},
Pages = {53-83},
Abstract = {The European Parliament (EP) is the only democratically elected body in
   the European Union. Yet, unlike most parliaments, it has no formal right
   of legislative initiative. Initiating legislation lies almost solely
   with the EU's executive bodies, the European Commission, and -to a
   limited extend -the European Council and the Council of Ministers. This
   paper investigates how the EP can and does influence the Commission to
   initiate legislation and to hold the Commission accountable in these
   areas. We therefore examine the process, institution, and impact of the
   EP's ``Non-Legislative Own-Initiative Reports{''} (INI) and its
   ``Legislative OwnInitiative Reports{''} (INL, based on Art. 225 TFEU).
   The paper covers the 5th, 6th and 7th legislative terms. Based on a
   complete and original set of data on INI-and INL-dossiers, it offers key
   insights into the informal (co-) leadership capacities of the European
   Parliament.},
