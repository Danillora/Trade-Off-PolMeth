@article{ WOS:000436046700001,
Author = {Wig, Tore and Kromrey, Daniela},
Title = {Which groups fight? Customary institutions and communal conflicts in
   Africa},
Journal = {JOURNAL OF PEACE RESEARCH},
Year = {2018},
Volume = {55},
Number = {4},
Pages = {415-429},
Month = {JUL},
Abstract = {Why are some ethnic groups embroiled in communal conflicts while others
   are comparably peaceful? We explore the group-specific correlates of
   communal conflicts in Africa by utilizing a novel dataset combining
   ethnographic information on group characteristics with conflict data.
   Specifically, we investigate whether features of the customary political
   institutions of ethnic groups matter for their communal-conflict
   involvement. We show how institutional explanations for conflict,
   developed to explain state-based wars, can be successfully applied to
   the customary institutions of ethnic groups. We argue that customary
   institutions can pacify through facilitating credible nonviolent
   bargaining. Studying 143 ethnic groups, we provide large-N evidence for
