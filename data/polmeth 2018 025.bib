@article{ WOS:000432658500003,
Author = {Weeks, Douglas},
Title = {Doing Derad: An Analysis of the UK System},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2018},
Volume = {41},
Number = {7, SI},
Pages = {523-540},
Abstract = {Since 2005, Britain has utilized a handful of interventionists to engage
   with those at risk of violent extremism and those convicted of terrorism
   related charges in order to manage its risk of terrorism. The goal of
   the interventionists is to deradicalize those that they interact with to
   facilitate their reintegration in society. This article discusses the
   mentoring environment in the United Kingdom, how intervention providers
   establish their credibility with individuals, communities, and
   government, some of the structural safeguards and their impact on
   mentoring, how success is conceived, and the absence of reporting.},
