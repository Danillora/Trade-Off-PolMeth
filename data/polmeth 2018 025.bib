@article{ WOS:000453026600007,
Author = {Peitz, Linus and Dhont, Kristof and Seyd, Ben},
Title = {The Psychology of Supranationalism: Its Ideological Correlates and
   Implications for EU Attitudes and post-Brexit Preferences},
