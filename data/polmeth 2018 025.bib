@article{ WOS:000434162900011,
Author = {Strother, Logan},
Title = {The National Flood Insurance Program: A Case Study in Policy Failure,
   Reform, and Retrenchment},
Journal = {POLICY STUDIES JOURNAL},
Year = {2018},
Volume = {46},
Number = {2},
Pages = {452-480},
Month = {MAY},
Abstract = {In this paper, I present an intensive case study of the development of
   the National Flood Insurance Program to advance two key arguments.
   First, the conventional model of adoption of general interest reform
   neglects an important aspect of political context: whether the relevant
   policy domain is one with or without publics. I argue that in domains
   without publics the politics of reform will differ substantially from
   the accepted model. Second, I argue that the type of learning necessary
   to address a given policy failure matters in reform politics.
   Instrumental learning is necessary but may not be sufficient for
   successful general interest reforms. When the social construction of a
   policy failure is such that many people misconceive of the fundamental
   purpose of a policy, social learning must take place before instrumental
   learning can be effective.},
Publisher = {WILEY},
