@article{ WOS:000429052700004,
Author = {O'Driscoll, Dylan},
Title = {Conflict in Kirkuk: A Comparative Perspective of Cross-regional Self-
   determination Disputes},
Journal = {ETHNOPOLITICS},
Year = {2018},
Volume = {17},
Number = {1},
Pages = {37-54},
Abstract = {There has been a failure to address the future of Kirkuk through
   political means, whilst tensions are once again coming to the fore there
   is a real risk that Kirkuk's future will be decided by force. This
   article examines the political options available for Kirkuk based on the
   resolution of similar issues of conflict in Northern Ireland and Breko,
   Bosnia and Herzegovina. Based on the advances made in these cases the
   article suggests that exogenous actors will have to play a role in
   reaching an agreement in Kirkuk, that a special status for Kirkuk is
   necessary, and that power sharing needs to be properly
   institutionalised.},
