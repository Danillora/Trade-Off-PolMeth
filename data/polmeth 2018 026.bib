@article{ WOS:000449781400006,
Author = {Yang, Yi Edward and Keller, Jonathan W. and Molnar, Joseph},
Title = {An Operational Code Analysis of China's National Defense White Papers:
   1998-2015},
Journal = {JOURNAL OF CHINESE POLITICAL SCIENCE},
Year = {2018},
Volume = {23},
Number = {4},
Pages = {585-602},
Month = {DEC},
Abstract = {Scholarly efforts to predict the future character of the U.S.-China
   relationship abound. Few however looks to leaders' beliefs as valid
   explanatory variables. In this paper, we argue that state leaders'
   belief systems are key to understanding both the states' intentions and
   policy choices. We analyze China's national defense white papers
   (1998-2015) published to date as the source material to gauge the core
   collective beliefs of three generations of Chinese leadership. The
   operational code framework is employed to conceptualize and measure
   these beliefs. Our results identified important crossgenerational
   changes in a few belief indicators. In the Xi Jinping era, for instance,
   the political world is seen as less friendly and cooperative strategies
   are viewed less favorably. In terms of tactics, the policy tools
   threaten and promise are viewed as significantly more useful by the
   current leadership than by past Chinese leaders.},
