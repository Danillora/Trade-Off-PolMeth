@article{ WOS:000435218300001,
Author = {Barone, Sylvain},
Title = {Building a narrative on environmental policy success. Reflections from a
   watershed management experience},
Journal = {CRITICAL POLICY STUDIES},
Year = {2018},
Volume = {12},
Number = {2},
Pages = {135-148},
Abstract = {This article deals with the advent of a narrative on environmental
   policy success. While a positivist position would begin by determining a
   certain number of supposedly objective criteria of success, we consider
   success as the result of a process aiming at stabilizing a particular
   interpretation of a situation. We study the case of a French lagoon
   presented by many actors as a success in terms of watershed and
   environmental management. We seek to analyze how this narrative on
   success was built and became dominant thanks to the intervention of
   particular actors and how, in return, these actors managed to reinforce
   their domination, both in the territory under study and in the water
   policy sector in France.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
