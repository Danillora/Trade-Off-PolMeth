@article{ WOS:000429391700005,
Author = {Tarlea, Silvana and Freyberg-Inan, Annette},
Title = {The education skills trap in a dependent market economy. Romania's case
   in the 2000s},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2018},
Volume = {51},
Number = {1},
Pages = {49-61},
Month = {MAR},
Abstract = {We discuss the political economic development of. Romania since 1989,
   with a focus on the evolution of higher education (HE). First, we place
   this evolution in the context of demand for. HE by prospective students
   and employers, focusing on the low demand for skills in the
   MNC-dominated Romanian economy. Second, we provide empirical insight on
   indicators of quality, enrolment, and funding as key features of the HE,
   system. We argue that Romania has evolved into a dependent market
   economy entrenched in a low-skills equilibrium, and that the weakness of
   the HE system is a key element in this process. (C) 2018 The Regents of
   the University of California. Published by Elsevier Ltd. All rights
   reserved.},
