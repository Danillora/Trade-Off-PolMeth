@article{ WOS:000436972800006,
Author = {Cappelen, Cornelius and Peters, Yvette},
Title = {Diversity and welfare state legitimacy in Europe. The challenge of
   intra-EU migration},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2018},
Volume = {25},
Number = {9},
Pages = {1336-1356},
Abstract = {It has convincingly been argued that in the United States, immigration
   is detrimental to welfare state support. In Europe, on the other hand,
   there is little evidence for such a negative relationship. This article
   examines whether a particular type of immigration - rapidly escalating
   intra-European Union (EU) immigration - reduces public support for the
   welfare state. We argue that because EU states cannot discriminate
   against resident EU nationals from other countries in the field of
   social security, intra-EU immigration will negatively affect attitudes
   towards welfare spending. To test this expectation, we use data from the
   European Social Survey of 2008, among other sources. The results of our
   multilevel analyses document a negative relationship between intra-EU
   immigration and welfare state support, supporting our hypothesis.},
