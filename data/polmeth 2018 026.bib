@incollection{ WOS:000472929000007,
Author = {Toemmel, Ingeborg},
Editor = {Ege, J and Bauer, MW and Becker, S},
Title = {Political Leadership in Turbulent Times - the Commission Presidency of
   Jean-Claude Juncker},
Booktitle = {EUROPEAN COMMISSION IN TURBULENT TIMES: ASSESSING ORGANIZATIONAL CHANGE
   AND POLICY IMPACT},
Series = {Schriftenreihe des Arbeitskreises Europaische Integration e.V.},
Year = {2018},
Volume = {105},
Pages = {133-157},
Abstract = {Despite controversies about ``new intergovernmentalism{''}, most
   scholars agree that the Commission has recently lost influence in
   European affairs. This raises the question whether Commission presidents
   can still exert political leadership. Such leadership aims at shaping EU
   policy-making and deepening integration. In the present situation, it
   might also focus on strengthening the position of the Commission. This
   chapter explores the leadership performance of Jean-Claude Juncker as
   Commission president. It questions whether and to which extent Juncker
   pursues the three goals mentioned above. Drawing on leadership theory
   and recent insights into leadership at European level, the paper
   elaborates an analytical framework focused on Juncker's performance.
   Empirically, the chapter analyses Juncker's performance in using
   strategically his agenda-setting and procedural powers for shaping EU
   policy-making, deepening integration and strengthening the position of
   the Commission. It concludes that Juncker, in face of multiple
   constraints, introduced a new, more political leadership style, marked
   by boldly restoring the Commission's role as `motor of integration', yet
   adopting an open and conciliatory stance vis-a-vis the European Council
   and, more broadly, potential followers.},
