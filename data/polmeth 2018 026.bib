@incollection{ WOS:000441825100018,
Author = {Coelho, Nuno M. M. S.},
Editor = {HuppesCluysenaer, L and Coelho, NMMS},
Title = {Emotions: Impediment or Basis of Political Life?},
Booktitle = {ARISTOTLE ON EMOTIONS IN LAW AND POLITICS},
Series = {Law and Philosophy Library},
Year = {2018},
Volume = {121},
Pages = {361-384},
Abstract = {This chapter focuses on the emotional ground of politics in Aristotle's
   thought. The passages on ``war lovers{''} and on the difference between
   Greek adult males (citizens by nature) and foreigners, slaves or women
   demonstrate that living in a polis as an equal and free partner requires
   a proper emotional temper. Aristotle's criticism of Platonic communism
   and of economic inequality shows that according to him emotions are
   necessary for stability of polis, but are also a source of instability.
   The way Aristotle describes the crowd as the best judge in real world
   situations shows how emotions help to establish and maintain the best
   regime. All of this helps to understand how emotions were seen as
   important for instituting, preserving and developing the polis-and how
   at the same time they could be their main obstacles.},
