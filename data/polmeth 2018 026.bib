@article{ WOS:000428460400005,
Author = {Rimkute, Dovile},
Title = {Organizational reputation and risk regulation: The effect of
   reputational threats on agency scientific outputs},
Journal = {PUBLIC ADMINISTRATION},
Year = {2018},
Volume = {96},
Number = {1},
Pages = {70-83},
Month = {MAR},
Abstract = {This article aims to explain the variation in the scientific risk
   assessments conducted by two regulatory agencies: the European Food
   Safety Authority (EFSA) and the French Agency for Food, Environmental
   and Occupational Health and Safety (ANSES). To explain the merits of
   scientific risk assessments that have caused polarization within the EU,
   this article draws on bureaucratic reputation theory. The theory argues
   that regulators are political organizations that are active in
   protecting their unique organizational reputations. The findings
   obtained from interviews, direct observations, and primary documents
   yield support for this framework: depending on reputational threats,
   agencies choose to emphasize either their role as guardians of the
   prevailing social values, or send strong professional signals by
   delivering a scientifically rigorous risk assessment.},
