@article{ WOS:000458746800002,
Author = {Gratadour, Audrey},
Title = {The limits of the law in understanding the Syrian migratory influx},
Journal = {ETUDES INTERNATIONALES},
Year = {2018},
Volume = {49},
Number = {2},
Pages = {262-290},
Month = {SPR},
Abstract = {Some decision makers are wary of the arrival of new ``migrants.{''}
   Journalists and politicians certainly have a share of responsibility for
   this situation, often fomenting fear through semantics. On the ground,
   this mindset places displaced persons at risk. Could the law help
   provide a clearer understanding of the situation, and contribute
   concretely to protecting the displaced? Given that the law is intended
   to be a voice of reason, it would seem so. But it is also legitimate to
   ask whether the confusion that exists around population movements is not
   directly related to a failure of law. Examining interactions between the
   law and its environment from the perspective of the interdisciplinary
   theory of complex dynamic systems may provide some answers.},
