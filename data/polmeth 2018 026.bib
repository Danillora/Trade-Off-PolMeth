@article{ WOS:000430040400008,
Author = {Hall, Edward},
Title = {Integrity in democratic politics},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
Year = {2018},
Volume = {20},
Number = {2},
Pages = {395-408},
Month = {MAY},
Abstract = {The complaint that many professional politicians lack integrity is
   common. However, it is unclear what such a judgement amounts to. Taking
   various codes of political ethics in the United Kingdom as my starting
   point, I examine the extent to which we can understand political
   integrity as a matter of politicians adhering to the obligations that
   official codes of ethics prescribe and, in a more general sense, the
   public-service ethos that underpins these codes. I argue that although
   this way of approaching the issue usefully draws our attention to an
   important class of positional duties that apply to politicians,
   commitment to principled political causes plays a further, indispensable
   role in coherent assessments of political integrity. In consequence, I
   claim that politicians of integrity succeed in furthering their deepest
   political commitments while avoiding malfeasance or misconduct. As such,
   the ascription of political integrity can often only be made when
   assessing a long train of action.},
