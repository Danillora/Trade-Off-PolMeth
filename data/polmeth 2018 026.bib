@article{ WOS:000435217800011,
Author = {Mathur, Navdeep},
Title = {The low politics of higher education: saffron branded neoliberalism and
   the assault on Indian universities},
Journal = {CRITICAL POLICY STUDIES},
Year = {2018},
Volume = {12},
Number = {1},
Pages = {121+},
Abstract = {Through an examination of recent events and controversies Indian
   universities, this article reflects on the neoliberal creep taking over
   academia. The narrative connects the suicide note of a Dalit caste
   doctoral student, a student festival of political dissent, missives from
   the education minister, the financialization of higher education, and a
   market-oriented performance management system to discipline the
   professoriate. The latter element in the narrative is illustrated
   through my own teaching and research practice whose intellectual
   foundations draw on Professor Frank Fischer's scholarship. This personal
   reflection draws on my experiences in seeking to inhabit the role of a
   facilitator of participatory learning, engaging directly with policy
   actors and their cultural modes of communication.},
