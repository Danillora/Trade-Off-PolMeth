@article{ WOS:000436443800006,
Author = {Berdal, Mats},
Title = {The State of UN Peacekeeping: Lessons from Congo},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2018},
Volume = {41},
Number = {5},
Pages = {721-750},
Abstract = {The article considers the state of UN peacekeeping through the prism of
   its long-running operation in the Democratic Republic of the Congo
   (DRC). Focusing in particular on the challenges raised by use of force
   and the protection of civilians in conditions of ongoing armed conflict,
   it argues that UN field operations must be aligned much more closely
   than they have been over the past 15 years to political and diplomatic
   efforts aimed at securing viable political settlements to internal
   conflict. The issues raised by the history of the UN's troubled mission
   in the DRC are deeply relevant to the wider discussion of the
   organisation's role in the field of peace and security.},
