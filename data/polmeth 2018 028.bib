@article{ WOS:000427725300008,
Author = {Epifanio, Mariaelisa and Pluemper, Thomas},
Title = {European integration and the race to the top in counterterrorist
   regulations},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2018},
Volume = {25},
Number = {5},
Pages = {787-799},
Abstract = {International terrorism can generate a race to the top' in
   counterterrorist regulations among countries potentially exposed to
   terrorist attacks. This regulatory competition' can be smoothed by
   international co-operation and an agreement on common minimum standards
   in counterterrorist regulations. We argue, and show empirically, that
   European Union members states have implemented a significantly lower
   number of counterterrorist regulations than countries that do not belong
   to the European Union. This finding supports the argument that the
   implementation of a minimum regulatory standard reduces the competitive
   upward pressure in counterterrorist regulations.},
