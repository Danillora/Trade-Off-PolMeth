@article{ WOS:000444483500003,
Author = {Meriluoto, Taina},
Title = {Turning experience into expertise: technologies of the self in Finnish
   participatory social policy},
Journal = {CRITICAL POLICY STUDIES},
Year = {2018},
Volume = {12},
Number = {3},
Pages = {294-313},
Abstract = {This article investigates the micro-level practices of
   subjectconstruction in Finnish participatory social policy. Through a
   governmental ethnography on projects that invite former beneficiaries to
   become `experts-by-experience' in social welfare organizations, I
   discern the possibilities for freedom in the participants'
   self-construction. By making use of Michel Foucault's conceptual tools
   of care of the self and confession, I illustrate how, contrary to the
   projects' emancipatory promise of providing the service users the
   freedom to reconstruct themselves, the projects entail practices that
   curb the participants' way of `knowing themselves'. They require the
   service users to reframe their raw experiences as neutral and objective
   knowledge, making alternative ways of knowing appear `irrational', and
   hence easily discountable. I conclude that despite the user involvement
   initiatives' promise of incorporating different forms of knowledge, the
   participants are in practice required to realign their way of knowing
   with the dominant knowledge paradigm in order to be accepted as
   participants.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
