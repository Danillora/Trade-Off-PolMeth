@article{ WOS:000440659900005,
Author = {Gonzalez, Camilo Ignacio and Verhoest, Koen},
Title = {The Formal Delegation of Regulatory Decisions in the Telecommunication
   Sector: An Explanation Using Classification Trees},
Journal = {REVIEW OF POLICY RESEARCH},
Year = {2018},
Volume = {35},
Number = {4},
Pages = {617-641},
Month = {JUL},
Abstract = {Research on regulation has traditionally focused on studying the
   delegation of regulatory competencies from political principals to an
   independent regulatory agency. In this article, we argue that this
   delegation is nuanced by different factors that affect whether a
   specific regulatory decision is formally delegated. We examine and
   explain formal delegation patterns at the level of individual regulatory
   decisions in twelve countries located in Europe, Latin America, and
   South Asia. The data were gathered by coding the twelve countries'
   telecommunications legislation. The data analysis was undertaken using a
   classification tree modela nonparametric model. We found that the
   maturity of the market has the greatest effect on the formal delegation
   of regulatory decisions, but this effect is also influenced by the other
   theoretical factors considered, particularly the level of political
   constraints and the type of regulation.
   Resumen
   La Delegacion Formal De Decisiones Regulatorias En El Sector De Las
   Telecomunicaciones: una Explicacion Utilizando Los arboles De
   ClasificacionLa investigacion acerca de la regulacion se ha enfocado
   tradicionalmente en estudiar la delegacion de competencias regulatorias
   de los principales politicos a una agencia regulatoria independiente. En
   este documento, argumentamos que esta delegacion varia levemente de
   acuerdo con diferentes factores que deciden si una decision regulatoria
   especifica esta formalmente delegada. Examinamos y explicamos los
   patrones formales de delegacion a nivel de las decisiones regulatorias
   individuales en doce paises localizados en Europa, Latinoamerica y el
   sur de Asia. Los datos fueron recolectados al codificar la legislacion
   de telecomunicaciones de los doce paises. El analisis de datos fue hecho
   con un modelo de clasificacion tipo arbolun modelo no parametrico.
   Encontramos que la madurez del mercado tiene el mayor efecto sobre las
   decisiones regulatorias, pero este efecto tambien esta influenciado por
   otros factores teoricos que se toman en cuenta, particularmente el nivel
   de limites politicos y el tipo de regulacion.},
