@article{ WOS:000434363800009,
Author = {Borry, Erin L. and DeHart-Davis, Leisha and Kaufmann, Wesley and
   Merritt, Cullen C. and Mohr, Zachary and Tummers, Lars},
Title = {Formalization and consistency heighten organizational rule following:
   Experimental and survey evidence},
