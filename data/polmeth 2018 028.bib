@article{ WOS:000441238300006,
Author = {van Leeuwen, Mathijs and van de Kerkhof, Marlie and van Leynseele, Yves},
Title = {TRANSFORMING LAND GOVERNANCE AND STRENGTHENING THE STATE IN SOUTH SUDAN},
Journal = {AFRICAN AFFAIRS},
