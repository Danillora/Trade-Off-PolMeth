@article{ WOS:000425030900016,
Author = {Chazarreta, Adriana},
Title = {Applying Stavenhagen's Analytical Approach to Research on Argentine
   Agrarian Social Structure},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2018},
Volume = {45},
Number = {2},
Pages = {166-179},
Month = {MAR},
Abstract = {Rodolfo Stavenhagen's analytical approach, as laid out in his 1965 Seven
   Erroneous Theses about Latin America, prioritized a dialectical
   perspective and relationships of functionality between two dynamics
   within a single structure and crystallized in fundamental conceptual
   proposals such as internal colonialism and a focus on the analysis of
   agrarian social structure. Application of the approach to current
   research problems in Argentina sheds light on the changes in agrarian
   social structure that have taken place in recent decades, in particular
   changes in dominant forms of land tenure, the relations of production,
   and the functional relationships between areas at different levels of
   development.},
