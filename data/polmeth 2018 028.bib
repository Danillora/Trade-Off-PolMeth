@incollection{ WOS:000472929000006,
Author = {Boettner, Robert},
Editor = {Ege, J and Bauer, MW and Becker, S},
Title = {Project Teams in the European Commission - A fair balance between
   efficiency and politics?},
Booktitle = {EUROPEAN COMMISSION IN TURBULENT TIMES: ASSESSING ORGANIZATIONAL CHANGE
   AND POLICY IMPACT},
Series = {Schriftenreihe des Arbeitskreises Europaische Integration e.V.},
Year = {2018},
Volume = {105},
Pages = {113-131},
Abstract = {The incumbent President of the European Commission, Jean-Claude Juncker,
   has made considerable changes to the internal organisation of the
   Commission by introducing so called ``project teams{''}. These clusters,
   each responsible for one of the great priority topics, gather
   Commissioners with related portfolios under the leadership of a
   Vice-President, which is invested with the right to veto initiatives of
   line Commissioners. The Commissioners that are assigned to a certain
   project team are expected to work in close cooperation with the
   respective Vice-President and report to him. The clusters therefore
   create a sort of sector-based hierarchy. While clustering the Commission
   is a legitimate approach and not per se illegal under the existing
   Treaties, this book chapter finds that the introduction of the project
   teams in their current design is not in conformity with primary law
   requirements as it violates the principle of collegiality.},
