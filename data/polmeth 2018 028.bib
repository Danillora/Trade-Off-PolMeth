@article{ WOS:000440675700008,
Author = {Manky, Omar},
Title = {Part-Time Miners: Labor Segmentation and Collective Action in the
   Peruvian Mining Industry},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2018},
Volume = {45},
Number = {5},
Pages = {120-135},
Month = {SEP},
Abstract = {Over the past two decades, besides the transformation of the
   organizational structure of the mining industry, there has been a shift
   in the spatial organization of production, expressed by the replacement
   of mining towns with mining hotels. Two case studies in Peru show that
   these changes have produced new forms of labor segmentation affecting
   workers' capacity to mobilize by turning those who are directly employed
   by the company into a minority. While subcontracted miners have managed
   to overcome organizational segmentation by organizing across companies,
   the spatial segmentation of the mining hotel model has made local
   coordination difficult. Workers are not, however, necessarily defeated,
   as greater spatial mobility may offer possibilities of networking with
   new actors.},
