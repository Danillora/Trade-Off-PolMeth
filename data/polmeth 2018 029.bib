@article{ WOS:000452438400001,
Author = {Hudson, Victoria},
Title = {The Ukrainian Orthodox Church of the Moscow Patriarchate as a Potential
   `Tool' of Russian Soft Power in the Wake of Ukraine's 2013 Euromaidan},
Journal = {EUROPE-ASIA STUDIES},
Year = {2018},
Volume = {70},
Number = {9},
Pages = {1355-1380},
Abstract = {This article considers the religious and spiritual aspect of the
   Euromaidan protests; specifically, the role of the Ukrainian Orthodox
   Church of the Moscow Patriarchate and to what extent the UOC(MP) can
   justifiably be seen as a tool of Russian `soft power' cultural and
   ideational influence. It examines the degree of institutional and
   ideological leverage that Moscow is able to exercise over the Ukrainian
   church, and then explores the position of the church during the
   protests, and how this has affected the authority and legitimacy of its
   standing as an opinion leader in Ukrainian society in the wake of the
   `Revolution of Dignity'.},
