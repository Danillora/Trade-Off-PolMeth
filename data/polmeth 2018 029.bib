@article{ WOS:000432213800001,
Author = {Maiolino, Elise},
Title = {``I'm not male, not white, want to start there?{''}: Olivia Chow and
   Identity Work in Toronto's 2014 Mayoral Election},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2018},
Volume = {39},
Number = {2},
Pages = {220-245},
Abstract = {This article uses sociological theories of identity work to extend the
   research on political leadership. Focusing on Toronto mayoral candidate
   Olivia Chow-a high-profile progressive politician, an instant
   frontrunner, and a stark contrast to Mayor Rob Ford's populist
   conservative agenda-this article argues that Chow was required to
   negotiate and mobilize identity in ways that were different from her
   white male opponents. Based on an intersectional analysis of participant
   observation in twenty mayoral debates, this article offers three
   concepts that illuminate forms of identity work on the campaign trail:
   dispositional requirements, ideological alignment, and political
   compensatory labour. This article illustrates that the racist and sexist
   terrain of politics requires a complex set of decisions and actions on
   the part of marginalized candidates.},
