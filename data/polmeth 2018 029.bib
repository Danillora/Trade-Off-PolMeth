@article{ WOS:000446508100001,
Author = {Roberts, Andrew},
Title = {Millionaires and the public in Czech politics},
Journal = {POST-SOVIET AFFAIRS},
Year = {2018},
Volume = {34},
Number = {6},
Pages = {353-366},
Abstract = {This article relies on a unique survey of Czech millionaires and the
   general public to probe the nature and extent of the differences in
   opinions between these two groups and their correspondence with public
   policy. Its main finding is that millionaires are substantially more
   right-wing than the public on economic issues and somewhat more
