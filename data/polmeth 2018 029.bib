@article{ WOS:000788163100003,
Author = {Mos, Martijn},
Title = {The fight of the Religious Right in Europe: old whines in new bottles},
Journal = {EUROPEAN JOURNAL OF POLITICS AND GENDER},
Year = {2018},
Volume = {1},
Number = {3, SI},
Pages = {325-343},
Month = {NOV},
Abstract = {European Union values such as human dignity and non-discrimination on
   the basis of sexual orientation are enshrined in treaties and therefore
   not subject to discussion. However, because the European Union does not
   have the tools for defining or enforcing its values, the Religious Right
   can exploit this ambiguity through strategic framing and norm
   contestation. I show how this played out for European Citizens'
   Initiatives on sexual and reproductive health and rights (`One of Us')
   and marriage and the family (`Mum, Dad \& Kids'). The petitioners
   pursued traditionally conservative goals, yet relied on technical,
   rather than moral or religious, arguments.},
