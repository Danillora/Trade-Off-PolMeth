@article{ WOS:000432999100003,
Author = {Durazzi, Niccolo and Fleckenstein, Timo and Lee, Soohyun Christine},
Title = {Social Solidarity for All? Trade Union Strategies, Labor Market
   Dualization, and the Welfare State in Italy and South Korea},
Journal = {POLITICS \& SOCIETY},
Year = {2018},
Volume = {46},
Number = {2},
Pages = {205-233},
Month = {JUN},
Abstract = {Challenging the new political-economic mainstream that considers trade
   unions to be complicit in labor market dualization, this article's
   analysis of union strategies in Italy and South Korea, most-different
   union movements perceived as unlikely cases for the pursuit of broader
   social solidarity, shows that in both countries unions have successively
   moved away from insider-focused strategies and toward solidarity for all
   in the industrial relations arena as well as in their social policy
   preferences. Furthermore, unions explored new avenues of political
   agency, often in alliance with civil society organizations. This
   convergent trend toward a social model of unionism is ascribed to a
   response of unions to a double crisis: that is, a socioeconomic crisis,
   which takes the form of a growing periphery of the labor market
   associated with growing social exclusion, and a sociopolitical crisis,
   which takes the form of an increasing marginalization of the unions from
   the political process.},
