Affiliations = {University of Tokyo},
Funding-Acknowledgement = {JSPS KAKENHI {[}JP15H03236]},
Funding-Text = {The research for this chapter was supported by JSPS KAKENHI Grant Number
   JP15H03236. An earlier version of the paper was presented in Japanese as
   `17-seiki-chuyo Yoroppa seiryoku no Nihon kenshi to kokusho'
   {[}Embassies and state letters sent from the European powers to Japan in
   the middle of the seventeenth century], in Nichiran kankeishi wo
