@article{ WOS:000437713900002,
Author = {Nitoiu, Cristian},
Title = {The European Union's ``Ideal Self' in the Post-Soviet Space},
Journal = {EUROPE-ASIA STUDIES},
Year = {2018},
Volume = {70},
Number = {5, SI},
Pages = {692-710},
Abstract = {During the last two decades, one of the cornerstones of the foreign
   policy of the European Union (EU) has been the development of a strong
   presence in its neighbourhood. The 2003 Security Strategy and the recent
   2016 Global Strategy highlighted the need to show that the EU can play a
   major role in the international arena by first establishing a strong
   presence in the neighbourhood and proving the union's effectiveness in
   the region. In this context, the aim of the essay is to explore the way
   the EU's ideal self' is constructed and perceived in the post-Soviet
   space.},
