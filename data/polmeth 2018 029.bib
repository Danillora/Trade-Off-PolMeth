@article{ WOS:000431006400004,
Author = {Zelce, Vita},
Title = {The Transformation of ``Holiday' in Post-Soviet Space: Celebrating
   Soviet Victory Day in Latvia},
Journal = {EUROPE-ASIA STUDIES},
Year = {2018},
Volume = {70},
Number = {3},
Pages = {388-420},
Abstract = {This article discusses the celebration in Latvia of the victory of the
   Soviet Union in World War II. Since the restoration of Latvia's
   statehood, 9 May has not been an official holiday, but it has becomeas
   Victory Day'the most important history-linked celebration for the
   Russian-speaking community in Latvia. The post-1991 history of Victory
   Day' makes it possible to track changes in: policies toward history and
   memory in Russia and Latvia; how political groups have used these
   celebrations to further their own agendas; and the organisation of
   events on public holidays.},
