@article{ WOS:000430099400001,
Author = {Ozkaleli, Umut},
Title = {Intersectionality in Gender Mainstreaming:Equity Organizing in Turkey},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2018},
Volume = {39},
Number = {2},
Pages = {127-150},
Abstract = {Historically, Turkey is home to multiple identities and cultures. In
   following with the critiques of Diversity Mainstreaming approach toward
   Gender Mainstreaming, this article argues that recognition of identities
   can be possible if an intersectional approach is adopted. To overcome
   the particular challenges of Gender Mainstreaming's implementation in
   Turkey's multicultural society, this article introduces a concept that
   supplements Gender Mainstreaming, called Equity Organizing, which
   further develops the views on intersectionality and equity that
   Diversity Mainstreaming proposes. It is different from Diversity
   Mainstreaming because it seeks to address the challenges to democracy in
   strong central and authoritarian leaning states. Equity Organizing is
   committed to the construction of progressive state policies for social
   justice through the realization of diverse identities.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
