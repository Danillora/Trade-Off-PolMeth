@article{ WOS:000440725600005,
Author = {Doose, Katja},
Title = {The Armenian Earthquake of 1988: A Perfect Stage for the
   Nagorno-Karabakh Conflict},
Journal = {EUROPE-ASIA STUDIES},
Year = {2018},
Volume = {70},
Number = {6, SI},
Pages = {924-941},
Abstract = {Natural disasters can sometimes have a tremendous impact on societies
   and can even contribute to the outbreak of violent conflicts. The onset
   of the Nagorno-Karabakh conflict is usually attributed to the lack of
   Soviet control over the periphery and the consequent resurgence of
   ethnicity'. Based on an analysis of how the main political actors in
   Moscow and the Caucasus framed the 1988 earthquake in Armenia in
   opposition to each other, this essay shifts the focus from political
   history to environmental history to argue that the disaster, and the
   narratives revolving around its origin and meaning, can further explain
   the exacerbation of the conflict.},
