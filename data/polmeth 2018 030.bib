@article{ WOS:000457588700002,
Author = {Soni, Suparna},
Title = {Political Quotas, NGO Initiatives and Dalits' Human Rights in Rural
   India},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2018},
Volume = {10},
Number = {3},
Pages = {388-405},
Month = {NOV},
Abstract = {Government policies and non-government agencies often intend to create
   socio-economic security and empower marginalized communities. However,
   we know little about how these separate arrangements together impact the
   human rights situations of marginalized communities. This study
   investigates how official representation quotas and communal engagement
   of non-governmental organizations (NGOs) reinforce each other in
   promoting improved social treatment of a marginalized community, Dalits.
   The article suggests that while formal representation creates an
   environment for intergroup contact, NGOs' communal engagement further
   promotes such contact and empowers the respective communities to
   challenge prevailing stereotypes. The study argues that the Dalits'
   opinion on their social status depends not only on whether they are
   represented through official quotas, but also on community engagement
   through local NGOs. The research utilizes `natural experiment' settings
   in the Indian state of Rajasthan, where official quotas currently exist
   in some village councils. By conducting a public opinion survey among
   Dalits in villages with and without representation quotas, the research
   is able to evaluate the effects of state and community-level actions
   aimed at promoting the well-being of marginalized communities. The
   findings indicate that political quotas and NGOs' initiatives of
   economic liberation and civil society engagements interact and
   positively affect societal perceptions that translate to the behavioural
   and attitudinal treatment of the Dalit community. This study contributes
   to the body of literature on civil society and the learning of new
   behavioural and attitudinal norms.},
