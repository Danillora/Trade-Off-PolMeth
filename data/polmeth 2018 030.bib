@article{ WOS:000441725500006,
Author = {Vandamme, Pierre-Etienne and Jacquet, Vincent and Niessen, Christoph and
   Pitseys, John and Reuchamps, Min},
Title = {Intercameral Relations in a Bicameral Elected and Sortition Legislature},
