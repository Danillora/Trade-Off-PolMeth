@article{ WOS:000788162800016,
Author = {Atchison, Amy L.},
Title = {Towards the good profession: improving the status of women in political
   science},
Journal = {EUROPEAN JOURNAL OF POLITICS AND GENDER},
Year = {2018},
Volume = {1},
Number = {1-2},
Pages = {279-298},
Month = {JUL},
Abstract = {Although women have made considerable inroads into political science,
   they still comprise only about one third of the profession. Women
   political scientists are concentrated at lower ranks and less
   prestigious institutions, less likely to be published in the
   discipline's top journals, and cited less frequently than men. There are
   indicators that women's marginalisation is related to exclusion from
   predominantly male networks in the discipline; thus, I propose a
   research agenda to map the extent of women's marginalisation in
   political science. In turn, I hope that this body of knowledge will push
   political scientists to address the structural inequalities embedded in
   the field.},
