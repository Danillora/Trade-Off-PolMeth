@article{ WOS:000438355000005,
Author = {Yusuf, Hakeem O.},
Title = {Colonialism and the Dilemmas of Transitional Justice in Nigeria},
Journal = {INTERNATIONAL JOURNAL OF TRANSITIONAL JUSTICE},
Year = {2018},
Volume = {12},
Number = {2},
Pages = {257-276},
Month = {JUL},
Abstract = {Adoption of the colonial template of governance by successive
   postcolonial governments in Nigeria has limited the ambit of
   transitional justice, rendering it incapable of addressing the root
   causes of systematic abuses and conflict in the country. Pathologies of
   colonial injustice and violence were transmitted into governance in the
   postcolony and this structural continuity has locked down the prospect
   of justice and reforms as an integral part of the governance complex.
   Critical analysis of the Nigerian experience calls attention to the need
   for transitional justice theory and praxis to engage with the colonial
   legacy in nonsettler, postcolonial societies. Understanding and engaging
   with the colonial legacy is critical to the prospects of successful
   transitional justice in postcolonial polities.},
