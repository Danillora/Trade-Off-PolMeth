@article{ WOS:000434932300004,
Author = {Purnell, Kandida},
Title = {Grieving, Valuing, and Viewing Differently: The Global War on Terror's
   American Toll},
Journal = {INTERNATIONAL POLITICAL SOCIOLOGY},
