@article{ WOS:000434275900010,
Author = {Grossheim, Martin},
Title = {The `Sword and Shield of the Party': how the Vietnamese People's Public
   Security Forces portray themselves},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2018},
Volume = {33},
Number = {3},
Pages = {439-458},
Abstract = {This paper shows that since the beginning of the reform period in 1986
   the regime in Hanoi has taken great pains to create the image of the
   state having legitimate and, indeed, heroic security organs that acted
   as the `saviors of the Vietnamese revolution' and still serve as the
   `shield and sword' of the Vietnamese Communist Party. I argue that while
   previously the socialist state used to regard the history of its
   security organs as top secret, over the last few years, a huge amount of
   resources have been mobilized to actively propagate a sacred and
   romanticized image of the security apparatus.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
