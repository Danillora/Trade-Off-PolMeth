@article{ WOS:000457588700003,
Author = {Schiff, Jennifer},
Title = {Reconciling Decentralization: The Human Right to Water, India's Yamuna
   River, and the Challenge of Transboundary Governance},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2018},
Volume = {10},
Number = {3},
Pages = {406-422},
Month = {NOV},
Abstract = {Clean freshwater access is a concern that is global in scope. The
   international community has codified the human right to water within a
   number of legal agreements and regimes, but the right remains sparsely
   implemented within states. With this in mind, this article examines the
   case of the Yamuna River, one of India's most polluted waterways, and
   asks if the Indian government maintains the capacity to abide by the
   international water rights policy to which it has tacitly agreed. The
   Yamuna, often referred to as a `dead' river due to its high levels of
   pollution, provides a fitting framework for exploring whether there are
   institutionalized limits on development that prevent stakeholders from
   complying with their human rights commitments. The findings of this
   analysis suggest that, in this case, the Indian government struggles to
   reconcile the competing interests presented by the need for a
   centralized human rights compliance mechanism and the Indian state's own
   constitutional commitment to decentralized grassroots-driven
   environmental policy. Ultimately, this case informs the literatures on
   human rights compliance and environmental governance by suggesting that,
   in certain contexts, the development of a centralized coordinating
   policy body may serve as a necessary condition to enhance the synergy
   between rights compliance and political decentralization.},
