@article{ WOS:000457588700009,
Author = {Salmonsson, Lisa and Hedlund, Daniel},
Title = {Where Do We Go From Here? Challenges and Strategies Following Additional
   Asylum Policy Restrictions in Sweden},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2018},
Volume = {10},
Number = {3},
Pages = {524-535},
Month = {NOV},
Abstract = {This policy note highlights issues concerning the right to seek asylum
   raised by a panel of migration experts in Sweden during the annual
   Swedish Forum for Human Rights in 2016. The discussion took place
   following Swedish asylum and immigration policy taking an increasingly
   restrictive turn. Two main themes are identified. The first theme
   encompasses views on the relationship between the legal framework's
   increased restrictiveness and discourses of securitization and explores
   the possibilities of moving beyond this pattern. The second theme
   highlights that different professionals and other staff engaged with
   asylum seekers and refugees can retain a (legal) space to resist
   repressive activities that challenge their professional code of conduct
   and personal integrity.},
