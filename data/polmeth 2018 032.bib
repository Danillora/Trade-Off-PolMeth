Language = {English},
Affiliation = {Weiss, L (Corresponding Author), Univ Sydney, Govt \& Int Relat, Camperdown \& Darlington Campus, Sydney, NSW 2006, Australia.
   Weiss, Linda, Univ Sydney, Govt \& Int Relat, Camperdown \& Darlington Campus, Sydney, NSW 2006, Australia.
   Thurbon, Elizabeth, UNSW, Sch Social Sci, Sydney, NSW, Australia.},
DOI = {10.1080/09692290.2018.1486875},
ISSN = {0969-2290},
EISSN = {1466-4526},
Keywords = {United States; infrastructural power; transformative capacity;
