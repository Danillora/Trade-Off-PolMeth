@article{ WOS:000454643300007,
Author = {Sakwa, Richard},
Title = {One Europe or None? Monism, Involution and Relations with Russia},
Journal = {EUROPE-ASIA STUDIES},
Year = {2018},
Volume = {70},
Number = {10, SI},
Pages = {1656-1667},
Month = {NOV 26},
Abstract = {The crisis in relations between Russia and the European Union (EU) is
   part of the broader breakdown of the post-Cold War security order. This
   commentary focuses on structural interpretation and identifies four
   interlinked processes shaping the crisis: tension between the logic of
   the enlargement and transformation; a dynamic of involution and
   resistance; the problem of monism, whereby the expanding self is unable
   adequately to engage with the un-integrated other; and the recent
   emergence of `other Europes' that may potentially overcome involution.
   The erosion of the Atlantic system provides an opportunity for delayed
   institutional and ideational innovation.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
