@article{ WOS:000430667300014,
Author = {Leon, Sandra},
Title = {Muddling up Political Systems? When Regionalization Blurs Democracy:
   Decentralization and Attribution of Responsibility},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
