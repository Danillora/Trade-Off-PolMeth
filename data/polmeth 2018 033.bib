@article{ WOS:000448768100004,
Author = {Cayford, Michelle and Pieters, Wolter and Hijzen, Constant},
Title = {Plots, murders, and money: oversight bodies evaluating the effectiveness
   of surveillance technology},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2018},
Volume = {33},
Number = {7},
Pages = {999-1021},
Abstract = {Intelligence agencies routinely use surveillance technology to perform
   surveillance on digital data. This practice raises many questions that
   feed a societal debate, including whether the surveillance technology is
   effective in achieving the given security goal, whether it is
   cost-efficient, and whether it is proportionate. Oversight bodies are
   important actors in this debate, overseeing budgets, legal and privacy
   matters, and the performance of intelligence agencies. This paper
   examines how oversight bodies evaluate the questions above, using
   documents produced by American and British oversight mechanisms.},
