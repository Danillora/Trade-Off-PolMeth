@article{ WOS:000454643300009,
Author = {Musialkowska, Ida and Dabrowski, Marcin},
Title = {EU-China Regional Policy Dialogue: Unpacking the Mechanisms of an
   Unlikely Policy Transfer},
Journal = {EUROPE-ASIA STUDIES},
Year = {2018},
Volume = {70},
Number = {10, SI},
Pages = {1689-1711},
Month = {NOV 26},
Abstract = {This article investigates EU-China dialogue on regional policy, a
   puzzling exercise in policy transfer because such policy is by its
   nature inward-oriented and the intricacies of regional development imply
   uncertainty about its effects in different contexts. The article sheds
   light on the reasons of both sides for engaging in this unlikely policy
   learning effort and identifies its actors and mechanisms. It also
   critically assesses this process, stressing its one-way nature and the
   active role of the EU as a `policy-sender', in contrast to most policy
   transfer literature citing demands by the policy-recipient as the
   predominant reason to engage in such cooperation.},
