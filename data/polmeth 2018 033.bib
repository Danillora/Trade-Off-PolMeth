@article{ WOS:000436071900004,
Author = {Chupein, Thomas and Glennerster, Rachel},
Title = {Evidence-Informed Policy from an International Perspective},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2018},
Volume = {678},
Number = {1},
Pages = {62-70},
Month = {JUL},
Abstract = {This article discusses the history of randomized controlled trials
   (RCTs) in development economics and the ways in which their use has
   shaped disciplinary practices and development policy over the past two
   decades. We first explain how increased use of academic-run RCTs
   internationally has led to important methodological breakthroughs that
   have advanced our knowledge of human behavior as well as changes in how
   research is conducted, including routine practice of in-field
   apprenticeships for young researchers and the establishment of robust
   research infrastructure in numerous developing countries around the
   world. We then explore three ways in which the scale-up of
   evidence-informed programs and policies based on findings from RCTs have
   achieved real-world impact, giving examples for each. These include how
   rigorous evidence can help to resolve contentious policy debates; enable
   policy-makers to assess external validity of findings and draw lessons
   across contexts; and support institutionalization of evidence use in
   various types of organizations.},
