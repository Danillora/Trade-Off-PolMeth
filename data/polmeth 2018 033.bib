@incollection{ WOS:000515053400009,
Author = {Good, Peter},
Editor = {Clulow, A and Mostert, T},
Title = {The East India Company and the foundation of Persian Naval Power in the
   Gulf under Nader Shah, 1734-47},
Booktitle = {DUTCH AND ENGLISH EAST INDIA COMPANIES: DIPLOMACY, TRADE AND VIOLENCE IN
   EARLY MODERN ASIA},
Series = {Asian History},
Year = {2018},
Pages = {211-235},
Abstract = {This chapter explores the involvement of the East India Company (EIC) in
   the creation of a fleet in the Gulf by Nader Shah of Persia between 1734
   and 1747. It considers the rationale behind the EIC's assistance to the
   Persians and what shape it took, considering the pre-eminent position
   and threat attached by contemporaries to European ships. The chapter
   draws on the EIC's Persian Gulf Factory Records which shed new light on
   the ongoing negotiations between the EIC and Persian officials in
   country. The EIC were active in supporting the construction of a Persian
   fleet, turning a profit by supplying, not only ships, but also supplies
   and stores necessary for their maintenance. The provisions provided were
   a useful means through which the EIC gained favour with the Persian
   Court in a period of ongoing conflict and uncertainty.},
