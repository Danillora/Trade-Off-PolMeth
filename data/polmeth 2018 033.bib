@article{ WOS:000429953100003,
Author = {Maher, Stephanie},
Title = {Out of West Africa: Human Smuggling as a Social Enterprise},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2018},
Volume = {676},
Number = {1},
Pages = {36-56},
Month = {MAR},
Abstract = {Drawing on 18 months of ethnographic research in Senegal, this article
   focuses on the sociality of migrant facilitation. Although it has become
   relatively common in media and policy reports to suggest that irregular
   migrants are manipulated by greedy and unscrupulous human smugglers,
   this article shows how migrants in Senegal are often familiar with their
   handlers and are more likely to call them a friend (ami) than a
   criminal. Also, most migrants do not see themselves as smuggled, which
   implies victimhood. Rather, they see themselves as making calculated
   choices to migrate based on a host of social factors. By exploring the
   relationships between handlers and migrants, this article reveals the
   social worlds of negotiation, assistance, and protection that feature
   prominently in West African migrant narratives and practices.},
