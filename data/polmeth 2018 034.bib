@article{ WOS:000437780700005,
Author = {Chandler, Andrea},
Title = {what kind of friendship is this? Russia's ``Crimean Syndrome{''}},
Journal = {EUROPEAN SECURITY},
Year = {2018},
Volume = {27},
Number = {2},
Pages = {201-223},
Abstract = {Russia's 2014 annexation of Crimea without the consent of Ukraine was a
   rare case of a state taking territory from a state with whom it
   previously enjoyed friendly relations. The paper seeks to explain the
   causes and consequences of this annexation by examining theories of
   democratic peace, constructivism and irredentism. In 1971, political
   scientist Myron Weiner published an article ``The Macedonian
   Syndrome{''} in the journal World Politics (vol. 23, no. 4, 665-683). In
   particular, the paper examines the applicability of Weiner's theses to
   Russia's 2014 attempt to annex Crimea from Ukraine. While Weiner's
   theory helps to explain Russia's moves, his theory can be updated to
   consider the consequences of those moves. Russia attempted to justify
   its annexation by transposing the concept of friendship from Ukraine
   itself to a piece of its territory - Crimea. This transposition rested
   on a false dichotomy between Ukraine and Crimea, and Russia's failure to
   live up to its international commitments to respect Ukrainian borders
   brought swift consequences in the form of Western sanctions.},
