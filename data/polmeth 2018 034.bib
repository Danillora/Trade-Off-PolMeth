@article{ WOS:000432002400013,
Author = {Daniel, William T. and Thierse, Stefan},
Title = {Individual Determinants for the Selection of Group Coordinators in the
   European Parliament},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2018},
Volume = {56},
Number = {4},
Pages = {939-954},
Month = {MAY},
Abstract = {Group coordinators are part of a highly influential circle of members of
   the European Parliament (MEPs). However, there is only scant knowledge
   about the powers and purpose of this position. This article draws upon
   theories of legislative organization and committee assignments to assess
   the question: What individual qualities determine the selection of group
   coordinators? Using a novel dataset of all MEPs elected to the 8(th)
   session of the European Parliament (EP), we consider various aspects of
   MEP professional background, experience in the chamber and interest
   group ties as potential predictors of this selection. Our findings
   indicate the particularly strong effect of incumbency in the committee
   system, as well as professional background for selection. Ultimately,
   the results bear relevance to the legislative organization of the EP as
   a legislature with both strong committees and strong parties.},
