@article{ WOS:000430099000005,
Author = {Norris, Mikel and Tankersley, Holley},
Title = {Women Rule: Gendered Leadership and State Supreme Court Chief Justice
   Selection},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2018},
Volume = {39},
Number = {1},
Pages = {104-125},
Abstract = {While women are underrepresented in many political institutions and
   leadership positions, nearly half of state supreme court chief justices
   are women. Is there something about the role of state supreme court
   justice that facilitates the recruitment of women to this important
   political position? We examine whether the selection of a woman chief is
   driven by the court's institutional need for women's leadership style or
   simply the supply of qualified justices. We find that ideological
   diversity drives demand for a woman chief. A supply of experienced women
   justices also has a significant impact on likelihood of selection.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
