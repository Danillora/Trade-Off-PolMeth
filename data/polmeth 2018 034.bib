@incollection{ WOS:000668697200003,
Author = {Oviedo, Gilberto L.},
Editor = {Delgado, JB},
Title = {Graphic Humor and the Formation of Individuality in 19th-century
   Colombia},
Booktitle = {HUMOR AND POLITICS: A TRANSCULTURAL PERSPECTIVE},
Series = {Humor-Aproximaciones Transdisciplinares},
Year = {2018},
Pages = {41-71},
Abstract = {This chapter will offer a historical profile of the forms of humorous
   expressions developed by the Colombian graphic media in the 19th
   century. The newly created republic found, in the language of satirical,
   jocular images, a suitable means to raise individuals' awareness of
   their responsibility within the social world. The graphic humor of the
   period criticized the prevalence of the colonial mindset in the
   republican life and opted for a questioning attitude towards social and
   political realities. The pictorial movement emphasized the importance of
   the formation of individuality in a population accustomed to being
   driven as a flock under the protection of the Church. The invitation of
   humorous graphic art to take a stand against the nation's destiny was
   extended through different graphic expressions such as realism,
   costumbrismo and caricature, all of which are interested in showing the
   contradiction of a sovereign, libertarian republic that preserved
   authoritarian, dogmatic traits of Spanish colonialism.},
