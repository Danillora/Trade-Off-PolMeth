@article{ WOS:000457588700007,
Author = {Eriksson, Sanna},
Title = {Temporary Relocation in an Academic Setting for Human Rights Defenders
   at Risk: Good Practice Lessons and Challenges},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2018},
Volume = {10},
Number = {3},
Pages = {482-507},
Month = {NOV},
Abstract = {With increasing threats against human rights defenders (HRDs) worldwide
   and shrinking civic space impacting on their ability to act, HRD
   protection through temporary relocation is needed more than ever. At the
   same time, temporary relocation schemes are starting to feel a shrinking
   space impacting on their ability to relocate HRDs. This practice note
   discusses current trends in temporary relocation through the example of
   the Protective Fellowship Scheme for HRDs at Risk based at the Centre
   for Applied Human Rights (CAHR), at the University of York in the UK.
   CAHR has ten years of experience hosting HRDs at risk on its Protective
   Fellowship Scheme, which is unique as a structured relocation programme
   based at a university. CAHR hosts HRDs in an educational setting that
   aims at providing a holistic support programme encompassing research and
   training opportunities alongside advocacy and networking activities, as
   well as rest and protection. This paper analyses both good practice and
   everyday challenges encountered in relocation, suggesting good practice
   lessons to reflect on.},
