@incollection{ WOS:000459673400001,
Author = {Barrault-Stella, Lorenzo and Weill, Pierre-Edouard},
Editor = {BarraultStella, L and Weill, PE},
Title = {The Making of Target Publics for Welfare Policies. From Targeting
   Practices to Resistances of Governed People},
Booktitle = {CREATING TARGET PUBLICS FOR WELFARE POLICIES: A COMPARATIVE AND
   MULTI-LEVEL APPROACH},
Series = {Logic Argumentation \& Reasoning},
Year = {2018},
Volume = {17},
Pages = {1-24},
Abstract = {This opening chapter provides a general sociological framework for the
   contributions collected about the targeting practices in the
   institutional framework of the welfare State and the making of the
   ``right{''} publics for social policies. The paper builds on the
   diversity of existing international research, before sketching out
   complementary lines of investigation in which this collective work is
   grounded. More specifically, we suggest the use of a comparative and
   multilevel approach, anchored in empirical research and mindful of the
   effective practices of targeting, as well as of the way in which the
   diverse groups within the potential publics react. Connecting in-depth
   case studies thus allows the observation of transformations occurring in
   modes of government at the international level. Finally, the text
   suggests a few ways in which the welfare State's targeting practices can
   be reformulated, without obscuring the social relations and the various
   forms of inequalities (of class, race, gender) to which targeting
   participates.},
