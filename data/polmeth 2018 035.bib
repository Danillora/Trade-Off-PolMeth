@incollection{ WOS:000668697200007,
Author = {Torres, Katjia},
Editor = {Delgado, JB},
Title = {The First Three Years of the Moroccan Political Transition (1999-2000),
   According to the Satire of Graphic Humor in the Weekly Publication
   Demain Magazine (20/01/2001-21/05/2003)},
Booktitle = {HUMOR AND POLITICS: A TRANSCULTURAL PERSPECTIVE},
Series = {Humor-Aproximaciones Transdisciplinares},
Year = {2018},
Pages = {157-190},
Abstract = {From the psychological perspectives of Freud's release/relief and
   Gruner's aggressiveness/superiority, we will analyze five critical
   cartoons, published in the Moroccan weekly publication Demain Magazine,
   addressing the three key elements of humor: structure, recipients and
   cartoonists. We will address the satire of graphic humor published in
   this weekly paper as a means to denounce the lack of freedom of the
   press in Morocco. To understand the critical component of these
   cartoons, we will succinctly present the specific historical context in
   which the magazine was born and developed, in its capacity as
   encyclopedic knowledge essential to resolving the three levels of
   understanding required. We will conclude by stating that satire is an
   effective vehicle to transmit important information that, from a
   traditional journalistic approach, does not break institutional
   censorship. One of the limitations of this study, approachable in future
   research, is the sociolinguistic analysis of critical cartoons, as a
   specific speech act of French as a lingua franca in Morocco, resulting
   from contact with native languages derived from Arabic. This
   circumstance offers a linguistic richness that is originally
   substantiated by the conversational implicatures, inferences and
   deductive processes captured in their cartoons.},
