@incollection{ WOS:000668697200008,
Author = {Carratala, Adolfo},
Editor = {Delgado, JB},
Title = {Cartoons About the Pope in the Spanish Press (1982-2011): Humor as a
   Driving Force or Reflection of a Cultural Change?},
Booktitle = {HUMOR AND POLITICS: A TRANSCULTURAL PERSPECTIVE},
Series = {Humor-Aproximaciones Transdisciplinares},
Year = {2018},
Pages = {191-221},
Abstract = {The concept of power and institutions have been a constant theme in
   satirical and humorous journalism, which has understood, as its own
   function, to attack these focal points of authority, thus fulfilling the
   role that humor must play in a free society: to be a weapon to embarrass
   those intending to control a hypocritical, ridiculous version of both
   the world and themselves. The Catholic Church and its top
   representatives have been revealed as a favorite target of these
   messages, especially in the Spanish journalistic tradition, which for a
   long time has been extremely fertile in anti-clerical satirical
   publications. This work aims to assess the state of such humor
   production through an approach to the period that goes from the end of
   Franco's dictatorship, which prohibited any critical expression against
   the ecclesiastical institution for almost four decades, until 2015, well
   into the 21st century, in which progressive secularization of Spanish
   society has already been consolidated. The objective is to determine
   whether the graphic humor published about the person acting as the
   visible leader of the Church, the Pope, has been a testimony or catalyst
   of this cultural change. For this, the study will analyze in detail the
   cartoons published by two important-and ideologically distant-newspapers
   (El Pais and ABC) regarding the eight trips made by the pontiff to Spain
   since the recovery of democracy. The results show a timid recovery of
   humorous aggression towards the Catholic Church.},
