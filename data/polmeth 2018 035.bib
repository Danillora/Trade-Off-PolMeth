@incollection{ WOS:000668697200009,
Author = {Lazaro Pulido, Manuel},
Editor = {Delgado, JB},
Title = {Humor and Laughter in Philosophical-Theological Thinking from the Middle
   Ages to Modernity},
Booktitle = {HUMOR AND POLITICS: A TRANSCULTURAL PERSPECTIVE},
Series = {Humor-Aproximaciones Transdisciplinares},
Year = {2018},
Pages = {223-248},
Abstract = {This work will discuss the presence of humor and laughter in the Middle
   Ages and its pre-modern readings from the perspective of
   philosophical-theological thinking. We will attempt to show that the
   claim that the Middle Ages was a dark and, thus, gloomy, sad period is a
   topic. Humor and laughter have always been present in man kind and
   western Christian theological literature. To this end, we will look at
   some examples of theological descriptions, philosophical-medical
   thinking and religious paraenesis. We will examine the reading of
   classical thought in the Middle Ages and the use of the first
   theological and projection speculations in the Renaissance that led to
   the Baroque thinking. Humor and laughter in philosophical- theological
   thought are elements that evolve at the rate of the thinking about man
   and the cosmos and their divine reference.},
