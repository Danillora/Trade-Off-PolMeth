@article{ WOS:000439835600005,
Author = {Barth, Christiane and Bijsmans, Patrick},
Title = {The Maastricht Treaty and public debates about European integration: the
   emergence of a European public sphere?},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2018},
Volume = {26},
Number = {2},
Pages = {215-231},
Abstract = {The Maastricht Treaty has widely been identified as a turning point in
   European integration. However, little dedicated research has been done
   into how `Maastricht' affected media discourses about Europe. In this
   article, we aim to shed more light on this matter by means of a
   qualitative frame analysis of media discourses in Britain and Germany,
   two countries that have traditionally had different perspectives on
   European integration. We show that in the course of the debate about the
   Maastricht Treaty the media discourses in the two countries gradually
   converged along the same aspects of European integration and
   increasingly used negative values to evaluate these aspects. This
   convergence of debates can be seen as representing a nascent
   transnational public sphere for the discussion of EU affairs.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
