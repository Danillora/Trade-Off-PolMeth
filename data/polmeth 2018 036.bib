@incollection{ WOS:000668697200005,
Author = {Mauro Sujatovich, Luis},
Editor = {Delgado, JB},
Title = {El Hijo del Ahuizote and Don Quijote: Political Humor in
   Mexico and Argentina at the End of the 19th Century},
Booktitle = {HUMOR AND POLITICS: A TRANSCULTURAL PERSPECTIVE},
Series = {Humor-Aproximaciones Transdisciplinares},
Year = {2018},
Pages = {109-134},
Abstract = {The chapter will comparatively analyze the political cartoons of the
   newspapers El Hijo del Ahuizote (Mexico City) and Don Quijote (Buenos
   Aires), published between 1884 and 1886 to identify the editorial stands
   on the presidencies of P. Diaz and J.A. Roca. From the content analysis
   we will establish the themes and figures of speech most frequently used
   by each, as well as their distinctive characteristics. The examination
   of the chosen political cartoons will reveal that both weekly
   publications confronted the presidents, becoming the most fervent,
   original, successful opponents, in a context marked by the fragility of
   republican institutions due to the strong selfishness of both presidents
   and the recent consolidation of national states. Consequently, their
   differences were only centered on the figures of speech and the
   narrative styles chosen to create their cartoons. On the one hand, El
   Hijo del Ahuizote decided to use caricatures that allowed recognizing
   such figures, emphasizing the construction of situations that alluded,
   from humor, to the problems raised by the management of P. Diaz. On the
   other hand, Don Quijote resorted to the animalization of J.A. Roca and
   other national leaders to create mutual understanding with its readers,
   without attaching so much importance to the characteristics of the
   context in which they were involved.},
