@article{ WOS:000429069200007,
Author = {Geddes, Andrew and Hadj-Abdou, Leila},
Title = {Changing the path? EU migration governance after the `Arab spring'},
Journal = {MEDITERRANEAN POLITICS},
Year = {2018},
Volume = {23},
Number = {1, SI},
Pages = {142-160},
Abstract = {This article shows how understandings amongst policy elites of a `new
   normal' form the basis for current and future EU action on migration in
   the Mediterranean region. This new normality centres on the
   understanding that Europe faces significant migratory pressures at its
   Mediterranean borders. By opening the `black box' of European and EU
   migration governance, the article seeks to provide fresh insight into
   how framing and frame enactment shape policy responses. Rather than
   detailing the `outputs' or `outcomes' of European migration governance
   systems such as laws and policy approaches - this paper adopts a
   different approach by exploring the underlying perceptions and
   understandings of migration held by actors within migration governance
   systems.},
