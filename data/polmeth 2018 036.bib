DOI = {10.1017/lap.2018.4},
Article-Number = {PII S1531426X18000043},
ISSN = {1531-426X},
EISSN = {1548-2456},
Keywords = {elites; turnovers; authoritarian regimes; authoritarian enclaves;
   subnational politics; democratization; Mexico},
Keywords-Plus = {DEMOCRATIZATION; ARGENTINA; DEMOCRACY; PARTIES; REGIMES; STATE},
