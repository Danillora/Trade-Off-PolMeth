@incollection{ WOS:000668697200006,
Author = {Coscia, Vanesa},
Editor = {Delgado, JB},
Title = {The Use of Political Graphic Humor and its Relationship with Labor
   Issues in the Argentine Media},
Booktitle = {HUMOR AND POLITICS: A TRANSCULTURAL PERSPECTIVE},
Series = {Humor-Aproximaciones Transdisciplinares},
Year = {2018},
Pages = {135-155},
Abstract = {This work is part of a broader research project that analyzes media
   discourses in the main newspaper of Argentina, Clarin, on the working
   world in the 2004-2007 period, characterized by economic recovery and
   high labor unrest.
   As part of the selection of these media narratives, this study will
   specifically analyze those jokes, cartoons and comics that were included
   in this period and accompanied the news on issues related to the
   problems of the working world. Based on this, it will be inquired as to
   whether the political-union graphic humor operates to complement the
   information contained in the ``serious{''} news and displays possible
   renewed meanings, or, on the contrary, whether it results in the meaning
   already offered by the headline, sub-heading or epigraph of a photo-as
   elements that frame the news-, and therefore, does not deviate from the
   editorial line that the newspaper has.
   It will then be argued that the resource of humor included in the pages
   of media seldom conveys other possible meanings beyond what is
   established by the agenda of a medium. In this line of thought, the
   general hypothesis guiding the research is that the representations
   built by mass media usually carry out processes of reducing, simplifying
   and stereotyping social problems.
   Methodologically, this study used a combination of socio-semiotic tools
   to account for the discursive and socio-communicational interrelations
   as a whole (news, jokes) on the media surface.},
