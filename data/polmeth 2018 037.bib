@article{ WOS:000433986700007,
Author = {Sola, Jorge and Rendueles, Cesar},
Title = {Podemos, the upheaval of Spanish politics and the challenge of populism},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2018},
Volume = {26},
Number = {1},
Pages = {99-116},
Abstract = {The rise of Podemos, a political force that is ideologically and
   organizationally new, has shaken Spanish politics. Only three years old,
   and taking advantage of the dual economic and political crisis, it has
   managed to put an end to the two-party political system, while becoming
   a source of inspiration (or grounds for concern) outside of Spain. The
   aim of this paper is to analyse the origins of Podemos, its main
   characteristics (its ideology, its organization and its social bases)
   and the impact it has had on Spanish politics.},
