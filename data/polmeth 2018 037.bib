@article{ WOS:000450458600002,
Author = {Dunkley, Ria and Baker, Susan and Constant, Natasha and
   Sanderson-Bellamy, Angelina},
Title = {Enabling the IPBES conceptual framework to work across knowledge
   boundaries},
