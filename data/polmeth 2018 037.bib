@incollection{ WOS:000668697200011,
Author = {Dorinson, Joseph},
Editor = {Delgado, JB},
Title = {Humor from the Edge},
Booktitle = {HUMOR AND POLITICS: A TRANSCULTURAL PERSPECTIVE},
Series = {Humor-Aproximaciones Transdisciplinares},
Year = {2018},
Pages = {291-322},
Abstract = {Precursors of modern stand-ups, badchonim and marshalikim (clowns and
   preachers) enjoined each other to ``tell it like it is{''} comically
   well before the advent of Jackie Mason. A seasonal event, the
   Purimshpiel (Purim Play) sanctioned irreverent humor and granted license
   to a number of fools-the lets, nar, and payats to act comically. Droll
   characters-shnorrers, schlemiels, schlimazels, and
   luftmentshen-originated in the East European shtetl or village.
   Jewish comedy will be compared with humor narratives created by
   African-Americans in coping with slavery, segregation, and identity.
   Certain prototypes found in social psychologist Gordon Allport's triad:
   accommodation, avoidance, and aggression play a transformative role.
   Slave narratives, minstrel shoes, stereotypes, and stand-up comedians,
   especially Bert Williams and Richard Pryor invite insightful study.
   Clearly, Sholem Aleichem's coupling of ``laughter and tears{''} applies
   to both minority groups as a liberating force. Ralph Ellison suggested
   that we lift the yoke when we change the joke, thereby establishing a
   positive, life-sustaining identity even in the throes of slavery and the
   Holocaust.},
Publisher = {UNIV COOPERATIVE COLOMBIA},
Address = {COLOMBIA 41-70, MEDELLIN, ANTIOQUIA 00000, COLOMBIA},
Type = {Article; Book Chapter},
