@incollection{ WOS:000668697200004,
Author = {Guzman Lopez, Jose Elias},
Editor = {Delgado, JB},
Title = {Humor and Political Instruction in 19th-century Mexico: El Pensador
   Mexicano and El Gallo Pitagorico},
Booktitle = {HUMOR AND POLITICS: A TRANSCULTURAL PERSPECTIVE},
Series = {Humor-Aproximaciones Transdisciplinares},
Year = {2018},
Pages = {73-108},
Abstract = {This work will analyze the role of satire as a media resource for
   political instruction and the moralization of Mexican society in the
   first half of the 19th century. The Spanish tradition is praised in the
   literature of New Spain and its subsequent transformation in the
   political press, after having decreed freedom of the press, and the use
   made of it to form the new Mexican society by two well-known journalists
   and writers of the time, Jose Joaquin Fernandez de Lizardi and Juan
   Bautista Morales, through their well-known political-satirical works: El
   Pensador Mexicano and El Gallo Pitagorico. We will start by recognizing
   that satire is a long-standing rhetorical device in the history of
   Western literature and that, throughout such a process, it retained many
   of the structural features and roles that have differentiated it to this
   day. However, we consider that, only by knowing the historical context
   and the pragmatic nature of its writing activity, is it possible to
   understand its moralizing, instructive and repressive function, while
   being scathing, mocking, jocular and humorous, which led to either its
   recognition in literary and academic circles or its censorship,
   persecution and prohibition in the social, religious and political
   spheres.},
