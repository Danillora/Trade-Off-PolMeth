@article{ WOS:000426784500007,
Author = {Petersen-Perlman, Jacob D. and Fischhendler, Itay},
Title = {The weakness of the strong: re-examining power in transboundary water
   dynamics},
Journal = {INTERNATIONAL ENVIRONMENTAL AGREEMENTS-POLITICS LAW AND ECONOMICS},
