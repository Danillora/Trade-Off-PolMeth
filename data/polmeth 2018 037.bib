   Colgan, Jeff D., Amer Univ, Washington, DC 20016 USA.
   Colgan, Jeff D., Woodrow Wilson Int Ctr Scholars, Washington, DC 20560 USA.},
DOI = {10.1162/GLEP\_a\_00443},
ISSN = {1526-3800},
