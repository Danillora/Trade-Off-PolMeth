   Davis, Sacha E., Univ Newcastle, Sch Humanities \& Social Sci, Newcastle, NSW, Australia.},
DOI = {10.1080/00905992.2018.1448375},
ISSN = {0090-5992},
EISSN = {1465-3923},
Keywords = {Balkanism; nationalism; travelogues; Hungarian; Saxon; Romanian},
Keywords-Plus = {HABSBURG MONARCHY; EASTERN-EUROPE},
Research-Areas = {Area Studies; Ethnic Studies; History; Government \& Law},
Web-of-Science-Categories  = {Area Studies; Ethnic Studies; History; Political Science},
