@article{ WOS:000441114200004,
Author = {Akopova, Elen and Nursapa, Assiya and Kuderin, Ilyas},
Title = {Current environmental problems in member states of the Eurasian Economic
   Union},
Journal = {INTERNATIONAL ENVIRONMENTAL AGREEMENTS-POLITICS LAW AND ECONOMICS},
