@article{ WOS:000429507900006,
Author = {Calu, Marius-Ionut},
Title = {Unintended consequences of state-building and the management of
   diversity in post-conflict Kosovo},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2018},
Volume = {46},
Number = {1},
Pages = {86-104},
Abstract = {This paper examines the adoption of a multiethnic liberal-democratic
   model of governance in post-independence Kosovo and the dual task of
   state-building to secure unity and manage diversity. This article
   explains why in post-conflict and post-independence Kosovo, its domestic
   sovereignty and legitimization have become conditioned by the
   integration, accommodation, and protection of its minorities. While the
   existing literature has mainly focused on the shortcomings deriving from
   the exogenous character of state-building in Kosovo, this paper aims to
   challenge and complement this view by drawing on the
   ``state-in-society{''} approach developed by Joel Migdal, which
   highlights that the actual states have less coherence than their
   theoretical counterparts. Analysis of post-independence Kosovo reveals
   the legislation-implementation gap and the unintended consequences
   arising under the impact of endogenous factors. Overall, this article
   shows that multiethnic statebuilding in Kosovo has been crucially
   transformed and ``limited{''} by local idiosyncrasies.},
