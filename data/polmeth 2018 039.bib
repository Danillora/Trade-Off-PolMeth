@incollection{ WOS:000441026000006,
Author = {Angom, Sidonia},
Book-Author = {Angom, S},
Title = {Lederach's Peacebuilding Framework},
Booktitle = {WOMEN IN PEACEMAKING AND PEACEBUILDING IN NORTHERN UGANDA},
Series = {Anthropocene-Politik Economics Society Science},
Year = {2018},
Volume = {22},
Pages = {65-115},
Abstract = {The gender-sensitive methodologies, together with the three-tier
   peacebuilding model by Lederach, constituted the theoretical framework
   within which the study was conducted. Lederach's model was modified to
   include peacemaking. Although gender-sensitive approaches to peacemaking
   and peacebuilding have increased in recent years, especially among
   scholars, in practice these processes often still fail to adequately
   address the countless needs of women and understand the complex
   interplay between gender, conflict, peacemaking and post-conflict
   rebuilding. The study was qualitative and field-based. Individual
   interviews and Focus Group Discussions were conducted from August 2009
   to July 2011 and involved 100 key participants. Prior to the publication
   of the book follow-up interviews were also conducted in 2016 to
   ascertain new developments of events. The majority of interviews took
   place in the northern districts of Gulu, Kitgum and Pader, with
   additional interviews in Kampala with key stakeholders. In the north, a
   wide variety of individuals were interviewed from government and
   non-governmental organisations, civilian communities and the returnees
   trying to resettle after many years in captivity and the experience of
   living in the IDP camps for more than two decades.},
