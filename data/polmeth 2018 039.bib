@incollection{ WOS:000816877000010,
Author = {Sampson, Lynette},
Editor = {Carr, PR and Hoechsmann, M and Thesee, G},
Title = {CRITICAL PEDAGOGY THROUGH PARTICIPATORY VIDEO Possibilities for
   Post-Colonial Higher Education in the Caribbean},
Booktitle = {DEMOCRACY 2.0: MEDIA, POLITICAL LITERACY, AND CRITICAL ENGAGEMENT},
Series = {Critical Media Literacies Series},
Year = {2018},
Volume = {1},
Pages = {127-146},
Abstract = {This chapter proposes that the use of participatory video in university
   teaching and learning is a necessary ontological input for Caribbean
   students of communication studies. The paper argues further that in the
   Caribbean, where the ravages of chattel slavery and colonialism
   influenced the region for over 500 years, and where the media hegemony
   of North America continues to dominate the region's communication flows
   today, there is an urgent need for the development of media literacy
   skills, civic responsibility, and indigenous knowledge by and about
   Caribbean society. The use of participatory video in communication
   studies is founded on Paulo Freire's ideas of education for liberation,
   and Bloom's taxonomy of educational objectives with respect to a
   framework for assessing the higher order skills being developed. This
   approach would give rise to meaningful community participation, provide
   opportunities for Caribbean students to resist dominant cultural
   constructions of Caribbean / personal identities, and for institutions
   to develop indigenous materials for teaching, learning, and advocacy.
   This chapter justifies its assertions through the examination of the
   case study, Traditional Pathways to Health, which provides examples of
   the application of this necessary educational input.},
