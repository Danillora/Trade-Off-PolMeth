@incollection{ WOS:000842456200004,
Author = {Musa, Aliyu and Kurawa, Gwadabe},
Editor = {Sengupta, E and Blessinger, P},
Title = {THE ROLE OF EDUCATION IN THE RESETTLEMENT OF INTERNALLY DISPLACED
   PERSONS IN NIGERIA: AN EXPLORATORY DISCOURSE ON THE PLIGHT OF THE
   DEVASTATED COMMUNITIES IN THE NORTHEAST OF NIGERIA},
Booktitle = {STRATEGIES, POLICIES AND DIRECTIONS FOR REFUGEE EDUCATION},
Series = {Innovations in Higher Education Teaching and Learning},
Year = {2018},
Volume = {13},
Pages = {21-35},
Abstract = {The focus of this chapter is to analyze the impacts of the Boko Haram
   -insurgency on education in the northeast of Nigeria. It also aims to
   -examine the effectiveness of the educational interventions in the
   devastated -communities in the northeastern states of Borno, Yobe,
   Adamawa, and Gombe. It begins with an introduction, in which the origin
   of the conflict is traced and linked to the group's campaign of violence
   that targets schools, teachers, education -workers, and students. The
   chapter discusses and chronologically lists out specific attacks on
   education and how the attacks have significantly affected access to
