@article{ WOS:000453408900001,
Author = {Wu, Yonghong and Shi, Yu},
Title = {How does intergovernmental fiscal environment affect general fund
   balances of major American cities?},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2018},
Volume = {44},
Number = {6},
Pages = {745-765},
Abstract = {This study explores the extent to which intergovernmental fiscal factors
   affect fiscal reserves in municipal general funds. Statistical results
   from panel data of 87 major cities in the US for the period from 1995 to
   2010 show that cities facing more restrictive limitations on local
   property taxation tend to maintain higher levels of unreserved general
   fund balances. Additional analyses also show that fiscally constrained
   cities accumulate surpluses in their general funds. This is consistent
   with the proactive approach in which municipal governments make fiscal
   decisions with the awareness of expected state constraints on their
   revenue-raising capacity. We call for consideration of relevant
   intergovernmental constraints in the determination of appropriate level
   of fund balances for municipal governments.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
