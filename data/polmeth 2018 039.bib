@incollection{ WOS:000431768100003,
Author = {Kalin, Nadine M.},
Book-Author = {Kalin, NM},
Title = {Educating the Artrepreneur},
Booktitle = {NEOLIBERALIZATION OF CREATIVITY EDUCATION: DEMOCRATIZING, DESTRUCTING
   AND DECREATING},
Series = {Creativity, Education and the Arts},
Year = {2018},
Pages = {19-40},
Abstract = {This chapter begins by asking: What if business remade creativity
   education on its terms? What would become of art and its education in
   the wake of creativity-for-business only? This chapter explores these
   questions in light of the intensifying entrepreneurialization of art and
   its education. I examine the Partnership for the 21st Century Skills'
   push for entrepreneurial literacy through art and creativity education
   along with the rise of artrepreneurs within contemporary art. I end with
   a plea for the relative autonomy of art education so that it may not
   only be a mode of vocationalization, but also a site of struggle and
   critical creativity.},
