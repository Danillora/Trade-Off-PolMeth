@article{ WOS:000605933800005,
Author = {Aydagul, Batuhan},
Title = {TURKEY'S PROGRESS ON GENDER EQUALITY IN EDUCATION RESTS ON GENDER
   POLITICS},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2019},
Volume = {18},
Number = {1},
Pages = {45-57},
Month = {SPR},
Abstract = {Turkey came a long way from being a negative outlier on gender parity in
   the global scene to ensuring almost universal enrollment for girls.
   Effective education and social policy interventions together with
   national stakeholder mobilization enabled this progress. Nevertheless,
   Turkey has room to advance so that all girls finish secondary education
   with a robust foundation to become active citizens. This will require
   the Ministry of National Education to overcome its quality deficiency in
   schools and mainstream gender equality to educational pedagogy and
   practice. Most importantly, stakeholders should acknowledge the role of
   gender politics in girls' education and engage in a genuine discussion
   about education and empowerment of girls. This would require an
   inclusive dialogue, acknowledging Turkey's ethnic, religious, social,
   and cultural differences.},
