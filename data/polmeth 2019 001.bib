@article{ WOS:000496514200003,
Author = {Cooper, Andrew F. and Alexandroff, Alan S.},
Title = {Assessing the variation of `leader-focused status' in contemporary
   global governance},
Journal = {CONTEMPORARY POLITICS},
Year = {2019},
Volume = {25},
Number = {5, SI},
Pages = {532-548},
Month = {OCT 20},
Abstract = {This article argues that with the `Rise of the Informals' the
   traditional conception of status in international relations is outdated.
   Whereas through the nineteenth and twentieth centuries status was
   primarily created by the consequences of war, in the twenty-first
   century attributions of status are more explicitly institutional based.
   With the shift from small concerts to larger apex forums, therefore,
   opportunities to shape status standing are extended. This process of
   `democratization' in terms of global governance leadership is especially
   important for a mixture of states without the structural capability of
   great powers. If a function generally of the type of agency, however,
   the importance of leaders is highlighted. The G20 underscores this
   theme, although the dynamic of `democratization' must be examined
   through different stages of institutionalization. Even with these
   nuances, though, it is the differences as much as the similarities with
   traditional fixed hierarchies that comes to the fore.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
