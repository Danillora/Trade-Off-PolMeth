@article{ WOS:000510178200012,
Author = {Elliott, Luba},
Title = {Art, Intelligence and Creativity},
Journal = {RUSI JOURNAL},
Year = {2019},
Volume = {164},
Number = {5-6},
Pages = {110-119},
Abstract = {Featuring works by David Ha, Tom White, Daniel Ambrosi and Joanne
   Hastie, this selection represents the varied practice of artists
   engaging with AI today. From developing AI tools that draw to pushing
   the limits of image-recognition techniques and incorporating AI
   aesthetics in their work, these artists explore the full spectrum of the
   creative possibilities presented by AI. David Ha developed a model able
   to sketch animals in a similar way to humans, while Tom White plays with
   image recognition capabilities of neural networks, presenting abstract
   images that are recognised as specific objects. Daniel Ambrosi's
   interpretations of landscapes are enhanced by DeepDream's hallucinatory
   aesthetic, without being overshadowed by the technology. In her `Machine
   Learning Abstracts', Joanne Hastie relies on AI to come up with abstract
   compositions that she then paints by hand.
   The following images and accompanying text were supplied by each named
   contributor.},
