@article{ WOS:000467079400001,
Author = {Hunziker, Paula},
Title = {The Dilemmas of Socratism in Modernity. Some Hypotheses about Arendt's
   Reading of Kant},
Journal = {ANACRONISMO E IRRUPCION},
Year = {2019},
Volume = {9},
