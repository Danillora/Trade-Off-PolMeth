@incollection{ WOS:000553034800010,
Book-Author = {Lal, BV},
Title = {Frequent flyers},
Booktitle = {LEVELLING WIND: REMEMBERING FIJI},
Series = {Pacific Series},
Year = {2019},
Pages = {149-160},
Abstract = {After the coup of 1987, more than 150,000 mostly Indo-Fijians left Fiji
   for other countries, principally North America and Australasia. The best
   and the brightest have left and many more would if they only could,
   draining the country of the talents and skilled manpower it can
   ill-afford to lose. There is by now much information available on the
   sociology and economics of migration from Fiji, but rather little is
   known about the extent of rupture and emotional trauma that displacement
   entails. The following story aims to capture the private experience of a
   couple of Indo-Fijians who migrated to Australia.},
