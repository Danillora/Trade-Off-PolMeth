@article{ WOS:000500542800001,
Author = {Wissenburg, Marcel},
Title = {The Anthropocene and the republic},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
