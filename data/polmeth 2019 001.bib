@article{ WOS:000497913100001,
Author = {Choi, Seung-Whan and Noll, Henry},
Title = {Democratic peace: Does ethnic inclusiveness reduce interstate conflict?},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2021},
Volume = {42},
Number = {2},
Pages = {179-196},
Month = {MAR},
Abstract = {In this study, we argue that ethnic inclusiveness is an important
   democratic norm that fosters interstate peace. When two states are
   socialized into the notion of ethnic tolerance, they acquire the ability
   to reach cooperative arrangements in time of crisis. Based on
   cross-national time-series data analysis covering the period 1950-2001,
   we illustrate how two states that are inclusive of their politically
   relevant ethnic groups are less likely to experience interstate disputes
   than states that remain exclusive. This finding was robust, regardless
   of sample size, intensity of the dispute, model specification, or
   estimation method. Therefore, we believe in the existence of ethnic
   peace: ethnic inclusiveness represents an unambiguous force for
   democratic peace.},
