@article{ WOS:000465150600004,
Author = {Posocco, Lorenzo},
Title = {Museum Politics in Turkey Under the Islamic Justice and Development
   Party (AKP): the Case of the Istanbul Museum of the History of Science
   and Technology in Islam},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
