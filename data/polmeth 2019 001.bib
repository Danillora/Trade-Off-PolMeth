@article{ WOS:000469859000007,
Author = {Sakellaropoulos, Spyros},
Title = {The Althusser-Poulantzas Discussion on the State},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2019},
Volume = {9},
Number = {1},
Pages = {128-141},
Abstract = {The aim of this article is to present and criticize Althusser's and
   Poulantzas's key positions on the State and its mechanisms. To begin
   with there is a discussion on the notion that the State is an
   aggregation of ideological and repressive apparatuses and on the
   position according to which the State is an instrument. This is followed
   by a rejection of Althusser's and Poulantzas's claims in the light of
   the thesis that the State is the material outcome of a correlation of
   forces in the context of a specific mode of production but that there is
   a limit to the changes that may be made within the capitalist mode of
   production.},
