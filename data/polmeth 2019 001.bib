
@article{ WOS:000504744400001,
Author = {Williams, H. Howell},
Title = {Moralizing queer dialectics: a response to Cyril Ghosh},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2021},
Volume = {24},
Number = {7},
Pages = {1061-1067},
Month = {NOV 10},
Abstract = {Ghosh's book makes important interventions in contemporary LGBTQ+ rights
   politics, but some analytical and methodological choices produce
   oversights. In this response I address three such instances. First, I
   trouble the use of the term `queer' in the text, arguing that the focus
   on critiquing binaries is a useful but narrow deployment of the term.
   Second, I discuss dialectics as a methodology to show that the
   dialectical project undertaken here is incomplete. Finally, I
   interrogate Ghosh's useful term `Radical Theory Creep' to demonstrate
   the need for more analytical clarity on when a theory's explanatory
   power has crept beyond its limits.},
