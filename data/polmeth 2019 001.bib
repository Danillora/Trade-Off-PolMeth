@article{ WOS:000489304700007,
Author = {Barnello, Michelle A. and Bitecofer, Rachel and Kidd, Quentin},
Title = {Ready for Hillary?: Explicit and Implicit Sexism in the 2016
   Presidential Election},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
Year = {2019},
Volume = {17},
Number = {2},
Pages = {295-313},
Month = {JUL},
Abstract = {The 2016 nomination of Hillary Clinton as the first female major party
   nominee for president created an unprecedented opportunity to test for
   evidence of explicit sexism in the electorate. Presidential elections
   normally produce two equally matched nominees with impressive public
   service resumes who behave similarly on the campaign trail. However,
   while Democrats were making history nominating the first female nominee,
   the Republicans also made history by selecting a nominee with no public
   service experience, a controversial personal background, and conduct
   that conflicted sharply with traditional norms of presidential
   candidates. In survey after survey, voters recognized that Clinton held
   a significant qualification advantage over Donald Trump. Yet, despite
   the fact that both men and women were more likely to rate Clinton as
   more qualified than her opponent overall, using an innovative approach
   via an original survey, we find evidence of implicit sexism in the way
   that some males evaluated Clinton compared to their female counterparts.},
