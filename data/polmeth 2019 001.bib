@article{ WOS:000511698600003,
Author = {Ulbricht, Lena and Roemer, Magnus},
Title = {Diversity and uniformity of German data protection oversight},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2019},
Volume = {13},
Number = {4},
Pages = {461-489},
Month = {DEC},
Abstract = {Against the backdrop of the recent reform of data protection law in the
   EU and Germany, this contribution analyzes to which extent data
   protection control differs between the German federal states. Whereas
   data protection law is set at EU level, the oversight takes place at the
   national level or, as in the German case, on the sub-national level.
   Relying on a qualitative analysis of the 18 independent data protection
   authorities in Germany, we found variety in their activities within a
   relatively uniform regulatory environment. The authorities differ with
   regard to their action orientation, their action, their regulatory
   knowledge and their external communication. An important dimension of
   differentiation is a tension between preventive and sanctioning
   oversight activities. In addition, German data protection authorities
   have developed different fields of expertise and communication. This
   paper has not aimed to find determinants for this variety. However, we
   found tentative evidence that the authorities' resources, their
   political culture and policy entrepreneurs were important factors for
   their choice of preventive versus sanctioning measures. In contrast, we
   found no clear evidence about the importance of the German division
   between East and West, regulatory competition and party politics. Our
   findings demonstrate which oversight strategies and measures are
   currently utilized by German data protection authorities to ensure an
   overall high level of data protection. They furthermore indicate various
   measures of how to prevent the regulatory competition in data
   protection.},
