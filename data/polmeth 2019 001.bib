@article{ WOS:000629143900003,
Author = {Ramirez Partida, Hector Ramon and Perez Varela, Luis Fernando},
Title = {Local economic development and tourism in Riviera Nayarit, Mexico; an
   approximation from public policy assessment},
Journal = {REVISTA MEXICANA DE ANALISIS POLITICO Y ADMINISTRACION PUBLICA},
Year = {2019},
Volume = {8},
Number = {1},
Pages = {29-47},
Month = {JAN-JUN},
Abstract = {This research paper aims to analyse the implementation of the tourism
   project Riviera Nayarit and so to assess its results as an instrument of
   public policy for local economic development in localities with strong
   tourism activity in Bahia de Banderas municipality. To carry out the
   analysis, this paper is based on the foundations of klario Polese (1998)
   Theory of Development from the roots o endogenous development, in his
   study about urban and regional economy, therefore, the assessment of the
   results is made by using the analysis of variance as a way of comparing
   the means of the variables at two different time-slots over time. Then,
   the variables are contrasted to determine policy causality through the
   Tukey-B test and estimated with SPSS-21. The balance of evidence,
   Ceteris Paribus, shows the existence of a strong spatial segregation
   derived, almost exclusively, from the government determination to
   promote local economic development by exploiting all types of available
   resources in those areas subject to touristic activities.},
