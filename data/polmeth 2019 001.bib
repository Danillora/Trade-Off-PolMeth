@article{ WOS:000504053600001,
Author = {Alotaibi, Hajed A.},
Title = {RETRACTED: Can Non-Muslim Courts Bring Legal Change in Sharia Laws?
   (Retracted Article)},
Journal = {JOURNAL OF POLITICS AND LAW},
