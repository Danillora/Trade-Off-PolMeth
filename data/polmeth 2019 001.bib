@article{ WOS:000474799100033,
Author = {Wilson, Steven L. and Herrera, Yoshiko M.},
Title = {Teaching Computerized Content Analysis for Undergraduate Research Papers},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
