@article{ WOS:000498894900012,
Author = {Moreir, Nelson Camatta and Gomes, Raoni Vieira},
Title = {Adpf 153 judgment by stf in the light of the philosophy of political
   memory: narrate the facts, join the rags and remember not to forget},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2019},
Volume = {119},
Pages = {363-396},
Month = {JUL-DEC},
Abstract = {This article intends to formulate a critical analysis on the judgment of
   ADPF 153 by STF, emphasizing the votes of the Minister Relator and of
   the Minister who opened the divergence. From Walter Benjamin's
   perspective of the political philosophy of memory, we sought to present
   the relevance of the narrative for the reconstruction of a past that it
   is not going to pass. With emphasis on the role of the narrator, it was
   ambitious to build a critique of the amnesty maintained in the court
   reviewed and its consequences for Brazilian contemporaneity. The present
   article sought to present a hypothesis for the diagnosis of the
   consequences of the option for broad, general and unrestricted amnesty
   and its maintenance by the Supreme Court.},
