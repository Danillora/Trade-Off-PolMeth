@article{ WOS:000457984400010,
Author = {Willumsen, David M.},
Title = {So far away from me? The effect of geographical distance on
   representation},
Journal = {WEST EUROPEAN POLITICS},
Year = {2019},
Volume = {42},
Number = {3},
Pages = {645-669},
Month = {APR 16},
Abstract = {Legislators face numerous trade-offs with regard to how to spend their
   time. One factor is, however, beyond their control: the distance between
   their constituency and the legislature. A more distant constituency
   implies increased travel, which decreases the time available for
   activities within the legislature itself, while also raising the
   possibility of centre-periphery dynamics in representation. Previous
   work has found that as distance between constituency and legislature
   increases, so does constituency focus, but it has not established why
   this is. This article explores the impact of geographical remoteness on
   representational activity, analysing a dataset of parliamentary activity
   in the British House of Commons (2005-2015), showing that the more
   remote an MP's constituency, the less likely that MP is to attend votes,
   while being more likely to sign Early Day Motions. The article further
   shows that this is most likely driven by a centre-periphery dynamic
   rather than simply being a response to longer travel time.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Willumsen, DM (Corresponding Author), Univ Innsbruck, Dept Polit Sci, Innsbruck, Austria.
