@incollection{ WOS:000553034800022,
Book-Author = {Lal, BV},
Title = {Between a rock and a hard place},
Booktitle = {LEVELLING WIND: REMEMBERING FIJI},
Series = {Pacific Series},
Year = {2019},
Pages = {453-474},
Abstract = {In the 2014 General Election, an overwhelming majority of Indo-Fijians
   voted for Bainimarama's Fiji First Party. At first glance this looks
   curious, a people who were the target of previous military coups
