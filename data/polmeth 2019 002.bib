@article{ WOS:000588630400001,
Author = {Sharon, Assaf},
Title = {Locke, liberty, and law: Legalism and extra-legal powers in the
   Second Treatise},
Journal = {EUROPEAN JOURNAL OF POLITICAL THEORY},
Year = {2022},
Volume = {21},
Number = {2},
Pages = {230-252},
Month = {APR},
Abstract = {The apparent inconsistency between Locke's commitment to legalism and
   his explicit endorsement of the extra-legal power of prerogative has
   confounded many readers. Among those who don't ignore or dismiss it, the
   common approach is to qualify the role or scope of prerogative. The
   article advocates the opposite approach. It argues that Locke's legalism
   should be understood within the context of his oft neglected conception
   of political liberty in terms of self-government. This not only allows
   for the reconciliation of Locke's legalism with his endorsement of
   extra-legal powers, but also provides a fuller, more accurate account of
   the role of law and of political liberty in Locke's theory.},
