@article{ WOS:000500085100001,
Author = {Lowande, Kenneth and Proctor, Andrew},
Title = {Bureaucratic Responsiveness to LGBT Americans},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
Year = {2020},
Volume = {64},
