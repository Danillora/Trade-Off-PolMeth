@article{ WOS:000502225600001,
Author = {Adamsky, Dmitry (Dima)},
Title = {Christ-Loving Warriors: Ecclesiastical Dimension of the Russian Military
   Campaign in Syria},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2020},
Volume = {67},
Number = {6},
Pages = {433-445},
Month = {NOV 1},
Abstract = {This article explores the ecclesiastical dimension of the Russian
   operation in Syria. It analyzes pastoral activities within the
   expeditionary force, and argues that the Orthodox Church provided the
   military with three deliverables. First, the clergy assisted the
   commanders in providing a sense of mission to the servicemen and
   injecting higher levels of motivation among them. Second, the military
   saw the clergy as enhancing unit cohesion and decreasing post-combat
   stress effects. Finally, the ROC assisted the Kremlin in legitimizing
   the intervention domestically. The article concludes with prospective
   trends of the nexus of religion and policy in the Middle East and
   beyond.},
