@article{ WOS:000472979800001,
Author = {Behnke, Joachim},
Title = {Thinking About Reforms of the German Electoral System. Should we
   Strengthen the Component of Plurality Voting of the Electoral System?},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2019},
Volume = {60},
Number = {2},
Pages = {195-219},
Month = {JUN},
Abstract = {Recently Pappi and Brauninger enriched the current debate about the
   German electoral system with aproposal to use the criterion of electoral
   responsivity for the evaluation of electoral systems, which would be
   connected with astrengthening of the plurality component of the
   electoral system. This essay is acritical response to this proposal. One
   of its key premises is that there should be astrict separation of
   normative and positive (empirical) propositions. It has to be clear how
   positive features of an institutional design support normative
   propositions. The reactivity in plurality systems of the share of seats
   of the biggest party to the distribution of the votes between the third
   parties is not compatible with the normative requirements, which are
   usually seen as constitutive for the concept of responsivity. This can
   be shown by theoretical considerations as well as by empirical case
   studies. In the end electoral responsivity coincides with the
   concentration effect of the plurality system. Therefore it cannot
   deliver an independent justification for the priority of aspecific
   electoral system, which exeeds the one delivered by the plurality
   system.},
