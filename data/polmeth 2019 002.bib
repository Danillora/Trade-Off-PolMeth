@article{ WOS:000495845000001,
Author = {Diesen, Glenn},
Title = {The Geoeconomics of Russia's Greater Eurasia
   Initiative(sic)(sic)(sic)Palabras clave},
Journal = {ASIAN POLITICS \& POLICY},
