@article{ WOS:000464499100004,
Author = {Weinschenk, Aaron C. and Dawes, Christopher T.},
Title = {The Effect of Education on Political Knowledge: Evidence From
   Monozygotic Twins},
Journal = {AMERICAN POLITICS RESEARCH},
