@article{ WOS:000498499700001,
Author = {Kazun, Anastasia and Semykina, Kseniia},
Title = {Presidential Elections 2018: The Struggle of Putin and Navalny for a
   Media Agenda},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2020},
Volume = {67},
Number = {6},
Pages = {455-466},
Month = {NOV 1},
Abstract = {The study compares the networked issue agendas of Vladimir Putin and
   Alexey Navalny in Russian mainstream media and on the Internet utilizing
   the theoretical framework of issue ownership theory. We analyze the
   period from December 12, 2016 to December 12, 2017. The analysis shows
   that the issue agendas of Putin and Navalny are similar in the
   mainstream media and on the Internet. In both media types, Putin is
   often mentioned in connection with economic issues and international
   relations, which attract the attention of the population and are
   perceived as important. Navalny is associated with the issues of civic
   activism, NGOs and anti-corruption.},
