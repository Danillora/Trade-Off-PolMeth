@article{ WOS:000605937600005,
Author = {Co, Mikael De Lara},
Title = {BEYOND DISINFORMATION},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2019},
Volume = {18},
Number = {3},
Pages = {49-56},
Month = {FAL},
Abstract = {Much has been said about the marriage of disinformation and political
   agendas. Beyond fake news, however, disinformation operators have
   another, perhaps more ominous strategic intent: to create an environment
   of social tension within which ``populist{''} authoritarians can thrive.
   We take a deep dive into the Philippine context, looking at the
   vitriolic spiral that set the stage for the rise-and continued
   popularity-of President Rodrigo Roa Duterte, the calcification of this
   culture that has pushed an already beleaguered political opposition's
   back against the wall, and the possibility of a way forward through a
   renewed focus on commonalities and mindfulness with regards to political
   language and tone.},
