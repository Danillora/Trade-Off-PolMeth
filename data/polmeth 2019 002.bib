@article{ WOS:000573935900010,
Author = {Kozyreva, P. M. and Smirnov, I, A.},
Title = {TRUST IN UNSTABLE RUSSIAN SOCIETY},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2019},
Number = {5},
Pages = {134-147},
Abstract = {This article is focused on analyzing dynamics of levels and issues of
   developing trust within modern Russian society, based on data from the
   RLMS-HSE. The main objective was evaluating peculiarities in the
   formation and distribution of generalized trust, which have been
   observed throughout the last decade of crisis. While performing the
   analysis, certain new positive shifts were revealed in levels of
   generalized trust, which have been observed within various
   socio-demographic groups, despite an increase in uncertainty in all
   areas of social life. The authors emphasize factors which contribute to
   and hinder the spread of trust. It is stressed that the most critical
   factor which contributes to Russian people's increasing trust towards
   others is an increased sense of personal security. Analyzing a vast
   range of empirical data leads to the belief that generalized trust
   appears to be the most important condition necessary for consolidating
   the foundations of democratic life and further developing civil society.
   However, participating in the activities of democratic institutions and
   voluntary organizations of civil society do not necessarily become a
   source of increased trust towards other people.},
