@article{ WOS:000474757100005,
Author = {Lamb, Charles M. and Boston, Joshua and Neiheisel, Jacob R.},
Title = {Power Plus Persuasion: The Anatomy of Kennedy's Housing Order},
Journal = {CONGRESS \& THE PRESIDENCY-A JOURNAL OF CAPITAL STUDIES},
Year = {2019},
Volume = {46},
Number = {1},
Pages = {109-134},
Abstract = {We examine the influence of various political actors on the substance
   and timing of presidential orders. Specifically, we analyze President
   John F. Kennedy's decision to issue E. O. 11063, which directed the
   federal bureaucracy to eliminate discrimination in the sale or lease of
   residential property overseen by the federal government. We show that
   this order did not result solely from Kennedy's policy preferences;
   rather, it was a product of persuasion by several political actors who
   coaxed Kennedy to act. This reinforces previous findings that the
   president's power of the pen is often conditioned by the informal
   political costs of unilateral executive action.},
