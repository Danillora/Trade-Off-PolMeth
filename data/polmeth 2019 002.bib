@article{ WOS:000478818200005,
Author = {Brenner, Rachel Feldhay},
Title = {A Remarkable ``Emotional Community{''} of Rescuers in Occupied Warsaw:
   The Cases of Zofia Kossak and Jaroslaw and Anna lwaszkiewicz},
Journal = {JOURNAL OF GENOCIDE RESEARCH},
Year = {2019},
Volume = {21},
Number = {3, SI},
Pages = {398-417},
Abstract = {Jaroslaw and Anna lwaszkiewicz and Zofia Kossak were posthumously
   awarded the Medal of the Righteous among the Nations for rescuing Jews.
   The former hid Jews on their estate of Stawisko near Warsaw, whereas the
   latter was the founder of Zegota, The Polish Council to Aid Jews. Their
   responses to the Jewish plight, however, emerged from divergent pre-war
   ideological identifications. The lwaszkiewiczes' affiliation with
   Skamander, a pre-war avant-garde group of Polish and Polish-Jewish
   poets, affirmed their secular vision the equality and the ``brotherhood
   of men.{''} The reality of war effected a powerful sense of
   responsibility towards the Jewish fugitives despite the general Polish
   acquiescence with the destruction of the Jews. Whereas the
   lwaszkiewiczes remained faithful to the ideals of the Enlightenment,
   Kossak, a pre-war Catholic nationalistic-antisemitic writer, recognized
   the need to transform her attitude towards the Jews. While she never
   retracted her anti-Jewish sentiments, Kossak perceived the destruction
   of Jews as an abominable transgression of the fundamentals of the
   Christian faith. Dismayed by the widespread Polish participation in the
   Jewish extermination, Kossak demanded that the patristic doctrine of
   contempt for the Jews be suspended and that the concept of love your
   neighbour as yourself' be extended to the Jewish people. The
   juxtaposition of the cases of the lwaszkiewiczes and Kossak sheds light
   on the emotional and ideological complexity of the ethical value system
   that compels an altruistic attitude towards the victims of terror. The
   lwaszkiewiczes' and Kossak's consciously chosen position as social
   outliers highlights the importance of microhistory in the investigation
   of witnesses' responses to victims of genocide.},
