@article{ WOS:000490808800001,
Author = {Guntermann, Eric and Blais, Andre},
Title = {How do voters react when their party forms a coalition they dislike?},
Journal = {WEST EUROPEAN POLITICS},
Year = {2020},
Volume = {43},
Number = {7},
Pages = {1480-1489},
Month = {NOV 9},
Abstract = {While coalitions are conventionally seen as opportunities for parties to
   realise their policy preferences or to secure their control over
   political offices, recent studies show that citizens have preferences
   for coalitions which influence their vote choice. However, these studies
   do not consider how party and coalition preferences influence each
   other. This study uses panel data from the German Longitudinal Election
   Study from the 2009, 2013 and 2017 German elections to determine whether
   voters punish the party for which they voted for being in a coalition
   they dislike or, alternatively, whether they become more supportive of
   that coalition. We find weak evidence for the former but strong evidence
   for the latter.},
