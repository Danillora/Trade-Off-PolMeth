@article{ WOS:000477064100002,
Author = {Fabbrini, Sergio},
Title = {Constructing and de-constructing the European political identity: the
   contradictory logic of the EU's institutional system},
Journal = {COMPARATIVE EUROPEAN POLITICS},
Year = {2019},
Volume = {17},
Number = {4, SI},
Pages = {477-490},
Month = {AUG},
Abstract = {The article investigates the forms that political identities have
   assumed in the EU through the functioning of its institutions. On the
   basis of the analytical distinction between supranational and
   intergovernmental institutional settings, the article shows that such a
   dual decision-making system has generated a contradictory logic with
   regard to the construction of political identity. If the supranational
   institutions have aimed to construct a European political identity with
   state-like features, the intergovernmental institutions have instead
   operated to de-construct that identity in order to preserve the national
   identities of the member states. With the multiple post-Lisbon Treaty
   crises, not only has the contrast between the two concepts increased,
   but the competition among national identities within the
   intergovernmental setting has triggered their transformation into
   nationalist claims.},
