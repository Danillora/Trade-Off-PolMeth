@incollection{ WOS:000465471500005,
Author = {Du, Ping and Yu, Shiyang and Yang, Daoling},
Editor = {Du, P and Yu, S and Yang, D},
Title = {Effects of Development: E-governance Fully Supports Construction of
   Service-Oriented Government},
Booktitle = {DEVELOPMENT OF E-GOVERNANCE IN CHINA: IMPROVING CYBERSECURITY AND
   PROMOTING INFORMATIZATION AS MEANS FOR MODERNIZING STATE GOVERNANCE},
Series = {Research Series on the Chinese Dream and Chinas Development Path},
Year = {2019},
Pages = {39-65},
Abstract = {Going to great lengths to promote E-governance is the strategic means to
   strengthening the capacity of the party to rule, deepening reforms to
   the administrative management system, and creating a service-oriented
   government that pleases the people. In 2002, the CPC Central Committee
   General office and State Council General Office jointly retransmitted
   the ``Leading Group for National Informatization Guiding Opinions on the
   Construction of National E-governance,{''} 1 which resolved to:
   establish E-governance construction as the focus of informatization
   work; drive the informatization of the development of the national
   economy and society with `` the government leading the way{''};and throw
   open the curtain of comprehensive, rapid development of E-governance in
   China. Over the past 10 years, E-governance construction has been stably
   promoted in China and has played an important role in improving public
   services, strengthening public administration, strengthening
   comprehensive regulation, improving macro-controls, and other areas,
   making E-governance construction an indispensable, effective method for
