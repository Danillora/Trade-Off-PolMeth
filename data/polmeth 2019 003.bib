@article{ WOS:000478605800005,
Author = {Esarey, Justin and Schwindt-Bayer, Leslie A.},
Title = {Estimating Causal Relationships Between Women's Representation in
   Government and Corruption},
Journal = {COMPARATIVE POLITICAL STUDIES},
