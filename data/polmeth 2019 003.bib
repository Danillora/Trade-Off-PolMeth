@article{ WOS:000482504100001,
Author = {Sokolov, Denis},
Title = {Islam in the Gold Heart of Russia: Ingush Islamic Communities in Kolyma},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2020},
Volume = {67},
Number = {4-5, SI},
Pages = {348-361},
Month = {SEP 2},
Abstract = {This article analyzes evolving patterns of migration between
   traditionally Muslim parts of the post-Soviet region and the Russian Far
   East, with an emphasis on the migration of Ingush between rural
   Ingushetia and urban areas of Magadan Oblast. It shows how the initial
   isolation of Ingush migrants from their homeland has been overcome with
   the aid of more affordable travel and new communications technologies.
   It describes the emergence of new Islamic communities among migrants in
   the Far East, the relations between these communities and municipal
   authorities, the rivalry between Sufi brotherhoods, and the recent split
   between Sufi and Salafi Muslims.},
