@incollection{ WOS:000813444900012,
Author = {De Maesschalck, Filip and Gemperle, Sergio},
Editor = {Wiesner, C and Harfst, P},
Title = {Multiple Legitimation Strategies of authoritarian Regimes},
Booktitle = {LEGITIMITAT UND LEGITIMATION: VERGLEICHENDE PERSPEKTIVEN},
Series = {Vergleichende Politikwissenschaft},
Year = {2019},
Pages = {347-372},
Abstract = {Recent research on legitimacy points to the relevance of various
   strategies authoritarian regimes use to try to legitimize themselves. So
   far, this research has focused on domestic legitimation strategies,
   neglecting the recognition or denial of legitimacy by international
   actors. In this chapter, however, we argue that the interplay of
   domestic and international (de)legitimation dynamics must be considered
   in the analysis of legitimation strategies. Accordingly, we explore the
   following research guiding assumptions for authoritarian regimes in this
   chapter. First, in dealing with authoritarian regimes, questions of
   legitimacy and legitimation of local regimes by international actors are
   subordinated. Second, through their influence on authoritarian
   legitimacy strategies, international actors may unintentionally
   contribute to the consolidation of authoritarian regimes. These
   assumptions relate to the overarching research question of to what
   extent and with what effect legitimacy dynamics influence authoritarian
   legitimacy strategies. This chapter addresses these questions by looking
   at the different legitimacy dynamics before, during and after the 2015
   elections in Burundi. As the example of Burundi shows, international
   actors can strengthen a regime through their intervention.
   Paradoxically, this happens not only through regime-legitimizing
   strategies, but also through delegitimizing activities. This raises the
   need for international actors to better analyze legitimation strategies
   of governments in order to anticipate their strategic adaptation to
   legitimating or delegitimating activities.},
Publisher = {SPRINGER VS/SPRINGER FACHMEDIEN},
