@article{ WOS:000475141600001,
Author = {Price, Kimala},
Title = {The rewards and challenges of creating an activist-scholar research
   program},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2020},
Volume = {8},
Number = {1},
Pages = {203-211},
Month = {JAN 1},
Abstract = {This essay discusses the author and her colleagues' development and
   implementation of an activist-scholar fellowship program. Based on the
   concept of feminist research justice, the program matches undergraduate
   and graduate students with local community organizations to work on
   collaborative research projects, which are based on the specific needs
   articulated by the participating community organizations. This program
   not only strengthens the organizations' political agendas by providing
   research assistance and the involvement of a new generation of feminist
   activist-scholars; it also provides the students with the opportunity to
   develop practical research and advocacy skills through experiential
   learning. The program also allows students to think critically about the
   relationship between academia and non-academic communities and the
   production of knowledge.},
