@article{ WOS:000503859300012,
Author = {Wilkins-Laflamme, Sarah and Reimer, Sam},
Title = {Religion and Grassroots Social Conservatism in Canada},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2019},
Volume = {52},
Number = {4},
Pages = {865-881},
Month = {DEC},
Abstract = {With a decisive Liberal party electoral victory in 2015, observers are
   now wondering if religious conservatism's role in the Canadian political
   landscape is waning. Using data from the Canadian Election Study (CES)
   from the years 2004 to 2015, we find that respondents' attitudes toward
   same-sex marriage and women working outside the home have moved left on
   the spectrum among both the general population and more religious
   voters. However, this does not go hand in hand with a decline in the
   effect strength of religiosity on the Conservative vote, which remains
   significant across the five federal elections examined in this study.
   Conservative religious voters now make up a smaller share of the adult
   population, but their issue positions on sexual morals and gender roles,
   along with the wider conservative value orientation these issues
   represent, remain important in their vote choice.},
