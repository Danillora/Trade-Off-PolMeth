@article{ WOS:000481504500005,
Author = {Roseth, Tom},
Title = {Moscow's Response to a Rising China Russia's Partnership Policies in Its
   Military Relations with Beijing},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2019},
Volume = {66},
Number = {4},
Pages = {268-286},
Month = {JUL 4},
Abstract = {This article analyzes Moscow's orientation toward Beijing. While China's
   rise raises worries for most neighboring states, it appears that for
   Russia, security concerns vis-a-vis the United States and NATO outweigh
   other considerations. Russo-Chinese military cooperation has increased
   through high-tech arms trade, bilateral exercises, and improved border
   relations. The article asks whether Moscow is downplaying a pragmatic
   partnership policy in favor of a more permanent strategic approach in
   its military policies toward China. By pursuing arms sales and bilateral
   exercises, Russia allows for vulnerability in its cooperation with
   China, indicating a strategic partnership. At the same time, deployments
   along the common border point to safeguarding and pragmatic elements,
   revealing a not fully coherent strategy.},
