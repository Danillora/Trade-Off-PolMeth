@article{ WOS:000483870300001,
Author = {Peterson, Rolfe Daus and Palmer, Carl L.},
Title = {The Dark Triad and nascent political ambition},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2019},
Month = {2019 AUG 31},
Abstract = {This research considers the effects of the personality characteristics
   of Machiavellianism, narcissism, and psychopathy (labeled the Dark
   Triad) on political ambition. Research on nascent ambition has shown
   that individuals who express political ambition differ by social
   background, gender, and personality. Using original survey research, our
   analyses find that Dark Triad traits are significantly related to
   ambition. Respondents who score higher in Machiavellianism are more
   likely to have higher political ambition, more likely to enjoy the
   specific aspects of campaigning, and more likely to predict they will be
   successful candidates. While narcissism is related to feeling qualified
   and thinking about running for political office, individuals scoring
   higher in narcissism are less likely to express interest in the specific
   work of political campaigning. The results have implications for
   understanding the traits that drive political ambition and how the body
   politic gets the politicians it needs, though possibly not the
   politicians it wants.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
