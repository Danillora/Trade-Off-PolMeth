@article{ WOS:000511427300016,
Author = {Martinez Gallego, Miguel Armando},
Title = {Life and vital value in Max Scheler and Hans Jonas},
Journal = {PENSAMIENTO AL MARGEN},
Year = {2019},
Number = {SI},
Pages = {271-291},
Abstract = {In his phenomenological ethics, Max Scheler conceptualized the essence
   of vital values in accordance with his understanding of the essence of
   life itself, in opposition to mechanism and vitalism. On the other hand,
   Hans Jonas also developed his environmental ethics of responsibility
   according to his own antimechanist ideas on the living being. We will
   present in detail Scheler's conceptions of the essence of life and vital
