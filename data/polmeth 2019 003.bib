@article{ WOS:000449337900002,
Author = {Farris, Massimiliano and Martinez Royo, Oscar},
Title = {The capitalism of the transnational holding companies in Chile's
   forestry sector: consolidation of a territorial hegemony},
Journal = {IZQUIERDAS},
Year = {2019},
Volume = {45},
Pages = {23-50},
Month = {FEB},
Abstract = {The forestry sector has determined the construction of the territory in
   the center-southern area of Chile. In order to understand such
   construction, the concept of hegemony, as well as its territorial realm
   have been used as analytical tools. The methodology considers
   territorial hegemony according to three dimensions:
   historical-political, economic-business and symbolic-cultural. It
   concluded that a hegemonic territoriality has been consolidated, and it
   is characterized by monoculture that is historically based on political
   practices that have favoured an oligopoly of transnational holdings.},
