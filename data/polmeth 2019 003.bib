@article{ WOS:000458680700009,
Author = {Knott, Eleanor},
Title = {Beyond the Field: Ethics after Fieldwork in Politically Dynamic Contexts},
Journal = {PERSPECTIVES ON POLITICS},
Year = {2019},
Volume = {17},
Number = {1},
Pages = {140-153},
Month = {MAR},
Abstract = {As researchers, when do our ethical obligations end? How should our
   ethical obligations respond to dynamic and unstable political contexts?
   Political scientists frequently work in dynamic political situations
   that can pose new ethical questions beyond those existing at the point
   of fieldwork. Yet, research ethics are often conceived in terms of a
   static, if not hermetically sealed, field site that remains frozen in
   time at the point we conduct fieldwork and collect data. I argue, first,
   that we need to consider more systematically how a dynamic field
   intersects with ethical obligations. Second, I argue that new and
   unexpected ethical questions can emerge after exiting the field,
   including responsibilities to research participants, dissemination, and
   publication, and returning to the field, which should be a part of how
   we conceive of ethical obligations.},
