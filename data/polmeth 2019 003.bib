@article{ WOS:000492781000007,
Author = {Blackledge, Paul},
Title = {Utopias: Future, Present and Concrete in Alasdair Maclntyre and C. L. R.
   James},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2019},
Volume = {9},
Number = {3},
Pages = {420-435},
Abstract = {Alasdair Maclntyre's Ethics in the Conflict of Modernity caps a long
   engagement with Marx and Marxism through a discussion of the life and
   work of C. L. R. James. Macintyre's critique of James is interesting
   both because James influenced Macintyre's early Marxism and because
   Maclntyre's mature critique of Marxism can be understood against the
   background of his failure to realise a broadly Jamesian political
   project in the 1960s. Moreover, Maclntyre's mature concept of a
   ``utopianism of the present{''} can, in part, be understood as an
   attempt to overcome the abstract utopian limitations of the politics he
   embraced in his youth. I argue that Maclntyre was right to reject the
   interpretation of Marxism he held in the mid-1960s, but that his general
   criticism of Marxism is less successful because Marxism, at its
   strongest, can be understood as a form of a utopianism of the present.
   The underlying flaw with the version of Marxism that Maclntyre embraced
   in the 1960s is not to be found in its abstractly utopian character but
   rather in its one-sided conception of the relationship between
   working-class self-activity and socialist consciousness. This is a
   problem, though inverted, that continues to haunt Maclntyre's
   interpretation of Marxism.},
