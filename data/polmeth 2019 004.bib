   Haro, Lia; Coles, Romand, Australian Catholic Univ, Inst Social Justice, Sydney, NSW, Australia.},
DOI = {10.1177/0090591719828725},
ISSN = {0090-5917},
EISSN = {1552-7476},
Keywords = {Sanctuary; Radical Democracy; Fugitive Democracy; Underground Railroad;
   Black Fugitive Thought; Social Transformation},
Research-Areas = {Government \& Law},
Web-of-Science-Categories  = {Political Science},
Author-Email = {liaharo@me.com},
