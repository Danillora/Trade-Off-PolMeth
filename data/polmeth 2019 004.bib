@article{ WOS:000470794900002,
Author = {Lantis, Jeffrey S.},
Title = {``Winning{''} and ``Losing{''} the Iran Nuclear Deal: How Advocacy
   Coalitions and Competition Shape U.S. Foreign Policy},
Journal = {POLITICS \& POLICY},
Year = {2019},
Volume = {47},
Number = {3},
Pages = {464-505},
Month = {JUN},
Abstract = {This study adapts the advocacy coalition framework (ACF) to help explain
   the puzzle of continuity and changes in U.S. nuclear diplomacy toward
   Iran. It shows how coalitions of governmental and nongovernmental actors
   promoted negotiation of the Joint Comprehensive Plan of Action to
   sharply curtail Iran's nuclear program in exchange for sanctions relief
   and engagement. This agreement remained in place from 2015 to 2018, even
   though President Trump and other opponents sought to dismantle the deal.
   This article demonstrates how competition and power balances shaped the
   foreign policy decision-making process over time. Evidence suggests that
   the ACF offers a compelling lens to explain continuity and change in
   foreign policy.},
