@article{ WOS:000489866800003,
Author = {Barth, Suzanne K. and Mittag, Nikolas and Park, Kyung H.},
Title = {Voter Response to Hispanic Sounding Names: Evidence from Down-Ballot
   Statewide Elections},
Journal = {QUARTERLY JOURNAL OF POLITICAL SCIENCE},
Year = {2019},
Volume = {14},
Number = {4},
Pages = {401-437},
Abstract = {The study of how voters respond to ethnic heuristics is complicated by
   the possibility that candidates differ along other dimensions that
   affect voter choice. This paper focuses on down-ballot statewide
   elections in which voters are plausibly ill-informed about candidates
   but can still infer race and ethnicity via the informational content in
   their names. Using nearly two decades of election results from the state
   of Texas, we find evidence of voters switching party support when their
   party's candidate has a distinctively Hispanic name. This result is more
   pronounced in counties that are expected to have higher levels of racial
   animosity. These findings are important since holding lower statewide
   office is a valuable stepping stone for minority politicians who aspire
   to higher office.},
