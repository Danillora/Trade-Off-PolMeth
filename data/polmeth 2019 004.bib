   Pierce, Douglas R., Calif Polytech Univ, Dept Polit Sci, Long Beach, CA 90802 USA.
   Lau, Richard R., Rutgers State Univ, Dept Polit Sci, 89 George St, New Brunswick, NJ 08901 USA.},
DOI = {10.1016/j.electstud.2019.102048},
Article-Number = {102048},
