@article{ WOS:000465932900001,
Author = {Hoffmann, Hanna and Springer, Frederik},
Title = {The Individual-Level Determinants of German Party Membership},
Journal = {GERMAN POLITICS},
Year = {2019},
Volume = {28},
Number = {2, SI},
Pages = {242-261},
Month = {APR 3},
Abstract = {Why do citizens join German parties? Do specific attributes and
   abilities play a determinant role in participation in political parties?
   The German Party Membership Study of 2009 enables us to answer these
   questions. On the basis of the telephone survey, we will address these
   issues by way of a systematic comparison of current party members with
   fellow citizens who never joined a party. For the purpose of analysing
   the individual-level determinants of joining a party, we use fundamental
   explanatory approaches to political participation: The socioeconomic
   standard model, the social-psychological approach, and the general
   incentives model. The results of our analyses clearly show that
   social-psychological attributes best explain the decision to join a
   party. Nevertheless, the findings for the determinants in both the
   socioeconomic standard model and the general incentives model complete
   the picture of citizens who are party members.},
