@article{ WOS:000461651400005,
Author = {McKee, Seth C. and Smith, Daniel A. and Hood, III, M. V. (Trey)},
Title = {The Comeback Kid: Donald Trump on Election Day in 2016},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2019},
Volume = {52},
Number = {2},
Pages = {239-242},
Month = {APR},
Abstract = {The surprise outcome of the 2016 presidential election continues to
   raise more questions as experts grapple with the evidence for why most
   prognosticators considered a Hillary Clinton victory almost certain.
   This article uses the 2016 Cooperative Congressional Election Study data
   to show that a primary explanation for why the election of Donald Trump
   was difficult to predict is that the bulk of his support did not
   materialize until Election Day, in the battleground states that he had
   to carry to win the Electoral College.},
