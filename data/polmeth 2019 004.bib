@article{ WOS:000488109900002,
Author = {Robinson, Nick},
Title = {Military Videogames More Than a Game},
Journal = {RUSI JOURNAL},
Year = {2019},
Volume = {164},
Number = {4},
Pages = {10-21},
Abstract = {This article demonstrates the significance of military videogames,
   exploring the changes in representations (how war is depicted) and
   production processes (the links between the military and videogames
   industries) that have occurred from the 1990s to the present. It argues
   that representations have moved from a focus on the 9/11 wars to
   depicting conflicts set in the past and/or future, so depoliticising
   popular culture. In terms of production, there have been corresponding
   changes, with a loosening of the historic links between the military and
   videogames industry.},
