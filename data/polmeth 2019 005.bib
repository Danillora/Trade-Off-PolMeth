   Walton, Grant W., Australian Natl Univ, Crawford Sch Publ Policy, Dev Policy Ctr, JG Crawford Bldg,Bld 132, Canberra, ACT 2601, Australia.},
DOI = {10.1080/10361146.2019.1616077},
EarlyAccessDate = {MAY 2019},
ISSN = {1036-1146},
EISSN = {1363-030X},
Keywords = {Quality of government; Papua New Guinea; corruption; anti-corruption;
