@article{ WOS:000477292800001,
Author = {Slapin, Jonathan B. and Kirkland, Justin H.},
Title = {The Sound of Rebellion: Voting Dissent and Legislative Speech in the UK
   House of Commons},
Journal = {LEGISLATIVE STUDIES QUARTERLY},
Year = {2020},
Volume = {45},
Number = {2},
Pages = {153-176},
Month = {MAY},
Abstract = {Using data on the content of debate associated with votes in the UK
   House of Commons from 1992-2015, this article examines how government
   party MPs employ language in legislative speech when they vote against
   the party line. We find a robust statistical association between dissent
   on votes and the use of first-person pronouns, simpler language, and
   giving longer speeches. Using a random forest algorithm for
   classification, we find that these language covariates are predictive of
   rebellion. The use of simpler, first-person language has implications
   for political representation and offers new insight into how MPs use
   votes to distinguish themselves from their party, perhaps reflecting
   attempts to connect with constituents.},
