@article{ WOS:000471947400006,
Author = {Hardardottir, Sigrun and Juliusdottir, Sigrun},
Title = {Public policy, the schoolsystem and teachers' role: Pathways of
   professional reforms for the benefit of pupils},
Journal = {ICELANDIC REVIEW OF POLITICS \& ADMINISTRATION},
Year = {2019},
Volume = {15},
Number = {1},
Pages = {113-134},
Month = {SPR},
Abstract = {The article focuses on public school policy in Iceland, in a historical
   perspective, the teacher' s role and contemporary challenges in the
   school system. Attention is paid to the increased complexity of
   teachers' role as educators and that of guarding the welfare and
   well-being of the individual schoolchild. The ongoing changes in
   teachers' role are related to changed social and family conditions
   coinciding with development in society and (human service) welfare
   institutions. This may especially regard school children when confronted
   by crises in their parents' lives or other unexpected, harmful
   situations in their personal life. Results from Icelandic research on
   current socio-educational conditions of schoolchildren and their
   families are presented. Increasing cooperation between school, health
   and social system with an interdisciplinary approach is advocated.
   Referring to present knowledge and call for revised legislation and
   policy issues some ideas of renewal of service are presented and
   discussed.},
