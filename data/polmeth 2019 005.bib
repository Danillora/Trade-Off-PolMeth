@article{ WOS:000619153200005,
Author = {Neill, Jeremy},
Title = {Fact of the matter: Rawls, political ideals, and worldview consensus},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2019},
Volume = {22},
Number = {6},
Pages = {725-746},
Abstract = {In this article, I argue that the fact of reasonable pluralism (FRP) - a
   famous Rawlsian assumption about the intellectual demographics of
   liberal democracies - is not as self-evident as is sometimes thought.
   The problem with the FRP is that in Political Liberalism Rawls is
   treating the freedoms and burdens story as being sufficient - in itself
   - to explain the demographics of reasonable pluralism. The inadequacy of
   the freedoms and burdens story is an indication that the FRP is
   empirically underdetermined as a way of glossing the belief demographics
   of liberal democracies. And it is a reason to wonder whether pluralism
   is as permanent as Rawls thinks. But worldview disagreements are not
   necessarily so incorrigible. In response to the FRP, it can be asserted
   that there is nothing that is inherent in the idea of a liberal
   democracy to preclude the choice by the citizens of a common worldview.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
