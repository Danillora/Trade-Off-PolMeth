@article{ WOS:000482392600007,
Author = {Marasco, Robyn},
Title = {Althusser's Gramscian Debt: On Reading Out Loud},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2019},
Volume = {31},
Number = {3, SI},
Pages = {340-362},
Abstract = {This essay measures Louis Althusser's debt to Antonio Gramsci in light
   of his general theory of reading and his reflections on the science,
   philosophy, and politics of Marxism. It considers their respective
   approaches to reading, highlighting the differences between a
   ``philosophical reading{''} in Althusser's sense and a ``political
   reading{''} in the spirit of Gramsci. Further, it considers how some of
   Althusser's key concepts-ideology, the state, conjuncture, utopian
   realism-are elaborated through a critical engagement with Gramsci's
   political thought.},
