@article{ WOS:000482950300005,
Author = {van Munster, Rens and Sylvest, Casper},
Title = {Appetite for destruction: Gunther Anders and the metabolism of nuclear
   techno-politics},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
