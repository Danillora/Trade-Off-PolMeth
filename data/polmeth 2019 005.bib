@article{ WOS:000469006800007,
Author = {Rosoux, Valerie},
Title = {The Administration of Post-War Memory in the Franco-German and
   Franco-Algerian Negotiations},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
