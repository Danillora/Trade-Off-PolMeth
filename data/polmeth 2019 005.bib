@article{ WOS:000476739200001,
Author = {Cortina, Jeronimo and Rottinghaus, Brandon},
Title = {Vote centers and turnout by election type in Texas},
Journal = {RESEARCH \& POLITICS},
Year = {2019},
Volume = {6},
Number = {3},
Pages = {1-7},
Month = {JUL},
Abstract = {The use of vote centers-specific locations in a county where all voters
   will vote-is on the rise nationwide, as more than a dozen states used
   this process by 2018. More states are moving toward using voting centers
   to remedy the problem of low voter turnout, with the assumption that the
   centralization of voting to several core county locations will increase
   voter accessibility. What we have less clear information about is the
   effect of vote centers on turnout in individual elections across several
   cycles. Using a natural experiment in Texas-a state that has three fixed
   election cycles-we find vote centers have a small positive impact on
   traditionally lower turnout elections but no effect on higher turnout
   elections. The cumulative impact of vote centers has a small effect on
   turnout over time. These results suggest a more cautious assessment is
   needed when considering the use and impact of vote centers.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Rottinghaus, B (Corresponding Author), Univ Houston, 447 Hoffman Hall, Houston, TX 77204 USA.
   Cortina, Jeronimo; Rottinghaus, Brandon, Univ Houston, 447 Hoffman Hall, Houston, TX 77204 USA.},
DOI = {10.1177/2053168019864224},
Article-Number = {2053168019864224},
