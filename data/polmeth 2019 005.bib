@article{ WOS:000469827800005,
Author = {Ceva, Emanuela},
Title = {Interactive justice: an introduction},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2019},
Volume = {22},
Number = {4},
Pages = {454-458},
Abstract = {This piece provides an illustration of the main traits of the
   interactive dimension of justice; this is the idea that the terms of
   some human forms of interactions may be inherently valuable as they
   instantiate the just treatment owed to persons in virtue of their moral
   status as claim-makers. Interactive justice is presented as the guiding
   idea for developing just institutional responses to the presence of
   value conflict in politics because it grounds the transformation of
   antagonistic conflict dynamics into cooperative terms. This idea thus
   offers the normative foundation for a normative theory of conflict
   management as an integration to the existing (and seemingly
   irreconcilable) theories of conflict resolution (grounded in the idea of
   end-state justice) and conflict containment (animated by a commitment to
   peace).},
