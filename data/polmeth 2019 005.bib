@article{ WOS:000495438600003,
Author = {Harunoglu, Nur Cetinoglu},
Title = {Understanding The US' Approach To The Gulf Monarchies From A Historical
   Perspective: Nixon's Period},
Journal = {SIYASAL-JOURNAL OF POLITICAL SCIENCES},
