@article{ WOS:000464840400003,
Author = {Turner, Ben},
Title = {From resistance to invention in the politics of the impossible: Bernard
   Stiegler's political reading of Maurice Blanchot},
Journal = {CONTEMPORARY POLITICAL THEORY},
Year = {2019},
Volume = {18},
Number = {1},
Pages = {43-64},
Month = {MAR},
Abstract = {In Bernard Stiegler's Automatic Society Volume 1: The Future of Work,
   `the impossible' and `the improbable' appear as explicit parts of his
   political project. In his philosophy of technology, the impossible
   highlights the structural incompleteness that technics imparts to human
   existence. This article will trace how Stiegler draws on the work of
   Maurice Blanchot to produce this conjunction between technics and
   indetermination, and explore its political ramifications. This will show
   that rather than being a recent aspect of Stiegler's work, the political
   use of the impossible brings Blanchot's subterranean influence to the
   fore. After briefly reconstructing Blanchot's understanding of language,
   impersonality, and writing, it will be shown that Stiegler shifts
   Blanchot's emphasis from the impossible to technical practices of the
   improbable. Stiegler follows an anthropological line where the
   experience of the impossible becomes a more general understanding of the
   experience of improbability through technical structures. Stiegler's
   reading of Blanchot is central in his break with the place of the
   impossible in the politics of resistance advocated by
   post-structuralism, allowing him to support a politics of invention
   which makes space for the improbable within a large-scale politics of
   technicity, rather than privileging indeterminacy itself.},
