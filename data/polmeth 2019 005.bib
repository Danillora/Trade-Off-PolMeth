@article{ WOS:000496521100004,
Author = {Schildkraut, Deborah J.},
Title = {The Political Meaning of Whiteness for Liberals and Conservatives},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
