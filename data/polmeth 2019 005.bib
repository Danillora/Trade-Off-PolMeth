Affiliation = {Marsteintredet, L (Corresponding Author), Univ Bergen, Dept Polit Comparada, Estudios, Noruega Christies Gate 15, N-5007 Bergen, Norway.
   Marsteintredet, Leiv, Univ Bergen, Dept Polit Comparada, Estudios, Noruega Christies Gate 15, N-5007 Bergen, Norway.},
ISSN = {1665-2037},
Keywords = {vice presidency; presidentialism; rules of succession; instability;
