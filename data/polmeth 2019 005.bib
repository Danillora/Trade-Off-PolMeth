@article{ WOS:000584408300001,
Author = {Wong, Mathew Y. H.},
Title = {Democratization as institutional change: Hong Kong 1992-2015},
Journal = {ASIAN JOURNAL OF COMPARATIVE POLITICS},
Year = {2021},
Volume = {6},
Number = {1},
Pages = {92-106},
Month = {MAR},
Abstract = {The limited democratization in Hong Kong has been on-going for more than
   three decades. Unlike typical democratizations as the abrupt breakdown
   of authoritarian regimes, the case of Hong Kong is characterized by
   gradual and incremental changes under a constitutional framework. The
   process, involving actors such as the Chinese government, colonial
   government, and local democratic forces, provides a wide variety of
   forms of institutional changes. By utilizing concepts from the
   literature on institutional change, namely path dependence, layering,
   and conversion, important junctures of democratization in Hong Kong are
   analyzed. It is found that the case can be suitably explained by the
   framework of institutional change. This research carries implications
   for the study of central-regional interactions, democratization, and
   Hong Kong politics.},
