@incollection{ WOS:000813444900010,
Author = {Buzogany, Aron and Frankenberger, Rolf and Graf, Patricia},
Editor = {Wiesner, C and Harfst, P},
Title = {And yet they regulate Policy-Making, Performance and Legitimation in
   Autocracies},
Booktitle = {LEGITIMITAT UND LEGITIMATION: VERGLEICHENDE PERSPEKTIVEN},
Series = {Vergleichende Politikwissenschaft},
Year = {2019},
Pages = {277-314},
Abstract = {In addition to repression and co-optation, authoritarian regimes also
   rely on redistributive-economic and legitimatory-ideological strategies
   to ensure their survival. In this article, we first discuss conceptually
   different possibilities of legitimation through policy-making that
   (also) come into question in autocracies. We illustrate this
   conceptualization by presenting examples of autocratic legitimation
   strategies in distributive, redistributive and regulatory policy fields.
   We show that legitimation through policy-making is a double-edged sword,
   as it harbors legitimation potentials as well as risks. The comparison
   of innovation, environmental and family policies shows that distributive
   policies are more likely to have a legitimizing effect, while regulatory
   and redistributive policies are at best ambivalent due to their
   conflictive character.},
