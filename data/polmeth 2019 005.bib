@article{ WOS:000459403400004,
Author = {Graham, Jeremy C. and Mueller, Danielle},
Title = {Military Expenditures and Income Inequality among a Panel of OECD
   Countries in the Post-Cold War Era, 1990-2007},
Journal = {PEACE ECONOMICS PEACE SCIENCE AND PUBLIC POLICY},
Year = {2019},
Volume = {25},
Number = {1},
Month = {FEB},
Abstract = {Does military spending exacerbate income inequality? After the Cold War,
   many developed countries sought to reduce military expenditures in the
   face of a new security environment without the clear and present threat
   of large-scale international conflict. The literature has presented
   mixed evidence on the economic effects of military spending. Moreover,
   during this era analysts in the OECD have become preoccupied with the
   economic indicator of income equality. Our study examines the
   relationship between military expenditures and income inequality.
   Complementing the established literature on the subject, we find that
   these two phenomena indeed possess a positive relationship and it is
   unlikely that this association is due to random chance. Our results are
   robust to the inclusion of control variables common in the literature.
   These findings lead us to contemplate the historical and theoretical
   account of the peace dividend narrative.},
