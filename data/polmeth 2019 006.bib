@article{ WOS:000508668000003,
Author = {Ramon Solano, Wilson Andres and Cun Cornejo, Violeta de Jesus and Jumbo
   Balcazar, Germania Lorena and Ramon Guaman, Carla Dayana},
Title = {Minds found in the Ecuadorian conflict since the critical reading of
   some written press stories},
Journal = {CUESTIONES POLITICAS},
