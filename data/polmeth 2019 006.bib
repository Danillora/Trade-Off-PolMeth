@article{ WOS:000502845200005,
Author = {Lafuente-Funes, Sara},
Title = {Assisted Reproduction in the Spanish Context: Egg Donation as the Engine
   of a Heteronormative Business Model},
Journal = {POLITICA Y SOCIEDAD},
Year = {2019},
Volume = {56},
Number = {3},
Pages = {645-667},
Abstract = {Assisted reproduction has transformed the way an increasing amount of
   people reproduce, along with social imaginaries around how reproduction
   takes place. It has also made possible to medicalize and commercialize
   reproduction in a completely new manner. Within Spain, the private
   sector is the leading one, even though there is extensive coverage in
   terms of techniques within the Public Sector (though long waiting lists,
   age and heteronormative access limitations do exist). This paper
   discusses the expansion of assisted reproduction in Spain unifying
   social and medical literature, reviewing data from the Spanish Fertility
   Society (SEF) and presenting some qualitative findings that examine the
   role of egg donation within clinics through interviewing professionals
   from them. Through analyzing this qualitative data, we show how these
   techniques are entangled in the reproduction of a heteronormative statu
   quo involved in naturalising and normalizing intra family gender roles
   both at the personal and cell levels.},
