@article{ WOS:000591875800004,
Author = {Manchio, Corinne},
Title = {Machiavelli and the forms of political advice facing disaster},
Journal = {LABORATOIRE ITALIEN-POLITIQUE ET SOCIETE},
Year = {2019},
Number = {23},
Abstract = {We question the status of secretaries during the Italian wars by
   analyzing expressions of political advice in the diplomatic and
   administrative correspondence of Machiavelli (1498-1512). We conduct a
   semantical analysis of the uses of ``consiglio{''} with the textual
   analysis tool Machiato. From the records of occurrences, we present our
   results in three stages: the problem of advising facing an unprecedented
   situation (omnipresence of the danger and choral practice of the
   advice), the affirmation of an advice deeply rooted in realism
   (necessity and opportunity) and finally, the tension between the
   practical vocation and the theoretical aspiration of the advice (the
