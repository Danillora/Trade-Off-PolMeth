@article{ WOS:000470016700003,
Author = {Regan, Joshua},
Title = {The piracy terrorism paradigm: an interlinking relationship},
Journal = {BEHAVIORAL SCIENCES OF TERRORISM AND POLITICAL AGGRESSION},
Year = {2019},
Volume = {11},
Number = {2},
Pages = {149-157},
Abstract = {Non-state actors (i.e. pirates and terrorists) are an international
   security concern. Prior studies have examined the political and economic
   impact that pirates and terrorists inflict on our society. Few studies,
   however, have attempted to answer whether there is an association
   between them. The purpose of this study is to provide empirical support
   on the relationship between pirates and terrorists. Longitudinal data
   collected from the National Geospatial Intelligence Agency (NGIA) and
   the Global Terrorism Database (GTD) provided the researcher with
   incidents of maritime piracy and terrorism 1985-2014 in three countries:
   Indonesia, Nigeria, and Somalia. A Pearson's R bivariate statistical
   tool was selected to determine if these incidents co-vary. The model
   summaries illustrate that acts of piracy and terrorism positively
   co-varied, with two models inferring statistical significance. Policy
   implications and recommendations will be discussed. These
   recommendations include increasing the number of naval patrols and
   strengthening local institutions.},
