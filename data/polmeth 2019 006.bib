@article{ WOS:000456273500025,
Author = {Fung, Courtney J.},
Title = {Negotiating the Nuclear and Humanitarian Crisis on the Korean Peninsula:
   A Simulation and Teaching Guide},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2019},
Volume = {52},
Number = {1},
Pages = {113-116},
Month = {JAN},
Abstract = {This article describes a simulation scenario based on of-the-minute
   thinking about the Korean Peninsula crisis. The scenario highlights the
   tradeoffs and difficulties in addressing the nuclear and humanitarian
   crisis, tasking students to negotiate to reach consensus on track I and
   track II levels. Students are negotiators, gaining experience and
   exposure to key international relations and political science concepts
   through active learning. An optional media-teams and press-conference
   component also is discussed. The scenario, grading rubric, and
   supplemental materials are included to give instructors a resource that
   is easily modified across groups varying in size, ability, and
   composition.},
