@article{ WOS:000504053600002,
Author = {Harris, Bede},
Title = {Do the Queensland Regulations Governing the Refund of Motor Vehicle
   Registration Fees Breach s 92 of the Commonwealth Constitution?},
Journal = {JOURNAL OF POLITICS AND LAW},
Year = {2019},
Volume = {12},
Number = {4},
Pages = {8-14},
Month = {DEC},
Abstract = {The federal system in Australia imposes inconvenience on people. One of
   these inconveniences arises from the fact that each State and Territory
   has its own vehicle registration regime. In contrast to other States in
   Australia, purchasers of vehicles from outside Queensland suffer from a
   particular burden in that they are denied a refund of the unexpired
   portion of vehicle registration fees. In this article it is argued that
   that denial is unlawful both as a matter of the interpretation of the
   applicable Regulations and because it infringes s 92 of the Commonwealth
   Constitution, which prohibits the imposition of protectionist burdens on
   interstate trade and commerce.},
