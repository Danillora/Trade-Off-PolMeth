@article{ WOS:000462638900008,
Author = {Fitch-Roy, Oscar and Benson, David and Woodman, Bridget},
Title = {Policy Instrument Supply and Demand: How the Renewable Electricity
   Auction Took over the World},
Journal = {POLITICS AND GOVERNANCE},
Year = {2019},
Volume = {7},
Number = {1},
Pages = {81-91},
Abstract = {The selection and design of renewable electricity support instruments is
   an important part of European Union (EU) energy policy and central to
   the governance of the Energy Union. In 2014, the European Commission
   published updated guidelines for state aid that are driving the EU-wide
   implementation of auctions for allocating revenue support to commercial
   scale renewable electricity generation. This article argues that the RES
   auction's rapid ascent towards dominance is explained by a coincidence
   of an activist interpretation of EU state aid law creating demand for
   knowledge about the instrument and the emergence of a ready source of
   supply from a burgeoning community of a RES auction specialists and
   experts. Knowledge gained through EU-wide implementation of auctions
   further adds to supply of auctions expertise among the community. The
   implications of positive feedback between instrument demand and the
   growing supply of knowledge about an instrument reinforces the
   importance of critical engagement between policymakers and policy
   experts.},
