@article{ WOS:000455830000008,
Author = {Lorey, Isabel},
Title = {Constituent power of the multitude},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
Year = {2019},
Volume = {15},
Number = {1, SI},
Pages = {119-133},
Month = {FEB},
Abstract = {How might we understand a constituent power that is thought not as
   constitutively juridical but nevertheless as a political power, one that
   issues from the heterogeneous multitude and develops its collective
   potentia not by its number but rather in its polyvocality? Such an
   alternative conception would need to leave gendered connotations, the
   idea of a masculinist bourgeois subject, and questions of belonging
   behind. It would need to issue radically from the mutual connectedness
   of singularities and, against the backdrop of queer-feminist and
   postcolonial considerations, place the necessity of care and
   reproduction at its center. Initial prompts for thinking a constituent
   power that is not constitutively juridical can be found in the work of
   Antonio Negri. The article further develops Negri's approach and
   connects it with the municipalist movements in Spain as examples of new
   forms of constituent and constituted power.},
