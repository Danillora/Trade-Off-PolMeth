@article{ WOS:000512950100004,
Author = {Novo Arbona, Ainhoa and Perez Castanos, Sergio and Garcia Rabadan,
   Jonatan},
Title = {Building a federal state: phases and moments of Spanish regional
   (de)centralization},
Journal = {ITALIAN POLITICAL SCIENCE REVIEW-RIVISTA ITALIANA DI SCIENZA POLITICA},
Year = {2019},
Volume = {49},
Number = {3},
Pages = {263-277},
Month = {NOV},
Abstract = {Two main elements characterize a country as federal and the development
   of its federal system. The first one is directly connected to
   competences and how regions may or may not have powers over public
   policies. The second one focuses on the fiscal arena, and how regions
   generate their own income and share it with the central government. This
   paper describes from a historical point of view the different phases
   that the Spanish federal process has followed since its beginning in
   1978 up to the last reform in 2016. These phases are related to the
   composition of both the regional and the national governments. It is
   therefore is important to investigate the connection between changes in
   these compositions and how they may have affected the phases of
   decentralization. Employing the new institutionalism paradigm and
   seeking for critical junctures in the different moments, we show how
   national governments are more important in shaping this process than
   regional ones.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {32 AVENUE OF THE AMERICAS, NEW YORK, NY 10013-2473 USA},
Type = {Article},
Language = {English},
Affiliation = {Castaños, SP (Corresponding Author), Univ Burgos, Dept Private Law, Burgos, Spain.
   Novo Arbona, Ainhoa; Perez Castanos, Sergio; Garcia Rabadan, Jonatan, Univ Burgos, Dept Private Law, Burgos, Spain.},
DOI = {10.1017/ipo.2018.23},
ISSN = {0048-8402},
EISSN = {2057-4908},
Keywords = {fiscal federalism; political decentralization; regions; federalism;
   decentralization; Spain},
Keywords-Plus = {DECENTRALIZATION},
