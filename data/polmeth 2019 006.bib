@incollection{ WOS:000813444900007,
Author = {Wagner, Aiko and Giebler, Heiko and Lewandowsky, Marcel},
Editor = {Wiesner, C and Harfst, P},
Title = {Legitimate Motives? The AfD Choice as an Articulation of By-Election
   Behavior, System Criticism or Factual Issue Preferences in the 2014
   European Election},
Booktitle = {LEGITIMITAT UND LEGITIMATION: VERGLEICHENDE PERSPEKTIVEN},
Series = {Vergleichende Politikwissenschaft},
Year = {2019},
Pages = {171-202},
Abstract = {This paper addresses the question of which individual motives favored
   the Alternative for Germany (AfD) in the 2014 European elections. We
   distinguish between secondary voting behavior, systemic criticism of the
   European Union, and general issue preferences. In doing so, we argue
   that the first two motives question the legitimacy of the EU to which
   the electoral act refers, either passively (as protest voting behavior)
   or actively (as the expression of fundamental criticism of the system
   through the electoral act). Empirically, we show on the basis of
   pre-election surveys in the German Longitudinal Election Study (GLES)
   that support for the AfD in the 2014 European election was primarily
   motivated by factual issue positions. Thus, the electoral motives cannot
   be explained primarily by push factors - and thus classified as protest
   - but by pull factors: the election of the AfD thus does not necessarily
   express a questioning of the European Union, but can, like the election
   of other parties, be explained by factual orientations.},
