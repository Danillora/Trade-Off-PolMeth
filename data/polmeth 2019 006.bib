@article{ WOS:000493050800003,
Author = {Parsons, Alex and Rumbul, Rebecca},
Title = {Did They Write Back? A Mandate Divide in Response to Constituent
   Casework in Devolved Bodies},
Journal = {PARLIAMENTARY AFFAIRS},
Year = {2019},
Volume = {72},
Number = {3},
Pages = {522-541},
Month = {JUL},
Abstract = {Three devolved bodies in the UK (the Scottish Parliament, the National
   Assembly for Wales and the London Assembly) use Multi Member
   Proportional Representation electoral systems. These bodies have two
   groups of representatives, regional and constituency, elected through
   different methods. Using a survey of members of the public who contacted
   their representative, constituency representatives are shown to have a
   higher response rate across all three bodies, and this effect is
   isolatable from the role of being a government party. In Scotland, this
   effect is demonstrable when comparing the behaviour of the same people
   holding different roles, suggesting that individuals adapt their
   behaviour to the expectations of the role.},
