@article{ WOS:000482950300006,
Author = {Craig, Campbell},
Title = {Solving the nuclear dilemma: Is a world state necessary?},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
Year = {2019},
Volume = {15},
Number = {3, SI},
Pages = {349-366},
Month = {OCT},
Abstract = {The unique dangers raised by the possibility of nuclear warfare have
   long prompted intensive debates about what political action is needed to
   avoid it. While most scholars contend that it is possible to prevent a
   nuclear war without fundamental political change, others argue that a
   substantial solution to the problem demands the abolition of the
   existing interstate system. Two such `radical' positions are the
   `Weberian' school, which insists that an authoritative world state is
   necessary, and Daniel Deudney's alternative, a liberal order based upon
   republican traditions of mutual restraint, internal power balancing and
   powerful arms control institutions. In this essay, I argue, using both
   historical and theoretical analysis, that the regime Deudney envisions
   would amount to the establishment of a Pax Americana. This would be
   rejected by illiberal nuclear powers and therefore fail to solve the
   nuclear problem.},
Publisher = {SAGE PUBLICATIONS LTD},
Address = {1 OLIVERS YARD, 55 CITY ROAD, LONDON EC1Y 1SP, ENGLAND},
Type = {Article},
