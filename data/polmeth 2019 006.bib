@incollection{ WOS:000813444900005,
Author = {Pickel, Gert},
Editor = {Wiesner, C and Harfst, P},
Title = {On the Way to Postdemocracy? The empirical Legitimacy of Democracies in
   comparative Perspective},
Booktitle = {LEGITIMITAT UND LEGITIMATION: VERGLEICHENDE PERSPEKTIVEN},
Series = {Vergleichende Politikwissenschaft},
Year = {2019},
Pages = {97-137},
Abstract = {Legitimacy is a central resource for democracies. But it is also a
   central resource for all political systems, including autocracies.
   Democratic systems require the acceptance of the normative concept of
   democracy by citizens, i.e. empirical legitimacy. This is based on
   partially varying conceptions of democracy. Nevertheless, the
   conceptions of these concepts are often very similar, and the aspect of
   individual freedom is in the foreground. Empirical analyses show this
   not only for Europe. In view of the available data, it is not possible
   to speak of a crisis of legitimacy or disenchantment with politics in
   Europe at present. However, there is widespread disenchantment with
   politicians and political parties. Nevertheless, there are dangers for
   the current democracies. For example, in addition to economic
   ineffectiveness in the eyes of citizens, perceived threats to their
   collective identity and their own social situation have an unfavorable
   effect on system support for the current political system. And this can
   be dangerous enough, for quite a few citizens democracy with one or the
   other authoritarian admixture seems acceptable on the grounds of system
   effectiveness.},
