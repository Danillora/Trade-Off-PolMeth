@article{ WOS:000474757100004,
Author = {McKay, Amy Melissa and Webb, Brian},
Title = {Presidential Position-Taking, Presidential Success, and Interest Group
   Activity},
Journal = {CONGRESS \& THE PRESIDENCY-A JOURNAL OF CAPITAL STUDIES},
Year = {2019},
Volume = {46},
Number = {1},
Pages = {89-108},
Abstract = {Do lobby groups help the American president achieve policy objectives?
   Existing research seldom evaluates interest groups and the president in
   conjunction, and as a result we have little systematic knowledge about
   how groups respond to presidential actions or whether they assist in
   realizing the president's policy agenda. Building on existing data
   obtained through interviews with 776 lobbyists, combined with variables
   we generate describing issue salience, congressional attention, the
   political context, and policy adoption, we show that interest groups
   adjusted their lobbying activity to better reflect the president's
   voiced preferences. Despite this strategy, we find that lobby groups had
   no significant marginal effect on policy adoption when controlling for
   the overwhelming influence of the president. The strong association
   between policy adoption and position-taking by the president withstands
   the inclusion of five alternative variables found in previous studies to
   condition the influence of the president over policy adoption.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {McKay, AM (Corresponding Author), Univ Exeter, Polit Off, Amory Bldg, Exeter EX4 4QJ, Devon, England.
   McKay, Amy Melissa, Univ Exeter, Exeter, Devon, England.
