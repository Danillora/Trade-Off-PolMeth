@article{ WOS:000503426100001,
Author = {Larson, Ethan},
Title = {The new `old country' and its `tenth province': Yugoslav pavilions and
   the `Emigration Question'},
Journal = {NATIONAL IDENTITIES},
Year = {2020},
Volume = {22},
Number = {3},
Pages = {325-345},
Month = {MAY 26},
Abstract = {What can three exhibitions in the 1930s tell us about diaspora
   nationalism and Yugoslavia? During the interwar period, the newly-formed
   Kingdom of Yugoslavia struggled to win the loyalty of its overseas
   population, which it called its `tenth province', through symbolic
   displays at World Fairs and International expositions in the United
   States. Although initially emigrant activists controlled these
   exhibitions, over the decade the Yugoslav foreign service took control.
   By analyzing this power struggle, I unravel how both the homeland and
   the host country shape the expression and presentation of diaspora
   nationalism.},
