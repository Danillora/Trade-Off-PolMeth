@article{ WOS:000469942100007,
Author = {Fielder, James D.},
Title = {The Narrative History of the Chocolate Wars: A Short and Tasty
   Bargaining Game},
Journal = {JOURNAL OF POLITICAL SCIENCE EDUCATION},
Year = {2019},
Volume = {15},
Number = {1, SI},
Pages = {82-93},
Abstract = {This single lesson classroom game is designed to test student knowledge
   of Realist and Liberal concepts through an imbalanced resource
   bargaining mechanic. The game is designed for approximately 20 students
   divided into state teams of three to four students each and uses
   chocolate to represent the state teams' economic capacity, military
   capability, and human security. Each state team had competing goals,
   over which they must cooperate or conflict for additional pieces. This
   game also uses an abstract design to maximize active learning through a
   short course of play while also making the game useful for testing
   concepts at any point in an International Relations course. In addition
   to offering a complete yet easily modifiable game for classroom use,
   this article also describes the game's pedagogical contribution to the
   discipline, game design notes, and techniques for preventing students
   from stuffing their faces with game pieces.},
