@article{ WOS:000503415200010,
Author = {Marszalek-Kawa, Joanna and Dahl, Michal},
Title = {JAPAN SELF-DEFENCE FORCES - ORIGIN AND DEVELOPMENT OF THE INSTITUTION},
Journal = {EUROPEAN JOURNAL OF TRANSFORMATION STUDIES},
Year = {2019},
Volume = {7},
Number = {2},
Pages = {142-151},
Abstract = {The purpose of this paper is to discuss the origins and evolution of
   Self-Defence Forces as an institution within the Japanese constitutional
   system. The analysis also aims to provide answers to the question of the
   compatibility of Jietai with the Japanese Constitution and its formal
   status. In order to address the research problem, we have decided to
   dedicate the first part to issues related to the constitutional
   principles of pacifism and anti-militarism. It is followed by the
   description of the process that led to the formation of Japanese
   Self-Defence Forces. We decided that the turning point of the analysis
   is the appointment of Liberal Democratic Party's leader, Abe Shinzo, as
   the 96th Prime Minister of Japan (December 26, 2012). The reforms
   undertaken by the politician resulted in a rapid change in the character
   of Self-Defence Forces, putting an end to the hitherto functioning model
   of Jietai.},
