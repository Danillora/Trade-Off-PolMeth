@article{ WOS:000489307700007,
Author = {Brannum, Kate},
Title = {GUATEMALA 2018: FACING A CONSTITUTIONAL CROSSROAD},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2019},
