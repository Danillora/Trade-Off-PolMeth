@article{ WOS:000470315900008,
Author = {Stengel, Frank A.},
Title = {Securitization as Discursive (Re)Articulation: Explaining the Relative
   Effectiveness of Threat Construction},
Journal = {NEW POLITICAL SCIENCE},
Year = {2019},
Volume = {41},
Number = {2},
Pages = {294-312},
Abstract = {This article develops a poststructuralist framework for the analysis of
   the process of threat construction or securitization. Taking on-going
   debates in securitization theory about the securitizing process as a
   starting point, the article draws on the poststructuralist discourse
   theory of the Essex School to theorize what makes some securitizing
   moves (attempts to securitize a certain issue) more effective than
   others, which remains a persistent and crucial gap in the current
   literature.},
