@article{ WOS:000488831400002,
Author = {Yang, Chun Hee and Park, Hahnkyu},
Title = {Game of Patience: Is Complete Denuclearisation of North Korea Possible?},
Journal = {EAST ASIAN POLICY},
Year = {2019},
Volume = {11},
Number = {3},
Pages = {14-24},
Month = {JUL-SEP},
Abstract = {North Korea watchers on denuclearisation negotiations from Singapore to
   Hanoi have been brought on a roller coaster ride. Although the recent
   fiasco at Hanoi seemed to justify North Korean experts' predictions that
