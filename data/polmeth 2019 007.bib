@article{ WOS:000473526200001,
Author = {Jorba, Helga},
Title = {Arendt, Scholem and the response of the Jewish world to the problem of
   antisemitism},
Journal = {REVISTA DE ESTUDIOS POLITICOS},
Year = {2019},
Number = {183},
Pages = {13-65},
Month = {JAN-MAR},
Abstract = {In 1941 Schoken Books published Major Trends in Jewish Mysticism by
   Gershom Scholem, which included the lectures he delivered throughout
   1938 during his stay at the Jewish Institute in New York. These lectures
   unveiled some of the most relevant aspects of the evolution of Jewish
   mysticism. We know, through their correspondence, that Arendt was
   impressed by this book even though she wrote sparsely about the subject.
   In this article, we dig into the motives behind these interests and
