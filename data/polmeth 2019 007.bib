@incollection{ WOS:000813444900008,
Author = {Wiesner, Claudia},
Editor = {Wiesner, C and Harfst, P},
Title = {Legitimation of the EU in Discourse? A German-French Comparison},
Booktitle = {LEGITIMITAT UND LEGITIMATION: VERGLEICHENDE PERSPEKTIVEN},
Series = {Vergleichende Politikwissenschaft},
Year = {2019},
Pages = {203-238},
Abstract = {Legitimacy and legitimation are related to each other in political
   practice, but they are conceptually distinct from each other. While a
   judgment on the legitimacy of political systems and orders is to be
   understood as a normative or empirical-analytical assessment of an
   actual state, legitimation refers to the practices and processes in
   which legitimacy is obtained or with which it is generated. Legitimation
   processes aim at generating or influencing legitimacy beliefs for
   political systems. Language-based practices that ascribe meaning to the
   systems or institutions in question have a central function here. This
   chapter looks a) conceptually-theoretically and b) empirically and
   comparatively at a specific type of language-based legitimation
   processes, namely legitimation discourses on the EU. Two national
   discourses on Europe are examined comparatively with regard to their
   function for the construction of legitimation or delegitimation for the
   EU, namely the discourses in Germany and France around the ratification
   votes on the EU Constitutional Treaty in 2005.},
