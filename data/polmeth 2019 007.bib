@article{ WOS:000470200300004,
Author = {van Ree, Erik},
Title = {Marx and Engels's theory of history: making sense of the race factor},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2019},
Volume = {24},
Number = {1},
Pages = {54-73},
Abstract = {This article argues that Karl Marx and Friedrich Engels's theory of
   history contained racist components. In Marx and Engels's understanding,
   racial disparities emerged under the influence of shared natural and
   social conditions hardening into heredity and of the mixing of blood.
   They racialized skin-colour groups, ethnicities, nations and social
   classes, while endowing them with innate superior and inferior character
   traits. They regarded race as part of humanity's natural conditions,
   upon which the production system rested. `Races' endowed with superior
   qualities would boost economic development and productivity, while the
   less endowed ones would hold humanity back. Marxist race thinking
   reflected common Lamarckian and Romantic-Nationalist assumptions of the
   era.},
