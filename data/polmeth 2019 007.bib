@article{ WOS:000467079400002,
Author = {Misseri, Lucas E.},
Title = {The Utopia of an Open Borders World: A Conditional Cosmopolitan Argument
   in its Favour},
Journal = {ANACRONISMO E IRRUPCION},
Year = {2019},
Volume = {9},
Number = {16},
Pages = {40-60},
Month = {MAY-OCT},
Abstract = {In this paper the subject-matter of the openness of national borders is
   approached, especially concerning to migrants from worst-off states.
   After reviewing some communitarian and liberal arguments against and in
   favour of the open borders - especially C. H. Wellman's position- a
   ``cosmopolitan conditional{''} argument is defended. ``Cosmopolitan{''}
   because the guiding idea of the equality of all human beings is
   accepted, independently of accidental facts as the place of birth.
   ``Conditional{''} because it is held the condition that there is a moral
   obligation to open the border to migrants if these lack a minimum of
   sufficient resources to live a life worth living.},
Publisher = {UNIV BUENOS AIRES},
