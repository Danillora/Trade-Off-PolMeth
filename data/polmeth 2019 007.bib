@article{ WOS:000456962900004,
Author = {Jenkins, David},
Title = {James Baldwin and Recognition},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2019},
Volume = {8},
Number = {1},
Pages = {82-107},
Month = {JAN 1},
Abstract = {As a novelist, playwright, and essayist James Baldwin is recognized as
   an important figure in American letters. In this article, I make the
   case for Baldwin as a resource for those wishing to develop the themes,
   concepts, and ontologies underlying philosophies of recognition, within
   the contemporary American context. I argue that because he places
   injustice and failures of recognition at the heart of American
   lives-both public and private-Baldwin enables theorists to acknowledge,
   first, the important functions performed by misrecognition, second, the
   work that needs to be done for Americans to achieve some kind of ethical
   minimum, and finally, the risks and dangers attendant to failing to take
   this work seriously.},
