@article{ WOS:000470147000002,
Author = {Melendez, Jose W. and Martinez-Cosio, Maria},
Title = {Designing for equitable civic engagement: Participatory design and
   discourse in contested spaces},
Journal = {JOURNAL OF CIVIL SOCIETY},
Year = {2019},
Volume = {15},
Number = {1},
Pages = {18-41},
Abstract = {A common theme of research on participatory democracy and citizen
   engagement centres on the need for empirical studies that conduct a deep
   exploration of the nuances and complexities of these processes. This
   article offers a distinct response to this need through an examination
   of two ethnographic case studies of a participatory budgeting process in
   a multilingual, highly diverse community in which Spanish-speaking
   Latino immigrants were involved. By analyzing participants discourse
   from the case studies, the findings highlight the risks and rewards of
   reframing participatory processes as design decisions rather than static
   procedures.},
