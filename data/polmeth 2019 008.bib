@article{ WOS:000470213700002,
Author = {Dai, Qian and Chu, Rong-Xuan},
Title = {National identification and intergroup attitudes of Chinese youth
   towards Americans, Japanese, and South Koreans},
Journal = {NATIONAL IDENTITIES},
Year = {2019},
Volume = {21},
Number = {1},
Pages = {21-38},
Abstract = {The study explored the strength of national identification and
   intergroup attitudes of Chinese youth (n = 591, aged 12-15) toward the
   Chinese, the Americans, the Japanese, and the South Koreans. The
   participants were asked to fill in a questionnaire and write down
   reasons why they `like' or `dislike' the four groups. The results showed
   a consistency in in-group favoritism. Gender and age were both related
   to Chinese youths' national identifications and their national
   intergroup attitudes. Confucian ethics, media influence and historical
   complex were identified as main factors that may contribute to their
   attitudes toward the four groups.},
