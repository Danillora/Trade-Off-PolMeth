@article{ WOS:000500985800003,
Author = {Rodd, Robin},
Title = {Democratic Citizenship as Uruguayan Cultural Heritage},
Journal = {DEMOCRATIC THEORY-AN INTERDISCIPLINARY JOURNAL},
Year = {2019},
Volume = {6},
Number = {1},
Pages = {27-48},
Month = {JUN},
Abstract = {Amidst a global turn towards authoritarianism and populism, there are
   few contemporary examples of state-led democratization. This article
   discusses how Uruguay's Frente Amplio (FA) party has drawn on a unique
   national democratic cultural heritage to encourage a coupling of
   participatory and representative institutions in ``a politics of
   closeness.{''} The FA has reinvigorated Batllismo, a discourse
   associated with social justice, civic republicanism, and the rise of
   Uruguayan social democracy in the early twentieth century. At the same
   time, the FA's emphasis on egalitarian participation is inspired by the
   thought of Uruguay's independence hero Jose Artigas. I argue that the
   cross-weave of party and movement, and of democratic citizenship and
   national heritage, encourages the emergence of new figures of the
   citizen and new permutations for connecting citizens with representative
   institutions. The FA's ``politics of closeness{''} is an example of how
   state-driven democratization remains possible in an age described by
   some as ``post-democratic.{''}},
