@article{ WOS:000479144600004,
Author = {Salas Alfaro, Renato},
Title = {The background of the material successes of international migration: the
   case of mexiquense migrants},
Journal = {CIMEXUS},
Year = {2019},
Volume = {14},
Number = {1},
Pages = {81-102},
Month = {JAN-JUN},
Abstract = {Studies on return migration often show the successes or failures of
   migrants, but do not combine them with the frictions and contradictions
   that imply the material and immaterial achievements they obtained.
   However, during the migration several obstacles are faced, that although
   migrants can overcome and even take some advantage of them, it is
   evident that they bring with them various consequences (illnesses,
   fractures, religiosity, marital separation, death experiences). These
   non-visible events constitute the background of different successes
   (owning a house, speaking another language, having business, bringing
   work knowledge), this is explored in this work. To this aim, a
   non-random sample of migrants is taken as a reference in the Estado de
   Mexico and the analysis is based on their own testimonies.},
Publisher = {UNIV MICHOACANA SAN NICOLAS HIDALGO, INST INVESTIGACIONES HISTORICAS},
