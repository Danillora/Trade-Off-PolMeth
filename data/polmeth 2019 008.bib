@article{ WOS:000469581800001,
Author = {McDonnell, Duncan and Morgenbesser, Lee},
Title = {The APSA journal list: popularity, purpose and performance},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2019},
Volume = {54},
