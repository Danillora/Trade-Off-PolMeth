@article{ WOS:000463405500001,
Author = {Sherlock, Thomas},
Title = {Russian Society and Foreign Policy: Mass and Elite Orientations After
   Crimea},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2020},
Volume = {67},
Number = {1, SI},
Pages = {1-23},
Month = {JAN 2},
Abstract = {Most Russians applaud the official narrative that Russia has reemerged
   as a great power. Yet they increasingly disagree with the assertion of
   the Kremlin that the United States is a looming external danger and a
   subversive force in Russian domestic politics. In line with these
   opinions, many Russians balk at the costs of confrontation with the
   West, demonstrating the initially limited and now waning political
   significance of the ``Crimea euphoria{''} (or ``Crimea effect{''}) and
   ``rally `round the flag{''} phenomena. Russian elites often differ from
   the general public in their stronger backing for a more assertive
   foreign posture. Nevertheless, such preferences are frequently moderated
   by the apprehension that Russia will neglect domestic modernization
   indefinitely if its foreign policy is confrontational.},
