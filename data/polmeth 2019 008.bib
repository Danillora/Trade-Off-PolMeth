@article{ WOS:000516826800010,
Author = {Fernandez Guzman, Eduardo},
Title = {Socio-economic and cultural aspects of upward social mobility. The case
   of the children of migrants from Huandacareo, Michoacan, who studied the
   university in the United States},
Journal = {CIMEXUS},
Year = {2019},
Volume = {14},
Number = {2},
Pages = {173-197},
Month = {JUL-DEC},
Abstract = {The concept and reality of intergenerational social mobility have not
   been copiously studied by History to examine the phenomenon of
   contemporary international migration. Social mobility can be analyzed
   from various historical abstraction edges: horizontal or vertical social
   mobility; ascending and descending; intragenerational and
   intergenerational; and individual and group mobility. Epistemologically
   on the subject of migration, these concepts open theoretical analytical
   paths, individually or comparatively. The objective of this work is to
   explore from the historiographical perspective of the history of the
   present the ascending and intergenerational social mobility in the
   phenomenon of international migration in the specific case of students
   children of Huandacarense migrants who finished university studies in
   the United States. The theoretical and methodological substrate of this
   work is close to the focus of the history of the present and oral
   history. We advance some theoretical debates and results on this
   specific line of research that is part of a research agenda that deals
   with social mobility and social change in contemporary international
   migration.},
