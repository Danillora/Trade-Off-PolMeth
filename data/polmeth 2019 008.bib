@article{ WOS:000503415200009,
Author = {Nocon, Jaroslaw and Dorosh, Lesia and Ivasechko, Olha},
Title = {PESCO AS THE MODERN DEFENSE INITIATIVE OF THE EUROPEAN UNION: POSITIONS
   OF WESTERN EUROPEAN COUNTRIES VS POSITIONS OF EASTERN EUROPEAN COUNTRIES},
Journal = {EUROPEAN JOURNAL OF TRANSFORMATION STUDIES},
Year = {2019},
Volume = {7},
Number = {2},
Pages = {128-141},
Abstract = {The EU member states' modern security initiatives, realized by the
   defence alliance for Permanent Structured Cooperation (PESCO) have been
   found out. PESCO implementation process was initiated and headed by
   France and Germany. The countries proposed a step-by-step approach to
   strengthening defence capabilities and operational commitments to
   increase the overall defence efforts of United Europe. Allegedly,
   nowadays there are many practical problems and challenges on the way to
   the successful implementation of PESCO. Its successful overcoming will
   be facilitated by the ability of initiator countries to deal with
