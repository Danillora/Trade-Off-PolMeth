@article{ WOS:000482361800001,
Author = {Rhodes, Martin J. and Epstein, Rachel and Boerzel, Tanja A.},
Title = {Introduction: peripheries in competition? Political and economic
   development in the EU},
Journal = {WEST EUROPEAN POLITICS},
Year = {2019},
Volume = {42},
Number = {5, SI},
Pages = {927-940},
Month = {JUL 29},
Abstract = {The aim of this special issue is to take a nuanced look at the political
   economy of development across the European Union since the financial
   crisis. In contrast to much of the literature on the EU since the
   financial crisis, which is largely concerned with macro-issues, and
   engages in normative critiques of EU policies, this issue focuses on
   multiple levels (including the micro-level of firms and banks), and the
   institutional make-up of member states, to restore the balance between
   EU, national and sub-national analysis. It also eschews normative
   critique. It focuses on the following two themes: the need for a more
   nuanced understanding of centreperiphery relations in the EU; and the
   need for a closer analysis of the interaction effects between European
   and national/sub-national levels in explaining the crisis and
   understanding its impact.},
