@article{ WOS:000470261800002,
Author = {Carey, Brian},
Title = {The preference satisfaction model of linguistic advantage},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2019},
Volume = {22},
Number = {2},
Pages = {134-154},
Abstract = {In this article, I argue that we should measure a person's linguistic
   advantage within a particular linguistic environment according to the
   extent to which his or her preferences about that environment are
   satisfied. I begin by outlining three criteria that a model of
   linguistic advantage should satisfy, if it is to serve as a plausible
   guide for theories of linguistic justice. I then describe a model that
   is capable of satisfying these criteria - the preference satisfaction
   model - and defend it from several potential objections. Finally, I
   consider some practical implications of the PSM, and describe how the
   model should be modified in cases where there are insufficient resources
   for it to be fully implemented.},
