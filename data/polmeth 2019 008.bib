@article{ WOS:000470200700008,
Author = {Bakalov, Ivan},
Title = {Whither soft power? Divisions, milestones, and prospects of a research
   programme in the making},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2019},
Volume = {12},
Number = {1},
Pages = {129-151},
Abstract = {The rapid expansion of soft-power scholarship has made its central
   concept one of the most recognisable terms in the discipline of
   international relations. Yet the increased attention has not spurred the
   resolution of the underlying conceptual issues. Instead, soft-power
   scholarship has drifted into separate streams: concept-driven and
   case-centred studies. The emerging divide impedes the healthy exchange
   that holds the key to a more resonant impact on the field of
   international relations. This article seeks to bridge the gap by
   revealing the common ground where both the unresolved problems and the
   conceptual achievements of the nascent research programme have their
   roots.},
