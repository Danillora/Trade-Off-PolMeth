@article{ WOS:000480512200003,
Author = {Arzheimer, Kai and Berning, Carl C.},
Title = {How the Alternative for Germany (AfD) and their voters veered to the
   radical right, 2013-2017},
Journal = {ELECTORAL STUDIES},
Year = {2019},
Volume = {60},
Month = {AUG},
Abstract = {Until 2017, Germany was an exception to the success of radical right
   parties in postwar Europe. We provide new evidence for the
   transformation of the Alternative for Germany (AfD) to a radical right
   party drawing upon social media data. Further, we demonstrate that the
   AfD's electorate now matches the radical right template of other
   countries and that its trajectory mirrors the ideological shift of the
   party. Using data from the 2013 to 2017 series of German Longitudinal
   Elections Study (GLES) tracking polls, we employ multilevel modelling to
   test our argument on support for the AID. We find the AfD's support now
   resembles the image of European radical right voters. Specifically,
   general right-wing views and negative attitudes towards immigration have
   become the main motivation to vote for the AM. This, together with the
   increased salience of immigration and the AfD's new ideological profile,
   explains the party's rise.},
