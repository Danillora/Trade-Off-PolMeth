@article{ WOS:000461822500015,
Author = {Muroki, Mercy and Cowley, Philip},
Title = {Getting Better, Slowly. Ethnicity, Gender and Party in London's Local
   Government},
Journal = {POLITICAL QUARTERLY},
Year = {2019},
Volume = {90},
Number = {1},
Pages = {117-123},
Month = {JAN-MAR},
Abstract = {This article reports black and Asian descriptive representation in
   London's elected local government. In both absolute and relative terms,
   the levels of descriptive representation are higher than previously, and
   London's Asian population is now represented proportionately, although
   black Londoners remain under-represented. Such aggregate levels of
   representation, however, mask considerable disparities between the two
   groups in many instances. The same is true of gender representation,
   with overall figures hiding the fact that Asian men are over-represented
   (and white women broadly proportionately represented) while black men
   are the least represented category analysed.},
