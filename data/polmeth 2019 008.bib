@article{ WOS:000489627900009,
Author = {Togaibayeva, S. S. and Yelyubayev, M. S. and Aikumbeckov, N. and
   Togaibayev, I, A. and Khanov, T. A.},
Title = {Features of the Commission of Fraud, Coupled with the Failure to Comply
   with the Provisions of a Civil Contract},
Journal = {JOURNAL OF POLITICS AND LAW},
