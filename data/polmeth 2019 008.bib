@article{ WOS:000455040500028,
Author = {Fernandez-Vazquez, Pablo},
Title = {The Credibility of Party Policy Rhetoric Survey Experimental Evidence},
Journal = {JOURNAL OF POLITICS},
Year = {2019},
Volume = {81},
Number = {1},
Pages = {309-314},
Month = {JAN 1},
Abstract = {This article analyzes how a party's policy statements affect voters'
   perceptions of where the party stands on a given issue. I argue that
   voters do not take a party's statements at face value because these
   messages can be a strategic tool to win elections. Voters discount
   popular statements because they may respond to vote-seeking incentives
   rather than reflect the party's sincere views. Espousing unpopular
   policies has less instrumental value in obtaining more votes and
   therefore is more credible. I have tested this argument with a survey
   experiment fielded in the United Kingdom that exposes respondents to
   Conservative and Labour Party statements on immigration and the National
   Health Service. I report evidence that popular statements tend to have a
   weaker effect on voter perceptions than unpopular ones. This finding
   suggests a paradox: the more a party needs to change its reputation in
   order to gain votes, the stronger the voters' skepticism.},
Publisher = {UNIV CHICAGO PRESS},
