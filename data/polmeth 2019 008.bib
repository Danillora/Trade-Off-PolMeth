@article{ WOS:000501624900005,
Author = {Den Uyl, Douglas J.},
Title = {A Tale of Two Capitalisms},
Journal = {TOCQUEVILLE REVIEW},
Year = {2019},
Volume = {40},
Number = {2},
Pages = {91-108},
Abstract = {In this paper I compare Adam Smith and Ayn Rand on their respective
   understandings of capitalism. The paper begins by describing the
   ``myth{''} that surrounds each of these thinker's depiction of
   capitalism followed by a discussion of how accurately those myths accord
   with the texts of these two thinkers. In the case of Smith I argue that
   the myth is somewhat wide of the mark, but in Rand's case much closer.
   It would appear, therefore, that we have two quite different
   understandings of the nature of capitalism. I conclude, however, by
   arguing that we may have only one capitalism with a focus on two
   different aspects of it, which are not necessarily in contradiction
   though those aspects may indeed be in tension. I end by suggesting that
   what may be keeping the tension from breaking apart is entrepreneurship.},
Publisher = {UNIV TORONTO PRESS INC},
