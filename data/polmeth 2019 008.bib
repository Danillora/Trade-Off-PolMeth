@article{ WOS:001487615200011,
Author = {Ritz, Manuel and Neumann, Oliver and Sager, Fritz},
Title = {Senkt New Public Management die Verwaltungsausgaben in den Schweizer
   Kantonen? Eine empirische Analyse über zwei Dekaden},
Journal = {SWISS POLITICAL SCIENCE REVIEW},
Year = {2019},
Volume = {25},
Number = {3},
Pages = {226-252},
Month = {SEP},
Abstract = {In light of the growing public spending in many regions of the world,
   this study scrutinizes relevant determinants of core public
   administration expenditure at the cantonal level in Switzerland,
   focusing particularly on New Public Management (NPM) reforms. The
   empirical study comprises all 26 cantons in the period from 1993 through
   2014 and uses both cross-sectional and longitudinal analytical methods.
   In addition to NPM, we control for various variables related to
   political parties, institutions, socioeconomics and culture. The results
   show no clear relationship between NPM reforms and expenditure, although
   such reforms were often intended to cut costs. Our explanation for this
   finding is that NPM as a concept is rather inconsistent. Various control
   variables are either positively or negatively related with expenditure.
   All in all, our findings suggest that public expenditure is still mostly
   determined by politics and can only be influenced by administrative
   reforms within certain narrow limits.
   In Anbetracht der vielerorts stetig wachsenden Staatsausgaben analysiert
   diese Studie wichtige Determinanten der kantonalen Ausgaben im Bereich
   der allgemeinen Verwaltung in der Schweiz mit besonderem Fokus auf New
   Public Management-Reformen (NPM). Die empirische Studie umfasst die 26
   Kantone im Zeitraum zwischen 1993 und 2014 und bedient sich dabei
   diverser Quer- und L \& auml;ngsschnittanalysemethoden. Neben
   NPM-Reformen wird f \& uuml;r diverse Parteien-, Institutionen-, Kultur-
   und sozio \& ouml;konomische Variablen kontrolliert. Die Ergebnisse
   zeigen keinen eindeutigen Zusammenhang zwischen NPM-Reformen und
   Verwaltungsausgaben, obwohl erstere oft als Mittel zur Kosteneinsparung
   betrachtet wurden. Wir erkl \& auml;ren diesen Befund mit der
   konzeptionellen Inkonsistenz von NPM. Diverse Kontrollvariablen weisen
   entweder positive oder negative signifikante Zusammenh \& auml;nge mit
   den Verwaltungsausgaben auf, was wir im Text genauer diskutieren.
   Insgesamt legen unsere Befunde den Schluss nahe, dass Staatsausgaben
   stets politisch begr \& uuml;ndet sind und durch Verwaltungsreformen nur
   bedingt substituiert werden k \& ouml;nnen.},
