@article{ WOS:000497450400003,
Author = {Garcia-Escudero Marquez, Piedad},
Title = {Assessment of the constructive vote of no confidence in a fragmented
   parliament},
Journal = {TEORIA Y REALIDAD CONSTITUCIONAL},
Year = {2019},
Volume = {44},
Pages = {101-136},
Abstract = {The adoption of a constructive vote of no confidence in Spain for the
   first time since the adoption of the Spanish 1978 Constitution has led
   to the change of political sign of the Government in the middle of the
   XII legislature or term, characterised by the fragmentation of the
   Congress of Deputies which has replaced the former nearly two-party
   system. The essay analyses the consequences of this change on the
   functioning and functions of Parliament and on the instability of a
   Government that has been in power for only nine months, taking stock of
   the period extending from the vote of no confidence to dissolution of
   Parliament, comparing it with the equally difficult previous period and
   with other terms, to ultimately show how the end of the two-party system
   has influenced Government-Parliament relations in the Spanish
   parliamentary political system.},
Publisher = {UNIV NACIONAL EDUCACION DISTANCIA},
Address = {PO SENDA REY,BRAVO MURILLO 38, MADRID, 7-28040, SPAIN},
Type = {Article},
Language = {Spanish},
Affiliation = {Márquez, PGE (Corresponding Author), Univ Complutense Madrid, Derecho Constituc, Madrid, Spain.
   Garcia-Escudero Marquez, Piedad, Univ Complutense Madrid, Derecho Constituc, Madrid, Spain.},
ISSN = {1139-5583},
Keywords = {Constructive vote of no confidence; fragmented Parliament; minority
