@article{ WOS:000493385800003,
Author = {Alvarez, Diego},
Title = {A General Characterization of Raul Prada's Concept of Variegation},
Journal = {CIENCIA POLITICA},
Year = {2019},
Volume = {14},
Number = {28},
Pages = {49-69},
Month = {JUL-DEC},
Abstract = {The concept of social variegation developed by Bolivian intellectual
   Rene Zavaleta Mercado has been crucial for political thinking in Latin
   America. Likewise, the reception of this notion by Bolivian
   intellectuals has been wide. This article characterizes the general
   elements of variegation notion on the thought of Bolivian philosopher
   Raul Prada, in contrast with the problem of social variegation developed
   by Zavaleta. Therefore, this article proposes three general dimensions
   of Prada's notion of variegation: an ontological dimension, a
   social-historical dimension and an epistemological dimension.},
