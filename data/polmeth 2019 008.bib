@article{ WOS:000472336600001,
Author = {Tsai, Chia-Hung and Tan, Alexander C. and Jung, Koochul},
Title = {An analysis of the 2014 New Zealand general election: do Internet use
   and online party mobilisation matter?},
Journal = {POLITICAL SCIENCE},
