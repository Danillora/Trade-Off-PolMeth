@article{ WOS:000564654400005,
Author = {Garcia Vergara, Cristhian Adrian and Rincon Villamizar, Nydia Maria},
Title = {Mapping community reality: An approach to voices of La Fortaleza
   community},
Journal = {CIUDAD PAZ-ANDO},
