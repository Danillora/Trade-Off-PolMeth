@article{ WOS:000601356900003,
Author = {Aldrin, Philippe and Vannetzel, Marie},
Title = {On the edges. A sociology of the secondary political actors in two small
   French towns},
Journal = {POLITIX},
Year = {2019},
Volume = {32},
Number = {128},
Pages = {31-63},
Abstract = {On a day-to-day basis, local political activity does not only involve
   professional or semiprofessional politicians. All of the practical
   dimensions of political activity, from the makeup of candidate lists to
   the daily exercise of municipal power, relies on the work of ``secondary
   actors,{''} i.e., novices, amateurs, and part-timers, who perform
   various roles. They might be delegates of associations or organizations,
   intermediaries or representatives of social groups, or nonpartisan
   grassroots activists. Together, they make up the ``edges{''} of local
   politics, which are the subject of this sociological discussion based on
   the case studies of two small French towns.},
