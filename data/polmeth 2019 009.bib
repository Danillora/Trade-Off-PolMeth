@article{ WOS:000605933800011,
Author = {Tokman, L. Yildiz},
Title = {A GENDER EQUALITY APPROACH TO ISTANBUL'S URBAN DEVELOPMENT},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2019},
Volume = {18},
Number = {1},
Pages = {107-116},
Month = {SPR},
Abstract = {The city is a complex mechanism in which various actors with different
   interests coexist. Within this structure, local governments try to
   safeguard the interests of different groups through urban policies and
   city planning. However, urban planning and urban development are still
   far from providing gender equality. Through the case study of Istanbul,
   this article outlines how traditional urban planning reinforces
   inequality between men and women and offers recommendations to achieve
   gender equality in urban development. The article argues that a gender
   equality approach to urban design and planning is key to an egalitarian
   and sustainable city.},
