DOI = {10.1080/13698230.2020.1694214},
EarlyAccessDate = {NOV 2019},
ISSN = {1369-8230},
EISSN = {1743-8772},
Keywords = {Solar radiation management; risk; precaution; regulation; moral hazard},
Keywords-Plus = {GOVERNANCE; ETHICS},
