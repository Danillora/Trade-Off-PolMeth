@article{ WOS:000474803600016,
Author = {Campbell, Rosie and Cowley, Philip and Vivyan, Nick and Wagner, Markus},
Title = {Why Friends and Neighbors? Explaining the Electoral Appeal of Local
   Roots},
Journal = {JOURNAL OF POLITICS},
Year = {2019},
Volume = {81},
