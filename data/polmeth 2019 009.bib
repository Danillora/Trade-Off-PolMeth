@article{ WOS:000605933800002,
Author = {Hyden, Therese},
Title = {SWEDEN'S FEMINIST FOREIGN POLICY ILLUSTRATED},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2019},
Volume = {18},
Number = {1},
Pages = {11-18},
Month = {SPR},
Abstract = {Sweden's feminist foreign policy is turning five years old. Launched
   against a backdrop of discrimination and violence against women
   globally, its purpose remains today. The Feminist Foreign Policy is
   built on three R's. It aims to strengthen women's and girl's Rights,
   Representation, and Resources based on the reality in which they live.
   This means that women should be able to fully enjoy their human rights,
   that women should be represented at decision-making tables, and that
   they should have access to resources. Gender equality is a foundation
   for economic growth, sustainable development, peace, and security.
   Involving men and boys is vital to achieving a change in gender norms.
   Today Sweden is joined by around 80 countries which have adopted their
   own policies on gender equality.},
