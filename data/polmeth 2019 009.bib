@article{ WOS:000521832800003,
Author = {Gomez Calero, Rosa Elena and Crespo Toro, Zoila Esperanza},
Title = {A political look at higher education in Ecuador in the framework of a
   proposal of freedom and opportunities for all},
Journal = {CUESTIONES POLITICAS},
Year = {2019},
Volume = {36},
Number = {63},
Pages = {30-48},
Month = {JUL-DEC},
Abstract = {Higher education plays an axial role in the integral development of
   every nation, since, in it, the human resource is formed that with its
   work and daily action serves as a motor for the national productive
   apparatus, while generating active citizenship. In consideration of this
   axiomatic premise, the objective of this essay is to provide reflections
   conducive to the definition of an educational proposal of freedom and
   opportunities for all. In the methodological, it is to consider that we
   are facing a reflexive and critical essay developed with an intimate
   language, devoid of the guidelines of traditional scientific discourse
   expressed in a depersonalized tone or through the third person plural.
   It is concluded that this proposal to improve higher education means, by
   itself, a political act that challenges the traditional conceptions on
   the subject and its consequent devices of power.},
