@article{ WOS:000504060600011,
Author = {Rezaei, Farhad},
Title = {Iran's Military Capability: The Structure and Strength of Forces},
Journal = {INSIGHT TURKEY},
Year = {2019},
Volume = {21},
Number = {4},
Pages = {183-214},
Month = {FAL},
Abstract = {Economic upheaval in the post-revolutionary period, its ideological
   concerns, the lessons of the war with Iraq and the drive to assert
   regional dominance have shaped Iran's emerging military doctrine. This
   research intends to determine Iran's military strength, relying on a
   particular formula that assesses a number of factors, including military
   budget, manpower, army assets, ballistic missile arsenal, proxy forces,
   and cyberwar-are capability. The research assumes that Iran's arsenal is
   very limited and somewhat antiquated, more suitable for defensive rather
   than offensive operations. The second objective of the research is to
   address the possible strategic consequences of Iran rebuilding its
   military capabilities on regional security. The article assumes that
   Iran will likely be judged by its intentions rather than its actions,
   meaning that if its rhetoric is offensive, then it is likely to be
   judged as a threat.},
