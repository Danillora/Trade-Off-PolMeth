
@article{ WOS:000482801900005,
Author = {Boukari, Mamadou},
Title = {The political budget cycle in French municipal elections: unexpected
   nonlinear effects},
Journal = {FRENCH POLITICS},
Year = {2019},
Volume = {17},
Number = {3},
Pages = {307-339},
Month = {SEP},
Abstract = {Using a large dataset of French municipalities, this article examines
   the joint determination of the win margin of victory of incumbent mayors
   and the size of the political budget cycle. A system of two simultaneous
   equations is estimated with the three-stage least squares method. The
   main findings are twofold. First, the effects of the win margin on the
   size of the fiscal cycle are U-shaped. This means that, in a close
   election, the incumbent mayor tends to reduce public expenditure while,
   if the incumbent is either certain to win or to lose the election,
   expenditures tend to be increased. Second, another nonlinear effect is
   revealed, linking mayors' time in office to their win margin of victory.},
