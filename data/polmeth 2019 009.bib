@article{ WOS:000468006900006,
Author = {Dox, Georg},
Title = {Between curtsey and genuflection Austria-Russia: an outline of their
   relationship},
Journal = {OSTEUROPA},
Year = {2019},
Volume = {69},
Number = {1-2},
Pages = {67+},
Abstract = {For historical reasons, Austria has a special relationship with Russia.
