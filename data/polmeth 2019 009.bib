@article{ WOS:000500985800004,
Author = {Pirsoul, Nicolas},
Title = {Sectarianism and Recognition in Iraq From Consociationalism to
   Deliberation?},
Journal = {DEMOCRATIC THEORY-AN INTERDISCIPLINARY JOURNAL},
Year = {2019},
Volume = {6},
Number = {1},
Pages = {49-72},
Month = {JUN},
Abstract = {This article uses the theory of recognition to analyze sectarian
   conflicts in Iraq. After describing the sectarian and historical
   background of contemporary Iraqi politics, the article critiques the
   implementation of consociationalism and policies influenced by liberal
   multiculturalism in deeply divided societies. It argues that these
   policies lead to a dangerous reification of identities. The article
   argues that a progressive implementation of deliberative democracy
   practices could improve identity-related issues in Iraq and explains how
   democratic practices are legitimized by the most influential Islamic
   religious figure in Iraq.},
