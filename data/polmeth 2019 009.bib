@article{ WOS:000472352500001,
Author = {Wallace, Rebecca},
Title = {``Warriors don't sleep til noon{''}: colonial rhetoric and the framing
   of Indigenous recipients of welfare in Canadian print news, 1990-2015},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2021},
Volume = {9},
Number = {2},
Pages = {300-318},
Month = {MAR 15},
Abstract = {This article explores the media framing of Indigenous peoples as
   recipients of welfare in eight English-language Canadian major dailies
   from January 1, 1990 to December 31, 2015. Using automated coding and
   cluster analyses to uncover core topics in the coverage, this research
   asks: (1) What are the central themes or frames in articles pertaining
   to Indigenous recipients of social assistance? (2) Does the use or
   frequency of these frames change over the selected timeline or between
   the different newspapers under study? (3) How does the coverage of
   Indigenous peoples as recipients of welfare contribute to larger
   connotations regarding Indigenous self-governance? This analysis finds
   that news stories predominantly focus on unemployment rates among
   Indigenous communities, as well as Indigenous peoples' experiences with
   the criminal justice system and supposed dependence on welfare. The
   coverage of Indigenous recipients represents a colonial narrative that
   relies on harmful stereotypes and does not reflect the colonial roots of
   many socio-economic problems facing Indigenous communities today.
   Effectively, this paper argues that the coverage of Indigenous
   recipients of welfare both overtly and covertly undermines support for
   Indigenous sovereignty by depicting Indigenous peoples and their leaders
   as ``unprepared{''} to manage their own affairs.},
