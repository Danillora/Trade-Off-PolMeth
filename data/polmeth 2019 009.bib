@article{ WOS:000509543300010,
Author = {Rodriguez Ramirez, Dairon Alfonso and Silva Rojas, Alonso and Maldonado
   Serrano, Jorge Francisco},
Title = {Constitutional values and principles as a framework for understanding
   the education of citizenship competences in Colombia with regard to the
   Saber Pro exam},
Journal = {REFLEXION POLITICA},
Year = {2019},
Volume = {21},
Number = {43},
Pages = {121-136},
Month = {DEC},
Abstract = {The objective of this article is to propose a reflection for a better
   and more precise understanding of what is implied in the formation in
   citizen competencies according to the assessment of the Saber Test. The
   first section will offer a reflection on the primary function of the
   axiological-political order that the constitutional values and
   principles have within the Colombian Political Constitution. The second
   section will present the multiple constitutional values and principles
   themselves that are the conceptual grounding for the exercise of the
   democratic citizenship; in order to do so, we will refer to the
   Constitutional text and to the Judicial Sentences of the Constitutional
   Court of Colombia in which the philosophical and political groundings
   are specified. In the last section, and as a conclusion, some finals
   remarks from a constitutional pedagogy scope, based on our research,
   will be given in accordance with the 41st article of the Constitution.},
