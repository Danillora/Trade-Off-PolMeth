@article{ WOS:000482513100013,
Author = {Carlson, David},
Title = {Modeling-Related Processes With an Excess of Zeros},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2019},
Volume = {7},
Number = {4},
Pages = {889-902},
Month = {OCT},
Abstract = {Political science research frequently models binary or ordered outcomes
   involving related processes. However, traditional modeling of these
   outcomes ignores common data issues and cannot capture nuances. There is
   often an excess of zeros, the observed outcomes for different actors are
   inherently related, and competing actors may respond to the same factors
   differently. This paper extends existing models and develops a
   zero-inflated multivariate ordered probit to simultaneously address
   these issues. This model performs better than existing models at
   capturing the true parameters of interest, estimates the nature of the
   related processes, and captures the differences in actors'
   decision-making. I demonstrate these benefits through simulation
   exercises and an application to party behavior in Mexico.},
