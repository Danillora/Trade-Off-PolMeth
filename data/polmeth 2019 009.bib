@article{ WOS:000477479200001,
Author = {Li, Hui and Gong, Ting},
Title = {Panacea or Pandora's Box? The Dual Effect of Marketization on Corruption},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2020},
Volume = {67},
Number = {2, SI},
Pages = {141-155},
Month = {MAR 3},
Abstract = {The relationship between corruption and the market has long been
   contested. What are the effects of marketization on corruption? Would
   market competition reduce corruption as some argue? Or is marketization
   a Pandora's Box for the spread of corruption as others suggest? Using
   China as a case and a dataset of 7,305 corruption-related court
   judgments, this paper seeks to address these questions. The findings
   indicate that marketization has a dual effect on corruption. Expanded
   market exchanges likely raises the monetary value accrued through
   corrupt activities, but marketization also exerts a conditional effect
   by restraining political power from seeking personal gains.},
