@article{ WOS:000471935000008,
Author = {Ilicic, Gordana and Smeriga, Peter},
Title = {THE INFLUENCE OF INSTITUTIONAL DESIGN ON THE DEMOCRATISATION IN BOSNIA
   AND HERZEGOVINA},
Journal = {POLITICKE VEDY},
