@article{ WOS:000482392600004,
Author = {Morfino, Vittorio},
Title = {Beyond the ``Repressive Hypothesis{''}: ``Subject-{''} and
   ``Libido-Effect{''} in Althusser},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2019},
Volume = {31},
Number = {3, SI},
Pages = {273-290},
Abstract = {The essay ``Ideology and Ideological State Apparatuses{''} (ISAs)
   contains a famous statement about ideology in relation to the
   psychoanalytic problematic: namely, that ``ideology is eternal, exactly
   like the unconscious.{''} However, Althusser does not offer any further
   clarification on this parallelism. Further, in a letter from the late
   1970s, he states that he has ``stopped short... before the question ...
   about the `relations' between ideology (or concrete ideological
   formations) and the unconscious.{''} But in a text that precedes the ISA
   essay, ``Three Notes on the Theory of Discourses,{''} published
   posthumously, Althusser tried to develop exactly a theory of the
   articulation of the ideological and of the unconscious, using a new set
   of concepts. In this text he combined systematically and for the first
   time what, borrowing a term from Balibar, could be called an ``ontology
   of relations,{''} with the categories of the ``encounter{''} and
   ``taking hold.{''}},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Morfino, V (Corresponding Author), Univ Milano Bicocca, Hist Philosophy, Milan, Italy.
   Morfino, V (Corresponding Author), Coll Int Philosophie, Crit Theory Soc, Paris, France.
   Morfino, V (Corresponding Author), Coll Int Philosophie, Paris, France.
   Morfino, Vittorio, Univ Milano Bicocca, Hist Philosophy, Milan, Italy.
