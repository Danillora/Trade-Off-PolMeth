@incollection{ WOS:000813444900006,
Author = {Fuchs, Dieter and Roller, Edeltraud},
Editor = {Wiesner, C and Harfst, P},
Title = {The Problem of Legitimacy in western European Democracies An Analysis
   under Conditions of Globalization},
Booktitle = {LEGITIMITAT UND LEGITIMATION: VERGLEICHENDE PERSPEKTIVEN},
Series = {Vergleichende Politikwissenschaft},
Year = {2019},
Pages = {139-170},
Abstract = {The paper analyzes for seventeen Western European countries whether
   there is a legitimacy crisis in democracy or whether there are signs of
   an erosion of legitimacy. The assumption here is that the effects of
   globalization could lead to more severe legitimacy problems. The
   theoretical basis is, first, a concept of political support for
   conceptualizing and measuring legitimacy and, second, theoretical
   considerations on the relationship between globalization and democracy.
   According to the empirical findings, it is not possible to speak of a
   legitimacy crisis of democracy, at least until 2014. However, there are
   signs of some erosion of legitimacy and satisfaction with democracy in
   some countries. Moreover, it can be shown that there are effects of
   globalization on satisfaction with democracy in one's own country.},
