@article{ WOS:000467982000005,
Author = {Obreja, Leyla-Denisa},
Title = {Human Rights Law and Intimate Partner Violence: Towards an
   Intersectional Development of Due Diligence Obligations},
Journal = {NORDIC JOURNAL OF HUMAN RIGHTS},
Year = {2019},
Volume = {37},
