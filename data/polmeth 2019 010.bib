@article{ WOS:000497957600002,
Author = {Montoya Arango, Vladimir and Perdomo Sanchez, Jenni Carolina},
Title = {Emerging Territorial Tensions in the Configuration of Productive
   Landscapes in the Southwest of Antioquia, Colombia},
Journal = {GEOPOLITICAS-REVISTA DE ESTUDIOS SOBRE ESPACIO Y PODER},
Year = {2019},
Volume = {10},
Number = {2},
Pages = {207-228},
Abstract = {Based on the productive landscape configuration in the southwest of
   Antioquia (Colombia), this article discusses the way in which the
   mid-course of the Cauca river has been transformed into an axis of
   economic intervention. Practices of development such as economic growth
   and neoliberalism policies dominate and dispute the local ways of life.
   Through ethnographic work and analysis of thematic cartography, the
   article examines how the geographic expansion of capitalism produces
   dispossession and confinement, at the same time new subjectivities and
   ways of social mobilization are configured.},
