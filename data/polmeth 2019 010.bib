@incollection{ WOS:000813444900011,
Author = {Inkinen, Saara and Thyen, Kressen},
Editor = {Wiesner, C and Harfst, P},
Title = {Regime Performance and political Trust in Autocracies The Effects of
   Economic Development and Representation through Elections},
Booktitle = {LEGITIMITAT UND LEGITIMATION: VERGLEICHENDE PERSPEKTIVEN},
Series = {Vergleichende Politikwissenschaft},
Year = {2019},
Pages = {315-346},
Abstract = {This paper explores how regime performance affects political trust in
   autocracies. Using attitudinal data from the World Values Survey and
   various macro data, a panel data analysis examines the influence of
   economic development and limited representation through elections on
   trust in two key regime institutions: parliament and government. The
   empirical analysis reveals that economic development only exerts a
   significant influence on trust in the government, while both
   institutions benefit equally from national parliamentary elections. The
   design of the electoral process is decisive for the generation of
   political trust: For example, the effect of elections on trust in
   parliament and government varies depending on whether political
   opposition and party pluralism are permitted or prohibited. Moreover,
   the two dimensions of electoral supply condition each other, indicating
   a complex relationship between autocratic elections and political trust.
   These findings are embedded in the current debate on the political
   legitimacy of autocratic regimes.},
