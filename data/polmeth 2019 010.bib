@article{ WOS:000461822500017,
Author = {Diamond, Patrick},
Title = {Breaking the Mould? Roy Jenkins' `Radical Centre' and British Social
   Democracy in the `New Hard Times'},
Journal = {POLITICAL QUARTERLY},
Year = {2019},
Volume = {90},
Number = {1},
Pages = {134-142},
Month = {JAN-MAR},
Abstract = {Social democratic politics in Britain requires compelling answers to
   three animating questions that Roy Jenkins posed in his landmark 1979
   Dimbleby lecture. Firstly, how can the British system of democracy be
   reformed to develop a culture of political participation and pluralism
   that leads to more equitable economic and social policies? Secondly, how
   can the centre-left and left unite to forestall long periods of Tory
   dominance in electoral politics, thereby avoiding calamities such as the
   2016 referendum on EU membership? And, thirdly, how can a flourishing
   intellectual ecosystem be cultivated on the progressive left that
   generates radical ideas for economic and social reform, recognising the
   importance of concerted dialogue between political traditions? This
   article revisits Jenkins' 1979 lecture by considering the prospects for
   `breaking the mould' of British politics in the `new hard times' of
   Brexit and `permanent austerity'.},
