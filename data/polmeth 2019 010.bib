@article{ WOS:000463835800013,
Author = {Barr, Michael and Wright, Paul},
Title = {Training graduate teaching assistants: What can the discipline offer?},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2019},
Volume = {18},
Number = {1},
Pages = {143-156},
Month = {MAR},
Abstract = {Graduate teaching assistants (GTAs) provide an invaluable contribution
   to higher education, yet their role is often overlooked and
   understudied. We report on a UK-based study to describe the experience
   and views of GTAs with particular focus on training and professional
   development. Our findings draw from a survey of 32 GTAs along with
   informal group discussions with a small number of under-graduates and
   academic staff. A key theme from our project is the value of
   subject-specific training. We explore this issue and raise the
   possibility that in addition to university departments, learned
   societies and academic associations within the discipline may have a
   role to play in providing pedagogical training for GTAs and early career
   academics.},
