@article{ WOS:000473635200001,
Author = {Blankenship, Benjamin T. and Stewart, Abigail J.},
Title = {Threat, trust, and Trump: identity and voting in the 2016 presidential
   election},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2019},
Volume = {7},
Number = {3},
Pages = {724-736},
Month = {JUL 3},
Abstract = {Using longitudinal data from 421 MTurk participants, collected over 6
   months surrounding the 2016 election between Hillary Clinton and Donald
   Trump, we investigated the relationships among identity centrality,
   institutional trust, and vote. Specifically, we were interested in
   whether trust in science and the media mediated the relationship between
   the centrality of racial, gender, and class identities, and how the
   importance of these identities was related to participants' votes.
   Results indicated that trust in these institutions did mediate the
   relationship between centrality and vote in different ways depending on
   level of privilege, identity domain, and type of trust. For instance,
   class centrality among working-class people predicted greater trust in
   the media, which predicted voting for Hillary Clinton, while race
   centrality among White people predicted less trust in science, which
   predicted voting for Trump.},
