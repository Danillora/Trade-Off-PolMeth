@article{ WOS:000456962900005,
Author = {Miller, Char Roone},
Title = {``Ghostly Voices Rose from the Fields{''}: Nebraska and Political
   Hopelessness},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2019},
Volume = {8},
Number = {1},
Pages = {108-142},
Month = {JAN 1},
Abstract = {Promised power, many people in our democratic age find their lives swept
   away by a factory owner's signature or their way of life eradicated by
   military violence. Dashed hopes produce anxiety and despair. Cornel West
   puts it succinctly: ``despair and hope are inseparable.{''} In this
   article, I argue that Bruce Springsteen's Nebraska explores the despair
   produced by the failure of hopeful agency and responds to that danger
   with a counterconception of agency (influenced by horror and sound) that
   conceives of a world in which objects actively participate. Ghostly
   sounds rise from earthy fields (as he sings on ``My Father's House{''}),
   calling to action with neither hope nor despair.},
