@article{ WOS:000564654400002,
Author = {Altamirano Acevedo, Hugo},
Title = {Characterising the Colombian elite from leftist leaders: From La
   Violencia to post-conflict period},
Journal = {CIUDAD PAZ-ANDO},
Year = {2019},
Volume = {12},
Number = {2},
Pages = {119-129},
Month = {JUL-DEC},
Abstract = {In Colombia, La Violencia was a period of aggravated crime that occurred
   during the 20th century, particularly between the 1940s and 1950s. This
   article analyses the Colombian leftist leaders from the end of the 20th
   century to the beginning of the 21st century by studying the
