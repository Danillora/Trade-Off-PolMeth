@article{ WOS:000472096700004,
Author = {Nieman, Mark David and Thies, Cameron G.},
Title = {Property Rights Regimes, Technological Innovation, and Foreign Direct
   Investment},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2019},
Volume = {7},
Number = {3},
Pages = {451-469},
Month = {JUL},
Abstract = {We argue that democratic institutions influence property rights in
   attracting foreign direct investment (FDI) by providing: (1) a coherent
   logic to the property rights regime that is created in a state and (2) a
   legitimate way to manage conflicts that arise in dynamic economies. We
   expect that the marginal effect of property rights in attracting FDI has
   increased over time with the rate of technological dynamism. We test
   this using a non-nested multilevel modeling strategy with random
   coefficients on data from 1970 to 2009. Our results demonstrate that the
   effect of property rights on attracting FDI is contingent on democratic
   institutions and that this effect becomes more pronounced over time.
   This effect holds for both developing and developed countries across all
   regions.},
