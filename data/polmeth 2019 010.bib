@article{ WOS:000459790400006,
Author = {Kim, In Seong and Cho, Hyun Koo},
Title = {Shutting Down the Kaesong Industrial Complex: A ``Coalitional
   Bargaining{''} Framework},
Journal = {ASIAN POLITICS \& POLICY},
