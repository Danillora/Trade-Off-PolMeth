@article{ WOS:000466762400008,
Author = {Jusko, Karen L. and Weisshaar, Katherine},
Title = {Are We Providing Enough to Those Who Have Too Little? Measuring Poverty
   Relief},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2019},
Volume = {7},
Number = {2},
Pages = {331-347},
Month = {APR},
Abstract = {This manuscript presents a new measure of safety net program
   effectiveness-a ``poverty relief ratio{''}-that is based on the
   estimated relationship between market income and social transfers, and
   reports the amount of income support provided, relative to the amount
   required to provide for all low-income households' basic needs. In an
   important advance over the standard poverty reduction rate measures, the
   poverty relief ratio preserves the rank order of observations across
   varying poverty thresholds. In this paper, we introduce this measure and
   demonstrate its validity by tracking major changes in federal policy and
   cross-state variation in safety net programs.},
