@article{ WOS:000486197600001,
Author = {Baldwin, Kate and Holzinger, Katharina},
Title = {Traditional Political Institutions and Democracy: Reassessing Their
   Compatibility and Accountability},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2019},
Volume = {52},
Number = {12, SI},
Pages = {1747-1774},
Month = {OCT},
Abstract = {This article revisits prominent frameworks for understanding traditional
   political institutions which make pessimistic assessments about their
   compatibility with democracy. Traditional political institutions are
   often assumed to be unaccountable because they are led by undemocratic
   leaders who are not subject to electoral sanctioning. However, drawing
   on new information from the TradGov Group dataset, an expert survey on
   the contemporary practices of more than 1,400 ethnic groups that
   currently have traditional political institutions, we show that these
   institutions contain their own distinct mechanisms of accountability. In
   a majority of cases, decision-making is consensual and leaders must
   account for their actions in various ways. We challenge the electoral
   accountability framework for understanding the quality of traditional
   leaders' performance, instead arguing that traditional political
   institutions can be compatible with democracy and even accountable to
   their citizens insofar as they adopt inclusive decision-making processes
   and their leaders have strong nonelectoral connections to the
   communities they represent.},
