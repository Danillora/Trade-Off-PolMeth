@article{ WOS:000511410700003,
Author = {Read, Roisin},
Title = {Comparing Conflict-related Sexual Violence: Expertise, Politics and
   Documentation},
Journal = {CIVIL WARS},
Year = {2019},
Volume = {21},
Number = {4, SI},
Pages = {468-488},
Abstract = {This paper explores knowledge production about sexual violence and
   highlights the role that comparison, across contexts and between
   different kinds of knowledge, plays in shaping what we know. It explores
