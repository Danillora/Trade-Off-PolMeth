@article{ WOS:000471828400019,
Author = {Ojeda, Christopher and Pacheco, Julianna},
Title = {Health and Voting in Young Adulthood},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2019},
Volume = {49},
Number = {3},
Pages = {1163-1186},
Month = {JUL},
Abstract = {Do changes in health lead to changes in the probability of voting? Using
   two longitudinal datasets, this article looks at the impact of three
   measures of health - physical health, mental health and overall
   well-being - on voting trajectories in young adulthood. The results show
   that self-rated health is associated with a lower probability of voting
   in one's first election, depression is related to a decline in turnout
   over time and physical limitations are unrelated to voting. Some
   familial resources from childhood are also found to condition when the
   health-participation effect manifests.},
