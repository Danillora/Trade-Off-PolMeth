@article{ WOS:000458336100004,
Author = {Ramirez Herrera, Mariana},
Title = {The Measurement of Well-Being Caught between Tradition and Modernity in
   Mexico's Poverty Reduction Strategy},
Journal = {POLITICS \& POLICY},
Year = {2019},
Volume = {47},
Number = {1, SI},
Pages = {79-104},
Month = {FEB},
Abstract = {In today's Mexico, more than half of the population lives in a state of
   precariousness. This reality highlights the dilemma between maintaining
   a traditional scheme of poverty reduction which assumes that human needs
   can be solved economically, with only limited success, or promoting a
   contemporary model of subjective well-being that has not yet proven its
   effectiveness. The main test facing both approaches is operational,
   hence agreeing on a method of identification and aggregation of the
   affected population is a key element for designing public policies that
   can sustain true well-being; a second aspect has to do with the
   coherence between theoretical ground and methodological design. Either
   way, measurement of well-being and identification of human needs pose an
   important challenge for public policies in Mexico, where poverty has
   been consistently underestimated. Thus, there is an urgent need for
   rethinking the social model of public policy in favor of a pro-poor
   vision.},
