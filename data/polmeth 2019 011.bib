@article{ WOS:000489307700010,
Author = {De Leon, Zoila Ponce and Ayala, Luis Garcia},
Title = {Peru 2018: Political Precariousness in the Times of Lava Jato},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2019},
Volume = {39},
Number = {2},
Pages = {341-365},
Abstract = {This article presents an analysis of the main political events of 2018.
   After the resignation of Pedro Pablo Kuczynski (PPK), the first year of
   Martin Vizcarra's government began. His government faced numerous
   scandals triggered by the Lava Jato case, resulting in the detention of
   key politicians (including former presidents PPK and Ollanta Humala) and
   the transformation of the three branches of government. Moreover, a
   series of audio recordings uncovered a network of illegal
   under-the-table practices within the Judiciary, which included the
   protection of politicians and the negotiation of positions and reduction
   of penalties. In response, Vizcarra's government promoted a group of
   institutional reforms via referendum, which are set in a context of
   institutional precariousness.},
