@article{ WOS:000514932300002,
Author = {Mendes, Isa},
Title = {Inclusion and Political Representation in Peace Negotiations: The Case
   of the Colombian Victims' Delegations},
Journal = {JOURNAL OF POLITICS IN LATIN AMERICA},
