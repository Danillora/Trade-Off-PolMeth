@article{ WOS:000492781000008,
Author = {MacAllister, James},
Title = {Utopianism of the Present: MacIntyre on Education and the Virtue of Hope},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2019},
Volume = {9},
Number = {3},
Pages = {436-446},
Abstract = {In this paper, I explore some of Maclntyre's ideas concerning utopia,
   education, and hope. I firstly analyse Maclntyre's suggestion that
   teachers are the forlorn hope of Western culture. I argue that the
   rather pessimistic tone of this conclusion is only seemingly (and not
   actually) at odds with what Maclntyre says elsewhere about the potential
   of education to challenge the worst excesses of advanced capitalism. To
   help build this argument, I secondly examine what Maclntyre says about
   the social virtue of hope in Marxism and Christianity. There he
   documents some significant concerns with Marxist ideology but
   nonetheless concludes that Marxism is the only project that can
   re-establish hope as a social virtue. I thirdly document how Maclntyre
   continues to champion the virtue of hope in his more recent work.
   However, the hue of this virtue has become both Marxist and
   Aristotelian. He now holds that a ``utopianism of the present{''} is
   needed to combat advanced capitalism. While utopians of the future
   sacrifice away the possibility of learning how to transform the present,
   utopians of the present refuse to make this sacrifice. Maclntyre's later
   work suggests he has renewed hope that a practice-based education can
   contribute to a utopianism of the present: a utopianism that can
   challenge the iniquities and distortions generated by market based
   economies.},
