@article{ WOS:000482295700011,
Author = {Sevi, Semra and Arel-Bundock, Vincent and Blais, Andre},
Title = {Do Women Get Fewer Votes? No.},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2019},
Volume = {52},
Number = {1},
Pages = {201-210},
Month = {MAR},
Abstract = {We study data on the gender of more than 21,000 unique candidates in all
   Canadian federal elections since 1921, when the first women ran for
   seats in Parliament. This large data set allows us to compute precise
   estimates of the difference in the electoral fortunes of men and women
   candidates. When accounting for party effects and time trends, we find
   that the difference between the vote shares of men and women is
   substantively negligible (+/- 0.5 percentage point). This gender gap was
   larger in the 1920s (+/- 2.5 percentage points), but it is now
   statistically indistinguishable from zero. Our results have important
   normative implications: political parties should recruit and promote
   more women candidates because they remain underrepresented in Canadian
   politics and because they do not suffer from a substantial electoral
   penalty.},
