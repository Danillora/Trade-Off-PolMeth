@article{ WOS:000461651400023,
Author = {Bernhard, Bill and Lyne, Mona},
Title = {When the Shepherd Rejoins the Flock: Returning to the Department after
   Serving as Chair},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2019},
Volume = {52},
Number = {2},
Pages = {293-295},
Month = {APR},
Abstract = {Reassuming the role of a ``regular{''} faculty member after serving as
   department chair presents a common set of challenges that often are
   overlooked and underappreciated in the discipline. To provide former
   chairs with guideposts for navigating this pivotal moment in their
   career, we offer a few thoughts on what our experience suggests are the
   keys to a successful transition. Among the issues to consider are time
   and space to regroup, mining what you have learned as chair to revamp
   research and teaching, careful management of your new social status in
   the department, and envisioning how you can lead without holding a
   formal position.},
