@article{ WOS:000472539300009,
Author = {Bandelow, Nils C. and Hartmann, Anja and Hornung, Johanna},
Title = {Winter is Coming - But Not Yet. German Health Policy Under the Third
   Merkel Chancellorship},
Journal = {GERMAN POLITICS},
Year = {2019},
Volume = {28},
Number = {3, SI},
Pages = {444-461},
Month = {JUL 3},
Abstract = {Health policy in Germany for a long time was considered to be hardly
   reformable. During the last 25 years, reforms became more frequent, yet
   substantial major policy change is still rare. When taking a closer
   look, German health policy follows phases of stability alternating with
   punctuations of this equilibrium when fundamental structural reforms
   occur. This contribution analyses the German health policy of the 18th
   legislative period against the backdrop of this newly emerging pattern
   of health policy making by applying Punctuated Equilibrium Theory (PET).
   Marginal reforms without profound reorganisation of the health system
   were only possible in this period because of the particularly favourable
   financial situation of sickness funds. Consequently, the conflicting
   parties and interest groups were able to postpone fundamental solutions
   to upcoming problems to a later point in time. After explaining the
   marginal reforms under the third Grand Coalition, it becomes clear that
   the dominant equilibrium is not going to last: Winter is coming and will
   bring about concussions in German health policy soon.},
