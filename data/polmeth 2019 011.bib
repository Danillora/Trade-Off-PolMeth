@incollection{ WOS:000557315900004,
Author = {Kalpokas, Ignas},
Book-Author = {Kalpokas, I},
Title = {Personalisation, Emotion, and Nudging},
Booktitle = {ALGORITHMIC GOVERNANCE: POLITICS AND LAW IN THE POST-HUMAN ERA},
Year = {2019},
Pages = {49-65},
Abstract = {Pleasure is undeservedly excluded from studies of algorithmic
   governance. Nevertheless, due to the incessant competition over
   attention, prevalent in today's media environment, the maximisation of
   pleasure and consumer satisfaction becomes a must in order to be able to
   exert the power of algorithmic governance in the first place. Therefore,
   the first part of this chapter is dedicated to a discussion of the
   importance of enthralling one's audience and the role of data therein.
   The second part, meanwhile, is focused on nudging strategies that are
   geared towards encouraging individuals to make predefined choices.
   However, in today's datafied, pleasurised, and personalised environment,
   nudging goes beyond mere encouragement: As showed in this chapter,
   options can be stacked in such a way that individuals simply cannot fail
   to choose the option intended by the choice architect.},
