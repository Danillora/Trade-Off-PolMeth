@article{ WOS:000479144600002,
Author = {Lopez Garcia, Arturo Cesar and Diaz Gonzalez, Heli Hassan},
Title = {Implementation of the Special Food Security Program in Mexico and
   Guatemala},
Journal = {CIMEXUS},
Year = {2019},
Volume = {14},
Number = {1},
Pages = {33-57},
Month = {JAN-JUN},
Abstract = {This qualitative paper is intended to document, through a descriptive
   and comparative study, the process of implementation of the Programa
   Especial de Seguridad Alimentaria in Mexico and Guatemala. To do so, a
   literature review was conducted. The results show that, despite being
   the same program and that both scenarios share an evident and extensive
   problem in terms of poverty, especially in rural areas, the PESA program
   was implemented differently in both countries, a process that was shaped
   by the different legal instruments, as well as the degree of involvement
   of the different actors in each of the scenarios.},
