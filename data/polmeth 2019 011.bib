@article{ WOS:000480512200002,
Author = {Duval, Dominic},
Title = {Ringing the alarm: The media coverage of the fulfillment of electoral
   pledges},
Journal = {ELECTORAL STUDIES},
Year = {2019},
Volume = {60},
Month = {AUG},
Abstract = {This article investigates the portrayal of electoral pledges in the news
   media. We know very little about the portrayal of electoral pledges in
   news media. This is problematic in view of the fact that most citizens
   obtain their information about electoral pledges from the media. More
   precisely, this article investigates whether the media alert citizens
   when a pledge is broken. It covers the 244 pledges made by the governing
   party, the Conservative Party, during the 2011 and 2008 Canadian
   elections. Our period of analysis ranges from the 2008 election
   (07/09/2008) to the end of the 2011 mandate (08/04/2015). The study
   finds that the news media do alert citizens when a pledge is broken and
   that what is often described as the ``burglar alarm model{''} in
   political communication provides an apt description of the dynamics at
   play in the coverage of electoral pledges.},
