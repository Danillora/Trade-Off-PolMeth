@article{ WOS:000469887800004,
Author = {Gil, Cameron Michael},
Title = {Spatial analysis of La Republique En Marche and French Parties,
   2002-2017},
Journal = {FRENCH POLITICS},
Year = {2019},
Volume = {17},
Number = {2},
Pages = {184-210},
Month = {JUN},
Abstract = {This article uses spatial analysis and regressions of party manifestos
   to investigate possible contributing factors of President Macron's novel
   political party, La Republique En Marche (LREM), success in the 2017
   legislative elections, particularly if LREM sits in a unique spatial
   position among French political parties in the wake of traditional,
   mainstream parties vacating the center position. This study uses both
   the economic left-right axis and the post-materialist, GAL-TAN axis
   accompanied by an analysis of the degree to which each party expresses
   sentiment about Europe. This approach permits a unique analysis of
   legislative elections since 2002 in order to set the stage for a
   multifaceted analysis of the 2017 spatial arrangement. Given the
   peculiarities of 2017, this study also attempts to examine the electoral
   momentum of President Macron's victory as a contributing factor to
   LREM's success. Ultimately, LREM is shown to exist in a unique position
   among French political parties, particularly in the wake of all
   traditional parties vacating the center point and the right wing having
   no proximal competition to LREM. Portions of the results point to a
   possible shift in the saliency of ideology as well as a new system
   polarity emerging.},
