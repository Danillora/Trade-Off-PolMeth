@article{ WOS:000454594800003,
Author = {True, Jacqui and Riveros-Morales, Yolanda},
Title = {Towards inclusive peace: Analysing gender-sensitive peace agreements
   2000-2016},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2019},
Volume = {40},
Number = {1},
Pages = {23-40},
Month = {JAN},
Abstract = {The presence of gender provisions in peace agreements affects women's
   participation in post-conflict societies as well as the chances that a
   post-conflict society will move towards gender equality. While there is
   an overall upward trend in the number of references to women's rights
   and gender equality in peace agreements, gender-sensitive agreements are
   not a given. Why and how are peace agreements with gender provisions
   adopted? We use statistical analysis to explain why some peace
   agreements adopt gender provisions while others have no such provisions.
   Based on an analysis of 98 peace agreements across 55 countries between
   2000 and 2016, we find that peace agreements are significantly more
   likely to have gender provisions when women participate in elite peace
   processes. Our study also shows that the likelihood of achieving a peace
   agreement with gender provisions increases when women's representation
   in national parliaments increases and when women's civil society
   participation is significant.},
