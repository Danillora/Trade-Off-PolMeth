@article{ WOS:000472979800004,
Author = {Pappi, Franz Urban and Kurella, Anna-Sophie and Braeuninger, Thomas},
Title = {Entry of new Parties into Parliament. Voter Preferences as a
   Precondition for the Success of the AfD in 1987, and the Greens in 1986},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2019},
Volume = {60},
Number = {2},
Pages = {273-298},
Month = {JUN},
Abstract = {We study the entry of new parties into parliament. New parties
   politicize issues that have so far been neglected by propagating an
   extreme policy. Whether they succeed depends on public attention to the
   new issue and the party's issue ownership. Decisive are voter attitudes
   that have to be polarized with respect to both positions on and salience
   of the new issue. We apply the model to the issue of migration policy
   following the refugee crisis of 2015/16 and nuclear energy policy after
   the Chernobyl accident of 1986. Applying asalience weighted spatial
   model we predict voters' party preferences for the established parties
   and the respective new party (Alternative fur Deutschland, Grune)
   successfully.},
