@incollection{ WOS:000475728100007,
Author = {Urbinati, Nadia},
Editor = {Levi, M and Rosenblum, NL},
Title = {Political Theory of Populism},
Booktitle = {ANNUAL REVIEW OF POLITICAL SCIENCE, VOL 22},
