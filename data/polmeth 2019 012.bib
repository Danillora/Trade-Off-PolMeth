@article{ WOS:000498134000022,
Author = {Loughlin, Martin},
Title = {What would John Griffith have made of Jonathan Sumption's Reith
   Lectures?},
Journal = {POLITICAL QUARTERLY},
Year = {2019},
Volume = {90},
Number = {4},
Pages = {785-793},
Month = {OCT},
Abstract = {In his 2019 Reith Lectures on the rise of law and decline of politics,
   Jonathan Sumption presents a thesis that, on its face, seems identical
   to that of J.A.G. Griffith's defence of the political constitution.
   Given the radical differences in their views on equality, democracy, and
   redistribution-with Griffith working in the tradition of democratic
   socialism espoused by the Webbs, Tawney and Laski, and Sumption
   expressing the libertarian philosophy underpinning Thatcherite
   policies-this is puzzling. This article sets their views in historical
   and political context and argues that the similarities are superficial,
   whereas the differences are profound. It then proceeds to show the
   weaknesses in Sumption's defence of his thesis.},
Publisher = {WILEY},
