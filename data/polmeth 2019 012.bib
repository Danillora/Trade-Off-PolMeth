@article{ WOS:000461576100004,
Author = {Pierson, Claire},
Title = {Gendering peace in Northern Ireland: The role of United Nations Security
   Council Resolution 1325 on women, peace and security},
Journal = {CAPITAL AND CLASS},
Year = {2019},
Volume = {43},
Number = {1, SI},
Pages = {57-71},
Month = {MAR},
Abstract = {United Nations Security Council Resolution 1325 on `women, peace and
   security' was passed in 2000 to recognise and enhance women's
   participation in peace-building. The Resolution has growing global
   significance in conflicted societies yet there is limited analysis of
   its implementation in specific social contexts. Utilising feminist
   theory on gender in conflicted societies and original empirical evidence
   from key grassroots community activists in Northern Ireland, I will
   consider the potential of the 1325 framework as a tool for
   conceptualising and achieving gender security and equality. This article
   contributes to an understanding of the importance of deep contextual
   interpretation for implementation of the women, peace and security
   agenda and argues for a feminist intersectional interpretation of the
   Resolution to enable its transformative potential for both
   peace-building and gender equality.},
