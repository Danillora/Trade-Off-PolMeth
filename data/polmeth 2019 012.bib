@article{ WOS:000485347200001,
Author = {Lee, Jongho and Boeckelman, Keith and Hardy, Richard J. and Davis, Kyle},
Title = {Positive Messages as a Motivator for Seeking Information about
   Candidates},
Journal = {POLITICS \& POLICY},
Year = {2019},
Volume = {47},
Number = {5},
Pages = {877-901},
Month = {OCT},
Abstract = {What, if anything, will motivate people to seek information about
   candidates during campaigns? Our study tackles this question drawing on
   data from a survey experiment conducted with a total of 1,421 college
   student subjects. Study participants were randomly divided into four
   groups, each of which was asked to read one of four brief statements
   about the candidacy of either Hillary Clinton or Carly Fiorina written
   in an either positive or negative tone. Study subjects were then asked
   if they would like to learn more about the candidate. The study reveals
   that those who were exposed to positive information about the
   presidential candidates or already had positive emotions toward either
   of them were more likely to express their desire to learn more about the
   candidates. The findings are consistent with the ``broaden-and-build
   theory of positive emotions,{''} which predicts an individual tendency
   to broaden their ``thought-action repertoires{''} when experiencing
   positive stimuli.},
