@article{ WOS:000497105700001,
Author = {Bazurli, Raffaele and Portos, Martin},
Title = {Crook!: The impact of perceived corruption on non-electoral forms of
   political behaviour},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
