@article{ WOS:000504933000002,
Author = {Chhibber, Pradeep and Verma, Rahul},
Title = {The Rise of the Second Dominant Party System in India: BJP's New Social
   Coalition in 2019},
Journal = {STUDIES IN INDIAN POLITICS},
Year = {2019},
Volume = {7},
Number = {2, SI},
Pages = {131-148},
Month = {DEC},
Abstract = {The social coalition that supported the Bharatiya Janata Party in 2019
   mirrored the demographic profile of the Hindu society. The party made
   substantial gains among the lower castes, the poor, rural voters, and
   less educated. How did BJP manage to attract these new voters? We argue
   that the immediate context of 2019 elections along with a profound
   ideological shift in Indian politics lies at the heart of the BJP's
   success. Underpinning the short-term factors of Modi's popularity, BJP's
   organizational advantage, heightened nationalistic sentiments, and
   expansive welfare politics, a new form of political majoritarianism
   delinked from religious Hindu nationalism was key to the party's ability
   to attract new voters.},
