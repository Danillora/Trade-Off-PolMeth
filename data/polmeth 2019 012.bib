@article{ WOS:000498134000006,
Author = {Ichijo, Atsuko},
Title = {What has the Brexit Process done to Scotch Whisky?},
Journal = {POLITICAL QUARTERLY},
Year = {2019},
Volume = {90},
Number = {4},
Pages = {637-644},
Month = {OCT},
Abstract = {The United Kingdom's membership of the European Union does not have any
   impact on Scotch whisky itself: Brexit or not, Scotch whisky is Scotch
   whisky. However, it features more prominently in the Brexit negotiations
   than, for example, beer. This is because Scotch whisky is a highly
   export-oriented product which brings large economic benefits to the
   UK-particularly Scotland-and its brand is protected by the international
   regime of intellectual property rights protection, especially, by the
   geographical indications regime championed by the EU. Moreover, the
   Brexit negotiations have led to the resurfacing of another political
   issue which affects Scotch whisky: Scottish independence. As a strong
   territorial brand, the constitutional status of Scotland is highly
   relevant to Scotch whisky and the investigation into the impact of the
   Brexit process on Scotch whisky has highlighted the inter-connectedness
   of various political issues.},
