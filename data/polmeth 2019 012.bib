@article{ WOS:000493508400003,
Author = {Duzenli, Faruk Eray},
Title = {``Is There No Alternative?{''}: An Overdeterminist Marxian Class
   Analysis of International Political Economy},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2019},
Volume = {31},
Number = {4},
Pages = {426-428},
Abstract = {David F. Ruccio's Development and Globalization is a collection of
   remarkable interventions, elegantly articulated, into debates over
   socialist planning, Third World development, and capitalist
   globalization from a postmodernist Marxian class-analytical perspective.
   Ruccio's analyses are both theoretical and empirical. They also provide
   superb examples of how the focus in the field of international political
   economy has changed over time, from planning to development and
   eventually to globalization. Yet this does not mean that these essays
   are a thing of the past; they not only provide the questions and the
   methodology with which economists should do research but also remind us
   why it is important for us to think, in new ways, about international
   political economy.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
