@article{ WOS:000501624900020,
Author = {Kahan, Alan S. and Marshall, Catherine and Nowicki, Joanna},
Title = {THE EFFECTS OF CAPITALISM ON PEOPLE, PERSONALITY AND CULTURE},
Journal = {TOCQUEVILLE REVIEW},
Year = {2019},
Volume = {40},
Number = {2},
Pages = {7-16},
Abstract = {The 19 essays in this collection present an international and
   pluridisciplinary approach to the relation between capitalism,
   personality and culture, from the eighteenth century to the present. In
   Part I: Capitalism andSocial Thought, they look primarily at social and
   economic thought, and inPart II: Representations of Commerce and
   Capitalism, they examine film, theatre, music, television, and
   journalism. By treating perspectives as widelyseparated as those of
   Montesqueiu, Marx, Rand, and Wajda, and examining representations from a
   number of countries and languages, including France, the United States,
   Poland, Hungary, Italy and the UK, the essays present acontrasting and
   rapidly shifting portrait of the meaning of some of the mostcontested
   terms in our present-day politics and society.},
