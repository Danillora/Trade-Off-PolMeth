@article{ WOS:000544382900001,
Author = {Bergh, Johannes and Christensen, Dag Arne and Matland, Richard E.},
Title = {When is a Reminder Enough? Text Message Voter Mobilization in a European
   Context},
Journal = {POLITICAL BEHAVIOR},
Year = {2021},
Volume = {43},
Number = {3},
Pages = {1091-1111},
Month = {SEP},
Abstract = {Sending text messages reminding people to vote has only been examined as
   a mobilization tool in three studies, two of them done in the United
   States. The results from these studies are mixed. We investigate this
   tool's effectiveness using a field experiment in a different context,
   municipal elections in Norway. We find strong mobilization effects among
   groups traditionally low in participation (immigrants and youth voters).
   Young native Norwegians show an intent-to-treat (ITT) effect of 4.58\%
   points, foreign nationals who have recently established Norwegian
   residency show an ITT effect of 2.93\% points, and among immigrants who
   have lived in Norway for 7 years or more the treatment effect is 2.7\%
   points. Even among native Norwegian voters over 30, with a baseline
   turnout of 73\%, text messages generate a 0.96\% points increase in
   turnout.},
