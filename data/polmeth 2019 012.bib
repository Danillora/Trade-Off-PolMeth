@article{ WOS:000503032600006,
Author = {Dewey, Susan and Hankel, Jennifer and Brown, Kyria},
Title = {Affective governance among street-involved women and their services
   providers},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2019},
Volume = {12},
Number = {3, SI},
Pages = {390-408},
Abstract = {Affect modulates and directs the governance of street prostitution in
   encounters between street-involved women and their social services and
   healthcare providers through three nodes on a circuit of affect. The
   first is the street, constituting a set of geographic and psycho-social
   phenomena that exert a formative and often intergenerational impact on
   street-involved women's lives. The second, selfhood, connects an
   understanding of trauma as inhibitory to cognitive development with the
   governmentality of mental health diagnoses. The third, embodied
   abjection, links the physical states and practices connected with
   addiction, homelessness, incarceration, and other forms of social
   suffering, with street-involved women's abjection.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
