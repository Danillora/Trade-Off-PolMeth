@incollection{ WOS:000516526400013,
Author = {Moe, Arild and Stokke, Olav Schram},
Editor = {Safronova, EI},
Title = {CHINA AND ARCTIC SHIPPING: POLICIES, INTERESTS AND ENGAGEMENT'},
Booktitle = {CHINA IN WORLD AND REGIONAL POLITICS: HISTORY AND MODERNITY},
Series = {Kitaj v Mirovoj i Regional noj Politike Istoria i Sovremennost},
Year = {2019},
Volume = {24},
Pages = {257-278},
Abstract = {China is widely perceived as an emerging key actor in Arctic shipping.
   The purpose of the article is to clarify how strong China's engagement
   really is; how much of it is purely commercial, and how much is a
   reflection of political goals. We take a bottom-up approach, examining
   use of arctic sea routes from the perspectives of Chinese governmental
   agencies and companies, but we also place shipping in the context of
