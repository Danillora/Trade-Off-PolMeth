@article{ WOS:000504687800001,
Author = {Ghosh, Cyril},
Title = {De-moralizing gay rights: a reply to my critics},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2021},
Volume = {24},
Number = {7},
Pages = {1091-1099},
Month = {NOV 10},
Abstract = {In this response to my critics, I offer a set of clarifications,
   acknowledgements, and defenses. I clarify, among other things, why my
   deployment of the term `queer' is narrow, and at times, succumbs to
   law's `legibility imperative'; why the concept of gay culture(s) I
   engage with is/are US-centric; and that I am not opposed to a more
   `complete' dialectical exercise than the one I have presented in my
   discussion of radical theory creep. In addition, in response to one of
   my critics, I offer some tentative remarks about the task of stipulating
   some definitional parameters to identify RTC. I acknowledge that my
   analysis centers the experience of gay men, to the occlusion of others,
   and that I have proposed at least one false equivalence in my work.
   Among the defenses I present, I explain why I cannot accept a definition
   of oppression drawn from Ann Cudd's work; and why I cannot agree with
   Ann Cudd's idea of a duty to resist oppression (at least not in the way
   she formulates it).},
