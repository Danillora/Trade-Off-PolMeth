@article{ WOS:000493508400005,
Author = {Amariglio, Jack},
Title = {``Capitalism Has a Conjunctural History but No Necessary Trajectory{''}:
   A Comment on David Ruccio's Development and Globalization},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2019},
Volume = {31},
Number = {4},
Pages = {432-440},
Abstract = {David Ruccio's book Development and Globalization is a watershed in how
   to write Marxian conjunctural history. Ruccio employs the notion and
   practice of conjunctural analysis through his use of concrete Marxian
   class analysis. His essays, written in the thirty-year period leading up
   to the 2007-8 Great Recession, are concerned with the problems of
   planning in transitional economies, of development and the abolition of
   capitalist class exploitation, and of globalization as a stand-in for
   imperialism. In each chapter, Ruccio resists the modernist Marxian urge
   to make all aspects of existing societies a manifestation of given
   `'laws of motion{''} of capitalism. In its place, Ruccio's conjunctural
   analyses successfully open up the space for seeing and analyzing class,
   state, and other social processes and subjectivities that are multiple,
   hybrid, and heterogeneous.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
