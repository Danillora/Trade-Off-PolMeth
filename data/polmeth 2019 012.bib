@article{ WOS:000475493800008,
Author = {Patty, John W. and Penn, Elizabeth Maggie},
Title = {Are Moderates Better Representatives than Extremists? A Theory of
   Indirect Representation},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2019},
Volume = {113},
Number = {3},
Pages = {743-761},
Month = {AUG},
Abstract = {Few, if any, elected representatives are capable of unilaterally
   implementing their platforms. Rather, they choose between options
   generated by other actors and/or external events. We present a theory of
   voters' preferences over representatives who will cast votes on their
   behalf, and show that in this setting voters' preferences over
   candidates' platforms will not look like voters' preferences over
   policies. We demonstrate that these induced preferences for
   representation tend to favor more extreme representatives, and we
   present two models of electoral competition in which induced preferences
   over representatives lead to elite polarization.},
