@article{ WOS:000468370500004,
Author = {Duggan, Alan},
Title = {Irish presidential election 2018},
Journal = {IRISH POLITICAL STUDIES},
Year = {2019},
Volume = {34},
Number = {2},
Pages = {303-314},
Month = {APR 3},
Abstract = {In late August 2018, Minister for Housing, Planning and Local Government
   Eoghan Murphy set 26 October as the date of Ireland's presidential
   election. The race took on a decidedly uncompetitive appearance in July
   2018 when incumbent Michael D. Higgins finally confirmed he would stand
   for re-election. He had, in the end, comfortably won the previous
   contest in 2011, receiving almost 40 per cent of votes on the first
   count and 57 per cent on the final count {[}see O'Malley, 2012.
   Explaining the 2011 Irish presidential election: Culture, valence,
   loyalty or punishment? Irish Political Studies, 27(4), 635-655]. Polls
   before the campaign revealed 59-76 per cent of people supported him for
   a second term, and he occupied a consistently comfortable position in
   polling carried out during the campaign. In the end, it was neither the
   unconcerned demeanour of Higgins nor his record-breaking victory that
   grabbed the headlines. Rather it was the injection of populist discourse
   into the campaign by candidate Peter Casey and his unanticipated success
   in garnering nearly a quarter of the vote that shocked the race out of
   its perfunctory rhythm. The nomination of candidates by councils, the
   negative campaigning against the incumbent and the populist discourse of
   Casey became the talking points of what was otherwise thought to promise
   a dreary foregone conclusion.},
