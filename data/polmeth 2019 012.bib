@article{ WOS:000490940300011,
Author = {Scholte, Jan Aart},
Title = {Civil society and global governance: Exploring transscalar connections},
Journal = {INTEREST GROUPS \& ADVOCACY},
Year = {2019},
Volume = {8},
Number = {3, SI},
Pages = {490-498},
Month = {SEP},
Abstract = {This commentary on the special issue `Advocacy Groups in Global
   Governance: Global and Domestic Opportunity Structures' makes four main
   points. First, one can reflect critically on the framing concepts of
   `advocacy groups,' `global governance,' and `opportunity structures.'
   Second, research on civil society and global governance needs to give
   serious attention to national as well as global dynamics. Third, studies
   need fully to examine the interrelation of national and global spheres
   in world politics; the two are not ontologically discrete. Fourth, it is
   important to dissect both the national and the global, particularly in
   order to expose the social stratifications that shape these arenas.},
