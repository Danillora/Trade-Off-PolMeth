@article{ WOS:000507234200010,
Author = {Jakubowski, Jakub},
Title = {Euroscepticism in a Pro-European State on the Basis of Media Content
   Analysis},
Journal = {BALTIC JOURNAL OF EUROPEAN STUDIES},
Year = {2019},
Volume = {9},
Number = {4},
Pages = {218-236},
Month = {DEC},
Abstract = {Against the background of research conducted in other EU countries,
   Euroscepticism is a marginal sentiment among Poles. Nevertheless, this
   attitude and the arguments associated with it are noticeable in many
   areas in the public space: among members of the public presenting
   Eurosceptic arguments on the social media, journalists in press
   commentaries or politicians themselves. The aim of this article is to
   analyse the scale of such statements, their specific character and
   presence in the Polish press during the election and post-electoral
   period in 2015-2017. The study involved content analysis of selected
   text units which allowed the fundamental research question to be
   answered-namely, what is the nature of Eurosceptic attitudes manifested
   in the Polish press during the electoral/post-electoral period of lively
   discussions on the role of the European Union in Poland.},
