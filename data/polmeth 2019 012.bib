@article{ WOS:000449338500003,
Author = {Delazari, Fagner and Nascimento dos Santos, Daiana},
Title = {Migration, story and decolonization in Brazil from Que horas ela
   volta? (The Second Mother)},
Journal = {IZQUIERDAS},
Year = {2019},
Volume = {46},
Pages = {47-64},
Month = {MAY},
Abstract = {This paper intends to analyze the brazilian movie The second mother (Que
   horas ela volta?, directed by Anna Muylaert), from the ideas of the
   postcolonial criticism, specially the notions of coloniality (Quijano),
   subalternity (Spivak), and decolonization of the imaginary (Akassi). We
   propose to investigate the aesthetic, political and social aspects
   present in this fiction movie, that allow us to affirm that the
   character Jessica represents an effort of rupture in relation to the
   present coloniality in the Brazilian social relations, especially with
   regard to the situation of northeastern women and domestic workers at
   the beginning of the 21st century.},
