@article{ WOS:000504019400013,
Author = {Gilchenco, Olena and Konstantinova, Yulia and Pashina, Natalia},
Title = {Public Influence on Legislative Process in the Problem of De-occupation
   of Donbas},
Journal = {BALTIC JOURNAL OF EUROPEAN STUDIES},
