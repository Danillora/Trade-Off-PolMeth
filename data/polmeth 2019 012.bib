@article{ WOS:000487028700004,
Author = {Elfversson, Emma},
Title = {The Political Conditions for Local Peacemaking: A Comparative Study of
   Communal Conflict Resolution in Kenya},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2019},
Volume = {52},
Number = {13-14},
Pages = {2061-2096},
Month = {NOV-DEC},
Abstract = {How does government bias affect prospects for peace agreements in
   communal conflicts? Government bias has been shown to have a strong
   impact on the incidence and dynamics of localized ethnic conflict, but
   the way that it affects conflict resolution remains underexplored. I
   argue that government bias makes the conflict parties less likely to
   overcome the commitment problem, because they cannot trust the
   government's willingness to guarantee or uphold any agreement they
   reach. Consequently, bias reduces the chances that the parties are able
   to reach a peace agreement. A systematic comparison of four cases in
   Kenya provides support for this argument. I also distinguish between
   bias related to strategic interest and bias related to relationships,
   and find that the former is more durable, whereas the latter is more
   likely to be influenced by political turnover, thereby opening up
   possibilities for peacemaking.},
