@article{ WOS:000494429800001,
Author = {Hanagan, Nora},
Title = {The Citizen, the Baker, and the Candlestick Maker: What Democrats Can
   Learn from the Arts and Crafts and Slow Food Movements},
Journal = {AMERICAN POLITICAL THOUGHT},
