@article{ WOS:000477732500009,
Author = {Chapekis, Athena and Moore, Sarah M.},
Title = {The prosecution of ``others{''}: presidential rhetoric and the
   interrelation of framing, legal prosecutions, and the Global War on
   Terror},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2019},
Volume = {12},
Number = {3},
Pages = {533-553},
Abstract = {In examining the Global War on Terror, the effects of presidential
   rhetoric on the framing of terrorism has been well documented. However,
   little previous work links terrorism and its status as an ``othered{''}
   phenomenon to differential legal prosecution in a post-9/11 era. Using
   the Prosecution Project data set, we compared ``othered{''} individuals,
   as defined by a Muslim, Arab/Middle Eastern, and/or foreign-born status,
   to ``non-othered{''} individuals charged with terroristic felonies.
   Furthermore, we subdivided the dataset into three analytical time
   blocks: the George W. Bush administration immediately post-9/11, the
   latter half of the Bush administration, and the Obama administration.
   For the first and third time blocks, we found that ``othered{''}
   individuals were prosecuted significantly more frequently than
   ``non-othered{''} individuals. These findings call into question the
   effect of presidential rhetoric and the national framing of terrorism on
   the legal prosecution of ``othered{''} individuals.},
