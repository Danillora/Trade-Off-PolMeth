@article{ WOS:000472558300003,
Author = {Revilla Blanco, Marisa},
Title = {From ``Ni una mas{''} to \#NiUnaMenos: Women's and Feminism in Latin
   America},
Journal = {POLITICA Y SOCIEDAD},
Year = {2019},
Volume = {56},
Number = {1},
Pages = {47-67},
Abstract = {This article focuses on the analysis of the dynamics of women's and
   feminist movements in Latin America which strengthen their regional
   articulation, and their effects on the construction of social movements
   based on complex, plural and diverse processes of collective
   identification. I highlight two interrelated processes which contribute
   to explain these movements' current characteristics. Firstly, the
   diversification of identities (from indigenous and afro-descendent women
   to lesbian feminism). And, secondly, the diversification of spaces for
   participation, from street demonstrations to digital participation using
   APP, including audiovisual productions (videos, rap, hip-hop). These two
   processes favor the widening of actions' scope and diffusion, and a
   larger presence of women's and feminist movements in social virtual
   communities.},
