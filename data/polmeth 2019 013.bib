@article{ WOS:000472135100003,
Author = {Ortiz-Mena, Antonio and Wayne, Earl Anthony},
Title = {FROM NAFTA TO THE USMCA AS SEEN FROM THE SOUTHERN PARTNERSHIP},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2019},
Volume = {17},
Number = {4},
Pages = {25-36},
Month = {WIN},
Abstract = {The North America trade and production platform built under NAFTA's
   umbrella has re-oriented the economic relationship between Mexico,
   Canada, and the United States. Millions of jobs across the continent are
   supported by the 1.3 trillion USD of annual trade and significant cross
   border investment. The US-Mexico economic relationship has particularly
   expanded. The new United States-Mexico-Canada Agreement (USMCA) would
   preserve these beneficial economic ties and provide an important
   modernization of NAFTA. While the prospects for approval in the US
   remain unclear, preserving a three-way trade agreement remains very much
   in the interest ofMexico, the United States, and Canada.},
Publisher = {TURKISH POLICY QUARTERLY},
Address = {ASMALI MESCIT MH OTELLER SK NO 8 KAT 2, BEYOGLU-ISTANBUL, 00000, TURKEY},
