@article{ WOS:000492591100002,
Author = {Ahrens, Leo},
Title = {Equity financing, labor market mobility and the Varieties of Capitalism:
   the mobilizing impact of stock markets},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2019},
Volume = {13},
Number = {3},
Pages = {353-374},
Month = {SEP},
Abstract = {The Varieties of Capitalism (VoC) theory postulates that firms in
   coordinated market economies use credit financing, which enables them to
   offer stable employment relations and secures their production strategy.
   Liberal market economies, on the other hand, exploit complementarities
   between equity financing and flexible labor markets. However, it has
   become difficult to uphold this dichotomy because coordinated market
   economies have been increasingly using equity financing since the 1990s.
   Using macro-quantitative panel data from 24 OECD countries, 1993-2010,
   this study analyzes which impact this institutional change had on
   employment mobility. It tests two empirical expectations derived from
   VoC theory. The results show that, firstly, equity financing moderately
   mobilizes labor markets. Secondly, the association is not weaker in
   coordinated market economies than in liberal market economies; on the
   contrary, there is evidence for a stronger impact in coordinated market
   economies. This is inconsistent with the theoretical expectations of VoC
   theory because employers should react to institutional change by
   maintaining strategical coordination in long-term labor relations to
   secure their comparative advantage. The result implies that the VoC
   conceptualization of institutional complementarities is too simple when
   it characterizes a coherent organization of the economy as a liberal or
   coordinated market economy as an efficiency-maximizing equilibrium. This
   interpretation supports prior critique of VoC's view on institutional
   change, which was solely based on qualitative case studies before.},
Publisher = {SPRINGER VIEWEG-SPRINGER FACHMEDIEN WIESBADEN GMBH},
