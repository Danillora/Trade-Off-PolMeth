@article{ WOS:000493508400006,
Author = {Morton, Adam David},
Title = {Reading for Class},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2019},
Volume = {31},
Number = {4},
Pages = {441-458},
Abstract = {This article contends that the arguments developed within and beyond
   Development and Globalization: A Marxian Class Analysis are vital to the
   project of building twenty-first century socialism. My surveying of
   David Ruccio's scholarship as well as the wider Rethinking Marxism
   project argues that both are indispensable reading for class, in a
   twofold sense. First, such interventions are indispensable reading for
   class in that they shape classrooms on political economy that span
   teaching and research in and beyond the academy. Second, they are
   indispensable reading for class in delivering a Marxist social class
   analysis of planning, development and noncapitalist relations at a time
   when many want to expunge class from social and economic theory. My
   focus on this double meaning of class is articulated across four key
   areas of attention, namely: (1) class effects and state theory; (2)
   socialist planning; (3) the political rule of capital; and (4)
   deconstructing capitalism, where the last vantage point turns to
   critique as part of a wider survey of the Rethinking Marxism legacy.
   Regardless of such critique-not so much the ``ruthless criticism{''} of
   all that exists but a benevolent criticism-my conclusion is that the
   radical legacy of Development and Globalization is vital to constructing
   a world of alternative class possibilities.},
