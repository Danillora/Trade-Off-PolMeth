@article{ WOS:000488462000002,
Author = {Sun, Xin},
Title = {Power Sharing, Patronage, and Land Institutions in China},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2019},
Volume = {66},
Number = {5},
Pages = {315-328},
Month = {SEP 3},
Abstract = {This article examines how single-party regimes share power among their
   ruling elites. An examination of land institutions in China suggests
   that the ruling Party relies on a statist and partially centralized land
   regime to distribute land development rights (land quotas). This
   institutional arrangement allows the Party's central elites to use land
   as patronage to build their clientelistic networks and share power among
   themselves. This argument is tested empirically using a data set of
   land-use conversion in Chinese provinces between 1999 and 2013. The
   findings of the research contribute to the literature on
   authoritarianism and land politics, and have important implications for
   land-institution reforms in China.},
