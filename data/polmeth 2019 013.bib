@article{ WOS:000490940300003,
Author = {Henry, Laura A. and Sundstrom, Lisa McIntosh and Winston, Carla and
   Bala-Miller, Priya},
Title = {NGO participation in global governance institutions: international and
   domestic drivers of engagement},
