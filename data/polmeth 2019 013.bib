@article{ WOS:000482233700003,
Author = {Kim, Younkyoo and Blank, Stephen},
Title = {Russia's Arms Sales Policy After the Ukraine Sanctions Palabras clave},
Journal = {ASIAN POLITICS \& POLICY},
Year = {2019},
Volume = {11},
Number = {3},
Pages = {380-398},
Month = {JUL},
Abstract = {Russia sells arms primarily to enhance its influence abroad in key
   regions, e.g., the Middle East, East, and South Asia. Since the
   government is the primary customer for this sector's goods, this sector
   does not risk collapse even if arms sales decline though arms sales
   bring vital foreign currency. But, the intervening motive between
   seeking enhanced global influence and maintaining the defense sector is
   the acquisition of elite rents since the arms trade offers multiple
   opportunities for corruption. We then focus on the impact of arms
   production and sales on international security. First that means looking
