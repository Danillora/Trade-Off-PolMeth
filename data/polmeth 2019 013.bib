@article{ WOS:000503415200013,
Author = {Mokra, Lucia and Juchniewicz, Piotr and Modrzejewski, Arkadiusz},
Title = {RULE OF LAW IN POLAND - INTEGRATION OR FRAGMENTATION OF COMMON VALUES?},
Journal = {EUROPEAN JOURNAL OF TRANSFORMATION STUDIES},
Year = {2019},
Volume = {7},
Number = {2},
Pages = {177-188},
Abstract = {The European Union treats the principle of the rule of law as one of the
   fundamental values of the European axiological system. The EU as a
   community of values treats the rule of law as a categorical imperative,
   which is an obligation for member states, which should respect it in an
   absolute way. The Treaty as well as the interpretation by EU
   institutions, particularly the Court of Justice of the EU, refers to
   this value and the obligation to implement it correctly. The recent
   Polish experience shows that the principle of the rule of law can be
   vitiated by governing powers. But this attitude has caused a reaction of
   European institutions protecting the principle of the rule of law, which
   is treated by them as a base for mutual trust.},
