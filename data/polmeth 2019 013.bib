@article{ WOS:000501581400011,
Author = {Otto, Sabine},
Title = {The Civilian Side of Peacekeeping: New Research Avenues},
Journal = {PEACE ECONOMICS PEACE SCIENCE AND PUBLIC POLICY},
Year = {2019},
Volume = {25},
Number = {4, SI},
Month = {DEC},
Abstract = {Almost three decades ago the United Nations (UN) entered an era of
   multi-dimensional peacekeeping operations, in which civilian and
   uniformed personnel work together. At the same time, human rights
   promotion became an integral part of such missions. Due to data
   limitations, there is little systematic knowledge about how civilian
   staff impacts human rights standards in the countries UN peacekeeping
   operations are deployed. I address this lacuna in two ways. First, I
   briefly outline the importance of civilian staff in UN peacekeeping
   operations and their roles in promoting human rights. Second, I provide
   explorative descriptive statistics on the number of civilian personnel
   in UN peacekeeping operations and the occurrence of violence against
   civilians committed by state forces.},
