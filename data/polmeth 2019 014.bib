Language = {Spanish},
Affiliation = {Barriga, PH (Corresponding Author), Univ Michoacana, Morelia, Michoacan, Mexico.
   Hernandez Barriga, Plinio; Chamu Nicanor, Felix, Univ Michoacana, Morelia, Michoacan, Mexico.},
DOI = {10.33110/cimexus140213},
