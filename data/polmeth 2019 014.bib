@article{ WOS:000479029200011,
Author = {Nemina, Pablo},
Title = {The Relationship between the IMF and Borrowing Governments in a
   Theoretical Viewpoint. The Contribution of IPE Centered on Interests},
Journal = {DESAFIOS},
Year = {2019},
Volume = {31},
Number = {2},
Pages = {341-373},
Month = {JUL-DEC},
Abstract = {The ability to provide financial assistance along with its role as
   auditor and guarantor of economic programs provides the International
   Monetary Fund (IMF) with an important power resource when intervening
   countries that face external imbalances. Regardless, the mere imposition
   of policies of the IMI over the borrowing governments does not
   characterize their relationship, neither that the IMF invariably shows
   dissatisfaction with the countries' non-compliance. This paper proposes
   a theoretical contribution for analyzing the relationship between the
   IMF and the borrowing governments based on the International Political
   Economy's (IPE) contributions about the incidence of the interests of
   world power countries and the financial sector, bureaucracies, and
   domestic actors of the country. The framework of analysis also considers
   the contributions of the constructivist sociological theory about the
   characterization of the all , and the borrowing governments as social
   actors, the restrictive and enabling character that structures have over
   the agency, and their implications for autonomy.},
