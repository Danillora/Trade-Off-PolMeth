@article{ WOS:000495449600002,
Author = {Doidge, Mathew},
Title = {ASEM's First Two Decades: A Role Discovered},
Journal = {BALTIC JOURNAL OF EUROPEAN STUDIES},
Year = {2019},
Volume = {9},
Number = {2},
Pages = {6-19},
Month = {SEP},
Abstract = {This article examines the first two decades of the transregional Asia
   Europe Meeting (ASEM) from its inception with the Bangkok Summit of
   1996. Examining instances of region building and the socialisation of
   states, it identifies the gradual emergence of a role for the forum, one
   that stands in some contrast to initial participant expectations. In
   this respect, rather than a structure for delivering substantive
   negotiated outcomes around issues such as trade liberalisation, the
   value of ASEM across its first 20 years came increasingly to be seen in
   its ideational aspects: identity building, norm diffusion, and dialogue
   without preconceptions.},
