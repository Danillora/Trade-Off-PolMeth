@article{ WOS:000469827800009,
Author = {Ryan, Cheyney},
Title = {Emanuela Ceva's Interactive Justice},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2019},
Volume = {22},
Number = {4},
Pages = {480-486},
Abstract = {Emanuela Ceva's Interactive Justice addresses the crucial need for
   dialogue between political philosophy and the field of conflict
   management. I raise two concerns. I ask how her procedures of
   interactive justice would cope with the Teri Schiavo case that she
   presents. Do they possess the type of generality justice seems to
   require, or are they necessarily particularistic, contingent, and
   opportunistic? I also question, here and more generally, framing matters
   in terms of value conflicts. Ceva's aim is to supplement political
   liberalism. I suggest her project can be developed into a more critical
   perspective towards that view.},
