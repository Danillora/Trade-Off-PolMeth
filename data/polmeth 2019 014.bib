@incollection{ WOS:000813444900009,
Author = {Osterberg-Kaufmann, Norma},
Editor = {Wiesner, C and Harfst, P},
Title = {Legitimacy Perception in the EU and Repertory Grid},
Booktitle = {LEGITIMITAT UND LEGITIMATION: VERGLEICHENDE PERSPEKTIVEN},
Series = {Vergleichende Politikwissenschaft},
Year = {2019},
Pages = {239-276},
Abstract = {This paper uses the Repertory Grid method to investigate the influence
   of political (European Union-specific) knowledge on perceptions of the
   legitimacy of the European Union. The aim of the study is firstly
   methodological. It is intended to test the significance of political
   knowledge on the object of investigation as a possible limitation of the
   Repertory Grid method. Here, the result shows the suitability of the
   method to represent not only rational decisions but also intuitive
   decisions in an integrative way. Second, in a still underdeveloped field
   of research, hypotheses are developed on the influence of political
   knowledge on perceptions of legitimacy. The data collected in a
   preliminary study provide indications that the importance of democratic
   norms and values for evaluating the legitimacy of national and European
   institutions is increasing, but at the same time criticism of the
   political system is also increasing and the perception of legitimacy is
   thus decreasing.},
