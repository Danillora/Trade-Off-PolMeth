@article{ WOS:000583726500008,
Author = {van der Waal, Margriet},
Title = {The social imaginary of precarious Europeans: the cultural
   representation of European socio-economic precarity in Tascha and
   Bande de Filles},
Journal = {POLITIQUE EUROPEENNE},
Year = {2019},
Volume = {66},
Number = {4},
Pages = {170-193},
Abstract = {This contribution focuses on how two artistic texts represent precarious
   Europeans: vulnerable social subjects (citizens, inhabitants of
   Europe/the EU) who may be deemed ``losers{''} of the European project.
   These precarious people are seldom - if at all - represented as
   participants in the complex processes of Europeanisation. The texts
   analysed and discussed in this contribution are the Dutch novel Tascha
   by Mira Feticu (2015) and the French feature film, Bande de Filles by
   Celine Sciamma (2014). I argue that a critical analysis of these
   narratives enables us to probe open the category that constitutes
   ``Europeans{''} and nudges us to rethink who is considered to be
   ``European{''}.},
