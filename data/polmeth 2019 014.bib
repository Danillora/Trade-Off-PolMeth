@article{ WOS:000476576700005,
Author = {Heinemann-Grueder, Andreas},
Title = {Hostage-takers or saviours of the state? Irregular battalions in Ukraine},
Journal = {OSTEUROPA},
Year = {2019},
Volume = {69},
Number = {3-4},
Pages = {51+},
Abstract = {In the spring of 2014, numerous irregular battalions were created in
   Ukraine. They went to the Donbas, where the police and the army had
   failed to defeat the separatist movement and the intervention of Russia.
   These battalions have rather different histories, but they have all been
   supported by the government. Now, most of them are subordinate to the
   Ministry of Internal Affairs and the Ministry of Defence. However, some
   militias of a more right-wing origin remain opposed to these ministries
   even today. While the extremist parties with which they are associated
   are being met with little political support, the battalions hit the
   headlines on a regular basis.},
