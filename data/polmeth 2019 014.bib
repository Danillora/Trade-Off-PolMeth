@article{ WOS:000467265600008,
Author = {Goodyear-Grant, Elizabeth and Tolley, Erin},
Title = {Voting for one's own: racial group identification and candidate
   preferences},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2019},
Volume = {7},
Number = {1},
Pages = {131-147},
Month = {JAN 2},
Abstract = {We know that voters have a baseline preference for candidates with whom
   they share a racial background, but whether and why a shared racial
   identity influences political decisions is the critical question. This
   article examines how shared racial characteristics lead racial minority
   voters to vote for candidates of the same race. Using a unique online
   experiment, we test the social psychological bases for racial affinity.
   We expect that for higher status minority groups, identity- rather than
   interest-based motivations offer the greatest explanatory value. Our
   findings support this supposition. Our results provide new insights into
   the political behavior of racial minority voters, and have implications
   for theories of vote choice and affinity, as well as practical
   applications for party strategists and candidates.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
