@article{ WOS:000492088600003,
Author = {Azam, Nicolas},
Title = {Between ``economic beliefs{''} and party constraints. The origins and
   structuring of the economic section of the PCF (1947-1961)},
Journal = {POLITIX},
Year = {2019},
Volume = {32},
Number = {126},
Pages = {37-60},
Abstract = {In 1947, the French Communist Party (PCF) set up an economics department
   to collect information for its leaders. The creation of this body and
   its journal, Economie et Politique, occurred in a context marked by the
   rise of the figure of an expert with specialized knowledge and the
   dissemination of economic belief, i.e., the imposition of an
   understanding of the social world through an economic lens. However,
   this innovation threatened the internal balance within the PCF, where
   most of the political staff came from the working class and were
   attached to certain fundamental doctrinal principles. Depending on their
   resources and the political situation, the members of the economics
   department were more or less inclined to comply with the party's
   existing standards, restricting them to a role of illustrating its
   partisan positions. This article shows the efforts made by members of
   the economics department to accommodate the party rules and to harmonize
   them with the knowledge they had gained from other areas. It explains
   the conflicts within the economics department and how they were
   politicized when they became embedded in the struggles within the
   political leading group.},
