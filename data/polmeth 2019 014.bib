@article{ WOS:000469184300003,
Author = {Vlcek, Tomas and Bodisova, Lubica and Vencourova, Zaneta},
Title = {The Northern Sea Route: Oil Transportation Alternative for Current Sea
   Lanes of Communication?},
Journal = {POLITOLOGICKY CASOPIS-CZECH JOURNAL OF POLITICAL SCIENCE},
Year = {2019},
Number = {1},
Pages = {50-73},
Abstract = {This paper deals with the Northern Sea Route, the Russian seaway, and
   its potential for maritime oil transport. The main objective is to
   determine if the Northern Sea Route might serve as an alternative to the
   current Sea Lanes of Communication used for maritime oil transport. It
   is concluded the route is not a suitable alternative to the current Sea
   Lanes of Communication, especially in light of the geography of oil
   production and consumption, as well as economic factors, legislative
   issues, and the undeveloped state of infrastructure. The Russian
   Federation, however, envisions the route as the next maritime highway.},
