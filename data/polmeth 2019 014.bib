@incollection{ WOS:000813444900004,
Author = {von Haldenwang, Christian},
Editor = {Wiesner, C and Harfst, P},
Title = {Legitimation and Legitimacy in Comparative Perspective Overview of the
   Research State},
Booktitle = {LEGITIMITAT UND LEGITIMATION: VERGLEICHENDE PERSPEKTIVEN},
Series = {Vergleichende Politikwissenschaft},
Year = {2019},
Pages = {57-96},
Abstract = {The legitimacy of political order has become an important reference
   point for the analysis of political regimes in recent years. However,
   the concept is difficult to operationalize and measure - especially in
   countries where legitimacy seems to be a particularly important resource
   (cases of political transformation and high state fragility). This
   chapter provides an overview of the state of research. Four dimensions
   of measurement are distinguished, based on a dialogic understanding of
   legitimacy. On the one hand, legitimation, i.e. the strategic
   acquisition of legitimacy, is understood as a process in which the
   legitimacy expectations of political subjects are linked to the
   performance of the political regime. On the other hand, the legitimacy
   claims of the rulers meet observable behavioral patterns of the ruled.
   The literature review makes clear that the dialogical character of
   legitimacy is not always taken into account. As a result, existing
   approaches often measure only individual aspects of legitimacy or even
   miss the core of the concept altogether.},
