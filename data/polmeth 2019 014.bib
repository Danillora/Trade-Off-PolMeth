@article{ WOS:000470200700005,
Author = {Bohas, Alexandre},
Title = {Das Auto. A case for anomic globalization through Volkswagen's `Made in
   Germany' label},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2019},
Volume = {12},
Number = {1},
Pages = {66-85},
Abstract = {This study of Volkswagen's national claim deals with the anomic
   dimension of globalization. Using Bourdieu's and Durkheim's framework of
   analysis, it examines the paradoxical promotion of national belonging by
   a transnational company. It makes three contributions: first, successful
   national narratives show the incompleteness of globalization which
   engenders anomie, the lasting divergence between people's habitus and
   their environment. Second, the national claim can be regarded as a
   reaction to globalization as well as to its pluralizing effect. Third,
   in reflexive terms, durable anomie is analyzed as reinforcing the place
   of the national level and the state in people's habitus.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
