@article{ WOS:000516826800014,
Author = {Hernandez Barriga, Plinio and Chamu Nicanor, Felix},
Title = {Cointegration analysis between the nominal exchange rate and the
   consumer price index in the APEC 2000-2018 countries},
Journal = {CIMEXUS},
Year = {2019},
Volume = {14},
Number = {2},
Pages = {261-283},
Month = {JUL-DEC},
Abstract = {This article objective is to identify the determinants of the long-term
   exchange rate pass-through into domestic prices in the APEC economies
   for the 2000-2018 period. To that aim we develop a panel data
   cointegration analysis between exchange rates and domestic prices of
   different groups of countries, according to a classification based on
   price stability, nominal exchange rate stability, growing rates, per
   capita income, economic development and trade openness. We conclude that
   the exchange rate pass-through into domestic prices is an economic
   rather that financial phenomenon. As pass-through is mainly related to
   the economic development level, the best way to reduce it is an
   industrial policy that supports growth of some key economic sectors in
   the less developed countries.},
Publisher = {UNIV MICHOACANA SAN NICOLAS HIDALGO, INST INVESTIGACIONES HISTORICAS},
Address = {BENEDICTO LOPEZ 122, COLONIA VENTURA PUENTE, AP 46-A, MICHOACAN, MORELIA
   CP 58020, MEXICO},
