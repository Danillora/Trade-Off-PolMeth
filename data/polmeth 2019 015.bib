@article{ WOS:000456253400004,
Author = {Oguz, Tunay},
Title = {Update on Racial Disparities in Access to Healthcare: An Application of
   Nonlinear Decomposition Techniques{*}},
Journal = {SOCIAL SCIENCE QUARTERLY},
