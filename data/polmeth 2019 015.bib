@article{ WOS:000470880800003,
Author = {Keena, Alex and Knight-Finley, Misty},
Title = {Are Small Donors Polarizing? A Longitudinal Study of the Senate},
Journal = {ELECTION LAW JOURNAL},
Year = {2019},
Volume = {18},
Number = {2},
Pages = {132-144},
Month = {JUN 1},
Abstract = {Current campaign finance law in the United States does little to redress
   biases in the donor population. One solution proposed by reformers is to
   expand the donor base to include a broader and more diverse subset of
   the population. Yet studies on the effects of ``small{''} money in
   elections suggest that these reforms may polarize politicians. We
   conduct a longitudinal study of the effects of campaign finance on
   ideological sorting in the U.S. Senate in order to understand whether
   money from small donors causes ideological extremism or whether senators
   adopt polarizing positions as a strategy for raising money from small
   donors. The Senate provides a unique window into this question, because
   senators serve six-year terms and thus enjoy periods of time when they
   are not immediately accountable to their supporters. We find that a
   senator's receipts from small donors in previous elections have no
   effect on their future behavior. Rather, causality appears to flow from
   the politicians to the donors. Senators' voting behavior leading up to
   reelection has a significant effect on the money raised from small
   donors during the reelection at the end of the term. These results
   suggest that further polarization is not an inevitable consequence of
   campaign finance reforms that aim to improve equality in representation
   by expanding access to campaign contributions.},
Publisher = {MARY ANN LIEBERT, INC},
Address = {140 HUGUENOT STREET, 3RD FL, NEW ROCHELLE, NY 10801 USA},
Type = {Article},
Language = {English},
Affiliation = {Keena, A (Corresponding Author), Virginia Commonwealth Univ, Dept Polit Sci, 827 West Franklin St, Richmond, VA 23284 USA.
