@article{ WOS:000575534600001,
Author = {Abbott, Andrew and Jones, Philip},
Title = {The cyclicality of government foreign-aid expenditure: voter awareness
   in ``good{''} times and in ``bad{''}},
Journal = {PUBLIC CHOICE},
Year = {2021},
Volume = {186},
Number = {1-2},
Pages = {97-117},
Month = {JAN},
Abstract = {While it has been argued that the cyclicality of government spending
   likely depends on the intensities of political pressure to increase
   expenditure, in economic upturns and downturns, it is important to
   explore the determinants of changes in the strengths of those pressures.
   This paper is the first (to our knowledge) to focus on the relevance of
   systematic changes in voter awareness of government spending.
   Predictions of the impact of changes in awareness are tested with
   reference to 23 OECD donor countries' foreign aid expenditures over the
   1999-2015 period. The evidence offers insights into the discretion
   governments exercise when ``fiscal illusion{''} increases and into the
   policy implications of systematic changes in voter awareness (in
   ``good{''} times and in ``bad{''}).},
