@article{ WOS:000503855200006,
Author = {Moseley, Fred},
Title = {Marx's Economic Manuscript of 1867-68 (Excerpt) Editor's Introduction},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2019},
Volume = {27},
Number = {4},
Pages = {145-156},
Month = {DEC},
Abstract = {This is an introduction to an English translation of a 25-page excerpt
   from Marx's Manuscript of 1867-68, which was published for the first
   time in German in 2012 in the MEGA, Volume II/4.3. This excerpt (see pp.
   162-92) is Marx's first and only attempt to incorporate unequal turnover
   times across industries into his theory of the equalisation of the
   profit rate and prices of production. The introduction attempts to
   clarify the overall logic of this excerpt as well as to point out Marx's
   many small errors in this messy first draft. The introduction concludes
   with the implications of this excerpt for the general interpretation of
   Marx's theory of prices of production (i.e. the transformation problem).},
