@article{ WOS:000544061500001,
Author = {Facchini, Francois and Melki, Mickael},
Title = {Egalitarianism and the democratic deconsolidation: Is democracy
   compatible with socialism?},
Journal = {PUBLIC CHOICE},
Year = {2021},
Volume = {186},
Number = {3-4, SI},
Pages = {447-465},
Month = {MAR},
Abstract = {The unprecedented reduction in popular support for democracy represents
   a risk of democratic deconsolidation. The new situation echoes old
   debates on the compatibility of democracy with capitalism and socialism.
   This article provides empirical support for the incompatibility of
   socialism with democracy by providing evidence suggesting that when
   citizens adopt egalitarianism as a supreme value, they are ready to
   sacrifice democracy for the sake of equality. Using individual data, we
   observe that the decline in support for democracy over generations and
   over time is accompanied by rising support for egalitarian values in US
   and European democracies. Moreover, democracies with stronger
   preferences for egalitarianism also have less public support for
   democracy, suggesting a tradeoff between both values.},
