@article{ WOS:000463617600006,
Author = {Hall, Joshua and Matti, Josh and Ferreira Neto, Amir B.},
Title = {Rent-seeking in the classroom and textbooks: Where are we after 50years?},
Journal = {PUBLIC CHOICE},
Year = {2019},
Volume = {181},
Number = {1-2, SI},
Pages = {71-82},
Month = {OCT},
Abstract = {In this paper, we provide an overview of the influence of Tullock's work
   on rent-seeking in the area of economics education. After summarizing
   the basic rent-seeking model in both a domestic and international
   context, we conduct an analysis of undergraduate and graduate textbooks
   in public economics. We find a majority of undergraduate texts cover
   rent-seeking in depth, but two texts provide zero coverage. No graduate
   textbook surveyed mentions rent-seeking. We conclude by summarizing the
   economic education literature on rent-seeking, which can be divided into
   either classroom experiments or popular culture examples.},
