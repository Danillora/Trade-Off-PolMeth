   Cole, Georgia, Univ Cambridge, Newnham Coll, Margaret Anstee Ctr Global Studies, Cambridge CB3 9DF, England.},
DOI = {10.1016/j.polgeo.2019.05.003},
ISSN = {0962-6298},
