@article{ WOS:000457508600006,
Author = {Garnett, Philip and Hughes, Sarah M.},
Title = {Obfuscated democracy? Chelsea Manning and the politics of knowledge
   curation},
Journal = {POLITICAL GEOGRAPHY},
Year = {2019},
Volume = {68},
Pages = {23-33},
Month = {JAN},
Abstract = {This paper interrogates the attention that Chelsea Manning has received
   within the academy. It begins from the observation that despite being
   responsible for the largest classified document leak, work within
   Political Geography and International Relations that engages with this
   data remains notably scant. This claim emerges from a systematic search
   of peer-reviewed materials using WikiLeaks materials as their empirical
   base, compiling a database of papers written about Manning. We then
   examine the possible reasons for this absence, focusing upon a series of
   what we term `obfuscating practices' by which state actors complicate
   access to publicly accessible knowledge, including access to the US
   Army's Freedom of Information Request Website, and the court documents
   from Manning's court-martial. Finally, we look at claims of an embargo
   around the publication of academic work in this area, conceptualising
   this as a politics of paranoia and commenting upon the implications of
   this for knowledge curation within the academy.},
