@article{ WOS:000476066100001,
Author = {Smith, Donal},
Title = {Collateral Constraints and the Interest Rate},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2020},
Volume = {67},
Number = {2},
Pages = {137-165},
Month = {MAY},
Abstract = {This paper develops a model in which an increase in financial frictions
   leads to a fall in the steady-state rate of interest. A perpetual youth
   overlapping generations (OLG) model is extended to incorporate a
   collateral constraint, this results in a transmission mechanism in which
   an interest rate fall occurs endogenously from a disruption to the
   credit market. It is found that that non-linearities exist in the
   relationship between changes in financial frictions and the interest
   rate. By specifying the mechanism by means of which this occurs, the
   model provides an explanation for why low interest rates have been
   observed with such persistence since the financial crisis.},
