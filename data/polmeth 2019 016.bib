@article{ WOS:000472687600016,
Author = {Arif, Imran and Hall, Joshua C.},
Title = {International flows of people and institutional change},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2019},
Volume = {58},
Pages = {276-288},
Month = {JUN},
Abstract = {International travel clearly increases human interaction over space and
   exposes societies to foreign influences, foreign ideas, and foreign
   institutions. Does international travel promote institutional change in
   a traveler's home country? This paper uses panel data from 149 countries
   to test the hypothesis of institutional change stemming from
   international travel. We generally find that foreign travel does not
   affect political institutions. In one sub-sample, we find limited
   evidence that international travel can be a determinant of institutional
   quality in the home country depending upon whether the home country is
   an autocracy or democracy.},
