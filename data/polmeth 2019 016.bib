@article{ WOS:000467860700027,
Author = {Kim, Na Yeon and Berry, Frances Stokes},
Title = {Do State-Customized TANF Work Policies Actually Reduce Unemployment?},
Journal = {SOCIAL SCIENCE QUARTERLY},
