@article{ WOS:000482656100002,
Author = {King, Bridgett A.},
Title = {State online voting and registration lookup tools: Participation,
   confidence, and ballot disposition},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2019},
Volume = {16},
Number = {3},
Pages = {219-235},
Abstract = {State online voting and registration lookup tools that allow voters to
   check their voter registration status, confirm their polling location,
   review a sample ballot, and check the status of an absentee or
   provisional ballot reduce the cost of information acquisition and
   service completion while providing voters with the information necessary
   to participate in the democratic process. The analysis evaluates the
   relationship between state voting and registration online lookup tools
   and voter turnout, voter confidence, and the disposition of provisional
   ballots. Findings suggest that voting and registration lookup tools are
   related to voter turnout and rejected provisional ballots.},
