@article{ WOS:000469934400006,
Author = {Vraga, Emily K. and Tully, Melissa},
Title = {Engaging with the other side: using news media literacy messages to
   reduce selective exposure and avoidance},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2019},
Volume = {16},
Number = {1},
Pages = {77-86},
Abstract = {We examined whether news media literacy (NML) messages attenuate
   selective exposure and avoidance. One week before the 2016 U.S.
   presidential election, participants were randomly assigned to see a NML
   video advertisement before entering a simulated news aggregation website
   where behaviors were unobtrusively tracked. For three of the four NML
   messages, higher levels of partisan selective exposure among Republicans
   compared to Democrats in the control condition were reduced to
   non-significance. There were no effects on selective avoidance for
   either group. Several NML messages limited partisan selective exposure
   among Republicans, offering a concrete option for addressing problems of
   selective exposure online.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
