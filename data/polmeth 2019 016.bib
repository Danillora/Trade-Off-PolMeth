@article{ WOS:000475807800011,
Author = {Grossman, Henryk},
Title = {A New Theory of Imperialism and the Social Revolution (First Three
   Parts of our)},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2019},
Volume = {27},
Number = {2},
Pages = {317-343},
Month = {JUL},
Abstract = {Grossman's first major, published study of Marxist economic theory was a
   devastating critique of Fritz Sternberg's ambitious and long book,
   Imperialism. The article exposed the way Sternberg failed to grasp
   Marx's method and basic economic arguments, and drew impatient
   revolutionary conclusions from assumptions of anti-revolutionary
   revisionism. It also outlined Grossman's own recovery, restatement and
   elaboration of features of Marx's economic analysis, and Lenin's
   conception of workers' revolution. This abridgement consists of the
   first three sections of the article.},
