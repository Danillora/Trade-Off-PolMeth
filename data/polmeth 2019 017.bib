@article{ WOS:000470918800003,
Author = {Liao, Shu-Yi and Wang, Lan-Hsun and Huang, Mao-Lung},
Title = {Does More Consumption Promote Real GDP Growth?},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2019},
Volume = {66},
Number = {3},
Pages = {384-403},
Month = {JUL},
Abstract = {This paper employs Hansen's (1999) panel threshold regression model
   {[}Journal of Econometrics 39 (1999) 345-68] based on a time series
   dataset of 109 countries from 1960 to 2007 to investigate the threshold
   relationship between the change in real GDP per capita and the
   consumption size (consumption-income ratio, APC). The results show that
   the consumption level should not exceed the 49.68\% threshold of real
   GDP per capita for each country regardless of the income level. Also,
   the relationship between the change in real GDP per capita and the
   consumption size seems to have `Armey curve' or `inverted-U shape'
   characteristic. In order to promote real GDP growth, our results suggest
   that the high-income, low-APC countries should encourage more
   consumption while the low-income, high-APC countries should encourage
   more saving.},
