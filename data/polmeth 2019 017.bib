@article{ WOS:000472588300008,
Author = {Heijnen, Pim and Schoonbeek, Lambert},
Title = {Rent-seeking with uncertain discriminatory power},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2019},
Volume = {56},
Pages = {103-114},
Month = {JAN},
Abstract = {We consider a two-player Tullock rent-seeking contest with uncertain
   discriminatory power in the contest success function. We examine the
   cases where both players are either informed or uninformed about the
   size of the discriminatory power, as well as the case where only one
   player has private information about it. We show that in all three cases
   the contest has a unique (Bayesian) Nash equilibrium. In each case we
   characterize key properties of the equilibrium.},
