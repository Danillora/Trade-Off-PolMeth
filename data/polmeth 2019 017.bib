@article{ WOS:000482814100001,
Author = {Gnangnon, Sena Kimm},
Title = {Does Aid for Information and Communications Technology Help Reduce the
   Global Digital Divide?},
Journal = {POLICY AND INTERNET},
Year = {2019},
Volume = {11},
Number = {3},
Pages = {344-369},
Month = {SEP},
Abstract = {The Aid for Trade (AfT) initiative was launched at the 2005 World Trade
   Organization Ministerial Conference with the aim of helping developing
   countries develop their trade capacity and better integrate into the
   global trade system. This article examines the impact of Aid for
   Information and Communications Technology (ICT) (a component of AfT) on
   the global digital divide of recipient countries, measured by the
   Internet penetration rate (i.e., the proportion of individuals using the
   Internet in a given country) and the Internet penetration distance
   (i.e., the difference between a country's Internet penetration rate and
   the world average). The analysis was conducted on a sample of 119
   countries, spanning the period 2004-2015. We find empirical evidence
   that higher AfT inflows for ICT are conducive to both higher Internet
   penetration rate and higher Internet penetration distance for recipient
   countries. However, more advanced AfT recipient-countries experience a
   higher positive effect of AfT for ICT than do relatively less advanced
   countries. These findings suggest that scaling up of AfT for ICT for
   developing countries (but particularly the poorest countries) is
   necessary to help them build the Internet-related infrastructure
   necessary to benefit from the growing digital economy.},
