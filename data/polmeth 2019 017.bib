@article{ WOS:000454897300004,
Author = {Belke, Ansgar H. and Osowski, Thomas U.},
Title = {Measuring fiscal spillovers in EMU and beyond: A Global VAR approach},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2019},
Volume = {66},
Number = {1, SI},
Pages = {54-93},
Month = {FEB},
Abstract = {This paper identifies and measures fiscal spillovers in the EU countries
   empirically using a global vector autoregression (GVAR) model. Our aim
   was to look at the sign and the absolute values of fiscal spillovers in
   a countrywise perspective and at the time profile (impulse response) of
   the impacts of fiscal shocks. We find moderate spillover effects of
   fiscal policy shocks originating in Germany and France. However, there
   is significant variation regarding the magnitude of the spillovers on
   individual destination countries and country clusters. Furthermore, we
   find some evidence that German or French fiscal spillovers are stronger
   on EMU than on non-EMU countries.},
