@article{ WOS:000470984900015,
Author = {Brown, Leanora and Hibbert, Keva},
Title = {The Incidence of Crime on Industry-Level Foreign Direct Investment: An
   Assessment of OECD Member Countries},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2019},
Volume = {100},
Number = {4},
Pages = {1228-1240},
Month = {JUN},
Abstract = {In this article, we assess whether the incidence of crime helps to
   explain the variation in sectoral FDI flows. Using a panel of 29
   Organization of Economic Co-operation and Development (OECD) countries
   for the period 2003-2012, we employ a generalized method of moments
   (GMM) estimation strategy due to the potential for endogeneity between
   our variables of interest. Our results indicate that crime deters
   investment to the service sector. In particular, this effect is observed
   in the following service industry subsector: financial services.
   Policymakers interested in boosting FDI in the affected sectors should
   be concerned with policies that focus on the reduction of criminal
   activities.},
