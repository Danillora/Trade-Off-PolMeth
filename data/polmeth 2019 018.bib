@article{ WOS:000488420100005,
Author = {Getmansky, Anna and Sinmazdemir, Tolga and Zeitzoff, Thomas},
Title = {The allure of distant war drums: Refugees, geography, and foreign policy
   preferences in Turkey},
Journal = {POLITICAL GEOGRAPHY},
Year = {2019},
Volume = {74},
Month = {OCT},
Abstract = {Previous research argues that countries often intervene in the conflicts
   that cause refugees to flow across their borders. Public opinion against
   refugees may pressure states to intervene to `solve the refugee
   problem.' We study what shapes public support for such intervention
   using a survey experiment in Turkey against the backdrop of the Syrian
   refugee crisis. We survey over 1,200 respondents with varied exposure to
   refugees, and randomize information about the consequences of hosting
   refugees to examine its effects on support for intervention in Syria.
   Emphasizing the negative externalities of hosting refugees, including
   their connection with militants, increases support for intervention
   among respondents who reside far from the Turkish-Syrian border. Closer
   to the border, this information reduces support for intervention in
   Syria. These findings highlight that vulnerability to the costs of
   intervention (proximity to the border) shapes public support for
   intervening. We also find that public opinion towards intervention is
   correlated with partisan identity and respondents' daily exposure to
   refugees.},
