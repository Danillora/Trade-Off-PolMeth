@article{ WOS:000545474300005,
Author = {Thi Kim Cuong Pham and Phu Nguyen-Van and Thanh-Tam Nguyen-Huu and Thi
   Anh-Dao Tran and Noukignon, Kone},
Title = {Subjective Well-Being and Social Comparison: A Comparative Study on
   Rural Thailand and Vietnam},
