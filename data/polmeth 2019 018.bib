@article{ WOS:000481841500001,
Author = {Upadhyaya, Shikha and Rosa, Jose Antonio},
Title = {Resilience in Social Innovation: Lessons from Women Market Traders},
Journal = {SOCIAL SCIENCE QUARTERLY},
