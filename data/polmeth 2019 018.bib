@incollection{ WOS:000463098700006,
Author = {Matsui, Shigenori},
Book-Author = {Matsui, S},
Title = {Reconstruction of the Tohoku Region},
Booktitle = {LAW AND DISASTER: EARTHQUAKE, TSUNAMI AND NUCLEAR MELTDOWN IN JAPAN},
Year = {2019},
Pages = {119-162},
Abstract = {After rescue and recovery missions were complete, the focus of
   government operations shifted to reconstruction of the Tohoku Region.
   The Japanese government attempted to provide assistance to disaster
   victims and to support community-rebuilding efforts. The central
   government adopted the fundamental policy for reconstruction and
   established the Reconstruction Agency. The reconstruction special zones
   program and reconstruction grants program were introduced to facilitate
   reconstruction. Large sums of public money have been spent on
   reconstruction. However, whether these measures have been effective and
   sufficient remains a hotly debated topic. Indeed, many of the local
   affected residents are deeply frustrated and disappointed. Why do they
   feel such frustration and disappointment? Is there something wrong with
   the reconstruction measures adopted by the Japanese government? This
   chapter examines the reconstruction process with particular focus on the
   legal system and its role to see whether they are indeed appropriate. It
   will reveal that the reconstruction process is fraught with roadblocks
   and show how fundamental problems of the Japanese legal system are
   manifesting in serious failures of law.},
