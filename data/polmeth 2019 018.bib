@incollection{ WOS:000798808500011,
Author = {Kukathas, Chandran},
Editor = {Knoll, M and Snyder, S and Simsek, N},
Title = {Justicitis},
Booktitle = {NEW PERSPECTIVES ON DISTRIBUTIVE JUSTICE: DEEP DISAGREEMENTS, PLURALISM,
   AND THE PROBLEM OF CONSENSUS},
Year = {2019},
Pages = {187-203},
Abstract = {This contribution argues that, despite its undeniable importance, the
   topic of justice has become an oversold theoretical and normative
   commodity. Theories of justice abound covering what seems to be nearly
   every aspect of human existence. Without criticizing the aim of
   improving these disparate aspects of human existence, a problem arises
   insofar as justice as a concept is overburdened; rather than resolving
   the problems it seeks to better, its conceptual proliferation seems to
   do little to improve actual human existence. The essay's main contention
   is that if justice is seen a la Rawls as the keystone of moral and
   political thinking, and given its primacy among the virtues, any
   injustice found within our social institutions will not be tolerated.
   The result is that matters very basic to our lives, such as choosing
   whether to send one's child to a private or public school, can be seen
   as a matter of justice or injustice, when, in fact, it is not. Hence,
   our conception of justice is sick and it needs a cure.},
