@article{ WOS:000484786800001,
Author = {Philpot, Tasha S. and Miller, Kenneth M.},
Title = {A New Face to the Race Card? Campaigns, Racial Cues, and Candidate
   Credibility},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2020},
Volume = {101},
Number = {1, SI},
Pages = {73-90},
Month = {JAN},
Abstract = {Objective. This research examines the effects of using positive racial
   imagery in the context of an electoral campaign. Method. This study
   utilizes an experiment that was embedded in a survey conducted as part
   of the 2014 Cooperative Congressional Election Study. Results. Unlike
   negative stereotypical images that activate racial prejudice, positive
   racial images can be used to improve perceptions of a candidate's
   perceived level of inclusivity and overall candidate evaluations. The
