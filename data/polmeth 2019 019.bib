@article{ WOS:000482656100001,
Author = {Hutchens, Myiah J. and Silva, David E. and Hmielowski, Jay D. and
   Cicchirillo, Vincent J.},
Title = {What's in a username? Civility, group identification, and norms},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2019},
Volume = {16},
Number = {3},
Pages = {203-218},
Abstract = {Understanding the effects of comments in online discussion spaces is
   gaining traction in the communication literature. Although there is
   ample research addressing potential negative effects of being exposed to
   uncivil comments, there is little research examining the effects of
   civil comments. In addition, more research is needed to address which
   variables serve as mediators and moderators relative to comment content
   and outcomes. This paper proposes that both who is making the comments
   and the expectations about the online environment will serve as
   moderators of civility's influence on the emotional response to reading
   these comments. These emotional outcomes will then affect intentions to
   engage in political discussion in the future. To examine this process, a
   2 (tone: civil/uncivil) X 2 (group identification: in-group/out-group)
   factorial-design experiment was conducted, with the group identification
   factor serving as a moderator. Exposure to civil (compared to uncivil)
   comments increased the positive emotion of enthusiasm, which
   subsequently resulted in increased intention to engage in political
   discussion. This indirect effect was moderated by whether comments were
   coming from in-group/outgroup members and perceptions of whether
   civility was common in online spaces.},
