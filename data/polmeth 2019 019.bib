@article{ WOS:000482656100004,
Author = {Heinsohn, Till and Fatke, Matthias and Israel, Jonas and Marschall,
   Stefan and Schultze, Martin},
Title = {Effects of voting advice applications during election campaigns.
   Evidence from a panel study at the 2014 European elections},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2019},
Volume = {16},
Number = {3},
Pages = {250-264},
Abstract = {Over the last several years, Voting Advice Applications (VAAs) have
   become an indispensable part of election campaigns in many countries.
   VAAs are designed to increase the political sophistication of citizens.
   In this paper we test whether these tools can accomplish this mission.
   Resorting to self-generated panel data for the 2014 European Election in
   Germany, we use entropy balancing techniques to deal with the problem of
   self-selection into the treatment. In doing so, we try to disentangle
   causal dynamics and offer analyses that underpin the tools' effects on
   users' interest in the election campaign as well as on electoral
   participation. Regarding policy knowledge, however, no VAA effect can be
   found within our data.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
