@article{ WOS:000479303600005,
Author = {Akoz, Kemal Kivanc and Hernandez-Lagos, Pablo},
Title = {Rents from power for a dissident elite and mass mobilization},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2019},
Volume = {66},
Number = {4},
Pages = {584-604},
Month = {SEP},
Abstract = {Popular uprisings in autocracies seldom lead to democratic regimes. We
   propose a model that helps explain how rents from power encourage
   popular revolts. We study why citizens would follow a dissident group
   seeking regime change, if rents from change accrue only to the group.
   Our model predicts that higher rents may increase the incidence of
   public mobilization because rents facilitate coordination. The results
   suggest that cohesive dissident groups may spur seemingly spontaneous
   mass mobilizations, even when the mass public know that the dissident
   group is driven by greed rather than a genuine desire to halt
   incumbent's rent-seeking activities.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Aköz, KK (Corresponding Author), Natl Res Univ, Higher Sch Econ, Moscow, Russia.
