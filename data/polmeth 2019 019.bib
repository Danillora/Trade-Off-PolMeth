@article{ WOS:000467860700017,
Author = {Hannibal, Bryce and Portney, Kent},
Title = {Correlates of Food-Energy-Water Nexus Awareness Among the American
   Public},
Journal = {SOCIAL SCIENCE QUARTERLY},
