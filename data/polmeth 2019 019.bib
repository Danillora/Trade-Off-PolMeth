@article{ WOS:000503855200007,
Author = {Panzer, Herbert},
Title = {Marx's Economic Manuscript of 1867-68 (Excerpt) Translator's
   Introduction},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2019},
Volume = {27},
Number = {4},
Pages = {157-161},
Month = {DEC},
Abstract = {This Introduction describes the approach and rules applied when
   translating a 25-page excerpt from Marx's Manuscript of 1867-68, as
   published in the MEGA, Volume 11/4.3. The draft status and terseness of
   the text required that the translation (see pp.162-92) proceed along
   with a working-out of its mathematical content. The translation's main
   guideline was to translate the draft such as it stood, while correcting
   figures and formulas wherever possible. Remaining major deficiencies and
   inconsistencies are discussed in depth, showing also what an outstanding
   level of acuity Marx had already achieved in a manuscript at first-draft
   stage.},
