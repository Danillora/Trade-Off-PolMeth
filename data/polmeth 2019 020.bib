@article{ WOS:000469934400005,
Author = {Dounoucos, Victoria A. and Hillygus, D. Sunshine and Carlson, Caroline},
Title = {The message and the medium: an experimental evaluation of the effects of
   Twitter commentary on campaign messages},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2019},
Volume = {16},
Number = {1},
Pages = {66-76},
Abstract = {Social media are an increasingly important communication tool in
   political campaigns, yet there is much to learn about how communication
   effects might differ for these platforms. In contrast to traditional
   media outlets, messengers often do not fully control their message on
   social media; rather, the audience often receive the candidate message
   along with comments and reactions, commonly uncivil ones. Using a survey
   experiment, we examine the persuasion implications of audience comments
   on candidate tweets. We find that commentary on tweets becomes part of
   the communicated message, with mostly positive comments offering a
   slight persuasive boost, and mostly negative comments offering a larger
   negative effect.},
