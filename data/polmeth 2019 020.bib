@article{ WOS:000476657900009,
Author = {Popa, Diana Mariana},
Title = {New versus Established Migrants in a Competitive Labor Market: A Focus
   on Central East Europeans in the Netherlands},
Journal = {INTERSECTIONS-EAST EUROPEAN JOURNAL OF SOCIETY AND POLITICS},
Year = {2019},
Volume = {5},
Number = {2},
Pages = {130-149},
Abstract = {The Netherlands is a relatively new, attractive, but less researched
   destination for Central East European migrants, who compete on the Dutch
   labor market with more established migrant groups. Relying on data from
   the European Social Survey, the present article addresses the following
   questions: What is the employment status of CEE migrants in the
   Netherlands? How likely is it that CEE migrants will be satisfied with
   their income in the Netherlands? Does the region of origin influence the
   likelihood of having a comfortable income in the Netherlands? To this
   end, the article compares CEE migrants with the dominant migrant groups
   found in the Netherlands. Findings show that CEE migrants are likely to
   be satisfied with their income levels in their new host country.
   Education levels but also region of origin have an impact on the
   likelihood of living comfortably with the current income level.},
