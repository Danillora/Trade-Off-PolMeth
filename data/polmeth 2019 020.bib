@article{ WOS:000478622100006,
Author = {Boukes, Mark},
Title = {Agenda-Setting With Satire: How Political Satire Increased TTIP's
   Saliency on the Public, Media, and Political Agenda},
Journal = {POLITICAL COMMUNICATION},
