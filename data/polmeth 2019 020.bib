Book-Author = {Matsui, S},
Title = {Aftermath of the nuclear meltdown},
Booktitle = {LAW AND DISASTER: EARTHQUAKE, TSUNAMI AND NUCLEAR MELTDOWN IN JAPAN},
Year = {2019},
Pages = {163-209},
Abstract = {In the aftermath of the nuclear accident at Fukushima No 1 Nuclear Power
   Plant, the central government and Tokyo Electric Power Co. (TEPCO) bear
   responsibility for fully containing the radiation leaks and abandoning
   the nuclear power plant. TEPCO also bears responsibility for paying
   damages and compensation to those affected by this disaster. But in
   order to make sure that all the damages were compensated, the government
   needed some mechanism to support TEPCO. The onus for accelerating
   environmental decontamination and enabling evacuees to return to their
   homes and communities lies with the central government. Further, the
   central government must adopt measures to facilitate reconstruction in
   Fukushima. All of these endeavors have given rise to many novel legal
   questions. This chapter examines a number of the legal questions
   presented by the aftermath of the nuclear accident in Fukushima and how
   the Japanese government responded to them.},
