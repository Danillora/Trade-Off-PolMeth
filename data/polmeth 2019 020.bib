@article{ WOS:000463015100004,
Author = {Francois, Abel and Gergaud, Olivier},
Title = {Is civic duty the solution to the paradox of voting?},
Journal = {PUBLIC CHOICE},
Year = {2019},
Volume = {180},
Number = {3-4},
Pages = {257-283},
Month = {SEP},
Abstract = {Although sense of civic duty is seen by many scholars as the most
   obvious solution to the paradox of voting, very few empirical studies
   provide clear evidence on that motive. We use blood donation to build
   proxies, focusing only on intrinsic motivations, and then introduce such
   measures into electoral turnout regressions. Our results show that civic
   duty has a strong influence on voter turnout rates, confirming that the
   satisfaction voters receive from voting matters regardless of election
   outcomes. The results are even stronger when we incorporate the number
   of plasma and platelet donations, which take more time and require
   stronger commitments from donors.},
