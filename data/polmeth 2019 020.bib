@article{ WOS:000467860700012,
Author = {Turner, Joel F. and Kash, Jeffrey P. and Lasley, Scott},
Title = {The Big Five and Southern Legislative Distinctiveness},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2019},
Volume = {100},
Number = {3},
Pages = {689-700},
Month = {MAY},
Abstract = {ObjectivePrevious research has found that southern state legislators
   demonstrate more progressive ambition than their nonsouthern
   counterparts. This presents an interesting puzzle with regard to why
   this difference exists. In this article, we apply the Five-Factor Model
   (conscientiousness, agreeableness, neuroticism, openness, and
   extraversion) to investigate whether regional personality differences
   exist that can help explain why southern legislators exhibit greater
   levels of progressive ambition than nonsouthern legislators.
   MethodsData from a national survey of state legislator are analyzed
   using ordinal regression.
   ResultsOur findings show that higher levels of extraversion and openness
   among the legislators are directly related to greater levels of
   political ambition. More specifically, southern legislators appear to
   possess disproportionately higher levels of extraversion and openness
   than their nonsouthern counterparts. This helps explain the greater
   levels of progressive ambition they express.
   ConclusionsIn addition to providing insight into progressive ambition,
   the results reinforce the notion of southern distinctiveness among state
   legislatures.},
