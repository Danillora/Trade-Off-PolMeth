@article{ WOS:000463921800004,
Author = {Harvie, David},
Title = {(Big) Society and (Market) Discipline: Social Investment and the
   Financialisation of Social Reproduction},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
