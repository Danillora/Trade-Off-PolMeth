@article{ WOS:000472588300015,
Author = {Conrad, Christian and Hartmann, Matthias},
Title = {On the determinants of long-run inflation uncertainty: Evidence from a
   panel of 17 developed economies},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2019},
Volume = {56},
Pages = {233-250},
Month = {JAN},
Abstract = {Based on a cross section of 17 advanced economies and data for the
   period 1975 to 2015, we examine how the interaction between monetary
   policy and macroeconomic conditions affects inflation uncertainty in the
   long-term. We construct a proxy for the unobservable inflation
   uncertainty based on the slowly evolving long-term variance component of
   inflation from a Spline-GARCH model (Engle and Rangel, 2008). We show
   that long-run inflation uncertainty is high if an inflation-tolerant
   central bank governor is in power during a period of high inflation, if
   the policy rate is below the one that is prescribed by the Taylor rule
   and during times of heightened stock and exchange rate volatility.},
Publisher = {ELSEVIER SCIENCE INC},
Address = {STE 800, 230 PARK AVE, NEW YORK, NY 10169 USA},
