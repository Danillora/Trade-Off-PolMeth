@article{ WOS:000460403200004,
Author = {Giorgi, Alberta and Annicchino, Pasquale},
Title = {Do Not Cross the Line: The State Influence on Religious Education},
Journal = {POLITICS AND RELIGION},
Year = {2019},
Volume = {12},
Number = {1},
Pages = {S55-S78},
Abstract = {The issues related to the role of religion in the public education
   system have been a public topic for a long time, and related debates
   have been cyclically revived by specific events. In this contribution,
   we explore the reasons why Italian grassroots actors do not tend to size
   up the European Court of Human Rights (ECtHR) jurisprudence and the
   plurality of juridical regimes dealing with religion and education as
   windows of opportunity. First, we analyze the intertwinement of
   different juridical regimes dealing with religion and education, and the
   national case law on the topic. Then, drawing on original
   semi-structured interviews, we analyze the indirect effects of the ECtHR
   case law on the mobilizations and advocacy strategies at the grassroots
   level around religion and education. Finally, we discuss the research
   outcomes, outlining how the non-interference of the Court in
   state-religions regimes may result in the limited impact and
   effectiveness of the Court's protection of religious freedoms.},
