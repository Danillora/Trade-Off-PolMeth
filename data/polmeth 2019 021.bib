@incollection{ WOS:000798808500012,
Author = {Siani, Alberto L.},
Editor = {Knoll, M and Snyder, S and Simsek, N},
Title = {Rawls on Overlapping Disagreement and the Problem of Reconciliation},
Booktitle = {NEW PERSPECTIVES ON DISTRIBUTIVE JUSTICE: DEEP DISAGREEMENTS, PLURALISM,
   AND THE PROBLEM OF CONSENSUS},
Year = {2019},
Pages = {207-224},
Abstract = {This paper claims that Rawls's idea of a political liberalism has a
   radical innovative potential for political philosophy that still needs
   to be fully appreciated. To this aim, the paper works between the poles
   of overlapping disagreement and reconciliation. Political liberalism
   relinquishes once and for all the idea that an adequate philosophical
   account of rationality is capable of establishing political consensus
   even among reasonable individuals. Accordingly, political agreement, if
   possible at all, has to be achieved on the basis of existing political
   ideas, whereby political liberalism delimits the domain of public
   reason, i. e., the space of the public discussion on the most reasonable
   political conception, which, according to Rawls, is ``justice as
   fairness{''}. Hence, the task of political liberalism and that of
   justice as fairness constitute the two complementary yet distinct
   aspects of a project of philosophical reconciliation, the limits of
   which this paper discusses in order to consolidate the thesis that
   overlapping disagreement can never be fully dissolved through
   philosophical means.},
Publisher = {WALTER DE GRUYTER GMBH},
