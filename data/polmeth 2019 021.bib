@article{ WOS:000458305400005,
Author = {Terai, Kimiko and Glazer, Amihai},
Title = {Why principals tolerate biases of inaccurate agents},
Journal = {ECONOMICS \& POLITICS},
Year = {2019},
Volume = {31},
Number = {1},
Pages = {97-111},
Month = {MAR},
Abstract = {Some agents are more accurate than others in estimating the best policy.
   The more accurately an agent estimates a policy's effects, the more he
   will resist biases, such as bribes from a special interest. Thus, a
   special interest needs to pay a larger bribe to an accurate agent than
   to an inaccurate agent. The accurate agent who is biased will then more
   likely cause harm than does an inaccurate agent who favors the special
   interest. Therefore, the principal may gain more from controlling biases
   of an accurate agent than of an inaccurate one. Thus, high ability of
   public officials may be associated with little corruption.},
