@article{ WOS:000488329000001,
Author = {Richards, Imogen},
Title = {A Philosophical and Historical Analysis of ?Generation Identity?:
   Fascism, Online Media, and the European New Right},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
