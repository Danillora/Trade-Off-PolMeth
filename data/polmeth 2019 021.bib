@article{ WOS:000486358300026,
Author = {Albanese, Giuseppe and Cioffi, Marika and Tommasino, Pietro},
Title = {Legislators' behaviour and electoral rules: Evidence from an Italian
   reform},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2019},
Volume = {59},
Pages = {423-444},
Month = {SEP},
Abstract = {We explore how electoral rules and the degree of civicness of the voters
   interact in shaping elected officials' behaviour. We examine the
   expenditure proposals sponsored by Italian Senators from 1994 to 2013
   and exploit the 2005 electoral reform that transformed a mainly
   majoritarian system into a mainly proportional one. First, we find that
   legislators elected in first-past-the-post districts are more likely to
   sponsor pork-barrel bills and to put effort into legislative activity
   than those elected with a closed-list proportional system. Second, more
   importantly, we show that the effects of the electoral rules are muted
   in areas with high civicness.},
