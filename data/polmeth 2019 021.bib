@article{ WOS:000502885000018,
Author = {Aycinena, Diego and Rentschler, Lucas},
Title = {Entry in contests with incomplete information: Theory and experiments},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2019},
Volume = {60},
Month = {DEC},
Abstract = {This paper studies entry decisions in contests with private values.
   Potential contestants observe their value and the common opportunity
   cost of entry, and make entry decisions simultaneously. Theory predicts
   that whether or not contestants are informed of the number of entrants
   prior to choosing their expenditures has no effect on entry or aggregate
   expenditures. We test these assertions in our experiments. We find
   substantial over-entry in both information structures. However, entry is
   higher when contestants are informed. Since expenditures do not, on
   average, differ across information structures, aggregate expenditure is
   also higher when contestants are informed. Contestants earn on average
   less than the opportunity cost of entry.},
Publisher = {ELSEVIER SCIENCE INC},
