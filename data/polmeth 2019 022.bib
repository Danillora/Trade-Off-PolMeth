@article{ WOS:000471786600001,
Author = {Adair-Toteff, Christopher},
Title = {Mannheim, Shils, And Aron and The `End of Ideology' Debate},
Journal = {POLITICS RELIGION \& IDEOLOGY},
Year = {2019},
Volume = {20},
Number = {1},
Pages = {1-20},
Abstract = {This essay traces the history of the concept of ideology from its
   beginnings in the early nineteenth century to the beginnings of the
   Twenty-first but pays close attention to those of Karl Mannheim, Edward
   Shils, and Raymond Aron. Not only are these three scholars experts on
   the concept of ideology, they worked with one another to help develop
   and refine its meaning and its application. With this examination of
   their discussions, we gain a more precise understanding of what an
   ideology is and a fuller sense of how it functions.},
