@incollection{ WOS:001364569900010,
Author = {Harris, Jeffrey Ryan},
Editor = {Choudhury, M and Watkins, DJ},
Title = {Jansenism, popular sovereignty, and the general will in the
   pre-Revolutionary crisis},
Booktitle = {BELIEF AND POLITICS IN ENLIGHTENMENT FRANCE: Essays in Honor of Dale K.
   Van Kley},
Series = {Oxford University Studies in the Enlightenment},
Year = {2019},
Pages = {221+},
Abstract = {Dale K. Van Kley's oeuvre has established that Jansenism's fragmented
   legacies still constituted a major political force in the
   pre-Revolutionary crisis of 1786-1789 and, during the Revolution itself,
   in the making of the Civil Constitution of the Clergy. This essay argues
   that, in one crucial respect, the influence of parlementary-Jansenist
   constitutionalism as a separate ideological front in the ``Revolutionary
   synthesis{''} was more significant in the pre-Revolutionary ideological
   conflict than even Van Kley has supposed. This essay further argues that
   Jansenists invented a distinctly Jansenist version of the ``general
   will{''} that, with Rousseau's and other variants, was integral to
   Revolutionary political culture.},
