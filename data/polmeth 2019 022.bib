@article{ WOS:000477968600017,
Author = {Kenda, Anka Mohoric},
Title = {Classification of Patient Complaints and Developing Patient Complaints
   Indicators},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
