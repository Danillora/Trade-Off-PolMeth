@article{ WOS:000458685200006,
Author = {Mayer, Michael},
Title = {Trigger happy: The foundations of US military interventions},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2019},
Volume = {42},
Number = {2, SI},
Pages = {259-281},
Month = {FEB 23},
Abstract = {The United States has repeatedly intervened militarily in situations
   where tactical success on the battlefield did not translate into
   meaningful political resolution of the issues triggering the
   introduction of military force. Although US military interventions are
   hardly a recent phenomenon, a series of systemic, political and
   institutional developments over the past several decades have been
   particularly conducive to the limited use of force as a policy option.
   These factors have reduced the costs and risks of military intervention,
   incentivising the use of force in situations when it may not be the
   optimal policy response.},
