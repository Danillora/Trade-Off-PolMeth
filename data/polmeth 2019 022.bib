@incollection{ WOS:001364569900013,
Author = {Watkins, Daniel J.},
Editor = {Choudhury, M and Watkins, DJ},
Title = {A suppression revisited: Jansenism, conservatism, and the anti-Jesuit
   ordinances of 1828},
Booktitle = {BELIEF AND POLITICS IN ENLIGHTENMENT FRANCE: Essays in Honor of Dale K.
   Van Kley},
Series = {Oxford University Studies in the Enlightenment},
Year = {2019},
Pages = {275+},
Abstract = {This essay uncovers the history of the anti-Jesuit ordinances of 1828 by
   looking at the ways in which Jansenist writers employed arguments that
   divided the conservative coalition in Restoration France. Jansenists
   constructed histories of the Jesuits that implicated them in the causes
   of the Revolution and in so doing invented an anti-Jesuit narrative that
   appealed to conservative sensibilities. Their arguments and the
   reactions against them within other strands of the conservative
   coalition reveal how nineteenth-century conservatism comprised a diverse
   spectrum of religious and political positions each with its own
   historical memories and agendas. Much like the Catholic Church of the
   eighteenth century, the nineteenth-century conservative alliance
   contained certain internal tensions that ignited most violently in
   issues of religious politics.},
