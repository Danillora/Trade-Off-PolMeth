@article{ WOS:000467658500003,
Author = {Pabst, Adrian},
Title = {`Obligations written in the heart': the primacy of association and the
   renewal of political theology},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2019},
Volume = {22},
Number = {2, SI},
Pages = {300-326},
Month = {JUN},
Abstract = {Notions of anarchy and an artificial state-centric system underpin much
   of modern political and IR theory. However, the primacy of cultural
   association over anarchy and artifice has once again come to the fore,
   notably in constructivist and `culturalist' approaches. The paper argues
   that these approaches make a valuable contribution to our understanding
   of the international but rest on a secular ontology that brackets
   religion and transcendent notions of morality and justice out of the
   equation. By contrast, the work of Edmund Burke offers an alternative
   conception of the international. Contrary to constructivism, Burke
   considers association not as socially constructed but as reflecting a
   natural order governed by customs and traditions. Such an order gives
   rise to `commonwealths of culture' governed by a transcendent morality
   that for Burke is God-given. And contrary to `culturalist' approaches,
   Burke conceptualises identity not in essentialist terms but rather as an
   organically evolving reality that is shaped by both ideas and material
   forcesnotions of common humanity and universal standards of justice,
   which are mediated through history and embodied in particular practices.
   For Burke, the international requires `obligations written in the
   heart', which are more fundamental than the formal standards of `papers
   and seals'.},
