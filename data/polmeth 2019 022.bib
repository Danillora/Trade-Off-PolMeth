@article{ WOS:000454833600006,
Author = {Ershova, Anastasia},
Title = {The Watchdog or the Mandarin? Assessing the impact of the Directorates
   General on the EU legislative process},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2019},
Volume = {26},
Number = {3},
Pages = {407-427},
Month = {MAR 4},
Abstract = {This article assesses the role of the Directorates Generals (DGs) led by
   the Commissioners in the EU legislative decision-making by testing two
   perspectives: `the Watchdog' and `the Mandarin' of the EU principals.
   The former suggests that a conflict of preferences between the DGs will
   notify the EU principals of prospective bureaucratic drift, leading to
   more restrictive discretionary limits imposed on the Commission in the
   implementation stage. However, if the lead DG acts as the Mandarin,
   proximity of its preference to the EU principals should result in more
   executive leeway as its policy preference is more beneficial for the EU
   legislators. To tests the conjunctions, this study focuses on two
   dimensions of EU political space: Left-Right (LR) and Pro/Anti-EU. The
   results suggest that the legislators will indulge the Commission with
   more leeway for the implementation when their preferences are more
   congruent with the position of the lead DG on the LR dimension, thus
   supporting `the Mandarin' perspective.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
