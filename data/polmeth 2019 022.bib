@article{ WOS:000493809500002,
Author = {Stankovic, Marko and Vuletic, Vladimir},
Title = {Amendments to the Serbian Law on Local Self- Government 2018-Goals and
   Achievements},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2019},
Volume = {17},
Number = {4},
Pages = {917-938},
Month = {OCT},
Abstract = {The paper analyzes the constitutional and legal foundations of local
   self-government in Serbia. The long-awaited reform of local
   self-government had two aspects: the correction of technical errors and
   alignment with the amendments to other laws, as well as the adoption of
   amendments related to the essence of local self-government. The second
   aspect mainly covered four issues: the competencies of local
   self-government, local self-government bodies, the position of the local
   community and the cooperation between units of local self-government.
   This reform, however, did not meet expectations, as it did not
   adequately solve any of the five key problems of local self-government
   in Serbia.},
