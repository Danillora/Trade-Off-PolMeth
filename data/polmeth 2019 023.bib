@incollection{ WOS:001364569900004,
Author = {Bell, David A.},
Editor = {Choudhury, M and Watkins, DJ},
Title = {A lawyer and a citizen revisited: the case of Claude-Joseph Prevost
   (1674-1753)},
Booktitle = {BELIEF AND POLITICS IN ENLIGHTENMENT FRANCE: Essays in Honor of Dale K.
   Van Kley},
Series = {Oxford University Studies in the Enlightenment},
Year = {2019},
Pages = {89+},
Abstract = {This essay is a biographical study of the Parisian Jansenist barrister
   Claude-Joseph Prevost (1674-1753). Prevost has historical importance in
   his own right, as the man who first shaped the Parisian Order of
   Barristers into an organization committed to defending Jansenists. He
   was also the mentor to the great Jansenist publicist and strategist
   Louis-Adrien Le Paige, and, like Le Paige, a member of the convulsionary
   movement. But his life and career also illustrate how social identities
   created affinities that could dispose people toward particular political
   and religious views. The essay insists that, to understand why people
   became Jansenists and defended Jansenists, we must place them into as
   broad a social context as possible.},
