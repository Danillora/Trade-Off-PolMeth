@article{ WOS:000466449200001,
Author = {Postula, Marta and Czekaj, Miroslaw and Klepacki, Jaroslaw},
Title = {Stress Tests as a Tool to Assess the Risk of Public Expenditure on Debt
   Servicing Increasing at Local Government Entities},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2019},
Volume = {17},
Number = {2},
Pages = {205-224},
Month = {APR},
Abstract = {National and local government's public debt is of special interest to
   economic sciences. With interest rates in the EU countries now at
   historically low levels, there is risk of a trend change causing
   development expenditure to be crowded out from budgets in favour of debt
   servicing costs. The analysis (stress test) of the sensitivity of debt
   servicing costs in the local government sector in Poland to an
   unexpected growth of market interest rates above the values forecasted
   by the MoF confirms that local governments are exposed to interest rate
   growth risk, and consequently to the risk of their budgets incurring
   unplanned expenditure.},
