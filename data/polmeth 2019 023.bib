@article{ WOS:000482579400001,
Author = {Pena-Miguel, Noemi and Cuadrado-Ballesteros, Beatriz},
Title = {Partisan and Electoral Cycles in Privatisation},
Journal = {POLITICAL STUDIES},
Year = {2020},
Volume = {68},
Number = {3},
Pages = {617-633},
Month = {AUG},
Abstract = {This study evidences the existence of partisan and electoral cycles in
   privatisation, by using a sample composed of 22 European countries
   during the period 1995-2013. Our findings suggest that privatisation
   reforms tend to be implemented by governments with different ideologies
   when elections are not immediate; however, results support a greater
   orientation of right-wing governments towards privatisation reforms in
   election periods. After the elections, right-wing governments continue
   adopting privatisation reforms, but governments from ideological
   traditions tend to oppose such reforms.},
