@article{ WOS:000458685200004,
Author = {Jensen, Benjamin and Valeriano, Brandon and Maness, Ryan},
Title = {Fancy bears and digital trolls: Cyber strategy with a Russian twist},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2019},
Volume = {42},
