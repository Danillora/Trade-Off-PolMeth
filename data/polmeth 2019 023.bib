@article{ WOS:000492038400010,
Author = {Persson, Anna and Rothstein, Bo and Teorell, Jan},
Title = {Getting the basic nature of systemic corruption right: A reply to
   Marquette and Peiffer},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2019},
Volume = {32},
