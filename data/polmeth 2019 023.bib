@article{ WOS:000509527100004,
Author = {Rosenberg, Andrew S.},
Title = {Measuring Racial Bias in International Migration Flows},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
Year = {2019},
Volume = {63},
Number = {4},
Pages = {837-845},
Month = {DEC},
Abstract = {Are international migration flows racially biased? Despite widespread
   consensus that racism and xenophobia affect migration processes, no
   measure exists to provide systematic evidence on this score. In this
   research note, I construct such a measure-the migration deviation.
   Migration deviations are the difference between the observed migration
   between states, and the flow that we would predict based on a racially
