
@article{ WOS:000456605700004,
Author = {Pierre, Jon and Licht, Jenny de Fine},
Title = {How do supreme audit institutions manage their autonomy and impact? A
   comparative analysis},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2019},
Volume = {26},
Number = {2},
Pages = {226-245},
Abstract = {Supreme audit institutions (SAIs) are gradually becoming important
   agents of public management reform; a development raising issues of
   autonomy and potential capture by auditees as well as by the political
   system. Drawing on exploratory case studies of the rules and strategies
   of four high-performing SAIs - those of Australia, New Zealand, Norway
   and Sweden - the article shows that SAIs have different approaches to
   the balance between autonomy and impact. These results imply that there
   is not only one way of organizing an efficient and autonomous SAI, but
   that different positions can prove viable.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
