@article{ WOS:000467854500005,
Author = {Pridgeon, Stephanie M.},
Title = {Metamorphoses of 1968: Latin America and the Israel-Palestine Question
   in Tomas Abraham's La dificultad (2015)},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2019},
Volume = {46},
Number = {3, SI},
Pages = {55-70},
Month = {MAY},
Abstract = {Throughout Tomas Abraham's novel of ideas La dificultad, Judaism is
   revealed to be central to the protagonist's understandings of
   revolutionary politics, philosophy, and his own identity. As is apparent
   in his affinity with Palestinian causes as a form of anti-imperialist
   solidarity, life experiences and politics are inseparable. That Abraham
   should have chosen to focus this autobiographical novel on the
   Hungarian-Argentine Jewish narrator's experiences with the Paris student
   movements of 1968 suggests that revolutionary movements and the
   challenges to the global Jewish community continue to affect his
   identity as a Jew, an Argentine, and a philosopher.},
