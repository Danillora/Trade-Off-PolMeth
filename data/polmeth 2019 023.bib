@article{ WOS:000478614700006,
Author = {Fawcett, Paul and Jensen, Michael J. and Ransan-Cooper, Hedda and Duus,
   Sonya},
Title = {Explaining the ``ebb and flow{''} of the problem stream: frame conflicts
   over the future of coal seam gas ({''}fracking{''}) in Australia},
Journal = {JOURNAL OF PUBLIC POLICY},
Year = {2019},
Volume = {39},
Number = {3},
Pages = {521-541},
Month = {SEP},
Abstract = {Why do issues ``fade{''} from the problem stream? This is an important
   but underresearched question, which this article examines by looking at
   the dynamic interaction between frames and frame sponsors. We develop a
   novel methodological approach that combines algorithmic coding (topic
   modelling) with hand-coding to track changes in the presence of frames
   and frame sponsors during periods of intense problematisation
   ({''}problem windows{''}) both within continuous contexts and
   diachronically across different contexts. We apply this approach
   empirically in a corpus of newspaper articles that pertain to the coal
   seam gas controversy in Australia - a divisive policy issue where frame
   conflicts are common. We find that elite actors have a particularly
   decisive impact on the problem stream in terms of both the evolution and
   duration of debate. Further, problem windows close in response to three
   different mechanisms: elite frame convergence; public statements (by
   government and industry); and elections.},
