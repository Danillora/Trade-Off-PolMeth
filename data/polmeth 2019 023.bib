@article{ WOS:000477667600007,
Author = {Stoychev, Stoycho P. and Tomova, Gergana},
Title = {Campaigning Outside the Campaign: Political Parties and Referendums in
   Bulgaria},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
