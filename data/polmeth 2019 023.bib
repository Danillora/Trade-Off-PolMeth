@article{ WOS:000474245400010,
Author = {Bellinger, Nisha M. and Son, Byunghwan},
Title = {Political Parties and Foreign Direct Investment Inflows among Developing
   Countries},
Journal = {POLITICAL STUDIES},
Year = {2019},
Volume = {67},
Number = {3},
Pages = {712-731},
Month = {AUG},
Abstract = {This article focuses on the nature of party systems to explain
   variations in foreign direct investment inflows within developing
   democracies. We hypothesize a positive relationship between the
   effective number of parliamentary parties and foreign direct investment
   inflows. Large effective number of parliamentary parties is indicative
   of the expropriation risks as well as stability of the political
   environment of host countries. We thus argue that expropriation risks
   are low when the presence of multiple parties makes drastic, impulsive
   changes in economic policies difficult. We also suggest that a larger
   number of parties represent diverse societal interests better, reducing
   the chances of underrepresented social groups driving political
   instability. The relationship between effective number of parliamentary
   party and foreign direct investment inflows is tested on a sample of 56
   developing democracies from 1985 to 2011. The evidence presented lends
   strong support to the argument and is found robust to a number of
   alternative empirical scenarios.},
