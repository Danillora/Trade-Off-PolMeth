@article{ WOS:000493769900002,
Author = {Wille, Tobias},
Title = {Representation and agency in diplomacy: how Kosovo came to agree to the
   Rambouillet accords},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
