@article{ WOS:000478599100004,
Author = {Kadioglu, I. Aytac},
Title = {The Oslo Talks: Revealing the Turkish Government's Secret Negotiations
   with the PKK},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2019},
Volume = {42},
Number = {10},
Pages = {915-933},
Month = {OCT 3},
Abstract = {This article explores the extent to which the secret talks influenced
   the path of official negotiations toward ending Turkey's Kurdish
   conflict, which the scholarly literature has yet to assess. Utilizing
   interviews with key political actors, this article aims to close this
   gap by assessing the Oslo talks (2006-2011) as the most comprehensive
   secret contact between the Turkish government and Kurdistan Workers'
   Party. It demonstrates that the secret track is not merely a
   ``pre-negotiation{''} stage aimed at de-escalating the violent conflict
   ahead of official talks, but also a crucial part of the negotiation
   stage aimed at establishing a final political agreement.},
