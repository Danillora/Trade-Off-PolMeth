@article{ WOS:000464944900007,
Author = {Hvidman, Ulrik},
Title = {Citizens' Evaluations of the Public Sector: Evidence From Two
   Large-Scale Experiments},
Journal = {JOURNAL OF PUBLIC ADMINISTRATION RESEARCH AND THEORY},
Year = {2019},
Volume = {29},
Number = {2},
Pages = {255-267},
Month = {APR},
Abstract = {The performance of public sector agencies constitutes a longstanding
   concern in both policy and academia. Recent research suggests that
   citizens associate the public sector with ineffectiveness and
   wastefulness. This article approaches this debate by examining how
   citizens' perception of whether an organization is public or not affects
   their performance assessment. The study is based on two large-scale
   experiments conducted among representative samples of Danish citizens.
