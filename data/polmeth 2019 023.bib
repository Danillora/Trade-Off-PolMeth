@article{ WOS:000474235600003,
Author = {Rhodebeck, Laurie and Gainous, Jason and Gray, Tricia},
Title = {Partisan values and gay rights: Public opinion about employment
   nondiscrimination},
Journal = {POLITICS},
Year = {2019},
Volume = {39},
Number = {3},
Pages = {300-314},
Month = {AUG},
Abstract = {Partisan elites justify policy positions by invoking underlying values,
   and political parties are associated with value reputations that connect
   particular values to specific policy positions. Value recruitment theory
   explains the relationship between value framing and policy positions.
   Newspaper content analysis and statistical analysis of survey data show
   that Democrats are more likely to frame employment nondiscrimination
   against gay rights as an equality issue, while Republicans are more
   likely to frame it as morality- and capitalism-based values.
   Surprisingly, however, equality framing has a stronger effect on
   Republicans. The study extends research on nondiscrimination in
   employment with an empirical test of value recruitment theory. The
   results largely confirm expectations that the application of values can
   be shaped through citizen attachment to parties, generate insights into
   value recruitment in policy debates, and point to other questions for
   further analysis.},
