@article{ WOS:000476441600001,
Author = {Kostadinova, Tatiana and Neshkova, I, Milena},
Title = {Reforming post-Communist bureaucracies: The effect of transitional
   justice},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2020},
Volume = {33},
