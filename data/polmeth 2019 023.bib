@article{ WOS:000469838000005,
Author = {Hollyer, James R. and Rosendorff, B. Peter and Vreeland, James Raymond},
Title = {Why Do Autocrats Disclose? Economic Transparency and Inter-elite
   Politics in the Shadow of Mass Unrest},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2019},
Volume = {63},
Number = {6},
Pages = {1488-1516},
Month = {JUL},
Abstract = {Autocratic governments hold a preference for opacity. Autocracies are
   less transparent than democracies and a closed informational environment
   preserves autocratic regimes from mass unrest. Yet, autocracies vary
   widely in the extent to which they disclose economic information. In
   this article, we offer an explanation for why some autocrats choose to
   disclose. We contend that, paradoxically, some autocratic leaders may
   benefit from increasing the capacity of the populace to mobilize. In so
   doing, autocratic leaders threaten rival members of the elite, reducing
   the risk of elite challenges and increasing their freedom of maneuver.
   We contend that transparency acts as one mechanism toward these ends. We
   formalize these intuitions and demonstrate empirically that leaders in
   transparent autocracies enjoy a reduced hazard of removal via coup
   relative to their opaque counterparts. Personalistic dictators and
   entrenched autocrats-who suffer the smallest risk of sanctioning by
   their elites-are particularly unlikely to disclose information.},
