@article{ WOS:000463576000010,
Author = {Keatinge, Tom and Keen, Florence},
Title = {Social Media and (Counter) Terrorist Finance: A Fund-Raising and
   Disruption Tool},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2019},
Volume = {42},
Number = {1-2, SI},
Pages = {178-205},
Month = {FEB 1},
Note = {International Conference on Terrorism and Social Media (TASM), Swansea
   Univ, Swansea, WALES, JUN 27-28, 2017},
Organization = {Swansea Univ Cyberterrorism Project},
Abstract = {The proliferation of social media has created a terrorist finance
   vulnerability due to the ease with which propaganda can be spread,
   promoting fund-raising for a certain cause. Social media companies
   recognize the importance of preventing violent extremist and terrorist
   content, but less attention is paid to their fund-raising role. As well
   as presenting a threat, the movement of terrorist fund-raising
   activities online creates a disruption opportunity. This article argues
   that social media companies need to display greater awareness of their
   vulnerability to supporting terrorist financing and greater
   collaboration with law enforcement and financial institutions to
   strengthen the integrity of the system against abuse.},
