@article{ WOS:000470320400010,
Author = {Robins, Daniel},
Title = {Brazilians in London: Ideology, Social Class, and Motivations for
   Migration, Settlement, and Return},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2019},
Volume = {46},
Number = {4, SI},
Pages = {154-168},
Month = {JUL},
Abstract = {The United Kingdom now boasts the largest Brazilian population in
   Europe, but so far the processes behind this relatively new migration
   stream remain undertheorized. An examination of Brazilian migration to
   London suggests that, although increased access to economic capital
   certainly plays a role in it, what migrants do with this capital may
   vary significantly and, further, that these differences are related to
   social class and ideology. The likelihood that they will settle or
   return is also linked to the degree to which they identify with the
   dominant ideology of the destination society.},
