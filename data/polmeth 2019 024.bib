@incollection{ WOS:000444179000005,
Author = {Ledstrup, Martin},
Book-Author = {Ledstrup, M},
Title = {The National and the Fashionable: Everyday Nationhood as Dress},
Booktitle = {NATIONALISM AND NATIONHOOD IN THE UNITED ARAB EMIRATES},
Series = {Modern Muslim World},
Year = {2019},
Pages = {89-108},
Abstract = {This chapter sees ordinary fashion as an exemplary instance of what
   Georg Simmel understood by modernity. Specifically, asking about the
   wearing of national dress in the Emirate of Ras Al Khaimah reveals that
   the fashionable becomes a concrete testimony to how the modern subject
   navigates social forms of the national between collective stabilization
   and individual transgression in everyday interaction. It begins with
   Georg Simmel's understanding of fashion and the way it was criticized by
   Herbert Blumer. It then shows how the problem of fashion reverberates in
