@article{ WOS:000491769600001,
Author = {Jadoon, Amira and Milton, Daniel},
Title = {Strength from the Shadows? How Shadow Economies Affect Terrorist
   Activities},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2022},
Volume = {45},
Number = {5-6, SI},
Pages = {517-534},
Month = {JUN 3},
Abstract = {Despite scholarly interest in the existence and consequences of the
   crime-terror nexus, little research has been done to examine how the
   presence of an illegal economic area, known as a shadow economy, impacts
   terrorist activity in a country. We propose a theory which argues that
   terrorists are able to rely on the partnerships and resources available
   in the shadow economy to increase their organizational capacity. Using
   data on terrorist activity and the size of the shadow economy, our
   empirical results show that the larger the shadow economy, the greater
   the number of overall terrorist attacks, fatalities, and certain types
   of mass-casualty events that a country experiences.},
Publisher = {TAYLOR \& FRANCIS INC},
Address = {530 WALNUT STREET, STE 850, PHILADELPHIA, PA 19106 USA},
