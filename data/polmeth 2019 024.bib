@article{ WOS:000461042600005,
Author = {Thiele, Leslie Paul},
Title = {Geoengineering and sustainability},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2019},
Volume = {28},
Number = {3, SI},
Pages = {460-479},
Month = {APR 16},
Abstract = {Geoengineering is regarded by advocates as a creative and responsible
   technological option in the face of a climate emergency. Critics often
   see it as a hubristic attempt to play God, with disastrous consequences
   for the planet and humanity. These antipodal perspectives are
   represented by the ideal types of Prometheans and Gaians. Prometheans
   and Gaians typically talk past each other. The geoengineering debate can
   be made more fruitful by well articulating their respective positions
   and subsequently situating them in the discourse of sustainability. A
   sustainability orientation does not answer the troubling question
   whether geoengineering should be developed and deployed. But it can
   foster a `fusion of horizons' between Prometheans and Gaians, providing
   common ground in an otherwise polarized debate and making a more
   productive dialogue possible.},
