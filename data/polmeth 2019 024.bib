@article{ WOS:000467878600006,
Author = {Maks-Solomon, Cory and Stoker, Robert P.},
Title = {Weathering the Storm: Social Policy and the Great Recession},
Journal = {POLICY STUDIES JOURNAL},
Year = {2019},
Volume = {47},
Number = {1, SI},
Pages = {S119-S137},
Month = {MAY},
Abstract = {This review discusses recent challenges to the welfare state arising
   from the Great Recession (GR). The GR was a significant event for social
   policy analysts, as it tested the responsiveness of welfare systems in
   the midst of a recent trend toward austerity politics in advanced
   economies. Social policy changes were part of the toolkit advanced
   democracies used to respond to the GR, and the welfare state mitigated
   the consequences of the GR. However, a stark limitation of the social
   safety net in the United States was the failure to assist immigrant
   households. The nexus of immigration and social policy is likely to be a
   significant controversy as we consider the meaning of social
   citizenship.},
