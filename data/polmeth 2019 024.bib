Author = {Peic, Goran},
Title = {Divide and Co-Opt: Private Agendas, Tribal Groups, and Militia Formation
   in Counterinsurgency Wars{*}},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2021},
Volume = {44},
