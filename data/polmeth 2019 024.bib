@article{ WOS:000476342000001,
Author = {Pennell, C. R.},
Title = {The Algerian State, Islamist Insurgents, and Civilians Caught in Double
   Jeopardy by the Violence of the Civil War of the 1990s},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2021},
Volume = {33},
Number = {7},
Pages = {1447-1468},
Month = {OCT 3},
Abstract = {During the Algerian Civil War of the 1990s responsibility for both
   targeted assassinations of prominent politicians and political activists
   and largescale massacres was frequently ascribed to both the government
   and the Islamic insurgents of the GIA. The same was true of the more
   mundane but much more numerous level of individuals who fell foul of
   both sides in the conflict and were frequently the targets of both.
   Using material from the asylum tribunals of several western countries
   this article describes how the widespread fear among the Algerian
   population was the result of the strategies of the government and GIA
   that both sought to intimidate, punish and exact revenge at a personal
   level leading to a widespread social dislocation.},
