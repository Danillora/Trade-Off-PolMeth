@article{ WOS:000464908400002,
Author = {Zimmermann, Lisbeth},
Title = {Beyond diffusion: cyclical translation of international rule-of-law
   commission models in Guatemala},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2019},
Volume = {22},
Number = {1},
Pages = {28-49},
Month = {MAR},
Abstract = {According to the prevailing diffusion approaches, norms and
   institutional models in the international realm emerge and are diffused
   through transnational advocacy networks. According to localisation
   research, they are adopted and changed by local actors, who adjust them
   to fit their interests and normative context. Neither approach takes
   into account the cyclical element of translation. Rather, they both
   present unidirectional models of diffusion. Focusing on Guatemala, this
   article shows how institutional models for international rule-of-law
   commissions in post-conflict states were translated in a cyclical
   process between different domestic and international contexts. It
   outlines two modes of cycles in this process: (1) norm application and
   (2) international norm change.},
