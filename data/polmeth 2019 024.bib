@article{ WOS:000484468500001,
Author = {Mahrenbach, Laura C.},
Title = {How shared ideas may hinder lobbying success: British preference
   formation in the G20},
Journal = {CAMBRIDGE REVIEW OF INTERNATIONAL AFFAIRS},
Year = {2020},
Volume = {33},
Number = {2},
Pages = {252-273},
Month = {MAR 3},
Abstract = {Much existing work on ideas in policymaking and lobbying has focused on
   how shared ideas can facilitate achievement of policy preferences.
   However, interviews with lobbyists and recent research indicate shared
   ideas may also hinder lobbying success-a topic largely ignored to date.
   I begin to address this gap by mapping two roles for shared ideas in
   lobbying processes: as icebreakers to facilitate communication with
   politicians and as instruments in pursuit of specific interests. I
   subsequently propose pathways via which shared ideas may facilitate or
   hinder lobbying success and illustrate these mechanisms in case studies
   examining British preference formation during G20 negotiations over tax
   reform. The analysis underlines the crucial role of strategic agency and
