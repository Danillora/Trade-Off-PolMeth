@article{ WOS:000494150700001,
Author = {Goerzig, Carolin},
Title = {Deradicalization through Double-Loop Learning? How the Egyptian Gamaa
   Islamiya Renounced Violence},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
