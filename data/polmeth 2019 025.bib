@article{ WOS:000473271600020,
Author = {Villanueva Rivas, Cesar},
Title = {``MUCH ADO ABOUT NOTHING{''}: CULTURAL DIPLOMACY AND THE COUNTRY IMAGE
   OF MEXICO DURING THE PRESIDENCY OF ENRIQUE PENA NIETO},
Journal = {FORO INTERNACIONAL},
Year = {2019},
Volume = {59},
Number = {3-4},
Pages = {1145-1178},
Month = {JUL-DEC},
Abstract = {This article analyzes how the cultural diplomacy of the government of
   Enrique Pella Nieto had a presence in the international relations of
   Mexico, but suffered from a conceptual disorder and irrelevance as a
   tool of foreign policy. The image of Mexico abroad had a good start with
   the so-called Mexican Moment, but collapsed following corruption-related
   events and the escalation of criminal activity, and specifically the
   Ayotzinapa case of 2014. Meanwhile, the budget for cultural diplomacy
   was merely a nominal and decorative figure exercised by the Department
   for Educational and Cultural Cooperation of Amexcid, whose expenditure
   was made irrelevant by the lack of coordination and redundancy of the
   initiatives of the Ministries of Culture, Economy, Tourism and the
   Presidency of the Republic.},
