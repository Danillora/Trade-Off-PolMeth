@incollection{ WOS:001364569900002,
Author = {Burson, Jeffrey D.},
Editor = {Choudhury, M and Watkins, DJ},
Title = {Entangling the ``century of lights{''} to disentangle the Enlightenment},
Booktitle = {BELIEF AND POLITICS IN ENLIGHTENMENT FRANCE: Essays in Honor of Dale K.
   Van Kley},
Series = {Oxford University Studies in the Enlightenment},
Year = {2019},
Pages = {25+},
Abstract = {Interpreting the historical importance of the Enlightenment era has
   become increasingly complex in recent decades. This essay reviews
   prominent competing narratives of historical interpretation concerning
   the Enlightenment with a view toward resurrecting a more satisfying and
   coherent interpretation of the movement (or movements). I argue that
   Dale Van Kley's contention-that eighteenth-century secular and religious
   reform movements might be most profitably studied as emerging from a
   century of ``plural lights{''}-points toward a fruitful paradigm of
   historical interpretation. Van Kley's conclusions, if coupled with
   theoretical perspectives associated with the scholarship of l'histoire
   croisee (historical entanglement), provide a more useful compass to
   navigate the fractured landscape of eighteenth-century studies.},
Publisher = {LIVERPOOL UNIV PRESS},
