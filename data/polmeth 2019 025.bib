@article{ WOS:000486162200001,
Author = {Kari, Martti J. and Pynnoniemi, Katri},
Title = {Theory of strategic culture: An analytical framework for Russian cyber
   threat perception},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2023},
Volume = {46},
Number = {1},
Pages = {56-84},
Month = {JAN 2},
Abstract = {The strategic environment is evolving rapidly with the recognition of
   cyberspace as a domain of warfare. The increased interest in cyber as a
   part of defence has heightened the need for theoretical tools suitable
   to assess cyber threat perceptions and responses to these threats.
   Drawing from previous research, we will formulate an analytical
   framework to study the formation of Russian thinking on cyber threats as
   a part of Russian strategic culture. This article identifies a sense of
   vulnerability, the narrative of Russia as a besieged fortress and the
   technological inferiority of Russia as specific factors influencing
   Russian cyber threat perception.},
