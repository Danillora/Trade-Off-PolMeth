@article{ WOS:000483232100003,
Author = {Landwehr, Jakob},
Title = {No way out? Opportunities for mediation efforts in the Donbas region},
Journal = {EAST EUROPEAN POLITICS},
Year = {2019},
Volume = {35},
Number = {3},
Pages = {291-310},
Abstract = {This article examines why the Minsk II Agreement has been unsuccessful
   in achieving sustainable peace and focuses on the conflict mediators'
   role in overcoming the stalemate. Based on negotiation theory, the
   underlying issues and circumstances before Minsk II and during the
   implementation process until 2019 are identified. The article postulates
   types of mediation leverage to overcome the stalemate. The findings
