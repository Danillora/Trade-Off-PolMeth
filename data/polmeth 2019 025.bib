@article{ WOS:000490434900001,
Author = {Freedman, Lawrence},
Title = {Political Impatience and Military Caution},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2021},
Volume = {44},
Number = {1},
Pages = {91-116},
Month = {JAN 2},
Abstract = {Two examples from recent British campaigns, the first from the Falklands
   War of 1982 and the second from the Iraq War of 2003, are used to
   consider attempts by civilian policy-makers to influence the conduct of
   ongoing military operations as a result of frustration with slow
   progress. Both cases highlight the importance of the higher command
   structures as providing a buffer between the government and local field
   commanders, and show the problems that can result when key strategic
   questions are left unanswered in the run-up to a campaign.},
