@article{ WOS:000473813100001,
Author = {Bianchini, Pascal},
Title = {The 1968 years: revolutionary politics in Senegal},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2019},
Volume = {46},
Number = {160},
Pages = {184-203},
Month = {APR 3},
Abstract = {In tune with the atmosphere of the `global 60s', Senegal experienced a
   major political crisis in May 1968 that began with a student strike,
   followed by the workers in a general strike. May `68 in Senegal was both
   global and local, and these aspects are not in opposition as has
   sometimes been the case in political and academic debates. The article
   goes beyond the events themselves and attempts to shed light on a period
   of `revolutionary politics' that was triggered by the revolt of 1968.
   This has scarcely been documented, but has had a real influence on
   contemporary Senegalese politics.},
