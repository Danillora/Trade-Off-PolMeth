@article{ WOS:000471833600006,
Author = {Bratberg, Oivind},
Title = {Blind zones of political science: Iver Neumann in a professional
   perspective},
Journal = {INTERNASJONAL POLITIKK},
Year = {2019},
Volume = {77},
Number = {2},
Pages = {158-166},
Abstract = {Iver Neumann has been an inter-disciplinary entrepreneur for political
   science in Norway. For Neumann, interdisciplinarity has been coupled
   with an understanding of politics as the search for meaning and
   identity. These features are well-developed in European political
   science, particularly within the field of international relations, but
   they are more rarely encountered in Norway. This brief article provides
   a sketch of political science as it evolved and matured in its Norwegian
   incarnation. In situating Iver Neumann within the discipline, I
   emphasise his international and eclectic orientation. The personal,
   national and international meet in Neumann's works, as do popular
   culture and politics.},
