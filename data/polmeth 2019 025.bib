@article{ WOS:000470062500003,
Author = {Collin, Katherine},
Title = {Peacemaking Referendums in Oceania: Making or Delaying Peace in New
   Caledonia and Bougainville},
Journal = {ETHNOPOLITICS},
Year = {2019},
Volume = {18},
Number = {2},
Pages = {139-157},
Abstract = {Secessionist conflicts present challenges for peacemaking. Separatists'
   and states' irreconcilable demands frequently prevent negotiated
   settlement. One peacemaking model for territorial conflict is temporary
   autonomy followed by a referendum on independence. Both sides have an
   opportunity to persuade voters of their positions while de-escalating
   conflict prior to a polarizing vote. This article examines the
   application of this model in two cases: the French territory of New
   Caledonia and the Papua New Guinean province of Bougainville, and argues
   that the model succeeds in temporary stabilization but may fail to alter
   the irreconcilable territorial claims of states and breakaway regions,
   risking conflict relapse.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
