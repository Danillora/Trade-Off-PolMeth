@article{ WOS:000468060600003,
Author = {van der Westhuizen, Janis and Milani, Carlos R. S.},
Title = {Development cooperation, the international-domestic nexus and the
   graduation dilemma: comparing South Africa and Brazil},
Journal = {CAMBRIDGE REVIEW OF INTERNATIONAL AFFAIRS},
Year = {2019},
Volume = {32},
Number = {1},
Pages = {22-42},
Month = {JAN 2},
Abstract = {Development cooperation is a foreign policy tool marked by deep-seated
   conflicts of interest and dilemmas of particular relevance to
   second-tier and non-nuclear countries that aim to change their
   international status and role. Building on the concept of `graduation
   dilemma', this article compares specific dilemmas that Brazil and South
   Africa face in their foreign policies concerning the development
   cooperation agenda at three levels: the domestic, the regional and the
   global level. The research question guiding this analysis is: how does
   the graduation dilemma manifest in relation to Brazil and South Africa's
   role in development cooperation?},
