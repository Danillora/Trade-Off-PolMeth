@article{ WOS:000474504700003,
Author = {Kailitz, Steffen and Umland, Andreas},
Title = {How post-imperial democracies die: A comparison of Weimar Germany and
   post-Soviet Russia},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2019},
Volume = {52},
Number = {2},
Pages = {105-115},
Month = {JUN},
Abstract = {While socioeconomic crisis - like in Germany after World War I and in
   Russia after the Cold War - is a necessary precondition for democratic
   erosion resulting in a breakdown of democracy, it is not a sufficient
   condition. We identify, in the cases of Weimar Germany and post-Soviet
   Russia, a post-imperial syndrome that includes nationalist irredentism
   and an ambition to return to the status quo ante of a ``great power{''}
   as a main reason why democratization faces specific and enormous
   challenges for former ``great powers.{''} A slide back to
   authoritarianism in post-imperial democracies takes a high toll. It is
   facilitated by international political conflicts, including annexation
   and wars, with new neighbouring states that harbor territories perceived
   as external national homelands like the Sudetenland or Crimea. (C) 2019
   The Regents of the University of California. Published by Elsevier Ltd.
   All rights reserved.},
