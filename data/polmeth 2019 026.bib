@article{ WOS:000454159300004,
Author = {Kemer, Thaise and Pereira, Alexsandro Eugenio and Blanco, Ramon},
Title = {THE PEACEBUILDING COMMISSION IN GUINEA-BISSAU OR THE LIMITS OF THE
   ESTABLISHED PEACE},
Journal = {FORO INTERNACIONAL},
Year = {2019},
Volume = {59},
Number = {1},
Pages = {89-146},
Month = {JAN-MAR},
Abstract = {Based on a case study and archival research, this article discusses the
   practices of the UN Peacebuilding Commission (PBC) in Guinea-Bissau,
   with the aim of rethinking the concept of Peace consolidation that
   arises from the acts of the Commission. The essential argument is that
   the PBC, by assuming the universal validity of the liberal peace
   paradigm, fails to adequately consider the local historical, political
   and socioeconomic conditions. By noting the poor fit between the PBC
   approach and the local reality, this study expands the debate on the
   role of the PBC in scenarios following the contemporary conflicts.},
