@article{ WOS:000481575500005,
Author = {Koktsidis, I, Pavlos},
Title = {The Decision to Use Violence: Opportunity Structures and the Albanian
   Insurgency in the Former Yugoslav Republic of Macedonia},
Journal = {ETHNOPOLITICS},
Year = {2019},
Volume = {18},
Number = {4},
Pages = {383-405},
Abstract = {The heterogeneous practices adopted by politicized ethnic groups and the
   variations in the types and intensity of mobilization pose a significant
   question for conflict analysts. What accounts for the shift of aggrieved
   ethnic groups, with stated political demands, from protest mobilization
   to organized violence? The proposed study builds a conceptual framework
   of violent conflict that allows scholars to look at variables related to
   the feasibility of violent conflict. The study assumes that the decision
   of group actors to use organized violence depends on the perception of
   structural conditions conducive to the emergence of violence: (i)
   ethno-spatial factors; (ii) operational factors; (iii) international
   factors; and (iv) state response. Selected opportunity structures have
   substantially influenced the violent trajectory of the Albanians in the
   Former Yugoslav Republic of Macedonia, establishing some clear
   congruency of theoretical directions with empirical findings. However, a
   comparative examination of variables across conflicts is important for
   identifying and explaining non-agreeable cases of violent and
   non-violent conflict.},
