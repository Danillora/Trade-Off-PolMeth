@article{ WOS:000478453100001,
Author = {Norris, Jesse J.},
Title = {Another Form of American Exceptionalism? A Comparative Analysis of
   Terrorism Sting Operations in the US and Abroad},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2021},
Volume = {33},
Number = {7},
Pages = {1399-1423},
Month = {OCT 3},
Abstract = {Sting operations can potentially thwart terrorist plots, but could also
   threaten civil liberties and alienate communities, making them a
   critical subject for counterterrorism research. Yet despite considerable
   research on U.S. cases, little is known about terrorism stings
   elsewhere. How common are such cases abroad, and how many feature strong
   entrapment claims or result in entrapment-related acquittals? In this
   study, data are gathered about non-U.S. terrorism stings, each of which
   is evaluated for entrapment indicators. Results show that, contrary to
   claims of American exceptionalism, terrorism stings could be identified
   in twenty-one countries, and the average number of entrapment indicators
   per case is similar between the U.S. and several countries. In addition,
   several non-U.S. cases present entrapment claims as strong as some of
   the most-criticized U.S. cases. However, relatively few non-U.S.
   terrorism stings (fifty-one) could be identified, while there are 156
   U.S. cases. In addition, unlike in the U.S., courts have acquitted
   defendants on entrapment grounds in a high proportion of non-U.S. cases.
   Political, cultural, and legal differences between the U.S. and other
   countries, and certain cross-national commonalities, are identified as
   likely accounting for these results. Potential implications of these
   findings for terrorism prevention and legal reform are considered.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
