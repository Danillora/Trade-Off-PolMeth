@article{ WOS:000480500200007,
Author = {Oates, John G.},
Title = {When delegation fails: the politics of indivisible sovereignty},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2019},
Volume = {22},
Number = {3},
Pages = {676-697},
Month = {SEP},
Abstract = {Despite the frequency with which governments delegate authority to
   international organisations, arguments about the indivisibility of state
   sovereignty continue to pose difficulties for governments seeking to
   delegate powers to international organisations. The existing theories of
   delegation assume that sovereignty is always divisible; they thus have
   difficulties accounting for those instances in which principled
   arguments about the need to preserve sovereignty preclude the
   possibility of delegation even when the balance of policy preferences
   would seem to favour it. In this paper, I develop a theory to explain
   the conditions under which sovereignty is constructed as indivisible. I
   distinguish between two discourses of sovereignty, national and
   administrative sovereignty, which represent the purpose of state power
   differently. When a discourse of national sovereignty intersects with a
   set of conditions related to the audience and the institutional rules
   governing delegation choices, sovereignty will be constructed as
   indivisible and lead to delegation failure. This argument is illustrated
   through an analysis of the US Senate's failure to ratify the Convention
   on the Rights of Persons with Disabilities. My argument sheds light on
   the relationship between sovereignty and delegation and identifies an
   important set of factors that help to explain the conditions under which
   delegation fails.},
