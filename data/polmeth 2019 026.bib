@article{ WOS:000462473400004,
Author = {Llano, Mercedes},
Title = {THE POLITICAL DETERMINANTS OF PROFESSIONALIZATION PROCESSES IN LATIN
   AMERICA. A MODEL OF INSTITUTIONAL CHANGE},
Journal = {FORO INTERNACIONAL},
Year = {2019},
Volume = {59},
Number = {2},
Pages = {437-477},
Month = {APR-JUN},
Abstract = {This paper develops a model of institutional change in order to analyze
   the determinants that operate on the level of institutionalization of
   meritocratic bureaucracies in two Latin American countries: Chile and
   Argentina. According to this model, the progress or degree of
   implementation of such meritocracies depends as much on the capacity for
   assertion and organization of civil society and state actors, as on the
   pre-eminence of a particular political and institutional framework. The
   empirical comparisons made by the model indicate that the intensity of
   presidential powers of patronage is the principal determinant of the
   degree of institutionalization of the meritocratic systems evaluated.},
