
@article{ WOS:000464501600013,
Author = {Jensen, Carsten and Kevins, Anthony},
Title = {Numbers and Attitudes towards Welfare State Generosity},
Journal = {POLITICAL STUDIES},
Year = {2019},
Volume = {67},
