@article{ WOS:000488375100001,
Author = {Tarapore, Arzan},
Title = {Defence without deterrence: India?s strategy in the 1965 war},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2023},
Volume = {46},
Number = {1},
Pages = {150-179},
Month = {JAN 2},
Abstract = {Why does combat success sometimes fail to produce a stable and durable
   post-war settlement? In the 1965 war, India successfully defended
   against a Pakistani invasion, but did not improve the long-term security
   of Kashmir or establish deterrence against Pakistan. I argue that, to
   deter rivals after war, states must couple battlefield success with
   credible signals of resolve, such as retaining captured territory or
   risking a wider war. In 1965, India used both denial and punishment
   strategies, but both failed to establish post-war deterrence because it
   judged the necessary signals of resolve to be too costly.},
