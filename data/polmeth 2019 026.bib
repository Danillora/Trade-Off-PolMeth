@article{ WOS:000497035600001,
Author = {Esen, Berk and Gumuscu, Sebnem},
Title = {Killing Competitive Authoritarianism Softly: The 2019 Local Elections in
   Turkey},
Journal = {SOUTH EUROPEAN SOCIETY AND POLITICS},
Year = {2019},
Volume = {24},
Number = {3},
Pages = {317-342},
Month = {JUL 3},
Abstract = {On 31 March 2019 Turkish voters ended the Islamist local governance in
   the country's largest cities after 25 years and handed the ruling AKP
   its most serious electoral defeat since its rise to power in 2002. The
   article explores the electoral strategies of major parties in the local
   election, offers a comparative analysis of the results, and discusses
   post-election developments, including the rerun in Istanbul. The
   election and its aftermath reaffirmed the competitive authoritarian
   nature of the regime, as the governing bloc enjoyed an uneven playing
   field, while the opposition had to meet a higher electoral bar than the
   incumbents to win. The economic crisis, growing discontent with the
   government's policies, and effective coordination of opposition parties
   facilitated this outcome.},
