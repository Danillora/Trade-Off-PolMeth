@article{ WOS:000457164800011,
Author = {Schillemans, Thomas and Bovens, Mark},
Title = {Governance, accountability and the role of public sector boards},
Journal = {POLICY AND POLITICS},
Year = {2019},
Volume = {47},
Number = {1},
Pages = {187-206},
Month = {JAN},
Abstract = {For many public organisations, boards are the primary accountability
   mechanisms for management. Boards have a multipronged portfolio:
   strategic advice, employment, external linkage and critical scrutiny.
   The literature casts serious doubts about their capabilities. There is
   `managerial dominance'. This article reviews existing studies on public
   sector boards and identifies important reasons why boards are likely to
   fail as accountability mechanisms. This sets the stage for a new
   research agenda. When are boards more effective on the micro-level? How
   to organise effective checks and balances around organisations? And why
   has the board-model become so popular: is this learning or
   blame-shifting?},
