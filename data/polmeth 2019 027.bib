@article{ WOS:000497950300006,
Author = {Gjesvik, Lars and Overbo, Emil Jorgensen},
Title = {Deter who? The importance of strategic culture for cybersecurity},
Journal = {INTERNASJONAL POLITIKK},
Year = {2019},
