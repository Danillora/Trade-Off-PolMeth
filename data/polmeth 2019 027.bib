@article{ WOS:000466759400006,
Author = {Fordham, Benjamin O.},
Title = {The Domestic Politics of World Power: Explaining Debates over the United
   States Battleship Fleet, 1890-91},
Journal = {INTERNATIONAL ORGANIZATION},
