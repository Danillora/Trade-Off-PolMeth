@incollection{ WOS:000617032500002,
Author = {Cherniss, Joshua L.},
Editor = {Muller, JW},
Title = {Isaiah Berlin and Reinhold Niebuhr: Cold War Liberalism as an
   Intellectual Ethos},
Booktitle = {ISAIAH BERLIN'S COLD WAR LIBERALISM},
