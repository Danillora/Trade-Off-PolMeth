@article{ WOS:000490669200001,
Author = {Landwehr, Claudia and Harms, Philipp},
Title = {Preferences for Referenda: Intrinsic or Instrumental? Evidence from a
   Survey Experiment},
Journal = {POLITICAL STUDIES},
