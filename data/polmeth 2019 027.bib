@article{ WOS:000493306000002,
Author = {Borriello, Arthur and Brack, Nathalie},
Title = {?I want my sovereignty back!? A comparative analysis of the populist
   discourses of Podemos, the 5 Star Movement, the FN and UKIP during the
   economic and migration crises},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2019},
Volume = {41},
Number = {7, SI},
Pages = {833-853},
Month = {OCT 3},
Abstract = {The multiple crises that the EU has faced over the last decade have
   provided fertile ground for the emergence of new political movements,
   often labelled as ?anti-system?, ?populist? and ?Eurosceptics?. One
   defining characteristic of these parties is their claim to represent
   ?the people? and their reliance on the idea of sovereignty. This article
   aims at examining how these populist parties have framed sovereignty in
   relation to the economic and migration crises. It argues that the binary
   opposition between EU integration and national sovereignty does not tell
   the whole story, and that the populist upsurge reflects instead
   competing versions of sovereignty at the national level. To test this
   hypothesis, we conduct a corpus-based analysis of the discourse of four
   leading populist parties between 2012 and 2017: the Front National, the
   UK Independence Party, the Movimento cinque Stelle and Podemos.},
