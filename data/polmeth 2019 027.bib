
@article{ WOS:000501956800001,
Author = {Marquetti, Adalmir Antonio and Hoff, Cecilia and Miebach, Alessandro},
Title = {Profitability and Distribution: The Origin of the Brazilian Economic and
   Political Crisis},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2020},
Volume = {47},
