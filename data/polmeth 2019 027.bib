@article{ WOS:000476984700004,
Author = {Gently, Kynan and Mason, Victoria},
Title = {``The Invisible Man{''}: H. G. Wells and Human Rights During the
   Interwar Period},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2019},
Volume = {41},
Number = {3},
Pages = {620-645},
Month = {AUG},
Abstract = {H.G. Wells is best known as ``the father of science fiction{''}.
   However, the bulk of his writing is both non-fiction and concerned with
   social justice. While it is widely held that !he Rights of Man (1940)
   helped shape the drafting of the 1948 Universal Declaration of Human
   Rights, this article argues that Well's influence extended well beyond
   this. Through his contribution to rights-based debates concerning social
   liberalism, internationalism, liberal internationalism, and
   international law, between the late 1890s and his death in 1946, Wells
   made crucial interventions in emerging discourses around rights and was
   a significant actor in rights-based civil society.},
