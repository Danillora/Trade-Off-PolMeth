@article{ WOS:000456813700006,
Author = {Mastro, Oriana Skylar},
Title = {It Takes Two to Tango: Autocratic underbalancing, regime legitimacy and
   China's responses to India's rise},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2019},
Volume = {42},
Number = {1},
Pages = {114-152},
Month = {JAN 2},
Abstract = {What factors do autocracies evaluate when responding to perceived
   threats and why might they fail to balance appropriately? I posit that
   autocratic leaders may choose greater exposure to an external threat if,
   by doing so, it preserves regime legitimacy. Specifically, the desire to
   promote a positive image to one's domestic public creates incentives to
   publicly downplay a rival's military progress, which then affects the
   state's ability to mobilize resources to respond to the growing threat.
   I test this theory in the case of China's response to India's military
   rise. This research contributes to balancing theory and empirical work
   on East Asian security.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
