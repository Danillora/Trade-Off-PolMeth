@article{ WOS:000474584200008,
Author = {Goes, Maria},
Title = {Between territory and space: assembling the Murmansk region from a
   security perspective},
Journal = {EAST EUROPEAN POLITICS},
Year = {2019},
Volume = {35},
Number = {2, SI},
Pages = {201-219},
Abstract = {This article explores connections between the Russian state and
   territory from a security perspective. At first, concepts of state,
   territory and space are critically interrogated with the use of Deleuze
   and Guattari's assemblage thinking. The article introduces striated and
   smooth space as analytical concepts revealing connections between the
   state and territory. Analysis exposes the state's attempt to capture the
   Arctic space in terms of state security and border protection. Based on
   the example of local security practices in the Murmansk region, the
   article demonstrates that an understanding of security cannot be reduced
   the borders of nation-state.},
