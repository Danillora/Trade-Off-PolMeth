@article{ WOS:000492038400009,
Author = {Li, Li and Pang, Baoqing and Wu, Yiping},
Title = {Isolated counties, administrative monitoring, and the misuse of public
   funds in China},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2019},
Volume = {32},
Number = {4},
Pages = {779-797},
Month = {OCT},
Abstract = {This article empirically investigates the effects of administrative
   monitoring on the misuse of funds by local governments and provides a
   representative model of fiscal decentralization with political
   centralization, where administrative monitoring relies on the top-down
   hierarchy of the bureaucratic system in China based on a unique data set
   from audit programs. We show a double effect between distance and
   monitoring in local China. The incentive for misusing public funds can
   be strengthened if the county (a) is governed by a leader whose tenure
   is longer than 3 years, (b) is governed by a politician from outside the
   county, or (c) receives more transfer payments. These findings suggest
   that the administrative monitoring of local governments can be vital to
   remedying the misuse of public funds.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Wu, YP (Corresponding Author), Shanghai Univ Finance \& Econ, 777 Guoding Rd, Shanghai 200433, Peoples R China.
   Li, Li, China Univ Polit Sci \& Law, Beijing, Peoples R China.
