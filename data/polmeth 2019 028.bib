@article{ WOS:000493100200022,
Author = {Krishnarajan, Suthan},
Title = {Economic Crisis, Natural Resources, and Irregular Leader Removal in
   Autocracies},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
Year = {2019},
Volume = {63},
Number = {3},
Pages = {726-741},
Month = {SEP},
Abstract = {Why do autocratic leaders escape revolution, coups, and assassination
   during times of economic crisis? I argue that the spike in natural
   resource revenues since the 1960s has increased autocratic crisis
   resilience. The availability of this alternative revenue stream provides
   autocratic leaders with a constant inflow of money, increases their
   ability to repress dissent, and improves their access to international
   credit. Extending the analysis back to 1875, I show that the
   relationship between economic crisis and irregular leader removal in
   autocracies is strong and robust before the 1960s, but disappears in
   more recent periods. Interaction analyses confirm that the effects of
   economic crisis are moderated by natural resource income. These findings
   are robust to an array of alternative specifications, including analyses
   that address endogeneity concerns via instrumental variable (IV)
   estimation. A more particular examination of the theoretical mechanisms
   also supports the argument. These findings challenge widely held beliefs
