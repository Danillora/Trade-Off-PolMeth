@article{ WOS:000464587100001,
Author = {Jung, Yoonjung and Kim, Euiseok and Kim, Wonjoon},
Title = {The scientific and technological interdisciplinary research of
   government research institutes: network analysis of the innovation
   cluster in South Korea},
