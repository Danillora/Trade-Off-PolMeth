@article{ WOS:000667328500004,
Author = {Oberg, Dan},
Title = {Ethics, the military imaginary, and practices of war},
Journal = {CRITICAL STUDIES ON SECURITY},
Year = {2019},
Volume = {7},
Number = {3, SI},
Pages = {199-209},
Month = {SEP 2},
Abstract = {Maja Zehfuss' book War and the politics of ethics makes the claim that
   Western practices of war are constituted by debates and ideas of ethics,
   illustrated in particular through the Western wars in Iraq and
   Afghanistan. Herein, this claim is assessed by drawing on Tarak Barkawi
   and Shane Brightons argument that in contemporary research, war tends to
   be conceived through secondary phenomena. The paper argues that research
   focusing on how war is shaped by secondary phenomena often falls back on
   a reductive understanding, in which war is represented as violent
   destruction. Moreover, that by seeing war as a multifaceted form shaped
   by military imaginaries centring on military theory, tactics, and
   operational art, we may gain a more comprehensive understanding of its
   ontology and practices.},
