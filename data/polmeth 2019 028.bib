@incollection{ WOS:000482946200006,
Author = {Wallace, Jennifer},
Book-Author = {Wallace, J},
Title = {Cross-Jurisdictional Analysis},
Booktitle = {WELLBEING AND DEVOLUTION: REFRAMING THE ROLE OF GOVERNMENT IN SCOTLAND,
   WALES AND NORTHERN IRELAND},
Series = {Wellbeing in Politics and Policy},
Year = {2019},
Pages = {127-149},
Abstract = {This chapter explores the key components and characteristics of the
   wellbeing approaches taken by Scotland, Wales and Northern Ireland.
   Despite different origins, there are remarkable similarities between the
   approaches taken by the devolved legislatures. Each framework blends
   objective and subjective data and all three prioritise quality of life
   indicators over environmental and economic indicators. Wallace goes on
   to argue that the impact of wellbeing frameworks on approaches to
   government can be seen as setting wellbeing as an overarching goal for
   government, wellbeing as a conversation to discuss social progress,
   wellbeing as a framework for public services and wellbeing as an
   approach to delivering public services.},
