@article{ WOS:000492895700006,
Author = {Jarvis, Lee},
Title = {Toward a Vernacular Security Studies: Origins, Interlocutors,
   Contributions, and Challenges},
Journal = {INTERNATIONAL STUDIES REVIEW},
