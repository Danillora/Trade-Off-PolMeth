@incollection{ WOS:000488605300019,
Author = {Reinert, Erik S.},
Editor = {Kattel, R},
Book-Author = {Reinert, ES},
Title = {MECHANISMS OF FINANCIAL CRISES IN GROWTH AND COLLAPSE: HAMMURABI,
   SCHUMPETER, PEREZ, AND MINSKY},
Booktitle = {VISIONARY REALISM OF GERMAN ECONOMICS: FROM THE THIRTY YEARS' WAR TO THE
   COLD WAR},
Series = {Anthem Other Canon Economics},
Year = {2019},
Pages = {529-553},
Abstract = {This paper provides a historical and theoretical overview of the
   mechanisms leading up to financial crises and financial bubbles. It
   suggests that the potentially explosive growth of the financial sector
   at the expense of the real economy fed by compound interest has since
   before ancient Mesopotamia under the rule of Hammurabi represented a
   real threat for such crises. A more modern and additional factor that
   builds up crises is Joseph Schumpeter's observation of the clustering of
   innovations. Carlota Perez has more recently developed Schumpeter's
   vision into a theory of techno-economic paradigms which about midway in
   their trajectory produce the build-up to financial crises. The theories
   of Schumpeterian economist Hyman Minsky, describing the mechanisms
   producing the collapse of financial bubbles complete the overview. The
   paper ends with recommendations to bring the West out of the present
   crisis by - once again - putting the real economy rather than the
   financial economy in the driver's seat of capitalism.},
