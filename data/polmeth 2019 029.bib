@article{ WOS:000503307900001,
Author = {Jackson, Christopher M.},
Title = {The EU and rule of law development in Kosovo: EULEX, domestic spoilers,
   and a two-level commitment problem},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2020},
Volume = {42},
Number = {7},
Pages = {955-973},
Month = {OCT 2},
Abstract = {This study addresses the problem of persistent elite corruption in
   post-conflict Kosovo. Despite the presence of the EU Rule of Mission
   (EULEX) since 2008, it has failed to convict prominent officials,
   perceptions of corruption are high, and perceptions of EULEX performance
   are low. This study analyzes how corrupt elites are able to avoid
   prosecution and retain impunity. Deployment of EULEX coincided with
   internationally mediated efforts at dialogue to normalize relations
   between Kosovo and Serbia. I argue that the dialogue with Serbia creates
   an opportunity for corrupt elites to avoid prosecution. Without credible
   threats of violence, corrupt elites use the threat of disrupting the
   dialogue to play spoilers in rule of law development. Findings indicate
   that EULEX has responded to spoiling signals from elites and refrained
   from prosecuting those capable disrupting the dialogue.},
