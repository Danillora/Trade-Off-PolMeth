@article{ WOS:000492436400001,
Author = {Skilling, Peter},
Title = {Collective desolation, individual consolations: contours of everyday
   neoliberal subjectivities},
Journal = {CRITICAL POLICY STUDIES},
Year = {2021},
Volume = {15},
Number = {1},
Pages = {55-73},
Month = {JAN 2},
Abstract = {There is a large literature dealing with neoliberal subjectivities at a
   conceptual and theoretical level. This paper joins a growing body of
   empirical studies of entrepreneurial subjectivity as it is lived and
   experienced. This article presents an analysis of a series of focus
   groups designed to explore the contours of the neoliberal subjectivities
   of a deliberately diverse group of citizens in contemporary New Zealand.
   Findings support the thesis that most people do not enthusiastically
   endorse neoliberalism at the level of its core ideas and values but,
   rather, accept it as an unalterable reality. This dynamic of
   ?disaffected compliance? was reinforced by the presence not just of
   negative affects (anxiety, helplessness and so on) but also, crucially,
   of positive affects. Feeling themselves unable to change the system that
   they (said they) did not like, participants derived pride, pleasure and
   satisfaction from their capacity to perform successfully within that
   unloved system.},
