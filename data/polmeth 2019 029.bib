@article{ WOS:000474245400009,
Author = {Leshem, Oded Adomi},
Title = {The Pivotal Role of the Enemy in Inducing Hope for Peace},
Journal = {POLITICAL STUDIES},
Year = {2019},
Volume = {67},
Number = {3},
Pages = {693-711},
Month = {AUG},
Abstract = {Protracted conflicts are also termed ``intractable{''} in part because
   they are perceived as irresolvable by those mired in the prolonged
   dispute. The conflict's perceived irreconcilability leaves little reason
   for citizens to strive for peace which, in turn, might further
   exacerbate the conflict. The central question posed in this study is
   whether hopelessness regarding the possibility for peace can be
   alleviated among citizens embroiled in protracted conflicts. Results
   from an experimental study administered in Israel show that hope can be
   instilled, even among those most skeptical, when an outgroup member
   claims that peace is possible but not when an ingroup member claims the
   same. A follow-up study revealed that hope induced by the experimental
   interventions withstood a period of conflict escalation and elicited
   active support for peacebuilding. The study demonstrates that hope
   inducement is a useful tool for carving a pathway out of detrimental
   structures of intractability.},
