@article{ WOS:000492830700001,
Author = {Pace, Michelle and Bilgic, Ali},
Title = {Studying Emotions in Security and Diplomacy: Where We Are Now and
   Challenges Ahead},
Journal = {POLITICAL PSYCHOLOGY},
