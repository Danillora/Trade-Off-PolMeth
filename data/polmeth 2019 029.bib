@incollection{ WOS:001364569900011,
Author = {Cottret, Monique},
Editor = {Choudhury, M and Watkins, DJ},
Title = {Jansenism during the Revolution: the Nouvelles ecclesiastiques in
   the face of dechristianization},
Booktitle = {BELIEF AND POLITICS IN ENLIGHTENMENT FRANCE: Essays in Honor of Dale K.
   Van Kley},
Series = {Oxford University Studies in the Enlightenment},
Year = {2019},
Pages = {235+},
Abstract = {The Nouvelles ecclesiastiques, the Jansenist periodical that traversed
   the entire eighteenth century, enlisted in the camp of the Revolution.
   In 1793, without renouncing its defense and exemplification of the
   constitutional clergy, it denounced the deviations of those who had
   abandoned the values of Catholicism. While it adapted very well to civil
   marriage and the institution of divorce, so long as the Church was not
   compelled to distribute the sacraments to the divorced, it rebelled
   against the marriage of priests. The periodical reported day by day on
   the resistance of a party of Parisian cures against the dechristianizing
   measures of the Commune of Paris. The great debate was about the public
   character of religion and therefore the maintenance of processions that
   had always marked the city with their imprint. The periodical defended
   Christian values but knew perfectly how to utilize the terms of the new
   law. The articles of the Constitution and those of the Declaration of
   the Rights of Man were placed in the foreground. The editors of the
   periodical knew well the secrets of Revolutionary politics and
   effectively managed the contradictions between the Convention, Commune,
   and Committee of Public Safety.},
