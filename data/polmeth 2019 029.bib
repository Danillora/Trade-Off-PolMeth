@article{ WOS:000492436100001,
Author = {Fuller, Crispian},
Title = {The institutions and heterogeneous geographical relations of austerity},
Journal = {CRITICAL POLICY STUDIES},
Year = {2020},
Volume = {14},
Number = {4},
Pages = {445-465},
Month = {OCT 1},
Abstract = {There is a general consensus that austerity is variegated in nature, but
   that a generic feature is the construction of discursive institutions
   framing the necessity for austerity and guiding actors. However, what is
   missing from accounts within political science and related disciplines
   is an appreciation of how these work through heterogeneous geographical
   relations. This paper examines how austerity has been discursively
   framed, justified and articulated through ?semantic? spatial austerity
   institutions. Utilizing a ?pragmatic sociology of critique? approach, it
   examines the UK Government?s austerity program. The paper finds that
   austerity works through spatially configured semantic institutions, and
   where there has been resistance this has not developed into a
   substantive social movement. More broadly, the paper argues that
   political science and public administration need to move beyond analysis
   of ?singular? geographical relations, to understanding the role of
   heterogeneous geographical relations characterizing state practices.},
