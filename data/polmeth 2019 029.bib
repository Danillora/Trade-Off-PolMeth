@article{ WOS:000470300800006,
Author = {Twort, Lauren},
Title = {An exploration of the narrative of education in Bo, Sierra Leone: a
   bottom-up perspective},
Journal = {PEACEBUILDING},
Year = {2019},
Volume = {7},
Number = {1},
Pages = {88-102},
Abstract = {There has been increased recognition by the international community of
   the important role access to education can play in a post-conflict
   setting. However, education alone is not a panacea and scholars have
   attempted to highlight the `two faces' of education, which can exhibit
   both positive and negative attributes. These studies point to the need
   to look at education, not just as a programmatic policy objective, but
   as a narrative within a wider context. This paper adopts an ethnographic
   approach to explore the webs of meaning attached to education as a
   narrative in everyday life, using the example of people from Bo, Sierra
   Leone. It reveals the limitations of the measures of success used within
   development organisations, which exclude the interrelated social,
   political and economic dynamics that are integral to identifying future
   challenges and opportunities concerning education.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
