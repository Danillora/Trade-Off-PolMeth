@article{ WOS:000488217800001,
Author = {Shanaah, Sadi},
Title = {Alienation or Cooperation? British Muslims' Attitudes to and Engagement
   in Counter-Terrorism and Counter-Extremism},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2022},
Volume = {34},
Number = {1},
Pages = {71-92},
Month = {JAN 2},
Abstract = {The dominant academic narrative portrays British Muslim communities as
   alienated by counter-terrorism policies and consequently reluctant to
   cooperate with authorities by taking action against Islamist extremism.
   This article reassesses and nuances the ``alienation narrative{''} with
   the use of unique data from three robust surveys of British Muslims. It
   finds that although a minority shows signs of alienation, most British
   Muslims are satisfied with and trust counter-terrorism policies as well
   as the government and the police. The level of willingness to take
   action against Islamist extremism is also high. The study confirms that
   aspects of alienation correlate with reduced willingness to take action
   against Islamist extremism, although they do not necessarily lead to
   disengagement.},
