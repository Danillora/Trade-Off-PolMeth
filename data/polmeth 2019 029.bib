@article{ WOS:000487576600003,
Author = {Bayramov, Vugar and Breban, Dan and Mukhtarov, Elmir},
Title = {Economic effects estimation for the Eurasian Economic Union: Application
   of regional linear regression},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
