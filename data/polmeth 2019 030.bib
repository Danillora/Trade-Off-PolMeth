@incollection{ WOS:001364569900008,
Author = {Garrioch, David},
Editor = {Choudhury, M and Watkins, DJ},
Title = {Varieties of religious behavior in eighteenth-century Paris: the
   material culture of leaders of confraternities},
Booktitle = {BELIEF AND POLITICS IN ENLIGHTENMENT FRANCE: Essays in Honor of Dale K.
   Van Kley},
Series = {Oxford University Studies in the Enlightenment},
Year = {2019},
Pages = {181+},
Abstract = {One might expect members of eighteenth-century Catholic confraternities
   to be very conservative in their religious behavior. This essay uses
   estate inventories to examine the material culture of the administrators
   of a number of Paris confraternities and asks what this can tell us
   about their religious practice. The analysis suggests a surprising
   variety, pointing to very traditional forms of piety alongside more
   modern ones, support for Jansenism, and even elements of the material
   culture of the Enlightenment. It suggests that, even within the
   confraternities, new forms of belief were appearing and that Paris
   Catholics cannot necessarily be seen as conservative.},
Publisher = {LIVERPOOL UNIV PRESS},
Address = {4 CAMBRIDGE ST, LIVERPOOL, ENGLAND},
