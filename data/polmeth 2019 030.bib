ISSN = {0162-895X},
EISSN = {1467-9221},
Keywords = {elite influence; elite polarization; partisan cues; public opinion},
Keywords-Plus = {PUBLIC-OPINION; POLITICAL-PARTIES; CUES; VOTERS; BIAS; REPRESENTATION;
