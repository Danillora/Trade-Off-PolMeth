@article{ WOS:000458685200007,
Author = {Angstrom, Jan and Petersson, Magnus},
Title = {Weak party escalation: An underestimated strategy for small states?},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2019},
Volume = {42},
Number = {2, SI},
Pages = {282-300},
Month = {FEB 23},
Abstract = {In this article, we develop the strategic rationale behind weak party
   escalation against stronger adversaries. There are, we suggest, four
   main strategies: to provoke a desired over-reaction from the stronger
   adversary; to compartmentalize conflict within a domain in which the
   weak party has advantages; to carve a niche with a stronger ally, and to
   forge a reputation of not yielding lightly. Spelling out these different
   logics contributes to the literature on small state strategies and
   escalation. It also suggests, contrary to much of the existing
   literature, that it can be rational for weak parties to escalate against
   great powers.},
