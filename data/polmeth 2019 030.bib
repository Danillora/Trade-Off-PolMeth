@article{ WOS:000456439100012,
Author = {Bernsand, Niklas},
Title = {Returning Chernivtsi to the Cultural Map of Europe: The Meridian
   Czernowitz International Poetry Festival},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
