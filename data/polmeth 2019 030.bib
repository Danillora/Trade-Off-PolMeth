@article{ WOS:000490619000001,
Author = {Hoban, Iuliia},
Title = {Children, conflict, and the detention of ?child soldiers? in Canada and
   the United States: How framing contests shape policies},
Journal = {JOURNAL OF HUMAN RIGHTS},
Year = {2019},
Volume = {18},
Number = {5},
Pages = {597-618},
Month = {OCT 20},
Abstract = {The detention of child soldiers is an example of an intractable policy
   controversy. In their advocacy of governance responses, policy-relevant
   actors adopt reductionist frames either of ?victims? ? that require
   special protections ? or of ?perpetrators? that necessitate the
   detention of individuals deemed threats. This article examines how the
   framing of a controversy influences policy responses. The ways in which
   policy actors in Canada and the United States acted from their frames
   and responded to frames of others on the issue of the detention of child
   soldiers is analyzed. Forty-five semi-structured interviews were
   conducted with representatives of nongovernmental organizations (NGOs),
   government officials, and military lawyers ? across two national
   contexts ? to build a comprehensive picture of these framing contests.
   Findings show that policy actors in both countries employed
   frame-diverging moves that prolong policy controversies. However, the
   adoption of the Canadian doctrine presents an example of a convergence
   of frames. NGOs and government officials engaged in strategies to create
   a shared framing that allowed to move beyond the perceived dilemma of
   child soldiers either as ?victims? or ?perpetrators.? These findings
   enhance not only our understanding of how framing dynamics shape
   governance responses but also demonstrate opportunities of using the
   research on framing processes in investigating complex experiences of
   children in conflict zones.},
