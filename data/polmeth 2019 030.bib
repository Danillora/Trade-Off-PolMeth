@article{ WOS:000472463600003,
Author = {Bailey, Christopher J.},
Title = {Assessing President Obama's climate change record},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2019},
Volume = {28},
Number = {5},
Pages = {847-865},
Abstract = {President Obama's climate change record is assessed by looking at his
   success in translating policy goals into policy outputs (laws or
   regulatory action) during his time in office. Obama's campaign speeches
   are examined to identify specific promises to take action on climate
   change before proceeding to examine whether, how and with what success
   he managed to act upon these pledges. Obama is shown to have set out a
   multi-pronged approach to deal with climate change in his campaign
   speeches and succeeded in translating many of his goals into policy
