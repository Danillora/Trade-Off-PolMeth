@article{ WOS:000479273400008,
Author = {Scoones, Ian and Mavedzenge, Blasio and Murimbarimba, Felix},
Title = {Young people and land in Zimbabwe: livelihood challenges after land
   reform},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2019},
Volume = {46},
Number = {159, SI},
Pages = {117-134},
Month = {JAN 2},
Abstract = {This article explores the livelihood challenges and opportunities of
   young people following Zimbabwe's land reform in 2000. The article
   explores the life courses of a cohort of men and women, all children of
   land reform settlers, in two contrasting smallholder land reform sites.
   Major challenges to social reproduction are highlighted, reflected in an
   extended `waithood', while some opportunities for accumulation are
   observed, notably in intensive agricultural production and
   agriculture-linked business enterprises. In conclusion, the implications
   of generational transfer of land, assets and livelihood opportunities
   are discussed in the context of Zimbabwe's agrarian reform.},
