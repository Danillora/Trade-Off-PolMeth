@article{ WOS:000461574200004,
Author = {Yoder, Brandon K.},
Title = {Hedging for Better Bets: Power Shifts, Credible Signals, and Preventive
   Conflict},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
