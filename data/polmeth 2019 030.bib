@incollection{ WOS:001364569900006,
Author = {Armenteros, Carolina},
Editor = {Choudhury, M and Watkins, DJ},
Title = {Jesuits of India: adapting Van Kley's ``Religion and the age of
   `patriot' reform{''} to South Asia},
Booktitle = {BELIEF AND POLITICS IN ENLIGHTENMENT FRANCE: Essays in Honor of Dale K.
   Van Kley},
Series = {Oxford University Studies in the Enlightenment},
Year = {2019},
Pages = {137+},
Abstract = {To spread Christianity in eighteenth-century India, European Jesuits had
   to shed their cultural heritage and become ascetes in the Indian
   tradition. The texts they wrote in these conditions provide privileged
   material for studying their identity: As sadhus removed from European
   control, they were exceptionally free to portray themselves and
   Christianity as they wished. Perusing the Jesuit Lettres edifiantes et
   curieuses (1703-1773) along with Jansenist critiques of the Jesuits'
   India missions, this essay helps fill the Indian lacuna in Jesuit
   studies. It also tests and complements Van Kley's description of Jesuit
   identity, contributes to current debates on Catholic Enlightenment, and
   sketches the early outlines of enlightened conservatism.},
Publisher = {LIVERPOOL UNIV PRESS},
