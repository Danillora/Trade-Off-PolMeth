@article{ WOS:000463576000011,
Author = {Mott, Gareth},
Title = {A Storm on the Horizon? ``Twister{''} and the Implications of the
   Blockchain and Peer-to-Peer Social Networks for Online Violent Extremism},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2019},
