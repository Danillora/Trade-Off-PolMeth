@article{ WOS:000489074500007,
Author = {Romanyshyn, Iulian},
Title = {The EU in multilateral arms negotiations: shaping the process or
   outcome?},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2019},
Volume = {41},
Number = {5},
Pages = {675-692},
Month = {JUL 4},
Abstract = {The European Union demonstrates varying patterns of impact in
   multilateral negotiations. In some areas the EU focuses on shaping the
   outcome document of negotiations through the use of multilateral means.
   In other areas, however, the Union is merely preoccupied with a process
   of negotiations that contributes to the collective benefit of
   multilateral dialogue. While discussions of the EU?s process and outcome
   effectiveness are not new, this article provides an explanation for when
   and why the EU is effective in terms of process or outcome of
   multilateral negotiations. The EU focuses on shaping the outcome of
   negotiations under a condition of prevailing uncertainty, whereas the
   dominance of complexity leads to the EU?s inclination to influence the
   negotiation process. In order to probe these assumptions, the article
   examines the EU?s role in the multilateral negotiations on arms trade
   and nuclear non-proliferation.},
