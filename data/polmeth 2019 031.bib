@article{ WOS:000498626900001,
Author = {Vasilopoulos, Pavlos and Brouard, Sylvain},
Title = {System Justification and Affective Responses to Terrorism: Evidence from
   the November 2015 Paris Attacks},
Journal = {POLITICAL PSYCHOLOGY},
Year = {2020},
