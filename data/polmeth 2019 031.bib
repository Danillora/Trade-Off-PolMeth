@incollection{ WOS:001364569900014,
Author = {Van Kley, Dale K.},
Editor = {Choudhury, M and Watkins, DJ},
Title = {Writing religion into the French century of lights: the confessions of a
   Protestant historian of the Catholic Jansenist controversy},
Booktitle = {BELIEF AND POLITICS IN ENLIGHTENMENT FRANCE: Essays in Honor of Dale K.
   Van Kley},
Series = {Oxford University Studies in the Enlightenment},
Year = {2019},
Pages = {295+},
Abstract = {In contrast to the 1990s attempt to write ``religion{''} back into the
   sixteenth-century wars of religion, Van Kley here describes his
   career-long efforts to write religion into the historiographical script
   of an eighteenth century long dominated by the narratives of an
   ``enlightened{''} exit from religion and the socioeconomic causes of a
   revolution that turned against religion. This intellectual autobiography
   acknowledges that this enterprise presupposed the conviction of the
   centrality of the ``religious{''} in some form; in it, Van Kley
   retrospectively explores the longs and shorts of this starting point in
   constant dialogue with a field that saw the rise and demise of purely
   socioeconomic explanations, describing to what extent his starting
   points may have changed that field as well as the extent to which this
   changing field affected them. Since the evolution of the field
   culminated in the liberation of the cultural and intellectual from the
   socioeconomic dimensions of experience, and therefore also the return of
   the Enlightenment, this autobiography also describes Van Kley's own
   gradual coming to terms with the Enlightenment from which his enterprise
   initially departed.},
