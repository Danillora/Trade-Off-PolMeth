@article{ WOS:000477063500002,
Author = {van Noort, Carolijn},
Title = {Seeking strategic narrative alignment: the case of BRICS and Brazil on
   the issue of infrastructural development},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2019},
Volume = {62},
Number = {1},
Abstract = {This article explores the challenges of multilateral forums to form
   coherent `strategic narratives' that align positively with foreign
   policy narratives of member states in a multi-stakeholder society.
   Specifically, it focuses on whether and how BRICS's communication on the
   issue of infrastructural development is strategically aligned with
   Brazil's foreign policy narrative towards Africa. The case study
   involves interview data from 2016 with non-state actors in Brazil. This
   empirical case highlights how narrative misalignment on the national and
   multilateral level results from a high degree of ambiguity in the
   narratives, their problematic relation to actual events, and faulty
   practices of `inclusive' participation.},
