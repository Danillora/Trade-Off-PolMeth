@article{ WOS:000500127200001,
Author = {Pevehouse, Jon C. W. and Nordstrom, Timothy and McManus, Roseanne W. and
   Jamison, Anne Spencer},
Title = {Tracking organizations in the world: The Correlates of War IGO Version
   3.0 datasets},
Journal = {JOURNAL OF PEACE RESEARCH},
