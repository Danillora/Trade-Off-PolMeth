@article{ WOS:000502416300001,
Author = {Arias-Maldonado, Manuel},
Title = {Blooming landscapes? The paradox of utopian thinking in the Anthropocene},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2020},
Volume = {29},
Number = {6},
Pages = {1024-1041},
Month = {SEP 18},
Abstract = {By reminding human beings that they are earthly creatures who depend on
   the habitability of their planet, the Anthropocene is changing our
   relation with time. On the one hand, looking backward, it confronts us
   with the abyss of deep time; on the other hand, the future itself
   acquires a threatening quality as the possibility of a `hothouse Earth'
   becomes increasingly plausible. I focus on the impact that such
   perceptive disruption has on green utopian thinking. The effect is
   paradoxical. Those who cling to an alarming account of the Anthropocene
   can offer only a renewed version of the austere ecotopia envisioned by
   early environmentalism - a modest utopia organized around frugality.
   However, the so-called ecomodernists take the opportunity provided by
   the new geological age to offer a genuine utopian vision: one that does
   not break with the present but rather improves it dramatically.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
