@article{ WOS:000475080400001,
Author = {Scala, Francesca and Paterson, Stephanie and Richard-Nobert, Laurence},
Title = {The gender logic and effects of instrument mixes: implementing eldercare
   policy in Canada},
Journal = {POLICY AND SOCIETY},
Year = {2019},
Volume = {38},
Number = {4, SI},
Pages = {554-571},
Month = {OCT 2},
Abstract = {Although policy instruments are often conceptualized as technical or
   neutral tools through which ideas and outcomes are connected, this
   article focuses on the discursive dimensions of policy instruments in
   Canada's eldercare policy to explore their gendered and gendering logics
   and effects. Using frame analysis, we examine government acts, public
   reports and policy documents to uncover how different policy instruments
   at the federal and subnational levels frame caregiving and the
   caregiver. In doing so, we identify three distinct and contradictory
   frames - caregiver as resource, caregiver as worker and caregiver as
   autonomy - and illuminate their effects for gender equality outcomes. By
   looking at the implementation of eldercare policies in different
   provinces and across different government levels, we advance a better
   understanding of the gendered meanings and effects of instruments mixes.},
