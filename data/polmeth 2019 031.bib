@article{ WOS:000487576600005,
Author = {Maskarinec, Pavel},
Title = {Ticket splitting, strategic voting and personal vote in the 2012
   Mongolian elections},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2019},
Volume = {52},
Number = {3},
Pages = {235-245},
Month = {SEP},
Abstract = {This article examines ticket splitting under Mongolia's new mixed-member
   majoritarian system used for the elections in 2012, reaching several
   conclusions. First, we confirm that strategic ticket splitting depends
   on party size, as especially candidates of larger parties receive, on
   average, more district votes. Second, we show that strategic voting is
   not a universal phenomenon under the Mongolian mixed-member majoritarian
   system. Finally, as personal vote rather than strategic voting generally
   influences electoral behaviour of Mongolian voters at the district
   level, we hypothesize that institutional factors alone are not
   sufficient to explain both ticket splitting and strategic voting. (C)
   2019 The Regents of the University of California. Published by Elsevier
   Ltd. All rights reserved.},
