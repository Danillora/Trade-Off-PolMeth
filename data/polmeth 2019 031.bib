@article{ WOS:000489392300001,
Author = {Kalabinski, Marta},
Title = {Teresa Prekerowa: Going against the Grain},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
Year = {2020},
Volume = {34},
Number = {2},
Pages = {400-422},
Month = {MAY},
Abstract = {In November 1940, an eighteen-year-old Polish girl went into the Warsaw
   Ghetto to see how she could help a girl she barely knew. Without
   initially telling family or friends, she went back a dozen times over
   the next two months, bringing food and medication to the girl and her
   family. Why would she go to such great lengths and at such considerable
   risk to herself to help someone she barely knew? Teresa Prekerowa's
   story provides some much needed insight into the psychology of rescuers,
   shedding light on what makes attempted rescue possible, and why it is
   impossible for the vast majority of people.},
