@article{ WOS:000462626900002,
Author = {Aklin, Michael and Kern, Andreas},
Title = {Moral Hazard and Financial Crises: Evidence from American Troop
   Deployments},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
