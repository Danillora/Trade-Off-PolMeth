@article{ WOS:000469563900001,
Author = {Baele, Stephane J. and Bettiza, Gregorio and Boyd, Katharine A. and
   Coan, Travis G.},
Title = {ISIS's Clash of Civilizations: Constructing the ``West{''} in
   Terrorist Propaganda},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2021},
Volume = {44},
Number = {11},
Pages = {887-919},
Abstract = {Depictions of the West abound in the propaganda produced by the
   self-proclaimed ``Islamic State,{''} presenting to potentially
   sympathetic audiences an overwhelmingly negative image of a supposedly
   homogeneous political entity. Combining quantitative and qualitative
   language and visual analysis, we systematically expose the various
   facets of this image and analyze the overall picture. Drawing on the
   ``clash of civilizations{''} literature as well as on research on
   extremist language, we conceptualize this presentation of the West as a
   powerful radicalizing voice shaping today's global civilizational
   politics.},
