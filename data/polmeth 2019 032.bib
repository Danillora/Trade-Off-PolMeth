
@article{ WOS:000460598100004,
Author = {Mohan, Surinder},
Title = {Military capabilities and regime type: the emergence of India-Pakistan
   rivalry},
Journal = {INTERNATIONAL POLITICS},
Year = {2019},
Volume = {56},
Number = {1},
Pages = {49-69},
Month = {FEB},
Abstract = {This article explores the outbreak and escalation of India-Pakistan
   rivalry from the perspective of power capabilities and the divergent
   nature of the two adversaries' political regimes. It contrasts the
   established debates' distributional aspects, particularly the notion of
   power asymmetry and the Pakistani policy-makers decision to initiate
   armed hostilities against India, to ascertain the onset of the First
   Kashmir War and the course of rivalry formation. It argues that,
   opposite to what scholars have actually viewed or assessed, Pakistan had
   significant military strength to wage the mountain warfare in Kashmir in
   1947 and based on this assessment its political-military leadership had
   taken a `calculated risk' to seize the area of strategic importance in
   Kashmir. Further, this article shows how India and Pakistan's different
   political foundations and regime types influenced their early actions
   which eventually drove them to develop a highly antagonistic
   relationship within the first decade of their independence.},
