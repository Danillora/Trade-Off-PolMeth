@article{ WOS:000461042600004,
Author = {McKinnon, Catriona},
Title = {Sleepwalking into lock-in? Avoiding wrongs to future people in the
   governance of solar radiation management research},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2019},
Volume = {28},
Number = {3, SI},
Pages = {441-459},
Month = {APR 16},
Abstract = {Arguments are advanced for two ways in which we can avoid the reckless
   endangerment of future people in the governance of solar radiation
   management (SRM) research, which could happen through lock-in to SRM
   deployment from research. SRM research is at an early stage, one at
   which the mechanisms of lock-in could start to operate. However, lock-in
   fit to endanger future people could be slowed or stopped through
   targeted governance. Governance of SRM research that does not include
   provisions to detect, slow, or stop lock-in fails the test of an
   intergenerationally adequate precautionary principle, and research
   governed without these provisions cannot itself be justified as a
   precaution against the impacts of climate change.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
