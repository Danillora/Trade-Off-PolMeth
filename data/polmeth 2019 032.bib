@incollection{ WOS:000444179000004,
Author = {Ledstrup, Martin},
Book-Author = {Ledstrup, M},
Title = {Strangers in the Nation: Nearness, Distance, and the Everyday},
Booktitle = {NATIONALISM AND NATIONHOOD IN THE UNITED ARAB EMIRATES},
Series = {Modern Muslim World},
Year = {2019},
Pages = {63-88},
Abstract = {This chapter reflects theoretically about how relocating from the
   metropole of Dubai to the northern Emirate of Ras Al Khaimah made
   interaction easier, but not easy. It invokes an interactionist
   perspective from the example of Georg Simmel to make sense of how both
   distance and nearness are interweaved in the ``below{''} of everyday
   nationhood. On the one hand, social scientists that study the Emirati
   elite citizenry through the everyday are studying up. On the other hand,
   when we change from one locality to another, the dynamics of interaction
   and positionality are changed as well. In effect, by relocating from
   Dubai to Ras Al Khaimah, the distinction between ``above{''} and
   ``below{''} made so often from the perspective of everyday nationhood
   becomes locally contingent and academically self-reflexive.},
Publisher = {PALGRAVE},
Address = {HOUNDMILLS, BASINGSTOKE RG21 6XS, ENGLAND},
Type = {Article; Book Chapter},
Language = {English},
Affiliation = {Ledstrup, M (Corresponding Author), Univ Southern Denmark, Hist Inst, Odense, Denmark.
   Ledstrup, Martin, Univ Southern Denmark, Hist Inst, Odense, Denmark.},
