@article{ WOS:000466367400009,
Author = {Zhang, Joy Y. and Barr, Michael},
Title = {The transformative power of commoning and alternative food networks},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2019},
Volume = {28},
Number = {4, SI},
Pages = {771-789},
Month = {JUN 7},
Abstract = {A commoning framework offers a critical lens to fully appreciate the
   scope and impact of alternative food networks (AFNs). Fieldwork from an
   AFN in southern China is drawn upon to show how commoning enacts changes
   in how members contextualise and anchor their social relations to one
   another with regards to sourcing food as a commons. A commoning
   framework gives a fuller picture of how the constitutive effects of AFNs
   reside not in their introduction of a new uniformity but in their
   navigation of the multiplicity of the social through its proposition and
   co-construction of a new `cognitive praxis'.},
