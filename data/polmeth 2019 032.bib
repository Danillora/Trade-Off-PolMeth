@article{ WOS:000486877800001,
Author = {de Wilde, Pieter and Rauh, Christian},
Title = {Going full circle: the need for procedural perspectives on EU
   responsiveness},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2019},
Volume = {26},
Number = {11},
Pages = {1737-1748},
Month = {NOV 2},
Abstract = {Research on policy responsiveness of the European Union has adopted the
   systemic model from national contexts. This focusses on the static
   congruence or the dynamic adaptation of aggregated policy output and
   similarly aggregated public opinion. Approaches in this vein provide
   relevant insights and uncover a surprising degree of EU responsiveness.
   Yet, this debate contribution argues that they only insufficiently
   capture indirect accountability chains and the emerging challenges of
   public EU politicization and mediatization. We establish the need for
   procedural perspectives that addresses how the different EU institutions
   perceive and digest public opinion and subsequently influence it through
   communication. To further this research agenda, we sketch the contours
   of a procedural model by highlighting possible variation at the input,
   throughput and output stages. Going full circle, we suggest, allows us
   to better understand the responses to public opinion and their wider
   implications for the societal acceptance of the unfinished supranational
   polity.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Rauh, C (Corresponding Author), WZB Berlin Social Sci Ctr, Dept Global Governance, Berlin, Germany.
   de Wilde, Pieter, Norwegian Univ Sci \& Technol NTNU, Dept Sociol \& Polit Sci, Trondheim, Norway.
   Rauh, Christian, WZB Berlin Social Sci Ctr, Dept Global Governance, Berlin, Germany.},
DOI = {10.1080/13501763.2019.1668043},
