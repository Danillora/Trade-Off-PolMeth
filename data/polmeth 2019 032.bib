@article{ WOS:000457740700006,
Author = {Bebbington, Anthony and Fash, Benjamin and Rogan, John},
Title = {Socio-environmental Conflict, Political Settlements, and Mining
   Governance: A Cross-Border Comparison, El Salvador and Honduras},
