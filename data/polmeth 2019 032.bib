@article{ WOS:000463425000001,
Author = {O'Driscoll, Cian},
Title = {Nobody wins the victory taboo in just war theory},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2019},
Volume = {42},
Number = {7, SI},
Pages = {901-919},
Month = {NOV 10},
Abstract = {This article examines how scholars of the just war tradition think about
   the ethical dilemmas that arise in the endgame phase of modern warfare.
   In particular, it focuses upon their reticence to engage the idiom of
   `victory'. Why, it asks, have scholars been so reluctant to talk about
   what it means to `win' a just war? It contends that, while just war
   scholars may have good reason to be sceptical about `victory', engaging
   it would grant them a more direct view of the critical potentialities,
   but also the limitations, of just war reasoning.},
