@article{ WOS:000471434000001,
Author = {Tkacheva, Tatiana and Golosov, Grigorii V.},
Title = {United Russia's Primaries and the Strength of Political Machines in the
   Regions of Russia: Evidence from the 2016 Duma Elections},
Journal = {EUROPE-ASIA STUDIES},
Year = {2019},
Volume = {71},
Number = {5},
Pages = {824-839},
Month = {MAY 28},
Abstract = {The primary elections of the United Russia party serve as a tool for
   party organisation and voter mobilisation. In this study, we employ
   United Russia's capacity to mobilise voter turnout in the 2016 primary
   elections as an indirect indicator of the strength of party-controlled
   regional political machines. Our analysis of the results of the 2016
   Duma elections in 83 regions of Russia demonstrates that voter turnout
   in the spring 2016 party primaries of United Russia to a significant
   extent explains cross-regional variation in party success.},
