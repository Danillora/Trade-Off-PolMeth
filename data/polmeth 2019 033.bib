@article{ WOS:000494671400001,
Author = {Trampusch, Christine and Fastenrath, Florian},
Title = {States' interests as limits to the power of finance: Regulatory reforms
   in early local government financialization in the US and UK},
Journal = {REGULATION \& GOVERNANCE},
Year = {2021},
Volume = {15},
Number = {2},
Pages = {245-261},
Month = {APR},
Abstract = {Through a comparison of typical and deviant cases, this study probes and
   refines the augmented power model which argues that the structural power
   of the financial industry fosters its instrumental power in influencing
   regulatory reforms under certain scope conditions. It shows the
   industry's success in influencing policymakers to authorize
   municipalities to use derivatives and thereby to financialize their debt
   management in the US (typical case). The failure of banks to acquire
   such a law in the UK (deviant case) reveals a hitherto little-noticed
   condition under which this power explanation collapses: states' fiscal
   and monetary constitution. We demonstrate that analyzing the operation
   of finance power requires a precise consideration of how states' fiscal
   and monetary constitution structures governments' responses to financial
   industry's regulatory preferences. Moreover, we conclude that
   synthesizing business power research with literature on the mutual
   dependence between states and finance helps to explain patterns of state
   financialization.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Trampusch, C (Corresponding Author), Univ Cologne, Chair Int Comparat Polit Econ \& Econ Sociol, CCCP, Postfach 411020, D-50870 Cologne, Germany.
   Trampusch, Christine; Fastenrath, Florian, Univ Cologne, Chair Int Comparat Polit Econ \& Econ Sociol, CCCP, Postfach 411020, D-50870 Cologne, Germany.},
DOI = {10.1111/rego.12292},
EarlyAccessDate = {NOV 2019},
