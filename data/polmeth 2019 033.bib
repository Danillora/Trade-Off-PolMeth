@article{ WOS:000476572400006,
Author = {Colman, Jonathan},
Title = {Toward ``World Support{''} and ``The Ultimate Judgment of History{''}:
   The US Legal Case for the Blockade of Cuba during the Missile Crisis,
   October-November 1962},
Journal = {JOURNAL OF COLD WAR STUDIES},
Year = {2019},
Volume = {21},
Number = {2},
Pages = {150-173},
Month = {MAY},
Abstract = {The immense secondary literature on the Cuban missile crisis of October
   1962 tends to overlook the U.S. government's promotion of a case in
   international law to legitimize and generate support for the naval
   blockade of Cuba. This article explores the development and presentation
   of the legal case and then gauges its success by looking at the response
