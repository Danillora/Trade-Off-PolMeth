@article{ WOS:000462093000016,
Author = {Jackson, Jenn M.},
Title = {Breaking Out of the Ivory Tower: (Re)Thinking Inclusion of Women and
   Scholars of Color in the Academy},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2019},
Volume = {40},
Number = {1, SI},
Pages = {195-203},
Month = {JAN 2},
Abstract = {In this \#MeToo moment, many women of color have called out those in
   power, namely men, who engage in sexual harassment and toxic
   masculinity. Furthermore, scholars, whose personal identities and
   research interests lie at the margins of gender, race, class, and
   sexuality, have drawn increasing attention to issues of gendered and
   racialized biases and harassment in the Academy. During our
   pre-conference session at the 2018 meeting of the American Political
   Science Association, we discussed substantive methods for reckoning with
   these conditions. We worked through the theoretical frameworks of Black
   Feminist and queer scholarship to orient ourselves toward actions that
   center the most marginalized. We identified methods for generating
   transformative solutions to campus and departmental problems facing
   students, faculty, and staff with intersectional identities. In all, the
   workshop proved both effective and generative for all in attendance,
   providing those present with concrete tools to build more equitable
   departments and classrooms.},
