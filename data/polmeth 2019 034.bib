@article{ WOS:000462552200009,
Author = {Hearty, Kevin},
Title = {Moral Emotions and the Politics of Blame and Credit during Transitional
   Justice Moments},
Journal = {INTERNATIONAL JOURNAL OF TRANSITIONAL JUSTICE},
Year = {2019},
Volume = {13},
Number = {1, SI},
Pages = {134-154},
Month = {MAR},
Abstract = {This article critically examines how moral emotions are intrinsically
   tied to the process of apportioning blame and credit in postconflict
   societies. In doing so, it draws out the complexity that underpins moral
   emotions during key junctures in transitional justice. Recognizing that
   moral emotions naturally stem from the making of a judgement about a
   target,' the article argues that the relationship between those
   experiencing the moral emotion and the target' fundamentally determines
   whether the target' becomes the object of blame or credit during seminal
   moments of transition. Through using a focused empirical case study, the
   article identifies why and how changes over time in the relationships
   between the judgemental self' and the target,' in personal and political
   identities, and in perceptions of what is and is not moral, influence
   the moral emotions expressed by different versions of the judgemental
   self' towards the target.'},
