@article{ WOS:000466737400005,
Author = {Goodchild, James},
Title = {A most pervasive memoir: R. V. Jones and his Most Secret War},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2019},
Volume = {34},
Number = {4},
Pages = {526-543},
Abstract = {This article challenges the erstwhile historical reliance upon the
   memoirs of the key protagonist of British Scientific Intelligence during
   the Second World War. Meticulous archival research has confirmed that
   the history of the genesis of British Scientific Intelligence has been
   distorted by the legacy and legend of R.V. Jones and his memoirs
   entitled Most Secret War. This article confirms that British
   Intelligence success in this regard was not solely the result of one
   man's heroic accomplishments but, in reality, involved many individuals
   and organisations whose valuable exploits have been hidden in the
   shadows by Most Secret War.},
