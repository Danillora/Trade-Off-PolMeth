@article{ WOS:000458003300008,
Author = {Duncanson, Claire},
Title = {Beyond liberal vs liberating: women's economic empowerment in the United
   Nations' Women, Peace and Security agenda},
Journal = {INTERNATIONAL FEMINIST JOURNAL OF POLITICS},
Year = {2019},
Volume = {21},
Number = {1},
Pages = {111-130},
Month = {JAN 1},
Abstract = {This article is about women's economic empowerment within the United
   Nations (UN) Women, Peace and Security (WPS) agenda. Based on analysis
   of the core agenda-setting documents, it traces where two different
   versions of women's economic empowerment, ``liberal{''} (including women
   in the formal economy) and ``liberating{''} (women collectively
   mobilizing to challenge the status quo), appear in the WPS agenda. It
   argues that the two exist in uneasy tension in the UN's aspirations for
   women's economic security post-war, but that when it comes to actual
   activities and achievements, the liberal version dominates over the
   liberating version. The article argues that it is important not to
   overstate the divide between the two approaches, and that the seeds of a
   liberating approach can be found within the liberal. It is initiatives
   to facilitate women's economic empowerment that contain opportunities
   for collective action to transform the structures of the economy that
   WPS advocates should advocate. This would strengthen the WPS agenda and
   its ability to contribute to security as feminists envisage it, as
   encompassing freedom from want as well as freedom from fear.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
