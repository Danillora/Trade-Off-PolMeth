@article{ WOS:000494167900001,
Author = {Melusky, Benjamin},
Title = {Gender and Fiscal Gridlock in the American States},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2020},
Volume = {41},
Number = {3},
Pages = {249-277},
Month = {JUL 2},
Abstract = {Prior research, polling, and accounts from female politicians themselves
   suggest that with greater representation by women comes a less
   confrontational leadership style with a greater willingness to reach
   bipartisan compromise. If women are more willing to work across party
   lines and compromise than their male counterparts, a state?s budget
   process should benefit from greater representation of women in the state
   legislature. This article examines all budget enactments in the American
   states from 1993 to 2006, focusing on both the occurrence and duration
   of budget failures (impasses), specifically examining if greater
   representation by women reduces gridlock and ensures the timely passage
   of a budget. While the findings offer some support that increasing
   female representation, in particular in the political minority, makes a
   government more collaborative only after it fails to pass its budget,
   the impact of gender ultimately is considerably nuanced depending both
   on polarization and distinct differences by legislative chamber.},
