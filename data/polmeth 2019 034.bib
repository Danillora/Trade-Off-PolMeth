@article{ WOS:000457875700005,
Author = {Jasina-Schaefer, Alina},
Title = {Where Do I Belong? Narratives of Rodina among Russian-speaking
   Youth in Kazakhstan},
Journal = {EUROPE-ASIA STUDIES},
Year = {2019},
Volume = {71},
Number = {1, SI},
Pages = {97-116},
Month = {JAN 2},
Abstract = {This essay engages critically with the personal narratives of rodina
   (home, motherland) among Russian-speaking youth in post-Soviet
   Kazakhstan. It is argued that the concept of rodina as an important
   locus of belonging cannot be imbued with a single meaning; instead, it
   is characterised by internal conflicts and variations. Supported by
   empirical material, the essay moves beyond the confines of `nation' and
   `nationality' to illustrate the different ways in which Russian speakers
   frame their perception of rodina, and how such narratives can influence
   the construction of self- and community-identification.},
