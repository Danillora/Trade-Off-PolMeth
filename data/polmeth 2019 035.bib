@article{ WOS:000450518600003,
Author = {Herron, Erik S. and Fitzpatrick, Brian and Palamarenko, Maksym},
Title = {The practice and implications of legislative proxy voting in Ukraine},
Journal = {POST-SOVIET AFFAIRS},
Year = {2019},
Volume = {35},
Number = {1},
Pages = {41-62},
Month = {JAN 2},
Abstract = {Governing parties often face the challenge of coordinating the behavior
   of legislators to pass bills and achieve their policy goals. Solutions
   to this collective action problem vary, but generally involve a
   combination of inducements and punishments to encourage legislators to
   toe the party line. ``Ghost voting,{''} a form of proxy voting in which
   legislators record roll-call votes in place of their absent co-partisans
   has been noted over time in many representative institutions. This
   article addresses the process of proxy voting in Ukraine's Verkhovna
   Rada, empirically demonstrating that proxy voting has been widespread
   and essential to the success of crucial legislation. At the same time,
   proxy voting creates impediments to measures of legislative unity and
   undermines accountability.},
