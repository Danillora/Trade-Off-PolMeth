@article{ WOS:000482655500003,
Author = {Coulthart, Stephen},
Title = {From laboratory to the WMD Commission: how academic research influences
   intelligence agencies},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2019},
Volume = {34},
Number = {6},
Pages = {818-832},
Abstract = {Academic research can improve national security, yet the process by
   which findings flow from the ivory tower to intelligence agencies is not
   well understood by scholars. This article addresses this gap through an
   exploratory case study of when research impacted intelligence: the
   incorporation of cognitive biases research in intelligence analysis. The
   results of this study illustrate the importance of idea
   entrepreneurs-individuals who promote academic research-as well as the
   need for making academic findings applicable to intelligence
   practitioners. These results, while based on a single case, suggest new
   avenues for scholarship exploring knowledge utilization in intelligence.},
