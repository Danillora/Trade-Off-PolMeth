@article{ WOS:000484504600001,
Author = {Smith, Kathleen E.},
Title = {A Monument for our Times? Commemorating Victims of Repression in Putin's
   Russia},
Journal = {EUROPE-ASIA STUDIES},
Year = {2019},
Volume = {71},
Number = {8},
Pages = {1314-1344},
Month = {SEP 14},
Abstract = {Three decades ago, a civic movement arose in the USSR around
   commemorating Stalin's victims. Yet only in 2017, following President
   Vladimir Putin's approbation, was a central monument to victims of
   repression constructed. Analysis of the genesis and results of the
   design process for the new monument shows that memory discourses in
   Russia have been harnessed to a form that allows civic activists and
   state officials to express a limited consensus. The truncated nature of
   the competition and the jury's safe choice, however, produced a monument
   unlikely to become a locus for popular or official remembrance.},
