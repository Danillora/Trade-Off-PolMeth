@incollection{ WOS:000834246200006,
Author = {Doak, Kevin M.},
Book-Author = {Doak, KM},
Title = {Tanaka's Final Years-And Beyond},
Booktitle = {TANAKA KOTARO AND WORLD LAW: RETHINKING THE NATURAL LAW OUTSIDE THE WEST},
Series = {Global Political Thinkers},
Year = {2019},
Pages = {109-120},
Abstract = {This short concluding chapter covers Tanaka's final -publications from
   1970 until his death in 1974. A key point is that two of his major
   articles during these final years were published in English, reaching a
   global readership. The chapter then assesses his legacy today,
   concluding that his globalist jurisprudence, his defense of the Natural
   Law, and even his Catholic faith-the font of his globalist valuesdeserve
   renewed attention particularly since, as a respected Japanese scholar
   and jurist, Tanaka resists any easy reduction of this globalism to mere
