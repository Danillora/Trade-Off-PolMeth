@article{ WOS:000450518600002,
Author = {Rotaru, Vasile},
Title = {Instrumentalizing the recent past? The new Cold War narrative in Russian
   public space after 2014},
Journal = {POST-SOVIET AFFAIRS},
Year = {2019},
Volume = {35},
Number = {1},
Pages = {25-40},
Month = {JAN 2},
Abstract = {There has been much debate about whether the US and Russia are locked in
   a new Cold War, but much less attention as to how the term is used in
   Russian political discourse. Through a close analysis of public
   statements, I analyze how the Cold War narrative has been used in the
   Russian public space since 2014, and assess how the ``resuscitation{''}
   of the Cold War paradigm has been used by Moscow's political elites, in
   order to discuss its impact on foreign policy. I document a distinct
   shift in Russian policymakers' use of the term in 2016 and trace this
   shift to domestic political considerations.},
