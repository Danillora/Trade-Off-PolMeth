@article{ WOS:000496249400015,
Author = {Murray, Philomena and Brianson, Alex},
Title = {Rethinking Britain's Role in a Differentiated Europe after Brexit: A
   Comparative Regionalism Perspective},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
