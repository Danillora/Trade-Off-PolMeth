@article{ WOS:000484037200001,
Author = {Kasikci, Mehmet Volkan},
Title = {The Soviet and the Post-Soviet: Street Names and National Discourse in
   Almaty},
Journal = {EUROPE-ASIA STUDIES},
Year = {2019},
Volume = {71},
Number = {8},
Pages = {1345-1366},
Month = {SEP 14},
Abstract = {While discussing Kazakhstan's post-Soviet identity, scholars treat
   `Kazakhisation' as a given, and the substance of the process of
   developing such an identity is usually ignored. This article gives an
   insight into this process by analysing the politics of street names in
   Almaty and its relation to collective memory in post-Soviet Kazakhstan.
   It is argued that the so-called `Kazakhisation' of the country has been
   shaped primarily by the Soviet legacy, and it is in no sense pursuing
   the elimination of the Soviet past, or moving essentially anti-Russian
   lines. In fact, the post-Soviet discourse of the Kazakh nation is not a
   rupture but a continuation of Soviet discourses.},
