@article{ WOS:000492957700003,
Author = {Sharp, Dustin N.},
Title = {Through a Glass, Darkly: Three Important Conversations for Human Rights
   Professionals},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2019},
Volume = {11},
Number = {2},
Pages = {296-304},
Month = {JUL},
Abstract = {The current moment in history is marked by a sense of anxiety about the
   future of human rights as a global project. In many instances, human
   rights pessimism has eclipsed a larger empirical picture of human rights
   progress. In assessing this paradox of perception, I argue that human
   rights professionals themselves contribute to this sense of pessimism
   and crisis in ways that can become counterproductive and demobilizing.
   Promoting a more positive and balanced human rights vision reflecting
   both progress and challenges will require several shifts in emphasis
   over the coming years, beginning with some neglected conversations for
   the broader field of human rights. These conversations relate to (1) an
   assessment of the ways in which the prism of professional experience has
   shaped the messaging and world view of human rights advocates; (2) the
   role of human rights critics in promoting new thinking and reform; and
   (3) the need for advocates to go beyond what I call the `legalistic
   referee model' in their attempts to promote positive change.},
