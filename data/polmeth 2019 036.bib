@article{ WOS:000476999400003,
Author = {de Sa, Tiago Moreira},
Title = {``The World Was Not Turning in Their Direction{''}: The United States
   and the Decolonization of Angola},
Journal = {JOURNAL OF COLD WAR STUDIES},
Year = {2019},
Volume = {21},
Number = {1},
Pages = {52-65},
Month = {APR},
Abstract = {In the mid-1970s, the United States and the Soviet Union decided to
   export the Cold War to Angola at levels that were unprecedented on the
   African continent. In the case of the United States, this led to immense
   support for local allies-the National Liberation Front of Angola and the
   National Union for the Total Independence of Angola-in the form of many
   tons of heavy weaponry, millions of dollars, and the use of mercenaries
   and even paramilitary operatives of the U.S. Central Intelligence
   Agency. This article explains U.S. actions in Angola from 1974 to 1976
   against the backdrop of the Cold War, highlighting the decision-making
   process in Washington, the international context, the internal context,
   and the actions of both superpowers.},
