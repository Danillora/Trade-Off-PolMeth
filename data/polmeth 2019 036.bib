@article{ WOS:000492957700011,
Author = {Phillips, Brian},
Title = {`Better Than A Great Many Good Works': Jenny Erpenbeck's Go,
   Went, Gone and the Primacy of Attention in Human Rights
   Practice},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2019},
Volume = {11},
Number = {2},
Pages = {406-421},
Month = {JUL},
Abstract = {Beginning with Simone Weil's writing on the subject of attention, this
   article reflects on the place of attention in contemporary human rights
   practice-specifically in relation to the challenges posed by the
   multiple patterns of migration that have so defined our era. Through the
   lens of Jenny Erpenbeck's remarkable novel, Go, Went, Gone, the article
   explores what attentiveness may mean today with regard to research and
   advocacy concerning migration issues. The article also asks how we
   secure and maintain the interest of the general public in the so-called
   global migration `crisis' at a historical moment characterized by mass
   distraction?},
