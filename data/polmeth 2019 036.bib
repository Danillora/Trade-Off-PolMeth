@article{ WOS:000506150100003,
Author = {Ashe, Fidelma},
Title = {Sexuality and Gender Identity in Transitional Societies: Peacebuilding
   and Counterhegemonic Politics},
Journal = {INTERNATIONAL JOURNAL OF TRANSITIONAL JUSTICE},
