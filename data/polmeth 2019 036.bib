@article{ WOS:000475041600001,
Author = {Bekenova, Kristina and Collins, Neil},
Title = {Knowing Me, Knowing You: Media Portrayal of the EU in Kazakhstan},
Journal = {EUROPE-ASIA STUDIES},
Year = {2019},
Volume = {71},
Number = {7},
Pages = {1183-1204},
Month = {AUG 9},
Abstract = {The dichotomy of Self/Other prevails in shaping identity. This article
   asks how and to what extent the elements of the EU's image produced by
   media discourse shape the national identity of Kazakhstan. It contends
   that a state's identity can be formulated not in opposition-that is, not
   `Us against Them'-but rather, `Us as One of Them'. It argues that, in
   the case of Kazakhstan, the predominantly positive media discourse about
   the EU `Other' contributes to a positive formulation of the Self via the
   legitimisation of the domestic regime on the national and international
   levels.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
