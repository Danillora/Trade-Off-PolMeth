@article{ WOS:000466629900001,
Author = {Lizotte, Mary-Kate},
Title = {Authoritarian Personality and Gender Differences in Gun Control
   Attitudes},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2019},
Volume = {40},
Number = {3},
Pages = {385-408},
Month = {JUL 3},
Abstract = {This article investigates the gender gap in gun control attitudes, in
   which women are more likely to support gun control than men. Women are
   less likely than men to own a gun and to see owning guns as a means of
   self-protection. Using the 2012 American National Election Study Data,
   this article tests authoritarianism, which includes the desire for
   security and a disposition toward higher levels of perceived threat, as
   an explanation for the gap. The results indicate that authoritarian
   women are more likely than authoritarian men to support gun control. In
   fact, authoritarianism appears to have the opposite effect on men and
   women's gun control attitudes. Authoritarianism is associated with
   higher levels of support for gun control among women and lower levels of
   support among men.},
