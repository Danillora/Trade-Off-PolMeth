@article{ WOS:000476063000001,
Author = {Heldt, Eugenia C. and Schmidtke, Henning},
Title = {Explaining coherence in international regime complexes: How the World
   Bank shapes the field of multilateral development finance},
Journal = {REVIEW OF INTERNATIONAL POLITICAL ECONOMY},
Year = {2019},
Volume = {26},
