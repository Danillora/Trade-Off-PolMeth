@article{ WOS:000470870400002,
Author = {Vinovskis, Maris A.},
Title = {History of Testing in the United States: PK-12 Education},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
