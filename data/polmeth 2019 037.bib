@article{ WOS:000488650300001,
Author = {Piroska, Dora and Podvrsic, Ana},
Title = {New European Banking Governance and Crisis of Democracy: Bank
   Restructuring and Privatization in Slovenia},
Journal = {NEW POLITICAL ECONOMY},
Year = {2020},
Volume = {25},
Number = {6},
Pages = {992-1006},
Month = {SEP 18},
Abstract = {The paper makes three contributions to the understanding of the
   post-crisis European banking governance. First, it offers a more
   comprehensive approach to banking governance, beyond the Banking Union,
   through its concept of ?New European Banking Governance? (NEBG) that
   incorporates EU state aid rules and fiscal regulations. Second, it
   considers the impact of NEBG on democratic institutions and processes in
   EU member states, an under-researched topic in the literature on
   European banking governance. Finally, through its in-depth case study of
   Slovenia it considers the NEBG in relation to peripheral Eurozone
   states. It argues that the post-crisis banking governance framework of
   the EU not only severely constrained the Slovenian state in its policy
   choices but rearranged its policy-making institutions in a way that
   restricted and continues to restrict democratic banking policy
   formation.},
