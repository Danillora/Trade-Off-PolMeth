@article{ WOS:000476668500009,
Author = {Rashkova, Ekaterina R. and Zankina, Emilia},
Title = {Ministerial Politics in Southeastern Europe: Appointment and Portfolio
   Allocation to Female Ministers},
Journal = {POLITICS \& GENDER},
Year = {2019},
Volume = {15},
Number = {2},
Pages = {211-239},
Month = {JUN},
Abstract = {In light of the growing scholarship on women's representation in the
   executive, we examine whether extant theories on the determinants of
   female ministerial appointments apply to and explain the role of women
   in the executive in new democracies. We are further interested in better
   understanding the dynamics of portfolio allocation to female ministers.
   Given the different meanings that the political left and right carry in
   new compared with established democracies, we argue that ministerial
   appointments and portfolio allocations reflect this difference and
   therefore show diverse results. Presenting data on women ministers in
   five southeastern European states between 1990 and 2018, we analyze the
   descriptive representation of women at the highest echelons of political
   power. We establish that while the standard claim found in the
   literature that left political parties stage more women and with more
   progressive views, as shown primarily in scholarship on Western
   democracies, southeastern European women ministers who are given
   portfolios traditionally reserved for men are appointed primarily by
   parties of the right. Moreover, we find little support for the link
   between legislative and executive representation; rather, we see that
   the appointment of women ministers is related to the
   institutionalization level of the parties in power.},
