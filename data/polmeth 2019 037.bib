@article{ WOS:000477954700004,
Author = {Pitty, Roderic},
Title = {Russian Views of Stalinism as a Negative Satellite of Capitalism},
Journal = {EUROPE-ASIA STUDIES},
Year = {2019},
Volume = {71},
Number = {6, SI},
Pages = {932-950},
Month = {JUL 3},
Abstract = {The conflation of Stalinism with socialism was a key aspect of the
   relationship between Stalinism and capitalism. The Russian Marxist
   Nodari Simoniya exposed that conflation in his book What We Have Built,
   which is discussed in this essay in the context of Russian
   historiography about Stalinism. The main themes of Simoniya's critique
   of Stalinism as a historical reversal are elaborated and linked to
   political images during the Cold War that distorted the meaning of
   socialism. Stalinism is revealed as a negative satellite of capitalism,
   constituting an external economic zone, not an alternative form of world
   order.},
