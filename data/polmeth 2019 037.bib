@article{ WOS:000485294700008,
Author = {Hacker, Jacob S.},
Title = {Medicare Expansion as a Path as well as a Destination: Achieving
   Universal Insurance through a New Politics of Medicare},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
