@incollection{ WOS:000525612900003,
Author = {Heyvaert, Pauline},
Editor = {Perrez, J and Reuchamps, M and Thibodeau, PH},
Title = {Fairies, Christmas miracles and sham marriages A diachronic analysis of
   deliberate metaphors in Belgian political discourse},
Booktitle = {VARIATION IN POLITICAL METAPHOR},
Series = {Discourse Approaches to Politics Society and Culture},
Year = {2019},
Volume = {85},
Pages = {35-59},
Abstract = {This research analyzes the use of potentially deliberate metaphors in
   Belgian governmental policy statements over a time span of ten years.
   The analysis of this type of metaphor allows for further investigation
   into metaphor variation. Our results suggest that one variable that
   plays a role in the variation of political metaphors is the political
   situation of a country: the more hectic the political landscape, the
   more metaphor variation might occur. Throughout this chapter, some of
   the most striking potentially deliberate metaphors and tendencies are
   discussed, with the aim of establishing a possible explanation for what
   drives metaphorical variation in political discourse.},
