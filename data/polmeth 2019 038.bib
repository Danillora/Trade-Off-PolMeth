@article{ WOS:000467969100008,
Author = {Saridakis, George and Mendoza Gonzalez, Miguel A. and Torres, Rebeca I.
   Munoz and Hand, Chris},
Title = {Do Self-employment Rates Converge? Evidence from European OECD Countries},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2019},
Volume = {57},
Number = {3},
Pages = {551-562},
Month = {MAY},
Abstract = {This article uses panel data methods for stationary and non-stationary
   data to examine whether self-employment rates converge for 21 OECD
   European countries from 1990 to 2011 (the period covered by the
   COMPENDIA database). This article shows that there is a process of
   conditional convergence of self-employment rates particularly within
   Southern, Northern and Western Europe. These regional groups were
   characterized by a decreasing trend in their average self-employment
   rates. However in Central Europe we find more mixed results across the
   tests used and observe a rising trend in the average self-employment
   rates. Finally, we find some weak evidence of convergence among all
   European countries.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
