@article{ WOS:000474116100001,
Author = {Reeves, Christopher},
Title = {From Intervention to Retrenchment: Poland's Strategic Culture and the
   2011 Libyan Campaign},
Journal = {EUROPE-ASIA STUDIES},
Year = {2019},
Volume = {71},
Number = {7},
Pages = {1140-1161},
Month = {AUG 9},
Abstract = {The willingness of successive Polish governments to support and
   participate in US-led multilateral military interventions-such as the
   2003 invasion of Iraq-has been explained in terms of Poland's prevailing
   strategic culture. Yet in 2011 Poland opted to exclude itself from
   participating in the NATO campaign against Libya. It is argued that this
   was not a counter-cultural decision, but was instead a case of one
   strategic subculture supplanting another. The support that the
   government received from opposition politicians and the press can be
   taken as evidence that the policy did not represent a radical departure
   from Poland's strategic culture.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
