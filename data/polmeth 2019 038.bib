@incollection{ WOS:001078351300006,
Author = {Bhatia, Jai},
Editor = {Harriss-White, B and Michelutti, L},
Title = {Crime in the air: spectrum markets and the telecommunications sector in
   India},
Booktitle = {WILD EAST: Criminal Political Economies in South Asia},
Year = {2019},
Pages = {140-167},
Abstract = {This chapter explores the intertwining of crime and governance produced
   in the markets for spectrum. In so doing it traces the criminalised
   expansion of telecom. It investigates the systemic plunder of airwaves
   and a form of intreccio that involves capital, political and
   bureaucratic elites. Crimes result from privileged and discretionary
   control over information and resources, politicised manipulation of
   regulative laws and selective enforcement failures.
   Evidence is suggestive of large-scale and expanding capital accumulation
   by business and the political elite and the exchange of regulatory and
   financial privileges in return for campaign finance. Alongside
   widespread nepotism and cronyism the chapter traces important parallel
   shifts into politics on the part of corporate businessmen and into
   business on the part of politicians and officials. It shows how
   regulative law is effectively manipulated and how politicians modify law
   retrospectively to make ad hoc exemptions benefitting
   political-cum-business cronies.},
