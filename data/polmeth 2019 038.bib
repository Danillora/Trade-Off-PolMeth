@article{ WOS:000492102900001,
Author = {Tkacheva, Olesya},
Title = {Digital media and perceptions of the United States among the Russian
   elite, 2004-2016},
Journal = {POST-SOVIET AFFAIRS},
Year = {2019},
Volume = {35},
Number = {5-6, SI},
Pages = {393-405},
Month = {NOV 2},
Abstract = {This paper seeks to explain why Russian elites' exposure to online media
   for their news contributed to stronger pro-American attitudes than
   reliance on traditional media. Two causal mechanisms are tested using a
   repeated cross-section of elite surveys. One operates at the level of
   attitudes and is suggested by the field of political communication; the
   other emerges from the literature on cognitive psychology and operates
   at the level of beliefs by providing a cognitive map through which
   individuals process information and reach conclusions. I find that both
   mechanisms are relevant, with framing effects being particularly
   important to hardliners' perceptions of security threats.},
