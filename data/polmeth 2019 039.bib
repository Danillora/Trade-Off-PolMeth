@article{ WOS:000464593600004,
Author = {Aburamoto, Mari},
Title = {The Politics of Anti-Corruption Campaigns in Putin's Russia: Power,
   Opposition, and the All-Russia People's Front},
Journal = {EUROPE-ASIA STUDIES},
Year = {2019},
Volume = {71},
Number = {3},
Pages = {408-425},
Month = {MAR 16},
Abstract = {The beginning of Vladimir Putin's third term was characterised by the
   politicisation of corruption. Opposition forces focused on corruption
   and criticised the regime for tolerating it. This article explores how
   the regime reacted to opposition criticism. The regime encountered a
   dilemma: it could not ignore the opposition but promoting
   anti-corruption reforms was risky for the regime. This study focuses on
   the regime's use of the All-Russia People's Front (Obshcherossiiskii
   Narodnyi Front-ONF) and concludes that it functioned as a `parastatal'
   anti-corruption movement, enabling the regime to deflect the blame
   without directly involving itself in the process.},
