@article{ WOS:000484384500008,
Author = {Ayee, Gloria Y. A. and Carew, Jessica D. Johnson and Means, Taneisha N.
   and Reyes-Barrientez, Alicia M. and Sediqe, Nura A.},
Title = {White House, Black Mother: Michelle Obama and the Politics of Motherhood
   as First Lady},
Journal = {POLITICS \& GENDER},
Year = {2019},
Volume = {15},
Number = {3, SI},
Pages = {460-483},
Month = {SEP},
Abstract = {In 2008, for the first time in the history of this country, a black
   woman became First Lady of the United States. During Barack Obama's
   presidency, Michelle Obama was ever present in the public eye for her
   advocacy on issues related to health, military families, education, and
   for promoting the interests of women and girls. This article contributes
   to ongoing scholarly discourse, as well as extensive media coverage and
   analysis, regarding Obama's role as wife and first lady by critically
   examining how the particular model of motherhood she embraced and
   exhibited, a model firmly rooted in the black American community, was
   designed to challenge negative stereotypes of black women, maternity,
   and families. We address the following questions in this work: How did
   Obama's identity as a black woman influence the policies she championed
   as first lady? Does Obama's mothering relate to stereotypes of black
   mothers and help (re)define black motherhood, and if so, how? What does
   it mean to be a black mater gentis or mother of the nation? Drawing on
   her speeches and policy initiatives, we reveal how Michelle Obama defied
   dominant and oppressive stereotypes of black women and mothers while
   simultaneously (re)defining black womanhood and motherhood for the
   nation.},
