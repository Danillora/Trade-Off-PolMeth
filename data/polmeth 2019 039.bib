@article{ WOS:000788167700006,
Author = {Blanchard, Soline},
Title = {The market for gender-equality expertise: providing services to public
   and private organisations in France},
Journal = {EUROPEAN JOURNAL OF POLITICS AND GENDER},
Year = {2019},
Volume = {2},
Number = {1, SI},
Pages = {93-111},
Month = {FEB},
