@article{ WOS:000469239700007,
Author = {Pereira, Joana Castro and Viola, Eduardo},
Title = {Catastrophic Climate Risk and Brazilian Amazonian Politics and Policies:
   A New Research Agenda},
Journal = {GLOBAL ENVIRONMENTAL POLITICS},
Year = {2019},
Volume = {19},
Number = {2},
Pages = {93-103},
Month = {MAY},
Abstract = {Climate and deforestation impacts are jeopardizing the resilience of the
   Amazon rainforest, one of the key elements in the Earth's climate system
   whose dieback may trigger catastrophic climate change. The potential
