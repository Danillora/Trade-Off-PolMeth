@article{ WOS:000477954700008,
Author = {Wheatcroft, Stephen G.},
Title = {Soviet Statistics under Stalinism: Reliability and Distortions in Grain
   and Population Statistics},
Journal = {EUROPE-ASIA STUDIES},
Year = {2019},
Volume = {71},
Number = {6, SI},
Pages = {1013-1035},
Month = {JUL 3},
Abstract = {This essay attempts to understand the history and politics of Soviet
   grain and demographic statistics, and to show how they were used by the
   government. It covers the period from Lenin until Stalin's death.
   Distortions arose because of over-ambitious planning. Attempts to
   correct these distortions in 1932 failed because of the cover-up of the
   famine. Also the results of the census of 1937 were rejected, but the
   repeat census of 1939 did provide more realistic indicators. For grain,
   distortions remained to the end of the Stalin period and even increased.
   The essay argues that the basic statistics available in the archives are
