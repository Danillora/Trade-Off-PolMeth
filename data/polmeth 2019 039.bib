@article{ WOS:000471308100011,
Author = {McCreanor, Tim and Wetherell, Margaret and McConville, Alex and Barnes,
   Helen Moewaka and Barnes, Angela Moewaka},
Title = {New light; friendly soil: affective-discursive dimensions of Anzac Day
   commemorations in Aotearoa New Zealand},
Journal = {NATIONS AND NATIONALISM},
Year = {2019},
Volume = {25},
Number = {3},
Pages = {974-996},
Month = {JUL},
Abstract = {This paper investigates affective-discursive dimensions of
   nation-building via commemorations of nationhood within Aotearoa New
   Zealand to ask about how these assemblages construct feeling
   trajectories for citizen participants. We report auto-ethnographic
   analyses of participation in specific Anzac Day war remembrance events
   that occurred in the capital city Wellington. Analyses point to the ways
   in which engagement in the choreographies of commemoration constructs
   varied emotion-laden subject positions for participants and how these
   psycho-social differences index and evoke contrasting memorial politics.
   We conclude that while the differences in affective ambience at
   different events may prompt citizens towards nationalistic or more
   conciliatory identity politics, the ceremonies create space for
   participants to feel and enact diverse affective practices.},
