@article{ WOS:000515113000005,
Author = {Phuong Pham and Gibbons, Niamh and Denkinger, Jana Katharina and Junne,
   Florian and Vinck, Patrick},
Title = {Justice Not Forgiveness: Perspectives on Justice and Reconciliation
   among Yazidi Women Refugees in Germany},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
