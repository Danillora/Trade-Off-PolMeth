@article{ WOS:000474453000007,
Author = {Shaffer, Ryan},
Title = {Indian spies inside Pakistan: South Asian human intelligence across
   borders},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2019},
Volume = {34},
Number = {5},
Pages = {727-742},
Abstract = {This article examines India's historical efforts to spy inside Pakistan
   from the mid-1960s to the early 1970s. It draws from memoirs of notable
   Indian spies who were jailed for espionage in Pakistan and spy `fiction'
   written by former Indian intelligence and military officers who allege
   their writing is based on actual cases. The article highlights
   commonalities among Indian spies using the words of Indian officers to
   better understand human intelligence efforts inside Pakistan. It finds
   that Indian spies in these books have initially been Hindus or from
   multi-religious families, from the Indian-Pakistan border and have been
   poorly treated by the Indian government and its intelligence services.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
