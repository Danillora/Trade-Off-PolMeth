@article{ WOS:000472924200001,
Author = {Kovanic, Martin and Coufalova, Aneta},
Title = {The legitimacy of intelligence surveillance: the fight against terrorism
   in the Czech Republic and Slovakia},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2020},
Volume = {35},
Number = {1},
Pages = {115-130},
Month = {JAN 2},
Abstract = {The activities of intelligence services and the expansion of their
   powers have moved to the center of public debates since the terrorist
   attacks in Europe. Major discussions have been focused on the mechanisms
   of mass surveillance, which entail the infringement of individual
   rights. The purpose of this article is to compare how surveillance
   powers of intelligence services in the Czech Republic and Slovakia
   developed in the context of fight against terrorism. Intelligence
   services in both countries tried to expand their surveillance powers and
   these attempts have been met with criticism from the political
   opposition, civil society and courts.},
