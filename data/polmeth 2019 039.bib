@article{ WOS:000473815100002,
Author = {Montambeault, Francoise},
Title = {``It Was Once a Radical Democratic Proposal{''}: Theories of Gradual
   Institutional Change in Brazilian Participatory Budgeting},
Journal = {LATIN AMERICAN POLITICS AND SOCIETY},
Year = {2019},
Volume = {61},
