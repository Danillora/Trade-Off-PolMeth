@article{ WOS:000745544600002,
Author = {Grigoryan, Aleksandr and Khachatryan, Knar and Ter-Matevosyan, Vahram},
Title = {Armenia-Turkey border opening: what determines the attitude of
   Armenians?},
Journal = {CAUCASUS SURVEY},
Year = {2019},
Volume = {7},
Number = {1},
Pages = {25-43},
Month = {MAR},
Abstract = {In spite of all previous efforts, the land border between Armenia and
   Turkey remains closed. Being one of the last reminders of the Cold War
   era, it significantly hinders the development of Armenia and eastern
   regions of Turkey. However, a closed border is more than a physical
   obstacle, as it also shapes the worldview and perceptions of the
   respective societies. Using the recent survey on ``Public Opinion Poll:
   The Ways for Normalization of Armenian-Turkish Relations{''}, we
   identify the determinants of respondents' attitudes towards the opening
   of the border. Among other results, we find that more awareness of the
   current Armenian-Turkish relationship increases the likelihood of the
   approval of the border. However, when selecting those respondents, who
   are either loyal to or approve the opening of the border, the awareness
   of the protocols' content decreases the likelihood of the approval of
   the opening border. Our findings are supported by the contact theory
   which we use as a conceptual framework.},
