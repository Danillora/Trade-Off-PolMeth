@article{ WOS:000471308100003,
Author = {Blanchet, Alexandre and Medeiros, Mike},
Title = {The secessionist spectre: the influence of authoritarianism, nativism
   and populism on support for Quebec independence},
Journal = {NATIONS AND NATIONALISM},
Year = {2019},
Volume = {25},
Number = {3},
Pages = {803-821},
Month = {JUL},
Abstract = {In recent years, Quebec has been undergoing a re-evaluation of
   immigration and integration policies. The secessionist Parti Quebecois
   had become the leader of this debate, which also coincided with a rise
   of right-wing nativist, populist and sometimes authoritarian movements
   in other Western societies. This paper aims to evaluate the similarity
   or dissimilarity of Quebec's nationalism to these other nationalisms. We
   use the 2015 Canadian Election Study data to explore the influence of
   authoritarianism, nativism and populism directly on support for
   secession and also, indirectly, on voting intentions. We find that
   authoritarianism has a negative influence on support for Quebec
   independence and independentist parties, while the pattern is the
   opposite, and the effects somewhat weaker, for nativism and populism.
   Hence, we argue that Quebec nationalism does not seem to correlate with
   the right-wing populist movement extending throughout many Western
   societies. Thus, Quebec nationalism was shown to be a distinct form of
   nationalism.},
