Type = {Article},
Language = {English},
Affiliation = {Kuru, D (Corresponding Author), Turkish German Univ, Dept Polit Sci \& Int Relat, Istanbul, Turkey.
   Kuru, Deniz, Turkish German Univ, Dept Polit Sci \& Int Relat, Istanbul, Turkey.},
DOI = {10.1080/13629395.2019.1700674},
EarlyAccessDate = {DEC 2019},
ISSN = {1362-9395},
EISSN = {1743-9418},
