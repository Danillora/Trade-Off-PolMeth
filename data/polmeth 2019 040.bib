@article{ WOS:000485901400003,
Author = {Brockett, Charles D.},
Title = {The Drug Kingpin Decapitation Strategy in Guatemala: Successes and
   Shortcomings},
Journal = {LATIN AMERICAN POLITICS AND SOCIETY},
Year = {2019},
Volume = {61},
Number = {4},
Pages = {47-71},
Month = {NOV},
Abstract = {This study analyzes whether Guatemalan success with the kingpin
   decapitation strategy of bringing major drug traffickers to justice has
   accomplished its greater objectives of reducing cocaine trafficking and
   drug-related violence. The analysis finds little evidence of success for
   the first objective in Guatemala but notable success for the second. One
   of the few studies to examine the application of this strategy outside
   Mexico and Colombia, its findings are interpreted in light of their
   contrasting experiences. The article provides an overview of drug
   trafficking in Guatemala and concise studies of two of its most
   important organizations targeted by the kingpin strategy.},
