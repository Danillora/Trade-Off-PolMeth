@article{ WOS:000490166800001,
Author = {Stanivukovic, Senka Neuman},
Title = {European dis/integration in times of complexity ? reassembling Europe in
   the 2014 protests and plenums in Bosnia and Herzegovina},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2019},
Volume = {27},
Number = {4, SI},
Pages = {529-542},
Abstract = {This paper problematizes European dis/integration from the location of
   Southeast Europe, particularly social and political struggles of the
   2014 protests and plenums in Bosnia and Herzegovina. The European
   project is understood as a product of multiple assemblages to transgress
   various binaries that capture the European Union as a unitary entity and
   define the conceptual work on European integration and disintegration.
   In its central argument, the paper suggests that the political and
   everyday practices and claims articulated by the protests and plenums
   help us rethink the peripherality of Bosnia and Herzegovina vis-?-vis
   the European project and therefore also the spatio-temporal and
   institutional boundaries of the European project.},
