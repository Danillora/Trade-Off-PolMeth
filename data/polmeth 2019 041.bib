@article{ WOS:000499069400001,
Author = {Hagerdal, Nils},
Title = {Toxic waste dumping in conflict zones: Evidence from 1980s Lebanon},
Journal = {MEDITERRANEAN POLITICS},
Year = {2021},
Volume = {26},
Number = {2},
Pages = {198-218},
Month = {MAR 15},
Abstract = {Records show that numerous countries experiencing civil wars - including
   Angola, Eritrea, Lebanon, and Somalia - witnessed environmental crime,
   such as the dumping of toxic waste. To explore the dynamics of waste
   crime in conflict zones I combine a historic overview of the
   international trade in toxic waste with a case study of the 1987 toxic
   waste dumping scandal in Lebanon. I show that conflict zones provide
   ideal conditions for waste criminals, that waste crime is an easy way
   for militias to profit, and that environmental crime differs sharply
   from other modes of predation in the political science literature.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
