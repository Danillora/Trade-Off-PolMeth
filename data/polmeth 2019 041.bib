@article{ WOS:000467259400003,
Author = {Massicard, Elise and Visier, Claire},
Title = {Reconsidering the role of non-public actors in Turkish policy-making},
Journal = {MEDITERRANEAN POLITICS},
Year = {2019},
Volume = {24},
Number = {3},
Pages = {311-319},
Month = {MAY 27},
Abstract = {Indirect forms of government have become increasingly prominent in
   Turkey over the last few decades. After giving an overview of the
   growing role of non-public actors in Turkish policy-making, we depart
   from the common idea that the phenomenon is a result of neoliberalism
   only and examine the multiple genealogies and complex dynamics at work.
   The paper then questions the preconception according to which the
   involvement of non-public actors implies a retreat of the state and
   frames these developments in terms of changing forms of government.
   Finally, we question the outcomes of this phenomenon in terms of policy
   and power reconfigurations.},
