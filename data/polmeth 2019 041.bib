@article{ WOS:000485289100011,
Author = {Stromso, Mette},
Title = {Investigating everyday acts of contributing as `admission tickets' to
   belong to the nation in Norway},
Journal = {NATIONS AND NATIONALISM},
Year = {2019},
Volume = {25},
Number = {4},
Pages = {1238-1258},
Month = {OCT},
Abstract = {This article examines the centrality of contributing to questions of
   belonging in Norway. Building on the diversity of individuals living
   within a shared national space, the study identifies how everyday acts
   of contributing were conceived of by the participants as `admission
   tickets' to belong. The study further investigates the participants'
   motivations and desires for contributing and unveils how, given their
   migration background (internal, international or non-migrant), they
   respond to expectations of contributing from different positions of
   ontological security. The participants reveal ontological (in)security
   at different geographical scales, and they seek to manage this less
   secure position of national or local belonging through their acts of
   contributing in everyday life.},
