@article{ WOS:000507856700007,
Author = {Roberts, Caroline and Gilbert, Emily and Allum, Nick and Eisner, Leila},
Title = {RESEARCH SYNTHESIS SATISFICING IN SURVEYS: A SYSTEMATIC REVIEW OF THE
   LITERATURE},
