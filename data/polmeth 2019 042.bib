@article{ WOS:000498222500002,
Author = {Jordaan, Sarah M. and Siddiqi, Afreen and Kakenmaster, William and Hill,
   Alice C.},
Title = {The Climate Vulnerabilities of Global Nuclear Power},
Journal = {GLOBAL ENVIRONMENTAL POLITICS},
Year = {2019},
Volume = {19},
