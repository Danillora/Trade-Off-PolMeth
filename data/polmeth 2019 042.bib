@article{ WOS:000485972800007,
Author = {Arsenault, Rachel and Bourassa, Carrie and Diver, Sibyl and McGregor,
   Deborah and Witham, Aaron},
Title = {Including Indigenous Knowledge Systems in Environmental Assessments:
   Restructuring the Process},
