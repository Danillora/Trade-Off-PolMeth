@article{ WOS:000490944300009,
Author = {Leigh, Michael},
Title = {A view from the policy community: a new strategic narrative for Europe?},
Journal = {EUROPEAN SECURITY},
Year = {2019},
Volume = {28},
Number = {3, SI},
Pages = {382-391},
Abstract = {Reflecting on the results presented in articles in this special issue,
   European leaders should take greater account of external perceptions in
   crafting the European Union's strategic narrative and guiding its
   actions. Failure to do so has impaired external policies like the
   Euro-Mediterranean Partnership, the European Neighbourhood Policy and
   Eastern Partnership. Leaders emerging from the Arab uprisings perceived
   the EU as complicit with their countries' former anciens regimes and
   Russian leaders see EU support for democracy and the market economy in
   former Soviet states as duplicitous and instrusive. Awareness of such
   perceptions should be filtered into EU decision-making, without
   validating views that European officials and diplomats consider
   misleading.},
