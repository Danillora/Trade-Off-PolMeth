@article{ WOS:000498222500006,
Author = {Bumpus, Adam and Thu-Ba Huynh and Pascoe, Sophie},
Title = {Making REDD plus Transparent: Opportunities for Mobile Technology},
Journal = {GLOBAL ENVIRONMENTAL POLITICS},
