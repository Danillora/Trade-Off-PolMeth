@article{ WOS:000667220200003,
Author = {Gogishvili, David and Harris-Brandts, Suzanne},
Title = {The social and spatial insularity of internally displaced persons:
   ``neighbourhood effects{''} in Georgia's collective centres},
Journal = {CAUCASUS SURVEY},
Year = {2019},
