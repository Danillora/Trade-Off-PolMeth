@article{ WOS:000471387400001,
Author = {Wilkinson, Catherine and Briggs, Jo and Salt, Karen and Vines, John and
   Flynn, Emma},
Title = {In participatory budgeting we trust? Fairness, tactics and
   (in)accessibility in participatory governance},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2019},
Volume = {45},
Number = {6},
Pages = {1001-1020},
Month = {NOV 2},
Abstract = {Participatory budgeting (PB) is a significant innovation in democracy
   and local development. PB provides the opportunity for citizens to
   engage in processes of deliberation and decision-making upon the
   allocation of public funds. As new critical discourse emerges
   surrounding this model of local government spending, a significant area
   warranting investigation concerns how trust, and indeed mistrust, factor
   into PB. Through an analysis of interviews with residents and Council
   staff engaged in PB processes in a county in the north of England, we
   highlight the ways in which issues of trust can impact on participation
   in these initiatives, and also strengthen relationships between voting
   delegates, project teams and local government. This paper argues that
   increasing the perceived accessibility, and reconsidering the inclusion
   of mass membership groups in PB, might help to create progressive,
   effective and trustful participation.},
