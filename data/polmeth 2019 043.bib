@incollection{ WOS:000456192400007,
Author = {Vayrynen, Tarja},
Book-Author = {Vayrynen, T},
Title = {Peacebuilding in Colonial Relations},
Booktitle = {CORPOREAL PEACEBUILDING: MUNDANE BODIES AND TEMPORAL TRANSITIONS},
Series = {Rethinking Peace and Conflict Studies},
Year = {2019},
Pages = {101-119},
Abstract = {Colonial relations are seldom discussed in terms of peacebuilding. Yet,
   dismantling colonial relations requires comprehensive processes,
   approaches and stages that transform the relations toward more
   sustainable, peaceful, relations. Nations do not always want to
   recognize colonial relations and the existence of the indigenous
   populations at the core of the nation and, thereby, long-term
   peacbuilding is impossible. This chapter discusses the nexus of colonial
   violence and peacebuilding through the example of Finnish-Sami settler
   colonial relations. Violence that functions though the indigenous bodies
   render the bodies mute as violence embedded in these relations is
   structural, slow as well as epistemic. The chapter continues the
   discussion on the preconditions of the political and peacebuilding
   started in the previous chapter.},
