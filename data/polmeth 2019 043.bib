@article{ WOS:000526716000011,
Author = {Batiashvili, Nutsa},
Title = {Power/Memory: New Elite, Old Intelligentsia, and Fixing of the Georgian
   Mind},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2019},
Volume = {47},
Number = {6},
Pages = {1083-1099},
Month = {NOV},
Abstract = {This article examines an ideological and a narrative rift between two
   elitist formations and two forms of nationalism that a practice of
   memory-making embodies. In the subterranean polemic where Soviet
   generation intelligentsia and liberal intellectuals animate the past on
   Russian-Georgian relations in two distinct ways, past becomes a critical
   terrain where the struggle over Georgia's geopolitical belonging and the
   resulting disputes on national identity take place. This analysis not
   only flashes out recent discursive rifts, linking them to the broader
   political processes, but traces the genealogies of the narrative
   practices that enable two idioms of nationalist discourse. It is both an
   analysis of post-socialist class formations and of the semantic fields
   within which their idioms are embedded.},
