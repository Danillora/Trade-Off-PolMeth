@article{ WOS:000476302800001,
Author = {Siwinska-Gorzelak, Joanna and Bukowska, Grazyna and Wojcik, Piotr},
Title = {The impact of revenue autonomy on the composition of local public
   spending: evidence from Poland},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2020},
Volume = {46},
Number = {4, SI},
Pages = {641-665},
Month = {JUL 3},
Abstract = {This paper studies the relationship between tax autonomy and the
   structure of local public expenditures. We rely on panel data from
   Poland for more than 2400 municipalities over the years 2002-2014. We
   use a measure of tax autonomy, based on `potential' own revenues,
   defined as revenues that the authorities are entitled to collect, before
   local tax policies have been implemented. This measure alleviates to
   some extent the endogeneity of indicators based on actually collected
   own taxes. Our results suggest a U-shaped relationship between the ratio
   of own local taxes and the share of capital expenditures and a negative
   relationship between the former ratio and the share of education
   spending.},
