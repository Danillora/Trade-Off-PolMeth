@article{ WOS:000458721500007,
Author = {Evstatiev, Simeon},
Title = {Milletic Secularism in the Balkans: Christianity, Islam, and Identity in
   Bulgaria},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
