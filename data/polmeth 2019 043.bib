@article{ WOS:000515066900012,
Author = {Metreveli, Tornike},
Title = {Rhyming the National Spirit: A Comparative Inquiry into the Works and
   Activities of Taras Shevchenko and Ilia Chavchavadze},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
