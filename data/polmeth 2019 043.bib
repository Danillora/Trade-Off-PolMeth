@incollection{ WOS:000519794900002,
Author = {Ma, Ngok},
Editor = {Ma, N and Cheng, EW},
Title = {From Political Acquiescence to Civil Disobedience Hong Kong's Road to
   Occupation},
Booktitle = {UMBRELLA MOVEMENT: CIVIL RESISTANCE AND CONTENTIOUS SPACE IN HONG KONG},
Series = {Global Asia},
Year = {2019},
Volume = {9},
Pages = {27-50},
Abstract = {The outbreak of the Umbrella Movement in 2014 followed decades of
   futility in the democracy movement. Years of conventional protests and
   bargaining had failed to bring about full democracy for Hong Kong. The
   rise of a new political identity and trends of radicalization in social
   and political movements fuelled the massive civil disobedience campaign.
   With Beijing handing down an election formula that allowed popular
   elections only after a Beijing-controlled committee screened the
   candidates, the opposition was prepared to launch an occupation
   campaign. Police violence then triggered the spontaneous participation
   of the masses, culminating in the 79-day occupation that signified a new
   stage of contentious movements for Hong Kong.},
