@article{ WOS:000501038200001,
Author = {Copik, Jan and Kopriva, Radek and Cmejrek, Jaroslav},
Title = {Mayors as a variable in typologies of local governments: a case study of
   the Czech Republic},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2021},
Volume = {47},
Number = {2},
Pages = {167-185},
Month = {MAR 4},
Abstract = {The study focuses on the position of mayors in the self-government of
   municipalities in the Czech Republic. The typology of municipal
   governments classifies the Czech Republic in the `collective form'
   category. The aim of the case study is to look for a way to supplement
   existing approaches to the typology of local governments. We emphasise
   the factual role of mayors in the formulation of development goals of
   municipalities, their initiative and weight in decision-making on
   budgets and investment actions. The study is based on data obtained
   through a questionnaire survey of municipal representatives and
   interviews with representatives of selected municipalities. Based on the
   results of both surveys, it is possible to prove that in spite of their
   not very strong legislative position, mayors can play a key role as
   initiators of investment projects and in promoting the municipal
   development agenda.},
