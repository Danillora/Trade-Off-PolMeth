@article{ WOS:000471551900001,
Author = {Buttazzoni, Adrian and Arku, Godwin and Cleave, Evan},
Title = {Practitioners' perspectives on in-house versus arm's-length structures
   of local economic development delivery in Ontario, Canada},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2019},
Volume = {45},
Number = {6},
Pages = {913-936},
Month = {NOV 2},
Abstract = {The structure for local economic development is an important part of the
   development process. In recent years, local governments have been
   switching structures, leading to debates about the most appropriate
   structure for an effective economic development delivery. Based on
   in-depth interviews with practitioners of local economic development,
   the paper examines the strengths and drawbacks of the two primary
   structures in the Province of Ontario: In-house and Arms-length. In
   doing so, the paper accomplishes two things: first, it adds to the
   empirical discussion on organisational approaches to economic
   development; second, it provides an informed perspective to assist
   communities that might be considering switching their structures.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
