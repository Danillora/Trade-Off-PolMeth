@article{ WOS:000515066900009,
Author = {Insebayeva, Sabina},
Title = {Japan's Central Asia Policy Revisited: National Identity, Interests, and
   Foreign Policy Discourses},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2019},
Volume = {47},
Number = {5, SI},
Pages = {853-867},
Month = {SEP},
Abstract = {This article focuses on the nature of Japan's foreign policy formulation
   and legitimization through a study of its interaction with Central Asian
   countries. The article examines foreign policy discourse that constructs
   Japan's ``self{''} vis-a-vis Central Asian ``other.{''} It reveals the
   textual mechanism through which reality, objects, and subjects are
   constructed, and it interprets the official statements contained in
   several foreign policy initiatives, in particular, the ``Eurasian (Silk
   Road) Diplomacy,{''} the ``Central Asia plus Japan,{''} and the ``Arc of
   Freedom and Prosperity,{''} as an attempt to understand the
   intersubjective knowledge and analytical lens through which Japanese
   foreign policy makers conceive and interpret the constructed
   ``reality,{''} produce foreign policy choices, and choose among
   identified alternatives.},
