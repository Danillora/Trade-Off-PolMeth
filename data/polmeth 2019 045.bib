@incollection{ WOS:000456550200009,
Author = {Deese, R. S.},
Book-Author = {Deese, RS},
Title = {Climate Change and the Future of Democracy},
Booktitle = {CLIMATE CHANGE AND THE FUTURE OF DEMOCRACY},
Series = {Environmental Challeges and Solutions},
Year = {2019},
Volume = {5},
Pages = {129-153},
Abstract = {There are three broad paths to establishing supranational democracy in
   order to address the transnational challenge of climate change. The
   first and most direct path would involve bypassing national governments
   altogether and organizing municipalities on a global scale. The second
   and most cautious path would involve creating an integrated federation
   of established democracies that could grow over time. The third and most
   ambitious path would involve democratizing already existing global
   institutions such as the United Nations. Each of these paths has
   benefits and drawbacks, but the middle path of pursuing greater
   political integration among established democracies is probably the most
   viable.},
Publisher = {SPRINGER INTERNATIONAL PUBLISHING AG},
Address = {GEWERBESTRASSE 11, CHAM, CH-6330, SWITZERLAND},
Type = {Article; Book Chapter},
Language = {English},
Affiliation = {Deese, RS (Corresponding Author), Boston Univ, Div Social Sci, Boston, MA 02215 USA.
