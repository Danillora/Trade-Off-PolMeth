@incollection{ WOS:000456550200007,
Author = {Deese, R. S.},
Book-Author = {Deese, RS},
Title = {Governing Ourselves},
Booktitle = {CLIMATE CHANGE AND THE FUTURE OF DEMOCRACY},
Series = {Environmental Challeges and Solutions},
Year = {2019},
Volume = {5},
Pages = {87-105},
Abstract = {The rapid integration of commerce, transport, and communication over the
   past century seems to suggest that the world is on the path to becoming
   a single global civilization. Although the form of governance that will
   maintain that civilization has yet to be determined, there is no reason
   to assume that it will be democratic, unless there is a deliberate and
   sustained effort to assure that it is. Given the symbiotic relationship
   between science and democracy, and the superior historical record of
   democratic governments on environmental issues, there is reason to
   believe that the establishment of democratic governance on a global
   scale would offer the best strategy for dealing with climate change.},
