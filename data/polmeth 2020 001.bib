@article{ WOS:000545845600001,
Author = {Cameron, Sarah and McAllister, Ian},
Title = {Policies and performance in the 2019 Australian federal election},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2020},
Volume = {55},
Number = {3},
Pages = {239-256},
Month = {JUL 2},
Abstract = {The Liberal-National Coalition success in the 2019 Australian federal
   election surprised many observers, with the opinion polls consistently
   predicting a Labor victory. The election was notable for Labor's
   proposals for wide-ranging tax changes, a historically unpopular Labor
   leader, and the fourth change of prime minister outside an election
   since 2010. Using the 2019 Australian Election Study, we test the
   influence of two models of voting, the first based on ideology together
   with an emphasis on policy, and the second on performance. The results
   show that performance was the dominant explanation for the result, with
   evaluations of party competence and leader popularity playing a major
   role in explaining voting behaviour in the election, both of which
   benefitted the Coalition.},
