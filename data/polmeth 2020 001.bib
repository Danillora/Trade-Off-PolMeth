   Fontaine, Sam A., Univ Calif Santa Barbara, Polit Sci, Santa Barbara, CA 93106 USA.},
DOI = {10.1111/psq.12676},
EarlyAccessDate = {SEP 2020},
ISSN = {0360-4918},
EISSN = {1741-5705},
