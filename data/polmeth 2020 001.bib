@article{ WOS:000549584800001,
Author = {Berezutskaya, Marina},
Title = {The history of bandura art or how bandura became the musical symbol of
   the Ukrainian nation},
Journal = {NATIONAL IDENTITIES},
Year = {2021},
Volume = {23},
Number = {5},
Pages = {491-509},
Month = {OCT 20},
Abstract = {Bandura is a unique Ukrainian national folk instrument. The analysis of
   the history of the bandura shows that bandura art appeared in the
   sixteenth century among Cossacks who struggled for freedom, independence
   and unity of the Ukrainian nation. The repertoire of Cossack bandurists
   who later became professional bandura players, reflected not only
   important historical events but also all the aspects of Ukrainian life.
   At all times bandurists songs answered the most important question for
   every nation - who are we? Bandurists songs formed self-consciousness of
   the Ukrainian people and contributed to the transformation of Ukrainian
   ethnos into a nation. Thus, the bandura art has become an important
   feature of the Ukrainian national identity and the bandura itself - the
   musical symbol of - Ukrainian nation.},
