@article{ WOS:000529365500006,
Author = {Budabin, Alexandra Cosima},
Title = {Caffeinated Solutions as Neoliberal Politics: How Celebrities Create and
   Promote Partnerships for Peace and Development},
Journal = {PERSPECTIVES ON POLITICS},
Year = {2020},
Volume = {18},
Number = {1},
Pages = {60-75},
Month = {MAR},
Abstract = {How do celebrities exert power to influence elite and popular thinking
   and policy around peace and development? Drawing from research on
   neoliberalism, celebrities, and ethical consumption, I build an
   interpretive analysis of two case studies of Brand Aid initiatives to
   argue first, that celebrities mobilize financial and political capital
   to create partnerships across businesses, NGOs, and the government in
   ways that embody neoliberal politics by ushering in new private actors;
   and second, that celebrities reinforce these neoliberal politics by
   promoting these partnerships to popular and elite audiences. I discuss
   how this paper contributes to unmasking neoliberal trends by showing how
   celebrities are deepening their engagement in ways that hold
   implications for democratic politics.},
