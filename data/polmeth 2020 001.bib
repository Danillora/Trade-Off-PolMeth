@article{ WOS:000575170900014,
Author = {Baliga, Sandeep and De Mesquita, Ethan Bueno and Wolitzky, Alexander},
Title = {Deterrence with Imperfect Attribution},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
