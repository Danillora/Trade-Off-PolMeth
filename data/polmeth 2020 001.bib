@article{ WOS:000527541900001,
Author = {Kassim, Hussein and James, Scott and Warren, Thomas and Hargreaves Heap,
   Shaun},
Title = {Preferences, Preference Formation and Position Taking in a Eurozone Out:
   Lessons from the United Kingdom},
