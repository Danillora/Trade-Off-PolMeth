@article{ WOS:000570817300001,
Author = {Hoskins, Te Kawehau and Bell, Avril},
Title = {Being Present: Embodying Political Relations in Indigenous Encounters
   with the Crown},
Journal = {CONTEMPORARY POLITICAL THEORY},
