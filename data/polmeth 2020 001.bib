@article{ WOS:000612306300007,
Author = {Hintz, Marcin},
Title = {GLOBAL CHRISTIAN RESPONSIBILITY IN THE ACTIVITY OF THE LUTHERAN WORLD
   FEDERATION IN THE TIME OF CORONAVIRUS PANDEMIC},
