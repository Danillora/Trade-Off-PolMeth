@article{ WOS:000556264500003,
Author = {Chartash, David and Caruana, Nicholas J. and Dickinson, Markus and
   Stephenson, Laura B.},
Title = {When the team's jersey is what matters: Network analysis of party
   cohesion and structure in the Canadian House of Commons},
Journal = {PARTY POLITICS},
