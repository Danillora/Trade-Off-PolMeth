@article{ WOS:000529442700007,
Author = {Hlepas, Nikolaos Komninos},
Title = {Checking the mechanics of Europeanization in a centralist state: The
   case of Greece},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2020},
Volume = {30},
Number = {2, SI},
Pages = {243-261},
Month = {MAR 14},
Abstract = {After a short period of transition to democracy, Greece joined EC in
   1981, in spite of its obvious misfit. Europeanization exercised strong
   adaptation pressure. Being one of the most centralist countries in
   Europe, Greece introduced decentralization reforms as part of a wider
   democratization endeavour. New policy fields were introduced and the
   domestic governance system became more complex and inclusive, especially
   through the introduction of the regional meso-level, even though the
   primacy of central government was not really challenged. Europeanization
   changed the structure of constraints and opportunities, the orientation,
   the beliefs and the priorities of actors. The EU became an important
   frame both for rescaling various interests as well as for the
   transformation of public administration. The crisis marked the shift
   from adaptation to conditionality and the revival of centralization and
   exclusive intra-state politics, while at the same time the central
   government is accountable for policy implementation to supra-national
   institutions.},
