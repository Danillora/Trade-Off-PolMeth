@article{ WOS:000607205400016,
Author = {Cuprov, Vladimir and Kosovskaja, Veda},
Title = {The forlorn tundra Oil spills in Russia: causes and solutions},
Journal = {OSTEUROPA},
Year = {2020},
Volume = {70},
Number = {7-9},
Pages = {279+},
Abstract = {One of the most serious environmental problems in Russia is the
   contamination of soil and water through crude oil and crude oil
   products. The main cause is corrosion damage to oilfield pipes that have
