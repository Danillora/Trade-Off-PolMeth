@article{ WOS:000539075400005,
Author = {Lopez, Mabel},
Title = {Intimate Violence in New Granada during the Enlightenment: Conjugal
   Cases of the Caribbean},
Journal = {CIENCIA POLITICA},
Year = {2020},
Volume = {15},
Number = {29},
Pages = {111-145},
Month = {JAN-JUN},
Abstract = {Based on a set of criminal trials that took place in the Caribbean and
   other contextual ones, this article aims to analyze couple conflicts in
   New Granada in the colonial period and its criticism by the glimpse of
   enlightened ideas. The text recovers the main conclusions of a doctoral
   dissertation prepared with 144 files sent on appeal to the Royal Court
   of Santa Fe for injury, abuse, mistreatment, homicide, abandonment,
   separation and divorce during the eighteenth century and the first years
   of the nineteenth century, which were contrasted with
   seventeenth-century corpus. This is a research carried out from a gender
   perspective and based on the configurational theory of Norbert Elias,
   which attempts to demonstrate long-term transformations regarding the
   way of approaching and prosecuting violence between spouses.},
