@incollection{ WOS:000614246300006,
Author = {Emerson, Peter},
Book-Author = {Emerson, P},
Title = {Majority Voting in Belfast, Dublin and London},
Booktitle = {MAJORITY VOTING AS A CATALYST OF POPULISM: PREFERENTIAL DECISION-MAKING
   FOR AN INCLUSIVE DEMOCRACY},
Year = {2020},
Pages = {77-105},
Abstract = {Throughout Britain and Ireland, people use majority voting, and in
   politics, they believe in a binary form of majority rule. The
   consequences have been huge, and this Chapter looks at those which have
