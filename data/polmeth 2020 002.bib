@article{ WOS:000523002700001,
Author = {Albert, Zachary and La Raja, Raymond J.},
Title = {Who should decide the party's nominee? Understanding public attitudes
   toward primary elections},
Journal = {PARTY POLITICS},
Year = {2021},
Volume = {27},
Number = {5},
Pages = {928-941},
Month = {SEP},
Abstract = {Primary elections in the United States reflect the most inclusive
   nomination process among political parties across democracies. The
   desire for mass participation in party nominations appears embedded in a
   widespread populist ideal. It remains unclear, however, the extent to
   which voters believe that elites should be able to influence the
   process. Using data from the 2018 Cooperative Congressional Election
   Study, we demonstrate that levels of political engagement, perceived
   ideological distance from the party, and partisanship predict the degree
   of support for popular selection of party nominees. We find key
   differences between the parties, with Republicans, particularly
   conservatives, more strongly opposed to elite influence. Surprisingly,
   most voters tend to have a pluralistic approach to selecting nominees,
   allowing for party officials and experts to weigh in on picking the
   party's candidates. The findings have implications for how we conceive
   of political parties and the kinds of nomination reforms that might be
   embraced by voters.},
