@article{ WOS:000515606900001,
Author = {McKeever, Anna},
Title = {Explaining policy deadlock: the case of Sarkozy's integration policy},
Journal = {FRENCH POLITICS},
Year = {2021},
Volume = {19},
Number = {1},
Pages = {51-64},
Month = {MAR},
Abstract = {What processes account for the deadlock in a given policy? While causes
   and mechanisms of policy change have been extensively researched, this
   paper sheds light on how formal institutional change can lead to policy
   deadlock. This paper examines two cases that demonstrate the absence of
   progress on integration policy during Sarkozy's time in office (as a
   Minister of the Interior and as a president). Drawing on elite
   interviews, the paper points out how institutional change can lead to
   departmental competition within new or combined structures, leading to
   policy deadlock, as new layered actors do not always have the capacity
   to push old ones out, resulting in further institutional change.},
