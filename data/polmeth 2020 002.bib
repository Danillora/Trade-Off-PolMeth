@article{ WOS:000613859900006,
Author = {Sokova, Elena},
Title = {Disruptive technologies and nuclear weapons},
Journal = {NEW PERSPECTIVES},
Year = {2020},
Volume = {28},
Number = {3},
Pages = {292-297},
Month = {SEP},
Abstract = {The current security environment faces two major challenges. On the one
   hand, the international arms control and security architecture are
   deteriorating. Doctrinal innovations, first in Russia and then in the
   United States, allow for limited use of nuclear weapons, including
   potentially the low-yield varieties. On the other hand, advanced
   conventional weapons, cyber warfare, artificial intelligence, and other
   emerging technologies increase the risk of conflict, which could
   escalate to the nuclear level. As risks increase, the political will to
   pursue cooperative solutions to prevent arms race and war continues to
   diminish. Some nuclear states are even reluctant to recommit to the
   Reagan-Gorbachev statement that ``A nuclear war cannot be won and must
   never be fought.{''}},
