@article{ WOS:000584343000008,
Author = {Kikuta, Kyosuke},
Title = {The Environmental Costs of Civil War: A Synthetic Comparison of the
   Congolese Forests with and without the Great War of Africa},
Journal = {JOURNAL OF POLITICS},
