@article{ WOS:000607205400004,
Author = {Bemmann, Albrecht and Petrov, Vladimir},
Title = {Woodland and forest management in Russia Its development, current status
   and prospects for the future},
Journal = {OSTEUROPA},
Year = {2020},
Volume = {70},
Number = {7-9},
Pages = {45+},
Abstract = {Russia has more woodland and forest than any other country in the world.
   The wood-lands are of major national and global ecological importance,
   particularly since they capture carbon in the soil, in dead wood and in
   the trees. Wood as a sustainable raw material is also an important
   economic factor, and for the state, the use of wood is of particular
   importance. Since trees take many years to grow, the positive and
   negative impact of forest management only becomes evident many decades
   after the event.},
