@article{ WOS:000599689400008,
Author = {Komolov, Oleg},
Title = {Deglobalization and the ``Great Stagnation{''}},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2020},
Volume = {10},
Number = {3},
Pages = {424-439},
Abstract = {Modern capitalism is going through a crisis of globalization. The
   activity of world trade and capital flows is decreasing, protectionist
   tendencies are growing, countries confront each other in currency wars
   and sanctions regimes. This originated from the extensive nature of the
   capitalist development of the latest decades through subordinating the
   non-capitalist periphery, its markets and sources of natural resources,
   using low-cost labor. It intensified the contradictions of the economic
   development of both the core and the periphery of the world economy:
   growing inequality, financialization, unemployment, slowdown in
   scientific progress and caused the long lasting ``great stagnation,{''}
   which can be identified by the lower growth rate of the world GDP after
   2008.},
