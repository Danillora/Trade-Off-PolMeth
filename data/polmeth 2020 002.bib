   Paris Albert, Sonia, Univ Jaume 1, Dept Filosofia \& Sociol, Castellon de La Plana, Spain.},
DOI = {10.30827/revpaz.v13i2.15838},
ISSN = {1988-7221},
Keywords = {hate; fear; discourses of hate; sentimental education; problematizing
   education},
Research-Areas = {Government \& Law},
Web-of-Science-Categories  = {Political Science},
Author-Email = {sparis@uji.es},
