@article{ WOS:000559231200001,
Author = {Cunliffe-Jones, Peter},
Title = {From Church and Mosque to WhatsApp-Africa Check's Holistic Approach to
   Countering `Fake News'},
Journal = {POLITICAL QUARTERLY},
Year = {2020},
Volume = {91},
Number = {3},
Pages = {596-599},
Month = {JUL},
Abstract = {False claims and misinformation spread in Africa not only on Facebook,
   in WhatsApp groups or on the dark web, but also in political meeting
   rooms, churches, mosques, and community gatherings. To reduce the harms
   caused by misinformation, Africa Check, the leading independent
   fact-checking organisation on the continent, believes a four-pronged, or
   holistic approach is needed: identifying and reducing the circulation of
   harmful misinformation; ensuring accurate information is more widely
   available; ensuring a more accurate understanding of topics it covers;
   and fostering fact-checking skills among the public. To achieve this, it
   reaches out and engages with a wide range of on- and offline communities
   and networks. The UK could learn from this approach to tackle the ways
   that misinformation continues to spread offline today.},
