Author = {Groenendyk, Eric and Krupnikov, Yanna},
Title = {What Motivates Reasoning? A Theory of Goal-Dependent Political
   Evaluation},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
