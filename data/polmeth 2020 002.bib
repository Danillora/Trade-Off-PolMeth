@article{ WOS:000574489500001,
Author = {Jaspal, Rusi and da Silva Lopes, Barbara Cristina and Breakwell, Glynis
   M.},
Title = {British national identity and life satisfaction in ethnic minorities in
   the United Kingdom},
Journal = {NATIONAL IDENTITIES},
Year = {2021},
Volume = {23},
Number = {5},
Pages = {455-472},
Month = {OCT 20},
Abstract = {This study examines British national identification in survey data from
   226 Indians, Pakistanis, Black Caribbeans and Black Africans. Black
   participants reported stronger ethnic identification and more frequent
   ethnic discrimination; and Pakistanis reported higher religiosity and
   more frequent religious discrimination, but stronger British national
   identification. Uncertainty about the meaning and accessibility of
   British national identity and perceived ethnic discrimination were
   associated with decreased British national identification. British
   national identification was in turn positively associated with life
   satisfaction. Uncertainty about the meaning and significance of
   Britishness challenges its value to overall identity maintenance, making
   it less accessible to ethnic minorities.},
