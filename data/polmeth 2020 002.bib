@article{ WOS:000523163800003,
Author = {Baniamin, Hasan Muhammad and Jamil, Ishtiaq and Askvik, Steinar},
Title = {Mismatch between lower performance and higher trust in the civil
   service: Can culture provide an explanation?},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2020},
Volume = {41},
Number = {2},
Pages = {192-206},
Month = {MAR},
Abstract = {The existing literature on institutional trust leads us to expect that
   institutional trust is determined by institutional performance. In the
   context of three South Asian countries (Bangladesh, Nepal and Sri
   Lanka), we find the opposite - that despite poor performance of the
   civil service, there is high institutional trust. In this study we
   examine whether such mismatch can be explained by an authoritarian
   cultural orientation, which generates unquestioning obedience and
   loyalty and may contribute to inflated institutional trust in the civil
   service of these countries.},
