@article{ WOS:000584459800001,
Author = {Buzdar, Muhammad Ayub and Fatima, Tahseen},
Title = {Frustration and motivation towards political participation in Muslim and
   non-Muslim young students in Pakistan},
Journal = {ASIAN JOURNAL OF COMPARATIVE POLITICS},
Year = {2022},
Volume = {7},
Number = {4},
Pages = {861-872},
Month = {DEC},
Abstract = {The socio-political conditions of Pakistan are not good for many
   religious and ethnic minority groups. This study explores the
   frustration and motivation of Muslim and non-Muslim young students
   around political participation in this environment. A survey research
   design was followed to achieve this objective. Seven-hundred secondary
   school students affiliated with Hindu, Christian, Muslim and Ahmadiyya
   communities participated in the study. A self-reported questionnaire was
   used to gauge the respondents' political efficacy, attitude towards
   democracy and motivation for expected political participation in the
   country. The findings demonstrate students' concerns, to varying
   extents, about the performance of political institutions in the country.
   The religious affiliations of the participants have significant
   influence on their motivation to contribute to political affairs. The
   results are alarming for a country with an uncertain democratic record
   for religious and ethnic minorities.},
