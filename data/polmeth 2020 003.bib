@article{ WOS:000609242200001,
Author = {Maskarinec, Pavel and Novotny, Lukas},
Title = {From the right to the left: transformation of the regional support of
   the technocratic populist ANO 2011 movement in Czech parliamentary
   elections of 2013 and 2017},
Journal = {AUSTRIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2020},
Volume = {49},
Number = {4},
Pages = {1-14},
Abstract = {The aim of this study is to explain the dominance of the ANO movement in
   the 2013 and 2017 parliamentary election, and thus contribute to the
   discussion about ``technocratic populism{''}. The article opens with a
   brief overview of the state of research on populism. In particular, the
   term technocratic populism. The next chapter presents the Czech protest
   movement ANO 2011 and its successes in the elections as an example of
   technocratic populism. The following analysis presents selected findings
   from party research and electoral geography and tries to answer the
   following question: Given its programmatic de-ideologization and
   technocratic communicative style, does the spatial support for the ANO
   movement cross the long-term spatial dividing lines of Czech party
   system, or is it more successful in traditionally left or right-wing
   areas of the Czech Republic, thus only gaining voters from the
   traditional blocs of the Czech party system?},
