@incollection{ WOS:000614246300007,
Author = {Emerson, Peter},
Book-Author = {Emerson, P},
Title = {Continental Europe: Are We All Little Bolsheviks?},
Booktitle = {MAJORITY VOTING AS A CATALYST OF POPULISM: PREFERENTIAL DECISION-MAKING
   FOR AN INCLUSIVE DEMOCRACY},
Year = {2020},
Pages = {107-151},
Abstract = {Although majority voting was first found to be inadequate by Pliny the
   Younger in Rome in the year CE 105 (Sect. 1.1.1), most people today,
   from Brest to Brest-Litovsk, still believe in majoritarianism. This
   chapter looks at some of the effects in this belief, briefly in western
   Europe, in some detail in the Balkans and the Caucasus, albeit with a
   little interlude in Turkey, and finally in the country which suffered
   more than any other from these ideas: Russia.},
