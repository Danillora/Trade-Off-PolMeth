@article{ WOS:000451966900008,
Author = {Gomez Baeza, Nicolas},
Title = {Surveillance, repression and labor discipline in the Sociedad
   Explotadora of Tierra del Fuego (1910-1919)},
Journal = {IZQUIERDAS},
Year = {2020},
Volume = {49},
Pages = {123-140},
Month = {JAN},
Abstract = {This paper seeks to deepen the analysis of violence executed against
   workers during the decade of 1910, in the spaces of the ``Sociedad
   Explotadora de Tierra del Fuego{''}, the most important sheep farming
   company at that period on the Territory of Magallanes. After reviewing
   different sources, it is proposed that such violence, far from being
   exceptional, were escalations that responded to objectives of labour
   discipline and maintenance of employer power, facing the agency
   developed from the local working class, and contributing to the
   configuration of conflict scenarios.},
