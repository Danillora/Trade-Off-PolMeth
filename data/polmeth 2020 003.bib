@article{ WOS:000588524600003,
Author = {Birac, Dimitrije},
Title = {POLITICAL ECONOMY OF CROATIA: RESTORATION OF CAPITALISM AS
   ``TRANSFORMATION{''} AND ``PRIVATIZATION{''}},
Journal = {POLITICKA MISAO-CROATIAN POLITICAL SCIENCE REVIEW},
