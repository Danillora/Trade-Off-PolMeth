@incollection{ WOS:000568773500003,
Author = {Kirkland, Christopher},
Book-Author = {Kirkland, C},
Title = {New Approach},
Booktitle = {CLASSIFYING ELECTIONS IN BRITAIN},
Year = {2020},
Pages = {21-38},
Abstract = {This chapter highlights some of the weaknesses of shoehorning ``new{''}
   elections into a model designed to demonstrate differences between
   national and European elections. It argues that using the terminology of
   first- and second-order elections is problematic given the new elections
   introduced to Britain over the last two decades. Rather than relying
   upon such terminology, it outlines how a more holistic classification of
   elections may be achieved and argues that in order to achieve this we
   should broaden our analysis of groups involved from simply voters to
   include political parties and the media. As well as offering a new
   method for understanding British elections, it argues that rather than
   distinguishing between firstand second-order a more accurate
   classification would differentiate elections based on geography-between
   national and subnational elections.},
Publisher = {PALGRAVE},
Address = {HOUNDMILLS, BASINGSTOKE RG21 6XS, ENGLAND},
Type = {Article; Book Chapter},
