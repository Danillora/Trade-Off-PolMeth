@article{ WOS:000524734000001,
Author = {Trithara, Dakoda},
Title = {Securitizing Disinformation: The Case of Westminster's Digital, Culture,
   Media and Sport Committee},
Journal = {DEMOCRACY \& SECURITY},
Year = {2025},
Volume = {21},
Number = {3},
Pages = {288-315},
Month = {JUL 3},
Abstract = {Major political events around the globe have been subject to
   disinformation campaigns that leverage intentionally misleading content
   to influence public opinion. The disruption created by the
   micro-targeted dissemination of disinformation presents a problem for
   democracies: how should democracies, where legitimacy is derived from
   free and fair elections, respond to this phenomenon? In the United
   Kingdom, Westminster's Digital, Culture, Media and Sport (DCMS)
   Committee opened an inquiry into the role of disinformation in the
   Brexit vote that culminated with numerous recommendations. Using
   securitization theory as a framework for analysis, this article explores
   the work of the DCMS Committee and its reception by the UK Government,
   Facebook, and the general public. It contributes to a deeper
   understanding of how democracies can respond to disinformation,
   providing a building block for future studies on state strategies to
   regulate technology platforms. The findings suggest the Committee
   successfully established a securitizing frame around disinformation.},
