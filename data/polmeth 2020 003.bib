@article{ WOS:000529365500005,
Author = {Richey, Lisa Ann and Brockington, Dan},
Title = {Celebrity Humanitarianism: Using Tropes of Engagement to Understand
   North/South Relations},
Journal = {PERSPECTIVES ON POLITICS},
Year = {2020},
Volume = {18},
Number = {1},
Pages = {43-59},
Month = {MAR},
Abstract = {Celebrity humanitarianism has been transformed in its scope, scale, and
   organization in the last thirty years. Its flourishing has generated
   considerable academic interest from a wide variety of disciplines that
   share two characteristics. First, these studies are-unusually-well
   connected, which means that different disciplines have not tended to
   develop their own separate literatures, but learn from each other's
   approaches. This makes it useful and important to identify ways
   different disciplinary approaches can complement each other. Second,
   most of this attention has focused on politics of celebrity
   humanitarianism in the global North. Yet focusing also on the South and
   on North/South relations will move the field forward. We argue that
   celebrity humanitarianism must be interpreted through the broader
   systems of which it is a part. We offer a heuristic typology of
   celebrity humanitarianism that continues to bridge between different
   disciplines and which identifies ways in which political science can
   complement existing studies. We also use this typology to refocus work
   on the politics of celebrity humanitarian relations away from merely
   Northern politics. This approach allows us to identify what sorts of
   politics and political solutions are being advocated by current forms of
   celebrity humanitarianism.},
