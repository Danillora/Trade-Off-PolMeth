@article{ WOS:000537877000001,
Author = {Hundt, David},
Title = {The changing role of the FIRB and the politics of foreign investment in
   Australia},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2020},
Volume = {55},
Number = {3},
Pages = {328-343},
Month = {JUL 2},
Abstract = {Australia's policy on foreign investment aims to achieve both the
   liberal goal of maximising capital inflows and the statist one of
   ensuring that those inflows are in `the national interest'. This article
   analyses the tensions between these goals through interviews with
   policymakers who have direct knowledge of the Foreign Investment Review
   Board (FIRB), which has functioned as an `offstage', pre-market
   regulator for capital inflows to Australia. The tensions between these
   policy goals were manageable because the FIRB exercised its powers
   rarely, decisively and quietly, and the government believed that foreign
   investment did not threaten the national interest. The emergence of
   state-owned enterprises as foreign investors, however, significantly
   altered this calculus, and the FIRB has been tasked with assessing the
   national security implications of proposed investments. Instead of
   working offstage, as in earlier decades, the FIRB has become central to
   debates about how Australia should respond to the rise of China.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
