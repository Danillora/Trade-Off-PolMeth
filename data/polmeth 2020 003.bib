@article{ WOS:000517852800001,
Author = {Vivyan, Nick and Wagner, Markus and Glinitzer, Konstantin and Eberl,
   Jakob-Moritz},
Title = {Do humble beginnings help? How politician class roots shape voter
   evaluations},
Journal = {ELECTORAL STUDIES},
Year = {2020},
Volume = {63},
Month = {FEB},
Abstract = {Motivated partly by descriptive representation concerns, political
   scientists have become increasingly interested in voters' preferences
   over the social class of their representatives. Whereas existing
   research focuses mainly on preferences concerning politicians' own
   immediate class markers, we argue that voters may also care about
   politician class roots - the social class of the household in which a
   politician grew up. Drawing on conjoint experiments fielded in Austria,
   Germany, and Britain, we show that in the latter two cases voters do
   care about class roots, displaying an average preference for politicians
   with more humble class roots. In followup experiments testing different
   explanations for this preference we find little evidence that voters
   treat humble roots as a signal of social mobility and therefore
   politician quality. Rather, preferences over class roots appear to be
   driven by class affinity biases. Our findings have implications for
   debates concerning the descriptive underrepresentation of the working
