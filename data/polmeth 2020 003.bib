@article{ WOS:000556755100002,
Author = {Correa, Diego Sanches and Chaves, Vanilda Souza},
Title = {Gender quotas and placement mandates in open and closed lists: Similar
   effects, different mechanisms},
Journal = {ELECTORAL STUDIES},
Year = {2020},
Volume = {66},
Month = {AUG},
Abstract = {In this paper, we argue that placement mandates, that is, regulations
   for gender ordering in party lists, enhance gender quotas' effectiveness
   in proportional representation electoral systems, independently of list
   type. In closed-list systems, placement mandates increase women's
   representation automatically as a result of the electoral rules, and,
   for this reason, we call this effect mechanical. In open-list systems,
   the effect is also strong due to systematic patterns of electoral
   behavior. We call this effect psychological. In order to demonstrate
   this, we analyze 140 Latin American legislative elections carried out
   between 1990 and 2018. Then, we compare the two most similar cases of
   open-list systems with different rules for placement mandates to
   illustrate the occurrence of the psychological effect. This is the first
   article to argue that placement mandates have similar effects in both
   kinds of lists as a function of distinct mechanisms.},
