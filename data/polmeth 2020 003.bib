@article{ WOS:000522112600003,
Author = {Alexandre-Collier, Agnes},
Title = {From Rebellion to Extinction: Where have all the Tory Remainer MPs Gone?},
Journal = {POLITICAL QUARTERLY},
Year = {2020},
Volume = {91},
Number = {1},
Pages = {24-30},
Month = {JAN},
Abstract = {This article examines the process of extinction of former remainer
   Conservative MPs who followed different paths in the run-up to the 2019
   general election: either choosing to stay loyal and merge with the rest
   of the parliamentary party, or to express their discontent and leave the
   party. When they chose to rebel against the leadership, only a small
   minority dared to vindicate their position. The other rebels constantly
   played down their identity as remainers and justified their views in an
   apologetic way. More remarkably, they were treated differently by Boris
   Johnson, who eventually suspended the last remainers and called for a
   general election to renew the composition of the party. The consequences
   of this last move are crucial in order to understand the radical
   transformation of the Conservative Party into the actual (and only)
   `party of Brexit'.},
