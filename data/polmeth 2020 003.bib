@article{ WOS:000612306300013,
Author = {Modrzejewski, Arkadiusz},
Title = {TRUTH IN THE PANDEMIC AND POST-PANDEMIC REALITY. MORAL AND POLITICAL
   CONSIDERATIONS},
Journal = {EUROPEAN JOURNAL OF TRANSFORMATION STUDIES},
Year = {2020},
Volume = {8},
Number = {1},
Pages = {221-237},
Abstract = {In this essay I examine the importance of truth in society an d public
   life. I adopt the Polish perspective, which, however, can also be
   considered in universal terms. Polish experiences are a kind of
   exemplification here, which I refer to quite briefly so as not to
   obscure the main theme, which is the moral dimension of truth in social
   and public life. The pandemic, in turn, acts as a lens in which the
   phenomena related to the exercise of power by populists and the flood of
   false information focus. Paradoxically, it also creates an opportunity
   to overcome the structures of lies and falsehoods. However, the future
   of truth in the post-pandemic world will not depend on random forces,
   but on a collective effort made to search for it, and on shaping the
   right attitudes and strengthening the institutional order that will
   foster freedom which is a necessary condition for truth.},
