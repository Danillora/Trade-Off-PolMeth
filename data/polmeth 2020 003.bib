@article{ WOS:000584632800001,
Author = {Gil-Barragan, Juan M. and Aguilera-Castillo, Andres and Suarez Galeano,
   Llanet},
Title = {A Bibliometric Analysis of China-Latin America Economic and Political
   Relations},
Journal = {LATIN AMERICAN POLICY},
Year = {2020},
Volume = {11},
Number = {2},
Pages = {290-312},
Month = {DEC},
Abstract = {This article shares the results of a bibliometric analysis of the
   literature on the economic and political relations between China and
   Latin America. This review resulted in the identification of the leading
   research themes, intellectual structure, and main trends in this
   academic field. It also proposes a research agenda for future studies.
   An analysis of 102 documents that included articles, books, and book
   chapters was carried out through a bibliometric analysis and
   bibliographic coupling. The analysis shows that this topic has been
   developed through three stages. The first stage was composed of articles
   that investigated mainly bilateral economic relations. The second
   evaluates a possible negative ``China effect{''} on Latin American
   countries, such as the reprimarization of the Latin American economy,
   the lost market share to China, and the increasing competitive pressures
   on the manufacturing sector. The third stage is an analysis of other
   dimensions, such as the implications of China-Latin America relations as
   part of the evolution of the world order, and Chinese investment as a
   political tool in Latin America. Finally, we found that, despite the
   research progress in these three stages, coverage of the topic is in a
   relatively incipient phase.},
