@article{ WOS:000584343000032,
Author = {Burge, Camille D. and Wamble, Julian J. and Cuomo, Rachel R.},
Title = {A Certain Type of Descriptive Representative? Understanding How the Skin
   Tone and Gender of Candidates Influences Black Politics},
Journal = {JOURNAL OF POLITICS},
Year = {2020},
Volume = {82},
Number = {4},
Pages = {1596-1601},
Month = {OCT 1},
Abstract = {Although sociologists and psychologists have repeatedly demonstrated
   that colorism shapes how Black Americans evaluate members of their
   racial group, the literature on colorism in electoral politics remains
   curiously and ironically bereft of studies of Black Americans. We fill
   this lacuna in our article by asking how and in what ways might the skin
   tone and gender of Black candidates shape Blacks' vote intention along
   with perceptions of representativeness? Using an original survey
   experiment with 1,260 Black Americans (of which we analyze 839) that
   varies the gender and skin tone of political candidates, we find that
   darker-skinned candidates are evaluated more favorably than
   lighter-skinned candidates. Our results advance the research on colorism
   in electoral politics and have sweeping implications for the vast body
   of literature on descriptive representation.},
Publisher = {UNIV CHICAGO PRESS},
Address = {1427 E 60TH ST, CHICAGO, IL 60637-2954 USA},
Type = {Article},
Language = {English},
Affiliation = {Burge, CD (Corresponding Author), Villanova Univ, Dept Polit Sci, Villanova, PA 19085 USA.
   Burge, Camille D., Villanova Univ, Dept Polit Sci, Villanova, PA 19085 USA.
   Wamble, Julian J., George Washington Univ, Dept Polit Sci, Washington, DC 20052 USA.
   Cuomo, Rachel R., Yeshiva Univ, Benjamin N Cardozo Sch Law, New York, NY 10003 USA.},
