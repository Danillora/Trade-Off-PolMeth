@article{ WOS:000540753600006,
Author = {Coticchia, Fabrizio and Vignoli, Valerio},
Title = {Italian Political Parties and Military Operations: An Empirical Analysis
   on Voting Patterns},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2020},
Volume = {55},
Number = {3},
Pages = {456-473},
Month = {JUL},
Abstract = {Since the end of the bipolar era, the military activism of several
   Western powers has raised questions about parliamentary control,
   fostering growing research and analyses on the features, drivers and
   consequences of the different kinds of oversight exercised by
   legislative assemblies. Within this scholarly debate, this article
   focuses on the under-studied case of Italy. How did Italian parties vote
   on military operations abroad in the post-Cold War era? In order to
   answer this question, the article presents the first detailed and
   comprehensive set of data on parliamentary votes over the deployment of
   the Italian armed forces in the post-Cold War era (i.e. from the
   beginning of the 1990s to the recent operation against ISIL). Thanks to
   this extensive new empirical material, the article assesses selected
   arguments developed by the literature on political parties and foreign
   policy, paving the way for further research.},
