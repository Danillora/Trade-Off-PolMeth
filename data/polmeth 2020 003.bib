@article{ WOS:000587488600003,
Author = {Laegreid, Per and Rykkja, Lise H.},
Title = {Nordic Administrative Collaboration: Scope, Predictors and Effects on
   Policy Design and Administrative Reform Measures},
Journal = {POLITICS AND GOVERNANCE},
Year = {2020},
Volume = {8},
Number = {4},
Pages = {21-32},
Abstract = {This article examines whether Nordic administrative collaboration is
   still `alive and kicking,' or whether it has been marginalized by
   increased integration into Europe and strong international reform
   trajectories. We analyse the scope and intensity of Nordic
   administrative collaboration from a structural perspective based on the
   perceptions of civil servants in the Norwegian central government. We
   also address the implications of Nordic collaboration for policy design
   and reform measures. The main conclusion is that Nordic administrative
   collaboration can best be described as differentiated integration. The
   scope of Nordic administrative collaboration is rather broad, and its
   internal structural features vary significantly. Nordic collaboration is
   perceived to have more of an effect on policy design than on specific
   administrative reform means and measures. However, structural features
   also matter.},
