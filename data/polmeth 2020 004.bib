@article{ WOS:000605946600009,
Author = {Damarad, Volha},
Title = {INFORMATION AND CYBER RESILIENCE IN BELARUS},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2020},
Volume = {19},
Number = {3},
Pages = {97-105},
Month = {FAL},
Abstract = {Five years ago, information resilience was named as a core element of
