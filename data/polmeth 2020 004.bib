@article{ WOS:000603597800001,
Author = {Rich, Timothy S. and Eliassen, Isabel},
Title = {Proximity, contact and tolerance of homosexuals in Korea: evidence from
   an experimental survey},
Journal = {ASIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2020},
Volume = {28},
Number = {3},
Pages = {195-211},
Abstract = {Compared to most developed democracies, South Korea lags behind in terms
   of LGBT tolerance. This analysis suggests that relational proximity and
   knowing a Korean LGBT person influence this tolerance. Through an
   experimental web survey design, results show that the closer the
   relational proximity (coworker versus friend versus family member), the
   less one stated their comfort with being around a homosexual. The same
   pattern endures among those who knew a South Korean LGBT person,
   although rates of acceptance were much higher. In addition, this
   analysis finds of South Korea's three main religions, only Protestant
   identification corresponded with a statistically significant decline in
   tolerance.},
