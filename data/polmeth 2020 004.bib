@article{ WOS:000550592700001,
Author = {Williams, Christine B. and Gulati, Jeff and Zeglen, Mateusz},
Title = {Following the money: uses and limitations of FEC campaign finance data},
Journal = {INTEREST GROUPS \& ADVOCACY},
