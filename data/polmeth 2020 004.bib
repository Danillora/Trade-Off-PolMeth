   Mavroudeas, Stavros D., Panteion Univ, Dept Social Policy, Athens, Greece.},
DOI = {10.1080/21598282.2020.1829981},
ISSN = {2159-8282},
EISSN = {2159-8312},
Keywords = {Engels; Marxism; Capital; MEGA},
Research-Areas = {Government \& Law},
