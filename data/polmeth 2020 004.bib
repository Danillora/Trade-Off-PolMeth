@article{ WOS:000534480900013,
Author = {Whiteside, Craig},
Title = {Lying to Win The Islamic State Media Department's Role in Deception
   Efforts},
Journal = {RUSI JOURNAL},
Year = {2020},
Volume = {165},
Number = {1},
Pages = {130-141},
Abstract = {This article analyses the Islamic State media department's involvement
   in deceptive influence efforts. Relying on an extensive database of the
   department's products from its origins to the present (Tawhid wal-Jihad,
   Al-Qa'ida in Iraq, the Mujahidin Shura Council, the Islamic State of
   Iraq and ISIS). Craig Whiteside identifies situations when decisions are
   made to involve the media in deception efforts, and why. He finds that
   the Islamic State used deception infrequently and carefully, usually in
   a defensive manner. while carefully balancing the need for credible and
   truthful information activities outside its deception efforts.},
