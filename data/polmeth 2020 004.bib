@article{ WOS:000625412800005,
Author = {Cooke, Maeve},
Title = {Self-Efficacious Subjects: Rosa on Alienation and its Antithesis},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2020},
Volume = {13},
Number = {3},
Pages = {366-381},
Abstract = {Alienation is a central concept in Rosa's critical social theory.
   Rejecting the idea of freedom as self-determination, he introduces
   instead self-efficacious subjectivity as the antithesis of alienated
   agency. He thereby loses sight of a component historically central for
   theories of alienation: a critically reflective relationship by
   individual subjects to their own life-activity. Without such a
   relationship, I argue, subjects may be entrapped by their particular
   life-orienting commitments and convictions concerning what it means to
   lead ethically good lives. I conclude that Rosa's rejection of the idea
   of self-determining agency impairs the critical scope and vision of his
   critical social theory.},
