@article{ WOS:000528000400001,
Author = {Gade, Emily Kalah},
Title = {Social Isolation and Repertoires of Resistance},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2020},
Volume = {114},
Number = {2},
Pages = {309-325},
Month = {MAY},
Abstract = {Checkpoints in the West Bank's Hebron Governorate represent Israel's
   ever-present power over Palestinian civilians. Drawing on 71 interviews
   conducted during the Intifada of Individuals (2015), this article
   inductively builds theory about the relationship between social
   isolation and different modalities of resistance. Rather than forcing
   civilians to comply with the state, checkpoint apparatus instead change
   the nature and texture of resistance. I suggest that checkpoints
   structure social connections for civilians on the ground. Checkpoint
   apparatus which inhibit social connection engender a feeling of
   hopelessness and foster support for individual, often violent,
   resistance. Where checkpoints isolate a community as a whole but did not
   disrupt within-community social connections, citizens maintain hope for
   the possibility of change, which facilitates a preference for collective
   resistance. This article identifies troubling consequences checkpoints
   have on civilians and highlights how oppressive state power can limit
   some modalities of resistance only to engender support for others.},
