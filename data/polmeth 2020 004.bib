@article{ WOS:000514400000006,
Author = {Balint, Peter},
Title = {Toleration, neutrality, and freedom: a reply},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2020},
Volume = {23},
Number = {2},
Pages = {203-211},
Month = {FEB 23},
Abstract = {In defending toleration against its many critics, Respecting Toleration
   has both conceptual and normative aims. Conceptually, I defend and
   explain the coherence of political toleration. This involves, in part,
   highlighting a distinction between two forms of toleration; one of which
   always involves objection, and one which does not. Normatively, I defend
   a particular understanding of toleration as the best way of
   accommodating contemporary diversity. In brief, the state should be
   guided by an active ideal of neutrality, and citizens must at minimum
   engage in forbearance tolerance with each others' differences. In this
   paper, I respond to four main lines of criticism. The first is that my
   understanding of toleration - in which objection is not always necessary
   - is too broad, and that my non-moralised understanding of forbearance
   tolerance requires additional context. Second, my discussion of
   neutrality runs together the distinction between an active/passive state
   with a large/small state; wrongly fails to distinguish between mere
   preferences and deeply held beliefs; and is really a concern about
   equality. Third, my freedom-based justification for toleration is too
   limited; and may, in fact, enable recognition rather than resist it.
   Fourth, my rejection of inter-citizen respect for difference is too
   quick.},
