@article{ WOS:000550706900004,
Author = {Santana, Andres and Montero, Jose Ramon and Rama, Jose},
Title = {Reassessing Downs: Testing the Influence of Voters' Characteristics in a
   Chilean Presidential Election},
Journal = {JOURNAL OF POLITICS IN LATIN AMERICA},
Year = {2020},
Volume = {12},
Number = {1},
Pages = {77-103},
Month = {APR},
Abstract = {This article puts Down's instrumental voter model (IVM) to a formal test
   using data of the 2017 Chilean national elections. It aims at two novel
   exercises in the research in the calculus of voting. Using a brand-new
   questionnaire with indicators on instrumental and consumption
   motivations for voting, we reassess the voting equation in Santiago de
   Chile. Furthermore, we analyse whether instrumental and consumption
   motivations have distinctive effects for individuals with different
   socio-demographic characteristics. Our results show that they do: women,
   younger, lower educated, and unmarried citizens are more responsive to
   both instrumental and consumption motivations. Moreover, the factors of
   the IVM travel better to Santiago de Chile than those of the consumption
   model.},
Publisher = {SAGE PUBLICATIONS LTD},
