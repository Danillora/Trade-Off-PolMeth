@article{ WOS:000533848200001,
Author = {Welch, Susan},
Title = {Gender and Selection During the Holocaust: Transports of Western
   European Jews to the East},
Journal = {JOURNAL OF GENOCIDE RESEARCH},
Year = {2020},
Volume = {22},
Number = {4},
Pages = {459-478},
Month = {OCT 1},
Abstract = {This paper examines the impact of gender on decisions that military,
   police, and bureaucrats made in selection of western European Jews for
   deportation venues, on initial treatment at Auschwitz and death camps,
   and on survival. Examining data collected and made public by Yad Vashem
   on 1100 transports carrying more than 550,000 Western European Jews to
   the east, I find that women comprised a majority on most transports and
   a disproportionate number of those sent to immediate death at Auschwitz.
   Patterns of gender composition of transports varied over the course of
   the war and from country to country in significant and mostly
   predictable ways. Overall survival rates for men were higher than for
   women based on transports carrying about 210,000 of the 550,000 people.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
