@article{ WOS:000614863100091,
Author = {Giroux, Henry A. and Filippakou, Ourania and Ocampo-Torrejon, Sofia},
Title = {Critical Pedagogy in the Age of Authoritarianism: Challenges and
   Possibilities},
Journal = {IZQUIERDAS},
Year = {2020},
Volume = {49},
Pages = {2083-2102},
Month = {JAN},
Abstract = {The discourse of authoritarianism and the echoes of a fascist past
   resurface. In this context, higher education, beyond favoring practices
   of freedom, has become an instrumentalized institution in order to
   reproduce and legitimize dynamics of domination. This article questions
   this reactionary form of educational and pedagogical action,
   particularly in its neoliberal version. At the same time, it explores
   how education can provide the theoretical and practical foundations to
   rethink its own purpose, together with the very nature of politics. In
   this sense, this article proposes that education and politics are
   completely inseparable dimensions.},
