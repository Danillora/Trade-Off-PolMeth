@article{ WOS:000549314200001,
Author = {Van Ree, Erik},
Title = {Productive forces, the passions and natural philosophy: Karl Marx,
   1841-1846},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2020},
Volume = {25},
Number = {3},
Pages = {274-293},
Month = {SEP 1},
Abstract = {This article explores the emergence of Karl Marx's concept of history
   over the period 1841 to 1846. Whereas in Marx's view the productive
   forces shaped human history, it is argued in this article that Marx
   believed the productive forces in their turn were fuelled by
   psychological drives; in effect, Marx made the passions the deepest
   motive force of history. `Historical materialism' as it crystallized in
   those early years was a theory of materialized subjectivity. Marx's
   comments on various Antique and Modern philosophies of nature evince
   that he discerned important parallels between the developmental
   processes of human history and of nature. If Marx traced the dynamism of
   the productive forces to the human passions, he was adhering to an
   essentially Romanticist ontology of self-creative, impassioned nature.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
