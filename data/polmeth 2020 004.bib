@article{ WOS:000546425900001,
Author = {Chueri, Juliana},
Title = {Social policy outcomes of government participation by radical right
   parties},
Journal = {PARTY POLITICS},
Year = {2021},
Volume = {27},
Number = {6},
Pages = {1092-1104},
Month = {NOV},
Abstract = {The literature has pointed to a change in radical right-wing parties'
   (RRWPs) position regarding the welfare state. Those parties have
   abandoned the neoliberal approach on distributive issues and have become
   defenders of social expenditure fordeservinggroups. Nevertheless, as
   RRWPs have joined with right-wing mainstream parties to form
   governments, their distributive policy position might cause conflict in
   a coalition. This study, therefore, addresses this puzzle by analysing
   the social policy outcomes of RRWPs' government participation. The
   conclusion is that those parties contribute to the welfare state
   retrenchment. However, policies are not affected evenly. Expenditure
   that targets groups regarded asundeservingby the radical right is
   retrenched the most.},
