@article{ WOS:000625411300001,
Author = {Stach, Stephan},
Title = {The Polish-Ukrainian Bulletin in Pilsudski's Poland - or, how to create
   space for dialogue and build trust in an authoritarian state},
Journal = {NATIONAL IDENTITIES},
Year = {2021},
Volume = {23},
Number = {4, SI},
Pages = {369-389},
Month = {AUG 8},
Abstract = {Historical works on Polish-Ukrainian relations in the interwar period
   mostly concern conflict history. The Polish-Ukrainian Bulletin, the
   subject of this article, was published from 1932 with the intention of
   contributing to a peaceful resolution of the conflict. In Poland, under
   the authoritarian regime of Jozef Pilsudski, the journal created a space
   for a relatively free debate on common questions and helped to build
   mutual trust across national divisions. Around the journal, networks of
   Polish and Ukrainian political and social activists emerged. These
   networks played a crucial role in the conclusion of the Polish-Ukrainian
   Normalization Agreement of 1935.},
