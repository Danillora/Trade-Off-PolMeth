@article{ WOS:000534139700001,
Author = {Rodon, Toni},
Title = {The Spanish electoral cycle of 2019: a tale of two countries},
Journal = {WEST EUROPEAN POLITICS},
Year = {2020},
Volume = {43},
Number = {7},
Pages = {1490-1512},
Month = {NOV 9},
Abstract = {In 2019 alone, Spanish citizens went to the polls at least four times -
   two general elections, European elections, local elections and, for
   some, regional elections. Moreover, in the 2016-19 legislature, the
   country witnessed a successful vote of no confidence that replaced a
   Conservative prime minister with a Socialist one; experienced an
   important constitutional crisis over the 2017 referendum on Catalan
   independence; observed the emergence for the first time of a viable
   far-right party; and ended with the first coalition government in the
   modern democratic history. The November 2019 election, the last in this
   long electoral cycle, left a fragmented and polarized political
   landscape and a left-wing cabinet - PSOE and Podemos - that does not
   have a majority in the chamber. This article presents the background,
   the results of the different elections and discusses how and why Spanish
   politics experienced a radical transformation likely to have an impact
