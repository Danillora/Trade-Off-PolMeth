@article{ WOS:000572132600028,
Author = {Vinichenko, Mikhail V. and Rybakova, V, Marina and Nikiporets-Takigawa,
   Galina Y. and Chulanova, Oxana L. and Ljapunova, V, Natalia},
Title = {The Influence of Artificial Intelligence on the Human Potential
   Development: The Views of Orthodox Clergy and Parishioners},
Journal = {CUESTIONES POLITICAS},
