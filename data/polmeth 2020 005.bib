@article{ WOS:000596450900004,
Author = {Patrick, David},
Title = {WHO WRITES THE NEWS? AUTHORSHIP AND GENDER REPRESENTATION IN PRESS
   COVERAGE OF THE 2014 REFERENDUM ON SCOTTISH INDEPENDENCE},
Journal = {SCOTTISH AFFAIRS},
Year = {2020},
Volume = {29},
Number = {1},
Pages = {50-81},
Month = {FEB},
Abstract = {This paper engages with newspaper coverage of the 2014 Scottish
   independence referendum, with the primary focus being authorship of news
   and comment articles. Taking a predominantly quantitative approach,
   weekday coverage during August and September 2014 from eight major
   newspapers in Scotland was statistically analysed. The findings reveal
   several notable observations. First, it is observed that a large
   proportion of articles are published without an attributable author.
   Second, the findings reveal that both news and comment outputs in 2014
   were dominated by a comparatively small number of individual
   journalists. Whilst the journalistic pool from this sample featured
   several hundred distinct journalists, more than half of all coverage was
   concentrated within the hands of only a few dozen people. Third, and
   most developed in this paper, is the consistent trend - across several
   variables - that women were marginalised within the news production
   process surrounding the 2014 referendum. With only a few exceptions,
   both news reporting and comment (including opinion, diaries and
   features) were dominated by male reporters; meaning that during a hugely
   important period of Scotland's political history, female voices and
   perspectives were regularly missing from the press coverage of what was
   seen by many as an unprecedented national conversation.},
Publisher = {EDINBURGH UNIV PRESS},
Address = {THE TUN-HOLYROOD RD, 12 2F JACKSONS ENTRY, EDINBURGH EH8 8PJ, SCOTLAND},
Type = {Article},
Language = {English},
Affiliation = {Patrick, D (Corresponding Author), Univ Free State, Int Studies Grp, Bloemfontein, South Africa.
   Patrick, David, Univ Free State, Int Studies Grp, Bloemfontein, South Africa.},
DOI = {10.3366/scot.2020.0306},
