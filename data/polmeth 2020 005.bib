@article{ WOS:000524446000006,
Author = {Sari, Bugra},
Title = {International Organization Status of the International Criminal Police
   Organization},
Journal = {SIYASAL-JOURNAL OF POLITICAL SCIENCES},
Year = {2020},
Volume = {29},
Number = {1},
Pages = {89-112},
Month = {MAR},
Abstract = {The International Criminal Police Organization (INTERPOL) functions as
   an international organization, and is recognized in this regard by the
   United Nations. However, the status of INTERPOL as an international
   organization, and thereby its international legal personality has been a
   point of controversy in the literature. In this respect, some
   international lawyers argue that since INTERPOL was not established by a
   formal international treaty, it cannot be classified as an international
   organization with international legal personality. Problematizing this
   argument, this study questions whether INTERPOL is an international
   organization. At the end of the discussion, the study reaches the
   conclusion, by demonstrating its relevant international legal bases,
   that INTERPOL is an international organization with international legal
   personality. Accordingly, the 1956 INTERPOL Constitution constitutes an
   international treaty in simplified form. Moreover, the Organization has
   autonomous organs with distinct will and international legal
   personality.},
