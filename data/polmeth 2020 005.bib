@article{ WOS:000570889400001,
Author = {Stinchfield, Bryan T. and Auch, Ted and Bratman, Eve},
Title = {Energy Security, International Investment, and Democracy: The Case of
   the United States Shale Oil and Gas Industry},
Journal = {DEMOCRACY \& SECURITY},
Year = {2020},
Volume = {16},
Number = {4},
Pages = {309-333},
Month = {OCT 1},
Abstract = {Proponents of the US shale oil and gas industry argued that American
   citizens' economic prosperity and national security were at stake if the
   industry was not rapidly expanded. Following copious amounts of a
   certain type of ``patriotic{''} rhetoric, the industry grew rapidly.
   Simultaneously, foreign ownership of US shale industry infrastructure
   occurred in tandem with calls for new policies and laws to limit US
   citizens' democratic rights with regard to the industry's activities. As
   a result, we argue that the development of the US shale industry has
   weakened national security by creating negative security externalities
   and eroding democratic values. We offer implications for other
   democratic societies rich in natural resources.},
