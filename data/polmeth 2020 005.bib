@article{ WOS:000524734500008,
Author = {Vovk, Natalia and Peleshchyshyn, Andriy and Markovets, Oleksandr},
Title = {REPRESENTATIONS OF THE PUBLIC ATTITUDES TOWARDS UKRAINIAN-POLISH
   RELATIONS IN WEB COMMUNITIES},
