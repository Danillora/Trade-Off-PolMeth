@article{ WOS:000756022800027,
Author = {Andres Urcola, Marcos and Elena Nogueira, Maria},
Title = {Food Production in Pandemic Times. The Essential Role of Family Farming},
Journal = {TEMAS Y DEBATES},
Year = {2020},
Number = {40, S},
Pages = {267-275},
Month = {DEC},
Abstract = {Certainly, a pandemic is an extraordinary event that disrupts daily life
   and enables one to reflect on an alleged normality along different
   dimensions. One of these dimensions, amongst many others, is that of
   food production and the range of subjects it entails. In Argentina, food
   production is closely linked with the agribusiness model, the
   deterioration of the environment, monopolies of some firms and chains,
   as well as price manipulation and inflation. Yet not all food production
   is represented under these lines or traits. In this paper it is our
   interest to critically reflect on the essential role of the subjects of
   family farming as food producers. In order to organize all
   considerations, a series of axes associated with the supply and
   consumption of food and with the problems and opportunities faced by
   family farmers within the framework of the current agri-food model are
   proposed.},
Publisher = {UNIV NAC ROSARIO, FAC CIENCIAS POLITICAS \& RELACIONES INT},
Address = {RIOMBA \& BERUTTI C U R, ROSARIO, 2000, ARGENTINA},
Type = {Article},
