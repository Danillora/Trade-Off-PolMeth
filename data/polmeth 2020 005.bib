   Okolikj, Martin, Katholieke Univ Leuven, Ctr Polit Res, Leuven, Belgium.
   Hooghe, Marc, Katholieke Univ Leuven, Leuven, Belgium.},
DOI = {10.1177/0192512120915907},
EarlyAccessDate = {MAY 2020},
Article-Number = {0192512120915907},
ISSN = {0192-5121},
EISSN = {1460-373X},
Keywords = {Economic voting theory; economic perceptions; partisanship; endogeneity;
