@article{ WOS:000547677700001,
Author = {Cho, Richard},
Title = {The effect of minority identity convergence on pan-ethnicity among Asian
   Americans},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2020},
Volume = {8},
Number = {3},
Pages = {453-470},
Month = {MAY 26},
Abstract = {Using data from a large-N survey experiment of Asian American
   respondents, the evidence is provided that Asian Americans define a
   pan-ethnic identity largely based on perceptions of spatial proximity
   with other racial groups on a broader social hierarchy in the United
   States. This racial triangulation, which has previously been attributed
   to explaining hostility between Asians and other racial minority groups,
   serves here instead as a context through which Asian Americans define a
   pan-ethnic identity via a shared commonality with other racial minority
   groups. Moreover, this racialized construction of pan-ethnicity is
   influenced by contextual derogatory cues against this commonality
   frequently included in recent political debates over the issue of
   immigration reform.},
