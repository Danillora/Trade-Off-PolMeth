@article{ WOS:000609093200001,
Author = {Sumonja, Milos},
Title = {Neoliberalism is not dead - On political implications of Covid-19},
Journal = {CAPITAL AND CLASS},
Year = {2021},
Volume = {45},
Number = {2},
Pages = {215-227},
Month = {JUN},
Abstract = {The news is old - neoliberalism is dead for good, but this time, even
   Financial Times knows it. Obituaries claim that it had died from the
   coronavirus, as the state, not the markets, have had to save both the
   people and the economy. The argument of the article is that these
   academic and media interpretations of `emergency Keynesianism'
   misidentify neoliberalism with its anti-statist rhetoric. For
   neoliberalism is, and has always been, about `the free market and the
   strong state'. In fact, rather than waning in the face of the
   coronavirus crisis, neoliberal states around the world are using the
   ongoing `war against the virus' to strengthen their right-hand grip on
   the conditions of the working classes.},
