@article{ WOS:000529442700003,
Author = {Sjoblom, Stefan},
Title = {Finnish regional governance structures in flux: Reform processes between
   European and domestic influences},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2020},
Volume = {30},
Number = {2, SI},
Pages = {155-174},
Month = {MAR 14},
Abstract = {The two main features of the Finnish two-tier system are, on the one
   hand, the strength of the traditional national ministerial structure
   and, on the other hand, an extensive local self-government. Due to
   severe structural and socio-economic challenges, Finnish subnational
   governance has been in constant flux and subject to successive reform
   processes since the accession of Finland to the European Union in 1995.
   This article addresses the effects of these reform processes on regional
   capacity building and specifies the mediating domestic factors, which
   account for an indirect EU impact on the rescaling of state authority.
   The impact of the EU is, according to the results, strongly mediated by
   domestic factors, particularly by an interaction between contextual and
   structural factors as well as path dependent developments. The
   conclusion is that subnational developments since the early 1990s cannot
   be interpreted as a strengthening of regions at the expense of the
   state.},
