@article{ WOS:000603655700006,
Author = {Ferraresi, Furio},
Title = {Max Weber: The Reason of a Dialogue},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
Year = {2020},
Volume = {32},
Number = {63},
Pages = {87-105},
Abstract = {The essay identifies and analyses the main similarities between Weber's
   and Foucault's work. They concern the three main areas of their
   reflection: knowledge, power and the subject. These thematic axes are
   always firmly intertwined in their work. With regard to knowledge, both
   authors practice a form of historical knowledge in which both the
   ``subject{''} of knowledge and the ``object{''} known are radically
   called into question. Regarding power, they abandon the model of the
   state and sovereignty in order to analyse the relations of power and
   domination that run through the entire social body and that ``govern{''}
   the conduct of individuals. As for the subject, both elaborate ethical
   forms of subjectivity and ``care of the self' that take on an ascetic
   significance. For Weber, however, it is a question of making objectivity
   itself a ``vocation{''} for the subject, while Foucault proposes a
   resistance and a struggle for a new subjectivity.},
