@article{ WOS:000515410500001,
Author = {Abou-Khalil, Walid J. and Aoun, Georges M.},
Title = {Does the Political Candidate's Quality Lead to Satisfaction?
   Transposition of the SERVQUAL Model to Politics (VOTQUAL): Case of the
   Loyal Lebanese Voter},
Journal = {JOURNAL OF POLITICAL MARKETING},
Year = {2020},
Month = {2020 FEB 14},
Abstract = {Like consumer satisfaction, satisfaction in politics usually stems from
   a comparison of perceived performance with expectations of performance.
   The SERVQUAL model compares expectations for a service to perceptions of
   the service. The current study adapts the model to political marketing.
   Focus groups in Lebanon transposed questions from the SERVQUAL to ones
   appropriate to political satisfaction. The resulting VOTQUAL instrument
   in English, French, and Arabic assesses candidate dimensions of:
   physical characteristics (perceptions only), trustworthiness,
   helpfulness, competence, and empathy (performance minus expectations).
   In a survey of 150 Lebanese voters, overall satisfaction with a
   preferred candidate was entirely a function of low expectations. Voter
   characteristics made little difference to either satisfaction or
   candidate ratings. The results indicate that the VOTQUAL may be a useful
   instrument in political marketing research.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
