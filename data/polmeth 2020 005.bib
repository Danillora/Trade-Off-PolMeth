@article{ WOS:000614863100002,
Author = {Deves, Eduardo},
Title = {The liberationist thinking in Oceania: Cartography of circulacion of
   Latin American ideas in South Pacific},
Journal = {IZQUIERDAS},
Year = {2020},
Volume = {49},
Pages = {209-221},
Month = {JAN},
Abstract = {The research consists of detecting the presence from the 1970s until the
   beginning of the 21st century, of two expressions of South American
   liberationist thinking (Paulo Freire's pedagogy and theology of
   liberation) in Oceania. Intellectual reception ecosystems and processes
   of circulation and re-elaboration are indicated in 4 intellectual
   spaces: higher education institutions (universities and religious
   seminaries), liberation movements, adult literacy, and indigenous
   peoples' organizations. Cases are detected in states and territories
   such as Australia, Fiji, New Caledonia, New Zealand, Papua New Guinea,
   Timor East and Vanuatu. Finally, this research concludes with the
   characterization of the ways in which the South American ideas were
   received and re-elaborated and the channels through which they arrived
   in the region and through which they circulated within it.},
