@article{ WOS:000614864700010,
Author = {Meza Bazan, Mario},
Title = {The New Left and the competition for the revolution in Peru during the
   Velasco government. 1968 - 1975},
Journal = {IZQUIERDAS},
Year = {2020},
Volume = {49},
Pages = {3168-3204},
Month = {JAN},
Abstract = {The reforms implemented in Peru by the military regime of General Juan
   Velasco Alvarado between 1968 and 1975 provoked intense competition and
   rivalry between his government and the New Left parties for imposing a
   new order. The article explains in this tense and polarized scenario
   between military and parties such as the MIR and VR, the factors that
   limited the formation of a solidly organized opposition to the military
   regime and its reforms and, in the case of the military, analyzes their
   fear and distrust towards these New Left parties.},
