@article{ WOS:000599199500003,
Author = {Terreaux-Scotto, Cecile},
Title = {Savonarola: the words and gestures of a preacher},
Journal = {LABORATOIRE ITALIEN-POLITIQUE ET SOCIETE},
Year = {2020},
Number = {25},
Abstract = {Dominican preacher Savonarola said: ``il mio dire {[}e] fare{''}
   ({''}when I speak, I do something{''}). Scholars have usually preferred
   to analyse the meaning of ``fare{''} rather than of ``dire{''}. We know
   why, when, where, to whom Savonarola preached. But the question of
   ``how{''} he spoke is still largely unexplored. There is indeed no work
   on the rhetorical action of Savonarola. Exploring this question, which
   has hardly ever been examined, except in a very marginal and secondary
   way, is nevertheless paradoxical, since the preacher insists that he
   does not want to use rhetoric. He states that he prefers the truth, the
   simplicity, and the immediacy of the Holy Scripture. However, despite
   this posture - purely theoretical - Savonarola's oratorical performance
   in the pulpit was particularly effective since, after discouraging
   audiences at first, he attracted impressive crowds starting in 1494.
   Although a preacher's voice and gestures are inherently elusive because
   of the sermon's essentially oral nature, it is possible to identify
   features of Savonarola's non-verbal language in his written and spoken
   texts, as well as in written and iconographic testimonies of his
   contemporaries.},
