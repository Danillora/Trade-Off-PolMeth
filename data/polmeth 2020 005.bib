@article{ WOS:000515267600001,
Author = {Dean, Jodi},
Title = {Communism or Neo-Feudalism?},
Journal = {NEW POLITICAL SCIENCE},
Year = {2020},
Volume = {42},
Number = {1},
Pages = {1-17},
Month = {JAN 2},
Abstract = {This lecture looks at the neo-feudalizing tendencies in contemporary
   capitalism. Expropriation, domination, and force have intensified to
   such an extent that it no longer makes sense to posit free and equal
   actors meeting in the labor market even as a governing fiction. Rent and
   debt feature as or more heavily in accumulation than profit, and work
   increasingly exceeds the wage relation. The alternative to neo-feudalism
   is communism.},
