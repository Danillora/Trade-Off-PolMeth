@article{ WOS:000532755700004,
Author = {Aldegani, Emiliano and Alejandro Murad, Omar},
Title = {Perspectives of the Apocalyptic. Figures of Devastation and
   Anthropological Degradation in Contemporary Cinema.},
Journal = {ANACRONISMO E IRRUPCION},
Year = {2020},
Volume = {10},
Number = {18},
Pages = {89-117},
Month = {MAY-OCT},
Abstract = {In this paper we propose to analyze some recurrent post-apocalyptic
   scenarios in contemporary myths according to three types of apocalypse:
   extinction, refoundation and exhaustion. These three ways of conceiving
   the apocalypse do not exhaust the possible scenarios, nor are they
   always easily discernible, but they can be useful for addressing
   contemporary narratives and their different types of time horizon. Our
   goal is to relate the post-apocalyptic imagination with certain forms of
   violence that are both foundational and destructive of the social. For
   this, we will use the theory of violence by Rene Girard, the figures of
   the plague and the contagion that it implies, and its main topics: the
   mimetic desire and the scapegoat. We maintain that each of the figures
   that inhabit these post-apocalyptic scenarios can be considered as a
   double or substitute for what we consider normal or civilized. To
   develop our perspective, the analysis will focus on the figures of the
   zombie, the body thief and the vampire and on what in each case can be
   considered their relations with the past and the future.},
Publisher = {UNIV BUENOS AIRES},
Address = {UNIV BUENOS AIRES, BUENOS AIRES, 00000, ARGENTINA},
Type = {Article},
Language = {Spanish},
Affiliation = {Aldegani, E (Corresponding Author), Univ Nacl Mar del Plata, Univ Nacl La Plata, CONICET, Mar Del Plata, Buenos Aires, Argentina.
   Aldegani, Emiliano, Univ Nacl Mar del Plata, Univ Nacl La Plata, CONICET, Mar Del Plata, Buenos Aires, Argentina.
   Alejandro Murad, Omar, Univ Nacl Mar del Plata, CONICET, Mar Del Plata, Buenos Aires, Argentina.},
ISSN = {2250-4982},
Keywords = {Apocalypse; Myths; Imagination; Cinema; Figures},
