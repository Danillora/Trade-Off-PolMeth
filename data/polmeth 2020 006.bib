@article{ WOS:000567825800003,
Author = {Daoust, Jean-Francois and Bol, Damien},
Title = {Polarization, Partisan Preferences and Strategic Voting},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2020},
Volume = {55},
Number = {4},
Pages = {578-594},
Month = {OCT},
Abstract = {In this article, we study how polarization affects the propensity of
   supporters of non-viable parties to cast a strategic vote. To do so, we
   rely on Canadian election panel surveys from the Making Electoral
   Democracy Work project that were specifically designed to identify
   strategic voting. We find that the polarization between viable parties
   increases the probability of a supporter of a non-viable party casting a
   strategic vote, because it increases how much she likes her favourite
   viable party, and decreases how much she dislikes her least favourite
   viable party. Polarization thus increases strategic voting because it
   alters partisan preferences.},
