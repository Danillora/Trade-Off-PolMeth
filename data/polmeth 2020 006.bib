@article{ WOS:000584343000006,
Author = {Schiumerini, Luis and Steinberg, David A.},
Title = {The Black Market Blues: The Political Costs of Illicit Currency Markets},
Journal = {JOURNAL OF POLITICS},
Year = {2020},
Volume = {82},
Number = {4},
Pages = {1217-1230},
Month = {OCT 1},
Abstract = {Countries with high inflation and stringent currency regulations
   frequently have thriving black markets for dollars. We argue that the
   black market for dollars has important political consequences: it hurts
   the pocketbooks of many citizens and, therefore, reduces support for the
   government. Evidence from Argentina supports this argument. Using
   aggregate time series data, we estimate that a 10\% increase in the
   black market price of the dollar relative to the official price reduces
   presidential approval by 2-5 percentage points in the long run. To test
   the pocketbook mechanism more directly, we use individual-level data
   from a list experiment. We find that people who bought dollars on the
   black market were 10 percentage points less likely to vote for the
   incumbent party in the 2015 presidential election. These findings
   suggest that the black market for dollars can have important political
   consequences.},
Publisher = {UNIV CHICAGO PRESS},
