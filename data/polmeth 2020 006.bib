@article{ WOS:000597360800001,
Author = {Jump, Robert Calvert and Michell, Jo},
Title = {Education and the geography of Brexit},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2023},
Volume = {33},
Number = {1},
Pages = {41-53},
Month = {JAN 2},
Abstract = {While it is well established that educational attainment is highly
   correlated with Brexit voting patterns, the predictive capacity of
   education has attracted less attention. Using full-sample and
   split-sample exercises, this paper demonstrates that educational
   attainment alone can correctly classify over 90\% of local authorities
   by voting outcome in the 2016 referendum to leave the EU, depending on
   the prediction model and classification method used. This illustrates
   the importance of education as a key factor in the geography of Brexit.},
