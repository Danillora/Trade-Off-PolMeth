@article{ WOS:000625412800007,
Author = {Rosa, Hartmut},
Title = {Beethoven, the Sailor, the Boy and the Nazi. A reply to my critics},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2020},
Volume = {13},
Number = {3},
Pages = {397-414},
Abstract = {The contribution clarifies the concept of resonance as a mode of
   relationship consisting of four elements: 1) affection, 2)
   self-efficacy, 3) transformation and 4) essential uncontrollability.
   Resonance is not based on harmony, fusion or unity, but on a
   transformative encounter requiring difference. Subsequently, the paper
   demonstrates that experiences of resonance have the power to break with
   given institutional or interpretive frames. It is precisely experiences
   of resonance which allow for individual, social and moral change.
   Furthermore, the paper discusses the relationship between strong
   evaluations, reflection and resonance and sketches the contours of an
   ethics of resonance.},
