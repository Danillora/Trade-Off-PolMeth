@article{ WOS:000571802000001,
Author = {Haass, Felix},
Title = {Insurgency and Ivory: The Territorial Origins of Illicit Resource
   Extraction in Civil Conflicts},
Journal = {COMPARATIVE POLITICAL STUDIES},
