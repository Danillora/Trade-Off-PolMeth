@article{ WOS:000568821400009,
Author = {Rozovsky, Liza},
Title = {Countering the sin of defencelessness Friedrich Gorenstein and
   (post-)Soviet Jewry},
Journal = {OSTEUROPA},
Year = {2020},
Volume = {70},
Number = {6},
Pages = {191+},
Abstract = {The emigres from the former Soviet Union who came to Israel in their
   hundreds of thousands in the early 1990s still remain a separate group
   from the cultural and political mainstream there today. In order to
   understand what characterised and motivated them, it is worthwhile
   taking the work of the Russian Jewish writer Friedrich Gorenstein into
   consideration, which can be read as a type of encyclopaedia of Soviet
   Jewry. His texts draw on both biblical sources and his own biography.
   The lesson that Gorenstein draws - that for the Jewish people,
   defencelessness is the greatest sin - is shared by many (post-)Soviet
   Jews, and not only in Israel.},
Publisher = {BWV-BERLINER WISSENSCHAFTS-VERLAG GMBH},
Address = { BEHAIMSTR 25-GIERKEZEILE 37, BERLIN, GERMANY},
Type = {Article},
Language = {German},
Affiliation = {Rozovsky, L (Corresponding Author), Tageszeitung Haaretz, Dresden, Germany.
