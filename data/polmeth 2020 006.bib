@article{ WOS:000603921000001,
Author = {McCrary, Lorraine Krall},
Title = {The politics of community: care and agency in people with intellectual
   disabilities at L'Arche},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2021},
Volume = {9},
Number = {2},
Pages = {409-422},
Month = {MAR 15},
Abstract = {Theorists of disability have long noted the lack of inclusion of people
   with disabilities in social and political life due to architectural,
   transportation, educational, and legal barriers, to name a few, and due
   to a lack of needed supports that facilitate inclusion. Examining the
   practices of a L'Arche community allows us to theorize another element
   of social and political inclusion: communities of care that support the
   development of agency. L'Arche a Trosly is a French community made up of
