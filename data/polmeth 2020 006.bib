@article{ WOS:000609249700006,
Author = {Gomez, Elias},
Title = {Max Weber and Norbert Elias: Power, Domination and Configurations},
Journal = {TEMAS Y DEBATES},
Year = {2020},
Number = {40},
Pages = {145-158},
Month = {JUL-DEC},
Abstract = {The works of Max Weber and Norbert Elias begun to be critically
   rediscovered in Latin America since few decades ago. Both authors are
   central to processual and relational approaches, although Norbert Elias
   never deepened his debts and differences with Max Weber. In this
   context, this article concretely analyzes Max Weber's concepts of power
   and legal domination, as well as the concepts of power and social
   configuration of Norbert Elias. The coincidences between both authors
   with respect to the centrality of the monopolies, and the moral
   irrationality of the world are especially emphasized. Towards the end,
   the main contributions of these coincidences are considered for a
   processual approach to social inequalities.},
