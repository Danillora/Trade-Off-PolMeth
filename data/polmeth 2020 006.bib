@article{ WOS:000514400000004,
Author = {Horton, John},
Title = {Conceptualising toleration},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2020},
Volume = {23},
Number = {2},
Pages = {191-196},
Month = {FEB 23},
Abstract = {This discussion focuses primarily on conceptual issues and on three
   specific features of Peter Balint's argument in particular. First, I
   seek to assess his boldest conceptual innovation, the abandonment of
   what is known as `the objection component' as an essential component of
   toleration within his broader understanding of the concept. Instead, I
   suggest an intermediate position between the `orthodox' conception and
   his view. Secondly, I raise some mildly sceptical questions about how
   far we can make sense of a purely `descriptive' sense of toleration.
   Finally, I offer a modest clarification of the conceptual dimension of
   the argument that Balint advances against those who deny that the
   liberal state can be both neutral and tolerant, showing that the
