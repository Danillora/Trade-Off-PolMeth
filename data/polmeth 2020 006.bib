@article{ WOS:000523590900016,
Author = {Garrote De Marcos, Maria},
Title = {Low magnitude constituencies and proportionality in Europe: a selective
   panoramic view},
Journal = {TEORIA Y REALIDAD CONSTITUCIONAL},
Year = {2020},
Volume = {45},
Pages = {451-478},
Abstract = {This paper describes the electoral systems of Ireland, Switzerland and
   Portugal. These three countries, like Spain, share the use of low
   magnitude constituencies within the framework of a proportional
   electoral system. However, each uses a different ballot structure, a
   circumstance that helps to understand the relevance of this element in
   the functioning of electoral systems. The analysis of the legal
   framework, its evolution and political performance offers us valuable
   information on different electoral models which can serve as a reference
   to assess some problematic aspects of the Spanish electoral system.},
