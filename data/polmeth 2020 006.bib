@article{ WOS:000524730400001,
Author = {Qureshi, Asim},
Title = {Experiencing the war ``of{''} terror: a call to the critical terrorism
   studies community},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2020},
Volume = {13},
