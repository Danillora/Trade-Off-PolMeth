@article{ WOS:000591693700001,
Author = {Newman, Brian and Otto, Amber},
Title = {Polls and Elections: The Economy and Events Still Matter (At
   Least a Little): Partisans' Presidential Approval in the Trump Era},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2022},
Volume = {52},
Number = {1},
Pages = {208-223},
Month = {MAR},
Abstract = {Donald Trump's historically stable and polarized approval ratings raise
   concerns that partisans' evaluations of Trump are detached from
   real-world conditions like the state of the economy and Trump's actions.
   We show that Republicans' (but not Democrats') approval and strength of
   (dis)approval tracked with stock market performance and perceptions of
   the economy. In addition, both Democrats and Republicans rewarded and
   punished Trump after major events. In some instances, Republicans and
   Democrats responded to events in similar ways, but in others, events
   further polarized partisans' views of Trump. At least to some degree,
   Republican approval responds to the economy and major events.},
