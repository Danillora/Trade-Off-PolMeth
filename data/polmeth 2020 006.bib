@article{ WOS:000507996700010,
Author = {Coman, Emanuel Emil},
Title = {Local leaders and the distribution of central government funds: Evidence
   from 18 European democracies},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
Year = {2020},
Volume = {59},
Number = {1},
Pages = {206-225},
Month = {FEB},
Abstract = {This article looks at the relationship between the political affiliation
   of local leaders and the distribution of government funds with the help
   of a new dataset on local elections from 18 European countries between
   2000 and 2013. It finds that central governments are more likely to
   target regions with high density of local councils affiliated with the
   parties in government only under certain institutional arrangements. The
   relationship exists where local councils enjoy little power and thus are
   less able to claim credit for the funds independent of the central
   government. The relationship is also present where local leaders are
   involved in the selection of candidates for national office.},
