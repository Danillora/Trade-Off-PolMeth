@article{ WOS:000614931400017,
Author = {Sparaga, Ol'ga},
Title = {Vertical or horizontal On two conflicting social orders},
Journal = {OSTEUROPA},
Year = {2020},
Volume = {70},
Number = {10-11},
Pages = {255+},
Abstract = {The philosopher Olga Shparaga has experienced the Belarusian remand
   prisons from the inside. She talks about the dirt and the cold, about
   the interrogations and the fear of the prison guards. In order to avoid
   the threat of legal proceedings due to the alleged organisation of mass
   unrest, she fled to Lithuania, where she became responsible for
   educational issues at the Coordination Council for the Transfer of
   Power. Despite the enormous wave of repressive measures to which the
   regime is subjecting society, she remains optimistic: to date, no-one
   has succeeded in attempting to stop the progress of time.},
