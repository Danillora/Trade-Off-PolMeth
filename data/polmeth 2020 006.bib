@article{ WOS:000572132600012,
Author = {Kovalova, V, Olena and Korniienko, V, Maksym and Pavliutin, V, Yurii},
Title = {The Role of Public Organizations in Ensuring National Security of
   Ukraine},
Journal = {CUESTIONES POLITICAS},
