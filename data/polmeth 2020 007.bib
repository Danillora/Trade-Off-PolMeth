@article{ WOS:000607562900008,
Author = {Batista Neto, Dilson Cavalcanti and Remedio, Jose Antonio},
Title = {Religion without God: Dworkin's approach of religious freedom},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2020},
Volume = {121},
Pages = {251-288},
Month = {JUL-DEC},
Abstract = {The research aims to critically analyze the conception of religious
   freedom from Ronald Dworkin, focused on his work called Religion Without
   God. Dworkin treats religious freedom as a right to ethical
   independence, which includes atheists and agnostics. Despite the merits
   of his proposal, the author's view is insufficient to explain and safely
   justify the issue of religious freedom in the context of a democracy.
   The research is divided into four topics: begins with the presentation
   of the core elements of Dworkin's proposal on religious freedom;
   subsequently, it indicates the virtues of its comprehensive and
   inclusivist approach; in the third topic, proposes a caveat pointing out
   possible inconsistencies of the Dworkin's conception; in the last topic,
   contrasts Religion Without God with an approach by John Rawls. The
   method used is the deductive, based on bibliographical, doctrinal and
   legislative research. It concludes that Ronald Dworkin's conception of
   religious freedom, while comprehensive, including its atheists and
   agnostics, is insufficient for the proper protection of all religious
   expressions within a democracy.},
