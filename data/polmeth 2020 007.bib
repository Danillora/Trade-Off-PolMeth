@article{ WOS:000629147400004,
Author = {Cruz, Pablo and Mendez, Benjamin and Culebro Moreno, Jorge E.},
Title = {Leadership and crisis management in Mexico: an area of improvised
   practices in sub-national governments},
Journal = {REVISTA MEXICANA DE ANALISIS POLITICO Y ADMINISTRACION PUBLICA},
Year = {2020},
Volume = {9},
Number = {2},
Pages = {32-46},
Month = {JUL-DEC},
Abstract = {The article examines some of the actions implemented during the crisis
   caused by COVID-19, in three sub-national governments: Mexico City, the
   State of Mexico and Jalisco, which are considered as examples to study
   capacities leadership in crisis management. For this, in the first
   instance a theoretical review is carried out on the relevant aspects of
   the crisis, which will help us with the proposed analysis. Next, the
   actions implemented by the sub-national governments are exposed, with
   emphasis on two of the strategic tasks of crisis management, critical
   decision making and generating meaning. Finally, in order to elaborate a
   discussion on the data presented, the final section points out the
   importance of developing capacities for crisis management at both the
   sub-national and national levels and thus moving away from improvised
   practices.},
