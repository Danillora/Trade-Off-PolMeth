@article{ WOS:000584498100001,
Author = {Ibonye, Vincent},
Title = {The Forum on China-Africa Cooperation (FOCAC) and knowledge transfer in
   Sino-Nigerian development cooperation},
Journal = {ASIAN JOURNAL OF COMPARATIVE POLITICS},
