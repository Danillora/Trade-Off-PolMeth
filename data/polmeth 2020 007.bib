@article{ WOS:000514226300004,
Author = {Carrieri, Luca},
Title = {The limited politicization of European integration in Italy: lacking
   issue clarity and weak voter responses},
Journal = {ITALIAN POLITICAL SCIENCE REVIEW-RIVISTA ITALIANA DI SCIENZA POLITICA},
Year = {2020},
Volume = {50},
Number = {1},
Pages = {52-69},
Month = {MAR},
Abstract = {This article analyzes the politicization of European integration in
   Italy. Has the euro crisis catalyzed a new electoral supply, which may
   have been matched by voters' responses? After the crisis, Italian
   parties have increased their entrepreneurial efforts to prime EU issues
   into the political debate, trying to win over votes. This pattern may
   have led to a full politicization of European integration. To study the
   transformations in party strategies and voting behavior, I analyze the
   fluctuations in EU issue entrepreneurship and EU issue voting between
   2009 and 2014. The findings reveal mixed results: parties have actually
   developed new strategies on EU issues, but without significantly
   changing voter preferences along the pro/anti-European dimension.},
