@article{ WOS:000507895200001,
Author = {Axe-Browne, Abigail and Hansen, Martin Ejnar},
Title = {Still dividing the electorate? Brexit and voter evaluation of candidates},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2021},
Volume = {31},
Number = {2},
Pages = {220-242},
Month = {APR 3},
Abstract = {Voters use various cues to make their decision. We examine the impact of
   the cue of candidate position on Brexit. Brexit is still on the agenda
   and we explore how this topic continues to divide the electorate. We
   test this by assessing the general preference of candidates among
   respondents in a series of surveys providing variation in the
   information regarding candidates' positions on Brexit and party
   identification. In contrast to existing literature, we find strong
   evidence that candidates can utilise either their own previous voting
   behaviour or that of their opponent to provide an electoral advantage.
   Leave supporting candidates are highly rated by leave voters and poorly
   rated by remain voters, and vice versa for remain candidates, although
   it is dependent on the party label of the candidate. The issue of Brexit
   has a strong impact, but partisanship is mostly of equal importance for
   the respondents.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
