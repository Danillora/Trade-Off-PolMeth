@article{ WOS:000512729800003,
Author = {Turner, Ian R.},
Title = {Policy Durability, Agency Capacity, and Executive Unilateralism},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2020},
Volume = {50},
Number = {1},
Pages = {40-62},
Month = {MAR},
Abstract = {In this article, I argue that the need to motivate bureaucrats to invest
   in high-quality policy implementation alters the appeal of executive
   unilateralism. If executive orders are less durable than legislation,
   then the bureaucracy will have weaker incentives to invest in policy
   making. This affects presidents' willingness to compromise and work with
   Congress to pass legislation, rather than pursue unilateral action.
   Unilateralism becomes less attractive as bureaucratic capacity increases
   and as the relative durability of executive orders decreases. I
   formalize this logic in a simple formal model and discuss fruitful
   extensions for future work.},
