@article{ WOS:000524777500001,
Author = {Wilson, James Lindley},
Title = {The liberal populism of Shmuel Nili's The People's Duty},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2021},
Volume = {24},
Number = {4},
Pages = {616-621},
Month = {JUN 7},
Abstract = {Shmuel Nili's The People's Duty presents an account of political
   morality centered on the duties, rights, and powers of the collective
   people. He defends an extensive set of popular entitlements against the
   domination of elites. Nili's populism is liberal and egalitarian, an
   important alternative to illiberal and anti-democratic forms of
   populism. I introduce and discuss Nili's conceptions of popular
   integrity and public ownership, which underpin his discussion of various
   forms of corruption and injustice in domestic and international
   politics.},
