@article{ WOS:000616501600006,
Author = {Melusky, Benjamin and Richman, Jesse},
Title = {When the local is national - A new high-water mark for nationalization
   in the 2018 United States state legislative elections},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2020},
Volume = {30},
Number = {3},
Pages = {441-460},
Abstract = {The November 2018 state legislative elections continued the recent trend
   towards increased (dynamic) nationalization. Control of 87 state
   legislative chambers and many governor's offices was determined by
   elections that took place in a highly nationalized environment in which
   voter choices were strongly shaped by support for or opposition to the
   national parties in general and President Donald Trump in particular.
   Nationalization reached a new peak in its influence on state legislative
   elections in 2018, while cross-state variation in the extent of
   nationalization was associated with legislative polarization and the
   quality of the state-legislative-information environment.},
