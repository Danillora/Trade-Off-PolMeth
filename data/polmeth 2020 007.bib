@article{ WOS:000539826800001,
Author = {Schleiter, Petra and Fleming, Thomas G.},
Title = {Parliamentary Prorogation in Comparative Context},
Journal = {POLITICAL QUARTERLY},
Year = {2020},
Volume = {91},
Number = {3},
Pages = {641-648},
Month = {JUL},
Abstract = {This paper examines the power to prorogue (or suspend) Parliament
   following the 2019 prorogation controversy in the UK. We outline the
   legal basis of prerogative-based prorogation, survey its uses in the UK
   and other Westminster systems, and compare it with equivalent rules in
   other European parliamentary democracies. The comparative perspective
   highlights the outlier status of the UK among comparable European
   democracies. In the UK, the absence of explicit legal limits on the use
   of prorogation gives the executive exceptional scope to employ the power
   for political purposes to sidestep Parliament. We conclude by discussing
   the implications of these findings for current discussions about the
   desirability of reforming the UK's prorogation rules and placing express
   legal limits on the executive's power.},
