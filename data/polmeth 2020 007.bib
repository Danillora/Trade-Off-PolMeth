@article{ WOS:000512729800008,
Author = {Williams, Raymond T.},
Title = {The Historical Presidency Unilateral Politics in the Traditional
   Era: Significant Executive Orders and Proclamations, 1861-1944},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
