@article{ WOS:000510729200004,
Author = {Golosov, V, Grigorii},
Title = {Useful, but Not Necessarily Idiots The Ideological Linkages among the
   Putin-Sympathizer Parties in the European Parliament},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2020},
Volume = {67},
Number = {1, SI},
Pages = {53-63},
Month = {JAN 2},
Abstract = {Using evidence from roll-call voting in the European Parliament and data
   from the Euromanifesto project, this study employs statistical analysis
   (linear and logistic regression models) in order to identify the
   ideological linkages among Putin-sympathizer parties in Europe. The
   analysis demonstrates that one of the linkages among them is provided by
   their rejection of European integration, and that Putin-sympathizer
   parties tend to display ideological radicalism on both ends of the
   left-right ideological continuum, thus falling into the category of
   anti-system parties. At the same time, one of the important shared
   characteristics of Putin-sympathizer parties is populism.},
