@article{ WOS:000562794100006,
Author = {Sweet-Cushman, Jennie},
Title = {Where does the pipeline get leaky? The progressive ambition of school
   board members and personal and political network recruitment},
Journal = {POLITICS GROUPS AND IDENTITIES},
