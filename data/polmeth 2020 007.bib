@article{ WOS:000612825100011,
Author = {Nadeau, Richard and Dassonneville, Ruth and Lewis-Beck, Michael S. and
   Mongrain, Philippe},
Title = {Are election results more unpredictable? A forecasting test},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2020},
Volume = {8},
Number = {4},
Pages = {764-771},
Month = {OCT},
Abstract = {Changes in voters' behavior and in the campaign strategies that
   political parties pursue are likely to have increased the importance of
   campaigns on voters' electoral choices. As a result, scholars
   increasingly question the usefulness and predictive power of structural
   forecasting models, that use information from ``fundamental{''}
   variables to make an election prediction several months before Election
   Day. In this paper, we empirically examine the expectation that
   structural forecasting models are increasingly error-prone. For doing
   so, we apply a structural forecasting model to predict elections in six
   established democracies. We then trace the predictive power of this
   model over time. Surprisingly, our results do not give the slightest
   indication of a decline in the predictive power of structural
   forecasting models. By showing that information on long-term factors
   still allows making accurate predictions of electoral outcomes, we
   question the assumption that campaigns matter more now than they did in
   the past.},
