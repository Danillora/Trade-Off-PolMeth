@article{ WOS:000527441600001,
Author = {Bodet, Marc Andre and Villeneuve-Siconnelly, Katryne},
Title = {Effective support and electoral dynamics in Quebec},
Journal = {FRENCH POLITICS},
Year = {2020},
Volume = {18},
Number = {3, SI},
Pages = {221-237},
Month = {SEP},
Abstract = {In this article, we explore the impact of electoral participation on
   electoral dynamics in Quebec. We propose a parsimonious model that
   considers the linguistic composition of districts and the effective
   electoral support for parties to explain electoral politics. In modeling
   these dynamics, we introduce three new concepts, namely leverage effect,
   push effect, and differentiation effect. Our findings show that the 2018
   election represents an extension of dynamics established in 2012 and
   played a corrective role to those of the 2014 election. They also
   demonstrate that leverage effects are not effective mechanisms for
   ensuring electoral success.},
