@article{ WOS:000584379000001,
Author = {Hajjaj, Bobby},
Title = {Nationalism and national identity formation in Bangladesh: A colonial
   legacy behind the clash of language and religion},
Journal = {ASIAN JOURNAL OF COMPARATIVE POLITICS},
Year = {2022},
Volume = {7},
Number = {3},
Pages = {435-448},
Month = {SEP},
Abstract = {The nature of identity formation is complex. The production of identity
   in South Asia, with its colonial past, has been largely dependent on the
   region's colonial history. In this article we chart the process of
   political identity formation in Bangladesh. We identify the various
   historical causes that led to the creation of each of the two types of
   identity prevalent today. These two divisive identities based on
   language and religion, one pitted against the other, each became the
   central platform of each of the two major political parties, the Awami
   League (AL) and the Bangladesh Nationalist Party (BNP). This
   disquisition shows clear patterns of political distress that resulted in
   the bifurcation of these two divisive political identities that ossified
   by the late 20th century due chiefly to the actions of the colonial
   government of the Raj.},
