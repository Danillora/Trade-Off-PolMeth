@article{ WOS:000614863100024,
Author = {Giannotti, Emanuel and Soares Goncalves, Rafael},
Title = {The cold war in the favelas and the poblaciones,
   1945-1964. A conflict between communists and the Catholic Church},
Journal = {IZQUIERDAS},
Year = {2020},
Volume = {49},
Pages = {642-662},
Month = {JAN},
Abstract = {The ``favelas{''} of Rio de Janeiro and the ``poblaciones{''} of
   Santiago de Chile were usually visited by external actors. It is through
   the accounts of social workers, students, political activists or
   volunteers that we know about the histories of these places. With the
   beginning of the cold war, the presence of these individuals took on a
   new meaning. In this backdrop, the conflict between the communists and
   the Catholic Church for the hegemony over the low-income neighborhoods,
   which sometimes led to violent confrontations, took a central stage.
   Yet, the activities developed by these institutions strengthened the
   organization of the inhabitants and physically improved these
   neighborhoods, fostering their urban, social and political integration.},
Publisher = {ARIADNA EDICIONES \& SAINT PETERSBURG STATE UNIV},
