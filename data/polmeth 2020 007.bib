@article{ WOS:000605946600006,
Author = {Cavus, Gulin},
Title = {FACT-CHECKING IN A POLARIZED WORLD},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2020},
Volume = {19},
Number = {3},
Pages = {57-65},
Month = {FAL},
Abstract = {With the speed at which information is circulating, disinformation is
   bound to change our lives in profound ways. Although some may seem
   harmless, every time a piece of false information touches someone, it
   leaves an impact. Dedicated and independent fact-checking organizations
   have been trying to reverse this negative trend to create value, and
   fight information disorder as a whole. These platforms are determined to
   explain that accurate information is accessible by anyone, and that no
   one needs to become an authority. However, organizations that do not
   agree with this idea can quickly become a target, especially in
   polarized societies. The only thing that can save us from being targeted
   is to provide transparency by engaging with our community and creating
   an impact on society.},
