@article{ WOS:000590818400001,
Author = {Bhuyan, Rupaleem and Yoon, Kate and Valmadrid, Lorraine},
Title = {Family Reunification as an Earned Right: A Framing Analysis of Migrant
   Workers' Pathways to Neoliberal Multicultural Citizenship in Canada},
Journal = {NEW POLITICAL SCIENCE},
Year = {2020},
Volume = {42},
Number = {4, SI},
Pages = {558-577},
Month = {OCT 1},
Abstract = {This article examines how the right to sponsor family members'
   immigration is framed by migrant caregivers in relation to Canadian
   legislation and political rhetoric. We focus on official immigration
   policy and political rhetoric between 2014-2018, a period when Canada
   imposed stringent language and education requirements on migrant workers
   seeking permanent residence. By examining how political rhetoric frames
   migrant caregivers as not earning ``enough{''} to migrate with their
   families, we illustrate how lawmakers promote tolerance for differential
   inclusion within multicultural discourse. Migrant caregivers, the
   majority of whom originate in the Philippines, similarly employ
   market-based values to position themselves as deserving the right to
   sponsor their family members' immigration. They also draw upon
   traditional ethics of reciprocal obligation, however, to frame their
   relationship to Canadian employers and the state in ways that
   reconstruct the terms of multicultural citizenship in Canada.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
