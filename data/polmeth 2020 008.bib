@article{ WOS:000568979400001,
Author = {Catalan Chamorro, Maria Jose},
Title = {Electronic process of law and its implementation in the Spanish legal
   system: a study of comparison with the British electronic process},
Journal = {IDP-INTERNET LAW AND POLITICS},
Year = {2020},
Number = {31},
Month = {OCT},
Abstract = {Spain is capable of launching an automated electronic judicial process,
   similar to the one implemented in the United Kingdom. In the present
   paper, we present in parallel the analysis of the British project and a
   proposal of lege ferenda for the inclusion of this modality in the
   Spanish legal system. The first stage of its development would conclude
   with the resolution of some of the civil proceedings under the Voluntary
   Jurisdiction Act, including a final proposal to enable citizens to file
   administrative or criminal complaints online. In this way, we would
   definitely bring access to justice closer to citizens, pretending a sort
   of self-defense regulated and guaranteed by judges and courts as well as
   close to citizens through a secure and intuitive computer platform.},
