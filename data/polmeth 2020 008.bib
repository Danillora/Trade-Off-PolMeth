@article{ WOS:000603655700004,
Author = {Breuer, Stefan},
Title = {Class, Estate and Stratification in Max Weber's Sociology of Religion},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
