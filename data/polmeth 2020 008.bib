@article{ WOS:000596451800002,
Author = {Elliott, Ian C. and Sinclair, Caroline and Hesselgreaves, Hannah},
Title = {LEADERSHIP OF INTEGRATED HEALTH AND SOCIAL CARE SERVICES},
