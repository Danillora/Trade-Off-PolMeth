@article{ WOS:000644480700006,
Author = {Garzon Espinosa, Eduardo},
Title = {UNIVERSAL BASIC INCOME: DESIGN CONSIDERATIONS AND RISKS IN ITS
   IMPLEMENTATION},
Journal = {REVISTA INTERNACIONAL DE PENSAMIENTO POLITICO},
Year = {2020},
Number = {15},
Pages = {91-108},
Abstract = {The Universal Basic Income proposal is becoming more and more known both
   in the academic field and outside it and that has led it to receive more
   and more attention and also more criticism. It is hardly questionable
   that the Universal Basic Income has very important advantages compared
   to other types of public social protection benefits: its coverage
   reaches the entire population and the economic security and freedom it
   offers allows no one to be forced to accept poor and precarious jobs to
   cover their basic needs. In addition, all this can be achieved at an
   affordable cost and through a relatively simple and fast application
   (especially compared to many conditional social benefits). However, the
   start-up of a Universal Basic Income is not exempt from questions or
   important risks to take into account. The aim of this work is to
   identify and analyze some of them, with the ultimate goal of overcoming
   them to perfect a measure that already has enormous economic and social
   potential.},
