@article{ WOS:000570725800001,
Author = {McClendon, Gwynet and Beatty Riedl, Rachel},
Title = {Using Sermons to Study Religions' Influence on Political Behavior},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2021},
Volume = {54},
