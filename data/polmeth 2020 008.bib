@article{ WOS:000527178000001,
Author = {Piotrowska, Barbara Maria},
Title = {The Price of Collaboration: How Authoritarian States Retain Control},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2020},
Volume = {53},
Number = {13},
Pages = {2091-2117},
Month = {NOV},
Abstract = {How does access to foreign or independent media affect the operation of
   a state security apparatus? This article answers this question
   concentrating on two characteristics of the informant network of the
   East German Stasi: the number of informants and their ``price.{''}
   Exposure to West German TV (WGTV) had the potential to decrease the
   supply of informants and increase the demand for them, pushing up the
   value of the payments the informants received, but leaving their
   quantity theoretically ambiguous. I verify this reasoning using a rare
   original data set of Stasi informants. Results show that informants were
   given approximately 70 East German marks worth of rewards more per year
   in the areas that had access to WGTV, as compared with areas with no
   reception-ironically an amount roughly equivalent to the cost of an
   annual East German TV subscription. These findings demonstrate how an
   authoritarian state can counteract the potentially destabilizing effect
   of foreign media.},
