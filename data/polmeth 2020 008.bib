@article{ WOS:000536750400008,
Author = {Fraga, Bernard and Holbein, John},
Title = {Measuring youth and college student voter turnout},
Journal = {ELECTORAL STUDIES},
Year = {2020},
Volume = {65},
Month = {JUN},
Abstract = {Young adulthood is a critical period in civic development. However,
   measuring electoral participation among this group generally-and the
   many young people who go to college in particular-is fraught with
   potential pitfalls stemming from a reliance on survey-based measures of
   voting. In this note, we compare patterns of youth turnout in two
   large-scale, survey-based datasets commonly used to measure voting, the
   Current Population Survey and the Cooperative Congressional Election
   Study, to two voter-file based datasets: the National Study of Learning,
   Voting, and Engagement (NSLVE) and a comprehensive nationwide voterfile
   provided by the Data Trust. We find high levels of concordance between
   measures in the NSLVE, Data Trust, and the CPS. However, despite linking
   their sample to validated voter records, the CCES does not mirror these
   benchmarks. We conclude by discussing the challenges and opportunities
   that shape the study of youth turnout.},
