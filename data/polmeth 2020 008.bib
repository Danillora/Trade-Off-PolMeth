@article{ WOS:000576264200008,
Author = {Kuhn, V, Adriaan P. and Graino Ferrer, Guillermo},
Title = {North of Rome or South of Brussels. The reinventing of the Lega Nord:
   From regionalism to lepenisation},
Journal = {REVISTA DE ESTUDIOS POLITICOS},
Year = {2020},
Number = {189},
Pages = {197-224},
Month = {JUL-SEP},
Abstract = {This article analyses the ideological, organisational and electoral
   evolution of the Italian political party Lega Nord. Based on a set of
   different approaches, its aim is to explain the emergence and
   consolidation of a new party model which resembles the evolution of
   other European populist parties, like the French Front National. In a
   first step, we analyze the relationship between the so-called northern
   question and the territorial political subcultures in Italy through the
   LN ideology; then, we deal with changes in leadership and the party
   structure; and finally, we address the crucial changes that led to the
   party's ``nationalization{''}, taking into account the populist success
   story in an international context.},
