@article{ WOS:000523590900012,
Author = {Urdanoz Ganuza, Jorge},
Title = {A constitutional antinomy? (Un)equal suffrage in the constitution of
   1978},
Journal = {TEORIA Y REALIDAD CONSTITUCIONAL},
Year = {2020},
Volume = {45},
Pages = {353-378},
Abstract = {The article examines the legal and political state of equal suffrage in
   Spain. The perspectives applied are fundamentally those of three
   well-differentiated subject areas: Political Philosophy, Constitutional
   Law and Political Science. In addition, the text also grounds on a more
   younger field, the Voting Theory. The conclusions reached are highly
   worrying: not only that Spain is the country with one of the highest
   verifiable incidence of inequality in its voting system, but also that
   its Constitution of 1978 is host to a legal antinomy that has impeded
   adequate constitutional protection to guarantee the fundamental right of
   its citizens to an equal vote.},
