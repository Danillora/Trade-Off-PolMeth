@article{ WOS:000591856700011,
Author = {Naydenko, V. N.},
Title = {ETHNIC CONFLICT IN THE CONTEXT OF RUSSIAN NATIONAL SECURITY},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2020},
Number = {1},
Pages = {150-162},
Abstract = {The resolution of ethnic conflicts is one of the priority problems of
   ensuring the security of citizens of Russia, its society and the state.
   Between October 2018 and March 2019, a survey of twenty highly qualified
   Russian experts in this field was conducted using questionnaire and
   in-depth interview methods. The purpose of the survey was to determine
   the degree of importance of various components of Russia's national
   security, the role and place of the ethno-national sphere, the degree of
   danger of ethnic conflict, and the ratio of external and internal
   threats to Russian Federation. The survey results indicate that security
   in the field of interethnic relations is one of the most important
   components of Russia's national security. Higher in importance, experts
   evaluated only the military and state security of the country. Security
   in the sphere of ethno-national relations is closely interconnected with
   military and state security and has a significant impact on them.
   Assessing the sources of threats, most experts (60\%) believe that at
   present, the main threats to Russia come approximately equally from
   abroad and from within the country; a smaller part of experts (40\%)
   consider that they are abroad. None of the respondents supported the
   view that these threats are mainly located within the country. Similar
   estimates were given by experts on the sources of potential threats to
   the Russian Federation for the next five to seven years.},
Publisher = {NONCOMMERCIAL PARTNERSHIP EDITORIAL BOARD POLIS-POLITICAL STUDIES},
