@article{ WOS:000573216500001,
Author = {Blofield, Merike and Touchton, Michael},
Title = {Moving Away from Maternalism? The Politics of Parental Leave Reforms in
   Latin America},
Journal = {COMPARATIVE POLITICS},
Year = {2020},
Volume = {53},
Number = {1},
Pages = {1+},
Month = {OCT},
Abstract = {Policies to address reconciliation of work and family have come to the
   political forefront around the world. One key policy is extending paid
   parental leaves. We construct a database on paid parental leaves for
   Latin American countries from 2000 to 2016. Quantitative analysis finds
   that higher per capita GDP, higher women's labor force participation,
   and more programmatic political parties increase the likelihood of
   parental leave reform, while political ideology, share of women in the
   legislature, and gender of the executive do not emerge as significant.
   We process-trace reforms in Argentina, Brazil, Chile, and Uruguay and
   show how programmatic party systems enable social demand to influence
   executive commitment to pursue policy reform and evidence-based
   expertise to design policy that facilitates more paternal involvement
   and social equity.},
