@article{ WOS:000512741100001,
Author = {Touchton, Michael R. and Klofstad, Casey A. and West, Jonathan P. and
   Uscinski, Joseph E.},
Title = {Whistleblowing or leaking? Public opinion toward Assange, Manning, and
   Snowden},
Journal = {RESEARCH \& POLITICS},
Year = {2020},
Volume = {7},
Number = {1},
Month = {JAN},
Abstract = {The release of classified documents through outlets like WikiLeaks has
   transformed American politics by shedding light on the innerworkings of
   governments, parties, and corporations. The high-profile criminal cases
   associated with such releases - those of Julian Assange, Chelsea
   Manning, and Edward Snowden - have highlighted important questions about
   journalism, government secrecy, and the public's ``right to know.{''}
   Scholars have focused on the journalistic and legalistic implications
   but have yet to explore how the public views those who release
   classified materials, and what factors affect those views. Using data
   from the 2018 Cooperative Congressional Election Study, we provide
   results from three embedded experiments testing the effects of two forms
   of framing on favorability ratings toward Assange, Manning, and Snowden.
   The first frame addresses partisanship (i.e., which party is injured by
   the release) and the second addresses how the action is framed (i.e.,
   did the person ``leak{''} or ``blow the whistle{''}). The data show that
   both the party and leaking/whistleblowing frames significantly affect
   favorability in expected ways. The release of classified materials comes
   with both costs and benefits, but public opinion appears to be more
   sensitive to its implications for partisan competition.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Uscinski, JE (Corresponding Author), Univ Miami, Dept Polit Sci, 1300 Campo Sano Ave, Coral Gables, FL 33146 USA.
   Touchton, Michael R.; Klofstad, Casey A.; West, Jonathan P.; Uscinski, Joseph E., Univ Miami, Dept Polit Sci, 1300 Campo Sano Ave, Coral Gables, FL 33146 USA.},
DOI = {10.1177/2053168020904582},
Article-Number = {2053168020904582},
EISSN = {2053-1680},
Keywords = {Whistleblowing; partisanship; framing; Julian Assange; Edward Snowden;
   Chelsea Manning},
