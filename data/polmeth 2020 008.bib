@article{ WOS:000585678100001,
Author = {Duhalt, Adrian},
Title = {The Rise and Fall of Pemex's Petrochemical Arm},
Journal = {LATIN AMERICAN POLICY},
Year = {2020},
Volume = {11},
Number = {2},
Pages = {275-289},
Month = {DEC},
Abstract = {Pemex's petrochemical arm was regarded as the backbone of Mexico's
   industrialization efforts from the 1960s to the 1980s. Steered by
   different governments, the state-owned petrochemical industry
   experienced a rapid expansion with the goal to encourage development of
   a much broader industrial base. The result was the articulation of
   productive activities, but economic hardship and policy changes in the
   1990s triggered a process of steady decline in terms of production and
   investment. As the government began to withdraw from the industry, the
   strategic character of the state-owned petrochemical industry eventually
   faded away. The negative consequences are vast yet seldom discussed. The
   lack of domestic production of petrochemical raw materials weakened
   national value chains such as fertilizers and plastics and led to a
   troubling rise in imports. This article sheds light on the development
   trajectory of the petrochemical arm of Pemex and situates the discussion
   within the new policy shifts on which Mexico has recently embarked.},
