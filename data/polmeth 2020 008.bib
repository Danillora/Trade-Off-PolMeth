@article{ WOS:000509641300001,
Author = {Navari, Cornelia},
Title = {Agents versus structures in English School theory: Is co-constitution
   the answer?},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
Year = {2020},
Volume = {16},
Number = {2, SI},
Pages = {249-267},
Month = {JUN},
Abstract = {While generally accepted as an interpretive theory, Bull's emblematic
   text demonstrates strong structural characteristics. Subsequent
   attributions move between the interpretive or `reflexive' and the
   institutional and structural. Recently, however, the idea has come
   forward that English School theory is, and maybe have been from the
   beginning, a form of structuration theory, a theory in which structures
   are not quite the hard determinants generally understood in structural
   theories, and interpreting agents are not quite so free to interpret
   structures in any tradition that seems appropriate to a matter at hand.},
