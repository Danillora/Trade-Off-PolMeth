@article{ WOS:000580721400002,
Author = {Allchorn, William},
Title = {Political responses to anti-Islamic protest: responses, rationales and
   role evaluations},
Journal = {BRITISH POLITICS},
Year = {2020},
Volume = {15},
Number = {4},
Pages = {393-410},
Month = {DEC},
Abstract = {Nine years after the formation of the English Defence League (EDL),
   scant academic attention has been paid to how UK politicians and
   policymakers have responded to anti-Islamic forms of protest. Drawing on
   a content analysis of 48 semi-structured elite interviews with Members
   of Parliament and local Councillors, this article proposes a typology
   and overview of responses issued by local politicians towards both the
   EDL and Britain First. The findings here are significant. While the
   majority of political responses have been largely exclusionary in nature
   (e.g. calling for proscriptions, bans and protest restrictions on
   anti-Islamic protest), how these exclusionary responses manifest
   themselves and what drives such responses varies greatly. Moreover, some
   responses have exhibited a more inclusionary character-with a minority
   of politicians involved in engagement and interaction work with both
   communities attracted to, and affected by, anti-Islamic protest.},
