@article{ WOS:000756022800022,
Author = {Delfino, Andrea},
Title = {The Incursion of Telework and the Constant Return of the Working Hours
   Regulation Debate},
Journal = {TEMAS Y DEBATES},
Year = {2020},
Number = {40, S},
Pages = {215-224},
Month = {DEC},
Abstract = {The abrupt incursion of teleworking in Argentina took place after the
   lockdown measures taken in order to contain the spread of Covid-19
   infections. Before the pandemic, telework was not specifically
   regulated. However, it was finally discussed and passed between the
   months of June and August 2020. This study discusses three
   characteristics of telework, which are closely related and are even
   confused within the notion of working time: the space and time
   relationship, the working hours limits, and the conflict between working
   time and the time necessary for other aspects of life. Some of these
   elements were partially incorporated into the new regulation.},
