@article{ WOS:000603117400002,
Author = {Betti, Andrea and Garcia Domonte, Aurora and Biderbost, Pablo},
Title = {FLIPPING THE CLASSROOM IN POLITICAL SCIENCE: STUDENT ACHIEVEMENT AND
   PERCEPTIONS},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2020},
Volume = {40},
Number = {3},
Pages = {589-615},
Abstract = {The flipped-classroom method is acquiring importance as a pedagogical
   technique to improve the learning performance of students. So far, most
   studies have examined the possible applications of the flipped-classroom
   method in the natural sciences realm, with most analyses certifying its
   positive impact. Its has been applied much less frequently in the social
   sciences realm, particularly in political science. Our project aims to
   generate data about active learning in political science by conducting
