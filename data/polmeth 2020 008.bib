@article{ WOS:000614863100049,
Author = {Carlos Rungruangsakorn, L.},
Title = {The conflict impact on the political-normative design in State's role:
   the Chilean Republic environmental role, a case of (dis)construction},
Journal = {IZQUIERDAS},
Year = {2020},
Volume = {49},
Pages = {1134-1154},
Month = {JAN},
Abstract = {This article aims to connect the liberal conflict theory with state role
   construction. In this framework, the theoretical model developed by
   Oszlak (2011) is approached, which explains how the state role is
   expressed in three levels of action: micro, meso and macro.
   From the above, the process of political-normative design is examined
   whereby the state's role in a given matter is formally manifest. For a
   better understanding of how the state role is created, a dialectical
   theoretical scheme called ``Cycle of the (des)construction of the role
   of the State{''} is proposed. Subsequently, this model is applied for
   analyze the environmental role conformation of Chilean State, with an
   emphasis on its environmental impact assessment system.},
