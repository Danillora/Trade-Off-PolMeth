@article{ WOS:000550322400003,
Author = {Portnoy, Andrii},
Title = {The Holodomor as genocide Historiographical and legal discussions},
Journal = {OSTEUROPA},
Year = {2020},
Volume = {70},
Number = {1-2},
Pages = {31+},
Abstract = {The famine of 1932/33 led to the death of millions of people in Ukraine.
   It is still a subject of controversy today whether the Holodomor can be
   classified as genocide. The use of the word ``genocide{''} as defined by
   the UN Genocide Convention is a delicate matter. Above all, the question
   of whether evidence can be provided of the intention of the perpetrators
   leads to different interpretations. In light of the judgements by the
   International Courts of Justice regarding the mass murders in Rwanda and
   Srebrenica, the Holodomor is also in effect genocide.},
