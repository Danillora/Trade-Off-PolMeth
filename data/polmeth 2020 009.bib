@article{ WOS:000546995900005,
Author = {Piromalli, Eleonora},
Title = {Does socialism need fraternity? On Axel Honneth's The Idea of
   Socialism},
Journal = {EUROPEAN JOURNAL OF POLITICAL THEORY},
Year = {2020},
Volume = {19},
Number = {3},
Pages = {375-395},
Month = {JUL},
Abstract = {In this article, after retracing the main lines of Honneth's The Idea of
   Socialism, I address two objections to it. Firstly, I question the
   marked substantiality of Honneth's proposed socialist `community of
   fraternal life', resulting from the conjunction of the idea of social
   freedom with the principle of fraternity he derives from the
   proto-socialists. On the basis of my objections, I then delineate an
   original theoretical model, denominated `socialism through convergence'
   (STC). While based on Honneth's concept of social freedom, STC can
   abstract from the element of fraternity. It is, by consequence, immune
   to the excess of substantiality of Honneth's perspective and potentially
   more attractive for the members of modern, pluralistic societies.
   Finally, I criticize Honneth's perspective for under-determining the
   element of normative social conflicts in the sphere of democracy and
   normalizing their expression into the forms of an orderly democratic
   deliberation; consequently, I show how the STC perspective can more
   effectively account for social struggles and political conflicts.},
