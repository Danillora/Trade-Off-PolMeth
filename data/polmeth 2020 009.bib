@article{ WOS:000597749400001,
Author = {Surridge, Paula},
Title = {Britain's Choice: Polarisation or Cohesion},
Journal = {POLITICAL QUARTERLY},
Year = {2021},
Volume = {92},
Number = {1},
Pages = {119-124},
Month = {JAN},
Abstract = {Britain's Choice, a report published by More in Common, develops a new
   segmentation of the British public based on their core beliefs, values
   and identities. Identifying seven segments, the report aims to better
   understand the British public, the goal being to work out how to build a
   more cohesive Britain-the choice facing Britain being a path to further
   division and polarisation or one to greater solidarity and shared
   values. The report strikes a broadly optimistic turn and highlights the
   shared values across the seven groups, but there are warning signs in
