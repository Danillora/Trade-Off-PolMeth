@article{ WOS:000577857200001,
Author = {Buyantueva, Radzhana},
Title = {What Motivates LGBT Activists to Protest? The Case of Russia},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2022},
Volume = {69},
Number = {3, SI},
Pages = {242-255},
Month = {MAY 4},
Abstract = {Russian LGBT activism started to emerge in the early post-Soviet years.
   However, only since the mid-2000s have Russian LGBT activists begun to
   engage in protest activity to a significant degree. In the ensuing
   years, they have organized (or attempted to do so) a variety of protest
   events on an annual basis. The paper draws on social psychological
   studies of collective action to examine what motivates Russian LGBT
   activists to protest. The study also seeks to investigate why they
   choose protests as a suitable tactic to achieve their goals.},
