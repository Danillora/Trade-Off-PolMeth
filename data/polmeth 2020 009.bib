@article{ WOS:000576541900001,
Author = {Laruelle, Marlene and Alexseev, Mikhail and Buckley, Cynthia and Clem,
   Ralph S. and Goode, J. Paul and Gomza, Ivan and Hale, Henry E. and
   Herron, Erik and Makarychev, Andrey and McCann, Madeline and Omelicheva,
   Mariya and Sharafutdinova, Gulnaz and Smyth, Regina and Sokhey, Sarah
   Wilson and Troitskiy, Mikhail and Tucker, Joshua A. and Twigg, Judyth
