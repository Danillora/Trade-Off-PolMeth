@article{ WOS:000600233700001,
Author = {Zachary Taylor, Mark},
Title = {The Historical Presidency: The Gilded Age Presidents and the
   Economy},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2021},
Volume = {51},
Number = {4},
Pages = {860-883},
Month = {DEC},
Abstract = {Do presidents matter for America's economic performance? This article
   reports several findings and hypotheses derived from a set of least
   likely cases, the Gilded Age presidents. These executives are often
   dismissed as weak and ineffective non-entities. However, statistical
   data and qualitative evidence suggest that presidents do matter, even
   during the Gilded Age. They are powerful and essential actors who can
   deeply affect American economic performance in three subtle ways. The
   (in)action of Gilded Age presidents during economic crises appears to
   have mattered most. More tentatively, presidential vision and skill at
   coalition building also appear to correlate strongly with economic
   performance. These findings are not merely of historical interest, as
   the similarities with the twenty-first century are striking. These cases
   therefore have important implications for theories of presidential power
   and performance, American political development, and the role of ideas
   in politics.},
