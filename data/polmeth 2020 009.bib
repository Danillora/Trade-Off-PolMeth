@article{ WOS:000614863100080,
Author = {Riquelme Brevis, Hernan and Lopez Dietz, Sandra and Riquelme Brevis,
   Matias and Meza Serra, Natalia},
Title = {Working conditions and work environment of workers in retail businesses
   of La Araucania, Chile},
Journal = {IZQUIERDAS},
Year = {2020},
Volume = {49},
Pages = {1804-1829},
Month = {JAN},
Abstract = {In this paper, an analysis of the working conditions and the
   working/post-work environment of workers in commercial shops in La
   Araucania is developed. The focus of this study was identifying gender
   inequalities in these two dimensions of retail that is increasingly
   taking center as economic sector in Chilean society. On the basis of a
   quantitative methodology, workers from five retail companies in the
   region were contacted (n=205), who answered a survey consisting of six
   sections. Among the main results -which were analyzed as particular
   variables- it is relevant to mention that there was a high level of
   precariousness regarding to working conditions; gender differences in
   health services; double feminized working hours; dissatisfaction related
   to routinely work in the company and gender wage gaps.},
