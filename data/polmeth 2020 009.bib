@article{ WOS:000605671100007,
Author = {Craney, Aidan},
Title = {Local Participation or Elite Capture in Sheep's Clothing? A Conundrum of
   Locally Led Development},
Journal = {POLITICS AND GOVERNANCE},
Year = {2020},
Volume = {8},
Number = {4},
Pages = {191-200},
Abstract = {This article discusses concepts of legitimacy and elite capture in
   locally led development through a case study of the Pacific-based Green
   Growth Leaders' Coalition (GGLC). GGLC is a fellowship of persons
   identified for their developmental leadership potential on issues of
   sustainability and economic growth. Members are recruited into an
   exclusive grouping dedicated to influencing positive developmental
   change through informal networks and political backchannels. With their
   membership representing people who both self-identify and are locally
   recognised as leaders, queries exist to the extent to which their
   efforts represent a shift towards greater ownership of developmental
   processes at local levels or simply reinforce elite capture of `local
   voice' in the most aid-dependent region in the world. Rather than
   necessarily offering straightforward answers to questions of legitimacy
   and elite capture, the example of GGLC demonstrates how complex the
   notion of locally led development can be in practice.},
