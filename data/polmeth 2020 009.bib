@article{ WOS:000595330300002,
Author = {Macdonald, Laura},
Title = {Canada in the North America Region: Implications of the Trump Presidency},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2020},
Volume = {53},
Number = {3},
Pages = {505-520},
Month = {SEP},
Abstract = {The election of Donald Trump and his decision to renegotiate the North
   American Free Trade Agreement (NAFTA) represented a shock to the
   Canadian and Mexican governments and business elites. Drawing on the New
   Regionalism(s) Approach (NRA), this article reviews the response of the
   Canadian state to the crisis in the North American regional project. I
   argue that this newer theoretical approach better explains the dynamics
   of regionalization or regional decomposition than mainstream theories by
   integrating the role played by uneven globalization, normative and
   ideational dimensions, and civil society in processes of regional
   integration and/or decomposition.},
