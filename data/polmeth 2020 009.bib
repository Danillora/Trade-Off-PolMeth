@article{ WOS:000592392100001,
Author = {Al Ganideh, Saeb F.},
Title = {Demystifying Arabs: Is soccer Arab societies' `crystal ball'?},
Journal = {NATIONAL IDENTITIES},
Year = {2021},
Volume = {23},
Number = {5},
Pages = {531-553},
Month = {OCT 20},
Abstract = {The current paper explores Arab societies through the lens of soccer
   rivalries. The rhetoric around soccer and identity in Arab countries
   illuminates a comprehensive overview on what is happening in these
   societies. Arguably soccer in the case of Arab societies clarifies the
   boundaries between `us' and `them' and provides clear aspects of
   contemporary culture. Soccer reveals that Arab countries are deeply
