Title = {FACTOR OF CHINA IN EVOLUTION OF THE INDIAN APPROACH TO THE INDO-PACIFIC
   REGION},
Journal = {SRAVNITELNAYA POLITIKA-COMPARATIVE POLITICS},
Year = {2020},
