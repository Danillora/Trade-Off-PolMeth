@article{ WOS:000574605400002,
Author = {Hutchinson, William},
Title = {Deceiving Autonomous Drones},
Journal = {INTERNATIONAL JOURNAL OF CYBER WARFARE AND TERRORISM},
Year = {2020},
Volume = {10},
