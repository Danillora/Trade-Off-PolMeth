@article{ WOS:000536225700001,
Author = {Juelich, Courtney L. and Coll, Joseph A.},
Title = {Rock the Vote or Block the Vote? How the Cost of Voting Affects the
   Voting Behavior of American Youth},
Journal = {AMERICAN POLITICS RESEARCH},
Year = {2020},
Volume = {48},
Number = {6},
Pages = {719-724},
Month = {NOV},
Abstract = {Young voters make up the largest portion of the electorate but vote at
   the lowest rates of any age group. While scholars have studied how
   culture affects youth political participation, few studies have analyzed
   how institutional barriers affect youth voting-even though these laws
   have been found to affect turnout of other disadvantaged groups.
   Considering younger citizens are more likely to be non-habitual voters
   with less political knowledge, efficacy, and resources, it is possible
   that these laws have magnified effects for youths. This could explain
   why new voters, facing new restrictions to voting, are participating at
   lower percentages than youths of earlier cohorts. Using the 2004-2016
   Current Population Survey (N = 360,000) and the Cost of Voting Index to
   test the effects of restrictive electoral environments on youth turnout,
   we find that restrictive environments disproportionately hurt young
   voters by decreasing the probability they turn out by 16 percentage
   points, compared with older voters.},
