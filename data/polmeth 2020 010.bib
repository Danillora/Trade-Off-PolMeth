@article{ WOS:000531859400002,
Author = {Audard, Catherine},
Title = {DEMOCRACY AND REASON: ARE THE COGNITIVE AND MORAL DEMANDS OF PUBLIC
   JUSTIFICATION EXCESSIVE?},
Journal = {TOCQUEVILLE REVIEW},
Year = {2020},
Volume = {41},
Number = {1},
Pages = {13-40},
Month = {APR 1},
Abstract = {Un consensus politique est-il possible sans intervention de la force ou
   de la persuasion, sur la seule base de la raison ? La reponse de John
   Rawls dans Liberalisme politique est celle de l'abstinence epistemique.
   En renoncant a faire intervenir le Vrai, mais seulement le Raisonnable,
   en mobilisant des raisons publiques et non des croyances personnelles ou
   collectives, il serait possible d'arbitrer les conflits politiques et de
   stabiliser les institutions democratiques sans intervention de la force.
   Mais cette solution a suscite de nombreuses critiques et, un peu a la
   maniere du << patriotisme constitutionnel >> defendu par Habermas, elle
   semble une base trop pauvre pour proteger le consensus democratique
   contre ses ennemis. Surtout cette demarche exige des competences
   cognitives et morales excessives. Apres avoir analyse les conditions de
   possibilite de l'abstinence epistemique, je repondrai a trois series
   d'objections et je conclurai qu'autant que de competences cognitives et
   morales, la justification publique a besoin d'un espace public vraiment
   democratique et pluraliste. C'est une question sociale et politique tout
   autant que cognitive ou morale.},
