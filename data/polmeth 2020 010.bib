@article{ WOS:000547266200018,
Author = {Palmer, Barbara and van Assendelft, Laura and Stegmaier, Mary},
Title = {Revisiting the Presence of Women in Political Science Journal Editorial
   Positions},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2020},
Volume = {53},
Number = {3},
Pages = {499-504},
Month = {JUL},
Abstract = {In 2010, an analysis of the top 50 political science journals showed
   that women were reasonably well represented as editors, associate
   editors, and board members compared to their numbers as senior faculty
   at PhD-granting institutions. As the presence of women in the profession
   has increased, have women kept up in these editorial positions? Overall,
   the data from 2018 suggest that they have. Although women are still
   significantly underrepresented as editors and associate editors at
   journals with small editorial staffs, they are well represented at those
   with medium-sized and large staffs. The proportion of women as board
   members also has kept pace with the proportion of female senior faculty
   at PhD-granting institutions, especially at the top five journals in the
   profession. There is still significant variation among journals but
   little change in their rankings: journals with the highest proportion of
   women as editors, associate editors, and board members in 2010 continued
   to lead the way in 2018.},
