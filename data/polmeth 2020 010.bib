@article{ WOS:000614864700068,
Author = {Garber Fuentes, Camilo},
Title = {The intellectual turns of CIEPLAN (1979-1989) From criticism to the
   adoption of neoliberalism and its conception of democracy},
Journal = {IZQUIERDAS},
Year = {2020},
Volume = {49},
Pages = {4404-4424},
Month = {JAN},
Abstract = {CIEPLAN was the most influential think tank in the opposition to the
   dictatorship and the democratic recovery in Chile, as it helped lay its
   political and economic foundations. Studies address it as a technocratic
   phenomenon and focuses on the trajectory of its members. Here we review
   the evolution of their ideas regarding democracy and neoliberalism, an
   economic system that is currently in crisis, through the sequential
   analysis of his written works. Three intellectual phases are found, and
   a series of explanations are offered for the turns that connect them.},
