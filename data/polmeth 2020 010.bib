@article{ WOS:000625412800010,
Author = {TenHouten, Warren D.},
Title = {Power, equity, and commitment in social exchange: A three-stage model of
   the sociocognitive appraisal process},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2020},
Volume = {13},
Number = {3},
Pages = {439-463},
Abstract = {We present and preliminarily test a theory of social exchange: (i) a
   socioeconomic event arises in which social exchange can serve
   individuals' self-interests; (ii) participants engage in a three-stage
   sociocognitive-appraisal process; and (iii), they agree to enact
   exchange behavior. In the appraisal process, exchanges begin with
   perceptions of negotiating power; rules of equity and fairness are
   considered, and an exchange-based communal relationship emerges. We
   analyze a corpus of 564 life-historical interviews with Euro-Australians
   and Australian Aborigines using a lexical-level content-analytic
   methodology. A causal model fit the data for the entire corpus and after
   disaggregation into four culture-sex groups.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
