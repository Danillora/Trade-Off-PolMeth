@article{ WOS:000530553600001,
Author = {Winburn, Jonathan and Winburn, Amanda},
Title = {The Role of Political Ideology in Public Opinion toward Enumerated
   Antibullying Policies in Public SchoolsPalabras Clave(sic)(sic)(sic)},
