@article{ WOS:000663146300001,
Author = {Dutard, Delphine Deschaux and Lequesne, Christian},
Title = {The Impact of Brexit on the EU foreign policy},
Journal = {POLITIQUE EUROPEENNE},
Year = {2020},
Volume = {70},
Number = {4},
Pages = {8-17},
Abstract = {Brexit debates have generated their steady stream of controversy and
   political tension in Britain and the European Union. However, little
   research has been done on the impact of Brexit on EU foreign policy,
   which is the research topic of this special issue. Brexit invites to
   analyse more specifically the evolution of EU foreign policy at both the
   supranational and the national (i.e. Member State) levels. The research
   questions about Brexit are therefore twofold. The first is: how to
   assess the effects of the withdrawal of a member state on the overall
   evolution of EU foreign policy? The second-taking into account the
   interdependencies between the national and European levels-is: how to
   understand the adaptation and change strategies of each national foreign
   policy in relation to EU foreign policy?},
