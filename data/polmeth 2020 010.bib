@article{ WOS:000514400000007,
Author = {Laegaard, Sune},
Title = {Accommodating toleration: on Balint's classical liberal response to the
   multiculturalism challenge},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2020},
Volume = {23},
Number = {2},
Pages = {212-218},
Month = {FEB 23},
Abstract = {Peter Balint identifies three challenges to toleration, one of which is
   the multiculturalism challenge. This is the charge that liberal
   toleration fails to accommodate minorities adequately, which requires
   positive recognition rather than negative toleration. I discuss his
   response to the multiculturalism challenge and its connection to a
   classical liberal view of toleration. This involves Balint's claim that
   liberal neutrality should be understood as reflective and
   `difference-sensitive', which should be realised by the state being
   `hands-off' in the sense of withdrawing support for privileged ways of
   life. I argue that Balint's classical liberal view that the state needs
   to be `hands-off' is in need of specification and that it does not fit
   well with his claim that neutrality needs to be reflective and
   difference-sensitive.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
