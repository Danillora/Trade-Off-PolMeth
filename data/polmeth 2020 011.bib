@article{ WOS:000589632200001,
Author = {Zamir, Sara},
Title = {The portrayal of the Oriental Jew in literary school anthologies: from
   the establishment of Israel until the present time},
Journal = {NATIONAL IDENTITIES},
Year = {2020},
Volume = {22},
Number = {5},
Pages = {519-532},
Abstract = {The aim of this research was to examine the narrative featuring oriental
   characters in the Sheaves school anthologies (1958-1966) in contrast to
   the Roots and Wings anthologies (2009). The findings show that in the
   Sheaves anthologies there are only three stories representing oriental
   characters. All three deal with the Zionist narrative of immigration to
   Israel. Two stories extol the `melting pot' immigration policy forcing
   immigrants to experience re-socialization by shedding their previous
   identity. However, in the Roots and Wings anthologies, there are six
   literary works representing the Oriental characters. Yet, two of the
   narratives perpetuate the oriental typecast.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
