@article{ WOS:000738803800025,
Author = {Onate y Zubia, Teresa},
Title = {Listening to Heidegger Today: The Jewish Question, the Greek Question,
   and the Spiritual Revolution of the Future Previous},
Journal = {PENSAMIENTO AL MARGEN},
Year = {2020},
Number = {SI},
Pages = {75-101},
Month = {MAR},
Abstract = {This writing intends to think critically about the overdetermination of
   our epochal period, that we could initially name as un-limited consumer
   capitalism with a financial and belicist base. For this purpose, we will
   interpret some especially difficult texts by Martin Heidegger, Aristotle
   and the ``pre-Socratic{''} thinkers. I am going to divide this
   contribution into three sections that collect different areas of
   research. In the first place the ``Jewish Question{''} in Martin
   Heidegger's thought, critically reproposed now by the publication of his
   Black Notebooks. The second issue will be the ``Greek Question{''}, a
   dimension to which I still think it necessary to return. I have called
   the third question ``the Return to the Previous Future{''}- otherwise it
   would not be a return-, where I interpret (in that transit) that we, the
   critical philosophers of the 21st century, are precisely in a Where in
   syncronic becoming, where spaces for articulation of the possible not
   given (or not received) are opened.},
