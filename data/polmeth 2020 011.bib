@article{ WOS:000512048700001,
Author = {Anisin, Alexei},
Title = {Exogenous events and media reporting of mass shootings},
Journal = {BEHAVIORAL SCIENCES OF TERRORISM AND POLITICAL AGGRESSION},
Year = {2021},
Volume = {13},
Number = {2},
Pages = {160-173},
Month = {APR 3},
Abstract = {A growing literature has revealed that not all mass shootings are
   covered equally by the media. Thus far, endogenous and contextual
   characteristics of mass shootings have been causally associated with
   heightened print and online news coverage such as offender identity,
   weapon usage, motivations, shooting location and fatality count. This
   study utilizes univariate logistic regression to investigate whether the
   occurrence of exogenous events can offset coverage of mass shootings as
   measured through article counts drawn from the New York Times counts on
   68 different cases. Results reveal that the odds of a given mass
