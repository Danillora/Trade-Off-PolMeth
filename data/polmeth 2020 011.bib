@article{ WOS:000680686700002,
Author = {Alexandrescu, Raluca},
Title = {ROMANIAN POLITICAL MODERNITIES AGAINST THE FRENCH POLITICAL MODEL DURING
   THE YEARS 1869-1879 (CONTEXTS, INFLUENCES, ACTORS)},
Journal = {ANALELE UNIVERSITATII BUCURESTI-STIINTE POLITICE},
Year = {2020},
Volume = {22},
Number = {1-2},
Pages = {19-34},
Abstract = {The decisive decade (1869-1879) was also reflected in the constitution
   of French-inspired political models abroad, including in South-East
   Europe. In Romania, ten years after the union of Wallachia and Moldavia
   in 1859, the reforms implemented by Prince Alexandru-Ioan Cuza under the
   strong inspiration of the regime of Napoleon III are indirectly starting
   to bear fruit. The French influence is noticed directly, in the wake of
   these reforms (even if the abdication of the prince in 1864 and the
   advent of the future King Charles I will slow down the process), but
   also indirectly, in the progressive structuring of a modem political
   system including through political discourse and the relationship to
   liberal democracy, in particular through the different forms of
   representation. The communication proposes, in the logic of the third
   axis mentioned, to question the modes and the rhythms of Romanian
   modernization in a decade where the actors of the new political regime
   put in place relate actively to the French political model.},
Publisher = {EDITURA UNIVERSITATII BUCURESTI - Bucharest Univ Press},
