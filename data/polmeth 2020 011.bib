@article{ WOS:000510024900017,
Author = {Anderson, Christopher J. and Getmansky, Anna and Hirsch-Hoefler, Sivan},
Title = {Burden Sharing: Income, Inequality and Willingness to Fight},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2020},
Volume = {50},
Number = {1},
Pages = {363-379},
Month = {JAN},
Abstract = {What explains citizens' willingness to fight for their country in times
   of war? Using six waves of the World Values Survey, this study finds
   that individual willingness to fight is negatively related with
   country-level income inequality. When income inequality is high, the
   rich are less willing to fight than the poor. When inequality is low,
   the poor and rich differ little in their willingness to fight. This
   change in the willingness to fight between low and high inequality
   countries is greater among the rich than among the poor. This article
   explores several explanations for these findings. The data are
   consistent with the argument that high inequality makes it more
   attractive for the rich to buy themselves out of military service.},
