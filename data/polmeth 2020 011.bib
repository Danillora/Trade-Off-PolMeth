@article{ WOS:000572612000001,
Author = {Simpson, Sid},
Title = {Nietzsche, irrationalism, and the cruel irony of Adorno and Horkheimer's
   political quietude},
Journal = {CONTEMPORARY POLITICAL THEORY},
Year = {2021},
Volume = {20},
Number = {3},
Pages = {481-501},
Month = {SEP},
Abstract = {Adorno and Horkheimer's legacy is incomplete without reference to their
   infamous political quietism. To thinkers such as Habermas, this was the
   unfortunate consequence of their alleged evacuation of reason. Attending
   to the treatment of Nietzsche inDialectic of Enlightenmentilluminates
   the distinct irony of such charges. Here, in their most popular book,
   Nietzsche is presented as precisely that which they praised him for
   warning against elsewhere: an advocate of cruelty animated by a
   reactionary morality. I contend that this exaggeration is not
   accidental, but rather illustrative; the authors present a consciously
   hyperbolized version of Nietzsche in order to articulate how he made
   possible his own misappropriation, and to distinguish themselves sharply
   from Nietzsche given their disagreements about the necessity of reason.
   Ultimately, however, even though Adorno and Horkheimer performatively
