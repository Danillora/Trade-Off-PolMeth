@article{ WOS:000581803100002,
Author = {Green, Marcus E.},
Title = {Political Imaginary of a Postcapitalist Climate},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2020},
Volume = {32},
Number = {4},
Pages = {412-416},
Abstract = {Jameson's oft-repeated remark that ``it is easier to imagine the end of
   the world than to imagine the end of capitalism{''} captures the
   mentality of the present conjuncture. The climate crisis is accompanied
   by a crisis of imagination in which it is difficult to conceive an
   alternative to carbon-emitting capitalism. In Climate Leviathan: A
   Political Theory of Our Planetary Future, Joel Wainwright and Geoff Mann
   envision the planetary future by examining the possible ways political
   and economic institutions may respond to climate change. For Wainwright
   and Mann, addressing climate change requires replacing liberal
   capitalism with a critical conception of the world that they label as
   Climate X, a postcapitalist, nonplanetary sovereign social formation.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
