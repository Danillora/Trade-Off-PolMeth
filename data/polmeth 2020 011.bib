@article{ WOS:000617374300008,
Author = {Depledge, Duncan},
Title = {NATO and the Arctic The Need for a New Approach},
Journal = {RUSI JOURNAL},
Year = {2020},
Volume = {165},
Number = {5-6},
Pages = {80-90},
Abstract = {With NATO activity increasing and great power competition building
   between the US, Russia and China across the Arctic, the Alliance needs
   to consider a new strategic approach to the region. Duncan Depledge
   examines how NATO's traditionally cautious approach to the Arctic is now
   outmoded given the recent pressures posed by Russian military activity,
   Chinese ambition and the renewal of US strategic interest in the region.
   In response, NATO must go beyond simply restating its interest in the
   Arctic and devise a new strategic approach that normalises Alliance
   engagement, keeps a weather eye on Chinese ambitions, and leaves scope
   for expanding Alliance activity still further as the region is
   transformed by environmental and geopolitical change.},
