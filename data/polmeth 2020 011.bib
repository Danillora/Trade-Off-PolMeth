@article{ WOS:000561456100001,
Author = {Thomas, Paul and Grossman, Michele and Christmann, Kris and Miah, Shamim},
Title = {Community reporting on violent extremism by ``intimates{''}: emergent
   findings from international evidence},
Journal = {CRITICAL STUDIES ON TERRORISM},
