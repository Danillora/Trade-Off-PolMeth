@article{ WOS:000584396900001,
Author = {Kumar, Deepak and Pratap, Bhanu and Aggarwal, Archana},
Title = {Public trust in state governments in India: Who are more confident and
   what makes them confident about the government?},
Journal = {ASIAN JOURNAL OF COMPARATIVE POLITICS},
Year = {2021},
Volume = {6},
Number = {2},
Pages = {154-174},
Month = {JUN},
Abstract = {Public trust in government is crucial for good governance, encompassing
   economic and social development of the region in a representative
   democratic setup. This study uses India Human Development Survey
   (2004-2005 and 2011-2012) data to examine the changing pattern of
   household confidence in state governments in India. Using a logit model
   analysis, we examine how the level of household confidence in the state
   government changes with households' socioeconomic status, personal
   experiences, and benefits received from government programs and direct
   social benefit schemes. We find that households with a low socioeconomic
   status (such as Scheduled Castes/Scheduled Tribes, and low-income and
   less educated households) are more likely to trust the state government.
   In addition, households that have had a positive experience or have
   received benefits from a government program and/or social scheme are
   more likely to exhibit high levels of trust in the state government. We
   also find that residents of less developed states are more likely to
   have high levels of trust in their state government than residents of
   highly developed states.},
