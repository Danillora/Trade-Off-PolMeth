@article{ WOS:000534514000001,
Author = {Elizondo, Silvana Laura},
Title = {Freedom of Navigation in Question: Emerging Global Challenges and the
   South American case},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
Year = {2020},
Number = {27},
Pages = {8-24},
Month = {MAY-AUG},
Abstract = {The United States established in 1979 the Freedom of Navigation Program,
   a diplomatic and military mechanism that challenges excessive maritime
   claims from coastal states, in order to reassure an interpretation of
   the Law of the Sea that would guarantee the mobility of its fleet. The
   purpose of this paper is to analyze the recent evolution of the Program
   and its application in South America. Documents from the US Departments
   of State and Defense, barely studied in the region, that define the US
   perspective of freedom of navigation are analyzed, as well as the
   supposed ``excessive claims{''} of the coastal countries and the
   evolution of the military operations associated with the Program,
   particularly in South America. The study concludes that since 2009 the
   US has shifted towards a less neutral application of the Program in
   South America as a response to the consolidation of southamerican
   regionalism, a phenomenon that we can see today in other scenarios such
   as the South China Sea.},
