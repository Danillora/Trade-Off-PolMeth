@article{ WOS:000510677400002,
Author = {Sandford, Mark},
Title = {Giving power away? The `de- words' and the downward transfer of power in
   mid-2010s England},
Journal = {REGIONAL AND FEDERAL STUDIES},
Year = {2020},
Volume = {30},
Number = {1},
Pages = {25-46},
Abstract = {Attempts to measure decentralization globally have frequently consisted
   of either quantitative measurement of `regional authority' or a more
   qualitative characterization of decentralization. The most frequent
   descriptions of the latter have come via the terms `deconcentration',
   `delegation' and `devolution'. These terms have been used in divergent
   ways by scholars but three broad approaches to decentralization
   processes can be synthesized from the literature. This article
   contributes to the literature by using the terms to assess the character
   of the `devolution deals' negotiated in England in the mid-2010s.
   Findings indicate that a number of substantive powers will be devolved
   as a result of the deals, but the bulk of the commitments are to joint
   working between the central and `regional' tiers of government.},
