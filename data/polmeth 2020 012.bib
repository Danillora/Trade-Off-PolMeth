@article{ WOS:000508706200002,
Author = {Zabler, Steffen and Reclam, Raphael and Grohs, Stephan},
Title = {Size and Democracy Revisited-Examining the Effect of County Size on
   Voting Behaviour in Germany},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2020},
Volume = {61},
Number = {1},
Pages = {87-110},
Month = {MAR},
Abstract = {The question of the connection between the size of local authorities and
   political participation is gaining renewed attention against a backdrop
   of the failed county reforms in the German federal states (Bundeslander)
   of Thuringia and Brandenburg. Particular emphasis has been placed on
   work that assumes that in larger counties (Landkreise) political
   integration will decrease and that this will have a negative influence
   on participation - especially in elections - even though neither theory
   nor previous empirical evidence provides a clear indication for this.
   Based on structural data covering all German counties, and with the help
   of a multilevel regression analysis, there was no empirical effect of
   this kind for the 2017 Bundestag election: controlling for additional
   factors, larger counties do not face lower turnout rates or a higher
   share of right-wing populist party votes. Any connections between the
   size of counties and voting behaviour are driven far more by systematic
   differences between the German federal states than by the size of the
   counties themselves. This study makes an important and extremely
   practical contribution to the objectification of the discussion of local
   authority reforms.},
