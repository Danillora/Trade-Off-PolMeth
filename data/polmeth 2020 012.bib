@article{ WOS:000568447000001,
Author = {Rainer, Elise and Goel, Anish},
Title = {Self-Inflicted Instability: Myanmar and the Interlinkage between Human
   Rights, Democracy and Global Security},
Journal = {DEMOCRACY \& SECURITY},
Year = {2020},
Volume = {16},
Number = {4},
Pages = {334-350},
Month = {OCT 1},
Abstract = {This article examines the case of Myanmar, specifically, how the
   country's domestic human rights record and lack of democratic structure
   has been the root cause of its economic and political instability. The
   systematic and genocidal persecution of the country's Rohingya
   population and denial of their democratic rights has become the central
   human rights issue upon which the international community defines
   Myanmar's reputation. The article identifies how Myanmar's actions have
   directly impacted its national security in three ways: internal
   insecurity, regional insecurity, and economic insecurity. Myanmar is a
   case study that demonstrates how ethnic conflict and democratic erosion
   threatens the legitimacy of the nation and hinders the realization of a
   stable country.},
