@article{ WOS:000599406600001,
Author = {Meka, Eltion and Kalemaj, Ilir},
Title = {Critics of Democracy: The Impact of Communist Legacy on Support for
   Democracy and Voting Behavior in Albania},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2022},
Volume = {69},
Number = {6, SI},
Pages = {486-497},
Month = {NOV 2},
Abstract = {We use the case of Albania to explore the effects of communist legacies
   on support for democracy and voting behavior. Using survey data, we show
   that those who are more critical of democracy tend to be of the older
   generation and reside in rural areas. These critics of democracy are
   also more likely to vote for Albania's communist successor party. We
   argue that these trends are a result of communist legacy and continually
   undermine the new regime's capacities to develop effective
   representation. In conclusion, we also show how communist legacy more
   generally undermines the development of robust political competition.},
