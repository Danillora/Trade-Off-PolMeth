@article{ WOS:000614864700069,
Author = {Pendola Ramirez, Patricia},
Title = {Goa and Chile united by the art of hope. Anticolonialism and
   postcolonialism in Butamalon and O ultimo olhar de Manu
   Miranda},
Journal = {IZQUIERDAS},
Year = {2020},
Volume = {49},
Pages = {4425-4448},
Month = {JAN},
Abstract = {This article connects two novels from different cultural and
   geographical origins: O ultimo olhar de Manu Miranda (2000) by
   mozambican Orlando Da Costa and Butamalon (1994) by chilean Eduardo
   Labarca. Through postcolonial and anticolonial theories, and using
   strategies from Comparative Literature, it is proposed that the corpus
   shows a remarcable political tendency, revealing the practices of a
   typical masked violence of colonialism and civic-military dictatorships.
   This research rereads in between the lines and against the grain the
   stamp of the authoritarian regimes that have governed and subjected not
   only Goeses but also Chileans and Mapuches.},
