@article{ WOS:000562028600001,
Author = {Coan, Travis G. and Merolla, Jennifer L. and Zechmeister, Elizabeth J.
   and Zizumbo-Colunga, Daniel},
Title = {Emotional Responses Shape the Substance of Information Seeking under
   Conditions of Threat},
Journal = {POLITICAL RESEARCH QUARTERLY},
