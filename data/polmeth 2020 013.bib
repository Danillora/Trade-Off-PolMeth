@article{ WOS:000561159900001,
Author = {Sanaullah},
Title = {Effectiveness of civilians' survival strategies: insights from the
   Taliban's insurgency (2007-09) in Swat Valley, Pakistan},
Journal = {GLOBAL CHANGE PEACE \& SECURITY},
Year = {2020},
Volume = {32},
Number = {3},
Pages = {275-296},
Month = {SEP 1},
Abstract = {Civilians deploy various strategies to survive violence in armed
   conflicts; literature, however, does not debate the relative
   effectiveness of such strategies. This article answers how certain
   strategies are valued and are employed frequently in comparison to
   others. Based on qualitative fieldwork in Swat Valley undertaken in 2016
   to 2019, where Taliban insurgents fought against the State of Pakistan
   in 2007-09, the research finds that civilians deployed resistance,
   accommodation, resolution, readjustment and withdrawal to survive. It
   further finds that as violence intensifies, strategies of resistance and
   accommodation give way to strategies of readjustment and withdrawal.},
