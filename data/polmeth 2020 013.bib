@article{ WOS:000614864700065,
Author = {Manuel Zeballos, Juan},
Title = {Bolivians in Cordoba, Argentina: an inferiorization product of three
   registers},
Journal = {IZQUIERDAS},
Year = {2020},
Volume = {49},
Pages = {4341-4361},
Month = {JAN},
Abstract = {This is an article about certain inferiorizing ideological tools of a
   capitalist society, which are hidden under the forms of stigma and
   discrimination practices. The case study focuses on those who have a
   bolivian origin are settled in the city of Cordoba, Argentina. There are
   three hypotheses to check. Bolivian workers suffer an inferiorization
   based on three factors that amalgamate with each other: nacionality,
   corporality and social class; the perception of these components is
   differential for those who suffer from them since, although they
   recognize xenophobia and biologicism, stigmatization by social class is
   not always appreciated; and although they reject the firs two, sometimes
   they reproduce the last one.},
