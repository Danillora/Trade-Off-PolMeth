Author = {Duarte Herrera, Lisbeth Katherine and Pedraza Beleno, Jose Adolfo},
Title = {The urban conflict in Medellin: violence during the negotiation process
   of justice and peace (2003-2006)},
Journal = {IZQUIERDAS},
Year = {2020},
