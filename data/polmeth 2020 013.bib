@article{ WOS:000531481500006,
Author = {Brodsky, Marcelo and Kayatekin, Serap A.},
Title = {When Personal Becomes Political},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2020},
Volume = {32},
Number = {2},
Pages = {229-247},
Abstract = {The work is a construction of personal and social history through an
   interpretation of selected works of Marcelo Brodsky from his albums
   Buena Memoria, Nexo, 1968: The Fire of Ideas, and Migrants. The text
   weaves a narrative of how the personal and political histories are
   enmeshed following through the migration of the Brodsky family to
   Argentina, the global economic and political crisis of 1968, the immense
   personal loss during the state terror unleashed in 1976 with the
   military coup and its aftermath. Personal suffering becomes a source of
   political action in the works of the artist who through an
   internationalist lens builds bridges across countries and cultures.},
