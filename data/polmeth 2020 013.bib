@article{ WOS:000525218900004,
Author = {Argote, Pablo},
Title = {Incumbency advantage and shocks in the electorate: the adoption of
   voluntary voting},
Journal = {EUROPEAN POLITICAL SCIENCE REVIEW},
Year = {2020},
Volume = {12},
Number = {2},
Pages = {173-197},
Month = {MAY},
Abstract = {Although the advantage that incumbents may have over challengers has
   been extensively studied, less is known about how incumbency advantage
   is affected by the adoption of voluntary voting. In this paper, I study
   incumbency advantage in Chile, a country that adopted voluntary voting
   in 2012. I find that incumbency advantage substantively decreases with
   voluntary voting. The primary mechanism explaining such reduction was
   the entry of high-quality challengers, who invested their campaign
   resources much more efficiently compared to office-holders. Overall,
   this paper contributes to the literature on electoral accountability by
   identifying how sensible is incumbency advantage to voluntary voting and
   the entry of high-quality challengers. Indeed, as incumbency advantage
   decreases when capable challengers compete for office, it suggests that
   ``scaring-off{''} skilled candidates is one of the drivers of such
   advantage.},
