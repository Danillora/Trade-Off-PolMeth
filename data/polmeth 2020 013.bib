@article{ WOS:000505966500001,
Author = {Lenard, Patti Tamara},
Title = {Free to be you and me: an introduction to Ghosh's De-Moralizing Gay
   Rights},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2021},
Volume = {24},
Number = {7},
Pages = {1048-1055},
Month = {NOV 10},
Abstract = {Although he does not characterize it this way himself, Cyril Ghosh's
   book De-moralizing Gay Rights is a book about freedom. Told from within
   the domain of queer theory, and constructed as a `polemical'
   interruption into three apparently distinct conversations taking place
   therein, Ghosh offers readers deep insight into how best to understand
   what it is to be free in democratic states and what rights should attend
   this freedom. In this symposium introduction, I situate Ghosh's three
   critical interventions into key political theoretic debates. First, I
   examine Ghosh's treatment of `identity', including his critique of the
   multiple pressures to conform faced by those whose identity is situated
   within the boundaries of `queer'; second, I situate Ghosh's response to
   marriage equality in the larger theoretical conversation that examines
   the value of marriage; and third, I consider the implied recommendations
   Ghosh offers to queer allies.},
