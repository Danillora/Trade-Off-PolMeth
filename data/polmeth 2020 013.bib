@article{ WOS:000578368200001,
Author = {Michelsen, Nicholas},
Title = {What is a minor international theory? On the limits of `Critical
   International Relations'},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
Year = {2021},
Volume = {17},
Number = {3},
Pages = {488-511},
Month = {OCT},
Abstract = {This article argues that `Critical International Relations', often
   counterpoised to `mainstream IR', has come to function as a major
   theoretical category in its own right. It argues that critique involves
   `minor theorising', defined as the practice of disturbing settled
   theoretical assumptions in the discipline. The article examines the role
   and significance of `minor theories' in the context of ongoing debates
   about Critical IR. It argues that critique is defined by context, and is
   politically and ethically ambiguous. The article concludes that the
   scope for critique could be advanced if the terms `Critical IR' and
   `Critical IR Scholar' are dropped from scholarly parlance.},
