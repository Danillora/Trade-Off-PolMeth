@article{ WOS:000506625200008,
Author = {Jones, Peter},
Title = {Individual integrity, freedom of association and religious exemption},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2020},
Volume = {23},
Number = {1, SI},
Pages = {94-108},
Month = {JAN 2},
Abstract = {Of the many questions Cecile Laborde addresses in her magisterial
   Liberalism's Religion, several relate to what she describes as `the
   puzzle of exemptions'. I examine some of the issues raised by her
   efforts to solve that puzzle: whether her ideal of moral integrity
   squares with the nature of religious belief; whether we should find the
   case for collective religious exemptions in freedom of association and
   the `coherence interests' of associations; how much significance we
   should give to the `competence interests' of organised religions; and by
   which criteria we should assess individual claims to religious
   exemption.},
