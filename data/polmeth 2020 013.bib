@article{ WOS:000569443700001,
Author = {Keil, Soeren},
Title = {Imposed unions and imperfect states: the State Union of
   Serbia-Montenegro and Bosnia and Herzegovina in comparative perspective},
Journal = {IRISH POLITICAL STUDIES},
Year = {2020},
Volume = {35},
Number = {3, SI},
Pages = {473-491},
Month = {JUL 2},
Abstract = {This paper explores the political conflicts generated by imposed unions
   and the unionist actors (international and domestic) that defend them.
   It shows how the imposed political institutions symbolise directly or
   indirectly not only unionism in these states but also the resulting
   contestation over the statehood of these unions. It takes as its
   examples the State Unions of Serbia and Montenegro and of Bosnia and
   Herzegovina. Building on the existing literature the paper examines the
   links between the imposed nature of these two states and the
   continuation of state contestation. It assesses how contestation becomes
   an integral part of a political system, undermining decision-making
   procedures and making the union completely dysfunctional. It asks how
   this dysfunctionality might be overcome. Finally, the paper addresses
   the comparative significance of these cases for partially functional
   unions, like Northern Ireland.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
