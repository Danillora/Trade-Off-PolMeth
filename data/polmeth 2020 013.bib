@article{ WOS:000598452400001,
Author = {Mishra, Jyoti and Attri, Vibha},
Title = {Governance, Public Service Delivery and Trust in Government},
Journal = {STUDIES IN INDIAN POLITICS},
Year = {2020},
Volume = {8},
Number = {2},
Pages = {186-202},
Month = {DEC},
Abstract = {Several studies in the past show that the work done by the incumbent
   government helps the ruling party to get re-elected. However, most of
   these studies focus on re-election and do not look at the impact
   assessment of governance on trust in government, which is a precursor of
   re-election. To fill this gap, this article explores whether perceptions
   of good governance lead to trust in government. The results support the
   view that governance at the state level leads to a higher trust with the
   state government. Of all the variables used to measure governance as
   access, benefitting from welfare schemes had the maximum impact on
   trust. Other governance measures like availing public services and
   citizens' interaction with the state too had a positive relationship
   with trust in the state government. Furthermore, having the same
   government at the centre and the state, strengthened one's trust in the
   state government.},
