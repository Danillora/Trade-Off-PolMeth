@article{ WOS:000553299800001,
Author = {Kulaev, Maksim},
Title = {Russian Trade Unions and the De-Escalation of Protests in Russian
   Regions},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
Year = {2022},
Volume = {35},
Number = {2},
Pages = {201-220},
Month = {JUN},
Abstract = {The article addresses the issues of social and labor protests in Russia
   and demonstrates how protests led by trade unions result in mitigation
   of conflicts and compromises with the authorities. The analysis focuses
   on activities of trade unions and their relations with the local
   authorities and employers at grassroots level. The research is based on
   in-depth interviews with leaders and activists of the largest trade
   union federation in three North Western regions of Russia, namely
   Leningrad Oblast, Pskov Oblast, and Karelia. The analysis of social and
   labor protests in these regions contributes to the understanding of
   relations among trade unions, the local authorities, and employers at
   the regional level.},
