@article{ WOS:000555081700001,
Author = {Vieira, Monica Brito},
Title = {The great wall of silence: voice-silence dynamics in authoritarian
   regimes},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2021},
Volume = {24},
Number = {3, SI},
Pages = {368-391},
Month = {APR 16},
Abstract = {How does the voice-silence dynamics affect the durability of
   authoritarian regimes? This article reformulates Hirschman's voice,
   loyalty, exit model to answer this question. It demonstrates that the
   model's heuristic value is significantly hampered by conceptual
   imprecision around the category of voice, a narrow understanding of
   exit, and - in particular - the neglect of the category of silence. Once
   these categories are conceptually reworked, and silence is placed next
   to voice and exit - as a core concept, not a residual category, in the
   model - the `dictator's dilemma' emerges as a `silence paradox' hinting
   at some of authoritarianism's main vulnerabilities. The case of China is
   used to illustrate the article's key theoretical-conceptual
   advancements.},
