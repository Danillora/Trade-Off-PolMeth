Author = {Do, Minh},
Title = {Throughput Legitimacy and the Duty to Consult: The Limits of the Law to
   Produce Quality Interactions in British Columbia's EA Process},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2020},
Volume = {53},
Number = {3},
Pages = {618-637},
Month = {SEP},
Abstract = {The duty to consult mandates that the Crown must consult affected
   Indigenous parties when Crown action may negatively impact Aboriginal
   rights or title claims. The Supreme Court of Canada (SCC) has emphasized
   that the duty should be characterized by honourable dealings and good
   faith negotiations. This article argues that the concept of throughput
   legitimacy can help evaluate the Crown's conduct in consultation. By
   analyzing 131 British Columbia Environmental Assessments (BC EAs), this
   article finds that the Crown struggles to uphold throughput legitimacy
   from the perspective of Indigenous peoples, particularly in the areas of
   transparency, accountability and effectiveness.},
