@article{ WOS:000605027900004,
Author = {Gonzalez Tule, Luis},
Title = {Vote buying in Colombia: How does the ghost dress and what are its
   implications?},
Journal = {REFLEXION POLITICA},
Year = {2020},
Volume = {22},
Number = {46},
Pages = {44-57},
Month = {DEC},
Abstract = {Although vote buying is a common practice in Colombian elections, there
   is little research that explores how networks operate and what their
   effects are on democratic institutions and the well-being of the
   population. This paper describes the operation of a vote buying network,
   based on a highly relevant case study occurred in Colombia, and discuss
   some of the implications of this practice. I argue that vote buying not
   only constitutes two electoral crimes, but impacts on political
   representation, elections, and deepens social inequalities. The case
   shows that vote buying affects especially the poorest, they are,
   paradoxically, the most likely to participate in a transaction.},
