@article{ WOS:000530286800001,
Author = {Goren, Paul and Smith, Brianna and Motta, Matthew},
Title = {Human Values and Sophistication Interaction Theory},
Journal = {POLITICAL BEHAVIOR},
Year = {2022},
Volume = {44},
Number = {1},
Pages = {49-73},
Month = {MAR},
Abstract = {Political sophistication systematically affects the structure,
   crystallization, and use of political values, but it remains unclear if
   sophistication manifests similar effects on human values. This paper
   integrates Shalom Schwartz (Adv Exp Soc Psychol 25:1-65, 1992, J Soc
   Issues 50:19-45, 1994) theory of human values with sophistication
   interaction theory to examine the degree to which education and
   political interest condition the structure, crystallization, and use of
   an important subset of values. We theorize that human values are (1)
   identically structured and equally crystallized in
   sophistication-stratified populations and (2) that relationships between
   human values and ideological judgments grow stronger at higher levels of
   sophistication. Using data from a nationally representative sample of
   10,765 Americans, we compare extremely sophisticated individuals (e.g.,
   people with doctorates) and extremely unsophisticated individuals (e.g.,
   high school dropouts) to demonstrate that neither education nor
   political interest affect value structure and crystallization.
   Sophistication has real, if somewhat limited, effects on value usage.},
