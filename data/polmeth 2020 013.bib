@article{ WOS:000614864700002,
Author = {Fajardo, Christian},
Title = {The common in the first volume of Karl Marx's Capital},
Journal = {IZQUIERDAS},
Year = {2020},
Volume = {49},
Pages = {3005-3023},
Month = {JAN},
Abstract = {This paper seeks to actualize the contemporary relevance of a thought
   about the common and the contingency in the third and fourth sections of
   the first volume of Karl Marx's Capital. Following this horizon, the
   text makes an exegesis of the epilogue to the second edition of the
   aforementioned book to show that Marx's conceptualization of the common
   has as its horizon not the necessity of the human bond, but the
   contingency of the social relations. Accordingly, in the second and
   third part an analysis is made of how capitalism captures that
   contingency of social relations through the violence involved in the
   production of surplus value.},
