@article{ WOS:000582598700005,
Author = {Fernandez, Victor Ramiro and Moretti, Luciano},
Title = {A New World System from the Global South: Great Convergence and
   Accelerated Geographic Displacement},
Journal = {GEOPOLITICAS-REVISTA DE ESTUDIOS SOBRE ESPACIO Y PODER},
Year = {2020},
Volume = {11},
Number = {2},
Pages = {313-344},
Abstract = {Since the beginning of the new century and under the transformations
   unfolded in the last 50 years from the cyclical crisis of capitalism, a
   growing spatial displacement of the accumulation process towards the
   Global South (GS) takes place. Taking the latter as the unit of analysis
   (and observing in its interior the interrelation of the macro regions
   that compose it), this displacement is analyzed through variables
   associated with its dynamics, dynamism and growing capacity to control
   the production chains. global. Inserting all of this within the
   framework of the demographic explosion of the SG, the results show an
   unprecedented alteration of the hierarchical structure of capitalism
   since its birth, while opening a new map for the examination of
   hierarchies and their functioning with respect to of the usual
   analytical scheme (center - semi-periphery - periphery) diffused by the
   theory of the world system. A redefinition of the analytical and
   strategic agenda depends on it at the global geopolitical and
   geoeconomic level.},
