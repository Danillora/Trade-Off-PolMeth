@article{ WOS:000614864700035,
Author = {Belen Morris, Maria},
Title = {The itineraries of the Argentinian Workers' Central during kirchnerist
   governments: autonomous, politically independent or neutral?},
Journal = {IZQUIERDAS},
Year = {2020},
