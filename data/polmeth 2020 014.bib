@article{ WOS:000540176300002,
Author = {Sipma, Take and Lubbers, Marcel},
Title = {Contextual-level unemployment and support for radical-right parties: a
   meta-analysis},
Journal = {ACTA POLITICA},
Year = {2020},
Volume = {55},
Number = {3},
Pages = {351-387},
Month = {JUL},
Abstract = {The contextual-level unemployment rate is often adduced to explain
   radical right support. It has been assessed before that research
   findings are mixed, but it is unknown why the association is so
   different across studies. Our meta-analysis examined 162 effects from 49
   studies, and indicated a positive overall effect, as predicted by main
   theories, but it was rather small. The positive effect was predominantly
   found among studies that theorized the effect, possibly indicating
   publication bias. The effect was positive in Western and Eastern Europe,
   but absent in Northern Europe. The positive effect was furthermore
   evident only after 2008, when the economic crisis hit Europe. Findings
   on the effect of unemployment being dependent on immigration were mixed
   as well. Our study calls for more comprehensive studies that bypass the
   focus on the main effect of unemployment and extend theorizing about the
   conditions under which unemployment affects support for the radical
   right.},
