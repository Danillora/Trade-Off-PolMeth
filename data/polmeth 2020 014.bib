@article{ WOS:000614864700070,
Author = {Margarucci, Ivanna and Luis Hernandez, Juan},
Title = {The Bolivian lefts in the ``pre-52{''}. A historiographic balance},
Journal = {IZQUIERDAS},
Year = {2020},
Volume = {49},
Pages = {4449-4478},
Month = {JAN},
Abstract = {Socialism and communism, anarchism and Trotskyism played a leading role
   in Bolivia's social and political history in the decades before the
   April 1952 Revolution. The present article attempts to recover that
   history, through a historiographic balance in which we critically
   analyze the existing bibliography in Bolivia and abroad on these issues.
   One of our main hypotheses is that the historical development that these
   different left wing expressions had in the Bolivia ``pre-52{''}, exceeds
   what militant and academic historiography has produced about them. Thus,
   in the article we concentrate on the reasons behind this imbalance
   between history and historiography and we advance in a historiographic
   path that allows us to weigh the contributions made and the
   interpretative lines developed until the present, identify the debates
   between authors and currents, and detect unexplored topics and problems
   that deserve to be incorporated in the agenda of future research.},
