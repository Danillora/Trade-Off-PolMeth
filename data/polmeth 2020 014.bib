@incollection{ WOS:000544817900005,
Author = {Galdieri, Christopher J.},
Book-Author = {Galdieri, CJ},
Title = {The 2020 New Hampshire Primary: A Look Ahead},
Booktitle = {DONALD TRUMP AND NEW HAMPSHIRE POLITICS},
Series = {Palgrave Studies in US Elections},
Year = {2020},
Pages = {97-122},
Abstract = {Why are two dozen Democratic candidates, ranging from a former vice
   president and well-known senators to obscure House members and a
   small-city mayor, all seeking the Democratic nomination for president in
   2020? The size of the Democratic field suggests that the formal and
   informal mechanisms that kept earlier years' fields to reasonable sizes
   have broken down, in part because party leaders want to avoid the
   appearance of favoring some candidates over others. Debate criteria may
   serve to winnow the field. Meanwhile, after years of speculation, only
   former Massachusetts governor William Weld has stepped forward to
   challenge Trump in the Republican primary.},
