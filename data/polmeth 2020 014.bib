@article{ WOS:000564149400002,
Author = {Martinez Meucci, Miguel Angel and Alfaro Pareja, Francisco},
Title = {Conflict Asymmetry and Alternative Mechanisms of Dialogue and
   Negotiation in Venezuela (2002-2019)},
Journal = {AMERICA LATINA HOY-REVISTA DE CIENCIAS SOCIALES},
Year = {2020},
Volume = {85},
Pages = {9-29},
Month = {AUG},
Abstract = {Over the past twenty years, political conflict in Venezuela has led to
   the implementation of at least five alternative mechanisms for dialogue
   and negotiation with international participation. Their results have
   been limited and none have achieved any positive, sustainable
   transformation of the conflict. This article argues that the asymmetry
   between the opposing coalitions has been a determining, and generally
   underestimated, factor in the implementation of such mechanisms.},
