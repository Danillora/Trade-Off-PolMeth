@article{ WOS:000596130100001,
Author = {Hobolt, Sara B. and Hoerner, Julian M. and Rodon, Toni},
Title = {Having a say or getting your way? Political choice and satisfaction with
   democracy},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
Year = {2021},
Volume = {60},
Number = {4},
Pages = {854-873},
Month = {NOV},
Abstract = {Citizen satisfaction with democracy is greater when parties offer
   choices that are congruent with voter preferences. But are citizens
   content with simply having a party that represents their views or does
   their satisfaction depend on whether that party can also be instrumental
   in implementing policies? We argue that instrumentality moderates the
   effect of ideological congruence on democratic satisfaction. Combining
   an analysis of cross-national survey data with an experimental conjoint
   design, we find that citizens able to vote for a congruent party with a
   chance of entering government are more satisfied with democracy, whereas
   congruence without instrumentality has no such effect.},
