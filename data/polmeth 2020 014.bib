@article{ WOS:000545809200005,
Author = {Junisbai, Barbara and Junisbai, Azamat},
Title = {Are Youth Different? The Nazarbayev Generation and Public Opinion in
   Kazakhstan},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2020},
Volume = {67},
Number = {3, SI},
Pages = {251-263},
Month = {MAY 3},
Abstract = {Analyses of the political and economic attitudes of the ``Nazarbayev
   generation{''} (aged 18-29) reveal the ways that young Kazakhstanis are
   distinct from older generations. Young people are more accepting of
   material inequality and less likely to make demands on the welfare state
   in favor of the poor and unemployed. They are also less likely to be
   troubled by nepotism and family rule. When compared to those who were
