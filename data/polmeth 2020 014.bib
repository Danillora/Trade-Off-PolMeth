@article{ WOS:000645219300004,
Author = {McCrory, Duncan},
Title = {Russian Electronic Warfare, Cyber and Information Operations in Ukraine
   Implications for NATO and Security in the Baltic States},
Journal = {RUSI JOURNAL},
Year = {2020},
Volume = {165},
Number = {7},
Pages = {34-44},
Abstract = {NATO is more dependent than ever on electronic systems which are being
   targeted by increasingly sophisticated electronic warfare (EW), cyber
   and information operations. Russia has demonstrated considerable
   proficiency across these domains, in particular during the crisis in
   Ukraine. Russia is now reported to be mounting a sustained information
   operations campaign in the Baltic states and there is concern regarding
   the potential impact of its EW and cyber capabilities on the operational
   effectiveness and survivability of NATO forces, should there ever be a
   confrontation. Duncan McCrory highlights some of the challenges facing
   the Alliance in these domains to raise awareness among the non-technical
   specialist community and provide options to enhance NATO's resilience
   and counter this evolving threat.},
