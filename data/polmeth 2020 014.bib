@incollection{ WOS:000614246300005,
Author = {Emerson, Peter},
Book-Author = {Emerson, P},
Title = {Governance: From Power-dividing to Power-sharing},
Booktitle = {MAJORITY VOTING AS A CATALYST OF POPULISM: PREFERENTIAL DECISION-MAKING
   FOR AN INCLUSIVE DEMOCRACY},
Year = {2020},
Pages = {53-73},
Abstract = {As noted, many people believe in a form of majority rule which is based
   on majority voting. But if the (simple or weighted) majority vote is
   inadequate, and if the modified Borda count, MBC, is known to be more
   precise, democracy would presumably be enhanced if the latter were
   adopted as the international norm for democratic decision-making. As
   shown in Chap. 1, this MBC is indeed robust, inclusive and accurate; but
   it is also non-majoritarian. If therefore it were to be introduced, that
   which follows from majority voting-the crudest form of majority
   rule-should also be reformed. Thus the basis of democratic governance,
   everywhere, could be that which is often advocated for post-conflict
   societies: all-party power-sharing. Easier said than done, some might
   say; after all, many a so-called stable democracy sometimes has great
   difficulty in forming a new, post-election administration, even only a
   majority coalition let alone an all-party executive. With a matrix vote,
   however, it can be both said and done quite easily. This Chapter
   critiques current practice and (therefore) proposes an alternative.},
