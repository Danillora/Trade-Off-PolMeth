@article{ WOS:000595330300009,
Author = {Thibault, Simon and Bastien, Frederick and Gosselin, Tania and Brin,
   Colette and Scott, Colin},
Title = {Lowering the Voting Age to 16? A Comparative Study on the Political
   Competence and Engagement of Underage and Adult Youth},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2020},
Volume = {53},
Number = {3},
Pages = {638-657},
Month = {SEP},
Abstract = {One reform considered for increasing voter turnout rates is to lower the
   voting age to 16 years old. Advocates of such a reform argue that young
   people would vote for the first time while they are still in school and
   living with their parents, which would provide a social context that is
   supportive of their electoral participation. However, opponents argue
   that 16- and 17-year-olds are not mature enough to take part in
   elections. Using data from a 2018 Quebec election survey that included a
   subsample of individuals aged 16 and 17, this study provides mixed
   evidence for both arguments.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {EDINBURGH BLDG, SHAFTESBURY RD, CB2 8RU CAMBRIDGE, ENGLAND},
