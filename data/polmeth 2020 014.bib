@article{ WOS:000524734500003,
Author = {Mikhailina, Tetiana and Grynyuk, Roman},
Title = {THE SPREAD OF SHADOW NORMS BEYOND STATE LEGAL SYSTEM: CHALLENGES OF A
   GLOBALIZED SOCIETY},
Journal = {EUROPEAN JOURNAL OF TRANSFORMATION STUDIES},
Year = {2020},
Volume = {8},
Number = {1},
Pages = {21-32},
Abstract = {The article analyzes the functioning and distribution of shadow norms in
   the globalized world. Shadow rules are classified according to their
   scope and degree of danger. Legal and social mechanisms of counteraction
   to shadow norms are considered. It is concluded that the mechanisms of
   combating shadow norms in a globalized society of different degrees of
   danger differ diametrically. Thus, in order to combat shadow norms that
   exist before or in parallel with legal norms, it is advisable to use
   social mechanisms. But, measures of legal responsibility should be
   strengthened with regard to illegal shadow norms.},
