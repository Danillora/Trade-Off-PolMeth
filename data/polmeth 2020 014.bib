@article{ WOS:000614931400020,
Author = {Fischer, Sabine and Kluge, Janis and Sahm, Astrid},
Title = {Sovereignty, subordination, integration Key issues between Moscow and
   Minsk},
Journal = {OSTEUROPA},
Year = {2020},
Volume = {70},
Number = {10-11},
Pages = {291+},
Abstract = {The key to solving the political crisis in Belarus lies in Moscow. The
   Kremlin is not only pursuing geopolitical interests, but also fears that
   there may be fallout from the Belarusian protests that will affect the
   domestic situation in Russia. For this reason, the Russian government is
   supporting Aliaksandr Lukashenka, the Belarusian head of state of many
   years, in his attempt to hold on to power. In return, Lukashenka is
   performing a U-turn with regard to integration policy and is abandoning
   his ``see-saw policy{''} between the EU and Russia. If he succeeds in
   forming common supranational institutions, Vladimir Putin would finally
   have achieved the aim of his Belarus policy after two decades. However,
   it remains unclear whether his tactics will actually pay off.},
