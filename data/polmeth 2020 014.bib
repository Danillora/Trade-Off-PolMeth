@article{ WOS:000605946600002,
Author = {Ahonen, Anneli},
Title = {THE EU' S RESPONSE TO FOREIGN DISINFORMATION},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2020},
Volume = {19},
Number = {3},
Pages = {11-16},
Month = {FAL},
Abstract = {As foreign actors' disinformation campaigns continue to increase, it is
   now more important than ever to ensure factual information is not only
   available to, but also reaches citizens. In 2015, in response to this
   emerging threat, the East StratCom Task Force was established under the
   European External Action Service. By detecting, analyzing, and exposing
   malign influence operations-especially Russia's manipulative
   interferences the Task Force has been countering disinformation and
   supporting EU's wider communication efforts in its Eastern neighborhood.
   This article sheds light on the Task Force's responses in the cases of
   Ukraine and Belarus, while underlining the vital role of democracies and
   democratic institutions in addressing the threat of foreign
   disinformation.},
