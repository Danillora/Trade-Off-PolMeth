@article{ WOS:000549250300011,
Author = {Castillo Rocha, Carmen and Montero Mendoza, Marcela},
Title = {Nonviolent Communication at interpersonal conflict: A design of a
   reflexive scale},
Journal = {REVISTA DE PAZ Y CONFLICTOS},
Year = {2020},
Volume = {13},
Number = {1},
Pages = {255-274},
Abstract = {In this paper, we consider conflict as an interactive process in a
   specific context capable of transformation (Lederach, 2007). We start
   explaining the relationship between conflict and the culture of peace,
   emphasizing the role of communication in this transformation,
   particularly under Marshall Rosenberg's proposal regarding Nonviolent
   Communication. In this framework, the objective of this research was to
   explore the personal reflection of youth people after they resolve the
