@article{ WOS:000561919400001,
Author = {Alexander, Jeffrey C.},
Title = {Civil Sphere and Transitions to Peace: Cultural Trauma and Civil Repair},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
Year = {2022},
Volume = {35},
Number = {1},
Pages = {85-93},
Month = {MAR},
Abstract = {What are the conditions for establishing solidarity after a period of
   intensive and divisive social conflict-what Kant called a cosmopolitan
   constitution? In this essay, I argue that such a widened solidarity
   depends on establishing a relatively independent civil sphere, the
   effective functioning of whose institutions depend, in turn, on a shared
   sacred discourse of civility. To speak such a shared language, however,
   requires much more than engaging in speech acts. It depends upon a
   deeply emotional and highly symbolic process, one in which public
   performances of reconciliation create new structures of feeling and
   identification. This theoretical argument is elaborated empirically with
   reference to post-Holocaust Germany, post-Franco Spain, and
   post-Apartheid South Africa.},
