@article{ WOS:000528000400010,
Author = {Goel, Sharad and Meredith, Marc and Morse, Michael and Rothschild, David
   and Shirani-Mehr, Houshmand},
Title = {One Person, One Vote: Estimating the Prevalence of Double Voting in US
   Presidential Elections},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2020},
Volume = {114},
Number = {2},
Pages = {456-469},
Month = {MAY},
Abstract = {Beliefs about the incidence of voter fraud inform how people view the
   trade-off between electoral integrity and voter accessibility. To better
   inform such beliefs about the rate of double voting, we develop and
   apply a method to estimate how many people voted twice in the 2012
   presidential election. We estimate that about one in 4,000 voters cast
   two ballots, although an audit suggests that the true rate may be lower
   due to small errors in electronic vote records. We corroborate our
   estimates and extend our analysis using data from a subset of states
   that share social security numbers, making it easier to quantify who may
   have voted twice. For this subset of states, we find that one suggested
   strategy to reduce double voting-removing the registration with an
   earlier registration date when two share the same name and
   birthdate-could impede approximately 300 legitimate votes for each
   double vote prevented.},
