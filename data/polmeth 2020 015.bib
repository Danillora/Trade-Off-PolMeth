@article{ WOS:000583727600002,
Author = {Sio-Lopez, Cristina Blanco},
Title = {Schengen as a Political Territory: Sources of Differentiation in the
   EU's `Free Movement of Persons' from 1985},
Journal = {POLITIQUE EUROPEENNE},
Year = {2020},
Volume = {67-68},
Number = {1-2},
Pages = {26-52},
Abstract = {This article analyses the historical dimensions of differentiation as
   part of the inter-institutional discussions to build the EU's free
   movement of persons (FMP). In this sense, it particularly highlights the
   critical role of the European Parliament (EP) within this remit from the
   Schengen Area's inception in 1985 to 2015, going through the most
   impactful post-Cold War critical junctures in the European integration
   process. Taking into account the salience of ``vertical{''}
   differentiation for this case-study, this article also addresses the
   diachronic consequences of a series of game-changing variations in the
   centralisation of power among different interconnected policy sectors.},
