@article{ WOS:000605942700006,
Author = {Armitage, Alanna},
Title = {ASSESSING COVID-19 THROUGH A GENDER LENS},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2020},
Volume = {19},
Number = {1},
Pages = {53-60},
Month = {SPR},
Abstract = {Despite the high-profile political figures and celebrities who have
   contracted the new coronavirus disease, this potentially deadly illness
   is no great equalizer. On the contrary, as it spreads rapidly around the
   globe, COVID-19 is both exposing and compounding existing disparities,
   including gender inequality. The pandemic and its associated effects are
   leaving women and girls more vulnerable to gender-based violence, sexual
   and reproductive health issues, increased caregiving burden, and
   economic distress. However, women's knowledge, strength, and crucial
   role in frontline health care and across their communities play an
   essential part in any successful response to the COVID-19 pandemic.},
