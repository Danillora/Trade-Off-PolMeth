@article{ WOS:000565918000008,
Author = {Lu, Catherine},
Title = {Structural injustice and alienation: a reply to my critics},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2020},
Volume = {23},
Number = {4},
Pages = {544-555},
Abstract = {This author's reply addresses critiques by Reinhard Wolf, Alasia Nuti,
   and Kimberly Hutchings of my 2017 book, Justice and Reconciliation in
   World Politics. First, I restate the normative and practical
   significance of focusing on challenges of structural injustice for
   resolving many serious and pressing problems in world politics,
   including climate change. Second, I begin to resolve some puzzles about
   the concept of alienation and its relationship to justice and
   reconciliation, by outlining two concepts of alienation, and
   distinguishing them from alienation as a cognitive-affective experience.
   Third, I clarify the limits and potential of decolonial political
   theory.},
