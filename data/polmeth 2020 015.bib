@article{ WOS:000504084400018,
Author = {McMahon, Nicole and Alcantara, Christopher and Stephenson, Laura B.},
Title = {The Qualifying Field Exam: What Is It Good For?},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
