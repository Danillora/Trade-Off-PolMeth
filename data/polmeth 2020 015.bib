@article{ WOS:000584343000022,
Author = {Bowler, Shaun and Gschwend, Thomas and Indridason, Indridi H.},
Title = {Coalition Policy Perceptions},
Journal = {JOURNAL OF POLITICS},
Year = {2020},
Volume = {82},
Number = {4},
Pages = {1458-1473},
Month = {OCT 1},
Abstract = {We examine how voters form expectations about the policy positions of
   coalition governments. The literature generally assumes that voters
   believe the influence of coalition parties on government policy is
   proportional to the coalition parties' sizes. Yet little is known about
   whether, or how, voters form such expectations. In this article we
   leverage data from Austria, Germany, and Sweden and find that voters do
   not see coalition party influence as proportional. Voters take account
   of the coalition parties' bargaining strength, perceiving smaller
   coalition parties to have disproportional influence on coalition policy.
   In other words, voters who live under and vote for coalition governments
   have a somewhat different sense of policy outcomes than the literature
   currently suggests.},
