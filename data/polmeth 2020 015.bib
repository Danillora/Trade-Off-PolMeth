@article{ WOS:000603655700010,
Author = {Blaszczyk, Cezary},
Title = {Lockean Intellectual Property Refuted},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
Year = {2020},
Volume = {32},
Number = {63},
Pages = {161-186},
Abstract = {Locke's theory of property is irreconcilable with intellectual property.
   Property-like titles in ideal objects cannot be introduced within the
   framework of the natural law, because they could constrain others from
   acts necessary for their survival. Nevertheless, followers of Locke's
   theory of politics choose to belittle this conclusion and even Locke
   himself supported early copyright legislation. The inconsistency is
   important, for it depicts the problem of legitimization of intellectual
   property as political and demonstrates liberal reification of various
   aspects of social life.},
