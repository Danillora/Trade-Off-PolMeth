@article{ WOS:000522112600011,
Author = {Cruddas, Jon},
Title = {Beyond Political Binaries: The 2019 General Election and the Dagenham
   and Rainham Constituency},
Journal = {POLITICAL QUARTERLY},
Year = {2020},
Volume = {91},
Number = {1},
Pages = {74-79},
Month = {JAN},
Abstract = {This article considers the results of the 2019 general election with
   reference to the Dagenham and Rainham constituency in outer East London.
   It was a key target for the Conservatives with a 70 per cent leave
   voting electorate. It did not change hands and might therefore provide
   insights into the wider debate regarding future coalitions and strategy
   within the modern left. This article considers these results with
   reference to arguments about a `Brexit realignment' on the left and
   whether Labour should rethink the nature of its political `base'. It
   argues for a more nuanced debate than that which currently exists, built
   around simple binaries organised around Brexit, class, age, education
   and geography.},
