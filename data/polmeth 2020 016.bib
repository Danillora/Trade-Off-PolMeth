@article{ WOS:000550815800003,
Author = {Paries, Matthieu Darracq and Papadopoulou, Niki},
Title = {Balance Sheet Policies in a Large Currency Union: A Primer on ECB
   Non-Standard Measures since 2014},
Journal = {REVUE D ECONOMIE POLITIQUE},
