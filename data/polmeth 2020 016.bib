@article{ WOS:000556043600005,
Author = {Kirby, Philip},
Title = {Battle Scars:Wonder Woman, Aesthetic Geopolitics and
   Disfigurement in Hollywood Film},
Journal = {GEOPOLITICS},
Year = {2020},
Volume = {25},
Number = {4},
Pages = {916-936},
Month = {AUG 7},
Abstract = {Wonder Woman (2017) has been one of the most lucrative entries in the
   recent superhero film canon. With a female lead and director, it has
   also proven a rich text for feminist analysis. Here, the film's
   engagement with questions of gender, aesthetic judgment and disability
   is explored through the lens of feminist geopolitics, and connected to
   the alignment between immorality and disfigurement in Hollywood more
   broadly. It is suggested that aesthetic value is used in the film as a
   key organizing principle for spatial and geopolitical claims. Through
   this analysis, the paper suggests the potential for further studies of
   disability, including disfigurement, in popular geopolitics.},
