@article{ WOS:000582559800006,
Author = {Bois, Marcel},
Title = {`March Separately, But Strike Together!' The Communist Party's
   United-Front Policy in the Weimar Republic},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2020},
Volume = {28},
Number = {3},
Pages = {138-165},
Month = {SEP},
Abstract = {The Communist Party of Germany (KPD) first coined the united-front
   policy in 1921, representing a promising effort to bolster Communist
   influence in the workers' movement of that country. As the first part of
   the article shows, the kpd recruited large numbers of new members and
   significantly improved its electoral returns as a result. Despite this
   success, however, the party only pursued the united-front policy in two
   phases (1921-3 and 1926). As illustrated in the second part of the
   article, the KPD bid farewell to the united front as `Stalinisation'
   began to set in during the Weimar Republic's final years - with lethal
   consequences, as its abandonment decisively abetted the victory of
   German fascism.},
