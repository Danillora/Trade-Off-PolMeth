@article{ WOS:000530020100002,
Author = {Harguindeguy, Jean-Baptiste and Sola Rodriguez, Gonzalo and Cruz Diaz,
   Jose},
Title = {Between justice and politics: the role of the Spanish Constitutional
   Court in the state of autonomies},
Journal = {TERRITORY POLITICS GOVERNANCE},
Year = {2020},
Volume = {8},
Number = {2},
Pages = {222-240},
Month = {MAR 14},
Abstract = {This study focuses on the role of the Spanish Constitutional Court in
   the state of autonomies between 1980 and 2014. It questions the
   evolution of the court in two fields. First, it demonstrates that the
   court profoundly shaped the contours of the devolution process through a
   dynamic of `judicialization'. Second, this research analyzes the
   politicization process of constitutional justice by territorial actors.
   This dynamic led to the quasiparalysis of the court from 2008 to 2012
   through the ruling of the reform of the Catalan statute. This paper
   concludes the court can be best interpreted as a `trustee' aspiring to
   remain an independent arbiter within a framework lacking alternative
   fora for regulating intergovernmental relations.},
