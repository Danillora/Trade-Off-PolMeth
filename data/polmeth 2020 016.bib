@article{ WOS:000513470500001,
Author = {Narins, Thomas P.},
Title = {The lure of Chinese state capitalism in Latin America and the Caribbean},
Journal = {TERRITORY POLITICS GOVERNANCE},
Year = {2021},
Volume = {9},
Number = {4},
Pages = {512-532},
Month = {AUG 8},
Abstract = {This paper examines the attractiveness of contemporary China's
   state-directed economic system beyond China's borders. It considers the
   political and economic affinities between China and Latin America and
   the Caribbean (LAC) in order to understand the allure of Chinese state
   capitalism in the region. Employing six broad governance variables, this
   project develops an empirically grounded tool - the Neoliberal-Dirigiste
   Continuum - to estimate the allure of Chinese state capitalism in 25
   countries within the region. Drawing on data from the Varieties of
   Democracy database, the Database of Political Institutions, the United
   Nations Comtrade database, the Academic Network of Latin America and the
   Caribbean on China Monitor (RED ALC-China), and the Inter-American
   Dialogue's China-Latin America Finance Database, this paper examines the
   relationship between LAC countries' political and economic organization
   and trade, Chinese outbound foreign direct investment, and the issuance
   of Chinese policy bank loans to LAC countries from 2005 to 2017. While
   the allure of Chinese state capitalism and its spread across the Global
   South over the last three decades is not specific to any one world
   region, examining the China-LAC political economic relationship in this
   context is useful because it represents a blending of the global and the
   contextual.},
