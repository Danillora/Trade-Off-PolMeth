@article{ WOS:000607745700006,
Author = {Hildebrand, Achim and Jaeckle, Sebastian},
Title = {Pervasive Polarization or Partial Convergence? Moral Attitudes of
   Religious and Secular People at Various Levels of Development},
Journal = {INTERNATIONAL JOURNAL OF PUBLIC OPINION RESEARCH},
Year = {2020},
Volume = {32},
Number = {2},
Pages = {306-317},
Month = {SUM},
Abstract = {The effects of religiosity on moral attitudes have been described in
   numerous studies (see for an overview: Hood, Hill, \& Spilka 2009, pp.
   381-411). There is also a long research tradition on how economic
   development promotes tolerance (Inglehart \& Welzel, 2005). Much less is
   known about how religiosity and development interact in their effect on
   moral attitudes. In a pioneering study, Gaskins, Golder, and Siegel
   (2013) analyzed this interaction for several moral attitudes which are
   part of the World Values Survey (WVS), namely, attitudes toward
   abortion, divorce, euthanasia, homosexuality, and suicide. Thus, in
   terms of moral attitudes, they-roughly speaking-cover attitudes toward
   sexuality/ partnership and attitudes toward the termination of life.
   Gaskins et al. (2013) not only confirm the generally assumed idea that
   the more religious a respondent is, the higher their level of social
   conservatism tends to be, but find that this effect of religiosity is
   stronger in more developed countries: 1 As development progresses, both
   groups (secular and religious) become less socially conservative and
   thus more tolerant, but secular people do so at a considerably faster
   rate, widening the gap between the two groups even further. This
   polarization fuels the culture war raging in many developed democracies
   over issues such as same-sex marriage or abortion law (Smith \&
   Tatalovich, 2003).},
