@article{ WOS:000523429300001,
Author = {Collins, Brian J. and Marichal, Jose and Neve, Richard},
Title = {The social media commons: Public sphere, agonism, and algorithmic
   obligation},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2020},
Volume = {17},
Number = {4},
Pages = {409-425},
Month = {OCT 1},
Abstract = {This paper takes a unique approach to framing the political obligation
   social media companies like Twitter and Facebook have in a democratic
   society by casting the public sphere as a common-pool resource. Over the
   last decade or so much of our civic discourse has moved to social media
   platforms such as Facebook and Twitter. This paper argues that just as
   citizens have an obligation to one another, social media companies have
   an obligation to promote agonistic forms of civic, political discourse
   through their algorithms. This algorithmic obligation that social media
   companies have must be reconciled with social media companies'
   formulation of their fiduciary shareholder obligations. We posit that a
   commons based view of social media discourse allows these companies to
   make both a democratic health and fiduciary interest argument.},
