@article{ WOS:000544370900001,
Author = {Gimpel, James G. and Harbridge-Yong, Laurel},
Title = {Conflicting Goals of Redistricting: Do Districts That Maximize
   Competition Reckon with Communities of Interest?},
Journal = {ELECTION LAW JOURNAL},
Year = {2020},
Volume = {19},
Number = {4},
Pages = {451-471},
Month = {DEC 1},
Abstract = {Regardless of who draws the redistricting plan in a state with numerous
   districts, there are always complaints about the outcome. We suggest
   that the inherent conflicts among the traditional redistricting criteria
   make any multi-district plan subject to criticism and vulnerable to
   litigation. In particular, the mandates to respect communities of
   interest while drawing competitive districts are frequently
   irreconcilable. This is because many communities of interest are
   themselves lopsidedly partisan, and including them whole or undiluted
   will impede the pathway to achieving an even partisan division in a
   district's electorate. Examples drawn from North Carolina and
   Pennsylvania serve to illustrate the constraints that map drafters face,
   regardless of for whom they are employed.},
Publisher = {MARY ANN LIEBERT, INC},
Address = {140 HUGUENOT STREET, 3RD FL, NEW ROCHELLE, NY 10801 USA},
