@article{ WOS:000547269200007,
Author = {Davis, Alexander E. and Gamble, Ruth},
Title = {Constructing an ``Iron{''} Unity: The Statue of Unity and India's
   Nationalist Historiography},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2020},
