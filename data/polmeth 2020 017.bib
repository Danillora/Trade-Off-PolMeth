@article{ WOS:000609428000001,
Author = {Mehrl, Marius},
Title = {Female Combatants and Wartime Rape: Reconsidering the Role of Women in
   Armed Conflict},
Journal = {ARMED FORCES \& SOCIETY},
Year = {2022},
Volume = {48},
Number = {2},
Pages = {464-479},
Month = {APR},
Abstract = {Whereas existing research posits that the presence of female fighters in
   armed groups decreases their propensity for wartime rape, one recent
   study tests this claim quantitatively and is unable to detect a
   statistically significant effect. This leads the author to conclude that
   female combatants do not decrease rape. Using that study's original
   data, this article reexamines the evidence for the relationship between
   female rebel combatants and wartime sexual violence. Replications of the
   original models suggest that they make strong functional form
   assumptions regarding numerous independent variables and time dependence
   and that relaxing them results in substantively different findings.
   Namely, women's participation in armed groups decreases groups' use of
   wartime rape. In support of Loken's organizational theory of rape,
   results also suggest that this effect is moderated by group norms. These
   findings contribute to the literature on female participation in rebel
   groups and beyond.},
