@article{ WOS:000536452000011,
Author = {Quecan Gamba, Ricardo},
Title = {The Administrative Precedent in Colombia: Implications and Difficulties},
Journal = {REVISTA ESTUDIOS SOCIO-JURIDICOS},
Year = {2020},
Volume = {22},
Number = {1},
Pages = {353-390},
Month = {JAN-JUN},
Abstract = {The figure of the administrative precedent is a recent legal institution
   for the Colombian legal system, its legal incorporation is the product
   of the last decade of Constitutional Court's jurisprudence, which had
   impelled the legislator to incorporate this legal category in Law 1340
   of 2009 -Competence Protection Law- and Law 1437 of 2011 -Administrative
   Procedure and Administrative Litigation Code-. This category currently
   represents essential theoretical and pragmatical challenges because it
   gives binding force to the administrations' past decisions to apply, in
   a uniform way, the same verdict when it has similar factual and legal
   circumstance. It allows these entities to be a direct source of law, and
   with it being able to generate all the controversies that it implies.},
