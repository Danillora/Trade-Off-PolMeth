@article{ WOS:000577575500001,
Author = {Le, Danvy and Arora, Maneesh and Stout, Christopher},
Title = {Are You Threatening Me? Asian-American Panethnicity in the Trump Era},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2020},
Volume = {101},
Number = {6},
Pages = {2183-2192},
Month = {OCT},
Abstract = {Objective This study explores the effect of Donald Trump's candidacy,
   and first year in office, on Asian-American linked fate. We argue that
   the use of anti-Asian and anti-immigrant messaging during the 2016
   election, and the enactment of discriminatory policies once elected,
   increased feelings of panethnic linked fate among Asian Americans.
   Method To test our hypotheses, we assess Asian Americans' levels of
   linked fate before the 2016 election, immediately after the 2016
   election, and one year after the 2016 election with several time-series
   surveys. Results We find that Asian-American linked fate is higher after
   the election and remains high one year later. Qualitative data collected
   through open-ended survey responses suggest that the increase in
   panethnic linked fate can be at least partially attributed to Trump's
   discriminatory rhetoric. Conclusion The results have implications for
   Asian-American political behavior, particularly mobilization, by
   invoking collective action through panethnic linked fate.},
