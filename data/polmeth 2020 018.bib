@article{ WOS:000539831800001,
Author = {Luo, Danglun and Liu, Congcong and Wu, Lifan},
Title = {Horizontal Networks and Economic Performance: Evidence from City Leaders
   in China},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2020},
Volume = {101},
Number = {4},
Pages = {1359-1373},
Month = {JUL},
Abstract = {Objectives We examine political connections at the horizontal level of
   local government officials and their impacts on economic growth in
   China. Methods We identify two types of horizontal networks: hometown
   tie and alumni tie, and analyze the biographic data of the top officials
   of 256 prefecture-cities during the period of 1999-2013. Results We find
   a positive relationship between city leaders' hometown tie and economic
   growth, measured by either the relative GDP per capita growth rate or
   the GDP per capita growth rate. Furthermore, we find the positive
   relationship is more profound in the low market-open cities. However, we
   do not find any significant impacts of the alumni tie on economic
   growth. Conclusions The hometown tie improves the city leaders' economic
   performance.},
