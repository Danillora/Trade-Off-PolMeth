@article{ WOS:000544371500001,
Author = {Shepherd, Michael E. and Fresh, Adriane and Eubank, Nick and Clinton,
   Joshua D.},
Title = {The Politics of Locating Polling Places: Race and Partisanship in North
   Carolina Election Administration, 2008-2016},
Journal = {ELECTION LAW JOURNAL},
Year = {2021},
Volume = {20},
Number = {2},
Pages = {155-177},
Month = {JUN 1},
Abstract = {Do local election administrators change precincts and Election Day
   polling place locations to target voters based on their partisanship or
   race? We systematically evaluate whether decisions consistent with
   targeting occur using the near universe of eligible voters, polling
   place locations, and precinct boundaries across three presidential
   elections in the closely contested state of North Carolina. We find no
   evidence that local administrators allocate precincts and polling places
   in a manner consistent with partisan manipulation for electoral gain.
   Some counties appear to differentially target opposition party voters
   with these changes, but the county-level variation we document is likely
   due to random variation rather than deliberate manipulation. There is
