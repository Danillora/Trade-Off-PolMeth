@article{ WOS:000534420800001,
Author = {Crepaz, Markus M. L.},
Title = {Coveting Uniformity in a Diverse World: The Authoritarian Roots of
   Welfare Chauvinism in Postmigration Crisis Germany},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2020},
Volume = {101},
Number = {4},
Pages = {1255-1270},
Month = {JUL},
Abstract = {Objective To explore the links between authoritarian personality traits
   and welfare chauvinism by controlling for a host of alternative
   explanations while also investigating whether these links are additive
   or interacting with other triggering variables.
   Method Using the online survey platform Qualtrics, a stratified sample
   of 1,010 respondents in Germany was recruited in January 2019.
   Authoritarian personality is operationalized by applying the fourfold
   child-rearing scale pioneered by Feldman and Stenner.
   Results Findings indicate that authoritarianism has an independent,
   additive effect on welfare chauvinism, but cultural and economic threats
   do not systematically mediate the effect of authoritarianism on welfare
   chauvinism.
   Conclusion Submissive authoritarianism is significantly linked to a
   desire to disentitle immigrants from using welfare benefits.},
