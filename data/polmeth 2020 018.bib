@incollection{ WOS:000855238200002,
Author = {Gottschalk, Marie},
Editor = {Eidlin, B and Mccarthy, MA},
Title = {CAUGHT IN THE COUNTRYSIDE: RACE, CLASS, AND PUNISHMENT IN RURAL AMERICA},
