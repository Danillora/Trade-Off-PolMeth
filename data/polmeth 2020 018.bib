@article{ WOS:000542918500003,
Author = {Minelgaite, Inga and Sund, Berit and Stankeviciene, Jelena},
Title = {understanding the nordic Gender diversity paradox},
Journal = {TALTECH JOURNAL OF EUROPEAN STUDIES},
Year = {2020},
Volume = {10},
Number = {1},
Pages = {40-57},
Month = {JUN},
Abstract = {Norway and Iceland consistently top global gender equality rankings and
   pioneer the introduction of various measures for increased gender
   diversity. Still, actual gender diversity in top-level positions is
   lacking. This article seeks to better understand the contradiction
   between gender equality as a value and the actual lack of gender
   diversity and presents a conceptual model built of existing literature,
   which draws on potential differences between values held at the societal
   level and the individual level, and subsequent consequences for
   attitudes to diversity and diversity-impacting behaviors. Conceptual
   propositions are set forth that can be developed into a testable
   hypothesis.},
