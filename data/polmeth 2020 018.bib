@article{ WOS:000538156800021,
Author = {Oros, Cornel and Zimmer, Blandine},
Title = {Budget uncertainty in a monetary union},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2020},
Volume = {63},
Month = {JUN},
Abstract = {Governments in a monetary union fear spending disturbances. We
   distinguish them according to their ability to hedge against these
   disturbances and assume that they derive their optimal fiscal decisions
   by using a robust control approach. Results show that governments being
   highly vulnerable to spending disturbances set excessive tax rates,
   thereby exacerbating the fiscal pressure detrimental to output and
   obliging the central bank to conduct an expansionary monetary policy.
   Countries whose governments have higher ability to hedge against
   spending disturbances then suffer from the inflationary consequences of
   this monetary policy.},
