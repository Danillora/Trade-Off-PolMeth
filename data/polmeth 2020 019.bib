@article{ WOS:000578569800002,
Author = {Bacchin, Elena},
Title = {Political Prisoners of the Italian Mezzogiorno: A Transnational Question
   of the Nineteenth Century},
Journal = {EUROPEAN HISTORY QUARTERLY},
