@article{ WOS:000528196000016,
Author = {Garcia Lopez, Gustavo A.},
Title = {Reflections on disaster colonialism: Response to Yarimar Bonilla's `The
   wait of disaster'},
Journal = {POLITICAL GEOGRAPHY},
Year = {2020},
