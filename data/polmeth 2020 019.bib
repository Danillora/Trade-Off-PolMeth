@article{ WOS:000536648500005,
Author = {Polin, Brian A. and Ehrman, Chaim M.},
Title = {The Curious Relationship Between Military Service and Entrepreneurial
   Intentions in Israel},
Journal = {ARMED FORCES \& SOCIETY},
