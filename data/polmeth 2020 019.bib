@article{ WOS:000598044000001,
Author = {Ballingrud, Gordon},
Title = {Ideology and Risk Focus: Conservatism and Opinion-Writing In the US
   Supreme Court},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2021},
Volume = {102},
Number = {1},
Pages = {281-300},
Month = {JAN},
Abstract = {Neuroscience proffers evidence that self-described conservatives have
   stronger fear responses and aversion to risk than self-described
   liberals. Combined with studies showing that judicial ideology drives
   the content of Supreme Court majority opinions, I argue that
   conservatism is linked to risk focus in Supreme Court majority opinions.
   I use the Language Inquiry and Word Count (LIWC) software on a sample of
   Supreme Court majority opinions, and find that conservative opinions
   score higher on the LIWC dimension called Risk Focus than liberal
   opinions. This effect is enhanced in criminal procedural cases. If
   conservative judges' perceptions of risk are inflated, and if such
   perceptions are reflected in the binding opinions that they author, then
   such opinions' heightened sense of risk may influence the perceptions of
   risk of lower-court judges, which may in turn affect their
   decision-making in such important areas as sentencing and convictions.
   Such a pattern raises important questions for the thousands of
   lower-court decisions which impact the basic liberties of American
   citizens.
   Objective To determine whether judicial ideology affects the focus on
   risk of Supreme Court opinions.
   Methods Original, random sampling of 1200-1300 Court opinions; use of
   LIWC software to analyze risk focus of each opinion; regression analysis
   of ideology on risk focus.
   Results As ideology becomes more conservative, the Court's opinions
   demonstrate increased evidence of focus on risk. This effect is
   pronounced in criminal procedure cases.
   Conclusion The theory is supported. Increasingly conservative Court
   opinions demonstrate an increased focus on risk.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
