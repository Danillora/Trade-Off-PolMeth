@article{ WOS:000548965900001,
Author = {Lim, So Hyung and Sziarto, Kristin},
Title = {Resignifying occupation: territorialization, place and transgression in
   the anti-corporatization movement at Seoul National University},
Journal = {TERRITORY POLITICS GOVERNANCE},
Year = {2022},
Volume = {10},
Number = {4},
Pages = {589-607},
Month = {JUL 4},
Abstract = {Occupation is a common strategy, including in anti-austerity movements.
   This paper examines the 2011 occupation at Seoul National University
   (SNU), which had an ambivalent position on anti-austerity activism, and
   mixed outcomes. The student occupiers held an occupied administrative
   building longer than previous student occupations, and their tactics
   were taken up by student activists around South Korea. However, the SNU
   occupation failed to influence the university's corporatization. Both
   the successes and failures of the SNU occupation related to their
   resignification of `occupation' relative to their subjectivity as SNU
   students, and from the way they enacted territorialization of the
   occupied administrative building. The resignification served to mobilize
   SNU students, but also masked the occupation's leaders' failure to
   negotiate the differences in their goals and refusal to connect with
   other constituencies against university austerity.},
