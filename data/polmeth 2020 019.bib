@article{ WOS:000508760900010,
Author = {Tkachenko, Andrey and Esaulov, Daniil},
Title = {Autocratic governors in public procurement},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2020},
Volume = {61},
Month = {JAN},
Abstract = {The personal role of sub-national rulers is crucial for regional
   development in countries with weak institutions. This paper studies the
   impact of regional governors' tenure in office and their local ties on
   procurement performance in Russia. To identify the causal effect, we
   construct instruments for governor's tenure by exploiting the regional
   vote share of ruling party in past parliament elections. We find the
   evidence that governors who do not have pre-governing local ties in the
   region (outsiders) demonstrate predatory behaviour, compared to
   governors with local ties (insiders). Namely, governors-outsiders
   restrict the competition at awarding stage significantly more than
   governors-insiders. Moreover, for governors-outsiders this restriction
   becomes stronger with tenure in office, while governors-insiders do not
   demonstrate such negative tenure effect. We argue that this restriction
   of competition by governors-outsiders cannot be explained by the
   intention of better contracts execution: the delays in execution and the
   probability of contract termination either increase or keep stable with
   tenure for governors-outsiders and these outcomes decrease with tenure
   for governors-insiders.},
Publisher = {ELSEVIER SCIENCE INC},
Address = {STE 800, 230 PARK AVE, NEW YORK, NY 10169 USA},
Type = {Article},
Language = {English},
Affiliation = {Tkachenko, A (Corresponding Author), Natl Res Univ Higher Sch Econ, Moscow, Russia.
   Tkachenko, Andrey; Esaulov, Daniil, Natl Res Univ Higher Sch Econ, Moscow, Russia.
   Tkachenko, Andrey, Bocconi Univ, Milan, Italy.},
