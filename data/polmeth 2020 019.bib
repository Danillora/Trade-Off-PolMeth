@article{ WOS:000536305500009,
Author = {Villa Garcia, Roberto},
Title = {A declining Suffrage? The Spanish elections to the Congress of 1923},
Journal = {HISTORIA Y POLITICA},
Year = {2020},
Number = {43},
Pages = {255-290},
Month = {JAN-JUN},
Abstract = {This article researches the elections to the Congress of Deputies of
   1923. Held under the government of the concentracion liberal, they were
   the last ones with the electoral law of 1907 and a last attempt to
   reconstruct the Spanish turno of parties. For this, the historiography
   considers them demonstrative of the stagnation of the electoral behavior
   of the Restoration, anchored in the demobilization and falsification of
   the results. The new data of this article, coming from parliamentary,
   governmental and newspaper sources, will allow to evaluate if the
   competition framework of 1923 continued propitiating the aforementioned
   distortions. Also, if there were relevant changes capable of redefining
