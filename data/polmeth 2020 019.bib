@article{ WOS:000588755600004,
Author = {Jimenez Aguilar, Carlos M. and Thoene, Ulf},
Title = {Frontier development policy and local governance in South America},
Journal = {TERRITORY POLITICS GOVERNANCE},
Year = {2020},
Volume = {8},
Number = {5},
Pages = {639-656},
Month = {OCT 19},
Abstract = {This paper studies the dynamics of horizontal and vertical coordination
   arising from a set of sectoral and intersectoral projects in the border
   areas of Colombia-Venezuela and Brazil-Argentina. These are
   characterized by some common features such as population density,
   conurbation and trade patterns that in the absence of border regulation
   policies strengthen border security and development guidelines and
   extend local leadership. Officials and directors named in local
   governance networks were surveyed and interviewed at the municipal
   level. This research concludes that patterns of feedback between the
   local and the national are fundamental to the success of limited and
   informal initiatives, but strategic in cross-border cooperation and
   interaction.},
