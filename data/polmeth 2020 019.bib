EISSN = {1873-5703},
Keywords = {Public procurement; Tenure in office; Governor; Competition; Efficiency;
   Contract execution},
Keywords-Plus = {AUTHORITARIANISM; INSTABILITY; INVESTMENT; ALLOCATION; CORRUPTION;
   DONATIONS; DEMOCRACY; ELITES; OFFICE; TENURE},
