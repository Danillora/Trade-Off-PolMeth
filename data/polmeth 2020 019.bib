@article{ WOS:000559349500001,
Author = {Luebke, Simon M.},
Title = {Political Authenticity: Conceptualization of a Popular Term},
Journal = {INTERNATIONAL JOURNAL OF PRESS-POLITICS},
Year = {2021},
Volume = {26},
Number = {3},
Pages = {635-653},
Month = {JUL},
Abstract = {Authenticity is a prominent concept in current political discourse. Its
   popularity in political communication has also inspired a growing but
   still fragmented body of research in communication science. Since this
   field lacks an integrative conceptualization, this article aims to
   clarify political authenticity for academic discourse and as a research
   object for communication science. The article provides a narrative
   review of research on authenticity in political communication and
   proposes an understanding of political authenticity as a social
   construction which is created and negotiated in complex communication
   processes among politicians, the media, and the audience. I propose
   performed, mediated, and perceived political authenticity as three
   analytic perspectives to deconstruct these complex processes and to link
   political authenticity with media and communication literature. Finally,
   I derive four dimensions of political authenticity (consistency,
   intimacy, ordinariness, and immediacy) and corresponding indicators that
   will help to operationalize it as a strategy by politicians, a product
   from mediation processes, and as an audience perception.},
