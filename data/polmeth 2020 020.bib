@article{ WOS:000532418500001,
Author = {Mueller, Stefan},
Title = {Media Coverage of Campaign Promises Throughout the Electoral Cycle},
Journal = {POLITICAL COMMUNICATION},
Year = {2020},
Volume = {37},
