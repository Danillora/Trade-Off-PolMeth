@article{ WOS:000551441700001,
Author = {Shino, Enrijeta and Binder, Michael},
Title = {Defying the Rally During COVID-19 Pandemic: A Regression Discontinuity
   Approach},
Journal = {SOCIAL SCIENCE QUARTERLY},
