@article{ WOS:000604436800003,
Author = {Cuyler, Zachary Davis},
Title = {Toward the Target and the Goal: Infrastructure Sabotage and Palestinian
   Liberation in the Pages of al-Hadaf},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2020},
Volume = {28},
Number = {4},
Pages = {67-101},
Month = {DEC},
Abstract = {This paper examines a 1969 infrastructure-sabotage campaign by the
   Popular Front for the Liberation of Palestine (PFLP) as reported in the
   pages of its weekly newspaper, al-Hadaf. While academic and policy
   discourse conceptualises sabotage in a way that emphasises its
   disruptive effects and sometimes obscures the positive political ends
   toward which acts of sabotage are directed, the PFLP conceptualised
   sabotage as a practice of revealing the political and economic relations
   that infrastructures sustain by disrupting them and marking progress
   toward an alternative political-economic order. For the PFLP, sabotage
   constituted a kind of concrete critique, a communicative act that
   conveys a theoretical analysis of processes of extraction, exploitation,
   and dispossession by physically interrupting them.},
Publisher = {BRILL},
