@article{ WOS:000522729700007,
Author = {Garcia Mac Gaw, Carlos},
Title = {The Ancient Mode of Production, the City-State and Politics},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2020},
Volume = {28},
Number = {1},
Pages = {215-249},
Month = {MAR},
Abstract = {This paper briefly examines the concept of the ancient mode of
   production as expressed in Karl Marx's Formations. It looks at how
   twentieth-century Marxist historiography picks up this concept in its
   characterisation of the Greco-Roman city-state. It explores the
   feasibility of the use of the concept in relation to the advancement of
   knowledge of the city-state, especially through the development of
   archaeology. It examines how social classes are structured and relations
   of exploitation are presented. And it analyses the need for politics in
   the organisation of this socio-economic form in terms of how it is
