@article{ WOS:000517424000005,
Author = {Ben-Shalom, Uzi and Moshe, Rinat and Mash, Rony and Dvir, Amit},
Title = {Micro-Sociology and New Wars: Visual Analysis of Terror Attacks During
   the ``Intifada of the Individuals{''}},
Journal = {ARMED FORCES \& SOCIETY},
