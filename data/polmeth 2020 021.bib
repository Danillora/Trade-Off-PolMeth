@article{ WOS:000538156800017,
Author = {Andreoli, Francesco and Olivera, Javier},
Title = {Preferences for redistribution and exposure to tax-benefit schemes in
   Europe},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2020},
Volume = {63},
Month = {JUN},
Abstract = {This paper provides evidence that attitudes towards redistribution are
   associated with the extent of generosity of the redistributive context
   experienced by the individual, as measured by the likelihood of
   receiving positive benefit transfers net of fiscal contribution. We
   estimate reduced form tax-benefit equations with the EU Statistics on
   Income and Living Conditions (EU-SILC), and match the implied parameters
   to the respondents of the European Social Survey (ESS) on the basis of
   their characteristics. The period of analysis is 2008-2016. For
   identification, we exploit exogenous cross-country and time variation in
   tax rules and market income to disentangle implications of exposure to
   tax-benefit rules on preferences for redistribution from the effects of
   changes in income inequality. We find that exposure to positive net
   benefits increases support for redistribution by 1.4\%-3\% on baseline
   models, the effect being robust across a variety of specifications.},
