@article{ WOS:000542918500007,
Author = {Parn-Lee, Evelin},
Title = {The Origins of Supranational State Aid Legislations: What Policymakers
   Must Know and Adhere to. The Case of Estonia},
Journal = {TALTECH JOURNAL OF EUROPEAN STUDIES},
