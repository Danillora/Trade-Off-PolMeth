@article{ WOS:000607746000005,
Author = {Tsfati, Yariv},
Title = {Personality Factors Differentiating Selective Approach, Selective
   Avoidance, and the Belief in the Importance of Silencing Others: Further
   Evidence for Discriminant Validity},
Journal = {INTERNATIONAL JOURNAL OF PUBLIC OPINION RESEARCH},
Year = {2020},
Volume = {32},
Number = {3},
Pages = {488-509},
Month = {FAL},
Abstract = {Recent research proposed self-report measures tapping three different
   strategies used by people to place themselves within an ideologically
   homogeneous information environment: selective approach, selective
   avoidance, and the belief in the importance of silencing others (BISO).
   Using online survey data collected in Israel (n = 749), the present
   investigation explores the discriminant validity of these constructs.
   Confirmatory factor model comparisons support their empirical
   differentiation. In addition, it is argued that the constructs are
   empirically different given the fact that they correlate differently
   with personality factors. BISO is more strongly and positively
   associated with authoritarianism. Selective avoidance is more strongly
   negatively associated with openness to experience. Selective approach
   was positively associated with empathy, with which selective avoidance
   was negatively associated.},
