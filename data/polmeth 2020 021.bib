@article{ WOS:000577079100015,
Author = {Sanchez, Jeremie},
Title = {Blinded like a state? Urban sanitation, improvement and high modernism
   in contemporary Myanmar},
Journal = {POLITICAL GEOGRAPHY},
Year = {2020},
Volume = {82},
Month = {OCT},
Abstract = {This article explores urban sanitation in the city of Mandalay, upper
   Myanmar, as an entry point to better understand the issue of improvement
   in the country in the current era of political change. Based upon
   qualitative data gathered in Mandalay between 2015 and 2018, the article
   focuses on the state's ways of seeing urban improvement after 2016, when
   the National League for Democracy government led by Daw Aung San Suu Kyi
   took office. The article builds upon James Scott's work on statecraft to
   frame the analysis, and it uses this framework to argue that a localized
   form of authoritarian high modernism is emerging and shaping urban
   improvement in Mandalay today. The case is noteworthy as high modernism
   in Mandalay is not associated with attempts to render the city more
   legible; on the contrary improvement seems to be carried out by a state
   unwilling, or unable, to see and thus being blinded. The article
   develops this argument to make a twofold contribution, to discussions on
   statecraft and the ``failure of development{''} in contemporary Myanmar
   from the perspective of political geography; and to debates on states'
   ways of seeing and the practice of improvement in postcolonial contexts.},
Publisher = {ELSEVIER SCI LTD},
