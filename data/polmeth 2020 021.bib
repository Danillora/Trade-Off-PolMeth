@article{ WOS:000869865000004,
Author = {Zahra, Heba Mohamed},
Title = {Who is a terrorist? Exploring the utility of interdisciplinary fields},
Journal = {REVIEW OF ECONOMICS AND POLITICAL SCIENCE},
