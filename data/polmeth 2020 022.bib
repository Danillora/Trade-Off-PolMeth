@article{ WOS:000542824200001,
Author = {Terai, Kimiko and Glazer, Amihai},
Title = {How an ineffective agent can increase his budget},
Journal = {ECONOMICS \& POLITICS},
Year = {2021},
Volume = {33},
Number = {1},
Pages = {133-147},
Month = {MAR},
Abstract = {A principal aware that an agent exerts little effort will nevertheless
   fund the agent, because the principal values the output the agent
   produces. The agent in turn decides how hard to work by anticipating how
   his behavior affects the budget the principal will give him. Under some
   conditions, the principal gives the agent large budgets anticipating
   that the agent will not work hard. The corrupt environment aggravates
   the problem. An agent who can set the budget ameliorates the problem,
   though incompletely.},
