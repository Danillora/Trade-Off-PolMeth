@article{ WOS:000508760900013,
Author = {Bellettini, Giorgio and Ceroni, Carlotta Berti and Monfardini, Chiara},
Title = {Immigration, ethnic diversity and voting: The role of individual income},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2020},
Volume = {61},
Month = {JAN},
Abstract = {We exploit a unique panel dataset merging data on individual
   socio-economic characteristics and individual turnout in an Italian
   municipality to investigate the relationship between ethnic diversity in
   residential neighborhoods and propensity to vote. Using these data, we
   document for the first time a differential effect of diversity on
   electoral turnout depending on household equivalent income.
   Specifically, we show that ethnic diversity in the neighborhood reduces
   the political participation of the poor, while it fosters that of the
   more affluent. These results highlight a potential democratic deficit
   stemming from reduced and unequal electoral turnout in increasingly
   heterogeneous neighborhoods.},
