@article{ WOS:000572101100004,
Author = {Malewska-Szalygin, Anna},
Title = {Emotions and Civility: Everyday Talks about Politics with Rural
   Inhabitants of Southern Poland},
Journal = {INTERSECTIONS-EAST EUROPEAN JOURNAL OF SOCIETY AND POLITICS},
Year = {2020},
Volume = {6},
Number = {2},
Pages = {61-80},
Abstract = {Typical conversations about political matters are charged with emotion.
   Political matters are understood here as a thematic field involving
   talks about central authorities and parliament, as well as comments on
   news provided by the media. Talks about this topic often occur during
   neighborly meetings and family or social gatherings. I conducted
   ethnographic interviews to analyze how rural inhabitants talk about such
   political matters. During the interviews, especially polyphonic ones, I
   observed the accompanying emotions, such as raised voices, faces
   bloodshot with irritation, lively gestures, the use of irony, and
   sometimes vulgar language and swearing. Anger, resentment, anxiety,
   fear, contempt, hostility, and even hatred were unmistakable signals of
   emotional involvement in political matters and engagement in debate
   about the common good and public affairs. Thus, the question arises: are
   such conversations a form of civility?},
Publisher = {CENTRE SOCIAL SCIENCES, HUNGARIAN ACAD SCIENCES},
