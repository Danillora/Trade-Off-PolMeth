@article{ WOS:000545823300001,
Author = {Migone, Andrea Riccardo},
Title = {Trust, but customize: federalism's impact on the Canadian COVID-19
   response},
Journal = {POLICY AND SOCIETY},
Year = {2020},
Volume = {39},
Number = {3, SI},
Pages = {382-402},
Month = {JUL 2},
Abstract = {This article explores how Canadian federalism, with its complex mix of
   competencies, and the country's punctuated gradualism policy style
   interface with urgent, complex decision-making like the COVID-19
   pandemic. We find that while punctuated gradualism favors tailored
   responses to pandemic management it is weaker when coordination and
   resourcing are to be undertaken during non-crisis situations and that,
   while the level of cooperation among Canadian jurisdictions has
   progressively increased over the years, policy is still almost
   exclusively handled at the federal, provincial and territorial levels.
   Furthermore, the model appears to have critical `blind spots' in terms
   of vulnerable communities that do not emerge as such until after a
   crisis hits.},
