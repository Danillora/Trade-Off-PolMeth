@article{ WOS:000521511900008,
Author = {Tran, Viet T. and Walle, Yabibal M. and Herwartz, Helmut},
Title = {The impact of local financial development on firm growth in Vietnam:
   Does the level of corruption matter?},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2020},
Volume = {62},
Month = {MAR},
Abstract = {We examine the effects of province-level financial development and
   corruption on the performance of Vietnamese firms in terms of the growth
   rates of sales, investment and sales per worker. Employing a large
   firm-level dataset of more than 40,000 firms for the period 2009-2013
   and applying a heteroskedasticity-based identification strategy, we find
   that province-level financial development promotes firm growth, while
   corruption hinders it. Most importantly, the marginal effect of
   financial development on firm growth depends negatively on the level of
   corruption. Moreover, financial development exacerbates the
   growth-retarding effect of corruption.},
