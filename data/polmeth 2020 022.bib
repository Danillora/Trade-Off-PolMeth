@article{ WOS:000590305500005,
Author = {Tobin, Sarah A.},
Title = {Islamic Neoliberalism for Jordan's Islamic Action Front in Islamic
   Banking and Finance},
Journal = {POLITICS AND RELIGION},
Year = {2020},
Volume = {13},
Number = {4},
Pages = {768-795},
Month = {DEC},
Abstract = {There is a paradox: why are there so many political and economic Islamic
   actors in the Middle East but not a large willingness on their part to
   adopt and promote Islamic banking and finance methodologies? This paper
   argues that the more vague and ambivalent these actors are on economic
   policy, the wider their appeal; and, by extension, the more compatible
