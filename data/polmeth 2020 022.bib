@article{ WOS:000547269200005,
Author = {Smith, Evan and Dukova, Anastasia},
Title = {ASIOand the Monitoring of Irish Republicans in Australia during the
   ``Troubles{''}},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2020},
Volume = {66},
