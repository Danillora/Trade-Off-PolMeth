@article{ WOS:000533218000001,
Author = {Ovadek, Michal},
Title = {``Popular tribunes{''} and their agendas: topic modelling Slovak
   presidents' speeches 1993-2020},
Journal = {EAST EUROPEAN POLITICS},
Year = {2021},
Volume = {37},
Number = {2},
Pages = {214-238},
Month = {APR 3},
Abstract = {Since its birth as an independent republic in 1993, Slovakia has been
   served by five different presidents. Due to limited competences, the
   presidents' have often relied on political speech as their principal
   tool to influence political developments. However, text as a source of
   data has been largely neglected in existing scholarship on Central
   European presidents. In this exploratory study, I classify the content
   of presidential speeches using a topic model and analyse topical
   patterns over time and across different presidents. I find that topical
   variation can provide useful insights into relevant issues such as
   agenda shifts or intra-executive conflict.},
