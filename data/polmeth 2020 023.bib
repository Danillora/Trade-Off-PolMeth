@article{ WOS:000571895400003,
Author = {Salyk-Virk, Melissa J.},
Title = {Building Community Resilience? Community Perspectives of the Countering
   Violent Extremism Pilot Program in Minneapolis/St. Paul},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2020},
Volume = {43},
