@article{ WOS:000536436600008,
Author = {Bakke, Kristin M. and Mitchell, Neil J. and Smidt, Hannah M.},
Title = {When States Crack Down on Human Rights Defenders},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
Year = {2020},
Volume = {64},
Number = {1},
Pages = {85-96},
Month = {MAR},
Abstract = {Research suggests that civil society mobilization together with the
   ratification of human rights treaties put pressure on governments to
   improve their human rights practices. An unexplored theoretical
   implication is that pressure provokes counterpressure. Instead of
   improving treaty compliance, some governments will have an interest in
   demobilizing civil society to silence their critics. Yet we do not know
   how and to what extent this incentive shapes governments' policies and
   practices regarding civil society organizations. In this article we
   argue and show-using a new global database of government-sponsored
   restrictions on civil society organizations-that when governments have
   committed to human rights treaties and, at the same time, continue to
   commit severe human rights abuses, they impose restrictions on civil
   society groups to avoid monitoring and mitigate the international costs
   of abuses.},
