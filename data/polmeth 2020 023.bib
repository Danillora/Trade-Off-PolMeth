@article{ WOS:000599375500001,
Author = {Zeglen, David},
Title = {Temporal ideologies in uneven and combined development},
Journal = {CAMBRIDGE REVIEW OF INTERNATIONAL AFFAIRS},
Year = {2020},
Volume = {36},
Number = {2},
Pages = {184-202},
Month = {DEC 6},
Abstract = {Scholars exhibit a general self-reflexive tendency to purge Eurocentrism
   from UCD in order to strengthen the concept. Yet, unless the role of the
   cultural in UCD is adequately theorized, UCD cannot fully overcome
   Eurocentrism. This article therefore advances a non-Eurocentric cultural
   theory of UCD based on Althusser's theory of ideology by positing that
   the whip of external necessity and the privilege of historical
   backwardness are cultural conditions specific to capitalism. I argue
   that in order for capitalism to reproduce itself, it relies upon a
   temporal ideology called `development.' However, capitalism also
   produces a material effect I call the `spatialization of unevenness'
   that contradicts the ideology of development. This contradiction is,
   however, mediated by the ideology of nationalism. The result of this
   mediation is the internalization of development as a whip of external
   necessity and the privilege of historical backwardness as a privilege of
   national culture in the ideology of nationalism.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
