@article{ WOS:000499984900002,
Author = {Vinson, Laura Thaut},
Title = {Disaggregating Ethnicity and Conflict Patterns: Evidence From Religious
   and Tribal Violence in Nigeria},
Journal = {ETHNOPOLITICS},
Year = {2020},
Volume = {19},
Number = {1},
Pages = {19-44},
Month = {JAN 1},
Abstract = {This paper argues that a conflict's association with religious identity
   can distinctly shape the pattern of inter-group ethnic violence. Drawing
   on an original dataset of ethnic violence in northern Nigeria spanning
   30 years, the paper finds that religious and tribal communal clashes are
   associated with distinct precipitants and patterns of violence due to
   religion's malleability and dynamism, as well as its `transcendent'
   allegiance. The paper concludes that scholars who fail to disaggregate
   analyses of ethnic conflict will be unable to fully explain when and
   where religious as opposed to other types of ethnic violence are likely
   to occur.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
