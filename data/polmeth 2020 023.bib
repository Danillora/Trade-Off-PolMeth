@article{ WOS:000547523200001,
Author = {Celik, Ayse Betul},
Title = {Agonistic peace and confronting the past: An analysis of a failed peace
   process and the role of narratives},
Journal = {COOPERATION AND CONFLICT},
Year = {2021},
Volume = {56},
Number = {1},
Pages = {26-43},
Month = {MAR},
Abstract = {This study analyzes how peace processes in socio-political environments
   that do not support `confronting the past' (CTP) initiatives are
   affected by the exclusion and delegitimization of alternative narratives
   different from dominant ones concerning the nature and history of ethnic
   conflicts, focusing on Turkey's failed peace process as a case study. It
   pays specific attention to the resistance against acknowledging
   alternatives to dominant narratives by considering the role played by
   bystanders and antagonistic citizens, who are not directly part of the
   conflict but nonetheless support it by remaining passive or
   directly/indirectly supporting dominant narratives. Driven by agonistic
   peace theory, the article shows how failing to turn these groups into
   agonistic citizens through some form of agonistic CTP initiative and
   allowing a space for alternative narratives can result in the fragility
   of efforts towards a transition to peace.},
Publisher = {SAGE PUBLICATIONS LTD},
