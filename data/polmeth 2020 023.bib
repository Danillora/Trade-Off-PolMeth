@article{ WOS:000547594400001,
Author = {Boaz, Danielle N.},
Title = {Indian and Immigrant Religious Freedom: the Lesser-Known Impact of Obeah
   Laws in the 21st Century Caribbean},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2020},
Volume = {58},
Number = {3, SI},
Pages = {320-343},
Month = {JUL 2},
Abstract = {Although typically emphasised as part of a larger scheme to ban
   African-derived faiths, Obeah and vagrancy statutes in the Caribbean
   were also part of a widespread trend within the British Empire to bar
   Indian spiritual practices. Furthermore, anti-Obeah and related laws
   represent a longstanding form of immigrant regulation in the British
   Empire that began in the sixteenth century. Drawing upon on analysis of
   the history of Obeah and vagrancy statutes, this article will
   interrogate the continued limitations that these laws place on Indian
   and immigrant religious freedom in the twenty-first century Caribbean.},
