@article{ WOS:000545115200001,
Author = {Ide, Tobias and Lopez, Miguel Rodriguez and Froehlich, Christiane and
   Scheffran, Juergen},
Title = {Pathways to water conflict during drought in the MENA region},
Journal = {JOURNAL OF PEACE RESEARCH},
Year = {2021},
