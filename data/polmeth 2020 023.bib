@article{ WOS:000589710100007,
Author = {Heier, Tormod and Mobech-Hanssen, Bjorn E.},
Title = {National Defence in a Digital Crisis?},
Journal = {INTERNASJONAL POLITIKK},
Year = {2020},
Volume = {78},
Number = {3},
Pages = {362-382},
Abstract = {Even though the Norwegian authorities are world leaders in
   digitalization of public services, its armed forces are falling behind.
   In its essence, the problem lies in a management model with 14 different
   services striving for power and influence. This is particularly so when
   it comes to whom should have supreme authority as information and
   communication technology (ICT) systems are standardised across the
   force. How can we describe, explain and comprehend this digital
   complexity? Contemporary research does not provide clear answers, much
   due to over-emphasis on external cyberthreats. The question of why
   Norway's armed forces are incapable of providing a proper defence
   against cyberthreats therefore remains unanswered. By means of
   instrumental and cultural theories, we find grave deficiencies due to
   internal rivalry and organisational fragmentation.},
