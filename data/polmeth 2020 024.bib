@article{ WOS:000527424300001,
Author = {Martinson, Ryan D.},
Title = {Counter-intervention in Chinese naval strategy},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2021},
Volume = {44},
Number = {2, SI},
Pages = {265-287},
Month = {FEB 23},
Abstract = {The prospect of U.S. military involvement in a regional war looms large
   in Chinese naval strategy. This article examines the Chinese Navy's
   evolving role in countering U.S. military intervention in a conflict
   over Chinese-claimed offshore islands. This role has both wartime and
   peacetime aspects. In peacetime, the PLA Navy serves a deterrence
   function, demonstrating China's ability and resolve to fight the U.S.
   military if the U.S. were to intervene. In wartime, the operations of
   the PLA Navy would sit at the heart of any maritime campaign, helping to
   achieve China's territorial objectives in spite of U.S. involvement.},
