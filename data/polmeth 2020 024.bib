@article{ WOS:000602977900001,
Author = {Sheingate, Adam},
Title = {Policy Regime Decay(sic)(sic)(sic)Palabras clave},
Journal = {POLICY STUDIES JOURNAL},
Year = {2022},
Volume = {50},
Number = {1},
Pages = {65-89},
Month = {FEB},
Abstract = {This paper develops the concept of policy regime decay to examine what
   happens when the political foundations of public policies break down.
   Policy regime decay consists of an erosion of consensus over policy
   goals, an unraveling of support coalitions, and an exhaustion of
   institutional capacity to structure the policy process. These elements
   of decay have observable implications and they inform an empirical
   strategy for a case study of U.S. food and agriculture policy. Using
