@article{ WOS:000551358000001,
Author = {Eriksson, Johan and Privalov, Roman},
Title = {Russian space policy and identity: visionary or reactionary?},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2021},
Volume = {24},
Number = {2},
Pages = {381-407},
Month = {JUN},
Abstract = {Why is there a lack of grand, forward-looking vision in contemporary
   Russian space policy? Our study reveals nothing that compares with
   either ambitious Soviet goals or contemporary American goals of being
   first, reaching farthest, and being a dominant power in space; nor are
   there any clear explanations available of what goals Russia pursues in
   space. Notwithstanding the celebrational rhetoric on Russia being an
   `acknowledged leader' which recurrently refers to its superpower past,
   the substance of contemporary Russian space policy is not focused on
   hegemony but rather on reaching equal status, catching up, being
   competitive, and strengthening independent access to space. Whether
   motivated by a shift to a less ambitious great power identity seeking
   equal status rather than dominance or departing from a perception of
   inferiority in comparison with the West, Russian space policy
   simultaneously seeks lasting space cooperation with the US and
   criticises the US for militarisation of space. This may seem paradoxical
   from a geopolitical perspective, but it makes sense from an identity
   perspective; for better or worse, the US remains Russia's `significant
   other' in space.},
