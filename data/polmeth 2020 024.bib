@article{ WOS:000523816200001,
Author = {Gheaus, Anca},
Title = {Child-rearing With Minimal Domination: A Republican Account},
Journal = {POLITICAL STUDIES},
Year = {2021},
Volume = {69},
Number = {3},
Pages = {748-766},
Month = {AUG},
Abstract = {Parenting involves an extraordinary degree of power over children.
   Republicans are concerned about domination, which, on one view, is the
   holding of power that fails to track the interests of those over whom it
   is exercised. On this account, parenting as we know it is dominating due
   to the low standards necessary for acquiring and retaining parental
   rights and the extent of parental power. Domination cannot be fully
   eliminated from child-rearing without unacceptable loss of value. Most
   likely, republicanism requires that we minimise children's domination. I
   examine alternative models of child-rearing that are immune to
   republican criticism.},
