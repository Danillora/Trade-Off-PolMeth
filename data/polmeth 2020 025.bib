@article{ WOS:000604994300006,
Author = {Miller-Idriss, Cynthia},
Title = {Afterword: whither gender and the far right?},
Journal = {POLITICS RELIGION \& IDEOLOGY},
Year = {2020},
Volume = {21},
Number = {4, SI},
Pages = {487-492},
Month = {OCT 1},
Abstract = {This afterward synthesizes the primary arguments of each of the
   contributors to this volume, and suggests several next steps for
   analyses of gender and the far right. The chapter suggests that there is
   a need for more serious inquiry in four domains: on the intersections of
   extremism and gender-based violence, on how gender relates to
