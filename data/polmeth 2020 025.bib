@article{ WOS:000542311200001,
Author = {Pettersson, Therese and Oberg, Magnus},
Title = {Organized violence, 1989-2019},
Journal = {JOURNAL OF PEACE RESEARCH},
Year = {2020},
Volume = {57},
Number = {4},
Pages = {597-613},
Month = {JUL},
Abstract = {This article reports on trends in organized violence, building on new
   data by the Uppsala Conflict Data Program (UCDP). The defeat of Islamic
   State (IS) in Syria and Iraq has pushed the number of fatalities, almost
   75,600, to its lowest level since the outbreak of the Syrian civil war
   in 2011. However, this de-escalation in Syria is countered by increased
   violence in Africa, as IS and other transnational jihadist groups have
   relocated their efforts there. Furthermore, violence has continued to
   increase in Afghanistan; UCDP recorded more than 31,200 fatalities in
   Afghanistan in 2019, which accounts for 40\% of all fatalities from
   organized violence across the globe. The general decline in fatalities
   from organized violence does not correspond with the trend in the number
   of active conflicts, which remained on a historically high level. UCDP
   recorded 54 state-based conflicts in 2019, including seven wars.
   Twenty-eight state-based conflicts involved IS (Islamic State), al-Qaida
   or their affiliates. In the past decade, conflicts involving these
