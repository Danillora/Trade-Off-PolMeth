@article{ WOS:000546184800001,
Author = {Lazarus, Latoya},
Title = {Enacting citizenship, debating sex and sexuality: conservative
   Christians' participation in legal processes in Jamaica and Belize},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2020},
Volume = {58},
Number = {3, SI},
Pages = {366-386},
Month = {JUL 2},
Abstract = {This article critically examines conservative Christians' participation
   in two legal processes: the review of Jamaica's 2009 Sexual Offences Act
   and related laws in 2017 and the Belizean constitutional challenge by
   Caleb Orozco in 2011-2016. It reveals how some conservative Christians'
   active participation foregrounds struggles over morality and nationhood.
   It also highlights common strategies used in their mobilisations;
   namely, an appeal to certain notions of democracy, whilst asserting the
   continuous relevance of certain Christian principles and teachings.
   These engagements address the present role of religion in the public
   spheres of these `postcolonial' nation-states and the possible dilemmas
   posed for democracy.},
