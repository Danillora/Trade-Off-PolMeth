@article{ WOS:000515530800001,
Author = {Pichler, Melanie and Brand, Ulrich and Goerg, Christoph},
Title = {The double materiality of democracy in capitalist societies: challenges
   for social-ecological transformations},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2020},
Volume = {29},
Number = {2},
Pages = {193-213},
Month = {FEB 23},
Abstract = {The complex relationship between democracy and the ecological crisis in
   capitalist societies and the associated challenges for social-ecological
   transformations is addressed. Based on a review that differentiates four
   strands of literature dealing with the role of the environment and
   democratic politics, it is argued that, despite their differences, most
   literature lacks a material foundation of contemporary democracies -
   both in its social and biophysical dimensions. In line with
   historical-materialist social theory, the concept of a double
   materiality of democracy is introduced to provide for an adequate
   understanding of the challenges for social-ecological transformations
   faced by current capitalist and fossil-fuel based democracies. The
   renewable energy transition illustrates these challenges.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
