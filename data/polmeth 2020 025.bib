@article{ WOS:000596857300010,
Author = {Steinveg, Beate and Medby, Ingrid A.},
Title = {Norwegian High North Narratives and Identity Construction in the North},
Journal = {INTERNASJONAL POLITIKK},
Year = {2020},
Volume = {78},
Number = {4},
Pages = {535-544},
Abstract = {In this short article we consider how Norwegian High North policies have
   been communicated through political ``narratives{''}. Through Narrative
   Political Analysis, we highlight the use of ideas about past, present
   and future in and as ``the north{''}. Domestically, this has meant a
   renarrativisation of identity and an increased focus on elements such as
   ocean and coastal histories; and internationally, it has meant a
   reassessment of Norway's position in the world. No least the country's
   international role has in this narrative been characterised by ideas of
   leadership, sustainability, and responsibility. We conclude that the
   High North narrative has evolved over time, but that the story is far
   from over; it continues at steady pace northwards - and towards the
   ocean.},
