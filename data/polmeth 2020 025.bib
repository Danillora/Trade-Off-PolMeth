@article{ WOS:000618435600001,
Author = {Rueda, Daniel},
Title = {Is Populism a Political Strategy? A Critique of an Enduring Approach},
Journal = {POLITICAL STUDIES},
Year = {2021},
Volume = {69},
Number = {2},
Pages = {167-184},
Month = {MAY},
Abstract = {The political-strategic approach is one of the most employed frameworks
   within the methodologically heterogeneous subfield of populism studies.
   In the last two decades, it has contributed to the analysis of populism
   both in Latin America and the United States and, more recently, in
   Western and Eastern Europe. That being said, a close inspection of its
   axioms and its conceptualization of the phenomenon shows that it is
   built on ill-conceived premises. This article intends to be a
   comprehensive critique of the approach that can contribute to the
   methodological progress of the field. It criticizes the three main
   dysfunctions of the approach: selective rationalism, leader-centrism,
   and normative bias.},
