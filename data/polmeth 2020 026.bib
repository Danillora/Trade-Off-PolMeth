@article{ WOS:000523320900008,
Author = {Bizberg, Ilan},
Title = {THE FAILURE OF CONTINUITY. POLITICAL ECONOMY IN THE ENRIQUE PENA NIETO
   ADMINISTRATION},
Journal = {FORO INTERNACIONAL},
Year = {2020},
Volume = {60},
Number = {2},
Pages = {629-682},
Month = {APR-JUN},
Abstract = {The presidency of Enrique Pena Nieto was supposed to mark a 180-degree
   turn with respect to the dominant issue of the previous government: the
   war on organized crime. To achieve this, a decision was made to focus on
   structural economic reforms, by expanding the model first implemented in
   the 1980s and that had continued undisturbed over four governments. This
   is despite the fact that over the past thirty years the performance of
   the Mexican economy, together with its socioeconomic impact, has been
   disappointing. This under-performance is all the more marked in light of
   the exceptional conditions in which the global economy developed between
   2000 and 2014, known as the commodities super cycle. This was
   characterized by accelerated growth in China that provided a huge
   stimulus for the demand and prices of raw materials, together with the
   expansion of financial resources from the United States, which led the
   majority of countries in Latin America to grow faster and, above all, to
   do so while reducing poverty and inequality. By contrast, Mexico failed
   to take advantage of the very favorable global economic conditions to
   improve the socioeconomic situation of its population.},
