@article{ WOS:000508081100001,
Author = {Levy, Yagil},
Title = {Theorizing the domestic legitimacy of using force},
Journal = {INTERNATIONAL POLITICS},
Year = {2021},
Volume = {58},
Number = {1},
Pages = {1-17},
Month = {FEB},
Abstract = {The question of what constitutes the legitimacy of using force against
   an external adversary has become especially relevant since the wars that
   followed 9/11, and post-Cold War interventions in human crises. This
   article is conceptually motivated to bridging some of the scholarly
   gaps, mainly by developing a systematic methodological approach to
   analyzing how democratic governments try to establish domestic
   legitimacy for using force, defining that legitimacy and
   operationalizing it. Thereafter, it analyzes the two components of this
   legitimacy-the ingrained and the dynamic-and the interrelationships
   between them, thereby developing a framework for an empirical analysis
   of specific cases.},
