Author-Email = {rsweet2@nd.edu},
Affiliations = {University of Notre Dame},
Funding-Acknowledgement = {Fulbright-Hays Doctoral Dissertation Research Award; Harry F. Guggenheim
   Dissertation Fellowship; Program of African Studies at Northwestern
   University; Harvard Academy of International and Area Studies at Harvard
   University},
Funding-Text = {Support for this research was provided by the Fulbright-Hays Doctoral
