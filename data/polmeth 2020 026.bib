@article{ WOS:000535273800010,
Author = {Shah, Qasim Ali and Nawab, Bahadar and Mehmood, Tahir},
Title = {The Role of Stakeholders in Post Conflict Peacebuilding in Swat,
   Pakistan},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2020},
Volume = {18},
Number = {1},
Pages = {211-229},
Month = {JAN},
Abstract = {Peacebuilding is a continuous process to transform conflicts into
   development opportunities for and by the stakeholders. This article
   explores the role of stakeholders in post-conflict peacebuilding in
   Swat. Applying Constructivist paradigm and Discourse Analysis, 80
   semistructured interviews were conducted by incorporating local
   community, civil society and the government. Study finds out that
   cultural, political, social and economic tiers of peacebuilding measures
   in Swat hardly achieved its purpose. The lack of institutional
   coordination and gaps in peacebuilding measures are important hurdles,
   which needs to be minimized for sustainable development processes in
   Swat.},
