@article{ WOS:000584653900004,
Author = {Moore, David},
Title = {Toward non-hagiographical reflections on Zimbabwe's `heroes': Dumiso
   Dabengwa's history},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2020},
Volume = {47},
Number = {165},
Pages = {449-468},
Month = {JUL 2},
Abstract = {Dumiso Dabengwa's death in May 2019 offers the opportunity to reflect on
   Zimbabwe's fraught liberation struggle and its consequences today. (1)
   That Robert Mugabe's passing was a few months later makes such
   considerations more pertinent. This article thus revisits some of the
   liberation war's moments of evident tension between its two main
   nationalist movements, as well as those in the post-1980 era. It also
   considers some of Dabengwa's last, most historically important and
   philosophically interesting reflections.},
