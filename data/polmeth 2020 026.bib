@article{ WOS:000560853700001,
Author = {Hadden, Jennifer and Bush, Sarah Sunn},
Title = {What's different about the environment? Environmental INGOs in
   comparative perspective},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2021},
Volume = {30},
Number = {1-2, SI},
Pages = {202-223},
Month = {FEB 23},
Abstract = {Existing scholarship has established that environmental NGOs influence
   political and ecological outcomes across a variety of settings. Yet this
   work tells us relatively little about how the networks, strategies, and
   organizations in the environmental NGO sector compare to other NGO
   sectors. This article argues that adopting a cross-sectoral perspective
   illuminates how theory and best practice developed for other INGO
   sectors can apply to the study of environmental INGOs, and vice versa.
   Drawing on quantitative data we find that a) on average, the ENGO sector
   is not more collaborative, better networked, more institutionalized, or
   more geographically imbalanced than other sectors; b) the ENGO sector is
   marked by a uniquely high level of concentration in terms of relational
   and economic resources. We call for more attention to the origins of
   these sectoral differences, and also draw out their implications for
   ENGO performance, suggesting that theories of `gatekeeping' may be
   particularly relevant to the study of environmental groups.},
