@article{ WOS:000509085900001,
Author = {Ashukem, Jean-Claude N.},
Title = {The SDGs and the bio-economy: fostering land-grabbing in Africa},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
