@article{ WOS:000586813200001,
Author = {Tunmibi, Sunday and Olatokun, Wole},
Title = {Application of digits based test to analyse presidential election data
   in Nigeria},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2021},
Volume = {59},
Number = {1},
Pages = {1-24},
Month = {JAN 2},
Abstract = {This study examined the application of Benford's Law to analyse the 2015
   and 2019 presidential election data in Nigeria. Data obtained from the
   electoral body were analysed using frequency distribution and Spearman
   Rank Correlation tests at 0.05 level of significance. Findings revealed
   that the second digits distributions of the vote counts for the two
   major political parties, registered voters and total vote counts in both
   elections do not conform to the distributional pattern predicted by
   Benford's Law. The study recommends that stakeholders should aim for an
   electoral reform that allows the use of forensic analysis of electoral
   data in Nigeria.},
