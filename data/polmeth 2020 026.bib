@article{ WOS:000527974000001,
Author = {Szymanowski, Rafal},
Title = {One step too far? The euro and European integration},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2020},
Volume = {42},
Number = {4},
Pages = {601-608},
Month = {MAY 18},
Abstract = {This review discusses four recent books which engage in a discussion on
   the euro and its meaning for European integration. Two of the reviewed
   books claim that the single currency is killing the European Union from
   the inside. They argue that even the most comprehensive proposals for
   the Eurozone reform are unable to repair the monetary union that was
   doomed to fail from the start. The other two studies reject this notion
   and insist that Europe's greatest mistake was not the creation of the
   single currency itself, but rather the negligence to supplement the euro
   with an institutional set-up appropriate for handling the banking crisis
   and overreliance on the US-style wholesale funding market. Read
   together, these contributions shed new light on the euro's biggest
   dilemmas after its twentieth anniversary and provide a valuable overview
   of the possible paths the euro might take in its third decade.},
