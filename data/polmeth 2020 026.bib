@incollection{ WOS:000832209900003,
Author = {Adamides, Constantinos},
Book-Author = {Adamides, C},
Title = {Protracted Conflicts: A Fertile Environment for Routinized
   Securitization},
Booktitle = {SECURITIZATION AND DESECURITIZATION PROCESSES IN PROTRACTED CONFLICTS:
   THE CASE OF CYPRUS},
Series = {Rethinking Peace and Conflict Studies},
Year = {2020},
Pages = {15-23},
Abstract = {This chapter provides an overview of the literature on protracted
   conflicts and more specifically on the characteristics of such
   conflicts, as they create a fertile ground for the development of the
   alternative modes of securitization examined in the book, namely the
   routinized, institutionalized, horizontal and bottom-up securitization.
   Particular attention is given to the psychological investments of the
   conflict parties and how it affects the social context in which
   securitization and audiences interact, construct and deconstruct
