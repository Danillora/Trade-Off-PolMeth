@article{ WOS:000555474500001,
Author = {Lacatus, Corina and Meibauer, Gustav},
Title = {Introduction to the special issue: Elections, rhetoric and American
   foreign policy in the age of Donald Trump},
Journal = {POLITICS},
Year = {2021},
Volume = {41},
Number = {1, SI},
Pages = {3-14},
Month = {FEB},
Abstract = {This introduction presents the special issue's conceptual and empirical
   starting points and situates the special issue's intended contributions.
   It does so by reviewing extant scholarship on electoral rhetoric and
   foreign policy and by teasing out several possible linkages between
