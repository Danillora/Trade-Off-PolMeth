@article{ WOS:000544918400001,
Author = {Cook, Joana and Haid, Haid and Trauthig, Inga},
Title = {``Jurisprudence Beyond the State: An Analysis of Jihadist ``Justice{''}
   in Yemen, Syria and Libya},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
