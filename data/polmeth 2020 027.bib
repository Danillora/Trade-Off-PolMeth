@article{ WOS:000511926500003,
Author = {Pedersen, Rasmus Brun},
Title = {Jumping on the Bandwagon: status seeking as a driver for Sweden's
   involvement in NATO-led operations?},
Journal = {INTERNATIONAL POLITICS},
Year = {2020},
Volume = {57},
Number = {1},
Pages = {41-56},
Month = {FEB},
Abstract = {Why do the non-aligned small states such as Sweden participate in
   NATO-led operations and interventions where it has no immanent interests
   at play? This article argues that status seeking and building a good
   relation with the USA plays a central role in explaining small states
   participation in US-led interventions. Theoretically, the article
   contributes to our understanding of the concept of `status' in
   international relations by offering a new explanation of the puzzling
   willingness of small unaligned states to use military means in NATO-led
   operations. Empirically, the article contributes to our understanding of
   the status-seeking strategies of the Nordic countries, which indicates
   that the Nordic countries in some respect have departed from the
   traditional Nordic emphasis on soft power.},
