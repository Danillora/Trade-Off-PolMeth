@article{ WOS:000567072300001,
Author = {van Elten, Katharina and Rehder, Britta},
Title = {Dieselgate and Eurolegalism. How a scandal fosters the Americanization
   of European law},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2022},
Volume = {29},
Number = {2},
Pages = {281-300},
Month = {FEB 1},
Abstract = {The paper deals with the conflicts associated with the so-called
   `Dieselgate' affair. It explains the interaction of civil society, law
   firms and political actors which reshapes the legal systems in Europe.
   Once the federal government in Germany had decided that European car
   owners should not get any kind of financial compensation, a
   transnational coalition of consumer organizations and law firms
   successfully initiated a legal mobilization campaign to counteract the
   political decision. As a consequence, collective litigation rights were
   strengthened in Germany and at the European level. Theoretically, the
   paper refers to the literature on adversarial legalism. Whether this
   judicialized style of conflict resolution is spreading to Europe
   ('Eurolegalism') has been a hotly debated topic in political science.
   Our main argument is that the Dieselgate case is a catalyst event to
   foster the transformation process towards `Eurolegalism'.},
