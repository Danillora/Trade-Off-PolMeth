@article{ WOS:000590345200001,
Author = {Mazwi, Freedom},
Title = {Sugar production dynamics in Zimbabwe: an analysis of contract farming
   at Hippo Valley},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2020},
Volume = {47},
Number = {166},
Pages = {568-584},
Month = {OCT 1},
Abstract = {This article examines the nature of contractual relations between sugar
   outgrowers (contract farmers) and Tongaat Hulett Zimbabwe (THZ). Drawing
   from a research study conducted at Hippo Valley Estate, using a
   mixed-methods approach, findings reveal asymmetrical power relations in
   favour of THZ as reflected by its domination in price determination,
   monopsony control and risk sharing. Despite radicalisation and attempts
   by the state to curb the powers of international finance, the article
   argues that monopoly capital continues to wield significant control in
   Zimbabwe, leading to growing farmer indebtedness. Thus, the
   state-capital alliance presents a stumbling block for accumulation by
   sugar outgrowers.},
