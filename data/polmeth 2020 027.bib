@article{ WOS:000519165700005,
Author = {Bisley, Nick},
Title = {Australia's American Alliance and the networking of forces in East Asia},
Journal = {INTERNATIONAL POLITICS},
Year = {2020},
Volume = {57},
Number = {2, SI},
Pages = {208-224},
Month = {APR},
Abstract = {As the region's security order is becoming unsettled, Australia has
   begun to adjust aspects of its strategic policy in response to these
   changes. This paper examines the evolution of Australian defence and
   security policy so as to assess the extent to which Australia's
   experience sheds light on changes to the structure of East Asia's
   security order. In response to the PRC's selective contestation of US
   hegemony, Australia has tightened its relationship with the USA,
   developed new strategic partnerships, committed to increasing its
   military capabilities while also actively engaging with security
   multilateralism. Through this, it is helping American regional hegemony
   develop an emerging networked quality. Its experience also shows the
   ongoing the dependence of the medium powers on the USA. Their
   operational and strategic limitations mean that while it has been
   restructured somewhat through diversification and networking, the East
   Asian hegemonic order remains organised around the USA.},
