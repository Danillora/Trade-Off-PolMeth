@article{ WOS:000546333600007,
Author = {Koenig, Pascal D. and Siewert, Markus B.},
Title = {Why don't citizens give governments credit when they deliver on
   electoral pledges?},
Journal = {POLICY AND POLITICS},
Year = {2020},
Volume = {48},
Number = {3},
Pages = {503-519},
Month = {JUL},
Abstract = {Why don't citizens give governments credit when they deliver on
   electoral pledges? This article develops an original analytical
   framework that addresses this important but under-researched question.
   It focuses on the concept of `credit claiming', which is the opposite of
   `blame shifting', and draws together existing research to identify a
   number of significant hurdles that governments must overcome in order to
   gain recognition for achievements. It highlights the challenges which
   policymakers face in communicating with citizens in an increasingly
   mediatised public sphere, and the extent to which their credit claiming
   efforts rely on the media as a `transmission belt'. It concludes that in
   liberal democracies governments are fighting an uphill battle to gain
   citizens' support and secure trust in broader democratic institutions
   and the political system as a whole.},
