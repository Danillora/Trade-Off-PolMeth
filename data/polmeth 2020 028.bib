@article{ WOS:000506936900001,
Author = {Hartung, Ulrich},
Title = {Inside Lobbying on the Regulation of New Plant Breeding Techniques in
   the European Union: Determinants of Venue Choices},
Journal = {REVIEW OF POLICY RESEARCH},
