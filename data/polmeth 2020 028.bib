@article{ WOS:000513650900010,
Author = {Jennings, Will and Farrall, Stephen and Gray, Emily and Hay, Colin},
Title = {Moral Panics and Punctuated Equilibrium in Public Policy: An Analysis of
   the Criminal Justice Policy Agenda in Britain},
Journal = {POLICY STUDIES JOURNAL},
