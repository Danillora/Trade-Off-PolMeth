@article{ WOS:000536507600003,
Author = {Buchanan, John and Chai, Dominic H. and Deakin, Simon},
Title = {Unexpected corporate outcomes from hedge fund activism in Japan},
Journal = {SOCIO-ECONOMIC REVIEW},
Year = {2020},
Volume = {18},
Number = {1},
Pages = {31-52},
Month = {JAN},
Abstract = {Hedge fund activism has been identified in the USA as a driver of
   enduring corporate governance change and market perception. We
   investigate this claim in an empirical study to see whether activism
   produced similar results in Japan in four representative areas:
   management effectiveness, managerial decisions, labour management and
   market perception. Experience from the USA would predict positive
   changes at Japanese target companies in these four areas. However,
   analysis of financial data shows that no enduring changes were apparent
   in the first three areas, and that market perception was consistently
   unfavourable. Our findings demonstrate that the same pressures need not
   produce the same results in different markets. Moreover, while the
   effects of the global financial crisis should not be ignored, we
   conclude that the country-level differences in corporate governance
   identified in the varieties of capitalism literature are robust, at
   least in the short term.},
