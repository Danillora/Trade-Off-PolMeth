@article{ WOS:000566764800001,
Author = {Buarque, Daniel},
Title = {A country on the fence: United Kingdom's perceptions of the status and
   international agenda of Brazil},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2020},
Volume = {63},
Number = {1},
Abstract = {Brazil has long strived for greater status and a stronger role in
   international politics, but its actual standing depends on the
   recognition it gets from outside. This paper analyses the perceptions of
   the status of Brazil from the perspective of the UK. It contributes both
   to the study of status in IR (by using a qualitative method with focus
   on perceptions and inter-subjectivity) and to the study of the status of
   Brazil (by analysing external perceptions about its standing in the
   global hierarchy). Based on interviews with British diplomats who served
   in Brazil, it shows that the UK believes the country misperceives its
   own place in the world and appears to remain ``on the fence{''} in
   global affairs.},
