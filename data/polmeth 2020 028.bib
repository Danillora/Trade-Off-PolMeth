@article{ WOS:000572809700001,
Author = {Widmann, Tobias},
Title = {How Emotional Are Populists Really? Factors Explaining Emotional Appeals
   in the Communication of Political Parties},
Journal = {POLITICAL PSYCHOLOGY},
