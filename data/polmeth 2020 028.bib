@article{ WOS:000573466200001,
Author = {Kleinberg, Mona S. and Lau, Richard R.},
Title = {Googling Politics: How Offloading Affects Voting and Political Knowledge},
Journal = {POLITICAL PSYCHOLOGY},
Year = {2021},
Volume = {42},
Number = {1},
Pages = {93-110},
Month = {FEB},
Abstract = {Studies suggest that individuals are less likely to process information
   they can easily look up. Instead of committing information to memory,
   they rely on the Internet to store information for them-a phenomenon
   known as ``offloading.{''} We examine the effects of offloading on
   political knowledge and voting behavior. Using data from a 10-wave panel
   experiment designed to study information processing in two distinct
   information environments, we demonstrate that people whose environment
   is conducive to offloading learnmoreduring an election campaign than
   individuals whose information environment is not conducive to
   offloading, even though they look at less information during the
   campaign. Individuals in the offloading condition also make better vote
   choices despite examining less information. These results suggest that
   offloading reduces cognitive load, thereby freeing up processing space
   in working memory, which can increase learning of information
   thatisaccessed, and improve decision-making.},
