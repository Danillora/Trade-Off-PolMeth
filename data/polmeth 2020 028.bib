@article{ WOS:000533733200001,
Author = {Freedman, Eric},
Title = {In the crosshairs: The perils of environmental journalism},
Journal = {JOURNAL OF HUMAN RIGHTS},
Year = {2020},
Volume = {19},
Number = {3},
Pages = {275-290},
Month = {MAY 26},
Abstract = {Journalists covering environmental issues around the globe are at
   heightened risk of murder, arrest, assault, threats, self-exile,
   lawsuits, and harassment because environmental controversies often
   involve influential business and economic interests, political power
   battles, criminal activities, and corruption, plus politically,
   culturally, and economically sensitive issues concerning Indigenous
   rights to land and natural resources. This study uses in-depth
   interviews with journalists and other experts to explore such
   situations, including the psychological effects on these journalists'
   sense of mission and professional practices. Interviewees generally did
   not seek therapy or counseling after their adverse experiences. Those
   experiences led some journalists to change their careers but
   strengthened the professional commitment that other journalists feel.},
