@article{ WOS:000569352000001,
Author = {Munyi, Elijah N.},
Title = {Africa's stalled structural transformation: the end of the flying geese?},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2020},
Volume = {47},
Number = {165},
Pages = {474-483},
Month = {JUL 2},
Abstract = {Is structural transformation happening in Africa, and can African states
   count on the neoliberal flying geese paradigm for industrialisation?
   Using recent data from World Bank and United Nations Conference of Trade
   and Development reports, this briefing suggests not. The post-2008
   crisis `end of labour-cost arbitrage opportunities' have nullified
   Africa's attractiveness in global labour-intensive manufacturing. This
   is a position that negates the generally optimistic narrative recently
   espoused by theJournal of African Economies(vol. 27) on Africa's
   industrialisation and China's putative catalytic role in it.},
