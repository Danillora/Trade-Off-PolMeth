@article{ WOS:000563416100001,
Author = {Brooks, Eleanor and Burgin, Alexander},
Title = {Political steering in the European Commission: a comparison of the
   energy and health sectors},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2021},
Volume = {43},
Number = {6},
Pages = {755-771},
Month = {AUG 18},
Abstract = {Upon taking office as President of the European Commission in 2014,
   Jean-Claude Juncker initiated a series of reforms to the internal
   workings of the EU executive. Existing research suggests that these have
   been successful in increasing political steering, creating a Commission
   which focuses on fewer fields, controls the policy activism of its
   officials and produces less legislative output. However, much of this
   research focuses on the Commission as a whole. This article addresses a
   gap in the literature by exploring how the Juncker reforms have impacted
   upon the policy-making structures and internal dynamics within two
   contrasting policy sectors - energy and health. It finds that the
   reforms have been successful in prioritising selected policy initiatives
   and increasing top-down steering of the Commission but that, whilst this
   has led to centralised coordination and reduced conflict in energy
   policy, it has served to curb purposeful opportunism and legislative
   activity in health policy.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
