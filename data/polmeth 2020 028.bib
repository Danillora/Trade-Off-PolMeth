@article{ WOS:000531670100001,
Author = {Broms, Rasmus and Dahlstrom, Carl and Nistotskaya, Marina},
Title = {Competition and service quality: Evidence from Swedish residential care
   homes},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2020},
Volume = {33},
Number = {3},
Pages = {525-543},
Month = {JUL},
Abstract = {Against a backdrop of increased levels of marketization of welfare
   services in OECD countries, this article aims to shed light on the
   separate effects of private ownership and competition for the market on
   service quality. Using residential elder care homes in Sweden as our
   case, we leverage unique panel data of ownership and competition against
   a set of indicators, pertaining to the structure, process, and outcome
   dimensions of care quality. The main finding of our analyses is that
   competition for the market does surprisingly little for quality: private
   entrepreneurs perform neither better nor worse under stiff competition
   and the quality of care is approximately the same in those nursing homes
   that are exposed to the market as in those that are not.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
