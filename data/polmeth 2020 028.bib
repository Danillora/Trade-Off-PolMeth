@article{ WOS:000587994200008,
Author = {Guzzini, Stefano},
Title = {Protean power as a plea for an open social ontology,
   non-efficient causal explanations, and cautious political practice},
Journal = {INTERNATIONAL THEORY},
Year = {2020},
Volume = {12},
Number = {3},
Pages = {449-458},
Month = {NOV},
Abstract = {This paper probes the attempt to use power analysis to link three
   domains: ontology, explanation, and the strategy of political actors. It
   shows how Katzenstein and Seybert develop an open social ontology that
   serves as the backdrop for explanations that need to be causal but
   indeterminate, and a cautious political practice. It exposes tensions in
   two important links. First, the open ontology becomes retranslated as an
   explanatory cause. Second, the call for being cautious because of
   `unknown unknowns' may just as well invite strategies of doubling down
   in control power.},
