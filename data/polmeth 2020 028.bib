@article{ WOS:000539089800001,
Author = {Hallama, Peter},
Title = {Struggling for a Socialist Fatherhood: ``Re-educating{''} Men in East
   Germany, 1960-1989},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
