@article{ WOS:000604726600001,
Author = {Blout, Emily and Burkart, Patrick},
Title = {White Supremacist Terrorism in Charlottesville: Reconstructing `Unite
   the Right'},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2023},
Volume = {46},
Number = {9},
Pages = {1624-1652},
Month = {SEP 2},
Abstract = {This paper is a qualitative study of the hybrid tactics of Unite the
   Right (UtR) participants before, during, and after the events of August
   11-12, 2017 in Charlottesville, Virginia. Drawing upon symbolic
   interactionism and levels of analysis theory, the authors detail UtR
   activities as manifested in three theaters: symbolically mediated,
   systems-technical/cybernetic and physical/kinetic. Three findings are
   discussed: that UtR was planned and executed as a martial event, that
   there was a robust information campaign targeting city officials and
   residents, and that there was cybernetic action targeting the city.
   Finally, the paper offers the term ``immersive terrorism{''} to describe
   the extended, multilevel, and multimodal nature of the UtR terror
   campaign.},
