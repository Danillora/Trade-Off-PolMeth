@article{ WOS:000532141800001,
Author = {Murphy, David Thomas},
Title = {A big, new Hitler? Capitalism, `Anglo-America', and Hitler's war},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2020},
Volume = {43},
Number = {6-7, SI},
Pages = {1063-1077},
Month = {NOV 9},
Abstract = {A new biography of Adolf Hitler contends that historians have overlooked
   Hitler's obsessive hatred for capitalism, and challenges the dominant
   historiography by asserting that both the Fuhrer's own war aims as well
   as German military resources were focused overwhelmingly on the
   democratic regimes to his west rather than on the communist one to his
   east. This essay reviews the argument, critically interrogating the
   work's source utilization as well as the implications of this view for
   historical conceptualization of the aims of Hitler's war.},
