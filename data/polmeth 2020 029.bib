@article{ WOS:000533643200007,
Author = {Brown, M. Anne},
Title = {The Body in the Emergence of Trust},
Journal = {ETHNOPOLITICS},
Year = {2020},
Volume = {19},
Number = {2},
Pages = {209-227},
Month = {MAR 14},
Abstract = {This discussion considers the significance of physical contact-in the
   sense of bodily and spatial presence or interaction-in the emergence of
   trust in the context of a legacy of violence. As an element of
   reconciliation (and like reconciliation), trust is understood as a
   continuum, rather than a completed state. Without proposing that
   physical contact is always essential for trust, or that relational trust
   removes the need for political settlements, the paper argues there is a
   visceral, face-to-face dimension to building trust that is often
   overlooked by more political accounts of reconciliation. Overlooking
   dimensions that involve feeling and body in the emergence of trust is
   likely to compromise the potential for effective reconciliation,
   particularly in contexts involving significant intercommunal violence.
   The argument is informed by case studies, illustrations and observation.
   It seeks to contribute to thinking about trust, particularly in contexts
   of reconciliation, through focussing on a significant, and arguably
   fundamental, dimension of it.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
