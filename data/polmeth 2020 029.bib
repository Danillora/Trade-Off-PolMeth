   Steinke, Andrea, Free Univ Berlin, ZI Lateinamer Inst, Berlin, Germany.},
DOI = {10.1080/21647259.2018.1491277},
ISSN = {2164-7259},
EISSN = {2164-7267},
Keywords = {Stabilisation; Counterinsurgency; Haiti; Brazil; Peacekeeping},
Keywords-Plus = {INTERVENTION; OPERATION; SECURITY},
