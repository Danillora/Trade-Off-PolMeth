@article{ WOS:000557622000001,
Author = {Portela, Clara and Pospieszna, Paulina and Skrzypczynska, Joanna and
   Walentek, Dawid},
Title = {Consensus against all odds: explaining the persistence of EU sanctions
   on Russia},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2021},
Volume = {43},
Number = {6},
Pages = {683-699},
Month = {AUG 18},
Abstract = {In response to Russia's actions in Ukraine in 2014, the EU introduced
   sanctions on Moscow. Despite increasing polarisation among member states
   after imposition, the sanctions package was consistently renewed. How
   can sanctions persistence be explained? While scholarly accounts
   highlight German leadership, commitment to norms, and policymakers'
   engagement, the EU's ability to uphold the sanctions in the face of
   uneven support among member states remains puzzling. With the help of a
   two-level game framework, according to which actors make decisions based
   on the interplay between the domestic and international levels, we argue
   that the interaction between the Council and domestic politics helped
   sustaining the consensus. To illustrate this dynamic, in an exploration
   of domestic factions in Spain and Poland, two member states displaying
   opposite attitudes towards Russia, we identify the presence of at least
   one actor whose preference deviates from the core, thereby facilitating
   consensus.},
