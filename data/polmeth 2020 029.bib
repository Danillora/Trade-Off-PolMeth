@article{ WOS:000767994200003,
Author = {Ngouana, Lyonnelle},
Title = {The communication space of humanitarian organizations as a space for
   negotiating legitimacy issues},
Journal = {ETUDES INTERNATIONALES},
Year = {2020},
Volume = {51},
Number = {3},
Pages = {455-479},
Month = {FAL},
Abstract = {Communication campaigns by humanitarian organizations are, beyond the
   instruments for convening a committed reaction, spaces for legitimizing
   an existence that must constantly be justified because it cannot be
   taken for granted. This discussion explores some issues of legitimacy in
   the humanitarian field and the mechanisms of affirmation and
   reactivation of legitimacy at stake in the communication devices of
   humanitarian NGOs. The aim is to highlight the place of the image in
   these processes, by showing how, through the humanitarian
   representations of the victim, humanitarian organizations legitimize
   their existence.},
