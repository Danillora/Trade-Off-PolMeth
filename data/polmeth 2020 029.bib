@article{ WOS:000509390300009,
Author = {Albalate, Daniel and Bel, Germa},
Title = {Do government formation deadlocks really damage economic growth?
   Evidence from history's longest period of government formation impasse},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2020},
Volume = {33},
Number = {1, SI},
Pages = {155-171},
Month = {JAN},
Abstract = {Several countries have experienced lengthy periods of government
   formation deadlock in recent years, as they have sought to form a new
   government. This study examines whether government formation deadlocks
   damage a country's economy. To do so, we analyze the case of Belgium,
   which took a record 541 days to create a post-election government,
   following the June 2010 federal elections. Employing the synthetic
   control method, our results show that the Belgium economy did not suffer
   an economic toll; on the contrary, gross domestic product per capita
   growth was higher than would have otherwise been expected. As such, our
   evidence contradicts frequent claims that long periods of government
   formation deadlock negatively affect an economy.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Albalate, D (Corresponding Author), Univ Barcelona, Dept Econometr Stat \& Appl Econ, Barcelona, Spain.
   Albalate, Daniel; Bel, Germa, Univ Barcelona, Dept Econometr Stat \& Appl Econ, Barcelona, Spain.
   Albalate, Daniel; Bel, Germa, Observ Policy Anal \& Evaluat OAP UB, Barcelona, Spain.},
DOI = {10.1111/gove.12410},
