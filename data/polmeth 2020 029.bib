@article{ WOS:000555026100002,
Author = {Selling, Niels},
Title = {What to lobby on? Explaining why large American firms lobby on the same
   or different issues},
Journal = {BUSINESS AND POLITICS},
Year = {2020},
Volume = {22},
Number = {3},
Pages = {449-476},
Month = {SEP},
Abstract = {What determines whether or not firms lobby on the same policy issues?
   Scholars offer two broad answers to this question. Firms that are (1)
   similar or (2) connected through interorganizational ties target the
   same policy issues. In this article, I argue that the co-occurrence of
   these two conditions produces the opposite outcome, namely a tendency to
   lobby on different issues. This expectation draws on ideas from
   collective action theory and the literature on issue niches. From these,
   I derive the following assumptions: similar firms share political
   objectives and they should, when possible, act collectively by jointly
   delegating their lobbying activities. The reason for doing this is that
   it allows them to focus on their issue niches. However, the ability to
   delegate hinges on coordination and monitoring, which is facilitated by
   interorganizational relations. To test this proposition, I study the
   largest American corporations. The dependent variable isactivity
