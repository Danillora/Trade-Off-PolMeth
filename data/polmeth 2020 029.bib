@article{ WOS:000550531500003,
Author = {Alakoc, Burcu Pinar},
Title = {Femme Fatale: The Lethality of Female Suicide Bombers},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2020},
Volume = {43},
Number = {9},
Pages = {796-814},
Month = {SEP 1},
Abstract = {Are female suicide bombers deadlier than male suicide bombers? Utilizing
   newly coded data on the tactical attributes of suicide terrorism
   worldwide from 1998 to 2015, this study shows that the use of female
   suicide bombers is not only positively correlated with the lethality of
   the suicide attacks, but also accentuates the existing tactical
   advantages of suicide terrorism. Especially in the cases of soft targets
   like civilians, and easily accessible locations, the deadliest outcomes
   result from those attacks carried out by female suicide bombers.},
