@article{ WOS:000511748800001,
Author = {Marchment, Zoe and Gill, Paul},
Title = {Spatial Decision Making of Terrorist Target Selection: Introducing the
   TRACK Framework},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2022},
Volume = {45},
Number = {10},
Pages = {862-880},
Month = {OCT 3},
Abstract = {Guided by previous research and recent empirical analyses, this paper
   gives insight into elements that characterize the spatial decision
   making of terrorist target selection. Five key factors explain why
   targets are chosen by terrorists. The authors propose that, generally,
   targets will be selected when they are Tolerable, Relevant, Accessible,
   Close and/or Known. This is followed by a discussion of attacks
   witnessed between January 2013 and December 2018 in the United Kingdom,
   and implications.},
