@article{ WOS:000537423700002,
Author = {Brunjes, Benjamin M.},
Title = {Competition and Federal Contractor Performance},
Journal = {JOURNAL OF PUBLIC ADMINISTRATION RESEARCH AND THEORY},
Year = {2020},
Volume = {30},
Number = {2},
Pages = {202-219},
Month = {APR},
Abstract = {Contracts are increasingly used in government as a way to insert
   competition into public agencies and policy implementation. Competition
   among contractors is theorized to drive down costs, encourage
   innovation, and boost accountability. However, there have been few
   studies on whether competition actually leads to improved performance
   among contractors. This analysis examines nearly 25,000 federal
   contracts to determine how competition is related to performance.
   Findings indicate that competitively sourced contracts are more likely
   to suffer from performance problems. However, relationships between
   contractor and agency correlate with fewer performance problems. This
   suggests that competition may not be leading to the benefits that many
   might expect. Instead, relationships and shared experiences, along with
   other factors, may matter more when seeking to improve contractor
   performance.},
