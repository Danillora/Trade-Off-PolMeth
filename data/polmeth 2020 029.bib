@article{ WOS:000539824500001,
Author = {Mosier, Samantha L. and Rimal, Arbindra and Ruxton, Megan M.},
Title = {A Song of Policy Incongruence: The Missing Choir of Consumer Preferences
   in GMO-Labeling Policy Outcomes},
Journal = {REVIEW OF POLICY RESEARCH},
Year = {2020},
Volume = {37},
Number = {4},
Pages = {511-534},
Month = {JUL},
Abstract = {This article focuses on the degree of policy congruence, and by
   extension policy responsiveness, of U.S. federal- and state-level GMO
   labeling laws from 2011-2016. Utilizing consumer survey data, evidence
   overwhelmingly demonstrates consumers prefer clear text-based indication
   if food products contain genetically modified ingredients. However, the
   federal law adopted in 2016 mandates GMO labeling but with exceptions
   permitted to clear on-package text labeling. The results of this study
   demonstrate that consumer preferences were not adequately represented at
   the federal level and were misaligned with state policy activities as
   captured in the aggregate outcome. State legislatures were actively
   proposing mandatory legislation with only a few cases of success, which
   did not adequately represent the wishes of the people. Given the
   misalignment and overall policy incongruence, the consequences of
   pending federal law are discussed in light of why the voices of the
   consumer choir were not heard by lawmakers.},
