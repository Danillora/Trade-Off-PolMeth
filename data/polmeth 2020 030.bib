@article{ WOS:000524731700001,
Author = {Coman, Ramona},
Title = {The rotating presidency of the EU Council as a two-level game, or how
   the ``Brussels model{''} neutralises domestic political factors: the
   case of Romania},
Journal = {EAST EUROPEAN POLITICS},
Year = {2020},
Volume = {36},
Number = {4, SI},
Pages = {586-602},
Month = {OCT 1},
Abstract = {This article examines the Romanian rotating presidency of the Council of
   the European Union during the first half of 2019 as a two-level game. It
   shows how, despite heightened political tensions at the domestic and
   European level, the Romanian rotating presidency managed to fulfil its
   main functions. It is argued that the preparation and the conduct of the
   rotating presidency is not only a two-level game, but also one in which
   diplomats and civil servants play a central role. Their centrality in
   the process neutralises domestic political factors such as Eurosceptic
   governmental attitudes, the lack of political vision or credibility.},
