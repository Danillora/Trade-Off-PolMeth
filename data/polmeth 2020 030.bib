@article{ WOS:000572797200001,
Author = {de Oliveira, Felipe Antunes},
Title = {Of economic whips and political necessities: a contribution to the
   international political economy of uneven and combined development},
Journal = {CAMBRIDGE REVIEW OF INTERNATIONAL AFFAIRS},
