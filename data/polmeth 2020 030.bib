@article{ WOS:000751640800006,
Author = {Lo, Sonny S. H.},
Title = {Comparative corruption scandals in Macau: the cases of Ao Man-long and
   Ho Chio-meng},
Journal = {PUBLIC ADMINISTRATION AND POLICY-AN ASIA-PACIFIC JOURNAL},
Year = {2020},
Volume = {23},
Number = {1, SI},
Pages = {47-57},
Abstract = {Purpose - This paper aims at comparing and contrasting the Ao Man-long
   scandal with the Ho Chio-meng case in Macau, drawing lessons from the
   two events and casting lights on the literature on corruption scandals.
   Design/methodology/approach - The study used documentary research and
   interpretative and analytical approaches.
   Findings - The two cases show considerable administrative discretion on
   the part of the principal officials involved, and remedial measures
   along the line of having more rigorous and frequent internal auditing
   may be necessary.
   Originality/value - Original analyses were conducted together with
   literature reviewand documentary research. This paper would be of
   interest to scholars and practitioners concerned with how Macau combats
   corruption.},
