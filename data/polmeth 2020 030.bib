@article{ WOS:000607545600004,
Author = {Loding, Tomas Hostad},
Title = {The financialization of local governments-the case of financial
   rationality in the management of Norwegian hydroelectric utilities},
Journal = {SOCIO-ECONOMIC REVIEW},
