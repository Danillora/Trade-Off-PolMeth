@article{ WOS:000584052900001,
Author = {Rosenberg, Justin},
Title = {Uneven and combined development: a defense of the general abstraction},
Journal = {CAMBRIDGE REVIEW OF INTERNATIONAL AFFAIRS},
Year = {2022},
Volume = {35},
Number = {3},
Pages = {267-290},
Month = {JUL 19},
Abstract = {This article responds to criticisms of the current revival of Uneven and
   Combined Development (UCD), particularly those aimed at the idea of a
   `general abstraction' of UCD. Three main charges have been pressed: that
   UCD is not a real theory; that its transhistorical extension has reduced
   it to an unhistorical reification; and that its language of `advanced'
   and `backward' betrays its enduring Eurocentric foundations. The article
   argues not only that UCD can be defended, but also that it is the
   general abstraction which enables UCD to make its strongest
   contributions to solving problems of theoretical insufficiency,
   ahistoricism and Eurocentrism in social thought more generally. Finally
   the article ends by speculating on the reason why the much-maligned
   general abstraction of UCD should turn out to be of such significance.},
