@article{ WOS:000589840500006,
Author = {Pagliari, Stefano and Young, Kevin L.},
Title = {Exploring information exchange among interest groups: a text-reuse
   approach},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2020},
Volume = {27},
Number = {11, SI},
Pages = {1698-1717},
Month = {NOV 1},
Abstract = {The presence of communication channels plays a key role in how interest
   groups engage in EU policymaking. However, the capacity of researchers
   to explain these patterns is constrained by the informal and opaque
   nature of such interaction. In this paper we develop a novel
   text-as-data approach which maps the informal patterns of information
   exchange among the stakeholders that engage with EU policymaking by
   detecting instances of text reuse among the comment letters submitted by
   these groups to the same policy proposal. We use this approach to
   analyse a novel dataset of publicly available comments to a wide a range
   of EU policies. We find that there are significant differences between
   the structure of information exchange networks and more formal lobbying
   coalitions in the EU, as well as between the groups that engage in these
   forms of coordination.},
