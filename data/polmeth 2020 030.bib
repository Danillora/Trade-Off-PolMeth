@article{ WOS:000567159700001,
Author = {Cote, Isabelle and Huang, Limingcui Emma},
Title = {Where are the Daughters? Examining the Effects of Gendered Migration on
   the Dynamics of ``Sons of the Soil{''} Conflict},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2020},
Volume = {43},
Number = {10},
Pages = {837-853},
Month = {OCT 2},
Abstract = {Why are female migrants rarely attacked in ``Sons of the Soil{''} (SoS)
   violence? Based on interviews with key stakeholders in Indonesia and
   China, we argue that women are shielded from the brunt of
   migration-related violence due to gendered patterns of migration and
   economic integration that highlights the positive contributions of
   female migration to the host region while drawing attention to the
   threat posed by male migration. By bringing together the literature on
   migration, gender, inequalities, and conflict, this article makes a
   foray into the previously unexamined dynamics affecting victimization
   patterns in armed conflict in general and SoS conflict in particular.},
