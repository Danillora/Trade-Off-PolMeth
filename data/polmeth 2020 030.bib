Affiliation = {Simon, A (Corresponding Author), Masaryk Univ, Pedag Competence Dev Ctr, Veveri 26-28, Brno 60200, Czech Republic.
   Bozoki, Andras, Cent European Univ, Dept Polit Sci, Vienna, Austria.
   Simon, Agnes, Masaryk Univ, Pedag Competence Dev Ctr, Veveri 26-28, Brno 60200, Czech Republic.},
DOI = {10.1080/21599165.2020.1813115},
EarlyAccessDate = {SEP 2020},
ISSN = {2159-9165},
EISSN = {2159-9173},
Keywords = {Dissident intellectuals; Hungary; regime change; roundtable
