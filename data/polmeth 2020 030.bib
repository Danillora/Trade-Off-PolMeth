@article{ WOS:000626079200011,
Author = {Dionne, Kim Yi and Turkmen, Fulya Felicity},
Title = {The Politics of Pandemic Othering: Putting COVID-19 in Global and
   Historical Context},
Journal = {INTERNATIONAL ORGANIZATION},
Year = {2020},
Volume = {74},
Number = {1},
Pages = {E213-E230},
Month = {DEC},
Abstract = {As COVID-19 began to spread around the world, so did reports of
   discrimination and violence against people from marginalized groups. We
   argue that in a global politics characterized by racialized inequality,
   pandemics such as COVID-19 exacerbate the marginalization of already
   oppressed groups. We review published research on previous pandemics to
   historicize pandemic othering and blame, and enumerate some of the
   consequences for politics, policy, and public health. Specifically, we
   draw on lessons from smallpox outbreaks, the third bubonic plague, the
   1918 influenza pandemic, and more recent pandemics, such as HIV/AIDS,
   SARS, and Ebola. We also compile reports to document the discrimination
   and violence targeting marginalized groups early in the COVID-19
   pandemic. This article lays bare the continuation of a long history of
   othering and blame during disease outbreaks and identifies needs for
   further inquiry to understand the persistence of these pandemic
   politics.},
