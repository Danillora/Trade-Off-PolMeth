@article{ WOS:000523320900010,
Author = {Flamand, Laura and Arriaga, Rafael and Santizo, Claudia},
Title = {EDUCATION REFORM AND EVALUATION POLICIES IN MEXICO: INSTRUMENTS FOR
   OVERCOMING THE LEARNING GAP AND PROMOTING EQUAL OPPORTUNITIES},
Journal = {FORO INTERNACIONAL},
Year = {2020},
Volume = {60},
Number = {2},
Pages = {717-753},
Month = {APR-JUN},
Abstract = {With the aim of developing evidence-based public policies, this paper
   seeks to compare the ways education policy has been evaluated before and
   after the 2013 education reform. It focuses on three crucial aspects of
   the evaluation: learning achievements, teacher evaluation and federal
   education programs. The comparative analysis tracing the key changes
   undergone by the evaluation process in four areas: decision making,
   stakeholders, specific objects of evaluation, and methods. We conclude
   that the information generated by the National System for Education
   Evaluation (SNEE) did not contribute to decisions that improved the
   quality of education, and we reveal the continued influence of the
   Ministry of Education (SEP) as well as the growing involvement of state
   governments in the SNEE.},
