@article{ WOS:000581667200001,
Author = {Gordon, Steven},
Title = {The importance of being African: public attitudes towards immigration
   selection in South Africa},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2021},
Volume = {59},
Number = {1},
Pages = {25-46},
Month = {JAN 2},
Abstract = {The African Union wants to make `being African' an important immigration
   admission criterion amongst its member states. This would be a
   large-scale change to border control in South Africa, altering the
   nation's symbolic boundaries. But what do ordinary South Africans think
   about this change? And what are the determinants of public support for
   drawing boundaries in this way? Using data from the South African Social
   Attitudes Survey, this paper argues that social identity is one of the
   primary drivers of how people think about this issue. This outcome
   showcases the way identity politics drives South African attitudes
   towards border control.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
