@article{ WOS:000587870700005,
Author = {Hwang, Gyu-Jin},
Title = {The political economy of welfare in Singapore: explaining continuity and
   change},
Journal = {POLICY STUDIES},
Year = {2020},
Volume = {41},
Number = {1},
Pages = {63-79},
Month = {JAN 2},
Abstract = {The Singapore model of the political economy of welfare is often
   understood as an anti-thesis to the high spending welfare state in
   Western Europe. In Singapore, the family is the key to promoting
   self-sufficiency. Individual responsibility is maximized through a
   series of highly regulated social provisions, designed to promote
   maximum social conformity and economic instrumentality. Singapore is
   also considerably different from its Northeast Asian counterparts due to
   its remarkable level of welfare regime stability. This article addresses
   the question of how this continuity has been possible for so long and
   whether it is likely to continue. It argues that the institutional
   coherence that is provided through a set of ideational frameworks is
   central in unpacking the stories behind Singapore's road to development.},
