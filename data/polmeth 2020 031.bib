@article{ WOS:000522966100001,
Author = {Kallenborn, Zachary and Bleek, Philipp C.},
Title = {Avatars of the Earth: Radical Environmentalism and Chemical, Biological,
   Radiological, and Nuclear (CBRN) Weapons},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2020},
Volume = {43},
Number = {5},
Pages = {351-381},
Month = {MAY 3},
Abstract = {Terrorists combining motivations and capabilities to conduct significant
   chemical, biological, radiological, or nuclear (CBRN) attacks are,
   logically and empirically, outliers. Certain characteristics of radical
   environmentalism heighten the risk of such outliers. The majority of
   even radical environmentalists embrace nonviolence. Those who turn
   violent mostly do so in limited ways due to a combination of motivations
   and capabilities. Fringe elements are motivated to commit
   large-scale-including CBRN-violence, but are mostly constrained by
   capability. Yet eco-radicalism also draws more capable adherents. If
   serious CBRN terrorism attacks occur-a risk about which analysts differ
   markedly-radical environmentalist fringe actors are plausible
   perpetrators.},
