@article{ WOS:000533108300001,
Author = {Efrat, Asif and Newman, Abraham L.},
Title = {Defending core values: Human rights and the extradition of fugitives},
Journal = {JOURNAL OF PEACE RESEARCH},
Year = {2020},
Volume = {57},
Number = {4},
Pages = {581-596},
Month = {JUL},
Abstract = {Are states willing to overlook human rights violations to reap the
   fruits of international cooperation? Existing research suggests that
   this is often the case: security, diplomatic, or commercial gains may
   trump human rights abuse by partners. We argue, however, that
   criminal-justice cooperation might be obstructed when it undermines core
   values of individual freedoms and human rights, since the breach of
   these values exposes the cooperating state to domestic political
   resistance and backlash. To test our argument, we examine extradition: a
   critical tool for enforcing criminal laws across borders, but one that
   potentially threatens the rights of surrendered persons, who could face
   physical abuse, unfair trial, or excessive punishment by the foreign
   legal system. We find support for our theoretical expectation through
   statistical analysis of the surrender of fugitives within the European
   Union as well as surrenders to the United States: greater respect for
   human rights correlates with the surrender of fewer persons. A case
   study of Britain confirms that human rights concerns may affect the
   willingness to extradite. Our findings have important implications for
   debates on the relationship between human rights and foreign policy as
   well as the fight against transnational crime.},
