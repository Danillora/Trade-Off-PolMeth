@article{ WOS:000585096700002,
Author = {Turska-Kawa, Agnieszka and Wojastik, Weldemar},
Title = {Incumbency Advantage in the Context of Electoral Manipulation.
   Experiences of Local Elections in Poland},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
