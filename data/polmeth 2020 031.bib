@article{ WOS:000592298900001,
Author = {Bowers, Ian and Kirchberger, Sarah},
Title = {Not so disruptive after all: The 4IR, navies and the search for sea
   control},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2021},
Volume = {44},
Number = {4, SI},
Pages = {613-636},
Month = {JUN 7},
Abstract = {Fourth Industrial Revolution technologies and their applicability at sea
   now dominate debates about the future of naval operations. This article
   examines the extent to which such technologies, including autonomous and
   unmanned weapon systems and artificial intelligence, will disrupt naval
   warfare. Using two case studies, the South China Sea and the Baltic Sea,
   this article finds that in the key operational output of attaining sea
   control these technologies will not disrupt naval warfare. While they
   may intensify the competition between the operational attributes of
   detection, stealth, range and lethality, they will ultimately sustain
   existing understandings of seapower and its strategic effects.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
