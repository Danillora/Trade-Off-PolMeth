@article{ WOS:000542674100001,
Author = {Ponce, Karina and Vasquez, Andres and Vivanco, Pablo and Munck, Ronaldo},
Title = {The October 2019 Indigenous and Citizens' Uprising in Ecuador},
Journal = {LATIN AMERICAN PERSPECTIVES},
