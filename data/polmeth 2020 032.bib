@article{ WOS:000544470900006,
Author = {Koutsioumpas, Konstantinos and MacNaughton, Gillian},
Title = {Mapping perceptions of human rights and cultivating Boston as a human
   rights city},
Journal = {JOURNAL OF HUMAN RIGHTS},
Year = {2020},
Volume = {19},
Number = {3},
Pages = {363-378},
Month = {MAY 26},
Abstract = {Human rights cities (HRCs) are local initiatives in which people learn
   to translate and implement international human rights norms in their
   local realities to advance social justice in their communities. Although
   locally created and guided, HRCs are also linked to an international
   network of cities working to advance social justice based on the
   Universal Declaration of Human Rights. In April 2011, the Boston City
   Council adopted a resolution that proclaims Boston an HRC. To date, no
   published research has examined the impact, if any, of this resolution.
   In this context, this study addressed three questions: (1) Are NGO
   staff, faculty, and City of Boston officials aware of the Boston HRC
   resolution? (2) If so, to what extent do they leverage the resolution in
   their social justice work? (3) If not, do they think the Boston HRC
   resolution could help them in their social justice work?},
