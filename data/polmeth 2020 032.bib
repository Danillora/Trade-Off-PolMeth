@article{ WOS:000561799100001,
Author = {Carmona, Margarida and Sindic, Denis and Guerra, Rita and Hofhuis, Joep},
Title = {Human and Global Identities: Different Prototypical Meanings of
   All-Inclusive Identities},
Journal = {POLITICAL PSYCHOLOGY},
