@article{ WOS:000585096700010,
Author = {Haladyj, Anna Malgorzata},
Title = {Product, Packaging and Waste Management Database (BDO): A New Public
   Service and its Application at the Local Level in Poland},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2020},
Volume = {18},
Number = {4},
Pages = {835-853},
Month = {OCT},
Abstract = {In Poland, a new register was established in January 2018, namely the
   Product, Packaging and Waste Management Database (in Polish - BDO). Its
   creation was partly necessitated by the implementation of Directive
   2012/19/EU on Waste Electrical and Electronic Equipment (WEEE), and
   Regulation (EC) 1013/2006. The BDO is not only the Register of
   Notifications and Decisions regarding International Waste Shipments and
   the Register of Facilities with Issued Pre-Consent, but it is also a
   waste management tool. The article qualifies the BDO register as a
   public register under the assumption that, in relation to its
   recognition as a public register, it can fulfil an additional public
   utility function, defined as a public service of general interest
   provided by public authorities. Moreover, the BDO register allows its
   database content to be searched according to subject terms. It also
   constitutes a database that fosters the development of B2B services at
   the local level as it may facilitate searching for local waste
   collection, transport, recovery (recycling), and disposal providers in
   accordance with the proximity principle.},
