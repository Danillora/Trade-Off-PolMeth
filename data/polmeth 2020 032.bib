@article{ WOS:000553657500001,
Author = {Davis, Brandon Rudolph},
Title = {Feeling Politics: Carceral Contact, Well-Being, and
   Participation(sic)(sic)(sic)Palabras clave},
Journal = {POLICY STUDIES JOURNAL},
