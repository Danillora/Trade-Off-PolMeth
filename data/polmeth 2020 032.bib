@article{ WOS:000553339900001,
Author = {Brandenberger, Laurence and Ingold, Karin and Fischer, Manuel and
   Schlapfer, Isabelle and Leifeld, Philip},
Title = {Boundary Spanning Through Engagement of Policy Actors in Multiple
   Issues(sic)(sic)(sic)Palabras clave},
Journal = {POLICY STUDIES JOURNAL},
