@article{ WOS:000593998700004,
Author = {Li, Tianfeng and Feiock, Richard C.},
Title = {Explaining State Budget Punctuations: Policy Transparency, Political
   Institutions, and Electoral Incentives},
Journal = {POLICY STUDIES JOURNAL},
