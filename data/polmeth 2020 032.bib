@article{ WOS:000670139100015,
Author = {Petridou, Evangelia and Zahariadis, Nikolaos and Ceccoli, Stephen},
Title = {Averting institutional disasters? Drawing lessons from China to inform
   the Cypriot response to the COVID-19 pandemic},
Journal = {EUROPEAN POLICY ANALYSIS},
