@article{ WOS:000533002100001,
Author = {Quinones-Perez, Argeo T. and Seda-Irizarry, Ian J.},
Title = {The Self-Inflicted Dimensions of Puerto Rico's Fiscal Crisis},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2020},
Volume = {47},
Number = {3, SI},
Pages = {87-102},
Month = {MAY},
Abstract = {The fiscal crisis in Puerto Rico, which constrains the ways in which the
   government can try to tackle the economic depression, is in important
   ways self-inflicted-the product of economic policies undertaken at the
   local level. When the crisis is approached in this way, the resolution
   of the island's colonial situation can be seen as a necessary but not
   sufficient condition for solving the problems of the depression's
   victims.
   La crisis fiscal en Puerto Rico, que limita las formas en que el
   gobierno puede potencialmente tratar de hacer frente a la depresion
   economica, es autoinfligida de manera importante: el producto de las
   politicas economicas implementadas a nivel local. Cuando se aborda la
   crisis de esta manera, la resolucion de la situacion colonial de la isla
   puede verse como una condicion necesaria pero no suficiente para
   resolver los problemas de las victimas de la depresion.},
