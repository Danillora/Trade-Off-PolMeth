@article{ WOS:000600163000001,
Author = {Friggeri, Felix Pablo},
Title = {Primitive Accumulation, Mafia Capitalism, and the Campesino Population
   in Paraguay},
Journal = {LATIN AMERICAN PERSPECTIVES},
