@article{ WOS:000513650900006,
Author = {Bevan, Shaun and Rasmussen, Anne},
Title = {When Does Government Listen to the Public? Voluntary Associations and
   Dynamic Agenda Representation in the United States},
Journal = {POLICY STUDIES JOURNAL},
Year = {2020},
Volume = {48},
Number = {1},
Pages = {111-132},
Month = {FEB},
Abstract = {The aim of the article is to examine how the population size of
   voluntary associations affects the process through which the public's
   issue priorities are translated into policy priorities. We conduct a
   time series analysis of political attention in executive and legislative
   agendas at the U.S. federal level in the period 1971-2001, covering all
   issues addressed by the U.S. government. We show that the number of
   voluntary associations in a policy area has a positive conditioning
   effect on the link between public priorities and attention for the
   president's State of the Union Address. However, our results do not find
   a positive effect for voluntary associations at later stages of the
   policy cycle, which experience a higher degree of institutional
   friction. The findings underline the importance of distinguishing
   between different stages of policymaking when considering the impact of
   voluntary associations on dynamic agenda responsiveness.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Bevan, S (Corresponding Author), Univ Edinburgh, Quantitat Polit Sci, Edinburgh, Midlothian, Scotland.
   Bevan, Shaun, Univ Edinburgh, Quantitat Polit Sci, Edinburgh, Midlothian, Scotland.
   Rasmussen, Anne, Univ Copenhagen, Dept Polit Sci, Copenhagen, Denmark.
   Rasmussen, Anne, Leiden Univ, Inst Publ Adm, Leiden, Netherlands.},
DOI = {10.1111/psj.12231},
ISSN = {0190-292X},
