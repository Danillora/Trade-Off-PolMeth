@article{ WOS:000570280900001,
Author = {White, Jessica},
Title = {Gender in Countering Violent Extremism Program Design, Implementation
   and Evaluation: Beyond Instrumentalism},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2023},
Volume = {46},
Number = {7},
Pages = {1192-1215},
Month = {JUL 3},
Abstract = {Using a countering violent extremism program as a case study, this
   article highlights the conceptual and practical challenges for
   meaningful inclusion of gender in security programing. Gender
   mainstreaming is commonly cited as a goal for stakeholders.
   Unfortunately, the security framework under which many counterterrorism
   programs are implemented and the general lack of understanding around
   the complexity of gender has often led to failures for meaningful
   inclusion in program design and evaluation. This article argues that a
   gender-sensitive approach is needed for effective countering violent
   extremism programing and counterterrorism policy, therefore is needed to
   create a more secure world for all individuals.},
