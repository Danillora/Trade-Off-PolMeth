@article{ WOS:000591073200007,
Author = {Isaksson, Ann-Sofie},
Title = {Chinese Aid and Local Ethnic Identification},
Journal = {INTERNATIONAL ORGANIZATION},
Year = {2020},
Volume = {74},
Number = {4},
Pages = {833-852},
Abstract = {Recent empirical evidence suggests that Chinese development finance may
   be particularly prone to elite capture and patronage spending. If aid
   ends up in the pockets of political elites and their ethno-regional
   networks, this may exacerbate ethnic grievances and contribute to ethnic
   mobilization. In this research note I examine whether Chinese
   development projects make local ethnic identities more salient in
   African partner countries. A new geo-referenced data set on the
   subnational allocation of Chinese development finance projects to Africa
   is geographically matched with survey data for 50,520 respondents from
