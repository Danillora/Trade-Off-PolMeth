@article{ WOS:000570760500001,
Author = {Evenett, Simon J.},
Title = {Power transition and the regulatory state in large emerging
   markets:Norm-breakingafter the global financial crisis},
Journal = {REGULATION \& GOVERNANCE},
Year = {2021},
Volume = {15},
Number = {3, SI},
Pages = {472-491},
Month = {JUL},
Abstract = {I examine the ways in which the global financial crisis that began in
   2007, and whose effects arguably condition policymaking to the present
   day, affected the ongoing global power transition as manifested by the
   unilateral decisions undertaken by official agencies comprising national
   regulatory states. The theory of that power transition articulated in
   the overview paper of this Special Issue is reinterpreted to take into
   account the circumstances of that crisis. Empirical evidence on the
   propensity to discriminate against foreign commercial interests by the
