@incollection{ WOS:000634922000003,
Author = {Pearce, Jenny},
Book-Author = {Pearce, J},
Title = {Violence within Politics: Critical Alternatives},
Booktitle = {POLITICS WITHOUT VIOLENCE?: TOWARDS A POST-WEBERIAN ENLIGHTENMENT},
Series = {Rethinking Political Violence},
Year = {2020},
Pages = {39-64},
Abstract = {Since the last decades of the twentieth century and as sensibility to
   violence and its varied manifestations has grown, we have new
   intellectual tools to explore the interface between violence and
   politics. Political theorists have begun to conceptualize politics
   without a human ontology of violence. In the wake of the Second World
   War and during the Cold War, Hannah Arendt sought to build the
   foundations for a clear distinction between politics and violence. In
   the post Cold War, Chantal Mouffe sought to build conflict into politics
   as a way of diminishing the use of violence and constructing the
   political as comprised of competing hegemonic projects between
   adversaries but not enemies. These contrasting approaches add to our
   repertoire of thinking tools for potentially imagining a violence free
   politics. However, the meanings of violence within these
   conceptualizations remain inadequate for such a task or for exploring
   the varied ways in which violence impacts on politics and politics on
   violence.},
