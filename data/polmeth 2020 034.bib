@article{ WOS:000553317200001,
Author = {Alenda-Demoutiez, Juliette},
Title = {White, democratic, technocratic: the political charge behind official
   statistics in South Africa},
Journal = {REVIEW OF INTERNATIONAL POLITICAL ECONOMY},
Year = {2022},
Volume = {29},
Number = {1},
Pages = {44-64},
Month = {JAN 2},
Abstract = {Central to governance, comparison and evaluation-and evolving at the
   intersection of economic ideas, interests and institutions-official
   statistics are far from neutral artifacts in both the global north and
   south. This article investigates the case of South Africa. Building on
   primary sources and interviews, I distinguish between three periods in
   the history of the country's official economic statistics. This
   history-and the nation's statistical self-presentation-have been shaped
   by the legacy of apartheid, the construction of the new democratic
   state, the influence of international standards setters, and heated
   political struggles in a nation divided along racial and class lines.
   Although the South African case is unique in many ways, it offers
   valuable insights into the evolution of official statistics outside of
   the global north more broadly.},
