@article{ WOS:000788375500002,
Author = {Jones, Marc Owen},
Title = {Disinformation superspreaders: the weaponisation of COVID-19 fake news
   in the Persian Gulf and beyond},
Journal = {GLOBAL DISCOURSE},
Year = {2020},
Volume = {10},
Number = {4, SI},
Pages = {431-437},
Month = {NOV},
Abstract = {Many of the studies of disinformation tend to reflect transatlantic
   security concerns, and focus on the activities of Russia and China.
   There is notably less analysis of disinformation in the Arabic-speaking
   world and wider MENA region. This article analyses a number of
   MENA-based COVID-19 disinformation campaigns from 2020, highlighting how
   COVID-19 disinformation has been instrumentalised by regional actors to
   attack rivals or bolster the legitimacy of their own regimes. It
   highlights in particular how certain `superspreaders' of disinformation
   tend to promote Saudi, Emirate and right wing US foreign policy in the
   Middle East.},
