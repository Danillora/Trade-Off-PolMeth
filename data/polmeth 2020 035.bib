   Jones, Phil Mike, Univ Derby, Dept Criminol \& Social Sci, Policing Res Methods, Derby, England.},
DOI = {10.1177/0032329220942395},
ISSN = {0032-3292},
EISSN = {1552-7514},
Keywords = {criminal careers research; Birth Cohort Study 1970; life course
   perspective; Thatcherism; economic restructuring},
Keywords-Plus = {LIFE-COURSE; PROPERTY CRIME; BUSINESS-CYCLE; UNITED-STATES;
   UNEMPLOYMENT; RATES; ENGLAND; TIME; CONSEQUENCES; INEQUALITY},
