@article{ WOS:000527794200010,
Author = {Yang, Yi},
Title = {China's Youth in NEET (Not in Education, Employment, or Training):
   Evidence from a National Survey},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2020},
Volume = {688},
Number = {1, SI},
Pages = {171-189},
Month = {MAR},
Abstract = {This study aims to understand the prevalence and characteristics of
   individuals aged 16 to 35 who are not in education, employment, or
   training (NEET) and the risk factors associated with being in NEET in
   China. The analysis uses the 2012 China Labor-Force Dynamics Survey to
   show that the NEET rate was 8 percent during the study period.
   Multilevel logistic regression indicates that women were more likely to
   be in NEET. Married women and female migrants had significantly higher
   risks of being in NEET, with migration having opposite effects for men
   and women. Education had protective effects against being in NEET,
   especially for women. Age had a nonlinear effect, with individuals aged
   22 to 25 at the highest risk of being in NEET. I discuss implications
   for public policy and directions for future studies on NEET in China.},
