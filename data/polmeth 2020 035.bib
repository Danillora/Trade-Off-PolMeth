Language = {English},
Affiliation = {Hien, J (Corresponding Author), Univ Stockholm, Stockholm, Sweden.
   Hien, J (Corresponding Author), Stockholm Univ, Inst Future Studies, Stockholm, Sweden.
   Hien, Josef, Univ Stockholm, Stockholm, Sweden.},
DOI = {10.1111/jcms.13017},
EarlyAccessDate = {MAR 2020},
ISSN = {0021-9886},
EISSN = {1468-5965},
Keywords = {European crisis; ordoliberalism; cultural political economy; Social
   Catholicism},
Keywords-Plus = {RELIGION; GASPERI; ALCIDE},
Research-Areas = {Business \& Economics; International Relations; Government \& Law},
Web-of-Science-Categories  = {Economics; International Relations; Political Science},
Author-Email = {josef.hien@statsvet.su.se},
Affiliations = {Stockholm University},
Funding-Acknowledgement = {European Research Council {[}340534]},
