@article{ WOS:000517939200001,
Author = {Defty, Andrew},
Title = {From committees of parliamentarians to parliamentary committees:
   comparing intelligence oversight reform in Australia, Canada, New
   Zealand and the UK},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2020},
Volume = {35},
Number = {3},
Pages = {367-384},
Month = {APR 15},
Abstract = {Some form of legislative oversight of intelligence has become the norm
   in most democratic states. The near universal acceptance of the need for
   democratic oversight does not, however, mark the end of a process of
   intelligence accountability. In many states following a period of
   establishment and then consolidation, intelligence oversight mechanisms
   have begun to evolve as oversight committees have sought extra powers
   and developed new roles. This article examines reforms in parliamentary
   intelligence oversight committees in Australia, Canada, New Zealand and
   the UK, focusing on the form, mandate, membership, powers and resources
   of the committees as well as their engagement with other parliamentary
   actors.},
