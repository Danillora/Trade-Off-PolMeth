@article{ WOS:000788177100006,
Author = {Hunklinger, Michael and Ferch, Niklas},
Title = {Trans{*} voting: demand and supply side of trans{*} politics in Germany},
Journal = {EUROPEAN JOURNAL OF POLITICS AND GENDER},
Year = {2020},
Volume = {3},
Number = {3},
Pages = {389-408},
Month = {SEP},
Abstract = {Trans{*} people and trans{*} issues have been part of the scientific
   literature for over a decade, though framed most of the time under
   umbrella terms such as `LGBT' or `LGBTIQ{*}' and often without further
   consideration regarding trans{*}-specific issues. In this article, we
   take an emancipatory approach and focus on trans{*} people as political
   subjects. For the first time, we thus present data on the political
   preferences, attitudes and voting behaviour of trans{*} people in
   Germany, and put them in relation to the parties' manifestos for the
   2017 German general election (the 2017 Bundestagswahl). We discuss our
   findings in the theoretical context of trans{*} citizenship and trans{*}
   visibility. This approach positions trans{*} people as citizens in the
   centre of analysis and adds to our understanding of the citizenship of
   minority groups in our modern societies.},
