@article{ WOS:000641168800001,
Author = {Hunt, Charles T. and Jacob, Cecilia and Gallagher, Adrian},
Title = {Progress, Problems, and Prospects: R2P 15 Years after the World Summit},
Journal = {GLOBAL RESPONSIBILITY TO PROTECT},
Year = {2020},
Volume = {12},
Number = {4},
Pages = {359-362},
Month = {OCT},
Abstract = {This short article introduces the GR2P Forum reflecting on the
   Responsibility to Protect (R2P) doctrine 15 years after it was
   institutionalised at the international level through the World Summit
   Outcome Document. It contextualises the relevance of critical
