@article{ WOS:000575344100012,
Author = {Haines, Fiona and Macdonald, Kate},
Title = {Nonjudicial business regulation and community access to remedy},
Journal = {REGULATION \& GOVERNANCE},
Year = {2020},
Volume = {14},
