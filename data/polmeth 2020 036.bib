@article{ WOS:000501298800005,
Author = {Eckersley, Robyn},
Title = {The Green State in Transition: Reply to Bailey, Barry and Craig},
Journal = {NEW POLITICAL ECONOMY},
Year = {2020},
Volume = {25},
Number = {1},
Pages = {46-56},
Month = {JAN 2},
Abstract = {The contributions comprising this special section are part of a more
   general wave of research that is revisiting and/or re-envisaging the
   environmental state. They do so from the perspective of critical
   political economy. This article provides an assessment of their
   respective contributions while also reflecting on how those seeking to
   understand the greening (or de-greening) of the state from this critical
   political economy perspective might extend their critical theory to
   `critical problem-solving' in ways that are attentive to the politics of
   transition. To this end, I play Bailey off against Barry and Craig to
   illustrate how critical problem-solving might be approached.},
