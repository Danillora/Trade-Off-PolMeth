@article{ WOS:000592035100001,
Author = {Sipulova, Katarina and Smekal, Hubert},
Title = {Between Human Rights and Transitional Justice: The Dilemma of
   Constitutional Courts in Post-Communist Central Europe},
Journal = {EUROPE-ASIA STUDIES},
Year = {2021},
Volume = {73},
Number = {1, SI},
Pages = {101-130},
Month = {JAN 2},
Abstract = {The essay sheds light on the role of constitutional courts in
   transitional justice, focusing on their role in mitigating the conflicts
   between transitional justice and international human rights commitments.
   Through a study of the Czech Constitutional Court's case law, we
   demonstrate that international human rights work both as a constraint
   and a tool. Constitutional courts use international human rights law
   selectively, when it suits their reasoning and views on transition.
   Nevertheless, the influence of international human rights law
   strengthens over time, especially when supported by the adverse rulings
   of international bodies, and gradually prevails over transitional
   justice justifications.},
