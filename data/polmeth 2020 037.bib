@incollection{ WOS:001112066900009,
Author = {Hoffmann, Andrea Ribeiro},
Editor = {Duina, F and Merand, F},
Title = {IS THE EUROPEAN UNION'S ROLE IN THE WORLD IN CRISIS TOO? A VIEW FROM
   LATIN AMERICA},
Booktitle = {EUROPE'S MALAISE: The Long View},
Series = {Research in Political Sociology},
Year = {2020},
Volume = {27},
Pages = {175-186},
Abstract = {This article explores two gaps in the literature on European Union (EU)
   crises: firstly, the external effects of the crises on EU actorness and
   its relations with other countries and regions and, secondly, the
   uniqueness of the EU crises when compared to other world regions. The
   article explores these questions and argues that the crises did affect
   external views on the EU and its role in the world due to the influence
   of third country perspectives on its actorness and its ``intermestic
   nature,{''} but that the EU is not the only regional organization in
   crisis. As the case of Latin American regionalism shows, other regions
   have suffered from common systemic factors at the global level as well
   as from the decreased EU support of regionalism abroad.},
