@article{ WOS:000571291000001,
Author = {Ajdacic, Lena and Heemskerk, Eelke M. and Garcia-Bernardo, Javier},
Title = {The Wealth Defence Industry: A Large-scale Study on Accountancy Firms as
   Profit Shifting Facilitators},
Journal = {NEW POLITICAL ECONOMY},
Year = {2021},
Volume = {26},
Number = {4},
Pages = {690-706},
Month = {JUL 4},
Abstract = {Corporations increasingly engage in innovative `tax planning strategies'
   by shifting profits between jurisdictions. In response, states try to
   curtail such profit shifting activities while at the same time
   attempting to retain and attract multinational corporations. We aim to
   open up this dichotomy between states and corporations and argue that a
   wealth defence industry of professional service firms plays a crucial
   role as facilitators. We investigate the subsidiary structure of 27,000
   MNCs and show that clients of the Big Four accountancy firms show
   systematically higher levels of aggressive tax planning strategies than
   clients of smaller accountancy firms. We specify this effect for three
   distinct strategies and also uncover marked differences across
   countries. As such we provide empirical evidence for the systematic
   involvement of auditors as facilitators in corporate wealth defence.},
