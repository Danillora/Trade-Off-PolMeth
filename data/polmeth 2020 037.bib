@article{ WOS:000606120500009,
Author = {Grimaldi, Anna Isabella},
Title = {Brazilian Portrayals of Indigenous Rights in Western Europe during the
   1970s and 1980s},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
