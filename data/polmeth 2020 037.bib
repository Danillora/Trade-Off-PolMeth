@article{ WOS:000531922800001,
Author = {Patton, Dana and Smith, Joseph L.},
Title = {Gender, Ideology, and Dominance in Supreme Court Oral Arguments},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2020},
Volume = {41},
Number = {4},
Pages = {393-415},
Month = {OCT 1},
Abstract = {Gender bias in political discourse has been demonstrated in many
   settings, including the US Supreme Court. We investigate the effects of
   ideology and gender in verbal interactions during Supreme Court oral
   arguments. We theorize that all justices possess unconscious gender
   schemas that lead them to speak more during presentations by women but
   that liberal and female justices likely have conscious egalitarian
   values that diminish the manifestation of gender schemas. We find that
   conservative justices speak more when female lawyers are arguing but
   that liberal justices show no such effect, suggesting that consciously
   held attitudes can mitigate the expression of unconscious gender
   schemas.},
