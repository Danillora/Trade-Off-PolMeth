@article{ WOS:000591879900015,
Author = {Lapidus, John},
Title = {Indirect and Invisible Regulations Set in Stone: A Driving Force behind
   the Rise of Private Health Insurance in Sweden},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2020},
Volume = {691},
Number = {1, SI},
Pages = {243-257},
Month = {SEP},
Abstract = {The Swedish welfare model is gradually losing its former
   characteristics. Notable is the extensive privatization of provision and
   the emerging privatization of funding, primarily through new and
   half-private services in health care, education, and elderly care. The
   clearest example of this trend is the rise of private health insurance,
   which is now signed by every tenth person of working age. This article
   points out different types of regulations that have provoked the rise of
   private health insurance, and discusses types of regulations that could
   potentially slow privatization. Further, this article analyzes three
   official welfare investigation reports. These reports avoid the decisive
   regulations they are supposed to discuss, and sometimes go against
   directives to do so. I argue that regulations for private health
   insurance have occurred without much debate, while every potential
   regulation against private health insurance is very much disputed by
   industry interests and many of the political parties.},
