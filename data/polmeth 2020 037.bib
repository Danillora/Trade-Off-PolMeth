@article{ WOS:000532005600001,
Author = {Fortescue, Stephen},
Title = {Russia's civil service: professional or patrimonial? Executive-level
   officials in five federal ministries},
Journal = {POST-SOVIET AFFAIRS},
Year = {2020},
Volume = {36},
Number = {4},
Pages = {365-388},
Month = {JUL 3},
Abstract = {The issue of poor performance of the Russian federal bureaucracy is
   addressed by linking performance to type of official, through analysis
   of biographical data on deputy ministers and division (departament)
   heads in five federal ministries since 2012, supplemented by internet
   searches on behavior, particularly of a corrupt nature. Education,
   previous career experience, and recruitment, including its timing
   relative to superiors and subordinates, are analyzed, in order to
   determine whether officials behave primarily as members of patrimonial
   teams, as members of problem-solving organizations, as self-serving
   individuals, as the instruments of capture by commercial organizations,
   or as servants of bureaucratic agency interests. The data do not
   strongly support any one conclusion, but problem-solving officials have
   the strongest presence.},
