@article{ WOS:000543247500001,
Author = {Asquer, Alberto and Krachkovskaya, Inna},
Title = {Uncertainty, institutions and regulatory responses to emerging
   technologies:CRISPRGene editing in theUSand theEU(2012-2019)},
Journal = {REGULATION \& GOVERNANCE},
Year = {2021},
Volume = {15},
Number = {4},
Pages = {1111-1127},
Month = {OCT},
Abstract = {This study aims to improve theoretical accounts of regulatory responses
   to emerging technologies by proposing a model of regulatory development,
   which incorporates a role for types of uncertainty and for existing
   regulatory institutions. Differently from existing theories of
   regulatory development, the model proposed here posits a sequence of
   cyclical activities where regulatory responses arise in incremental
   fashion out of efforts to make sense of emerging technologies and to
   ponder the applicability of existing regulatory tools. The model is
   discussed on the basis of the comparison between regulatory responses to
   the emergence of CRISPR gene editing in the US and the EU in the period
   2012-2019. The comparison between the two cases suggests how regulatory
   responses to emerging technologies are affected by expectations of
   future technological and regulatory developments and by existing
   regulatory institutions.},
