@article{ WOS:000557878100001,
Author = {Cornacchione, Teresa and Tuning, Rachel},
Title = {Women Behaving Differently: Anti-Establishment Party Membership and
   Female Parliamentary Activity},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2020},
Volume = {41},
Number = {4},
Pages = {457-476},
Month = {OCT 1},
Abstract = {When will women politicians vary in their legislative behavior? We argue
   that anti-establishment parties, which are increasingly electorally
   viable, will incentivize their female members to be more active than and
   pursue agendas counter to women in traditional, mainstream parties.
   Using originally coded data of parliamentary questions from the eighth
   session of the European Parliament, we assess how anti-establishment
   women differ from mainstream party women. We find that
   anti-establishment women are significantly more active than mainstream
   party women and do not prioritize classic feminine issues. We contribute
   to the literature on women's legislative behavior by considering how
   establishment party membership provides an opportunity for women to
   close, and potentially reverse, the gender gap in legislative behavior.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
