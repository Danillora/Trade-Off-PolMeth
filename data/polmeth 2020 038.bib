Author = {Levingston, Oliver},
Title = {Minsky's moment? The rise of depoliticised Keynesianism and ideational
   change at the Federal Reserve after the financial crisis of 2007/08},
Journal = {REVIEW OF INTERNATIONAL POLITICAL ECONOMY},
Year = {2021},
