@article{ WOS:000515368400001,
Author = {Smeets, Max},
Title = {U.S. cyber strategy of persistent engagement \& defend forward:
   implications for the alliance and intelligence collection},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2020},
Volume = {35},
Number = {3},
Pages = {444-453},
Month = {APR 15},
Abstract = {This article evaluates the implications of U.S. cyber strategy of
   persistent engagement for the alliance and intelligence collection.
   Whilst the strategy may have benefits for certain alliance
   relationships, I identify four potential negative consequences; loss of
   allied trust, disruption allied intelligence operations and
   capabilities, exploitability of the strategy by adversaries, and the
   implementation (and justification) of persistent engagement by other
   countries. This paper concludes suggesting several ways forward,
   including the creation of a new NATO-memorandum of understanding on
   cyber operations.},
