@article{ WOS:000573357300005,
Author = {Singh, Shweta},
Title = {In between the ulemas and local warlords in Afghanistan: critical
   perspectives on the ``everyday,{''} norm translation, and UNSCR 1325},
Journal = {INTERNATIONAL FEMINIST JOURNAL OF POLITICS},
Year = {2020},
Volume = {22},
Number = {4, SI},
Pages = {504-525},
Month = {AUG 7},
Abstract = {This article makes the case for centering the gaze on the
   ``everyday,{''} motivated by a desire to understand how the Women, Peace
   and Security (WPS) agenda, even in its twentieth year, is often about
   translation(s) rather than transformation(s). In this article, I explore
   the following research question: how does the everyday inform norm
   translation and constrain the transformative potential of United Nations
   Security Council Resolution (UNSCR) 1325? Most research on WPS has paid
   scant attention to the everyday as a site of analysis. The challenge is
   that the multiple sites, actors, and practices that constitute the
   everyday for women are often regarded as ``little nothings.{''} Through
   a case study of Afghanistan, I highlight the significance of these
   ``little nothings,{''} which I argue are intrinsically political and
   which interact with, impinge upon, align with, or resist global
   normative frameworks such as the WPS agenda. I thus bring the ``everyday
   turn{''} in international relations (IR) theory into conversation with
   the literature on norm translation. Methodologically, I draw on field
   insights from my time spent in Kabul in November 2016 (as a member of
   Track II Dialogue facilitated by South Asians for Human Rights and the
   Afghan Women's Network (AWN)), and use participant observation and
   interviews to reconstruct the quotidian experiences of women
   specifically from the perspective of participation in the public sphere.},
