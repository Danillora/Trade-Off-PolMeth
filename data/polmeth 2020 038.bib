@article{ WOS:000524941400012,
Author = {Cowell-Meyers, Kimberly B. and Evans, Elizabeth and Shin, Ki-young},
Title = {Women's Parties: A New Party Family},
Journal = {POLITICS \& GENDER},
Year = {2020},
Volume = {16},
Number = {1, SI},
Pages = {4-25},
Month = {MAR},
Abstract = {Women's parties have a unique and important role to play in the
   representation of women and women's issues and interests. They are
   neither a new nor a rare phenomenon and have emerged in a variety of
   contexts across time and space. And yet we know relatively little about
   them. This article argues that women's parties matter and that the study
   of women's parties matters. We contend that women's parties constitute a
   discrete party family; while there is a diverse range of women's
   parties, they can be viewed as a coherent group with similar origins,
   ideology, and naming patterns. This article offers the first research
   framework for the comparative study of women's parties. Building our
   knowledge of women's parties, we argue, is important for those
   interested in gender and politics, particularly those concerned with the
   representation of women's issues and interests.},
