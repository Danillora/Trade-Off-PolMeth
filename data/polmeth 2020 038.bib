@article{ WOS:000513258900010,
Author = {Pollack, Harold A. and Humphreys, Keith},
Title = {Reducing Violent Incidents between Police Officers and People with
   Psychiatric or Substance Use Disorders},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2020},
Volume = {687},
Number = {1, SI},
Pages = {166-184},
Month = {JAN},
Abstract = {This article describes evidence-based strategies designed to reduce the
   prevalence of police encounters with people in behavioral crisis (PBCs)
   and to make such encounters less dangerous for all parties when they do
   occur. Some of these strategies are implemented by law enforcement,
   including gun violence restraining orders and the training of officers
   to provide time, distance, and cover during encounters with PBCs. Other
