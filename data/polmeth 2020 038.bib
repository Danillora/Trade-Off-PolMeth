@article{ WOS:000523970900001,
Author = {Pacher, Andreas},
Title = {Transnistria's Order of Friendship: Legitimacy Through Diplomatic
   Practice},
Journal = {EUROPE-ASIA STUDIES},
Year = {2020},
Volume = {72},
Number = {7},
Pages = {1209-1231},
Month = {AUG 8},
Abstract = {What strategies do de facto states employ in conducting their diplomacy?
   This article examines Transnistria's Order of Friendship, a state award
   that primarily targets foreigners, by analysing the profiles of all
   known award recipients. The Order is found to be a political tool to
   enhance Transnistria's domestic and external legitimacy in the absence
   of recognised sovereignty. The findings confirm the literature on
   Transnistria's `bandwagoning' with Russia, indicate that a patron's
   support must be actively sought rather than taken for granted, and-since
   many awardees are from jurisdictions other than the patron state
   Russia-point to the need to examine the foreign policy of de facto
   states more holistically.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
