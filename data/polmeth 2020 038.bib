@article{ WOS:000555184600001,
Author = {Litvinenko, Anna and Zavadski, Andrei},
Title = {Memories on Demand: Narratives about 1917 in Russia's Online Publics},
Journal = {EUROPE-ASIA STUDIES},
Year = {2020},
Volume = {72},
Number = {10},
Pages = {1657-1677},
Month = {NOV 25},
Abstract = {This article analyses the digital remembrance of the Russian Revolution
   in the year of its centenary. It examines what memory narratives about
   1917 were constructed by leading Russian online media in 2017, in the
   absence of an overarching narrative about the event imposed by the
   state. The authors reveal a multiplicity of digital memories about the
   revolution and discuss their implications for the regime's stability. It
   is argued that the flexible nature of digital remembrance does not
   necessarily challenge authoritarian rule and can even work in its favour
   by allowing one to target-and satisfy-various sections of a fragmented
   society.},
