@article{ WOS:000603966800016,
Author = {Cassino, Dan and Besen-Cassino, Yasemin},
Title = {Of Masks and Men? Gender, Sex, and Protective Measures during COVID-19},
Journal = {POLITICS \& GENDER},
Year = {2020},
Volume = {16},
Number = {4},
Pages = {1052-1062},
Month = {DEC},
Abstract = {Since the beginning of the COVID-19 pandemic in the United States, men
   have been consistently less likely to report wearing a protective face
   mask. There are several possible reasons for this difference, including
   partisanship and gender identity. Using a national live-caller telephone
   survey that measures gender identity, we show that men's gender
   identities are strongly related to their views of mask wearing,
   especially when gender identity is highly salient to the individual. The
   effects of this interaction of sex and gender are shown to be separate
   from the effects of partisanship. While partisanship is a significant
   driver of attitudes about face masks, within partisan groups, men who
   report ``completely{''} masculine gender identities are very different
   from their fellow partisans.},
