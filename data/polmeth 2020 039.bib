@article{ WOS:000521536900001,
Author = {Johnson, Loch K.},
Title = {Reflections on the ethics and effectiveness of America's `third option':
   covert action and U.S. foreign policy},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
