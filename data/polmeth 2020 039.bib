@article{ WOS:000555657800001,
Author = {Wallace, Rebecca and Goodyear-Grant, Elizabeth},
Title = {Writing Gender Out or Working It Back In? Media Coverage of Child
   Benefits in Canada},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2020},
Volume = {41},
Number = {4},
Pages = {441-456},
Month = {OCT 1},
Abstract = {This article analyzes the (de)gendering of child benefits in Canadian
   print media, using an automated analysis of coverage of two governments'
   child benefits policies. The analysis finds that gender and women's
   equality are deprioritized in the framing of the policies' beneficiaries
   and goals, even when the party in power shifts from the right wing of
   the spectrum to center-left. There is some variation in coverage across
   newspapers and across journalists. Notably, women journalists' coverage
   of child benefits policies is more attentive to gender and women's
   equality, suggesting that they may serve as agents of change in bringing
   gender back into the national conversation.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
