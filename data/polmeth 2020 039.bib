@article{ WOS:000567761300001,
Author = {Faude, Benjamin and Parizek, Michal},
Title = {Contested multilateralism as credible signaling: how strategic
   inconsistency can induce cooperation among states},
Journal = {REVIEW OF INTERNATIONAL ORGANIZATIONS},
Year = {2021},
Volume = {16},
Number = {4},
Pages = {843-870},
Month = {OCT},
Abstract = {This paper analyzes how patterns of international cooperation are
   affected if a group of states, led by a major power, pursues a strategy
   of ``contested multilateralism{''} (CM). We conceptualize CM as a
   reaction to deadlock in institutional adjustment bargaining where CM
   lowers the gains actors can reap from cooperation in the short run. We
   demonstrate that, in the long run, CM nevertheless can have positive
   effects on international cooperation and specify when this is the case.
   Because of the costs associated with it, CM conveys a credible signal of
   the resolve of a dissatisfied group of states to contest the
   institutional status quo. Due to this capacity, CM alters the
   institutional and strategic environment within which institutional
   adjustment bargaining takes place. As a result, CM opens up the
   possibility for inter-institutional accommodation that increases
   realized cooperation gains. We probe the plausibility of our theoretical
   reasoning with empirical case studies on competitive regime creation in
   multilateral development finance and on regime-shifting in the
   governance of international trade in genetically modified organisms.},
