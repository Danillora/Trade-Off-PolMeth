@article{ WOS:000513444000001,
Author = {Yrigoy, Ismael},
Title = {The Political Economy of Rental Housing in Spain: The Dialectics of
   Exploitation(s) and Regulations},
Journal = {NEW POLITICAL ECONOMY},
Year = {2021},
Volume = {26},
Number = {1},
Pages = {186-202},
Month = {JAN 2},
Abstract = {This paper analyses how the exploitation of tenants in Spain is boosting
   income for banks, hedge funds and pension funds. It does so by tracing
   the origins of the money invested in a Tres Cantos housing project in
   Madrid. The paper makes the following claims: First, the exploitation
   taking place in households -referred in this paper as secondary- is
   increasingly related to worker exploitation, and thus this particular
   type of exploitation is increasingly relevant to the dynamics of capital
   accumulation. Second, the key role of secondary exploitation of tenants
   in the revenue-making strategies of pension funds, hedge funds and banks
   is augmented and mediated by a myriad of regulations being implemented
   at the national and supranational scales. Theoretically, the paper
   contests the Marxian claim that household exploitation is `secondary' to
   the exploitation taking place in the production process.},
