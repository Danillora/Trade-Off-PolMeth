@article{ WOS:000641155600003,
Author = {Ribeiro, Mikelli Marzzini L. A. and Medeiros, Marcelo de Almeida and
   Cunha Leite, Alexandre Cesar},
Title = {China's Engagement with R2P: Pluralist Shaper?},
Journal = {GLOBAL RESPONSIBILITY TO PROTECT},
Year = {2020},
Volume = {12},
Number = {3},
Pages = {271-298},
Month = {JUL},
Abstract = {Recently, the behaviour of emerging powers in relation to R2P has been
   understood as that of norm-shapers: states that seek to adjust the norm
   so that it conforms to their particular understandings. In this
   behaviour, respect for sovereignty is seen as a central concern. In the
   English School of International Relations, identification with the
   institution of sovereignty reflects an approach called pluralism.
   China's behaviour is consistent with a pluralist approach to
   international relations. This paper aims to examine the Chinese
   positions regarding R2P, in order to identify pluralist traits in them.
   The procedure was to raise, inductively, prescriptions made by China,
   associating them with theoretical categories. From an analysis of the
   Chinese positions, it can be demonstrated that China's behaviour reveals
   a paradigmatic case of a pluralist normshaper. Qualitative Content
   Analysis techniques were used as a methodological approach, while
   MAXQDAplus software was applied as a tool to aid in the coding of
   declarations.},
