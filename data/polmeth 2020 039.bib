@article{ WOS:000591109100001,
Author = {Betz, Timm and Pond, Amy and Yin, Weiwen},
Title = {Investment agreements and the fragmentation of firms across countries},
Journal = {REVIEW OF INTERNATIONAL ORGANIZATIONS},
