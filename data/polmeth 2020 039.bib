@article{ WOS:000544412800001,
Author = {Mainwaring, Sarah},
Title = {Division D: operation Rubicon and the CIA's secret SIGINT empire},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2020},
Volume = {35},
Number = {5},
Pages = {623-640},
Month = {JUL 28},
Abstract = {The CIA is a popular topic of study for intelligence historians.
   Renowned for covert action and human intelligence operations, we know a
   lot about how they became woven into the tapestry of world history. More
   recently, scientific and technical intelligence emerged as a central
   theme, uncovering the work of the `Wizards of Langley'. This article
   suggests we still have much to leam. Exploring CIA's historic interest
   in communications intelligence, it uncovers ways they controlled a
   significant proportion of the world's communications systems. Working
   internationally, and disagreeing with NSA, the CIA influenced important
   systems well into the twenty-first century.},
