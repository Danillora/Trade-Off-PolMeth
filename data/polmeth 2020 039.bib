@article{ WOS:000596248100001,
Author = {Taggart, Jack R.},
Title = {Global development governance in the `interregnum'},
Journal = {REVIEW OF INTERNATIONAL POLITICAL ECONOMY},
Year = {2022},
Volume = {29},
Number = {3},
Pages = {904-927},
Month = {MAY 4},
Abstract = {Global governance is widely perceived to be `gridlocked', `unravelling',
   and `unfit for purpose'. The legitimacy of old institutions is breaking
   down, yet new institutions struggle to establish themselves as viable
   alternatives. Though overlooked, global development governance is no
   exception. Heterogeneous development actors, approaches, and
   understandings increasingly characterize the field. However, attempts to
   create a common platform have foundered. This article contributes to a
   growing body of literature that depicts world order as a Gramscian
   `interregnum' to account for the crisis facing global development. Its
   primary contribution lies in the examination of the Global Partnership
   for Effective Development Cooperation as a vantage point to explore the
   politics, dynamics and struggles over the means and ends of
   `development' in the current conjuncture. Here, competing projects vie
   for influence, yet none command the support necessary to supplant the
   `old' order. Although this article finds that the condition of
   `interregnum' will likely persist, it also explores what transformative
   elements of a possible future exist within present practices.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Taggart, JR (Corresponding Author), Univ Cambridge, Newnham Coll, Cambridge, England.
   Taggart, Jack R., Univ Cambridge, Newnham Coll, Cambridge, England.},
DOI = {10.1080/09692290.2020.1852948},
EarlyAccessDate = {NOV 2020},
ISSN = {0969-2290},
EISSN = {1466-4526},
