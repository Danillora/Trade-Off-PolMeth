@article{ WOS:000641168800002,
Author = {Evans, Gareth},
Title = {The Dream and the Reality},
Journal = {GLOBAL RESPONSIBILITY TO PROTECT},
Year = {2020},
Volume = {12},
Number = {4},
Pages = {363-365},
Month = {OCT},
Abstract = {In this contribution the author takes stock of the extent to which R2P
   has both met and fallen short of the dreams of its founders since its
   endorsement by the 2005 World Summit. As an effective reactive
   counter-force to atrocity crimes already under way, R2P's record has
