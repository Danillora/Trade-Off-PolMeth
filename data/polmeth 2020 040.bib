@article{ WOS:000579379400001,
Author = {Tas, Hakki},
Title = {The formulation and implementation of populist foreign policy: Turkey in
   the Eastern Mediterranean},
Journal = {MEDITERRANEAN POLITICS},
Year = {2022},
Volume = {27},
Number = {5},
Pages = {563-587},
Month = {OCT 20},
Abstract = {Although populism does not dictate a coherent ideological or
   programmatic agenda, some of its elements still leave distinct marks on
   the formulation and implementation of foreign policy. This paper argues
   for the study of populism in its tangible policy impacts and scrutinizes
   the nexus of populism and foreign policy in contemporary Turkey under
   President Erdogan's rule. Despite the abundant references to the
   `people' in the populist rhetoric, it identifies personalization in
   foreign policy decision-making, nationalization in foreign policy
   implementation, and civilizationalization in the foreign policy
   discourse. Having established the patterns of populist foreign policy
   from a wider reading, this study then examines, generally, how populism
   has informed Turkish foreign policy and, specifically, Turkey's approach
   to the recent border disputes over the gas fields in the Eastern
   Mediterranean.},
