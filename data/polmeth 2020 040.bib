@article{ WOS:000563085700004,
Author = {Jayet, Cyril},
Title = {The Meaning of the European Union and Public Support for European
   Integration},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2020},
Volume = {58},
Number = {5},
Pages = {1144-1164},
Month = {SEP},
Abstract = {This paper proposes a cognitive approach that focuses on the cognitive
   representations that underpin public support for the European Union. Its
   objectives are threefold: (1) to describe systems of representation
   associated with support for the EU; (2) to illustrate the importance of
   less commonly studied representations, such as liberal cosmopolitan
   representations; and (3) to demonstrate that at the beginning of the
   2000s, the EU often represented greater economic prosperity and/or
   social protection for many Central, Eastern and Southern European
   countries, although the situation has changed significantly since then.
   This paper uses Eurobarometer data from 2004 to 2017 as well as
   multilevel models to examine how cognitive representations of the EU can
   explain public support for the EU, including variations across
   countries.},
