@article{ WOS:000531934600001,
Author = {Benstead, Lindsay and Kao, Kristen and Lust, Ellen},
Title = {Does it matter what observers say? The impact of international election
   monitoring on legitimacy},
Journal = {MEDITERRANEAN POLITICS},
Year = {2022},
Volume = {27},
Number = {1},
Pages = {57-78},
Month = {JAN 1},
Abstract = {Scholars and democracy promoters often suggest that electoral observers'
   (EOs') assessments impact public opinion in a straightforward manner,
   yet, research on communication cautions against these sanguine
   assumptions. We test the impact of EO statements on public opinion in
   two very different contexts using survey experiments conducted among
   3,361 Jordanians and Tunisians. Our results demonstrate the need for
   democracy promoters to consider negative consequences when implementing
   democracy promotion programmes, and for scholars to undertake further
   research regarding the impacts of election monitoring on domestic
   attitudes.},
