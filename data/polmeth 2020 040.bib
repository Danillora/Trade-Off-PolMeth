@article{ WOS:000574683600001,
Author = {Saleh, Sadi Sadiyev and Khudam, Elman Nasirov and Ibrahim, Khayal
   Iskandarov and Simons, Greg},
Title = {South caucasus and a `New Great Game': the communication of competition
   in securitised international relations},
