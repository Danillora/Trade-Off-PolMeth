@article{ WOS:000545519000001,
Author = {Haas, Bianca and Haward, Marcus and McGee, Jeffrey and Fleming, Aysha},
Title = {Explicit targets and cooperation: regional fisheries management
   organizations and the sustainable development goals},
Journal = {INTERNATIONAL ENVIRONMENTAL AGREEMENTS-POLITICS LAW AND ECONOMICS},
