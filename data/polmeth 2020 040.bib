@article{ WOS:000641152800002,
Author = {Pytlak, Allison},
Title = {Are Arms Trade Treaty Meetings Being Used to Their
   Full Potential?},
Journal = {GLOBAL RESPONSIBILITY TO PROTECT},
Year = {2020},
Volume = {12},
Number = {2},
Pages = {156-177},
Month = {MAY},
Abstract = {When adopted in 2013 the international Arms Trade Treaty (ATT) was
   widely heralded for its life-saving potential and for bringing human
   rights and humanitarian concerns squarely into international arms
   transfers decision-making processes. This article takes a critical look
   at the ATT meeting cycle, which comprises an annual conference of states
   parties (CSP), as well as preparatory sessions and meetings of the
   Treaty's working groups. The article is guided by the question, are ATT
   meetings being used to their full potential to meet the Treaty's
   objectives and prevent atrocities? It studies two aspects of the ATT
   meeting cycles-working groups and annual CSP thematic areas of focus -
