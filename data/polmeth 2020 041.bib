@article{ WOS:000550957100001,
Author = {Vairel, Frederic},
Title = {How self-limiting mobilization works in Morocco},
Journal = {MEDITERRANEAN POLITICS},
Year = {2022},
Volume = {27},
Number = {3},
Pages = {344-368},
Month = {MAY 27},
Abstract = {This article draws on three years of immersive field experience to
   analyse the long-term effects of repression on contentious practices in
   Morocco. Informed by Charles Tilly's insights about contentious
   practices, I show how activists from various organizations - the
   Moroccan Truth and Justice Forum; Islamists from Justice and
   Beneficence; and the February 20 Movement (the Moroccan manifestation of
   the Arab popular uprisings) - carefully chose the type of action they
   took, the number of people they mobilized, and the precise time and
   location of their actions. This self-limitation was based on their
   assessments of what authorities would tolerate. It allowed activists to
   continue their work without the dire consequences that occur during
   times of harsh repression.},
