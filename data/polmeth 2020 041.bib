@article{ WOS:000532355500001,
Author = {Koppa, Maria Eleni},
Title = {Turkey, Gulf States and Iran in Western Balkans: more than the Islamic
   factor?},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2021},
Volume = {29},
Number = {2},
Pages = {251-263},
Month = {APR 3},
Abstract = {The article examines the influence of Turkey, the Gulf States and Iran
   in the Western Balkans and the role that Islam has been playing in the
   region since the 90 s. Starting from the idea that religion is a tool of
   soft power but also a vehicle for addressing other concerns, the main
   argument is that the aforementioned actors, use Islam as an instrument
   of foreign policy mixed with realpolitik. Special emphasis is given to
   Turkey, due to its traditional links with the Balkans through their
   common Ottoman past. The article also tries to show how the current
   projection of religion has severe repercussions in the region as it is
   directly linked with the issue of foreign fighters that present a new
   challenge for the Western Balkans.},
