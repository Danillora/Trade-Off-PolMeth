@article{ WOS:000557895100001,
Author = {Chen, Muyang},
Title = {Beyond Donation: China's Policy Banks and the Reshaping of Development
   Finance},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
Year = {2020},
Volume = {55},
Number = {4},
Pages = {436-459},
Month = {DEC},
Abstract = {This paper seeks to demystify and characterize China's official
   development finance by examining lending mechanisms of China's two
   policy banks-China Development Bank and Export-Import Bank of China.
   Using quantitative and qualitative data, the paper shows how and why
   policy banks implement a peculiar means of development finance, i.e.,
   funding projects in developing countries with relatively high-interest
   rate loans, differing from low-interest rate development-finance credits
   from industrial countries. The paper argues that China's official
   development finance is not only about practicing economic statecraft or
   facilitating export-led growth; it is also the internationalization of a
   development-finance model that has facilitated its own development in
   the past decades. In this model, the state does not play a direct role
   in allocating fiscal revenue; rather, it plays an indirect role in
   enhancing creditworthiness of projects and making them financially
   viable. The ``state-supported, market-based{''} Chinese credits reshape
   development finance and offer an alternative option for the developing
   world.},
