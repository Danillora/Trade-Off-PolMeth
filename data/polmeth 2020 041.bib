@incollection{ WOS:000488267800005,
Author = {Wu, Bin},
Book-Author = {Wu, B},
Title = {Evolution of Hangzhou Government Performance Management System},
Booktitle = {GOVERNMENT PERFORMANCE MANAGEMENT IN CHINA: THEORY AND PRACTICE},
Series = {China Academic Library},
Year = {2020},
Pages = {53-102},
Abstract = {The Hangzhou government performance management system started with an
   objective-based responsibility assessment system in the early 1990s. In
   2000, the external evaluation was introduced to select ``satisfactory
   Entity and Dissatisfactory Entity{''} (hereinafter referred to as
   ``satisfaction-based selection{''} or ``Vote for Excellence{''}). In
   2005, the comprehensive appraisal and evaluation were implemented. After
   2011, an all-round performance management system was phased in. The
   evolution went through four stages and three leap-forwards, developing
   from closed internal appraisal to open external appraisal to
   comprehensive performance evaluation to all-round promotion of
   performance management development. Its nature is the gradual
   transformation of closed government management to diversified government
   governance, and it is an epitome of the transition of traditional local
   Chinese government governance into a modern one.},
