@article{ WOS:000505276000002,
Author = {Kissane, Bill},
Title = {On the shock of civil war: cultural trauma and national identity in
   Finland and Ireland},
Journal = {NATIONS AND NATIONALISM},
Year = {2020},
Volume = {26},
Number = {1, SI},
Pages = {22-43},
Month = {JAN},
Abstract = {What role does national identity play after civil war? Is reconstruction
   possible on the basis of an existing identity, or does a new identity
   have to be found? Much depends on whether narratives of conflict are
   unifying. I use the tools of cultural sociology to explain why the
   Finnish Civil War of 1918 has become a unifying `cultural trauma' for
   the Finns, whereas the Irish Civil War of 1922-23 never became the
   dominant referent in Irish national identity. The difference is
   explained by the greater shock civil war posed to Finnish national
   identity.},
