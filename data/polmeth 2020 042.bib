@incollection{ WOS:000488267800006,
Author = {Wu, Bin},
Book-Author = {Wu, B},
Title = {Value System of Government Performance Management},
Booktitle = {GOVERNMENT PERFORMANCE MANAGEMENT IN CHINA: THEORY AND PRACTICE},
Series = {China Academic Library},
Year = {2020},
Pages = {103-128},
Abstract = {Since the 1970s, the new public management movement has begun to orient
   government performance toward both efficiency and democracy which to
   some extent contributes to the integration of instrumental and value
   rationality, and of politics and administration, reversing the
   long-standing dichotomy of the two pairs. It means that government
   performance management is not just a managerial tool, but a vehicle that
   carries the core value of modern governance. In the pursuit of
   government performance management, Hangzhou aims to achieve the best
   performance and satisfy people's needs. In doing so, Hangzhou has formed
   a core set of values for government performance management-openness,
   democracy, accountability, and performance-under which the government
   allows the people to judge its performance and tailors its performance
   to suit the needs of people.},
