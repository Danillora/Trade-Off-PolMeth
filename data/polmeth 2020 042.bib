@article{ WOS:000710530000002,
Author = {Shoesmith, Dennis and Franklin, Nathan and Hidayat, Rachmat},
Title = {Decentralised Governance in Indonesia's Disadvantaged Regions: A
   Critique of the Underperforming Model of Local Governance in Eastern
   Indonesia},
Journal = {JOURNAL OF CURRENT SOUTHEAST ASIAN AFFAIRS},
Year = {2020},
Volume = {39},
Number = {3},
Pages = {359-380},
Month = {DEC},
Abstract = {This article investigates the challenges facing decentralised governance
   in poor and underdeveloped areas in Eastern Indonesia. The Timor Tengah
   Selatan (TTS) regency in West Timor in Nusa Tenggara Timur province is
   taken as a case study. Indonesia's radical decentralisation programme
   applied a national model of decentralised governance, not taking into
   account the different conditions applying to disadvantaged regions
   (daerah tertinggal, DRs). In the TTS regency, decentralised governance
   is underperforming in two core areas - administration and fiscal
   viability - while making some progress in political decentralisation.
   Governance is restricted by limited social capacity, a poor resource
   base, and a lack of investment capital and infrastructure. The question
   then arises: if the uniform model of decentralisation is not performing
   adequately in TTS, is there a more appropriate model of local governance
   and central subnational relations that can better perform in DRs? While
   not detailing the features of a new model, this article identifies the
   areas requiring policy development.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Franklin, N (Corresponding Author), Charles Darwin Univ, Coll Indigenous Futures Arts \& Soc, Darwin, NT, Australia.
   Shoesmith, Dennis; Franklin, Nathan, Charles Darwin Univ, Coll Indigenous Futures Arts \& Soc, Darwin, NT, Australia.
   Hidayat, Rachmat, Univ Jember, Dept Publ Adm, Jember, Indonesia.},
DOI = {10.1177/1868103420963140},
ISSN = {1868-1034},
