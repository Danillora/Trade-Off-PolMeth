@article{ WOS:000593156500008,
Author = {Geers, Sabine},
Title = {NEWS CONSUMPTION ACROSS MEDIA PLATFORMS AND CONTENT A TYPOLOGY OF YOUNG
   NEWS USERS},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2020},
Volume = {84},
Number = {1, SI},
Pages = {332-354},
Abstract = {In a changing information environment in which people increasingly
   select a combination of media platforms to consume news, scholars have
   taken a more comprehensive approach in measuring news consumption by
   examining news media repertoires. This study specifically (1) examines
   news repertoires of young people, based on their combined usage of media
   platforms and news content preferences; and (2) examines their variation
   due to social background characteristics. Results from a survey of Dutch
   adolescents (N = 1,084; age 16 to 21) reveal four distinct news
   repertoires: labeled minimalists, omnivores, traditionalists, and online
   news users. Findings further suggest that platform-based news
   repertoires are related to preferences for specific news content.
   Finally, this study contributes to the digital divide literature by
   demonstrating that inequalities in news media usage related to education
   do not seem to apply to younger citizens.},
