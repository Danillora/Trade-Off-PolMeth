@article{ WOS:000529849500004,
Author = {Yeophantong, Pichamon},
Title = {China and the Accountability Politics of Hydropower Development: How
   Effective are Transnational Advocacy Networks in the Mekong Region?},
Journal = {CONTEMPORARY SOUTHEAST ASIA},
