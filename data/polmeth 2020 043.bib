@article{ WOS:000513517700002,
Author = {Gaffney, Alfie Christopher Byron and Evensen, Darrick},
Title = {Addressing the Elephant in the Room: Learning from CITES CoP17},
Journal = {GLOBAL ENVIRONMENTAL POLITICS},
Year = {2020},
Volume = {20},
Number = {1},
Pages = {3-10},
Month = {FEB},
Abstract = {The seventeenth Convention on International Trade in Endangered Species
   (CITES) conference of the parties (CoP) exhibited a draconian opposition
   to any trade in ivory. A key component of this opposition was the
   intergovernmental regime's consolidation of North-South power
   differentials through the increased presence and influence of Northern
   conservation-focused NGOs. Using the example of ivory, this Forum
   article unpacks this dynamic before advocating for more participatory,
   decentralized, and polycentric approaches to the global governance of
   endangered species trade at future CITES CoPs.},
