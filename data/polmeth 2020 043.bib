@article{ WOS:000559852700003,
Author = {Ardovini, Lucia and Mabon, Simon},
Title = {Egypt's unbreakable curse: Tracing theState of Exceptionfrom
   Mubarak to Al Sisi},
Journal = {MEDITERRANEAN POLITICS},
Year = {2020},
Volume = {25},
Number = {4},
Pages = {456-475},
Month = {AUG 7},
Abstract = {This paper uses Giorgio Agamben'sState of Exceptionas a theoretical
   approach that allows us to see how emergency legislations operate in the
   region as mechanisms of control and dominant paradigms of governance.
   Relying on Egypt as a case study, this paper traces the significance of
   emergency rule throughout Mubarak's era up until Al Sisi's 2014
   Constitution. It applies a four-stage analytical framework to
   investigate whether or not Egypt was indeed ruled by the exception
   throughout its turbulent recent history, while under the guise
   ofEmergency Rule. In doing so, we aim to provide an analysis of the
   legal structures that shape Egyptian politics, while also adding to
   debates on theState of Exception, particularly on its application in the
   non-Western world.},
