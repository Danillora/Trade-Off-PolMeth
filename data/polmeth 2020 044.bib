@article{ WOS:000545215700006,
Author = {Paredes, Maritza and Dosek, Tomas},
Title = {The Subnational Indigenous Quota in Peru: The Paradoxes of Political
   Representation},
Journal = {LATIN AMERICAN POLITICS AND SOCIETY},
Year = {2020},
Volume = {62},
Number = {3, SI},
Pages = {123-148},
Month = {AUG},
Abstract = {The objective of this article is to explain the territorial variation in
   indigenous political representation at the subnational level in Peru.
   The Peruvian state introduced a weak indigenous electoral quota, and its
   effects have varied across provinces. This article presents a typology
   that combines descriptive and substantive dimensions of political
   representation. Using a subnational comparative method, the four cases
   studied illustrate distinct dynamics. The article argues that these
   differing dynamics are explained by a combination of sociostructural
   variables (i.e., political articulation and cohesion of indigenous
   organizations as a result of a conflict) and individual variables (i.e.,
   candidates' political capital). This article demonstrates how, even in
   an institutionally adverse environment that does not feature ethnic
   parties, both descriptive and substantive indigenous political
   representation can be achieved, and that these two dimensions are not
   necessarily related. The article is based on fieldwork and interviews
   with key provincial political actors.},
