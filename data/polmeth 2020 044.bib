@article{ WOS:000642329300008,
Author = {Wiesehomeier, Nina and Verge, Tania},
Title = {CORRUPTION, OPPORTUNITY NETWORKS, AND GENDER: STEREOTYPES OF FEMALE
   POLITICIANS' CORRUPTIBILITY},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2020},
Volume = {84},
Number = {2},
Pages = {538-550},
Month = {SUM},
Abstract = {Given the gender stereotype that women are more ethical than men, people
   should assess female politicians as being less corruptible. Yet
   information about access to networks suggests that opportunities to
   engage in unethical behavior may counter this perception. Using a
   conjoint analysis in a nationally representative survey in Spain, a
   country shaken by corruption scandals, we asked respondents to identify
   the more corruptible politician between two hypothetical local
   councilors by imagining an investor willing to offer a bribe to advance
   business interests. Results indicate that female politicians do
   symbolically stand for honesty. However, this assessment is offset by
   embeddedness cues signaling a woman politician's access to opportunity
   networks. We discuss our findings in light of instrumentalist arguments
   for an increase of women in politics as a means to combat corruption.},
