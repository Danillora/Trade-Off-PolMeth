@article{ WOS:000549690200001,
Author = {Skvrnak, Michael},
Title = {You'll never rule alone: how football clubs and party membership affect
   coalition formation},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2021},
Volume = {47},
Number = {2},
Pages = {312-330},
Month = {MAR 4},
Abstract = {The research on coalition formation has used the abundance of
   local-level data to test the classical theories of coalition formation.
   I take the classical theories as a baseline and supplement them with
   additional factors related to how political agents are embedded in the
   local community. In particular, I investigate how the social capital of
   elected councillors attained in local football clubs and the membership
   bases of local party branches affect coalition formation. The results of
   conditional logit models show that coalitions are more likely to be
   formed between parties whose elected councillors are members of a local
   football club. In addition, a higher share of elected councillors who
   are not party members is associated with preference for oversized
   coalitions. This indicates that when local party branches are not sure
   about their own and their coalition partners' unity, they prefer to
   ensure coalition survival by including an additional party.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
