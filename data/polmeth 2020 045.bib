@article{ WOS:000569094300009,
Author = {Milacic, Filip},
Title = {Europeanization and the Statehood Problem: The Cases of Croatia and
   Serbia},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2020},
Volume = {48},
Number = {5},
Pages = {861-875},
Month = {SEP},
Abstract = {Croatia and Serbia are two countries that started their paths toward the
   EU with similar conditions. Nevertheless, in 2018, the end of this
   study's period, the two countries are characterized by different
   outcomes regarding European integration. This paper puts forward one key
   determinant for the more successful Europeanization process of
   Croatia-resolved statehood problem-and claims that the unresolved
   statehood problem in Serbia led to higher adoption costs of EU rules.
   Therefore, this article seeks to improve the theorization of the
   relationship between Europeanization and the statehood problem.},
