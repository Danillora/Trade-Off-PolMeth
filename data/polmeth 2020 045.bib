@article{ WOS:000512139300001,
Author = {Roberts, Jane},
Title = {The leadership of place and people in the new English combined
   authorities},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2020},
Volume = {46},
Number = {6},
Pages = {995-1014},
Month = {NOV 1},
Abstract = {This paper explores the theory and early practice of elected political
   leadership in the first six English mayoral combined authorities (CAs).
   It draws on the history of local government in England, policy
   documentation, and theory on leadership, place and institutional
   formation to analyse the early leadership of the metro-mayors from the
   perspective of the leadership of place. It suggests that public policy
   and the literature on political leadership could usefully recognise the
   power of place in the exercise of leadership. The paper concludes by
   raising questions around the development of CAs and their mayors within
   the wider governance landscape.},
