@incollection{ WOS:000668702000009,
Author = {Kan, Hoi-Yi Katy},
Book-Author = {Kan, HYK},
Title = {Power As Constantly Reconstituting and the Prospects of Carnivalesque
   Politics},
Booktitle = {DIGITAL CARNIVALESQUE: POWER DISCOURSE AND COUNTER NARRATIVES IN
   SINGAPORE SOCIAL MEDIA},
Series = {Cultural Studies and Transdisciplinarity in Education},
Year = {2020},
Volume = {10},
Pages = {151-167},
Abstract = {In this chapter, I return to the key theorizations of power that I
   discussed in the earlier part of this book. I begin broadly by examining
   the usefulness of Foucauldian governmentality as a theory of power in
   explicating the modus operandi of Singapore's PAP government in curbing
   free speech in the country's media space. I then move on to examine the
   limitations of Foucauldian governmentality and Bourdieusian cultural
   perspective specifically in the area of artistic and literary
   performance. Next, I theorize contemporary power relations in vernacular
   digital discourses by foregrounding two important aspects: the emergence
   of information brokers and the affective dimension in power relations.
   In the final section, I theorize on the potentials of carnivalesque as
   subpolitics and as well as tool of social critique in the context of
   Singapore.},
