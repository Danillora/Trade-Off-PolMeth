@article{ WOS:000733384100012,
Author = {Loiacono, Alessia},
Title = {Ancient rhetoric and Machiavelli's experience at the French court},
Journal = {LABORATOIRE ITALIEN-POLITIQUE ET SOCIETE},
Year = {2021},
Number = {26},
Abstract = {Machiavelli's first diplomatic mission to France (1500) is a significant
   episode, and not only with regard to the political friendship between
   the Florentine Republic of the Great Council and the monarchy of Louis
   XII. It also represents a significant moment in the genesis of modern
   political thought. In Machiavelli's official correspondence during this
   mission, we find concepts associated with classical and humanistic
   cultural heritage. These are, nonetheless, reinterpreted in the context
   of the Italian Wars.},
