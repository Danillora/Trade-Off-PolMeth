@article{ WOS:000866238800004,
Author = {Ojo, Tinuade Adekunbi},
Title = {Impact of Institutional Quality and Governance on Financial Inclusion
   for Women in South Africa: A Case of Gauteng Women Entrepreneurs},
Journal = {STRATEGIC REVIEW FOR SOUTHERN AFRICA},
