@article{ WOS:000764142100001,
Author = {Guruvayurappan, Darsan},
Title = {Rethinking Defection: An Analysis of Anti-defection Laws in India},
Journal = {PARLIAMENTARY AFFAIRS},
Year = {2023},
Volume = {76},
Number = {2},
Pages = {443-464},
Month = {MAR 28},
Abstract = {Are anti-defection laws efficient? Many parliamentary democracies have
   enacted anti-defection laws to prevent frequent government collapses and
   political instability. Using the anti-defection law in India as a
   reference, I argue that the party-based anti-defection laws enacted in
   many Westminster-style Parliamentary democracies do not achieve their
   goals, yet drastically impact representative decision-making. I show how
   the entitlements and protections granted by anti-defection laws to
   political parties and legislators do not achieve the purpose of
   maintaining government stability and suggest two options to improve it.},
