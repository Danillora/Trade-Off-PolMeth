@article{ WOS:000717895900010,
Author = {Fedele, Valentina},
Title = {THE HIRAK. THE VISUAL PERFORMANCE OF DIVERSITY IN ALGERIAN
   PROTESTS},
Journal = {PARTECIPAZIONE E CONFLITTO},
Year = {2021},
Volume = {14},
Number = {2, SI},
Pages = {681-701},
Abstract = {Based on the analysis of the first year of the Hirak, the Algerian
   protest movement started on February 2019, the article focuses on its
   visual performance and on the narrative emerging from it. Banners,
   posters and photos highlight the HiraRs specific attempt to give a new
   meaning to the discourse about the Algerian people and its unity.
   Particular attention is given to the visual performance of Algerian
   diversity exemplified by the deployment of the Amazigh flag during the
   protests. The flag became more than a cultural symbol and represented an
   instance of freedom and a manifestation of the people's will to escape
   traditional hetero-definitions of belongings and identities.},
