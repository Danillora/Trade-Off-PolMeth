@article{ WOS:000685489600004,
Author = {Billings, Dwight B.},
Title = {A Stranger in a Strange Land: David Ruccio's Occasional Links \&
   Commentary},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2021},
Volume = {33},
Number = {3},
Pages = {360-365},
Month = {JUL 3},
Abstract = {In this article I examine the remarkable output of David Ruccio's
   Occasional Links \& Commentary. I stress the multidisciplinary nature of
   Ruccio's blog (especially its relationship to the humanities), its
   ruthless criticism of capitalism and mainstream economics, its effort to
   link everyday and academic knowledges, and, ultimately, its pedagogical
   value for conveying vast amounts of economic information for a broad
   readership while exemplifying a non-essentialist approach to Marxian
   theory. I illustrate these points by examining two blog series in
   particular: David's extensive commentary on what he has called the
   ``Second Great Depression{''} that followed the financial collapse of
   2008 and his ongoing discussions of utopian thought.},
