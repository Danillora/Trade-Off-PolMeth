@article{ WOS:000866238800012,
Author = {Tella, Oluwaseun},
Title = {South Africa's Curriculum Transformation: Insights from
   Post-Independence Africa and Post-Civil Rights Movement in the United
   States},
Journal = {STRATEGIC REVIEW FOR SOUTHERN AFRICA},
