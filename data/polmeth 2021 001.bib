@article{ WOS:000629905100001,
Author = {Yildiz, Taylan Ozgur},
Title = {The Metaphorology of the New Authoritarianism A Case Study on the
   Renewal of the Corporeal Body Myth},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2021},
Volume = {62},
Number = {2},
Pages = {253-279},
Month = {JUN},
Abstract = {This study contributes to the metaphorological analysis of the new
   authoritarianism. Assuming that autocratization is characterized by the
   myth of the corporeal body rather than by rational elements, the paper
   uses the case of Turkey to show that its central mechanism lies in the
   narrative activation of the domestic metaphor and its translation into
   Foucauldian pastoral power under conditions of crisis. This mechanism,
   which seemed worn out with the growing distance from the Kemalist
   revolution, has experienced a new transformative capacity since the
   failed coup attempt in 2016. The article concludes with general
   reflections on the performance of authoritarian myths.},
