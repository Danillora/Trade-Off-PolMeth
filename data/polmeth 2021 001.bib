@article{ WOS:000706614500038,
Author = {Blanco Suarez, Jorge Orlando and Mendez Duarte, Jefferson},
Title = {The (neo) Pentecostals movement and political parties and the hegemonic
   fight into the Colombian political system crisis (1991-2018)},
Journal = {IZQUIERDAS},
Year = {2021},
Volume = {50},
Month = {APR},
Abstract = {The paper interprets the insertion of evangelical political movements,
   specifically pentecostal and neopentecostals into the Colombian
   political system, from 1991 to the present. Through a large documental
   analysis that includes most of the research about the matter,
   journalistic fonts and a short ethnographic approach, argues that such
   insertion is a key factor of traditional forces hegemony. The
   Pentecostal and Neopentecostals political movements Works by mean of a
   structural positioning into the sociopolitical, economic and cultural
   planes. Contrary to what one might think, it is suggested that their
   role, rather than being an alternative of power, is relegated to
   counteracting counter-hegemonic initiatives on the political and
   sociocultural level. The role of these movements is, in that sense, a
   hegemonical base for the production a reproduction of hegemony and a key
   factor to deal contra hegemonical projects.},
Publisher = {ARIADNA EDICIONES \& SAINT PETERSBURG STATE UNIV},
Address = {LAGUNA INVERNADA 0246, CENTRAL STATION, SANTIAGO, 7500618, CHILE},
Type = {Article},
Language = {Spanish},
