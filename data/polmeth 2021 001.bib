@article{ WOS:000727963400002,
Author = {Geoana, Mircea},
Title = {NATO 2030: KEEPING US SAFE IN A MORE COMPETITIVE WORLD},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2021},
Volume = {20},
Number = {2},
Pages = {11-18},
Month = {SUM},
Abstract = {In this new age of growing global competition and increasing complex
