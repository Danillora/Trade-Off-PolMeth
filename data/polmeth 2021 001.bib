@article{ WOS:000640591800009,
Author = {Septak, Miroslav},
Title = {EUROPEAN PARLIAMENT ELECTIONS IN AUSTRIA. SIGNAL OF OTHER TRIUMPH OF
   PEOPLE'S PARTY},
Journal = {POLITICKE VEDY},
Year = {2021},
Volume = {24},
Number = {1},
Pages = {205-230},
Abstract = {The elections into European Parliament in May 2019 have so far attracted
   the marginal attention of researchers. In the case of Austria, they were
   important to test before the autumn early parliamentary elections and
   indicated the electoral preferences of the time. The study is focused on
   analysis based on historical periodicals, relevant statistical data, and
   polls election campaign and election results with emphasis on Austria.
   Let us not forget electoral programs electoral leaflets, in contemporary
   political science research almost neglected resources for election
   analysis. The Austrian elections into European Parliament can be
   described as second-order elections, as they weakened government parties
   and strengthened the then nonparliamentary Greens and Opposition's
   Liberals, a lower turnout than in the National Council and the
   electorate's emphasis on national issues than on European Union issues.
   Only the fifth feature of in second-order elections, the preference of
   candidates over party affiliation remained unfulfilled, as this time
   voters considered the programmatic positions of candidates more
   important than individual leaders due to the preference of program
   positions by the electorate over the individual leaders of candidate
   parties.},
