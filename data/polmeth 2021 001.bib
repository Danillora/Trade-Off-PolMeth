@article{ WOS:000695354100001,
Author = {Hernandez Zapata, Edwin Alexander and Morales Herrera, Milton Danilo and
   Vivares Porras, Diana Vanessa},
Title = {Discourse of Work among Former Combatants of the FARC. Socio-Labor
   Barriers to Reintegration},
Journal = {AMERICA LATINA HOY-REVISTA DE CIENCIAS SOCIALES},
Year = {2021},
Volume = {88},
Pages = {3-21},
Month = {AUG},
Abstract = {Discourse analysis is used to identify the meanings and socio-labor
   barriers perceived by former FARC combatants in the post-conflict
   scenario, which must be understood to establish frameworks for action in
   the peace-building process. The interpretative repertoire ``to be
   crossed out{''} is shown as an ontological mark in the former combatant
   that deprives him of access to different social settings and that
   hinders the identity configuration, fostering feelings of nullification
   of his civil utility.},
