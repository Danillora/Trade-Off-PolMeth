@article{ WOS:000674755300003,
Author = {Fox, Amos},
Title = {On Sieges},
Journal = {RUSI JOURNAL},
Year = {2021},
Volume = {166},
Number = {2},
Pages = {18-28},
Abstract = {Sieges, while not as flashy as hybrid warfare or grey-zone conflict, nor
   as trendy as disruptive technology and its associated concepts, command
   a central position in the wars of the post-Cold War era. Furthermore,
   despite being commonly associated with the tactical and operational
   level of conflict, sieges are often strategic and decisive affairs, as
   the siege of the US Embassy in Baghdad in December 2019 clearly
   demonstrated. Sieges bring a degree of decisiveness back to the
   battlefield, and those of the Russo-Ukrainian War (2014-present) bring
   this point to bear. In this article, Amos Fox suggests that
   policymakers, strategists and practitioners must understand how sieges
   operate and where they fit within war.},
