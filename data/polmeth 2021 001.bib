@article{ WOS:000727529400001,
Author = {Draper, Jamie},
Title = {Labor Migration and Climate Change Adaptation},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2022},
Volume = {116},
Number = {3},
Pages = {1012-1024},
Month = {AUG},
Abstract = {Social scientific evidence suggests that labor migration can increase
   resilience to climate change. For that reason, some have recently
   advocated using labor migration policy as a tool for climate adaptation.
   This paper engages with the normative question of whether, and under
   what conditions, states may permissibly use labor migration policy as a
   tool for climate adaptation. I argue that states may use labor migration
   policy as a tool for climate adaptation and may even have a duty to do
   so, subject to two moral constraints. First, states must also provide
   acceptable alternative options for adaptation so that the vulnerable are
   not forced to sacrifice their morally important interests in being able
   to remain where they are. Second, states may not impose restrictive
   terms on labor migrants to make accepting greater numbers less costly
   for themselves because doing so unfairly shifts the costs of adaptation
   onto the most vulnerable.},
