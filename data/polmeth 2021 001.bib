@article{ WOS:000702283600005,
Author = {Figueroa, Jose-Antonio},
Title = {Privatized War, Lumpen Capitalism, and Racism in the Ecuador-Colombia
   Border},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
Year = {2021},
Number = {31},
Pages = {78-89},
Month = {SEP-DEC},
Abstract = {This article analyzes the impact of the privatization of the Colombian
   war in the Ecuador-Colombia border and how this privatized war is
   related to the dispossession of the Afro-descendant population. Through
   the analysis of Plan Colombia and the appearance of private armies in
   the Colombian war, the article shows how the war responds to the
   interests of the transnational arms businesses. The responses that the
   Colombian state has given to the demands of the Afro-descendant
   population of the Narino coast reveal how the culturalist discourse that
   has been imposed since the 1990s has served to consolidate the exclusion
   and dispossession produced by the privatized war.},
