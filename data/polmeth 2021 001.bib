@article{ WOS:000658402500004,
Author = {Oztig, Lacin Idil and Gurkan, Turkan Ayda and Aydin, Kenan},
Title = {The Strategic Logic of Islamophobic Populism},
Journal = {GOVERNMENT AND OPPOSITION},
