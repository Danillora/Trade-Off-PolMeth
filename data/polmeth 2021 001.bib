@article{ WOS:000650968300002,
Author = {Gerver, Mollie},
Title = {Must refugees return?},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2021},
Volume = {24},
Number = {4},
Pages = {415-436},
Month = {JUN 7},
Abstract = {It is widely accepted that states have a right to control immigration,
   but must accept refugees at risk in their home countries. If this is
   true, perhaps states have a right to deport refugees once their lives
   are no longer at risk in their home countries. I raise three types of
   arguments against this claim, and in support of refugees' right to
   remain. Citizenship-based arguments hold that refugees have a right to
   obtain citizenship, and with citizenship comes the right to remain.
   Plans-based arguments hold that refugees have a right to plan their
   lives, and they will struggle to plan without the right to remain.
   Reciprocity-based arguments hold that refugees have a right to
   reciprocal relationships with citizens, far easier if they know they can
   remain. I reject the first two arguments, and defend the third.},
