@article{ WOS:000637940200001,
Author = {Kroger, Sandra and Lorimer, Marta and Bellamy, Richard},
Title = {The Democratic Dilemmas of Differentiated Integration: The Views of
   Political Party Actors},
Journal = {SWISS POLITICAL SCIENCE REVIEW},
Year = {2021},
Volume = {27},
Number = {3, SI},
Pages = {563-581},
Month = {SEP},
Abstract = {Differentiated integration (DI) appeals as a pragmatic way of
   accommodating political and economic differences among member states
   (MS). However, it potentially challenges their equal standing in EU
   decision-making, creating the possibility for some MS to dominate
   others. As such, it risks undermining the democratic legitimacy of the
   EU. Drawing on 35 interviews with party actors in seven MS, we find many
   shared these concerns, thereby questioning the acceptance of DI. While
   they considered DI could support self-determination at the national
   level, they worried it might result in arbitrary exclusion and growing
   inequality at the EU level. To be non-dominating, they contended
   differentiated policies must remain open for all to join, be based on
   clear criteria, and allow all MS a say, though only participating states
   should be entitled to vote on differentiated policies in the Council,
   whereas all MEPs should be able to vote in the European Parliament.},
