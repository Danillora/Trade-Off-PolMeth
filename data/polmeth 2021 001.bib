@article{ WOS:000649359000011,
Author = {Dzankic, Jelena and Keil, Soeren},
Title = {Post-Partition Citizenship Policies: Lessons from Post-Yugoslav Federal
   States},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2021},
Volume = {51},
