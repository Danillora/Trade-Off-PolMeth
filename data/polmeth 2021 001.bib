@article{ WOS:000700484900001,
Author = {Cox, Dame Laura},
Title = {A Woman's Place is in the House: Reclaiming Civility, Tolerance and
   Respect in Political Life},
Journal = {POLITICAL QUARTERLY},
Year = {2022},
Volume = {93},
Number = {1},
Pages = {17-24},
Month = {JAN},
Abstract = {This article is based on the keynote address given by the author on 6
   May 2021 at a conference organised by Maison Francaise
   d'Oxford/Worcester College, Oxford, with the support of The Political
   Quarterly and the Alfred Toepfer Stiftung. It considers the world of
   angry and divisive politics in which we currently reside and the serious
   adverse impact this is having on women in politics and in public life
   generally. It examines the causes of the increasing misogyny that exists
   both online and offline, why there is both an individual and an
   institutional responsibility to take action to tackle it, and how these
   problems need urgently to be addressed if women are to continue to play
   an active role in political life.},
