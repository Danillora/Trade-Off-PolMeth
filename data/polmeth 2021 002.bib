@article{ WOS:000677249000001,
Author = {Chamberlain, Adam and Yanus, Alixandra B.},
Title = {Density dependence in economic sectors in the Progressive Era},
Journal = {INTEREST GROUPS \& ADVOCACY},
Year = {2022},
Volume = {11},
Number = {1},
Pages = {1-25},
Month = {MAR},
Abstract = {This analysis applies the energy-stability-area (ESA) model to the
   commercial and agricultural sectors during the Progressive Era. Using
   state-level counts of commercial and agricultural organizations derived
   from federal government reports, we find clear evidence that both
   sectors were density dependent-the number of commercial and agricultural
   organizations was bounded by the interested population. Additional
   analysis reveals that younger states had greater numbers of state and
   local commercial organizations than older states, and that greater
   government costs and constituent interest helped drive increases in
   agricultural organizations. The level of partisan competition and the
   partisan composition of the state government, however, did not
   systematically influence group formation in either economic sector. In
   total, this study provides new insights into the application of the ESA
   model and the development of the American interest group state.},
Publisher = {PALGRAVE MACMILLAN LTD},
