@article{ WOS:000609700200001,
Author = {Forster, Elisabeth and Taylor, Isaac},
Title = {Asking the fox to guard the chicken coop: In defense of minimalism in
   the ethics of war and peace},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
Year = {2022},
Volume = {18},
Number = {1},
Pages = {91-109},
Month = {FEB},
Abstract = {Dominant normative theories of armed conflict orientate themselves
   around the ultimate goal of peace. Yet the deployment of these theories
   in the international sphere appears to have failed in advancing toward
   this goal. In this paper, we argue that one major reason for this
   failure is these theories' use of essentially contested concepts-that
   is, concepts whose internally complex character results in no principled
   way of adjudicating between rival interpretations of them. This renders
   the theories susceptible to manipulation by international actors who are
   able to pursue bellicose policies under the cover of nominally pacific
   frameworks, and we show how this happened historically in a case study
   of the Korean War of 1950-1953. In order to better serve the goals of
   peace, we suggest, the rules of war should be reframed to simpler, but
   more restrictive, normative principles.},
