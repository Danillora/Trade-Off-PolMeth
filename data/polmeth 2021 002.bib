@article{ WOS:000693560300001,
Author = {Andersson Malmros, Robin},
Title = {Translating Ideas into Actions: Analyzing Local Strategic Work to
   Counter Violent Extremism},
Journal = {DEMOCRACY \& SECURITY},
Year = {2021},
Volume = {17},
Number = {4},
Pages = {399-426},
Month = {OCT 2},
Abstract = {Despite the growing importance of local action to counter violent
   extremism (CVE), empirical research on the local organization and
   management of CVE is scarce, especially regarding public administrators'
   strategic work to translate policies and recommendations into frontline
   practice. Based mainly on ethnographic data and departing from new
   institutional theory, the paper refines our understanding of the
   symbolic, material, and relational work used to translate a diverse flow
   of ideas into concrete action in diverse institutional settings. Due to
   the institutional complexity, the cultural skill of the local CVE
   coordinator is identified as pivotal to successfully legitimizing and
   implementing CVE efforts.},
