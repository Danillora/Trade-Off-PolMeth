@article{ WOS:000766714700006,
Author = {Andres Rossi, Ignacio},
Title = {The Peronist left and economic ideas: Montoneros and their journalistic
   wing of Noticias in the face of the oil crisis (1973-1974)},
Journal = {IZQUIERDAS},
Year = {2021},
Volume = {50},
Month = {APR},
Abstract = {The newspaper Noticias sobre todo lo que pasa en el mundo (from now on,
   Noticias) constitutes a piece of journalistic analysis of vital
   importance for the approach of the Argentine left in the 1970s. Linked
   to the Montoneros guerrilla movement, it constituted a profuse periodic
   initiative, successful and of great relevance characterized by a staff
   of renowned intellectual and political personalities in national life.
   We specifically propose an analysis of the debates that were opened in
   said publication around the 1973 oil crisis, which occupied a large
   space on its pages, as well as its national incidence. For this, an
   analysis has been reconstructed on the economic ideas of Montoneros, the
   importance of its main references in the economic field, the
   characteristics of said publication and examined the available numbers.
   We have found that Noticias gave great importance to the debates around
   the oil crisis and that these involved important aspects of its
   politicaleconomic conceptions around vital issues such as international
   relations, the role of the State, the place of companies and the
   village.},
