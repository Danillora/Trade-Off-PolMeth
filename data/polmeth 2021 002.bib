@article{ WOS:000613762300001,
Author = {Candilis, Philip J. and Cleary, Sean D. and Dhumad, Saleh and Dyer,
   Allen R. and Khalifa, Najat},
Title = {Classifying terrorism: a latent class analysis of primary source
   socio-political and psychological data},
Journal = {BEHAVIORAL SCIENCES OF TERRORISM AND POLITICAL AGGRESSION},
