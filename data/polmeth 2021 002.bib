@article{ WOS:000833599700003,
Author = {Semenov, Andrej},
Title = {The Washington Agreement: A Supplementary Document to the EU Efforts in
   Kosovo?},
Journal = {INSIGHT TURKEY},
Year = {2021},
Volume = {23},
Number = {4},
Pages = {259-275},
Month = {FAL},
Abstract = {This paper analyzes the Washington Agreement (WA) in the context of
   European Union (EU) efforts to facilitate the dialogue between Kosovo
   and Serbia. The starting point is based on the idea that EU-led
   negotiations seek consensus between leadership in Belgrade and Pristina,
   as opposed to unilateral or imposed solutions. The central question is
   as follows: is the WA a supplementary document to the Brussels process?
   While certain points of the agreement are a mere repetition of previous
   agreements signed by the two parties, the agreement commits Kosovo to
   the mini-Schengen and brings a novelty into the dynamics of the
   Belgrade-Pristina relationship, such as in the case of the joint
   management of Gazivode/Ujmani lake. With a new Kosovar government, the
   mini-Schengen point, together with the point of Kosovo and Serbia's
   pledge to open/move embassies to Jerusalem, remains the most contested
   part of the Agreement. Finally, the two sides have reaffirmed their
   commitment to western values and agree to become less dependent on
   Russia's energy and China's technology supplies.},
