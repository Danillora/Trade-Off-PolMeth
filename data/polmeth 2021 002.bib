@article{ WOS:000740758500037,
Author = {Kuteynikov, Dmitry and Izhaev, Osman and Lebedev, Valerian and Zenin,
   Sergey},
Title = {Human rights during the mass introduction of artificial intelligence and
   robotic systems into public life},
