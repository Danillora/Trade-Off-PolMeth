@article{ WOS:000661936200011,
Author = {Redepenning, Dorothea},
Title = {The sounds of remembrance Musical interpretations of Babi Yar},
Journal = {OSTEUROPA},
Year = {2021},
Volume = {71},
Number = {1-2},
Pages = {141+},
Abstract = {In art, Babi Yar was portrayed as the site of one of the largest
   massacres of the Second World War and as a place of remembrance even
   before a monument was erected there. Dmitry Shostakovich's first
   movement of his 13th Symphony, which is based on the famous poem by
   Yevgeny Yevtushenko, towers above all other commemorative musical works.
