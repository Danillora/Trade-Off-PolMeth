@article{ WOS:000629718800001,
Author = {Carter, Christopher L.},
Title = {The Representational Effects of Communal Property: Evidence from Peru's
   Indigenous Groups},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2021},
Volume = {54},
Number = {12},
Pages = {2191-2225},
Month = {OCT},
Abstract = {Why do some indigenous groups achieve coethnic political representation
   while others do not? In this paper, I highlight the primary role of
   communal property in shaping indigenous representation. While scholars
   often laud the developmental benefits of communal land titling, I argue
   that formalizing collectively held land can inhibit indigenous
   coordination to achieve political representation. Where communal land is
   informally held, indigenous groups are more likely to invest in
   traditional institutions that facilitate collective action to elect
   coethnic candidates to political office. Conversely, titling communal
   property secures indigenous land access but in the process erodes
   traditional institutions that would otherwise promote collective action
   during elections. I test my argument using a multi-method approach that
   includes interviews and experiments with three-hundred Peruvian
   indigenous leaders, historical land-title data, and information scraped
   from mayoral candidate CVs. The findings suggest that the oft-cited
   economic benefits of collective property may generate negative political
   effects.},
