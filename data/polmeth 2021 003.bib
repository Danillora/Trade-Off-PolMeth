@article{ WOS:000705322900001,
Author = {Fernandez-I-Marin, Xavier and Rapp, Carolin H. and Adam, Christian and
   James, Oliver and Manatschal, Anita},
Title = {Discrimination against mobile European Union citizens before and during
   the first COVID-19 lockdown: Evidence from a conjoint experiment in
   Germany},
Journal = {EUROPEAN UNION POLITICS},
Year = {2021},
Volume = {22},
Number = {4},
Pages = {741-761},
Month = {DEC},
Abstract = {One of the greatest achievements of the EU is the freedom of movement
   between member states offering citizens equal rights in EU member
   states. EU enlargement and the COVID-19 pandemic allow for a critical
   test of whether EU citizens are indeed treated equally in practice. We
   test preferential treatment of EU citizens in two hypothetical choice
   experiments in Germany at two different time points: in the period
   before and during the COVID-19 pandemic lockdown. Theories of responses
   to threat suggest that the COVID-19 crisis should increase
   discrimination against mobile EU citizens. While our findings reveal
   sizeable discrimination based on nationality and language proficiency of
   mobile EU citizens, the findings also suggest that, contrary to
   expectations, discrimination did not increase in the initial COVID-19
   crisis period.},
