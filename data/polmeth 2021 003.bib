@article{ WOS:000792159300001,
Author = {Ahler, Douglas J. and Roush, Carolyn E. and Sood, Gaurav},
Title = {The micro-task market for lemons: data quality on Amazon's Mechanical
   Turk},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
