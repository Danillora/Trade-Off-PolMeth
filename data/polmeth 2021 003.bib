   Malpas, Amelia; Hilton, Adam, Mt Holyoke Coll, S Hadley, MA 01075 USA.},
DOI = {10.1515/for-2021-0012},
ISSN = {2194-6183},
EISSN = {1540-8884},
Keywords = {democratic party; inequality; neoliberalism; party platforms; content
   analysis},
