@article{ WOS:000628822100007,
Author = {Bolgherini, Silvia and Lippi, Andrea},
Title = {Oblique-change matters: `bradyseismic' institutional change in local
   government},
Journal = {ITALIAN POLITICAL SCIENCE REVIEW-RIVISTA ITALIANA DI SCIENZA POLITICA},
Year = {2021},
Volume = {51},
Number = {1},
Pages = {117-135},
Month = {MAR},
Abstract = {In the last 30 years, Local Governments all over Europe experienced an
   intense season of institutional change of unprecedented width and
   intensity. This paper focuses on a neglected type of institutional
   change, a more indirect one - here labeled oblique-change - that however
   strongly influences the overall LG institutional change and local
   autonomy. Taking 2012 as the climax of the austerity period in Europe
   and Italy as a pilot case for future comparisons, this article shows
   that oblique-change matters to a considerable extent, and that it is
   much more frequent and highly impacting than expected. Moreover, it
   argues that bradyseismic adjustments provoked by oblique-change may turn
   out in an equally profound change of the local government's asset, as
   that induced by major reforms.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {32 AVENUE OF THE AMERICAS, NEW YORK, NY 10013-2473 USA},
