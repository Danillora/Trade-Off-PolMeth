@article{ WOS:000692073600001,
Author = {Yi, Chengzhi and Hu, Shujia and Zeng, Runxi},
Title = {The Influence of Religious Belief on Political Voting Behaviour of Urban
   Residents: Empirical Analysis Based on China's Survey Data},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Year = {2024},
Volume = {9},
Number = {1},
Pages = {63-84},
Month = {MAR},
Abstract = {The relationship between religious belief and political voting behaviour
   remains controversial and is influenced by political and cultural
   contexts. This study explores the influence of religious belief on the
   political voting behaviour of urban residents in China based on urban
   sample survey data from the Chinese General Social Survey (CGSS) 2010
   and finds that both religious identity and religious behaviour have a
   significant positive association with political voting behaviour.
   Compared with those without religious belief, the probability of urban
   residents with religious belief voting in urban residents' committee
   elections is significantly increased. Urban residents who participate in
   religious activities more frequently are more active in voting in
   residents' committee elections. Buddhist beliefs, Taoism, and folk
   beliefs have a positive association with urban residents' voting in
   these elections, indicating that local religious beliefs promote these
   residents' political voting behaviour. This study supports that the
   relationship between religious belief and political voting behaviour in
   China is consistent with the theoretical understanding in a broader
   context.},
