@article{ WOS:000664383700001,
Author = {Yu, Xiaowei},
Title = {Bourdieu's Talk: High-Education-Low-Income Issue Among Skilled
   Immigrants from Chinese Mainland in Canada},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Year = {2022},
Volume = {7},
Number = {2, SI},
Pages = {288-304},
Month = {JUN},
Abstract = {An attempt of positioning, describing, and explaining
   high-education-low-income issue among skilled immigrants from Chinese
   mainland in Canada was conducted using Bourdieu's concepts of habitus,
   field, and capital. It is concluded that the issue is largely caused by
   habitus conflicts and transitions happened short after the recent
   Chinese professional immigrants arrived in Canada as well as devalued
   cultural capital and lost of social capital that they ever had in China
   before. Meanwhile, the study indicated that high-education-low-income
   issue universally exists among recent immigrants from various ethnic
   groups in Canada currently. Immigration, in Bourdieu's terms, was a
   course of devaluation and reproduction of capital that immigrants ever
   held before and will hold in the future. Potential success in
   immigration exists in sufficient preparation both psychological and
   physical for their habitus transition in advance and positively possible
   capital conversion both material and non-materials following their
   arrival of the new country.},
