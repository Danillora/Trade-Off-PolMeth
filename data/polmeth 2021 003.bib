@article{ WOS:000672764400008,
Author = {Jordao, Eduardo Ferreira and Bogossian, Andre Martins},
Title = {Three objections to paternalistic regulation},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2021},
Volume = {122},
Pages = {241-267},
Month = {JAN-JUN},
Abstract = {The article analyses the paternalistic arguments for state regulation,
   considering that paternalistic justifications are one of the main
   reasons to regulate. It is noted that paternalistic regulation brings
   problems of three kinds: epistemic, institutional, and referring to the
   very linguistic framework used by law. More specifically, we argue that,
   when such justifications are used, there are (i) cognitive and
   situational limitations to the regulator; (ii) valid questions regarding
   the real motivations of regulatory interventions based on paternalism;
   and (iii) accentuation of the problem of ``incongruence{''} between the
   effects of the application of norms and the (first order) reasons that
   justify them.},
