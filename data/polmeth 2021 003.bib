@article{ WOS:000683481400005,
Author = {Gotz, Roland},
Title = {Russia's pandemic policy Measures and results},
Journal = {OSTEUROPA},
Year = {2021},
Volume = {71},
Number = {3},
Pages = {51+},
Abstract = {In Russia, stricter measures were taken during the first half of 2020 to
   stem the spread of the coronavirus than in many other countries.
   However, in the second half of 2020 and in the spring of 2021, they were
   largely rescinded. Even so, the pandemic policy appeared to have been a
   success: the state budget was not badly affected, and domestic
   production declined to a lesser extent than in other countries, since
   business activity was not significantly restricted. The country boasts
   of its favourable position in the global ranking list of infections and
   deaths reported to the WHO. However, this official data does not reflect
   reality. The death rate, which increased considerably in 2020 compared
   to 2019, indicates that the real number of Covid-19 deaths and
   infections is six times higher in 2020, and three times higher during
   the first quarter of 2021.},
