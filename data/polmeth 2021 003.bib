@article{ WOS:000657983900001,
Author = {Hack, Jonathan S. and Jenkins, Clinton M.},
Title = {The Attorneys' Gender: Exploring Counsel Success before the US Supreme
   Court},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2022},
Volume = {75},
Number = {3},
Pages = {632-645},
Month = {SEP},
Abstract = {Stereotypes are powerful heuristics structuring decision-making, with
   research suggesting that gender-based stereotypes place women at a
   professional disadvantage. This paper tests whether attorneys' gender
   influences Supreme Court outcomes. We construct an attorney-focused data
   set combining personal and professional attributes with case-level
   characteristics from 1946 to 2016. Our approach brings clarity to
   previous findings, enabling a longitudinal analysis of women
   participation before the Court. We find that attorney gender does not
   influence party success. In doing so, we show that a more nuanced
   approach is needed when studying the intersection between judicial
   outcomes and attorney traits.},
