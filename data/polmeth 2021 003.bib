@article{ WOS:000661891400005,
Author = {Reyes Herrera, Milton and Perez Enriquez, Diego and Barreiro Santana,
   Katalina},
Title = {Ecuador's Repositioning: Intrastate Competition and the 2017-2019
   Context},
Journal = {DESAFIOS},
Year = {2021},
