@article{ WOS:000638132100001,
Author = {Ahmad, Nyarwi},
Title = {What Drive Marketization and Professionalization of Campaigning of
   Political Parties in the Emerging Democracy? Evidence from Indonesia in
   the Post-Soeharto New Order},
Journal = {JOURNAL OF POLITICAL MARKETING},
