@article{ WOS:000680965300001,
Author = {Enders, Adam M.},
Title = {Issues versus Affect: How Do Elite and Mass Polarization Compare?},
Journal = {JOURNAL OF POLITICS},
Year = {2021},
Volume = {83},
Number = {4},
Pages = {1872-1877},
Month = {OCT},
Abstract = {Conventional wisdom holds that political elites harbor more
   ideologically constrained issue attitudes and are more polarized on such
   issues than their mass counterparts. In contrast, the mass public is
   more polarized in its emotional reactions to political stimuli. How
   affective polarization among the mass public compares to affective
   polarization among elites is, however, an open question. Using items
   common to the Convention Delegate Studies and American National Election
   Studies, I compare the mass public to political elites when it comes to
   both affective and ideological polarization. I find that elites are more
   affectively polarized than the mass public and more affectively
