@article{ WOS:000756949500001,
Author = {Havercroft, Jonathan},
Title = {The British Academy Brian Barry Prize Essay Why Is There No Just Riot
   Theory?},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2021},
Volume = {51},
Number = {3},
Pages = {909-923},
Month = {JUL},
Abstract = {Given the prevalence of riots throughout human history, the lack of
   normative theorizing about them compared to other forms of political
   violence is striking. The author hypothesizes that this is due to riots'
   extra-institutionality. Riots are extra-public because they involve the
   participation of crowds, rather than institutionalized groups such as
   parties or social movements. They are extra-state because they violate
   the state's monopoly on violence. Riots are extra-legal because they
   constitute a form of unlawful assembly. They are also
   extra-parliamentary because they operate outside the normal legislative
   process. This article considers justifiable reasons to resist each of
   these foundational institutions, and proposes provisional criteria for a
   justifiable riot. The author concludes by urging political theorists to
   further examine the normative dimension of riots.},
