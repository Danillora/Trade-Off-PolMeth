@article{ WOS:000727071900001,
Author = {Miguelez, Fausto and Alos, Ramon and Molina, Oscar},
Title = {Job regulation in the digital revolution?},
Journal = {POLITICA Y SOCIEDAD},
Year = {2021},
Volume = {58},
Number = {3},
Abstract = {Spain is in the midst of a digital revolution. The quantitative effects
   of the digital revolution on employment remain highly uncertain,
   although we know that it will be important in the elimination of some
   ones, the creation of others and the transformation of many more. Based
   on the opinions of the protagonists in the current changes (experts,
   employers and trade unionists) and on the specialized literature, we
   have reviewed the main challenges that may affect Spain until 2025.
   Taking into account the country's weaknesses and strengths with regard
   to digitalization, we have assessed possible policies aimed to improve
   the quality and quantity of employment. To give more consistency to our
   analysis, we have studied the regulatory experiences of three European
   countries, Germany, France and United Kingdom, in the case of platform
   economies.},
