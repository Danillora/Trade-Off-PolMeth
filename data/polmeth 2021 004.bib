@article{ WOS:000657198000001,
Author = {Rollandi, Isabel},
Title = {Modern Freedom and the Loss of Nature. The Quarrel between the Ancients
   and Moderns According to Leo Strauss},
Journal = {ANACRONISMO E IRRUPCION},
