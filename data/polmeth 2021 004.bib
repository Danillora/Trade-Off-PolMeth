@article{ WOS:000647371900001,
Author = {Rubabshi-Shitrit, Ayelet and Hasson, Sharon},
Title = {The effect of the constructive vote of no-confidence on government
   termination and government durability},
Journal = {WEST EUROPEAN POLITICS},
Year = {2022},
Volume = {45},
Number = {3, SI},
Pages = {576-590},
Month = {APR 16},
Abstract = {Governments in countries with the constructive vote of no-confidence
   (CVNC) tend to be more durable than governments in countries with a
   regular vote of no-confidence (RVNC). Although this is the common claim
   in the limited literature that exists about the CVNC, no comprehensive
   study and no empirical tests have been conducted to date. We begin to
   fill this lacuna. Using a two-stage analysis including a Multivariate
   Cox proportional Hazard Model, we find that the type of no-confidence
   vote affects the government's term of office, so that in countries with
   a CVNC, governments in general, and post-election governments in
   particular, last longer in office than governments in RVNC countries.},
