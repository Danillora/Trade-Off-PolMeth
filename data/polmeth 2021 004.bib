@article{ WOS:000658384500003,
Author = {Bosworth, William},
Title = {Power obsessed},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2021},
Volume = {14},
Number = {2},
Pages = {288-300},
Abstract = {Governance is best analysed using rational choice theory to identify the
   extent to which different groups can systematically punish and reward
   others. This is roughly in line with Dowding's important book Rational
   Choice and Political Power. It is different though in the sense that it
   does not focus on the measurement of social power, but rather a subset
   of it. Focusing on the latter avoids difficulties relating to systematic
   luck, such as distinguishing it from legitimacy and the possession of
   information, and also clarifies the importance of coordination problems
   for the analysis.},
