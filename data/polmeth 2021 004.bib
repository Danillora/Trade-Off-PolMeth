   Holliday, Wesley H., Univ Calif Berkeley, Berkeley, CA 94720 USA.
   Pacuit, Eric, Univ Maryland, College Pk, MD 20742 USA.},
DOI = {10.1177/09516298211043236},
ISSN = {0951-6298},
EISSN = {1460-3667},
Keywords = {Arrow's impossibility theorem; independence of irrelevant alternatives;
   majority cycle; spoiler effect; voting theory},
Keywords-Plus = {IRRELEVANT ALTERNATIVES; ARROWS THEOREM; SOCIAL CHOICE; INDEPENDENCE;
