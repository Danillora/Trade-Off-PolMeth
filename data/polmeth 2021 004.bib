@article{ WOS:000712672000001,
Author = {Betancourt, Diana Sanchez and Vivier, Elme},
Title = {The Relevance of Contemplative Studies and Practices for Improving
   Participatory Urban Governance},
Journal = {POLITIKON},
Year = {2021},
Volume = {48},
Number = {4},
Pages = {607-624},
Month = {OCT 2},
Abstract = {Evidence suggests that substantial and robust community engagement is
   essential to local urban governance. In this paper, we reflect on the
   importance of a more comprehensive approach to community engagement that
   values both the cognitive and the affective, the technical and the
   relational, and the outer and inner dimensions of human experiences. The
   intangible and inner dimensions receive little attention in public
   administration and political science fields, or in governance processes
   themselves. The paper aims to make a start in addressing this gap by
   engaging ideas from the emerging field of contemplative studies. This
   field brings attention to the inner worlds of people, and the importance
   of valuing affective and embodied experiences. We explore how community
   engagement processes could potentially embody these insights to
   strengthen participatory urban governance, and reflect on some of the
   risks and limitations.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
