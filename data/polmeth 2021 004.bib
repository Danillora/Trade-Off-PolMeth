@article{ WOS:000716549500006,
Author = {Canesso, Annagiulia},
Title = {Politics of Psychoanalytic Knowledge: Canguilhem's Reading of Freud},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
Year = {2021},
Volume = {33},
Number = {64},
Pages = {91-110},
Abstract = {Georges Canguilhem has never systematically discussed Sigmund Freud's
   thinking. This essay aims to reconstruct Canguilhem's reading of
   Freudian psychoanalysis, highlighting its strategic use within the
   framework of Canguilhem's controversy against psychology. From this
   point of view, knowledge is established as a field of political
   struggle: unlike psychology and its deterministic and normalizing
   drifts, psychoanalysis allows Canguilhem to affirm the irreducibility of
   thought to brain localizations, to emphasize its erratic power and to
   think the constitution of an eccentric subjectivity.},
