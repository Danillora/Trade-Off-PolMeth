@article{ WOS:000707691000001,
Author = {Kaul, Nitasha and Kannangara, Nisar},
Title = {The Persistence of Political Power: A Communist `Party Village' in
   Kerala and the Paradox of Egalitarian Hierarchies},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
