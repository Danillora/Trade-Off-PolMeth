@article{ WOS:000649400800001,
Author = {Rosen, Jonathan D.},
Title = {Understanding support for tough-on-crime policies in Latin America: The
   cases of Mexico, El Salvador, and Honduras},
Journal = {LATIN AMERICAN POLICY},
Year = {2021},
Volume = {12},
Number = {1},
Pages = {116-131},
Month = {MAY},
Abstract = {This article examines tough-on-crime strategies in El Salvador,
   Honduras, and Mexico. These three countries have been plagued by gangs
   and organized crime-related violence. Scholars have written about the
   consequences of iron fist strategies, which have resulted in increasing
   levels of violence. This article seeks to fill a gap in the scholarly
   literature by examining what factors influence perceptions about
   tough-on-crime strategies, using survey data in the cases of Mexico, El
   Salvador, and Honduras. The article finds that fear is a key factor that
   influences support for mano dura policies.},
