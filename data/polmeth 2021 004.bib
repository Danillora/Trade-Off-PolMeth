@article{ WOS:000634495600009,
Author = {Gaman-Golutvina, V, O. and Nikitin, I, A. and Arteev, S. P.},
Title = {INTERFACE BETWEEN POLITICAL SCIENCE AND POLITICAL DISCOURSE: CONFLICT
   DIMENSION},
