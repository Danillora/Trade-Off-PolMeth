@article{ WOS:000827234000004,
Author = {Saad-Diniz, Eduardo and Gianecchini, Joao Victor},
Title = {REGULATORY ROLLBACKS IN THE AMAZON RAINFOREST: A NUANCED LOOK INTO THE
   EFFECTS OF ENVIRONMENTAL VICTIMIZATION},
Journal = {STATE CRIME},
Year = {2021},
Volume = {10},
Number = {2},
Pages = {257-283},
Abstract = {Since the beginning of lair Bolsonaro's presidential mandate, existing
   Brazilian environmental regulations started being rolled back to benefit
   a small, but powerful, Brazilian bourgeoise, the so-called ``Brazilian
   agribusiness.{''} The process of deregulation in Brazil (regulatory
   rollback) is responsible for significant environmental and social harm,
   which affects largely marginalized peoples and indigenous communities in
   the Brazilian Amazon rainforest. It is also responsible for operating,
   and maintaining, dynamics of social inclusion and exclusion which are
   deepening the democratic deficit in Brazil, which is here denominated as
   the ``mechanisms of environmental victimization.{''} To demonstrate the
   Brazilian regulatory rollback the authors analyse legislative and
   regulatory amendments since the beginning of lair Bolsonaro's mandate.
   Through this analysis, the authors seek to demonstrate how the symbiosis
   between state and corporate interests can promote significant social and
   environmental victimization.},
Publisher = {PLUTO JOURNALS},
Address = {C/O MARSTON BOOK SERVICES LTD, PO BOX 269 ABINGDON, OXFORDSHIRE, OX14
   4YN, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Saad-Diniz, E (Corresponding Author), Univ Sao Paulo, Ribeirao Preto Law Sch, Criminol \& Criminal Law, Ribeirao Preto, Brazil.
