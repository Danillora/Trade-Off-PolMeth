@article{ WOS:000668847800001,
Author = {Miller, David Ryan},
Title = {Empirical approaches to the study of access},
Journal = {INTEREST GROUPS \& ADVOCACY},
Year = {2021},
Volume = {10},
Number = {3},
Pages = {286-302},
Month = {SEP},
Abstract = {While organized interests employ a broad range of activities in pursuit
   of their goals, practitioners and scholars alike deem access, or direct
   contacts with policy-makers, as the ``gold standard{''} of activities.
   However, this type of access is difficult for empirical researchers to
   study because scant records of direct contacts exist. In this essay, I
   discuss the role of access in studies of organized interests and
   policy-making and describe three common approaches to the empirical
   study of access: official records of access, when they exist; survey
   self-reports by organized interests and policymakers; and experiments. I
   identify the strengths and limitations of each approach and provide
   guidance and recommendations for empirical researchers using these
   approaches to study access.},
