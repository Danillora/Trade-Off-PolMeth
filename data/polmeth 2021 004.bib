@article{ WOS:000766714700016,
Author = {Deves, Eduardo},
Title = {``Intellectual ecosystem{''} as a unit of analysis: places where the
   ideas are self-developed},
Journal = {IZQUIERDAS},
Year = {2021},
Volume = {50},
Month = {APR},
Abstract = {This work aims to show how to investigate the intellectual work of a set
   of intelligences located in a territory. First, the notion of
   ``intellectual ecosystem{''} is presented. This concept seeks to name
   the set of intellectual organisms that interact in a space, as well as
   to explain its possibilities in relation to other nearby notions, in
   particular the notion of ``intellectual field{''}. It gives arguments
   about the possibilities of this notion to study ideas and intellectual
   work within the place. Second, working methods and procedures are
   proposed, considering matters such as classification of intellectual
   ecosystems, specific procedures for classes, and ways of processing
   information. Third, one case is synthetically addressed: La Paz-El Alto,
   Bolivia, around the year 2000. Finally, conclusions that articulate
   ``intellectual ecosystem{''} with other notions are offered,
   simultaneously asking several questions for future research.},
