@article{ WOS:000710841100001,
Author = {Kaminski, Pawel and Riedel, Rafal},
Title = {When illiberalism looms-mortality anxiety and the perceived existential
   threat among Polish and Slovenian interest groups},
Journal = {INTEREST GROUPS \& ADVOCACY},
Year = {2021},
Volume = {10},
Number = {4, SI},
Pages = {454-477},
Month = {DEC},
Abstract = {In this paper, we assess the consequences of illiberal tendencies and
   democratic backsliding for interest groups in post-communist Europe.
   Based on empirical material, we explore to what extent the (party) state
   is attempting to transform civil society through financial means such as
   pooling financial resources for interest groups into central state
   funds, funneling finances to specific loyal or target groups, or
   redefining the parameters for fundraising activities. Our starting
   assumption is that the interest groups from the two representative
   countries will differ in their responses to various types of illiberal
   tendencies (stronger democratic backsliding in Poland and less
   democratic erosion in Slovenia). We first expect groups in Slovenia to
   be less likely than Polish organizations to report existential threats
   related to the negative implications of democratic backsliding.
   Furthermore, we hypothesize that organizations with better contacts and
   ideological proximity with the governing party are less likely expected
   to report existential threats. Following the recognized literature of
   the field, we also test the Europeanization and internationalization
   factor by hypothesizing that organizations that are members of
   international associations as well as those that receive funds from the
   European Union are more immune to (perceived) existential threats.},
