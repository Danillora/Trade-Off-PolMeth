@article{ WOS:000820485400010,
Author = {Carvalho, Leticia and Braga Rosas Duarte, Geraldine Marcelle Moreira and
   Nogueira Jones, Maria Eugenia},
Title = {From containment to resolution? MINUSCA and the peace process in the
   Central African Republic},
Journal = {REVISTA DE PAZ Y CONFLICTOS},
Year = {2021},
Volume = {14},
Number = {2},
Pages = {204-231},
Month = {JUL-DEC},
Abstract = {The article aims to investigate the role played by the United Nations
   (UN) in conflict resolution in the Central African Republic (CAR).
   Specifically, we will seek to answer the following question: how has the
   stabilization character of MINUSCA contributed to the peace process in
   the CAR? Our hypothesis is that the stabilization character of the
   operation and the robust use of force have contributed to the success of
   the peace process in terms of containing conflict, but not exactly in
