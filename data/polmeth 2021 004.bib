@article{ WOS:000707080500002,
Author = {Berardi Spairani, Adrian Pablo},
Title = {Social Militancy in Jujuy (1990-2015): A Study of Militant Trajectories},
Journal = {TEMAS Y DEBATES},
Year = {2021},
Number = {41},
Pages = {37-57},
Month = {JUN},
Abstract = {This article aims to analyze trajectories of social activists in the
   province of Jujuy, Argentina, investigating the way in which the
   subjects establish their commitment, the conditions that determine their
   militant registration and their political work. In this sense, the
   history of two militants of social organizations that began military in
   the period (1990-2015) will be analyzed through a microstructural study,
   recovering the career perspective proposed by Howard Becker.
   To achieve the established objectives, a qualitative methodology was
   developed using the life story technique, which allowed reconstructing
   the trajectories, recovering the look that the militants have on their
   own activity.},
