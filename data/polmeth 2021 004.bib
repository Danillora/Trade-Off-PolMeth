@article{ WOS:000717895900021,
Author = {Gonzalez, Francisco Javier Alarcon},
Title = {YOUNG PEOPLE'S POLITICAL PARTICIPATION: NEW AND OLD FORMS IN
   CONTEMPORARY SPAIN},
