@article{ WOS:000673974800009,
Author = {Koenig, Pascal D. and Waldvogel, Thomas},
Title = {Ni gauche ni droite? Positioning the candidates in the 2017 French
   presidential election},
Journal = {FRENCH POLITICS},
Year = {2021},
Volume = {19},
Number = {2-3, SI},
Pages = {281-303},
Month = {SEP},
Abstract = {The 2017 French presidential election is regarded as a watershed in
   French politics, with Emmanuel Macron winning the election as the leader
   of a newly founded political movement. This paper probes the idea that
   the French political ideological space has lost its bipolar structure
   and that the election forms a continuation of a decreasing political
   polarization blurring the left-right divide, a trend posited to have
   occurred since the 2000s. It uses an inductive scaling approach drawing
   on information about candidate policy stances from the Voting Advice
   Application Boussole Presidentielle to extract candidates' positions and
   to explore which issue domains structure their inter-relations. The
   resulting candidate policy space does not suggest a dissolution of
   ideological differences between left and right. Yet, while Macron and Le
   Pen do not transcend a polarized left-right divide, they do mark
   deviations from a clear-cut bipolar left-right block structure and
   increase political complexity.},
