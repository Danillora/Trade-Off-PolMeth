@article{ WOS:000756949500013,
Author = {Newman, Benjamin and Merolla, Jennifer L. and Shah, Sono and Lemi,
   Danielle Casarez and Collingwood, Loren and Ramakrishnan, S. Karthick},
Title = {The Trump Effect: An Experimental Investigation of the Emboldening
   Effect of Racially Inflammatory Elite Communication},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2021},
Volume = {51},
Number = {3},
Pages = {1138-1159},
Month = {JUL},
Abstract = {This article explores the effect of explicitly racial and inflammatory
   speech by political elites on mass citizens in a societal context where
   equality norms are widespread and generally heeded yet a subset of
   citizens nonetheless possesses deeply ingrained racial prejudices. The
   authors argue that such speech should have an `emboldening effect' among
   the prejudiced, particularly where it is not clearly and strongly
   condemned by other elite political actors. To test this argument, the
   study focuses on the case of the Trump campaign for president in the
   United States, and utilizes a survey experiment embedded within an
   online panel study. The results demonstrate that in the absence of
   prejudiced elite speech, prejudiced citizens constrain the expression of
   their prejudice. However, in the presence of prejudiced elite speech -
   particularly when it is tacitly condoned by other elites - the study
   finds that the prejudiced are emboldened to both express and act upon
   their prejudices.},
