@article{ WOS:000866238800007,
Author = {Muchanyuka, Muneyi Rewayi},
Title = {The Zimbabwean Response to RENAMO Incursions: A Conflict Transformation
   Perspective},
Journal = {STRATEGIC REVIEW FOR SOUTHERN AFRICA},
