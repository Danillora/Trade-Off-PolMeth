@article{ WOS:000629765700001,
Author = {Vranceanu, Alina and Lachat, Romain},
Title = {De parties influence public opinion on immigration? Evidence from Europe},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2021},
Volume = {31},
