@article{ WOS:000661936200013,
Author = {Botanova, Kateryna},
Title = {A place of absence Serhiy Bukovsky's Film Spell Your Name},
Journal = {OSTEUROPA},
Year = {2021},
Volume = {71},
Number = {1-2},
Pages = {185+},
Abstract = {Babi Yar is regarded as being of great importance in the history of
   Ukraine, yet for a long time, the massacre was hardly anchored at all in
   the collective memory of the country. One milestone in the way in which
   events were viewed was the film Spell Your Name, which is based on
   interviews from the archives held by Steven Spielberg's Holocaust
   Foundation. In his film, Serhiy Bukovsky reflects on the distance that
   separates us from historical events. At the heart of the film are
   memory, forgetting and repression. Spell Your Name circles around Babi
   Yar as a blank spot in the Ukrainian national memory. Here, the focus is
   not on reconstructing the past, but on how history is treated in the
   present day.},
