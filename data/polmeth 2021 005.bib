@article{ WOS:000736025500001,
Author = {Allen, Tim and Atingo, Jackline and Parker, Melissa},
Title = {Rejection and Resilience: Returning from the Lord's Resistance Army in
   Northern Uganda},
Journal = {CIVIL WARS},
Year = {2022},
Volume = {24},
Number = {2-3, SI},
Pages = {357-384},
Month = {JUL 3},
Abstract = {This article focuses on young people who returned from the Lord's
   Resistance Army in northern Uganda, mostly as children, over ten years
   ago. They are, by definition, resilient, because they have survived, but
   there are important variations. For the most part, those who managed to
   gain status in the LRA, are those most likely to present themselves as
   `resilient' according to notions of self-reliance and
   entrepreneurialism. The majority are not in this position. They are
   mainly living in rural locations, and commonly face social rejection and
   extreme poverty. Supporting normative models of resilience has
   exacerbated deprivation of the most vulnerable.},
