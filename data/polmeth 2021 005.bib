@article{ WOS:000736395500001,
Author = {Omeje, Kenneth},
Title = {Reflections on How the Nigerian Government Has Managed the Covid-19
   Crisis},
Journal = {AFRICAN SECURITY},
Year = {2021},
Volume = {14},
Number = {4, SI},
Pages = {484-505},
Month = {OCT 2},
Abstract = {This paper evaluates the efforts of government in fighting the spread of
   Covid-19 in Nigeria. The paper examines the government's rapid response
   measures to contain the virus within the initial months of its
   manifestation in Nigeria, as well as the various steps taken by the
   government to combat the spread of the disease, and consequently redress
   its impact on the economy. The paper argues that the greatest challenge
   that Nigeria has faced in managing the Covid-19 crisis is the weak
   capacity of its health sector, which is partly a reflection of the wider
   problem of poor institutional and governance capacity of the state. A
   durable revamp and fixing of the health sector, the paper argues, will
   require a comprehensive and proactive governance approach that will
   simultaneously address the overlapping challenges of economic,
   political, and social sector securities for the Nigerian populace.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
