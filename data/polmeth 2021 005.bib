@article{ WOS:000792281800001,
Author = {Marquardt, Kyle L. and Pemstein, Daniel},
Title = {Estimating latent traits from expert surveys: an analysis of sensitivity
   to data-generating process},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2023},
Volume = {11},
Number = {2},
Pages = {384-393},
Month = {APR},
Abstract = {Models for converting expert-coded data to estimates of latent concepts
   assume different data-generating processes (DGPs). In this paper, we
   simulate ecologically valid data according to different assumptions, and
   examine the degree to which common methods for aggregating expert-coded
   data (1) recover true values and (2) construct appropriate coverage
   intervals. We find that the mean and both hierarchical Aldrich-McKelvey
   (A-M) scaling and hierarchical item-response theory (IRT) models perform
   similarly when expert error is low; the hierarchical latent variable
   models (A-M and IRT) outperform the mean when expert error is high.
   Hierarchical A-M and IRT models generally perform similarly, although
   IRT models are often more likely to include true values within their
   coverage intervals. The median and non-hierarchical latent variable
   models perform poorly under most assumed DGPs.},
