@article{ WOS:000627320500005,
Author = {Karvatska, Svitlana and Blikhar, Mariia and Huralenko, Nataliia},
Title = {Evolutionary trends in the interpretation of the European Court of Human
   Rights under the European Convention on Human Rights},
