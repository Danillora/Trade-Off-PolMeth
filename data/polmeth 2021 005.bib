@article{ WOS:000673235400001,
Author = {Cohen, Alex and Alden, John and Ring, Jonathan},
Title = {Using a ``Gateway Game{''} to Stimulate Student Interest and Build
   Foundational Knowledge},
Journal = {JOURNAL OF POLITICAL SCIENCE EDUCATION},
Year = {2021},
Volume = {17},
Number = {1, SI},
Pages = {104-115},
Month = {DEC 22},
Abstract = {Active learning-and gaming, in particular-is now a well-established part
   of many political science courses. First, we discuss the design and
   implementation of a ``Gateway Game{''}, a pedagogical tool with broad
   applicability and test its effectiveness in increasing student
   motivation, satisfaction, and learning. Crucially, we provide
   instructors with all the tools necessary to run the game in their
   classroom to experience this for themselves. Second, using surveys of
   classroom participants, we show that the game increases student
   satisfaction, motivation, and learning, suggesting that this game
   comprises an exciting entry point for learning in many environments.
   Third, we describe how this game can be integrated into a variety of
   courses and subfields.},
