@article{ WOS:000730381600001,
Author = {Deysine, Anne},
Title = {RUTH BADER GINSBURG OR RESISTANCE BY DISSENTING OPINION},
Journal = {TOCQUEVILLE REVIEW},
Year = {2021},
Volume = {42},
Number = {2},
Pages = {5-25},
Month = {DEC 1},
Abstract = {This article examines Ruth Bader Ginsburg's (RBG) role as a minority
   justice who sought to curb the Supreme Court's rightward drift by
   writing dissenting opinions that she sometimes read aloud to the media,
   the public, and her colleagues whose reasoning, motivation, and decision
   she criticized. She initially focused on issues of discrimination and
   equal pay; thus, in the Ledbetter case her dissent pushed Congress to
   legislate against the Court's restrictive interpretation. It then
   broadened its focus by denouncing the privileged treatment of religious
   freedom over anti-discrimination laws. On voting rights, RBG has opposed
   the conservative majority who routinely validate discriminatory voting
   changes passed by Republican state majorities. And these decisions are
   increasingly made surreptitiously, under the shadow docket, and thus fly
   under the radar. This makes the situation worse, and makes it all the
   more necessary to reform the Supreme Court or reduce its powers.},
