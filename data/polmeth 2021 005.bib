@article{ WOS:000650227400001,
Author = {Hayton, Richard},
Title = {Conservative Party Statecraft and the Johnson Government},
Journal = {POLITICAL QUARTERLY},
Year = {2021},
Volume = {92},
Number = {3},
Pages = {412-419},
Month = {JUL},
Abstract = {This article uses the statecraft framework outlined by Andrew Gamble to
   analyse the Conservative government of Boris Johnson. It argues that
   under Johnson, the Conservative Party is pursuing its most far-reaching
   statecraft strategy since the Thatcher era, aimed at re-establishing the
   conditions for Conservative hegemony. At the heart of Johnson's
   statecraft lies Brexit itself, which serves as a national cause around
