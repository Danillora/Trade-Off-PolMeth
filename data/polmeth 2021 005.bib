@article{ WOS:000672090200001,
Author = {Dafermos, Manolis},
Title = {Rethinking the relationship between Marx's Capital and Hegel's
   Science of Logic: The tradition of creative Soviet Marxism},
Journal = {CAPITAL AND CLASS},
Year = {2022},
Volume = {46},
Number = {1},
Pages = {77-93},
Month = {MAR},
Abstract = {This article sheds light on the little known and poorly understood
   extensive discussion on the relationship between Marx's Capital and
   Hegel's Science of Logic in the tradition of creative Soviet Marxism.
   The exploration of the mechanism of ascending from the abstract to the
   concrete and its relation to the movement of thought from the concrete
   to the abstract was one of the key points of this discussion. The
   ascending from the abstract to the concrete is a crucial issue of the
   dialectical logic developed in German Classical Philosophy, especially
   in Hegel's Science of Logic. Marx implemented the method of ascent from
   the abstract to the concrete to investigate a historically concrete
   object (the capitalist mode of production) as an organic whole.},
