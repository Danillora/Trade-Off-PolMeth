@article{ WOS:000629669300001,
Author = {Fischer, Sophie-Charlotte and Wenger, Andreas},
Title = {Artificial Intelligence, Forward-Looking Governance and the Future of
   Security},
Journal = {SWISS POLITICAL SCIENCE REVIEW},
Year = {2021},
Volume = {27},
