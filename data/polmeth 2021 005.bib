@article{ WOS:000727962200004,
Author = {Wang, Maya},
Title = {TURKEY NEEDS TO RESPOND TO CHINA'S CRIMES AGAINST HUMANITY},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2021},
Volume = {20},
Number = {1},
Pages = {53-56},
Month = {SPR},
Abstract = {The Chinese government's oppression of Uyghurs and other Turkic Muslim
   peoples has reached unprecedented levels since late 2016. Human Rights
   Watch research demonstrates that the Chinese government has committed,
   and continues to commit, crimes against humanity, including mass
   arbitrary detention and pervasive restrictions on religious freedoms.
   The response or lack, thereof-of Muslim majority countries and the
   Organisation of Islamic Cooperation (OIC) to these crimes has been
   particularly disappointing. The Turkish government, for example, has not
   taken the lead many had hoped it would in advocating for the rights of
   Uyghurs and other Turkic peoples. But it is not too late. Turkey still
   has the chance to implement concrete policies to hold the Chinese
   government accountable for its human rights abuses against Uyghurs and
   other Turkic peoples in Xinjiang.},
