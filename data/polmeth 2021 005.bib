@article{ WOS:000864759400013,
Author = {Bartoletti, Ivana},
Title = {HOW REFRAMING PRIVACY WOULD UPHOLD DEMOCRACY IN THE DIGITAL AGE},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2021},
Volume = {20},
Number = {4},
Pages = {129-136},
Month = {WIN},
Abstract = {AI and algorithmic decision making are everywhere, and increasingly
   determine what happens to us, as well as editing the news and adverts we
   see, acting as our gateway to reality. The world is grappling with these
   consequences of AI. Many countries, businesses and organizations are
   equipping themselves with policies, guidelines and strategies to harness
   the benefits of this technology while tempering the risks. I argue that
   privacy can continue to make an effective and substantial contribution
   to upholding human rights and democracy in our datafied societies but
   this can only happen if we reframe privacy as a public concept, making
   privacy law more dynamic on the way.},
