@article{ WOS:000706614500001,
Author = {Benassai, Paola},
Title = {Proletarian images. Revista de Oriente (1925-1926) and the
   construction of a workerist visual culture in Argentina},
Journal = {IZQUIERDAS},
Year = {2021},
Volume = {50},
Month = {APR},
Abstract = {The Russian Revolution was an event of worldwide resonance. Under this
   impact, from Latin America different intellectual and political
   organizations developed strategies to support the bolshevik regime and
   spread the new soviet reality. This work is based on the Revista de
   Oriente (1925-1926), a monthly Argentine publication from the mid-1920s
   that was edited by the Asociacion Amigos de Rusia. From the perspective
   of visual culture, the article aims to investigate the aesthetic facet
   of the magazine and understand to what extent its visuality sought to
   generate empathy for the revolution in its readers' universe.},
