@article{ WOS:000701801800004,
Author = {Krnic, Raseljka and Adamovic, Mirjana and Radacic, Ivana},
Title = {ATTITUDES OF EXPERTS ON THE POLICY OF PROSTITUTION REGULATION IN CROATIA},
