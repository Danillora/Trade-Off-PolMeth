@article{ WOS:000789925500012,
Author = {Korhonen, Iikka},
Title = {Sanctions and Counter-Sanctions The Economic Impact of Decoupling
   Policies},
Journal = {OSTEUROPA},
Year = {2021},
Volume = {71},
Number = {10-12},
Pages = {211+},
Abstract = {The sanctions imposed on Russia have an effect on the country's foreign
   trade and on the inflow of direct investments. This has been proven in
   many studies. The exact extent is in dispute. What is certain is that
   fluctuations in the price of oil have a far greater influence on
   Russia's economy. Russia's response has been import substitution. But
   counter-sanctions in the agricultural sector have also led to a certain
   loss of prosperity. In the high-tech sector, it is becoming apparent
   that Russia today is having greater difficulties in keeping up with
   permanent development. However, a change of policy in Moscow is
   unlikely.},
