@article{ WOS:000774639200001,
Author = {Edgell, Amanda B. and Boese, Vanessa A. and Maerz, Seraphine F. and
   Lindenfors, Patrik and Lindberg, I, Staffan},
Title = {The Institutional Order of Liberalization},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2022},
Volume = {52},
Number = {3},
Pages = {1465-1471},
Month = {JUL},
Abstract = {When authoritarian regimes liberalize, are there observable patterns in
   the ordering of reforms, and are these patterns distinct for cases that
   transition to democracy? While the prevailing literature tends to focus
   on exogenous `determinants' of democracy, this letter describes the
   endogenous dynamics of liberalization itself. Using pairwise domination
   analysis, it assesses the institutional order of reforms during 371
   episodes of liberalization in autocracies between 1900 and 2019. Based
   on twenty-four indicators of democratic institutions and practices, our
   findings reveal (1) a clear pattern of reform during liberalization
   episodes, (2) with strong similarities across outcomes, but also that
   (3) reforms to the administration of elections tend to develop
   comparatively earlier in episodes of liberalization that produce a
   democratic transition.},
