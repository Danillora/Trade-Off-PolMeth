@article{ WOS:000631494000001,
Author = {Wang, Xiaoguang and Liu, Qian},
Title = {The Evolution of the Sino-Russian Oil and Gas Partnership: From
   Structure-Centred to Actor-Centred Engagement},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Year = {2022},
Volume = {7},
Number = {4, SI},
Pages = {503-523},
Month = {DEC},
Abstract = {This article examines the high-profile Sino-Russian oil and gas
   partnership, perhaps the most significant economic cooperation in
   Sino-Russian bilateral relations, which has lasted for almost 3 decades,
   with definite phases of development and equally of stagnation. More
   importantly, the energy partnership is employed as an empirical case
   study to contribute to one of the most critical debates in international
   relations (i.e. between the structural and the actor-specific analytical
   perspectives). This study found that the partnership's development
   embodies an evolution that originated with political engagement, through
   bargaining and adaptation, and finally reached a business-minded
   development stage. The structural perspective provides a convincing
   explanation of the rationality of the initial approaches between Beijing
   and Moscow, while the actor-specific perspective is better for
   understanding the developments that followed thereafter, with the
   evolution of the countries' cooperation.},
