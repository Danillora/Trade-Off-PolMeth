@article{ WOS:000695409400001,
Author = {Zieba, Ryszard},
Title = {France-Poland security relations},
Journal = {FRENCH POLITICS},
Year = {2022},
Volume = {20},
Number = {1},
Pages = {71-94},
Month = {MAR},
Abstract = {France and Poland have had dynamic security relations for over 100
   years. Initially, the bonding element was the sense of threat posed by
   Germany. Therefore, in the interwar period, France and Poland were bound
   by the allied treaty of 1921. However, both countries lost their
   independence during WWII, and the alliance was not renewed after the
   war. During the Cold War division, convergence and divergence of
   security interests were evident. After the breakthrough of 1989, Poland
   joined NATO and the EU and, together with France, found itself in the
   Western security community. At that time, France represents a
   Eurocentric vision of security with elements of mondialisme, and Poland
   prefers an Atlantic option. These convergences and differences in
   understanding their own security interests mean that relations between
   France and Poland are meandering, changeable and even complicated, which
   can be seen in their changing dynamics throughout the post-Cold War
   period.},
