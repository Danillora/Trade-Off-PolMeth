@article{ WOS:000675413100001,
Author = {Rogers, Cara J.},
Title = {The French Experiment: Thomas Jefferson and William Short Debate
   Slavery, 1785-1826},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2021},
Volume = {10},
Number = {3},
Pages = {327-362},
Month = {JUN 1},
Abstract = {In 1785, Thomas Jefferson printed his plan for ending slavery in
   Virginia: all slaves born after a certain date should be free, and the
   state should educate the freed children, colonize them to some
   hospitable location, and then support the new nation until it was strong
   enough for independence. For a Virginian like Jefferson, emancipation
   seemed most possible when imagined in tandem with racial separation.
   However, while living in Europe during the late 1780s, Jefferson and his
   young secretary William Short began devising an alternative, much more
   inclusive plan to end slavery: import German laborers, mingle them with
   African-descended workers, and use European tenant farming-or
   metayage-to transition slaves to freedom. Although neither of
   Jefferson's plans came to fruition, examining his and Short's
   correspondence from the 1780s through the 1820s offers new insights into
   Jefferson's thought on immigration, race, and the possibility of a
   multiracial republic.},
