@article{ WOS:000697384700008,
Author = {Marlow-Stevens, Samuel and Hayton, Richard},
Title = {A Rhetorical Political Analysis of Theresa May's Statecraft on Brexit},
Journal = {PARLIAMENTARY AFFAIRS},
