@article{ WOS:001054560500003,
Author = {de Perini, Pietro},
Title = {Italy and international human rights: facing role contestation},
Journal = {CONTEMPORARY ITALIAN POLITICS},
Year = {2021},
Volume = {13},
Number = {3},
Pages = {314-330},
Month = {JUL 3},
Abstract = {Did the 2018-2019 Conte I Cabinet's approach to human rights also mirror
   a deeper change in the established national conception of the role the
   country should play in the international human rights regime? Applying
   Role Theory to the analysis of Italian decision makers' positions on the
   country's foreign policy vis-a-vis human rights norms and institutions
   during this period, the article reveals the existence of a phase of
   `role contestation' among Italian leaders. A new isolationist,
   uncooperative and delegitimising conception of both multilateralism and
   its human rights priorities clashed with a commitment to preserve the
   traditional conception of the country as a responsible multilateralist
   and principled actor, producing contradictory foreign policy behaviour
   and affecting the country's international reputation and credibility.},
