@article{ WOS:000737650400001,
Author = {Ibsen, Bjarne and Elmose-Osterlund, Karsten and Iversen, Evald Bundgaard
   and Hoyer-Kruse, Jens and Pedersen, Marlene Rosager Lund},
Title = {Civil society's impact on quality of life},
