@article{ WOS:000657209400002,
Author = {Cairo, Heriberto},
Title = {The Geopolitics of Global Violence in World-Systems Analysis: Relevance
   and Problems},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
Year = {2021},
Number = {30},
Pages = {22-39},
Month = {MAY-AUG},
Abstract = {The article aims to reflect critically on the role of violence and, in
   particular, of global wars in the analysis of world-systems, turning to
   other theoretical perspectives, such as structuring and critical
   geopolitics, to better understand the issue. In this way, we can assert
   that the struggle for supremacy is not necessarily a struggle for
   hegemony, which would inevitably result in periodic world wars as a
   prelude to the rise of great powers to hegemonic status. Hegemony refers
   more to the rules and practices that constitute a geopolitical order
   than to a certain hierarchical ordering of states. There is no general
   pattern of explanation for all wars: both global power configurations
   and the interrelationships between the local and the global must be
   taken into account.},
