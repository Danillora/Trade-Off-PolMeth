@article{ WOS:000706614500006,
Author = {Moraga Valle, Fabio},
Title = {The Iberoamerican Congress of socialist students of Guadalajara 1936.
   The ideological tensions between internationalism and latin americanism},
Journal = {IZQUIERDAS},
Year = {2021},
Volume = {50},
Month = {APR},
Abstract = {The work addresses an almost unknown chapter in the saga of young Latin
   American students to strengthen the foundations of anti-imperialism and
   workers' struggles in Latin America. In August 1936, in the Mexican city
   of Guadalajara, the Ibero-American Congress of Socialist Students was
   held, which created the Confederation of Socialist Students of Indo
   America, CEADA, which existed between 1936 and 1939. At this meeting
   students from various countries of the continent converged to create an
   anti-imperialist and anti-fascist front of struggle against the advance
   of the interventionist policies of the United States and of fascism in
   the Western world. The article analyzes the ideological and political
   elements that were played out in this context and the effects it had on
   the young politicians who gathered at the continental event.},
