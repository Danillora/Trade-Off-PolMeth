@article{ WOS:000706614500016,
Author = {Aguirre, Felix},
Title = {A brief history of the most elegant socialism. The intellectual origins
   of the Fabian Society, 1880-1889},
Journal = {IZQUIERDAS},
Year = {2021},
Volume = {50},
Month = {APR},
Abstract = {Using the tools provided by intellectual history, we analyze the ideas
   and doctrines contained in the political propaganda of the first
   generation of Fabian socialists (1880-1890) with the idea of exploring
   the extent to which the economic agreements discussed in its origins by
   this version of English socialism appears subordinated to a handful of
   moral purposes that we can trace in the intellectual environment of that
   period. Our conclusions suggest that Fabianism is neither a third way
   nor the result of a fortunate dialectical synthesis between liberalism
   and socialism, but a political culture of its own with the capacity of
   agency to update contemporary ideological debate.},
