@article{ WOS:000625411700001,
Author = {Negura, Petru},
Title = {Nation-building and mass schooling of ethnic minorities on the Romanian
   and Soviet peripheries (1918-1940): a comparative study of Bessarabia
   and Transnistria},
Journal = {NATIONAL IDENTITIES},
Year = {2021},
Volume = {23},
Number = {4, SI},
Pages = {433-454},
Month = {AUG 8},
Abstract = {The paper examines the local responses to mass schooling in the rural
   areas of Romanian Bessarabia and Soviet Transnistria (1918-1940). Both
   Romania and the USSR aimed at deeply transforming the local populations.
   Romania implemented schooling to assimilate ethnic minorities within the
   model of a nationalizing state, while the USSR adopted an inconsistent
   nationalizing policy, determinedly imposing compulsory education for all
   children. The resistance to schooling among ethnic minorities was less
   intense in Transnistria than in Bessarabia. In both cases, the state
   authorities abandoned, in the late 1930s, the schooling in minority
   languages for the benefit of the titular nationalities.},
