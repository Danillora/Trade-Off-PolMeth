@article{ WOS:000747908300007,
Author = {Schenoni, Luis L. and Braniff, Sean and Battaglino, Jorge},
Title = {Was the Malvinas/Falklands a Diversionary War? A Prospect-Theory
   Reinterpretation of Argentinas' Decline},
