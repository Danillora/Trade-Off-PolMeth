@article{ WOS:000766714700007,
Author = {Saa Espinoza, Marcela},
Title = {The unconquered uncovering. Analysis of images of young and naked bodies
   in Chile (1968-1973)},
Journal = {IZQUIERDAS},
Year = {2021},
Volume = {50},
Month = {APR},
Abstract = {This article investigates the processes of politicization of the youth
   body, as well as the symbolizations and incidences of images in the
   field of cultural policies in Chile during a turbulent historical
   moment: 1968-1973 - moments before the Popular Unity and until the coup
   in Chile. This is a period in which both the cultural industry and the
   discussion in society itself incorporate topics and images into their
   respective agendas with a different representation of the youth body:
   their undressed body. In this way, during this period, youth nudity was
   positioned as a topic that broke into the cultural and media sphere -
   cinema, magazines and music: a sort of ``uncover{''} that did not manage
   to position itself but fleetingly. In the light of a genealogy of the
   youth, different pieces of the visual culture of the period were
   analyzed to understand its emergence, circulation and effects on
   society, and thus account for some strategies for making ideas and
   images of youth in Chile during this period of the 20th century.},
