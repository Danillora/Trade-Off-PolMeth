@article{ WOS:000766714700004,
Author = {dos Santos, Leonardo Soares},
Title = {The Peasant Leagues of Brazil (1947-48)},
Journal = {IZQUIERDAS},
Year = {2021},
Volume = {50},
Month = {APR},
Abstract = {The object of this article is the little studied history of the Peasant
   Leagues created by the Communist Party of Brazil after 1945. I point out
   some of the reasons for the little interest that scholars have dedicated
   to such Leagues, in contrast to what was observed with the study of
   those that arose in Pernambuco in the following decade. I also indicate
   how, even after the party was declared illegal in May 1947, some Leagues
   continued to function. I also analyze how such activities were impacted
   by a gradual radicalization, crystallized in the ``1948 Manifesto{''}.
   This analysis was based on party documents, editorials published in the
   press and police reports. In addition to put into context the
   information obtained from the political scenario, an attempt was made to
   specify the different attempts to take over these entities.},
