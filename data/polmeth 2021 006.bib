@article{ WOS:000751597500022,
Author = {Compte Nunes, Guillem},
Title = {NOTES FOR A REGROUNDING OF DEMOCRACY},
Journal = {REVISTA INTERNACIONAL DE PENSAMIENTO POLITICO},
Year = {2021},
Number = {16},
Pages = {379-401},
Abstract = {From a normative and interdisciplinary approach, a critique of
   liberal-representative democracy is made based on the principal-agent
   model, which entails identifying a conflict of political interest that
   vitiates representative government. It is then shown how this conflict,
   in different versions, appears in theories of political science, public
   administration and law, and that its resolution repeatedly implies a
   certain separation of powers. Based on this analysis, a new foundation
   for democracy is proposed, consisting of direct popular ownership of the
   political system, which can be institutionalized in a Citizens' Power.},
