@article{ WOS:000706614500012,
Author = {Camargo Brito, Ricardo},
Title = {The constituent process and the limits of what is constituted: The
   Chilean case},
Journal = {IZQUIERDAS},
Year = {2021},
Volume = {50},
Month = {APR},
Abstract = {Following the proposal of the ``third constitutional way{''} deployed by
   Curd) Cobos, this article proposes a reading of the Chilean
   constitutional process in which the normative and descriptive dimensions
   of it appear conjugated. An old Aristotelian rationale allows us to
   observe the constituent processes, including the Chilean one, as
   unfinished expressions of a new order that is demanded and that its
   retrospective constitutional consecration should not ignore. Understood
   in this way, the Chilean constituent process generated as of October 18,
   offers risks but also immense opportunities in the face of the challenge
   of establishing a new radically democratic social order.},
