@article{ WOS:000730990600001,
Author = {Pollichieni, Luciano and Mumford, Andrew},
Title = {Terrorism, Slavery and the Evolution of the `Crime-Terror Nexus'},
Journal = {RUSI JOURNAL},
Year = {2021},
Volume = {166},
