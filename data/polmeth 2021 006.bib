@article{ WOS:000702394100001,
Author = {Cambalikova, Monika and Uhlerova, Monika},
Title = {REGIONAL SOCIAL DIALOGUE IN SLOVAKIA: CURRENT STATE, DEVELOPMENT,
   PERSPECTIVES},
Journal = {POLITICKE VEDY},
Year = {2021},
Volume = {24},
Number = {3},
Pages = {8-32},
Abstract = {Based on three basic models of interest policy (regional corporatism
   model, regional pluralism model and mixed model) and in the context of
   regional development and the development of civil society, we analyze
   the regional social dialogue in Slovakia. The empirical part of the
   study consists of a comparison of two analogous researches of actors of
   regional social dialogue in Slovakia from 1999 and 2020. Research
   findings and analyzes confirm that in Slovakia, regional social dialogue
   has not been yet institutionalized in any of the mentioned models.
   Actors in the regional social dialogue cooperate mainly in the regional
