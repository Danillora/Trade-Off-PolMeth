@article{ WOS:000674606500002,
Author = {Nunez Grijalva, Jorge},
Title = {Legal Values in the Criminal Regulation of Unfair Competition in Ecuador},
Journal = {CUESTIONES POLITICAS},
Year = {2021},
Volume = {39},
Number = {69},
Pages = {21-41},
Month = {JUL-DEC},
Abstract = {In all areas of the legal world there are higher aspirations, which
   represent legal values to be protected, like the justice, the common
   good and legal security stand out. The present work was proposed to
   analyze if the Ecuadorian Legislator, in its process of construction and
   promulgation of the criminal law regulating against the unfair
   competition, incorporated these three values into it. Regrettably, the
   results show an apparent absence of the three legal values in criminal
   law, leaving legal operators at a disadvantage in view of the need to
   control this type of crime and society, awaiting compliance. Through an
   exercise of legal hermeneutics, the study starts from a real problem in
   the Ecuadorian legal system of the criminal law against of the unfair
   competition, which demands to be discussed in the search for the State
   to take the necessary measures to solve this problem.},
