@article{ WOS:000736812800030,
Author = {Daigle, Delton T. and Stuvland, Aaron},
Title = {Teaching Political Science Research Methods Across Delivery Modalities:
   Comparing Outcomes Between Face-to-Face and Distance-Hybrid Courses},
