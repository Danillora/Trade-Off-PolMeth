@article{ WOS:000680966100001,
Author = {Larsen, Henrik},
Title = {NATO in an Illiberal World The Case for Differentiated Partnerships},
Journal = {RUSI JOURNAL},
Year = {2021},
Volume = {166},
Number = {3},
Pages = {84-92},
Month = {APR 16},
Abstract = {Partnerships remain NATO's only external policy instrument in an
   illiberal world. However, NATO needs to differentiate more clearly
   between its partners and intensify security cooperation with like-minded
   states to mitigate the threats posed by Russia and China. Henrik Larsen
   proposes the following differentiation: `Advanced Partners' in Western
   Europe and the EU, stressing the benefits of their technological
   strengths and shared values to bolster joint resilience capacity;
   `Enhanced Partners' in the Western Balkans and Eastern Europe, focusing
   on the need to improve governance and resilience vis-a-vis illiberal
   powers; and `Worldwide Partners', prioritising both NATO's southern
   periphery, to manage regional instability, and the Asia-Pacific, to
   counter threats posed by the rise of China. Partnership reform should
   accompany a reflection about the role of resilience in NATO's security
   and defence portfolio.},
