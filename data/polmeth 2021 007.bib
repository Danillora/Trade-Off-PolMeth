@article{ WOS:000722872100001,
Author = {Han, Kyung Joon},
Title = {Radical right success and mainstream parties' position ambiguity on
   immigration},
Journal = {ACTA POLITICA},
Year = {2022},
Volume = {57},
Number = {4},
Pages = {818-835},
Month = {OCT},
Abstract = {How does the electoral success of radical right-wing parties (RRPs)
   change mainstream parties' (MPs) issue competition behavior on
   immigration? Though studies find that MPs emphasize immigration-related
   issues as RRPs grow, literature suggests that party position ambiguity
   also constitutes political parties' issue competition behavior. We argue
   that the radical right effect on MPs' position ambiguity relies on the
   advantages or disadvantages that immigration-related issues offer to
   MPs. We find that MPs blur their immigration position in the face of
   rising RRPs when they are troubled by their internal division. When they
   are not, they clarify their position. The results imply that MPs
   consider their own circumstances on an issue as well as external
   pressure from rising RRPs in handling immigration-related issues. The
   results also imply that position ambiguity as well as selective issue
   emphasis comprises political parties' issue competition.},
