@article{ WOS:000766714700001,
Author = {Heredia Alarcon, Angel},
Title = {Values and political discourse: an approach to the figure of the ``new
   man{''} in the discourse of the PCP-Shining Path (1968-1980)},
Journal = {IZQUIERDAS},
Year = {2021},
Volume = {50},
Month = {APR},
Abstract = {The study analyzes the representation of the ``new man{''} proposed by
   the Communist Party of Peru - Shining Path (PCP-SL) before the Internal
   Armed Conflict. This notion alludes to a model figure that represents
   the values or qualities of the new society to be built, as a product of
   a process of social change. In the PCP-SL this figure was embodied by
   Jose Carlos Mariategui, a Peruvian Marxist thinker, considered by said
   organization as its founder. From the analysis of his party documents,
   it is concluded that the qualities attributed to Mariategui responded to
   the need to outline the party's work in the face of the urgency of the
   beginning of the armed struggle, in addition to revealing a close link
   with the Maoist tradition of ``man new{''}.},
Publisher = {ARIADNA EDICIONES \& SAINT PETERSBURG STATE UNIV},
