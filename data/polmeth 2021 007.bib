@article{ WOS:000695885200001,
Author = {Lambert, Karras J. and Coyne, Christopher J. and Goodman, Nathan P.},
Title = {The Fatal Conceit of Foreign Intervention: Evidence from the Afghanistan
   Papers},
Journal = {PEACE ECONOMICS PEACE SCIENCE AND PUBLIC POLICY},
Year = {2021},
Volume = {27},
Number = {3},
Pages = {285-310},
Month = {SEP},
Abstract = {The fatal conceit of foreign intervention refers to the limitations
   faced by governments using discretionary power to address perceived
   problems in foreign societies. Drawing on evidence from the
   ``Afghanistan Papers{''}-a collection of internal government documents
   compiled by the Special Inspector General for Afghanistan Reconstruction
   (SIGAR) and released in December 2019-we demonstrate how the failure of
   the U.S.-led intervention in Afghanistan to meet the stated objectives
   illustrates the fatal conceit of foreign intervention. Weexplore the
   limitations faced by foreign government interveners in three stages,
   reflecting distinct decision nodes within a means-ends framework in
   which the policies relating to foreign intervention are formulated and
   implemented.},
