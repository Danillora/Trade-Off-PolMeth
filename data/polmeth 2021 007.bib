@article{ WOS:000693666900010,
Author = {Capitaine, Brieg and Helly, Denise},
Title = {Hate through a Canadian conservative movement worldview. The Christian
   Heritage Party on the Internet},
Journal = {POLITICA Y SOCIEDAD},
Year = {2021},
Volume = {58},
Number = {2},
Abstract = {This article aims to capture the political imaginary of a Christian
   Canadian neoconservative party. The socio-anthropological analysis of
   the content of the Christian Heritage Party website describes the
   etiological, ontological, and axiological grammars that shapes the
