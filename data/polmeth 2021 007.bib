@article{ WOS:000715064800001,
Author = {Cunial, Santiago L.},
Title = {Transitions for whom? Political alignment and subsidies for solar energy
   projects in rural Colombian municipalities},
Journal = {LATIN AMERICAN POLICY},
Year = {2021},
Volume = {12},
Number = {2},
Pages = {300-332},
Month = {NOV},
Abstract = {Does having a politically aligned mayor affect the amount of funds that
   municipalities receive to develop solar energy? This article contributes
   to the literature on energy transitions by considering how political
   alignment affects the subnational distribution of subsidies for the
   development of small-scale solar projects in non-interconnected
   municipalities in Colombia. By analyzing the 2015 Colombian mayoral
   elections with a regression discontinuity design, I show that
   municipalities that have a mayor aligned with the national incumbent
   party receive on the average U.S.\$256 per capita more than those in
   which the major is not politically aligned. Yet, this effect holds only
   for municipalities with close elections, which in turn provides evidence
   of the swing-voter hypothesis. I theorize that these differences can be
   explained by the nature of off-grid solar systems as a private good in
   Colombian municipalities. The allocation of subsidies to aligned
   subnational regions is a strategy to consolidate the rule of the
   national party at the local level, whereby the national government can
   optimize electoral outcomes and legislative outputs.},
