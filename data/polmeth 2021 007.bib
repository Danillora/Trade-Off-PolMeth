@article{ WOS:000632023700001,
Author = {Zaidi, Syed Muhammad Saad},
Title = {American Global Supremacy under Threat? The Chinese Factor},
Journal = {POLITICS \& POLICY},
Year = {2021},
Volume = {49},
Number = {2},
Pages = {502-528},
Month = {APR},
Abstract = {In contemporary times, the widely debated topic in international
   politics is the future of the United States as the sole superpower of
   the world. Many political analysts see the ``unipolar moment{''} to be
   in grave danger, especially by a rapidly rising China. The advocates of
   China as the next superpower term the economic crisis of 2008 to be a
   watershed moment. Since the financial meltdown post-2008, China began to
   unfold its geoeconomic might to the world, which in turn gave it great
   geopolitical capital to challenge-to an extent-the U.S. hegemonic
   status. However, when a purely empirical analysis is conducted between
