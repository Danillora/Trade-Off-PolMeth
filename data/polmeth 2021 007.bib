@article{ WOS:000609698800001,
Author = {Yates, Douglas A.},
Title = {Dynastic rule in Syria and North Korea: Nepotism, succession, and
   sibling rivalry},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2022},
Volume = {43},
Number = {3, SI},
Pages = {450-463},
Month = {JUN},
Abstract = {Dynastic rule in republics is a global trend. Using a qualitative
   life-story-and-family-history method to compare two republican dynasties
   - the Assads of Syria and Kims of North Korea - this article examines
   how ruthless kin groups establish themselves in power, practice nepotism
   and corrupt republican institutions of government with dynastic
   succession. Focusing on sibling rivalry, a potentially destructive
   threat to dynasties, it contributes to an emerging political science
   literature on republican political families by exploring five factors
   that shape sibling conflict and cooperation.},
