@article{ WOS:000642035300003,
Author = {Ilonszki, Gabriella and Dudzinska, Agnieszka},
Title = {Opposition behaviour against the third wave of autocratisation: Hungary
   and Poland compared},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2021},
Volume = {20},
Number = {4},
Pages = {603-616},
Month = {DEC},
Abstract = {Hungary and Poland are often placed in the same analytical framework
   from the period of their `negotiated revolutions' to their autocratic
   turn. This article aims to look behind this apparent similarity focusing
   on opposition behaviour. The analysis demonstrates that the
   executive-parliament power structure, the vigour of the
   extra-parliamentary actors, and the opposition party frame have the
   strongest influence on opposition behaviour, and they provide the
   sources of difference between the two country cases: in Hungary an
   enforced power game and in Poland a political game constrain opposition
   opportunities and opposition strategic behaviour.},
