@article{ WOS:000747734800001,
Author = {Durao, Aylton Barbieri},
Title = {Law as a form of social integration according to Habermas},
Journal = {ANALECTA POLITICA},
Year = {2021},
Volume = {11},
Number = {21},
Month = {JUL-DEC},
Abstract = {Integration problems in complex society occur because of the division
   between the political and economic systems, which use different forms of
   strategic rationality, based on middle power and money, respectively,
   and the world of life that uses communicative rationality. In Theory of
   Communicative Action, Habermas points out that complex society leads to
   a dead end as systems colonize the world of life. Nonetheless, from
   Facticity and Validity, he shows how problems of integration can be
   resolved by the law that uses the two forms of rationality by virtue of
   the double meaning of its validity. As a result, he can make the
   translation between the communicative language of the world of life and
   the strategic language of systems.},
