@article{ WOS:000768292100007,
Author = {Calderon Sanchez, Dulfary and Mora Vargas, Ilit Dahab},
Title = {The role of the blue angels in implementation. A look from the street
   habitability project in Bogota},
Journal = {CIMEXUS},
Year = {2021},
