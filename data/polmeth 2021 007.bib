@article{ WOS:000674606500008,
Author = {Vozniuk, Andrii and Kamensky, Dmitriy and Dudorov, Olexandr and Movchan,
   Roman and Andrushko, Andriy},
Title = {Unconstitutionality of criminal liability for filing inaccurate
   information in Ukraine: critical legal analyses},
Journal = {CUESTIONES POLITICAS},
