@article{ WOS:000634591300001,
Author = {McBride, Jared},
Title = {The Many Lives and Afterlives of Khaim Sygal: Borderland Identities and
   Violence in Wartime Ukraine},
Journal = {JOURNAL OF GENOCIDE RESEARCH},
Year = {2021},
Volume = {23},
Number = {4, SI},
Pages = {547-567},
Month = {OCT 2},
Abstract = {At first glance, the dark mid-century wartime odyssey of Khaim Sygal, a
   simple Jewish schoolteacher from L'viv, appears as a horrible, but
   fascinating, historical oddity. Sygal served as a Soviet policeman from
   1939 to 1941, transformed himself into a Ukrainian during the war,
   served in a Ukrainian nationalist militia, and ultimately, participated
   in the Holocaust alongside the Germans as an auxiliary police force
   member in Volhynia. After the war, he morphed into a Polish Jew and
   Holocaust victim before being arrested, tried, and executed in 1953 by
   the Soviets. Sygal's story first garnered attention in the late 1970s,
   and, since then, commentators from the Soviet Union to post-Soviet
   Ukraine to contemporary North America have been drawn to his case for
   its exceptional character. This article provides a new account of
   Sygal's biography and then dissects and analyzes various interlocking
   narratives about Sygal. In the process, I demonstrate how these
   narratives have been used to advance political goals and intentionally
   obfuscate the truth about Sygal's complicated past, and I show how
   untangling these narratives provides a lens into historical, as well as
   contemporary understandings of the Nazi occupation of Eastern Europe.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
