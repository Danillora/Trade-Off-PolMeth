@article{ WOS:000734528800001,
Author = {Dassonneville, Ruth and McAllister, Ian},
Title = {Are they different?A comparative study of European populist party
   members},
Journal = {PARTY POLITICS},
Year = {2023},
Volume = {29},
Number = {1},
Pages = {16-25},
Month = {JAN},
Abstract = {Party membership is in decline across the established democracies, but
   populist parties appear to be reversing this trend. The expansion of
   populist parties' membership base raises the question whether these
   parties are fulfilling their role as actors who serve as a ``corrective
   to democracy{''} by mobilizing the socially and politically marginalized
   to become politically active. Using large comparative datasets from the
   European Social Survey and the World Values Survey, we examine the
   characteristics of populist party members in terms of their
   socio-demographic background and their political attitudes. We show
   that, with the exceptions of income levels and church attendance, the
   members of populist and non-populist parties look very much alike and
   hold similar political attitudes. These findings imply that mobilizing
   the politically disaffected remains a challenge, even for populist
   actors.},
