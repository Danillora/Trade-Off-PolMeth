@article{ WOS:000621660600001,
Author = {Abu, Ofir and Ben-Porat, Guy},
Title = {Citizenship, minorities and the police: attitudes of Jewish Ethiopian
   Israelis toward police reforms},
Journal = {CITIZENSHIP STUDIES},
Year = {2021},
Volume = {25},
Number = {3},
Pages = {426-444},
Month = {APR 3},
Abstract = {Minority groups striving for integration may support representation in
   state institutions as well as recognition measures not as ends in
   themselves but only insofar as they believe that these reforms can
   advance their full integration into mainstream society. We illustrate
   this argument by focusing on perceptions about police reforms among
   Israeli Jews of Ethiopian descent, a racial minority. Drawing on data
   from public opinion surveys, focus groups, and interviews with community
   activists, our analysis indicates that Ethiopian Israelis support
   recruitment of co-ethnic officers (representation) and cultural
   sensitivity training of police officers (recognition) because they
   believe it could change police's attitudes and behaviors. However, they
   are also conscious of the downsides of these reforms as they might
   perpetuate their stigmatization as a distinct group that requires
   special accommodation. This paper suggests that successful integration
   strategies need to take into account how minority groups view their
   status within the state's citizenship regime.},
