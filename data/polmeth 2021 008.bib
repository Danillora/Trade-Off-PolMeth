@article{ WOS:000630595100005,
Author = {Togman, Richard},
Title = {Foucauldian security and the threat to democratic policy-making},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
