@article{ WOS:000727523200001,
Author = {Yin, Weiwen and Huo, Weidong and Lin, Danyang},
Title = {The effects of state coercion on voting outcome in protest movements: a
   causal forest approach},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2023},
Volume = {11},
Number = {3},
Pages = {645-653},
Month = {JUL},
Abstract = {In this research note, we examine how Hong Kong voters respond to police
   violence in the recent social movement. We use causal forests, a machine
   learning algorithm, to estimate the impact of tear gas usage specific to
   each constituency. Based on the 2019 District Council Election outcome,
   we find that there is heterogeneity in the effect of state coercion on
   the vote share of pro-democracy candidates, depending on many
   socioeconomic characteristics of the constituency. The results imply
   that economic concerns still matter in the struggle to obtain democracy:
   citizens who sense economic insecurity in social unrest show less
   disapproval of state violence.},
