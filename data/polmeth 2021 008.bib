@article{ WOS:000653697700001,
Author = {Shoub, Kelsey and Stauffer, Katelyn E. and Song, Miyeon},
Title = {Do Female Officers Police Differently? Evidence from Traffic Stops},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
Year = {2021},
Volume = {65},
Number = {3},
Pages = {755-769},
Month = {JUL},
Abstract = {Political scientists have increasingly begun to study how citizen
   characteristics shape whether-and how-they interact with the police.
   Less is known about how officer characteristics shape these
   interactions. In this article, we examine how one officer
   characteristic-officer sex-shapes the nature of police-initiated contact
   with citizens. Drawing on literature from multiple fields, we develop
   and test a set of competing expectations. Using over four million
   traffic stops made by the Florida State Highway Patrol and Charlotte
   (North Carolina) Police Department, we find that female officers are
   less likely to search drivers than men on the force. Despite these lower
   search rates, when female officers do conduct a search, they are more
   likely to find contraband and they confiscate the same net amount of
   contraband as male officers. These results indicate that female officers
   are able to minimize the number of negative interactions with citizens
   without losses in effectiveness.},
