@article{ WOS:000695354100007,
Author = {Martin Sanchez, Juan and Breuer, Martin},
Title = {The ILO and the Definition of Latin America as a Region of Special
   Concern: the Andean Indian Programme between the Indigenous Question and
   Universalist Modernization},
Journal = {AMERICA LATINA HOY-REVISTA DE CIENCIAS SOCIALES},
Year = {2021},
Volume = {88},
Pages = {119-136},
Month = {AUG},
Abstract = {This article studies how the ILO as a bureaucratic and political
   apparatus shaped the representation of Latin American as a world region
   differing form the historical ``norm{''} of modern societies. In
   particular, it analyzes how the Andean Indian Program (PIA) reworked and
   amplified three recurrent aspects in the contribution of the Andes to
   the representation of Latin America: the regional character, the
   indigenist claim and the experimental novelty.},
