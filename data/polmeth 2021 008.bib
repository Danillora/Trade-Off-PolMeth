@article{ WOS:000664965200003,
Author = {Kaiser, Wolfram},
Title = {Victimizing Europeans: Narrating Shared History in the European
   Parliament's House of European History},
Journal = {POLITIQUE EUROPEENNE},
Year = {2021},
Volume = {71},
Number = {1},
Pages = {54-78},
Abstract = {The European Parliament's House of European History in Brussels, which
   opened in 2017, is the first museum in Europe devoted to telling the
   history of modern Europe. Based on a close reading of its permanent
   exhibition, non-participant observation, document analysis and
   interviews, this article explores and interprets the museum's
   master-narrative about the First World War, the totalitarian regimes and
   the Second World War and post-war European integration. It demonstrates
   how and why the museum blames nationalism as the root cause of conflict
   and war in Europe. With its strong focus on the similarities between
   National Socialism and Stalinism as regimes of oppression, moreover, it
   tells a story of Europeans as victims of abstract oppressive regimes.
   Paradoxically, the House of European History in this way employs the
   narrative tools of nationalism for overcoming nationalism.},
