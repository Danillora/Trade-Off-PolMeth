@article{ WOS:000717481400001,
Author = {Lopez Vences, Rita B. and Lopez Reyes, Jose R.},
Title = {Defiance of democracy in Oaxaca},
Journal = {LATIN AMERICAN POLICY},
Year = {2021},
Volume = {12},
Number = {2},
Pages = {554-570},
Month = {NOV},
Abstract = {This article describes the quality of democracy in Oaxaca, Mexico, its
   evolution in recent years, and how it compares with the national index.
   It then analyzes political competitiveness in recent local elections,
