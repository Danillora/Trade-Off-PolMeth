@article{ WOS:000661950200001,
Author = {Dumouchel, David},
Title = {At the heart of the storm: Election public opinion and the refugee
   crisis},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2021},
Volume = {54},
Number = {2},
Pages = {249-266},
Month = {JUN},
Abstract = {Scholarship on media storms has yet to systematically assess their
   impacts on public opinion. This article aims to fill this gap by using
   survey data to analyze the evolution of public opinion with respect to
   the so-called ``refugee crisis,{''} a media storm that occurred during
   the 2015 Canadian federal campaign. The evidence shows that the storm
   influenced citizen attitudes in regard to key frames of the issue and
   that the effect persisted until the end of the campaign. They also
   demonstrate how the most prominent frame became a determinant of voting
   intention, and ultimately, vote choice. Overall, these findings suggest
   that citizens are receptive to media storms and therefore offer a
   concrete example of how media market logic can have a preponderant
   influence in the public sphere.},
