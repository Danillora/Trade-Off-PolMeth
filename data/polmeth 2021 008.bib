@article{ WOS:000827234000003,
Author = {Koros, Dimitris},
Title = {THE NORMALIZATION OF PUSHBACKS IN GREECE: BIOPOLITICS AND RACIST STATE
   CRIME},
Journal = {STATE CRIME},
Year = {2021},
Volume = {10},
Number = {2},
Pages = {238-256},
Abstract = {The paper discusses pushback operations in Greece as a generalized
   anti-immigration policy that involves practices constituting racist
   state crime. Beginning from a description of the illegal operations on
   the Greek/Turkish borders, this paper examines the constant denial
   tactics of the government and the response of national, regional and
   international organizations and institutions. It is shown that pushbacks
   have become a standardized frontline tool of border management. This
   phenomenon is approached from a criminological viewpoint in terms of
   racist state crime and as a racist biopolitical technology of power for
   the government of the migrant populations, and for the defence of
   society and national identity. Showing that pushbacks are a becoming a
   central border policy in Europe, the paper concludes with a discussion
   of the need for both legal and non-legal counter-strategies.},
