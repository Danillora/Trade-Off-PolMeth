@article{ WOS:000674606500033,
Author = {Boiarov, Viktor and Larkin, Mikhail and Kyrychenko, Oleh and Penkov,
   Sergey and Kruhlov, Oleh},
Title = {Features of the investigation of hooliganism committed by football fans},
Journal = {CUESTIONES POLITICAS},
