@article{ WOS:000774313300003,
Author = {Ahmed, Rashid and Baig, Muhammad Ali and Malik, Saif Ur Rehman},
Title = {The Pashtuns use of Suicide Bombing as a Military Operation in
   Afghanistan and Pakistan},
