@article{ WOS:000630932100003,
Author = {Souroujon, Gaston},
Title = {The traps of meritocracy. A tour of the most significant problems that
   hide the merit},
Journal = {REVISTA DE ESTUDIOS POLITICOS},
Year = {2021},
Number = {191},
Pages = {59-80},
Month = {JAN-MAR},
Abstract = {The article sets out to deconstruct a certain common sense that has been
   articulated on merit as the fairest criterion for the distribution of
   rewards and positions within society. To this end, a double path will be
   followed, first of all, a historical one, by which an attempt will be
   made to synthetically detail the history of the category in the 20th
