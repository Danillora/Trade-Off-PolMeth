@article{ WOS:000782433500004,
Author = {Sibiri, Hagan},
Title = {FOCAC at Twenty A ``Bargained Institutional Framework{''} for Shared
   Impacts},
Journal = {CHINA QUARTERLY OF INTERNATIONAL STRATEGIC STUDIES},
