@article{ WOS:000628925700001,
Author = {Aleman, Eduardo and Micozzi, Juan Pablo},
Title = {Parliamentary rules, party norms, and legislative speech},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
