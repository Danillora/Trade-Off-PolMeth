@article{ WOS:001025648100006,
Author = {Fisher, Mark},
Title = {Reading the imperceptible tremors of an unimaginable future},
Journal = {PENSAMIENTO AL MARGEN},
Year = {2021},
Number = {15},
Month = {DEC-APR},
Abstract = {Is dialectical thinking essential to anti-capitalism? Under this central
   question in the tradition of Marxist and post-Marxist thought, Mark
   Fisher reviews Fredric Jameson's monumental effort to save ``the
