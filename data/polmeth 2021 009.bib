@article{ WOS:000766714700014,
Author = {Vorotnikova, Tatiana and Ferreyra Bertone, Carlos Alfredo},
Title = {The USSR as seen by the left-wing press in Argentina during the 1920s},
Journal = {IZQUIERDAS},
Year = {2021},
Volume = {50},
Month = {APR},
Abstract = {The following article proposes to analyze how the image of the USSR was
   captured and communicated from the different elaborations contained in
   the left press, and therefore, subsumed in an ideological space of open,
   interconnected and distant borders of sharp delimitations during the
   1920s. Basically, the study of newspapers and magazines is used, pointed
   out in ``La Correspondencia Sudamericana{''} and ``Revista de
   Oriente{''}, representative options of the spatial-temporal cut involved
   and of varied compositions. At the same time, an analytical tracking is
   combined that focuses attention on issues such as: the advantages of the
   planned economy vs. capitalist system, working conditions and working
   class life, in addition to the ongoing cultural development. For the
   above, it is hoped to crystallize how these elaborations could shape
   ways of making strategies of information dissemination and even
   political-party penetrations that would nourish the exercise of
   digesting the irruption of the USSR.},
