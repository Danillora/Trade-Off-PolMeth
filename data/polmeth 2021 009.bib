@article{ WOS:000647253600001,
Author = {Bolleyer, Nicole},
Title = {Civil society - Politically engaged or member-serving? A
   governance perspective},
Journal = {EUROPEAN UNION POLITICS},
Year = {2021},
Volume = {22},
Number = {3},
Pages = {495-520},
Month = {SEP},
Abstract = {Which types of civil society organizations are politicized as indicated
   by regular political engagement and why? If they are, how wide-ranging
   are their political action repertoires? This article proposes an
   `organizational governance perspective' on civil society organizations'
   political engagement by arguing that organizations resembling
   traditional `voluntary associations' are less likely to be politicized
   and to employ a broad political action repertoire than those resembling
   highly professionalized `voluntary agencies'. Applying event count
   regressions to new data from four recent population surveys widely
   substantiates the proposed perspective, thereby challenging prominent
   arguments about the detrimental effects of professionalization and state
   dependency on organizations' ability to contribute to democratic
   representation.},
