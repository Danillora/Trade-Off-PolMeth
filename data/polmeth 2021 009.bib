@article{ WOS:000675654200001,
Author = {Zittel, Thomas and Louwerse, Tom and Helboe Pedersen, Helene and
   Schakel, Wouter},
Title = {Should we conduct correspondence study field experiments with political
   elites?},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2023},
Volume = {44},
Number = {4},
Pages = {459-470},
Month = {SEP},
Abstract = {Correspondence study field experiments with political elites are a
   recent addition to legislative studies research, in which unsolicited
   emails are sent to elites to gauge their responsiveness. In this
   article, we discuss their ethical implications. We advance from the
   viewpoint that correspondence study field experiments involve trade-offs
   between costs and benefits that need to be carefully weighted. We
   elaborate this argument with two contributions in mind. First, we
   synthesize ethical considerations in published work to explore what the
   specific trade-offs are and how they can be mitigated by experimental
   design. We conclude that correspondence study field experiments with
   political elites are worth pursuing given their potential to further
   good governance. But they also involve distinct trade-offs that are
   particularly challenging. Second, we draw from our own considerations
   while designing a comparative correspondence study field experiment and
   stress challenges resulting from cross-national designs. In sum, we aim
   to facilitate further reasoned discussion on an important methodological
   issue.},
