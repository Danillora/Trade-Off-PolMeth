@article{ WOS:000636011100001,
Author = {Nakatani, Miho},
Title = {How do political decision-making processes affect the acceptability of
   decisions? Results from a survey experiment},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2023},
Volume = {44},
Number = {2},
Pages = {244-261},
Month = {MAR},
Abstract = {This study aims to identify the types of city council decision-making
   processes that influence public perceptions of procedural fairness and
   the acceptability of decisions. Using an online experimental scenario
   survey conducted in Japan, this study found that, given the opportunity
   to participate in the decision-making process and when the decision is
   reached through a compromise among council members, people tend to feel
   that the process is fair and accept the decision even when it is
   unfavourable to them. This result is important for the governance of
   many advanced countries with low economic growth rates but great public
   demands. Additionally, this study highlights the process preferences of
   the public, which has received little attention compared with research
   on policy preferences.},
