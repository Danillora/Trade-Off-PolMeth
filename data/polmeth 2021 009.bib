@article{ WOS:000727962200005,
Author = {Torun, Tuba},
Title = {THE ISTANBUL CONVENTION KEEPS ALIVE},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2021},
Volume = {20},
Number = {1},
Pages = {59-66},
Month = {SPR},
Abstract = {The Council of Europe Convention on preventing and combating violence
   against women and domestic violence, also known as the Istanbul
   Convention, was signed by Turkey on 11 May 2011. A landmark treaty for
   women's rights, the Convention designated gender inequality as the
   source of violence against women, defining violence as not solely a
   domestic issue but one of a larger societal problem. Despite being the
   most comprehensive legal framework that strives to combat domestic
   violence and gender inequality to date, Turkey withdrew from the treaty
   with a Presidential Decree at midnight on 20 March 2021. While providing
   an overview of the domestic dynamics that led to Turkey's withdrawal,
   this article shines a light on the collective struggle for gender
   equality in Turkey, arguing that society is now aware of violence more
   than ever.},
