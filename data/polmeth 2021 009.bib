@article{ WOS:000634661500001,
Author = {Gavrilova, Sofia},
Title = {Regional Memories of the Great Terror: Representation of the Gulag in
   Russian Kraevedcheskii Museums},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2023},
Volume = {70},
Number = {5, SI},
Pages = {479-493},
Month = {SEP 3},
Abstract = {The article examines the representation of the Stalinist repressions in
   regional and local-kraevedcheskii-museums across several regions of
   Russia. Different types of exclusion and inclusion of repressions in
   museum expositions are in focus, revealing several common patterns
   across the selected case studies: displacement; depersonalization;
   ahistorical representations. The In the majority of the case studies
   Gulag is still a geo-historical ``blank spot,{''} erased both from
