@article{ WOS:000704182700001,
Author = {Eckhouse, Laurel},
Title = {Metrics Management and Bureaucratic Accountability: Evidence from
   Policing},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
