@article{ WOS:000658919100001,
Author = {Fukuoka, Kazuya and Takita-Ishii, Sachiko},
Title = {Teaching how to love your country in schools?: a study of Japanese youth
   narratives on patriotic education},
Journal = {NATIONAL IDENTITIES},
Year = {2022},
Volume = {24},
Number = {3},
Pages = {247-270},
Month = {MAY 27},
Abstract = {In 2018, the Japanese government introduced a controversial education
   policy, which made moral education (dotoku) a formal (as opposed to
   supplementary) subject in elementary schools, and the new scheme intends
   to teach, foster, and grade patriotic feelings among school children. By
   referring to semi-autobiographical narratives of 80 Japanese college
   students, this exploratory study delves into the question of how the
   Japanese college youth perceive the efficacy of patriotic education. Do
   we need to teach school children how to love Japan? If so, how? If not,
   why? This study suggests that the youth narratives do not necessarily
   align with this shift.},
