@article{ WOS:000675790700012,
Author = {Galais, Carol and Rico, Guillem},
Title = {An unjustified bad reputation? The Dark Triad and support for populism},
Journal = {ELECTORAL STUDIES},
Year = {2021},
Volume = {72},
Month = {AUG},
Abstract = {While research concludes that populist leaders stand out for their
   ``dark{''} personalities, studies on the psychological underpinnings of
   citizens' populist attitudes are scant and inconclusive, with some
   agreement on the association between disagreeableness and populist
   attitudes. By bringing the Dark Triad personality traits of narcissism,
   psychopathy, and Machiavellianism into the picture, we aim to answer
   whether populist citizens -like populist leaders-score higher in this
   dark traits, and whether the addition of the Dark Triad helps to better
   understanding the populist personality. Using an online Spanish sample,
   our analyses find that using the Dark Triad does indeed add to our
   understanding of populist attitudes. In opposition to widespread
   intuitions about the character of populists, we find that psychopathy
   and Machiavellianism are negatively associated with support for
   populism, with only narcissism being positively related to the
   people-centric dimension of populist attitudes.},
