@article{ WOS:000752537100001,
Author = {Tomasello, Federico},
Title = {The Government of Public Hygiene. Epidemic, the Social Question and
   Forms of Living in the City of the Industrial Revolution},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
Year = {2021},
Volume = {33},
Number = {65},
Pages = {21-44},
Abstract = {This article explores the forms of housing and living of the subaltern
   classes in the French urban context of the early 19th century affected
   by the social consequences of the Industrial Revolution. It focuses on
   the social representations and discourses developed by the liberal
   elites in order to retrace the genesis of the concepts of ``domestic
   comfort{''} and ``public hygiene{''}. The cholera epidemic of 1832 and
   the growth of the hygienist movement are identified as crucial driving
   forces for the emergence of plans of urban and housing reform for the
   working class aimed at protecting the social order and public health.},
Publisher = {UNIV BOLOGNA, DEPT POLITICAL \& SOCIAL SCIENCES, POLITICAL SCIENCE},
