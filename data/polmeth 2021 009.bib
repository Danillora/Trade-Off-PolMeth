@article{ WOS:000664696300001,
Author = {Krishnamurthy, Meena},
Title = {Martin Luther King Jr. on Democratic Propaganda, Shame, and Moral
   Transformation},
Journal = {POLITICAL THEORY},
Year = {2022},
Volume = {50},
