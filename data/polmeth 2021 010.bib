@article{ WOS:000715358600004,
Author = {Elstub, Stephen and Carrick, Jayne and Khoban, Zohreh},
Title = {DEMOCRATIC INNOVATION IN THE SCOTTISH PARLIAMENT: AN EVALUATION OF
   COMMITTEE MINI-PUBLICS},
Journal = {SCOTTISH AFFAIRS},
