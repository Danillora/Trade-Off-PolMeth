@article{ WOS:000722759400001,
Author = {Blackburn, Matthew},
Title = {The persistence of the civic-ethnic binary: competing visions of the
   nation and civilization in western, Central and Eastern Europe},
Journal = {NATIONAL IDENTITIES},
Year = {2022},
Volume = {24},
Number = {5, SI},
Pages = {461-480},
Month = {OCT 20},
Abstract = {The normative binary of `good-progressive' and `bad-retrograde'
   nationalism, traceable to the civic and ethnic dichotomy, is alive and
   well in studies of nationalism and populism today. This article
   underlines the insufficiency of this approach, firstly by examining
   three stances on the civic nation in the West, each of which rejects
   ethnic nationalism and reflect different fundamental concerns. Moving
   east, in Central Europe the binary is inverted and turned against
   `liberal cosmopolitans'; in Russia, the Kremlin's `state-civilization'
   project can be viewed as a distinct trend in nation-building for
   non-Western contemporary great powers.},
