@article{ WOS:000657089200001,
Author = {Drolet, Jean-Francois and Williams, Michael C.},
Title = {From critique to reaction: The new right, critical theory and
   international relations},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
Year = {2022},
Volume = {18},
Number = {1},
Pages = {23-45},
Month = {FEB},
Abstract = {Across the globe, radical conservative political forces and ideas are
   influencing and even transforming the landscape of international
   politics. Yet IR is remarkably ill-equipped to understand and engage
   these new challenges. Unlike political theory or domestic political
   analyses, conservatism has no distinctive place in the fields' defining
   alternatives of realism, liberalism, Marxism, and constructivism. This
   paper seeks to provide a point of entry for such engagement by bringing
   together what may seem the most unlikely of partners: critical theory
   and the New Right. Important parts of today's New Right represent
   self-conscious appropriations of Critical themes and thinkers-turning
   them to self-declared ``reactionary{''} ends. Developing outside the
   confines of the academy, these forms of thought have woven insights from
   across Critical theory into new and mobilizing forms of conservative
   ideology, seeking to link that ideology to social forces that play
   increasingly active roles in global politics. Our intention here is not
   to somehow blame Critical perspectives for the ideas of the New Right,
   either directly or by association. Rather, we seek to show how an
