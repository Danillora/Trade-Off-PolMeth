@article{ WOS:000756949500019,
Author = {Smidt, Hannah and Perera, Dominic and Mitchell, Neil J. and Bakke,
   Kristin M.},
Title = {Silencing Their Critics: How Government Restrictions Against Civil
   Society Affect International `Naming and Shaming'},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2021},
Volume = {51},
Number = {3},
Pages = {1270-1291},
Month = {JUL},
Abstract = {International `naming and shaming' campaigns rely on domestic civil
   society organizations (CSOs) for information on local human rights
   conditions. To stop this flow of information, some governments restrict
   CSOs, for example by limiting their access to funding. Do such
   restrictions reduce international naming and shaming campaigns that rely
   on information from domestic CSOs? This article argues that on the one
   hand, restrictions may reduce CSOs' ability and motives to monitor local
   abuses. On the other hand, these organizations may mobilize against
   restrictions and find new ways of delivering information on human rights
   violations to international publics. Using a cross-national dataset and
   in-depth evidence from Egypt, the study finds that low numbers of
   restrictions trigger shaming by international non-governmental
   organizations. Yet once governments impose multiple types of
   restrictions, it becomes harder for CSOs to adapt, resulting in fewer
   international shaming campaigns.},
