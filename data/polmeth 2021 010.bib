@article{ WOS:000678878900001,
Author = {Angervil, Gilvert},
Title = {A Comprehensive Application of Kingdon's Multiple Streams Framework: An
   Analysis of the Obama Administration's No Child Left Behind Waiver
   Policy},
Journal = {POLITICS \& POLICY},
Year = {2021},
Volume = {49},
Number = {5},
Pages = {980-1020},
Month = {OCT},
Abstract = {Kingdon's multiple streams framework (MSF) is very influential, but
   problems exist with its application. Most applications neither fully use
   the MSF's problem stream, policy stream, politics stream, policy
   entrepreneur, and policy window concepts nor all their subcomponents.
   These trends indicate a lack of shared language and understanding of the
   policy process necessary for the MSF's theoretical development. Most
   applications employing the concepts identify subcomponents but disregard
   their operationalizations. Moreover, research overwhelmingly cites
   Kingdon's 1984 publication while neglecting studies contributing to new
   MSF understandings. Using the Obama administration's No Child Left
   Behind waiver policy, this article applies the MSF concentrating on its
   contemporary understandings, the focus concepts, and the concepts'
   subcomponents while operationalizing them. This allows an enhanced
   analysis and MSF's policy process understanding, identifies the MSF's
   strengths and weaknesses, notes nuances, offers new insights, and
   recommends future research.},
