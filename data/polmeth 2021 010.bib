@article{ WOS:000626255100005,
Author = {Majumdar, Sayonee},
Title = {A Class-Focused Theory of Minimum Support Price and Agricultural
   Distress in India},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2021},
Volume = {33},
Number = {1},
Pages = {71-97},
Abstract = {Minimum support price (MSP) is posed as a remedial response to the
   distress condition of Indian farmers. This essay dissects MSP and the
   debate regarding it from the class-focused theory of surplus labor. The
   analytical tool of class sets shows the limited scope of MSP compared to
   the disaggregated nature of the Indian agricultural sector and shows
   that the MSP policy debate is blind to class questions and their
   consequences. Bringing in various overdetermining and contradictory
   effects of class-related production and circulation processes, value
   theory is applied to unravel the dissimilar crisis conditions of
   agrarian enterprises. Once MSP is combined with class and other nonclass
   processes, it follows that the MSP outcome will not have uniform effects
   across enterprises and will not secure its self-proclaimed objective of
   redressing farmer distress.},
