@article{ WOS:000605042000001,
Author = {Lamb, Matt and Steinberg, Alan},
Title = {The tangled web: is cyber participation still predictive of offline
   civic engagement?},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2022},
Volume = {32},
Number = {4},
Pages = {847-866},
Month = {OCT 2},
Abstract = {Research examining cyber participation provide a preponderance of
   evidence that it has an independent effect on voting and other offline
   activities. However, most of the studies were conducted when online
   participation was a novelty. As more of our personal and professional
   lives have moved online, does cyber participation still have an
   independent effect on offline activities like voting? Do these findings
   still hold true across national contexts? We find that while cyber
   participation is predictive of various types of public participation,
   such as attending meetings or participating in a march, in the American
   and Canadian contexts, it is not predictive of voting in either country.
   Age and income have a statistically significant and negative
   relationship with cyber participation in both contexts. This suggests
   that the determinants of cyber participation are different from
   traditional participation.},
