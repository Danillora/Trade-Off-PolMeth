@article{ WOS:000604554800004,
Author = {Valentini, Laura},
Title = {The natural duty of justice in non-ideal circumstances: On the moral
   demands of institution building and reform},
Journal = {EUROPEAN JOURNAL OF POLITICAL THEORY},
Year = {2021},
Volume = {20},
Number = {1},
Pages = {45-66},
Month = {JAN},
Abstract = {Principles of distributive justice bind macro-level institutional
   agents, like the state. But what does justice require in non-ideal
   circumstances, where institutional agents are unjust or do not exist in
   the first place? Many answer by invoking Rawls's natural duty `to
   further just arrangements not yet established', treating it as a
   `normative bridge' between institutional demands of distributive justice
   and individual responsibilities in non-ideal circumstances. I argue that
   this response strategy is unsuccessful. I show that the more unjust the
   status quo is due to non-compliance, the less demanding the natural duty
   of justice becomes. I conclude that, in non-ideal circumstances, the
   bulk of the normative work is done by another natural duty: that of
   beneficence. This conclusion has significant implications for how we
   conceptualize our political responsibilities in non-ideal circumstances,
   and cautions us against the tendency - common in contemporary political
   theory - to answer all high-stakes normative questions under the rubric
   of justice.},
