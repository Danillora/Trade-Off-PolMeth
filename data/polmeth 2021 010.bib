@article{ WOS:000606944800001,
Author = {Larkin, Craig and Nasasra, Mansour},
Title = {The ``inclusion-moderation{''} illusion: re-framing the Islamic movement
   inside Israel},
Journal = {DEMOCRATIZATION},
