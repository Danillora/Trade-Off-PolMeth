@article{ WOS:000702283600006,
Author = {Paez, Gustavo and Trejo, Yhimaina and Rondon, Karina and Gulfo, Nilsa},
Title = {An Approach to the Study of Suicide in Venezuela},
Journal = {URVIO-REVISTA LATINOAMERICANA DE ESTUDIOS DE SEGURIDAD},
