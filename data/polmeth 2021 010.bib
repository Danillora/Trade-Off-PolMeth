@article{ WOS:000642634600007,
Author = {Hendrickson, Petra},
Title = {Effect of Active Learning Techniques on Student Excitement, Interest,
   and Self-Efficacy},
Journal = {JOURNAL OF POLITICAL SCIENCE EDUCATION},
Year = {2021},
Volume = {17},
Number = {2},
Pages = {311-325},
Month = {APR 3},
Abstract = {What is the impact of active learning techniques on student excitement,
   interest, and self-efficacy in a course? An American Foreign Policy and
   National Security course was designed around the utilization of a number
   of active learning techniques, including simulations, a debate, and
   counterfactual analysis. Students in the course were surveyed about
   their feelings of excitement toward the course, interest in course
   material, and feelings that they could do well in the course as a result
   of each aspect of course design. Results of the surveys reveal that the
   active learning techniques did indeed increase students' excitement,
   interest, and self-efficacy, suggesting that courses utilizing course
   designs centered on student engagement and enjoyment could bolster
   students' overall learning in courses.},
