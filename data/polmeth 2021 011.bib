@article{ WOS:000751597500004,
Author = {Martinez de Bringas, Asier},
Title = {THE COMMON-INDIGENOUS IN THE FACE OF GLOBALIZATION. PLOTS TO UNDERSTAND
   THE STRUGGLES FOR DECOLONIZATION},
Journal = {REVISTA INTERNACIONAL DE PENSAMIENTO POLITICO},
Year = {2021},
Number = {16},
Pages = {65-89},
Abstract = {The objective of this work was to measure the expropriating and
   destructive character that colonialism has had and still has on the
   reality of indigenous peoples, and how colonialism continues to cast its
   shadows of destruction globally in times of ecological annihilation. We
   consider the indigenous-commons as the nucleus from which to understand
   the proposal of transformation and community resistance of indigenous
   peoples; the place that gives foundation and meaning to their political
   practice of rights. We have moved towards a political critique of the
   concept of Rights of Nature, in a double sense: as a critique of what it
   is not, of its misrepresentation by the State; as an alternative
   proposal to understand the centrality that the ecology of living beings
   has in indigenous worldviews. Finally, we conclude with what we consider
   the main fractures in the protection of indigenous rights, gaps that
   ultimately rest on the need to annul and destroy the indigenous
   understanding of life (s) due to the critical potential that they imply
   for the new colonial reason.},
