@article{ WOS:000704916000008,
Author = {Rivera-Perea, Tania},
Title = {Emotions, Senses and Potentialities in Toco canter. Travesia contra el
   Olvido: Critical Discourse Analysis of the Music About the Colombian
   Conflict Tania},
Journal = {CIENCIA POLITICA},
Year = {2021},
Volume = {16},
Number = {31},
Pages = {201-231},
Month = {JAN-JUN},
Abstract = {This paper analyzes the testimonial discourses contained in the musical
   compilation Toco cantor. Travesia contra el olvido through critical
   discourse analysis. The senses and emotionalities, that are mobilized by
   means of the musical language on such compilation, are pointed out, such
   as denunciation, resistance, mourning, healing, social cohesion, and
   memory construction. These are part of the thematic horizon of the
   concept of testimony. Each of the musical pieces are also contextualized
   as expressions of power relations and violence typical of the Colombian
   conflict. On the other hand, it is argued that the testimonial
   discourses, contained in the subaltern musical language can become
   transformative, and acquire political potential to insert themselves in
   the public sphere, and resist against hegemonic and domination
   discourses.},
Publisher = {UNIV NACL COLOMBIA, FAC DERECHO, CIENCIAS POLITICAS},
Address = {CIUDAD UNIV, EDIF 201, BOGOTA, 00000, COLOMBIA},
Type = {Article},
Language = {Spanish},
