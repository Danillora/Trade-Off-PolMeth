@article{ WOS:000709992300002,
Author = {Avelino, Flor},
Title = {Theories of power and social change. Power contestations and their
   implications for research on social change and innovation},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2021},
Volume = {14},
Number = {3},
Pages = {425-448},
Month = {SEP 2},
Abstract = {This paper proposes a meta-theoretical framework for studying power in
   processes of change and innovation. Power is one of the most contested
   concepts in social and political theory. This paper discusses seven
   prevailing points of contestation: Power over versus power to, centred
   versus diffused, consensual versus conflictual, constraining versus
   enabling, quantity versus quality, empowerment versus disempowerment and
   power in relation to knowledge. The paper reviews how different scholars
   have dealt with above-mentioned points of contestation and identifies
   how different theories of power can be translated into specific
   empirical questions to systematically explore power in processes of
   social change and innovation.},
