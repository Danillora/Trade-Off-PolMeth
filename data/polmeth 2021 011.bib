@article{ WOS:000672938100001,
Author = {Franke, Ulrich and Hofferberth, Matthias},
Title = {Proclaiming a prophecy empty of substance? A pragmatist reconsideration
   of global governance},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
