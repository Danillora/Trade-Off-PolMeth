@article{ WOS:000683935600001,
Author = {Bischof, Daniel and Cohen, Gidon and Cohen, Sarah and Foos, Florian and
   Kuhn, Patrick Michael and Nanou, Kyriaki and Visalvanich, Neil and
   Vivyan, Nick},
Title = {Advantages, Challenges and Limitations of Audit Experiments with
   Constituents},
Journal = {POLITICAL STUDIES REVIEW},
Year = {2022},
Volume = {20},
Number = {2},
Pages = {192-200},
Month = {MAY},
Abstract = {Audit experiments examining the responsiveness of public officials have
   become an increasingly popular tool used by political scientists. While
   these studies have brought significant insight into how public officials
   respond to different types of constituents, particularly those from
   minority and disadvantaged backgrounds, audit studies have also been
   controversial due to their frequent use of deception. Scholars have
   justified the use of deception by arguing that the benefits of audit
   studies ultimately outweigh the costs of deceptive practices. Do all
   audit experiments require the use of deception? This article reviews
   audit study designs differing in their amount of deception. It then
   discusses the organizational and logistical challenges of a UK study
   design where all letters were solicited from MPs' actual constituents
   (so-called confederates) and reflected those constituents' genuine
   opinions. We call on researchers to avoid deception, unless necessary,
   and engage in ethical design innovation of their audit experiments, on
   ethics review boards to raise the level of justification of needed
   studies involving fake identities and misrepresentation, and on journal
   editors and reviewers to require researchers to justify in detail which
   forms of deception were unavoidable.},
