@article{ WOS:000636015600001,
Author = {Plescia, Carolina and Sevi, Semra and Blais, Andre},
Title = {Who Likes to Vote by Mail?},
Journal = {AMERICAN POLITICS RESEARCH},
Year = {2021},
Volume = {49},
Number = {4},
Pages = {381-385},
Month = {JUL},
Abstract = {Interest in voting by mail has increased during the coronavirus as a way
   to avoid in person contact. In this study, we conducted a survey in
   February 2020 in the United States to examine citizen preferences to
   cast their ballot at a polling station, over the internet, or by mail.
   By including simultaneously internet and mail as alternative voting
   options to the polling station we aim to disentangle convenience (both
   alternative options are presumably more convenient) from novelty
   (internet is more novel than mail and polling station voting). We find
   that the person who likes voting by mail the most is an older
   White-American with little interest in politics; and the person who
   likes voting by mail the least is a younger African-American or Latino
   with high interest in politics. All in all, the biggest cleavage in
   citizens' preferences about how to vote is generational, not
   ideological.},
