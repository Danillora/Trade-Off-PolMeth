@article{ WOS:000626925900001,
Author = {Smith, Steven B.},
Title = {Isaiah Berlin and Leo Strauss: Notes Toward a Dialogue},
Journal = {CRITICAL REVIEW},
Year = {2020},
Volume = {32},
Number = {4, SI},
Pages = {539-555},
Month = {OCT 1},
Abstract = {Berlin and Strauss shared surprisingly compatible views about four
   matters of great importance. The first is the need for political
   philosophy, which Berlin traced to value pluralism and Strauss to the
   inherent incompleteness and contestability of our knowledge of politics,
   due to its comprehensive nature. Second, Berlin and Strauss each opposed
   social-scientific positivism: Berlin, because it contradicts human
   freedom and responsibility; Strauss, because it depends on an untenable
   and nihilistic distinction between facts and values. Third, both
   philosophers wished to place, in the ground currently occupied by
   positivist political science, the study of statecraft, embodied in such
   figures as Churchill and FDR. Finally, Berlin and Strauss agreed on what
