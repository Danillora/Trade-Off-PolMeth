@article{ WOS:000721592900008,
Author = {Madra, Yahya M.},
Title = {Late Neoclassical Economics and Its Modes of Negation: A Response to
   Chakrabarti, Glynos, and Primrose},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2021},
Volume = {33},
Number = {4},
Pages = {595-610},
Month = {OCT 2},
Abstract = {Despite the fact that psychoanalytical concepts are absent in Yahya M.
   Madra's Late Neoclassical Economics, the three commentaries on the book,
   by Chakrabarti, Glynos, and Primrose, read the text in a certain
   psychoanalytical key, and for good reason. Their readings demonstrate
   that a number of unnamed psychoanalytical models silently informed the
   discourse analysis mobilized throughout the text. Reading late
   neoclassical economics as an internally differentiated field of defense
   formations against contradictions, antagonisms, and conflicts that arise
   from the constitutive destructuredness of the economy, this response
   essay by Yahya M. Madra explores the applicability of the key
   psychoanalytical concepts of negation (repression, foreclosure,
   disavowal) for a discourse analysis of economic models.},
