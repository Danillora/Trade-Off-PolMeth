@article{ WOS:000637381400001,
Author = {Banack, Clark},
Title = {Ethnography and Political Opinion: Identity, Alienation and
   Anti-establishmentarianism in Rural Alberta},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
