@article{ WOS:000728600800001,
Author = {Smith, Robert C.},
Title = {Presidential Responsiveness to Black Interests From Grant to Biden: The
   Power of the Vote, the Power of Protest},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2022},
Volume = {52},
Number = {3},
Pages = {648-670},
Month = {SEP},
Abstract = {The Biden administration is the most responsive to Black interests since
   the presidency of Lyndon Johnson. This article places the
   administration's responsiveness in historical perspective by examining
   presidential responsiveness to Black interests, beginning with the
   presidency of Ulysses Grant, and focusing on the wk of Black protest and
   the Black vote in activating responsiveness. After a brief discussion of
   theories of presidential power and responsiveness to Black interests,
   the article examines the factors that shaped the Biden administration's
   moons ea, especially the role of the Black Lives Matter protests; the
   substantive content of the Biden policies; and their likely
   effectiveness in an era of partisan polarization and in the historical
