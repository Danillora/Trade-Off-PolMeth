@article{ WOS:000728285000011,
Author = {Hochschild, Jennifer and Piston, Spencer and Weaver, Vesla Mae},
Title = {My Group or Myself? How Black, Latino, and White Americans Choose a
   Neighborhood, Job, and Candidate when Personal and Group Interest
   Diverge},
Journal = {PERSPECTIVES ON POLITICS},
Year = {2021},
Volume = {19},
Number = {4, SI},
Pages = {1184-1204},
Month = {DEC},
Abstract = {Amid growing inequality within racial and ethnic groups, how do
   Americans decide where to live, where to work, and for whom to vote?
   While previous research has examined racial patterns in voting
   decisions, it provides less insight into individual-level decisions
   about neighborhoods, candidates, and employment-even while these
   decisions also organize the political world. We theorize about the role
   of a key variable stratifying these individual-level decisions:
   education. To test our argument, we analyze nationally representative
   survey data and a new survey experiment that varies incentives to leave
   one's racial group environment. We find that among Blacks and Latinos,
   but not whites, those with higher levels of formal education are
   disproportionately likely to respond to incentives to leave their own
   group. We conclude with reflections on the implications of this
   educational divide for intra-racial inequality.},
