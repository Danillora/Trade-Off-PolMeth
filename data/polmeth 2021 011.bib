@article{ WOS:000695354100005,
Author = {Sanchez Ruiz, Abraham and Mejia Reyes, Carlos and Camacho Ruiz, Marco
   Antonio},
Title = {Gendered Patterns in Torture Practices Committed by the Mexican Armed
   Forces (2011-2019)},
Journal = {AMERICA LATINA HOY-REVISTA DE CIENCIAS SOCIALES},
Year = {2021},
Volume = {88},
Pages = {81-99},
Month = {AUG},
Abstract = {This work analyzes the different experiences of victims of torture by
   the Mexican Army to observe the relationship between the methods
   employed and gender. The results reveal that the torture practices used
   differ by gender. While the methods of immobilization, asphyxia,
   electrification and mutilation were mainly used against men, women were
   more likely to suffer nudity, bruises on the genitals, harassment and
   rape. Furthermore, women disproportionately suffered violence compared
