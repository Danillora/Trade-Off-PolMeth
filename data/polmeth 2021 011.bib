@article{ WOS:000752223200014,
Author = {Graciele Nielsson, Joice and Dezordi Wermuth, Maiquel Angelo},
Title = {The domain of the female body: an approach to the public dimension of
   violence against women in Brazil},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2021},
Volume = {123},
Pages = {539-580},
Month = {JUL-DEC},
Abstract = {The article starts from the analysis of two recent phenomena that have
   affected Brazil and Latin America: a) the advance of a conservative
   front that seeks to discredit and the debate on gender issues; b) the
   alarming increase in violence against women. Based on this phenomenon, a
   reformulation of the classical theories about gender violence and its
   confinement to the private space (domestic) is proposed, as an exclusive
   result of affective relationships between men and women. The objective
   is to explore the existence of a public, political and state dimension
   of the crimes of patriarchy as a pedagogy of cruelty, presenting as a
   hypothesis, based on the studies of anthropologist Rita Segato, the
   consideration that patriarchal violence is structuring of the state
   model of colonial modernity, and, therefore, its reproduction is
   fundamental for the support and perpetuation of this power model. The
   first part traces the path from low intensity patriarchy to
   modern-colonial patriarchy, resulting in the female body as a political
   territory-body. In the second, it configures gender violence as
   patriarchal violence, that is, public and, therefore, political. It
   uses, in the investigation, the historical approach method and the
   inductive procedure method, in a qualitative character.},
