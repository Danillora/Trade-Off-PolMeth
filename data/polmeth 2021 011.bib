@article{ WOS:000756969200004,
Author = {Hurrelmann, Achim and Merand, Frederic and White, Stephen E.},
Title = {Eurosphere or Anglosphere? Canadian Public Opinion on Brexit and the
   Future of Transatlantic Relations},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2021},
Volume = {54},
Number = {3},
Pages = {571-592},
Month = {SEP},
Abstract = {Close relations with Europe, especially but not only with the United
   Kingdom, have long been a consensual tenet of Canadian foreign policy,
   which has supported European integration since the 1970s. The UK's
   withdrawal from the European Union threatens to upset this consensus.
   While Liberal Prime Minister Justin Trudeau has consistently cast Brexit
   in a negative light, Conservative leaders Andrew Scheer and Erin O'Toole
   have commented positively on the UK's leaving the EU. Did their
   polarized views resonate in the population? To answer this question,
   this article analyzes the results of an original survey of voting-age
   Canadians. Findings show a strong degree of correspondence between
   positions expressed in party discourse and preferences in the Canadian
   population. This raises the possibility that the difference between two
   models of transatlantic relations, which we call Eurosphere and
   Anglosphere, could emerge as a new fault line in Canadian foreign
   policy.},
