@article{ WOS:000705506200001,
Author = {Finger, Leslie K. and Gift, Thomas Creigh and Miner, Andrew},
Title = {DCPS or Sidwell Friends? How Politician Schooling Choices Affect Voter
   Evaluations},
Journal = {AMERICAN POLITICS RESEARCH},
Year = {2022},
Volume = {50},
Number = {3},
Pages = {326-335},
Month = {MAY},
Abstract = {Voters often rely on informational shortcuts, such as the background
   traits of politicians, to decide which candidates to support at the
   ballot box. One such background trait is family composition,
   particularly parental status. Research, however, has mostly overlooked
   whether the value-laden choices that politicians make regarding their
   families-like what neighborhoods they live in, where they worship, and
   what schools they send their children to-affect how constituents view
   them. We conduct a survey experiment in the U.S. that presents
   respondents with hypothetical biographies of politicians that randomly
   vary one of the most important decisions that politicians make regarding
   their families: whether to send them to public or private school. We
   find that: (1) voters are more inclined to vote for politicians with
   children in public school; and (2) this preference may be due to voters
   perceiving these politicians as both warmer and more committed to public
   services.},
