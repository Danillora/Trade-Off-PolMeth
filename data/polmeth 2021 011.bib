@article{ WOS:000721321100007,
Author = {Kamenova, Valeriya},
Title = {Internal democracy in populist right parties: the process of party
   policy development in the Alternative for Germany},
Journal = {EUROPEAN POLITICAL SCIENCE REVIEW},
Year = {2021},
Volume = {13},
Number = {4},
Pages = {488-505},
Month = {NOV},
Abstract = {For the past decade, populist left- and right-wing parties have been on
   the rise in Europe. Yet, there are only a few studies on the internal
   organizational dynamics of these parties. Drawing on a new and unique
   data from fieldwork observations and interviews with party members from
   the Alternative for Germany (AfD), this article examines the internal
   democratic mechanisms in policy formation. The AfD displays a high
   degree of internal participation - an important but contrasting addition
   to the comparative research on radical right parties, which fail to
   sustain a democratic internal organization and consistently adopt
   mechanisms to centralize power in the leadership. The findings from this
   field research suggest that populist parties may actually engage in
   meaningful intra-party democracy and internal deliberative practices to
   invigorate the connections between citizens and their party
   representatives.},
