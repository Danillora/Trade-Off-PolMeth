@article{ WOS:000617232400001,
Author = {Walther, Daniel and Hellstrom, Johan},
Title = {Government termination in Europe: a sensitivity analysis},
Journal = {WEST EUROPEAN POLITICS},
Year = {2022},
Volume = {45},
Number = {3, SI},
Pages = {591-611},
Month = {APR 16},
Abstract = {The aim of this article is to examine which factors robustly influence
   cabinet duration in Central and Eastern Europe (CEE). In this respect
   the article investigates the role political institutions (including vote
   of no-confidence procedures), parliamentary and cabinet attributes, and
   various contextual factors have on cabinet stability. By using a type of
   sensitivity analysis (i.e. Extreme Bounds Analysis) on a panel of about
   180 cabinets in CEE, it is possible to examine which factors
   consistently predict the probability of government survival. It is found
   that only a few factors are robust to alternative model specifications,
   namely: government type, fragmentation in parliament, the level of
   unemployment, and restrictive no-confidence votes procedures which make
   it more difficult for the opposition to bring down a government. Thus,
   the results indicate that cabinet duration in CEE is not primarily
   affected by sui generis factors to the region, but by factors of more
   general relevance.},
