@article{ WOS:000674904300001,
Author = {Fernandez Gomez, Jorge David and Pineda, Antonio and Gordillo-Rodriguez,
   Maria-Teresa},
Title = {Celebrities, Advertising Endorsement, and Political Marketing in Spain:
   The Popular Party's April 2019 Election Campaign},
Journal = {JOURNAL OF POLITICAL MARKETING},
Year = {2024},
Volume = {23},
Number = {2},
Pages = {123-148},
Month = {APR 2},
Abstract = {One of political marketing's theoretical tenets is the translation of
   commercial advertising techniques to politics. This paper focuses on
   celebrity endorsement as a commercial technique which is translated into
   political campaigning. Celebrity endorsement relates directly to
   celebrity politics, that is, the result of celebrity culture getting in
   touch with politics, as a way of revitalizing the offering of parties
   and candidates. In this context, our study analyzes the representation
   and use of celebrity politicians in Spain by the conservative Popular
   Party (PP) during the April 2019 national election. In order to shed
   light on the way celebrities were used in the PP's social media
   campaign, we content-analyze 1,117 Twitter messages taken from the
   party's profile, as well as conservative presidential candidate Pablo
   Casado's profile. Analytical variables include frequency of mentions,
   the strategic axis (rational/emotional) with which celebrities were
   depicted, the features which were highlighted, and the type of
   endorsement performed by the celebrities. Message topics and functions
   were also added to the analysis. Results indicate a weak and flawed
   strategic use of celebrities, who were scarcely mentioned, and depicted
   in a non-innovative, traditional fashion. Supplemental data for this
   article is available online at
   https://doi.org/10.1080/15377857.2021.1950099 .},
