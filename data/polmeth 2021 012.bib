@article{ WOS:000730381600005,
Author = {Theodore, Rachel},
Title = {ALEXIS DE TOCQUEVILLE, THINKER OF THE EGALITARIAN SOCIAL IMAGINATION OF
   MODERN DEMOCRATIC SOCIETIES},
Journal = {TOCQUEVILLE REVIEW},
Year = {2021},
Volume = {42},
Number = {2},
Pages = {81-103},
Month = {DEC 1},
Abstract = {In this article, we analyze, in the first instance, Tocqueville's
   concept of ``equality of conditions{''} by showing that this concept can
   be divided into three distinct equalities in his work: equality of
   consideration, equality of opportunity, and equality in political
   rights. By analyzing the case of American and French societies, we
   conceptualize equality of consideration as a matrix and ``imaginary{''}
   equality of modern democratic societies, which conditions the other two
   equalities. Then, we show how Tocqueville's concept of ``equality of
   conditions{''} is in fact a real ``social imaginary{''} that institutes
   and builds the democratic societies of our time.},
