@article{ WOS:000720235000001,
Author = {Baylor, Christopher},
Title = {Feminists, labor unions, and teachers' associations: a labored coalition},
Journal = {INTEREST GROUPS \& ADVOCACY},
Year = {2022},
Volume = {11},
Number = {1},
Pages = {75-108},
Month = {MAR},
Abstract = {Soon after their formation in the 1960s, second-wave feminist interest
   groups adopted a broadly liberal agenda and sought like-minded allies
   among unions. Shared ideology was inadequate to explain the ultimately
   successful formation of their coalition. Two of the largest labor
   allies, the AFL-CIO and National Education Association, worked together
   with feminist interest groups because of their leadership's perceived
   interests of their union members, changing in a shifting political
   environment. Though committed to feminist policy success, each group
   maintained its distinctive perspective, and only in hindsight does the
   coalition appear to be the culmination of ideological consistency.},
