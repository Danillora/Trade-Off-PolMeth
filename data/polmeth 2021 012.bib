@article{ WOS:000610459400001,
Author = {Baccini, Leonardo and Brodeur, Abel and Nossek, Sean and Shor, Eran},
Title = {Terrorism and Voting Behavior: Evidence from the United States},
Journal = {RESEARCH \& POLITICS},
Year = {2021},
Volume = {8},
Number = {1},
Month = {JAN},
Abstract = {This article examines the impact of terrorism on voting behavior in the
   United States. We rely on an exhaustive list of terror attacks over the
   period 1970-2016 and exploit the inherent randomness of the success or
   failure of terror attacks to identify the political impacts of
   terrorism. We first confirm that the success of terror attacks is
   plausibly random by showing that it is orthogonal to potential
   confounders. We then show that on average successful attacks have no
   effect on presidential and non-presidential elections. As a benchmark,
   we also rely on a more naive identification strategy using all the
   counties not targeted by terrorists as a comparison group. We show that
   using this naive identification strategy leads to strikingly different
   results overestimating the effect of terror attacks on voting behavior.
   Overall, our results indicate that terrorism has less of an influence on
   voters than is usually thought.},
