@article{ WOS:000706614500010,
Author = {Bernal Meza, Raul},
Title = {Liberation Theology in French Polynesie},
Journal = {IZQUIERDAS},
Year = {2021},
Volume = {50},
Month = {APR},
Abstract = {The article analyzes the hermeneutics of Liberation Theology in
   Polynesian thought. It is identified, as theological thought, from
   European authors who have studied Polynesian exegetes in their mother
   tongue. It is concluded that it is not related to that original of the
   Liberation Theology of Latin American origin, because its origin,
   interpretation and instrument as a word of God, is different, in spite
   of sharing the same denomination as biblical hemeneutics. However, it is
   a theology aimed at the peoples oppressed by European colonization.},
