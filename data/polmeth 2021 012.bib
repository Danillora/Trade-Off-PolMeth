@article{ WOS:000659906200001,
Author = {Sanchez Munoz, Cristina},
Title = {Hannah Arendt and the Weimar Republic: A hidden relation},
Journal = {REVISTA DE ESTUDIOS POLITICOS},
Year = {2021},
Number = {192},
Pages = {13-36},
Month = {APR-JUN},
Abstract = {This article shows Hannah Arendt's relationship with the Weimar Republic
   through her work. Although there are no explicit references to the
   Republic, the thesis of an implicit presence is maintained -a hidden
   relation-, which is shown in the implicit theming of relevant aspects of
   Arendt's analysis. In this sense, an analysis is offered first of all of
   the philosophical influences of the Weimar context, especially of her
   teacher Jaspers. Next, the problems that the Weimar Republic was
   dragging along are examined: the Jewish question and the decline of the
   Nation-State. Both problems lead to the lack of a solution to the rights
   of minorities. Finally, Arendt's ideas about the people's councils as a
   political system, present at the birth of the Republic, but also in
   other historical moments and in other political traditions, are
   presented. The article ends by pointing out Arendt's federalist
   approach, connected to the councils.},
