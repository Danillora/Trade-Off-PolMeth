@article{ WOS:000678860500001,
Author = {Mioni, Michele},
Title = {The ``Good Citizen{''} as a ``Respectable Worker:{''} State,
   Unemployment, and Social Policy in the United Kingdom and Italy, 1930 to
   1950},
Journal = {POLITICS \& POLICY},
