@article{ WOS:000746259500001,
Author = {Santos Gomez, David Ernesto},
Title = {Debates to invent an exceptionality: the concept of populism in Colombia
   and its relation to violence (1970-2010)},
Journal = {ANALECTA POLITICA},
Year = {2021},
Volume = {11},
Number = {21},
Pages = {245-271},
Month = {JUL-DEC},
Abstract = {This article analyzes the debates on the definition of the concept of
   populism in Colombia and its relation to the concept of violence between
   1970 and 2010. Following the footsteps of certain intellectuals and
   their hypotheses on violence, we also believe that this came as a
   consequence of several populist phenomena that did not come to power,
   such as gaitanismo and anapismo, or as antecedents of those that did, as
   in the case of uribismo. In our opinion, this link between populism and
   violence constructed by the authors is a fundamental part of the
   invention of Colombia as an exceptional case in Latin America. The work
   is developed thanks to different perspectives of the new intellectual
   history and tries to reconcile some of its theoretical and
   methodological tools to think of its own object of study and approach it
   from its own dimensions.},
