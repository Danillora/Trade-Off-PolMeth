@article{ WOS:000631961200001,
Author = {Terry, George Spencer and Makarychev, Andrey},
Title = {Performative Diplomacy and Popular Geopolitics: The Case of Russian
   Anti-COVID Assistance to Italy},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2022},
Volume = {69},
Number = {1, SI},
Pages = {83-91},
Month = {JAN 2},
Abstract = {The article looks at this response to the COVID-19 crisis through the
   theoretical prism of health diplomacy with a heavily performative
   essence at its core. First, we delve into how this event was popularly
   understood in Italy. Next, we explore how the Kremlin presented its
   Italian ventures through its own popular channels. Finally, we
   synthesize our findings, drawing conclusions regarding Russia's and
   Italy's subjectivity and sovereignty.},
