@article{ WOS:000706614500025,
Author = {Perez, Olivia Cristina},
Title = {Critical systematization of Brazilian academic interpretations about the
   2013 June Journeys},
Journal = {IZQUIERDAS},
Year = {2021},
Volume = {50},
Month = {APR},
Abstract = {This article addresses certain interpretations of Brazilian protests
   known as the June Journeys of 2013. The qualitative research found 134
   Brazilian academic publications, among scientific articles, theses and
   dissertations. Based on this material we devoloped a critical
   systematization of the literature on the Journeys in which we discern
   the arguments present in the texts on the origins, characteristics and
   legacies of the Journeys, problematizing some of these interpretations.
   We argue that June 2013 was a cycle of protests that involved a variety
   of actors, agendas and repertoires producing diverse legacies. In this
   way, we provide the reader with an understanding of the Journeys,
   pointing to the disputes and complexities of the phenomenon, thus
   avoiding certain generalizations about it.},
