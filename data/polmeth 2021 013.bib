@article{ WOS:000631430000012,
Author = {Micozzi, Juan Pablo and Lucardi, Adrian},
Title = {How valuable is a legislative seat? Incumbency effects in the Argentine
   Chamber of Deputies},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2021},
Volume = {9},
Number = {2},
Pages = {414-429},
Month = {APR},
Abstract = {In many developing countries, national legislative seats are considered
   less valuable than (subnational) executive positions. Even then,
   ambitious politicians may seek a legislative seat either (a) as a window
   of opportunity for jumping to an executive office; or (b) as a
   consolation prize when no better option is available. Using a regression
   discontinuity design adapted to a pr setting, we examine these
   possibilities in the Argentine Chamber of Deputies between 1983 and
   2011. In line with the consolation prize story, we find that marginal
   candidates from the Peronist party-which controls most provincial
   governorships-are more likely to be renominated and serve an additional
   term in the legislature, but not necessarily to jump to an executive
   office. The effect is stronger in small provinces.},
