Author = {Alagna, Mirko},
Title = {Four Walls. The House (and its Distruction) in Hannah Arendt's Thought},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
Year = {2021},
Volume = {33},
Number = {65},
Pages = {127-145},
Abstract = {In Arendt the domestic sphere is the other of the political sphere. In
   this contribution, however, the house will be described directly.
   Firstly, by reconstructing some of the different dichotomies included in
   the macro-differentiation between house and square; secondly by probing
   the connections and ties that bind house and square. Thirdly, by trying
   to read the modern emergence of the two poles ``society{''} and
   ``interiority{''} as a double process of simultaneous expansion and
   concentration of domestic semantics and domestic performances.},
