@article{ WOS:000792255300001,
Author = {Umit, Resul},
Title = {Rallies around the flag-draped coffins: the electoral effects of
   security force casualties in terror attacks},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2023},
Volume = {11},
Number = {1},
Pages = {53-62},
Month = {JAN},
Abstract = {Security forces are one of the main targets of political violence. This
   paper examines the effect of their casualties on electoral outcomes.
   Between two general elections in 2015, Turkey experienced a series of
   attacks that killed 153 members of its security forces. Based on the
   as-if random assignment of their funerals across the country, I estimate
   that government vote share increases in the funeral places of security
   force terror victims. However, in the localities with recurring
   funerals, it decreases by a similar percentage. These non-linear changes
   provide strong evidence for the rally theory.},
