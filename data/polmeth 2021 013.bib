@article{ WOS:000697738800003,
Author = {Powell, Stephen R. and Florea, Adrian},
Title = {Introducing the Armed Nonstate Actor Rivalry Dataset (ANARD)},
Journal = {CIVIL WARS},
Year = {2021},
Volume = {23},
Number = {2},
Pages = {177-206},
Abstract = {While research on interstate rivalries is abundant, scholarship
   examining nonstate rivalries remains limited. To address this
   shortcoming, in this article we introduce the Armed Nonstate Actor
   Rivalry Dataset (ANARD) - a dataset which captures dyadic rivalries and
   militarised disputes among armed nonstate actors in the Middle East and
   North Africa (MENA) between 1993 and 2018. We begin by explaining why
   fine-grained data on militarised interactions between armed nonstate
   organisations are needed for a comprehensive understanding of conflict.
   We then provide details of the data collection process and coding
   practices. Finally, we identify the contributions that ANARD can make to
   conflict research.},
