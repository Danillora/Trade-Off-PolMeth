@article{ WOS:000668640300001,
Author = {Ares, Macarena},
Title = {Issue politicization and social class: How the electoral supply
   activates class divides in political preferences},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
Year = {2022},
Volume = {61},
