@article{ WOS:000734517900001,
Author = {Duggan, Niall and Hooijmaaijers, Bas and Rewizorski, Marek and Arapova,
   Ekaterina},
Title = {Symposium: `The BRICS, Global Governance, and Challenges for South-South
   Cooperation in a Post-Western World'},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2022},
Volume = {43},
Number = {4, SI},
Pages = {469-480},
Month = {SEP},
Abstract = {Over the past decades, the BRICS (Brazil, Russia, India, China, South
   Africa) countries have experienced significant economic growth. However,
   their political voices in global governance have not grown on par with
   their economic surge. The contributions to the symposium `The BRICS,
   Global Governance, and Challenges for South-South Cooperation in a
   Post-Western World' argue there is a quest for emerging markets and
   developing countries to play a more significant role in global
   governance. There is a widening gap between the actual role of emerging
   markets and developing countries in the global system and their ability
   to participate in that system. However, for the moment, various domestic
   and international political-economic challenges limit this quest. To
   understand why this is the case, one should understand the BRICS
   phenomenon in the broader context of the global power shift towards the
   Global South.},
