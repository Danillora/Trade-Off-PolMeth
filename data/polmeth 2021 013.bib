@article{ WOS:000751885100005,
Author = {Rodeschini, Silvia},
Title = {``Women in the lead{''}. Images of gynecocracy in United States utopian
   tradition at the end of the 19th century},
Journal = {STORIA DEL PENSIERO POLITICO},
Year = {2021},
Volume = {10},
