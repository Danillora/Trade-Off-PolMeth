@article{ WOS:000642275700002,
Author = {Banda, Kevin K. and Carsey, Thomas M. and Curiel, John},
Title = {Incumbency status and candidate responsiveness to voters in two-stage
   elections beginning with a primary},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2021},
Volume = {31},
Number = {2},
Pages = {263-281},
Month = {APR 3},
Abstract = {Theories of representation suggest that candidates should respond
   ideologically to their constituency. Two-stage elections like those in
   the U.S. force candidates to decide which parts of their constituency
   they should respond to: citizens who are active enough to participate in
   primaries or those who only participate in general elections. We posit
   that non-incumbent candidates should mostly focus on the preferences of
   primary voters while incumbents should be largely unmoved by the
   preferences of either set of voters. We test these expectations using
   data from U.S. House and Senate contests and find support for our
   theory. Our results suggest that scholars should pay closer attention to
   the two-stage nature of U.S. elections when evaluating electoral
   responsiveness.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
