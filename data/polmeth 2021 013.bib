@article{ WOS:000740706000007,
Author = {Alvarez Itriago, Rosangel Mariela},
Title = {Venezuelan political system in the period 1999-2020: persistence or
   breach},
Journal = {REFLEXION POLITICA},
Year = {2021},
Volume = {23},
Number = {48},
Pages = {81-96},
Month = {DEC},
Abstract = {This article examines the changes that have occurred in the Venezuelan
   Political System during the period 1999-2020, under the presidency of
   Hugo Chavez and his successor, Nicolas Maduro, based on the analytical
   approach proposed by David Easton (1999) , and it is tried to understand
   up to where the Socialism of the XXI Century, a political project
   promoted in that period, anchored in personal leadership, the call for a
   broad popular mobilization and the proclamation of a new
   institutionality, introduces a series of tensions that tend to threaten
   the persistence of the democratic system. From a descriptive
   historical-documentary research, it is concluded that the primacy of the
   Executive Power over the rest of the public powers and the use of
   non-democratic resources show elements of authoritarian regression in
   the country.},
