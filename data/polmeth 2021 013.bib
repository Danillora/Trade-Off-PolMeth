@article{ WOS:000711095200012,
Author = {Sychra, Zdenek},
Title = {The Czech Republic's European Policy},
Journal = {OSTEUROPA},
Year = {2021},
Volume = {71},
Number = {4-6},
Pages = {205+},
Abstract = {Czech society's relationship to European integration has changed
   considerably over the past three decades. After accession to the EU in
   2004, a decade of enthusiasm was followed by ten years, during which
   Czech politics looked to Brussels, partly sobered, partly disillusioned,
   and domestic critics of the EU gained a greater hearing. The euro crisis
   and especially the issue of migration have led to a further cooling.
   Today, Czech politics is cultivating a pragmatic relationship with the
   European Union and seeks to derive advantages from its membership. But,
   in reality, many Czechs see European institutions as a foreign and
   threatening power that is undermining state sovereignty and social
   values.},
