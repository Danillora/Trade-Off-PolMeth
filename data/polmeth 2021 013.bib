@article{ WOS:000699007700001,
Author = {Chan, Nathan Kar Ming and Jasso, Francisco},
Title = {From Inter-Racial Solidarity to Action: Minority Linked Fate and African
   American, Latina/o, and Asian American Political Participation},
Journal = {POLITICAL BEHAVIOR},
Year = {2023},
Volume = {45},
Number = {3},
Pages = {1097-1119},
Month = {SEP},
Abstract = {Recent literature in race, ethnicity, and politics has assessed how
   minority linked fate, defined as ``the idea that ethnoracial minorities
   might share a sense of commonality that extends beyond their particular
   ethnoracial group to other ethnoracial groups (Gershon et al., in
   Politics Groups Identities 7(3):642-653, 2019),{''} shapes attitudes
   toward descriptive representation and support for coalition building.
   However, scholarship has yet to examine the influence of minority linked
   fate on political participation. We argue that similar to those who view
   the interests of co-ethnics as a proxy for their individual interests,
   Latina/os, Asian Americans, and African Americans who express linked
