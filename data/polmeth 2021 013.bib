@article{ WOS:000670883300001,
Author = {Clegg, Liam and Rogers, Chris},
Title = {Privileging privatisation: accounting practices and state transformation
   in the UK},
Journal = {BRITISH POLITICS},
Year = {2022},
Volume = {17},
Number = {4},
Pages = {452-468},
Month = {DEC},
Abstract = {How do accounting practices interact with processes of state
   transformation? Focusing on the privatisation of social housing in the
   UK, we clarify an important mechanism through which accounting practices
   served to constitute material incentives in favour of privatisation. Our
   archival research demonstrates that the UK government's atypical
   practice of including public corporations' liabilities in its own debt
   calculations shaped discussions and decisions over the transfer of
   public housing stock to non-state Housing Associations in the 1980s. By
   unpacking the constitutive relationship between accounting practices and
   material incentives, we advance and bring together scholarship on state
   transformation and the politics of accounting.},
