@article{ WOS:000762390900017,
Author = {Quiring, Manfred},
Title = {Discrimination across the Board Russia's Law on ``Foreign Agents{''}},
Journal = {OSTEUROPA},
Year = {2021},
Volume = {71},
Number = {8-9},
Pages = {241+},
Abstract = {Since 2012, Russia has had legislation on ``foreign agents{''}. This
   contradicts the Russian constitution and violates international
   conventions. Contradictory legal provisions, unclear criteria for
   classification as a ``foreign agent{''}, the constant expansion of lists
   among non-profit organizations, independent media, and individuals, as
   well as administrative arbitrariness create an atmosphere of fear and
   uncertainty. A decline of civil society, a decrease in transparency and
   citizen control, and a deterioration in communication between the
   authorities and society can be observed. This leads to increasingly poor
   governance.},
