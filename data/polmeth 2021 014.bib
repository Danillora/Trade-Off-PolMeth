@article{ WOS:000635992200001,
Author = {Korishetti, Sumedha},
Title = {Challenges to state control of territory: Comparative analysis of Yemen,
   Afghanistan and Myanmar},
Journal = {ASIAN JOURNAL OF COMPARATIVE POLITICS},
Year = {2022},
Volume = {7},
Number = {1, SI},
Pages = {115-131},
Month = {MAR},
Abstract = {States around the world have lost control over their territory to armed
   non-state actors, including states like Yemen, Afghanistan and Myanmar
   in the Asian region. This article aims to understand why these states
   are unable to exercise control over all of their territory. The study
   identifies and examines four major challenges faced by states in
   maintaining control over their territory - lack of state legitimacy and
   effectiveness, strategic motives of armed non-state actors,
   socio-economic motives of armed non-state actors and external
