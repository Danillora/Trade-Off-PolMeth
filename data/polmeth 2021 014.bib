@article{ WOS:000634590800001,
Author = {Vandeweerdt, Clara},
Title = {In-group interest cues do not change issue attitudes},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2022},
Volume = {10},
Number = {5},
Pages = {828-836},
Month = {OCT 20},
Abstract = {In this paper, I investigate whether people change their attitudes about
   societal issues when they learn that those issues affect others like
   them. In three pre-registered survey experiments, I find that these
   in-group interest cues have little to no effect on issue attitudes. This
   is true for social groups based on gender, race/ethnicity, and sexual
   orientation. People who closely identify with an in-group do not react
   more strongly to group interest information. Findings suggest that
   neither linked fate nor in-group favoritism necessarily cause people to
   care about issues affecting their group. They raise new questions about
   exactly when and why group memberships influence political attitudes.},
