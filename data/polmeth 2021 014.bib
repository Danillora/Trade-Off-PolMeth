@article{ WOS:000637381400003,
Author = {Gordon, Kelly},
Title = {Mobilizing Victimhood: Situating the Victim in Canadian Conservatism},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2021},
Volume = {54},
Number = {1},
Pages = {41-59},
Month = {MAR},
Abstract = {This article offers an examination of the discursive significance of the
   ``victim{''} in the Conservative Party of Canada through a critical
   discourse analysis of two key pieces of legislation (Bill C-10 and Bill
   C-36) tabled by the Harper Conservative government. The central argument
   contends that while all populist arguments may be a form of victim
   argument, not all conservative victim arguments are
   populist-particularly ones directed at issues related to women and
   gender equality. The article finds that, perhaps due to the reactive
   nature of conservative ideology, conservative politicians in Canada
   adopt an ``ambidextrous{''} approach to victims-mobilizing two distinct
   and, at times, contrasting sets of arguments. The article concludes by
   proposing two possible explanations for this ambidexterity, one stemming
   from the literature on organizational management and the other from
   theorizations of the reactive nature of conservative ideology.},
