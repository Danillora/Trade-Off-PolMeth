@article{ WOS:000828997900003,
Author = {Pender, Kieran},
Title = {Improving Workplace Culture: Lessons from the Legal Profession},
Journal = {AUSTRALASIAN PARLIAMENTARY REVIEW},
Year = {2021},
Volume = {36},
Number = {2},
Pages = {36-53},
Abstract = {In recent years, the legal profession has been rocked by allegations of
   sexual harassment against senior lawyers, barristers and judges, in
   Australia and elsewhere. At a local, national and international level,
   much effort has gone towards improving workplace culture in the
   law-addressing the drivers of inappropriate behaviour and properly
   responding to incidents when they arise. Given the structural
   similarities between parliamentary and legal workplaces, what lessons
   can those seeking to drive change in the Australian Parliament learn
   from cognate efforts in the legal profession? This article, drawing on
   the author's experience leading the International Bar Association's work
   in this field, identifies transferable best-practice for the
   parliamentary context. It considers the need for an inclusive campaign
   for workplace safety, flexible reporting models, quasi-independent
   oversight and efforts to address the wider context contributing to the
   unacceptable prevalence of sexual harassment.},
