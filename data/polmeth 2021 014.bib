@article{ WOS:000618137500001,
Author = {Eberle, Jakub and Smekal, Hubert and Ocelik, Petr and Krpec, Oldrich},
Title = {Political science in Central and Eastern Europe: integration with
   limited convergence in Czechia},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2021},
Volume = {20},
Number = {1},
Pages = {183-203},
Month = {MAR},
Abstract = {This article discusses the integration of scholars from Central and
   Eastern Europe (CEE) into the broader field of European political
   science. Evaluating data from 2000 to 2020, we ask whether CEE scholars
   managed to ``close the gap{''} stemming from the initially
   underdeveloped state of post-communist political science. We contend
   that the results are rather mixed: CEE scholars have been increasingly
   present, yet achieved only very limited access to the top levels and
   mostly remain in a position of dependency. Using the case of Czechia, we
   discuss the factors that have likely contributed to the perpetuation of
   this state of integration with limited convergence.},
