@article{ WOS:000634994400001,
Author = {Engler, Sarah and Brunner, Palmo and Loviat, Romane and Abou-Chadi,
   Tarik and Leemann, Lucas and Glaser, Andreas and Kubler, Daniel},
Title = {Democracy in times of the pandemic: explaining the variation of COVID-19
   policies across European democracies},
Journal = {WEST EUROPEAN POLITICS},
Year = {2021},
Volume = {44},
Number = {5-6, SI},
Pages = {1077-1102},
Month = {SEP 19},
Abstract = {In fighting the spread of COVID-19, the drastic measures undertaken by
   governments worldwide demonstrate a trade-off between public health and
   fundamental democratic principles. Yet this behaviour is not consistent
   across democracies, which motivates this paper to examine why some
   democracies were willing to constrain individual freedoms and
   concentrate power more than others during the pandemic's first wave.
   Creating two indices to measure the degree to which COVID-19 policies
   interfere with these democratic principles in 34 European countries, the
   analyses show that the large variation cannot be solely explained by
   pandemic-related factors. It is argued that the strong protection of
   democratic principles already established in `normal' times makes
   governments more reluctant to opt for restrictive policies. By
   highlighting how differences in policy responses are attributed to
   provisions guaranteeing individual liberties, this paper contributes to
   a better understanding of how democracies handle the democratic dilemma
   in times of crises.},
