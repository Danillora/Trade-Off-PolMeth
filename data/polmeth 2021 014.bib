@article{ WOS:000637329300006,
Author = {Yousef, Ayman and Ozcelik, Sezai},
Title = {Palestine: Reconciliation and Peacebuilding. Perspectives from the Civil
   Society Organizations},
Journal = {CONFLICT STUDIES QUARTERLY},
