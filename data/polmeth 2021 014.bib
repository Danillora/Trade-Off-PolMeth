@article{ WOS:000614820700001,
Author = {Godrej, Farah},
Title = {The tales we tell: Bombay, Mumbai and I},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2022},
Volume = {25},
Number = {5, SI},
Pages = {703-722},
Month = {JUL 29},
Abstract = {This essay traces the evolution of the city now known as Mumbai -
   formerly Bombay - intertwined with the author's life-experiences of
   being embedded in the city. Scholarly, literary, and journalistic
   treatments all assert that the city's renaming represents a move toward
   a fundamentalist, anti-secular and nativist form of Hindu supremacist
   politics. But this is also a form myth-making: the story of Bombay's
   epic cosmopolitanism and the story of its decline are both equally
   mythic. Deconstructing the myths inherent within their personal
   narrative and relationship to the city, the author attempts to uncover
   what lies beneath the death of `Bombay's' cosmopolitanism, as signified
   by the rise of `Mumbai.'},
