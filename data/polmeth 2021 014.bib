@article{ WOS:000641907400001,
Author = {Wimpy, Cameron and Whitten, Guy D. and Williams, Laron K.},
Title = {X Marks the Spot: Unlocking the Treasure of Spatial-X Models},
Journal = {JOURNAL OF POLITICS},
