@article{ WOS:000646932000001,
Author = {Weiss, Tobias and Koenig, Marina and Stecker, Christian and Mueller,
   Jochen and Blaette, Andreas and Lewandowsky, Marcel},
Title = {``Since Cologne{''}, and ``after Chemnitz{''}-key events in German
   parliamentary discourse},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2021},
Volume = {15},
Number = {1},
Pages = {39-80},
Month = {MAR},
Abstract = {New Year's Eve 2015 in Cologne and unrests in Chemnitz in 2018 are key
   events in recent German history that are closely linked to conflicts in
   migration an immigration policy. We evaluate how these events were
   processed in the parliamentary discourse in German state legislatures.
   To this end, we bring together qualitative and quantitative approaches
   to text analysis and analyze a comprehensive corpus of plenary debates
   that took place since the events at 2015-16 New Year's Eve. Our
   quantitative text analysis shows that Koln and Chemnitz have
   long-lasting impact as points of reference in parliamentary discourse.
   With the help of a framing analysis we identify parties' interpretative
   patterns. We find that the New Year's Eve in Cologne has challenged,
   however, not fundamentally changed the partisan interpretative patterns.
   Chemnitz 2018, by contrast, reveals new partisan interpretations as well
   as an increasing polarization between the AfD and the established
   parties.},
Publisher = {SPRINGER VIEWEG-SPRINGER FACHMEDIEN WIESBADEN GMBH},
