@article{ WOS:000728138900006,
Author = {Martinez-Franzoni, Juliana and Fernandez-Calderon, Adriana},
Title = {Expanded duties or expanded rights? A decade of conflicting state
   regulations of fatherhood in Chile, Costa Rica and Uruguay},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2021},
Volume = {41},
Number = {3},
Pages = {563-586},
Abstract = {In the last decade, the role of the State regarding paternity was
   addressed by presidents, con-gresses and civil society actors with
   increasing political power. Fatherhood and the defects and biases
   against it have been linked to social problems that transcend the family
   itself, such as child poverty and domestic violence. The comparative
   literature on recent political processes on this matter, however, is
   practically non-existent. Based on primary sources, this article
   analyzes a decade of transformation in the regulation of male
   breadwinning and care-giving, particularly when parenting goes to courts
   in Chile, Costa Rica and Uruguay, linking both to the political
   processes and accounting for their important variations. The article
   concludes that measures to promote parental rights or duties reflected
   the political orientation of left and conservative political parties and
   executive powers, respectively.},
Publisher = {PONTIFICIA UNIV CATOLICA CHILE, INST CIENCIA POLITICA},
Address = {AV VICUNA MACKENNA 4860, SANTIAGO, 00000, CHILE},
Type = {Article},
Language = {English},
Affiliation = {Martínez-Franzoni, J (Corresponding Author), Univ Costa Rica, San Jose, Costa Rica.
   Martinez-Franzoni, Juliana; Fernandez-Calderon, Adriana, Univ Costa Rica, San Jose, Costa Rica.},
DOI = {10.4067/S0718-090X},
ISSN = {0718-090X},
