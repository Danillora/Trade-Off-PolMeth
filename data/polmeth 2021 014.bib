@article{ WOS:000630919500002,
Author = {Kuhn, Patrick M. and Mellish, Samuel and Vivyan, Nick},
Title = {Psychological threat and turnout misreporting {*}},
Journal = {ELECTORAL STUDIES},
Year = {2021},
Volume = {70},
Month = {APR},
Abstract = {Direct survey measures of turnout often suffer from misreporting,
   particularly among non-voters. We investigate whether turnout
   misreporting in online surveys can be reduced by two new turnout
   question designs aimed at strengthening or buffering respondent?s
   self-integrity against the perceived psychological threat of admitting
   non-voting. Drawing on evidence from survey experiments embedded in vote
   validated online surveys after the 2016 UK EU Referendum, we find that
   neither technique significantly improves turnout reporting accuracy. Our
   findings inform innovations in survey measurement of turnout and
   sensitive survey topics more generally.},
Publisher = {ELSEVIER SCI LTD},
Address = {125 London Wall, London, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Kuhn, PM (Corresponding Author), Univ Durham, Sch Govt \& Int Affairs, Durham, England.
