@article{ WOS:000727304100001,
Author = {Arora, Maneesh and Stout, Christopher T.},
Title = {After the ballot box: How explicit racist appeals damage constituents
   views of their representation in government},
Journal = {RESEARCH \& POLITICS},
Year = {2021},
Volume = {8},
Number = {2},
Month = {OCT},
Abstract = {Recent scholarship finds that the use of explicit racial appeals can be
   an effective mobilization strategy in some situations. Consequently,
   U.S. politics has witnessed the resurgence of such appeals. Yet, the
   effects of racial appeals are not ensconced in electoral contexts, but
   may have potential downstream effects on interest representation and the
   ability for politicians to build broader coalitions This study uses a
   survey experiment to test the effect of exposure to racist comments used
   by a 2018 U.S. Senate candidate on perceived interest representation
   among Black and White respondents. The results show that Black and
   liberal White respondents who are exposed to these comments feel that
   the candidate is less able to represent their interests. Thus, we find
   that the use of explicit racial appeals potentially alienates a
   substantial proportion of people which could potentially lead to greater
   dissatisfaction with government.},
