@article{ WOS:000699733300001,
Author = {Trombetta, Federico and Rossignoli, Domenico},
Title = {The price of silence: Media competition, capture, and electoral
   accountability},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2021},
Volume = {69},
Month = {SEP},
Abstract = {Is competition in the mass media market an effective deterrent against
   media capture? Does it prevent political groups from influencing
   reporting? This paper shows that in some cases it does not. Building on
   the literature on media capture, the model highlights that, under fairly
   generic assumptions, high competition in the media market can drive the
   cost of media capture to zero, making capture easier. Moreover, it
   highlights conditions on the parameters where the effect of competition
   on capture is non-monotonic, i.e. capture may occur for levels of
   competition lower, but also higher, than those leading to media freedom.},
