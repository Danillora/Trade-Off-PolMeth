@article{ WOS:000645103000002,
Author = {Zubikova, Adela and Svejnova-Hoesova, Katarina and Chytil, Zdenek},
Title = {Selected Determinants of Employment of Persons in Post Productive Age on
   Labour Market in Czech Republic and Slovakia},
Journal = {POLITICKA EKONOMIE},
Year = {2021},
Volume = {69},
Number = {2},
Pages = {170-192},
Abstract = {Pension systems are strained with negative impacts on the state budget
   as a consequence of the endangering trend of worsening demographic
   structure and population aging. The aim of this paper is to map trends
   of employment rates of persons 65-69 years old in the Czech Republic and
   Slovakia between 1998 and 2017 and to analyse selected determinants
   affecting these rates. These determinants are then subjected to
   statistical and regression analysis using panel data for the period
   1998-2017. Based on the analysis results, the authors come to three
   conclusions for both countries: firstly, the increase in pensions is not
   a limiting factor in the employment of persons aged 65-69; secondly, the
   participation of persons with tertiary education is of great relevance,
   implying the need for support to tertiary education; thirdly, the
   results confirmed the significance of the health determinant of persons
   aged 65-69, ensuing the need for stimulating investment in health.},
