@article{ WOS:000810964600004,
Author = {Mihaila, Vlad},
Title = {``MISS ROMANIA{''} IN THE USA (1929): THE INTERPLAY BETWEEN BEAUTY AND
   POLITICS},
Journal = {ANALELE UNIVERSITATII BUCURESTI-STIINTE POLITICE},
Year = {2021},
Volume = {23},
Number = {1},
Pages = {59-85},
Abstract = {This article focuses on the links between political discourse and
   feminine beauty in one of Romania's first national beauty pageants. By
   selecting its ``Miss Romania{''} in March 1929 and sending her to
   compete in the international ``Miss Universe{''} pageant in Galveston,
   Texas, the popular magazine Realitatea Ilustrata sought to affirm its
   role in creating a visible symbol of the Romanian nation. ``Miss
   Romania{''} was promoted and legitimized as an incarnation of national
   unity capable of assuring internal cohesion and external renown. In this
   way, the idealization of the national beauty winner and her
   transformation into a symbol of collective virtue translated political
   and propagandistic ambitions in terms of feminine identity.},
