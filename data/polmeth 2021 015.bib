@article{ WOS:000700786700009,
Author = {Gabdrakhmanova, G. F.},
Title = {DISCURSIVE IDENTITY CONSTRUCTION IN MESSAGES TO THE REGIONAL PARLIAMENT.
   CASE OF THE REPUBLIC OF TATARSTAN},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2021},
Number = {5},
Pages = {146-160},
Abstract = {The article raises the topic of identity construction in public speeches
   of the leaders of regional subjects of the country. How and what
   identities do they build in discourse? What grounds are used by them for
   the population to identify with the region? Is the ongoing regional
   identity policy combined with an identity policy implemented at the
   national level? These issues are considered on the example of one of the
   constituent entities of the Russian Federation - the Republic of
   Tatarstan. Texts of the Messages of Tatarstan republic President R.N.
   Minnikhanov to State Council of the Republic of Tatarstan were explored
   using the theory of critical discourse analysis. This allowed using
   linguistic tools to reveal the symbolic space of public speeches of the
   regional leader. The purpose of their analysis was to identify tactics
   and techniques used by the regional leader in constructing the
   discursive reality of identity - all-Russian, regional, ethnic,
   religious, professional, gender. The head of Tatarstan speaks most of
   all about professional identity - about the accumulation of human
   capital in the territory entrusted to him, and about specific areas of
   the economy. The construction of all-Russian and regional identities
   illustrates the complexity of their construction and combination. The
   President of Tatarstan solves this problem with the idea of the
   consolidation of the Tatarstan community and the stability of the
   political system of the republic in the name of national security of
   Russia. He talks a lot about ethnic issues and the image of the region
   as a territory open to cooperation and creating favorable conditions for
   investment. All this becomes a brand of Tatarstan. Thus, the proposed
   approach takes into account various context-sensitive discursive and
   social practices of the discourse of the identity of a regional leader.},
Publisher = {NONCOMMERCIAL PARTNERSHIP EDITORIAL BOARD POLIS-POLITICAL STUDIES},
