@article{ WOS:000634869200006,
Author = {Iglesias-Pascual, Ricardo and Paloma, Virginia and Benitez, Isabel},
Title = {The role of contextual factors in the electoral resurgence of extreme
   right-wing forces in Spain: The case of Andalusia},
