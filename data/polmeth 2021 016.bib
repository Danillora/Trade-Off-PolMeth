@article{ WOS:000692584600001,
Author = {Brosius, Anna and Ohme, Jakob and de Vreese, Claes H.},
Title = {Generational Gaps in Media Trust and its Antecedents in Europe},
Journal = {INTERNATIONAL JOURNAL OF PRESS-POLITICS},
Year = {2022},
Volume = {27},
Number = {3, SI},
Pages = {648-667},
Month = {JUL},
Abstract = {We test generational differences in media trust and its antecedents,
   including political trust, interest, and orientation, as well as
   perceptions of media inaccuracy and media bias. We rely on original
   survey data from ten European countries, collected in 2019. We find no
   differences in the levels of media trust between generations, but we
   find that key correlates of media trust relate differently to it in
   different generations. For example, political interest is more strongly
   correlated with media trust for Millennials than for other generations.
   Perceptions of bias and inaccuracy have a strong negative correlation
   with media trust overall, but it is stronger for older generations.
   These results suggest, that in the long term, societal developments, and
   in particular debates about media bias and misinformation may impact
   media trust of young generations differently as they grow older-however,
   our data give no indication of that creating generational gaps in media
   trust.},
