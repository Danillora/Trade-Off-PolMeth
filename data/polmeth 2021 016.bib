@article{ WOS:000646849500001,
Author = {Thurman, Neil and Obster, Fabian},
Title = {The regulation of internet pornography: What a survey of under-18s tells
   us about the necessity for and potential efficacy of emerging
   legislative approaches},
Journal = {POLICY AND INTERNET},
