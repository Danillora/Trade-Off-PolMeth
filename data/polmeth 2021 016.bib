@article{ WOS:000756609900010,
Author = {Comi, Simona and Grasseni, Mara and Resmini, Laura},
Title = {Can judicial efficiency improve territorial attractiveness to FDI? The
   Italian experience},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2021},
Volume = {70},
Month = {DEC},
Abstract = {This paper explores the role that judicial efficiency may play as a
   determinant of inward FDI at sub-national level. Italy is an ideal case
   to deal with this still unexplored issue since the same law apply in all
   the national territory, but the degree of law enforcement varies
   considerably across different courts. We found that judicial efficiency
   affects positively FDI inflows. This result, though heterogeneous across
   different economic sectors, is robust to different specifications and
   sample selections. Our results have interesting policy implications,
   since they highlight the importance of non-targeted FDI policies as
   factors driving inward flows.},
