@article{ WOS:000701955800001,
Author = {Heap, Shaun Hargreaves and Talavera, Oleksandr},
Title = {Policy innovation through noise in implementation: Best to be grey (or
   silver) on Friday, in Halifax},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2023},
Volume = {70},
Number = {1},
Pages = {19-37},
Month = {FEB},
Abstract = {When the implementation of regulations requires judgement, there is
   bound to be noise in the application of the rules, but is there bias in
   the noise such that policy innovation, in effect, occurs in
   implementation? We use a recently available large postcode data set on
   the MOT road safety testing in the UK to answer this question. There is
   significant bias: the probability of failing varies systematically
