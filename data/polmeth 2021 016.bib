@article{ WOS:000658397300001,
Author = {Wagner, Gary A. and Elder, Erick M.},
Title = {Campaigning for retirement: State teacher union campaign contributions
   and pension generosity},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2021},
Volume = {68},
Month = {JUN},
Abstract = {Since Tullock (1972) first asked why there is so little money in U.S.
   politics, several studies have found evidence that political activism
   has the potential to yield significant returns. This study is the first
   to directly investigate the returns to public sector union activism by
   leveraging a transactions-level dataset from the National Institute on
   Money in Politics to estimate the rela-tionship between teachers' union
   campaign contributions and the generosity of teacher pension systems.
   Our results show that more politically engaged teachers' unions, as
   evidenced by their aggregate campaign contributions, are successful at
   both securing a higher level of retirement benefits and at shifting a
   greater burden of the financing of those benefits to the sponsoring
   gov-ernment. Our estimates are in line with recent related studies and
   imply an investment return on campaign contributions of nearly 1500\%.},
