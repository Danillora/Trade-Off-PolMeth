@article{ WOS:000730692000001,
Author = {Neff, Timothy and Pickard, Victor},
Title = {Funding Democracy: Public Media and Democratic Health in 33 Countries},
Journal = {INTERNATIONAL JOURNAL OF PRESS-POLITICS},
Year = {2024},
