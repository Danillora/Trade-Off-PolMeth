@article{ WOS:000695351300001,
Author = {Pennington, Mark},
Title = {Foucault and Hayek on public health and the road to serfdom},
Journal = {PUBLIC CHOICE},
Year = {2023},
Volume = {195},
Number = {1-2, SI},
Pages = {125-143},
Month = {APR},
Abstract = {This paper draws on the work of Michel Foucault and Friedrich Hayek to
   understand threats to personal and enterprise freedom, arising from
   public health governance. Whereas public choice theory examines the
   incentives these institutions provide to agents, the analysis here
   understands those incentives as framed by discursive social
   constructions that affect the identity, power, and positionality of
   different actors. It shows how overlapping discourses of scientific
   rationalism may generate a `road to serfdom' narrowing freedom of action
   and expression across an expanding terrain. As such, the paper
   contributes to the growing literature emphasising the importance of
   narratives, stories and metaphors as shaping political economic action
   in ways feeding through to outcomes and institutions.},
