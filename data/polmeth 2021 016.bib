@article{ WOS:000748999900001,
Author = {Zeman, Martin},
Title = {Estimation of Costs of Unemployment and Their Reflection in Public
   Budgets in the Czech Republic in 2017-2019},
Journal = {POLITICKA EKONOMIE},
Year = {2021},
Volume = {69},
Number = {6},
Pages = {627-650},
Abstract = {The aim of the presented research is to quantify the costs of
   unemployment in the Czech Republic in the years before start of the 2020
   exogenous crisis. The costs of unemployment are quantified as direct
   costs and induced costs. The research method is based on the
   con-struction of the average unemployed. I identify his/her average
   income level, length of unemployment and age, as well as level of
