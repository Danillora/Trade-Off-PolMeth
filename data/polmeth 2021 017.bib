@article{ WOS:000737901900006,
Author = {Bozkurt, Umut},
Title = {State-Bourgeoisie Relations under Neoliberalism with Turkish
   Characteristics},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
