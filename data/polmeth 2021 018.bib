@incollection{ WOS:000778573500002,
Author = {Tsimonis, Konstantinos D.},
Book-Author = {Tsimonis, KD},
Title = {`Juniority' and the generational subordination of party youth
   organizations},
Booktitle = {CHINESE COMMUNIST YOUTH LEAGUE: Juniority and Responsiveness in a Party
   Youth Organization},
Series = {China-From Revolution to Reform},
Year = {2021},
Pages = {47-79},
Abstract = {Chapter 2 develops the analytical framework of this book which
   concentrates on the institutionalization of youth's subordination as a
   social category and as a political subject. It discusses the Chinese
   mass organizations' efforts to negotiate dependency and respond to
   social demands since the 1980s, and the youth leagues' experiences as
   arenas of generational politics in former socialist regimes. It then
   develops this study's systematic approach on the subordination of youth
   in politics based on the works of Foucault, Bourdieu and Butler, and
   explains how the concept of `juniority' captures the process of
   conditioning youth as a `junior' political subject. It further explains
   the discursive and organizational ways juniority has been
   institutionalized in the League, rendering its adaptation a highly
   difficult proposition.},
