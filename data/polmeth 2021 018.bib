@article{ WOS:000692919400001,
Author = {Sebastian, Marcel},
Title = {The influence of social movements on policy change: delayed success in
   banning dog slaughter in Germany},
Journal = {SOCIAL MOVEMENT STUDIES},
Year = {2022},
Volume = {21},
Number = {6},
Pages = {766-781},
Month = {NOV 2},
Abstract = {The Animal Welfare Movement, among other things, aims to change animal
   welfare policies. Why certain animal welfare campaigns have been more
   successful than others remains unclear. This article examines the course
   of such policy changes in animal welfare based on the 1986 ban on dog
   slaughter in Germany. It answers the research question: `How can we
   explain the influence of the animal welfare movement on policy change
   regarding the ban on dog slaughter?' This article analyses the case of a
   long-delayed policy change, although the demand had received broad
   public and political support. Drawing on social movement studies, this
   article proposes that the delay is due to framing errors on the part of
   the animal welfare movement and argues that policy change could only
   take place through the help of allies in the field of politics. Using
   the process-tracing method, over 1,000 pages of policy documents and
   other relevant documents were analysed. The results show that the dog
   slaughter ban was delayed because policy makers feared that a morally
   motivated ban could be extended to other animal species. Only frame
   bridging with public health discourse by actors from the field of
   politics was able to overcome this barrier. This article contributes to
   social movement studies by analysing a case of frame bridging triggered
   by movement allies, thereby deepening our understanding of the relevance
   of the wider cultural environment of a contested policy and of
   interactions between movements and allies in policy change.},
