Language = {English},
Affiliation = {Farley, B (Corresponding Author), Virginia Commonwealth Univ, Wilder Sch Govt \& Publ Affairs, Richmond, VA 23284 USA.
   Farley, Bill, Virginia Commonwealth Univ, Wilder Sch Govt \& Publ Affairs, Richmond, VA 23284 USA.},
DOI = {10.1017/S089803062000024X},
Article-Number = {PII S089803062000024X},
ISSN = {0898-0306},
EISSN = {1528-4190},
Keywords = {FDR; Congress; Legislative Reform; Second Bill of Rights; congressional
