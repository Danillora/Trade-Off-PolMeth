@article{ WOS:000681177400001,
Author = {Pils, Eva},
Title = {Complicity in democratic engagement with autocratic systems},
Journal = {ETHICS \& GLOBAL POLITICS},
Year = {2021},
Volume = {14},
Number = {3},
Month = {JUL 3},
Abstract = {Autocratic control of civil society, including academia, can be extended
   to democratic societies and institutions in ways that pose threats to
   liberal-democratic values, such as academic freedom, for example through
   mechanisms and practices that lead to academic self-censorship. Engaging
   critically with the literature on `sharp power' and `authoritarian
   influencing' addressing this phenomenon, this paper argues that
   democratic actors who, without sharing the repressive goals of
   autocracies, contribute to their success in settings of international
