@incollection{ WOS:000861132000006,
Author = {Boer, Roland},
Book-Author = {Boer, R},
Title = {Towards Enmeshed Socialist Governance},
Booktitle = {FRIEDRICH ENGELS AND THE FOUNDATIONS OF SOCIALIST GOVERNANCE},
Series = {SpringerBriefs in Philosophy},
Year = {2021},
Pages = {91-112},
Abstract = {What forms of governance would emerge gradually during socialist
   construction? The answer has two parts. To begin with, public Gewalt
   loses its political character and becomes the administration of things
   and conduct of forces and relations of production, for the genuine good
   of society. While the statements on such matters from Engels (and Marx)
   are brief, Engels also drew from an abundance of information concerning
   pre-state forms of social organisation. He found complex and
   many-layered types of governance that were not separated from but stood
   `in the midst of society' (and thus could not be a `state'). We may
   speak of the `enmeshed governance' of `baseline communism', with its
   attendant form of democracy. What relevance does this material have for
   socialism, let alone communism? The answer is found in an 1882 work,
   `TheMark'. Based on research into pre-state forms of social organisation
   in German history, Engels points out that communism would entail a
   dialectical transformation (Aufhebung) of this baseline communism. This
   transformation would both negate the original baseline communism and
   transform its core features into a qualitatively different reality. In
   this situation, it becomes increasingly difficult to speak of a `state',
   let alone the separation of state and society.},
Publisher = {SPRINGER INTERNATIONAL PUBLISHING AG},
