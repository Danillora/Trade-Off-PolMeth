@article{ WOS:000630021400001,
Author = {Fahey, James J.},
Title = {Building Populist Discourse: An Analysis of Populist Communication in
   American Presidential Elections, 1896-2016},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2021},
