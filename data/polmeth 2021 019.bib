@article{ WOS:000685208000004,
Author = {Mostofa, Shafi Md and Subedi, D. B.},
Title = {Rise of Competitive Authoritarianism in Bangladesh},
Journal = {POLITICS AND RELIGION},
Year = {2021},
Volume = {14},
Number = {3},
Pages = {431-459},
Month = {SEP},
Abstract = {We examine the changing nature of an authoritarian regime, which is
   emerging from the social and political conditions shaped by the
   unconsolidated democracy in Bangladesh. Drawing on desk-based research
   combined with interviews from the field, we argue that the current form
   of the authoritarian regime in Bangladesh represents the characteristics
   of competitive authoritarianism. We find that authoritarianism in
   Bangladesh combines ``election manipulation{''} with three additional
   social and political mechanisms: ``marginalization of political
   oppositions{''} leading to the oppositional void, ``institutionalization
   of authoritarian policies,{''} and ``co-option of religious leaders.{''}
   By adding these new mechanisms of authoritarian politics and tracing the
   links between politics and religion, we aim to expand the theory of
   competitive authoritarianism and unpack the puzzle of democratic
   consolidation in Bangladesh.},
