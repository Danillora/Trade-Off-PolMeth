@article{ WOS:000756609900015,
Author = {Groll, Thomas and O'Halloran, Sharyn and McAllister, Geraldine},
Title = {Delegation and the regulation of US financial markets},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2021},
Volume = {70},
Month = {DEC},
Abstract = {We analyze the institutional determinants of U.S. financial market
   regulation with a general model of the policy-making process in which
   legislators delegate authority to regulate financial risk at both the
   firm and systemic levels. The model explains changes in U.S. financial
   regulation leading up to the financial crisis. We test the predictions
   of the general model with a novel, comprehensive data set of financial
   regulatory laws enacted specifically between 1950 and 2009. The
   theoretical and empirical analysis finds that economic and political
   factors impact Congress' decision to delegate regulatory authority to
   executive agencies, which in turn impacts the stringency of financial
   market regulation, and our estimation results indicate that political
   factors may have been stronger and resulted in inefficiencies.},
