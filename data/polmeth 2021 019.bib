@article{ WOS:000661075700001,
Author = {Sun, Jie and Makosa, Lewis and Yang, Jinkun and Yin, Fangyuan and Jachi,
   Moses and Bonga, Wellington Garikai},
Title = {Externalities of economic sanctions on performance of intra-industry
   non-sanctioned firms: Evidence from Zimbabwe},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2021},
Volume = {68},
Number = {5},
Pages = {643-664},
Month = {NOV},
Abstract = {This paper investigates the effect of targeted economic sanctions by the
   United States and the European Union on the performance of
   intra-industry non-sanctioned firms. Using data of non-sanctioned firms
   listed on the Zimbabwe stock exchange during the period 2009-2018, our
   regression results show that non-sanctioned firms in the same industry
   as sanctioned firms perform better than ordinary non-sanctioned firms,
   signalling the positive competitive effect. A mediating test suggests
   that sanctions increase the market share of non-sanctioned firms in the
   same industry as sanctioned firms and subsequently increase their
   performance.},
