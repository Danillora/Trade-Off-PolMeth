@article{ WOS:000689717400001,
Author = {Pickup, Mark and de Rooij, Eline A. and van der Linden, Clifton and
   Goodwin, Matthew J.},
Title = {Brexit, COVID-19, and attitudes toward immigration in Britain},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2021},
