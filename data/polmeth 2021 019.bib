@article{ WOS:000640988400004,
Author = {Burke, Ryan},
Title = {The Uncommon Defense Policy: History, Evolution, and Future Directions},
Journal = {JOURNAL OF POLICY HISTORY},
Year = {2021},
Volume = {33},
