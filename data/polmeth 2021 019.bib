@article{ WOS:000696392300001,
Author = {Zeiss, Jessica and Carlson, Les and Ball, A. Dwayne},
Title = {Uncalculated first-party externalities given a beverage tax},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2021},
