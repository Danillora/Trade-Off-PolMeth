@article{ WOS:000609525500001,
Author = {Ridout, Travis N. and Fowler, Erika Franklin and Franz, Michael M.},
Title = {The Influence of Goals and Timing: How Campaigns Deploy Ads on Facebook},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2021},
Volume = {18},
Number = {3},
Pages = {293-309},
Month = {JUL 3},
Abstract = {We examine how campaigns use advertising in the current digital era -
   and how spending decisions depend on the stage of the campaign and the
   particular goal being pursued. Our investigation relies on a database
   consisting of thousands of ads placed on Facebook by 24 different U.S.
   Senate campaigns in 2018. In the end, we find that campaigns pursue a
   large number of goals in their Facebook advertising and that the
   sequence of the campaign has a major influence on the goals pursued.},
