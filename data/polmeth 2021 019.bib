   Dutt, Pawan Kumar; Nyman-Metcalf, Katrin, Tallinn Univ Technol, TalTech Law Sch, Ehitajate Tee 5, EE-19086 Tallinn, Estonia.},
DOI = {10.2478/bjes-2021-0017},
