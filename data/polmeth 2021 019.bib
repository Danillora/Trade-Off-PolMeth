@article{ WOS:000634851200001,
Author = {Martinez-Ebers, Valerie and Branton, Regina and Calfano, Brian},
Title = {The Impact of Subjective Social Position on Public Evaluations of Police},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2021},
Volume = {102},
