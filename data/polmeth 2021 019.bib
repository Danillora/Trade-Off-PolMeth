@article{ WOS:000657987700001,
Author = {Heiberger, Raphael and Majo-Vazquez, Silvia and Castro Herrero, Laia and
   Nielsen, Rasmus K. and Esser, Frank},
Title = {Do Not Blame the Media! The Role of Politicians and Parties in
   Fragmenting Online Political Debate},
