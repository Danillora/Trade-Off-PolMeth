@article{ WOS:000691886200001,
Author = {Halimatusa'diyah, Iim and Prihatini, Ella},
Title = {Young voters and political participation in Indonesia: Revisiting a
   marriage gap},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2021},
Volume = {102},
Number = {6},
Pages = {2552-2564},
Month = {NOV},
Abstract = {Objectives This article responds to an understudied question on how does
   marital status make a difference in terms of voter turnout among young
   people in the Muslim world. Methods The article draws on the 2010 Muslim
   Youth Survey and focus on 1127 respondents aged between 15 and 25 in
   Indonesia. Using binominal logistic regression analysis, it seeks to
   examine how marital status affects Indonesian young people's voting
   behaviour in three types of elections held in 2009: legislative,
   presidential, and regional. Results The finding confirms that married
