@article{ WOS:000636102600001,
Author = {McGregor, Shannon C. and Barrett, Bridget and Kreiss, Daniel},
Title = {Questionably legal: Digital politics and foreign propaganda},
Journal = {JOURNAL OF INFORMATION TECHNOLOGY \& POLITICS},
Year = {2022},
Volume = {19},
Number = {1},
Pages = {1-17},
Month = {JAN 2},
Abstract = {In this study, we map the legal work seven U.S. digital consultancies
   and public relations firms undertook across social media and digital
   platforms of behalf of four foreign governments. We find these firms
   used a range of different strategies on social and digital media, very
   few of which featured legally required disclosures linking the content
   to their country of origin. Firms targeted journalists and other elites,
   but exactly how is not clear. Our most powerful findings regard what is
   absent. Our study reveals as much about the inconsistencies and
   inadequacies of the current FARA disclosure process and gaps in tech
   firms' ad archives as it does about the content and strategies of the
   messages themselves. We conclude with a series of recommendations for
   technology firms and the Department of Justice for enforcing FARA
   regulations as they relate to social and digital content.},
