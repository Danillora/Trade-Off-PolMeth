@article{ WOS:000716496700001,
Author = {Gomez-Aguinaga, Barbara},
Title = {Messaging ``en Espanol{''}: The Impact of Spanish Language on Linked
   Fate Among Bilingual Latinos},
Journal = {INTERNATIONAL JOURNAL OF PRESS-POLITICS},
Year = {2023},
Volume = {28},
Number = {3},
Pages = {627-647},
Month = {JUL},
Abstract = {While the number of U.S. residents who speak non-English languages at
   home is on the rise, little is known about the sociopolitical
   implications of exposure to minority languages among multilingual
   speakers in the United States. This study analyzes whether exposure to
   Spanish, a U.S. minority language, impacts perceptions of linked fate
   among bilingual Latinos, and if so, whether the consumption of ethnic
   media amplifies this effect. Through a population-based survey
   experiment among bilingual Latinos, this study finds that Latinos who
   are exposed to content in Spanish are more likely to report in-group
   linked fate than their counterparts exposed to the same message in
   English. Moreover, these effects are stronger among consumers of
   Spanish-language news. This study contributes to our understanding of
   the role of minority languages and ethnic media on pan-ethnic identities
   and highlights the importance of the growing linguistic diversity in the
   United States.},
