@article{ WOS:000810335600006,
Author = {Drabo, Daouda},
Title = {Nogaro's ``Positive Theory of Money{''}: Contribution to the Explanation
   of Hyperinflationary Phenomenon},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2021},
Volume = {131},
Number = {1},
Pages = {107-135},
Month = {JAN-FEB},
Abstract = {This article proposes a reconstitution of Nogaro's approach to monetary
   phenomena, and highlights his contribution to the explanation of
   hyperinflationary phenomena. To do this, we present his methodological
   approach and his conception of money: the historical method and monetary
   nominalism. These led him to propose an original interpretation of the
   hyperinflationary crises that emerged in the aftermath of the two Great
   World Wars. He emphasizes the importance of the exchange rate and
   monetary practices in the dynamics of hyperinflationary phenomena;
   elements of analysis that are later found among francophone monetary
   institutionalists and post-Keynesian authors. Nogaro has also sought to
   propose international monetary reform to remedy exchange rate
   instability.},
