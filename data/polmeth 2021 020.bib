@article{ WOS:000658726600009,
Author = {Almeida Diez, Adrian},
Title = {The ``first generation{''} of the West-German Red Army Faction. Legacies
   of the `68 Movement and the question of subjectivity (1970-1975)},
Journal = {HISTORIA Y POLITICA},
Year = {2021},
Number = {45},
Pages = {235-263},
Month = {JAN-JUN},
Abstract = {The aim of this article is to explain the ideological framework of the
   first generation of the Red Army Faction (RAF), a West-German armed
   organization. This framework will be linked to the question of
   subjectivity developed by the student movement in The Federal Republic
   of Germany in 1968. In order to achieve a logical explanation for these
   questions, firstly, we will consider summarizing the intellectual
   connection between the student movement and the theories developed by
   the Frankfurt School. Secondly, we will analyse the most important
   theoretical papers of the first generation of the Red Army Faction. As
   for the main results of this analysis, we could say that this terrorist
