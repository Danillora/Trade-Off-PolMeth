@article{ WOS:000652353300009,
Author = {Gaviria Mesa, Jorge Ivan and Granda Viveros, Monica Lucia},
Title = {Sovereignty and Environment in the 1991 Constitution. An Analysis of the
   Use and Effects of Glyphosate in the Fight Against Drugs in Colombia},
