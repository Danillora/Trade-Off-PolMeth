@article{ WOS:000685160100001,
Author = {Curiel, John A. and Clark, Jesse T.},
Title = {Disparities in Poll Closures in the Age of COVID-19: A Case Study of
   Wisconsin},
Journal = {ELECTION LAW JOURNAL},
Year = {2021},
