@article{ WOS:000685994300001,
Author = {Ozeren, Suleyman and Cubukcu, Suat and Cash, Gabriel},
Title = {Exposure to Extremist Content and Public Sympathy for ISIS},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
