@article{ WOS:000681124100003,
Author = {Haddad, Heidi Nichols and Cui, Isaac},
Title = {Localizing Rights Compliance: The Case for Cities as ``Shadow
   Reporters{''} at International Human Rights Treaty Bodies},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2021},
Volume = {43},
Number = {3},
Pages = {491-514},
Month = {AUG},
Abstract = {`` Shadow reports{''} by nongovernmental organizations (NGOs) and
   national human rights institutions (NHRIs) are commonplace within the
   international human rights treaty monitoring process. They became so for
   a simple reason: shadow reports improve the reporting process by
   providing useful information. This article contends that shadow reports
   from cities would do the same. Using the example of reports sent by the
   City of Berkeley, California, this article advocates for
   institutionalizing city shadow reporting because such reports can
   provide frontline information and help socialize cities into human
   rights compliance, even (and perhaps especially) when at odds with their
   national government.},
