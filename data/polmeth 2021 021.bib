@article{ WOS:000681124100006,
Author = {Freedman, Rosa and Lemay-Hebert, Nicolas and Monroy-Santander, Louis},
Title = {Seeking Justice for the Victims of Cholera in Haiti: Framing the
   Reparations Debate Through Transitional Justice},
Journal = {HUMAN RIGHTS QUARTERLY},
