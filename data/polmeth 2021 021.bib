@article{ WOS:000631374200001,
Author = {Thaler, Mathias},
Title = {What If: multispecies justice as the expression of utopian desire},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2022},
Volume = {31},
Number = {2, SI},
Pages = {258-276},
Month = {FEB 23},
Abstract = {One way of understanding calls for `multispecies justice' is to
   interpret them as utopian demands for a desirable future in which the
   structural anthropocentrism of conventional forms of morality, including
   environmental ethics, has been thoroughly abolished. I hope to clarify
   what kind of utopia multispecies justice might specifically entail.
   Mobilizing a conceptual framework developed by the science fiction
   author Octavia Butler, three potential plotlines for the utopia of
   multispecies justice are identified: the What-If, the If-Only and the
   If-This-Goes-On. Each of these engages the various tasks of utopianism
   in interestingly different ways. The key argument is that multispecies
   justice primarily raises a challenging What-If question: as a critical
   interrogation of how we should process and experience the world (and our
   place within it), its power derives from the `educated hope' to disrupt
   and reassemble outdated frameworks for making sense of the
   nature/culture divide.},
