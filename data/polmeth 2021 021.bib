@article{ WOS:000721086100001,
Author = {Souza Pimenta, Marilia Carolina B. and Garcia Suarez, Marcial Alecio and
   Ferreira, Marcos Alan},
Title = {Hybrid governance as a dynamic hub for violent non-state actors:
   examining the case of Rio de Janeiro},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2021},
Volume = {64},
Number = {2},
Abstract = {This paper aims to expand the concept of hybrid governance and analyses
   the case of Rio Janeiro, where criminal control coexists with the state.
   This study addresses the following research question: is Rio de Janeiro
   an important case for expanding the concept of hybrid governance in
   peace and security studies? The results show that not only can it be
   considered a space of hybrid governance, but also a dynamic hub with
   constant violence outbreaks with local and global impact.},
