@article{ WOS:000713131400001,
Author = {Jay, Zoe},
Title = {A tale of two Europes: How conflating the European Court of Human Rights
   with the European Union exacerbates Euroscepticism},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
Year = {2022},
Volume = {24},
Number = {4},
Pages = {563-581},
Month = {NOV},
Abstract = {This article focuses on the phenomenon of conflating the European Court
   of Human Rights with the European Court of Justice and European Union in
   British political and media discourse. Scholars of the European Court of
   Human Rights and Euroscepticism often acknowledge conflation, but rarely
   specify the forms it takes or its specific effects on British
   perceptions of the legitimacy of European institutions. This article
   identifies three main forms of conflation: muddled conflation, ambiguous
   conflation, and deliberate conflation. It shows that conflation can be
   both a symptom of deeper Eurosceptic disregard for the roles and
   purposes of distinct European institutions, and a deliberate rhetorical
   tool, intended to weaken the legitimacy of separate institutions by
   tying criticisms of one to the other. The article demonstrates that
   conflating the different Europes contributes to the persistence of
   Strasbourgsceptic narratives in the British political sphere by
   exacerbating pre-existing concerns and providing additional
   opportunities to raise them in public.},
