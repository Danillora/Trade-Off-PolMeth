@article{ WOS:000731294600008,
Author = {McAdam, Marika},
Title = {Continuing to pay the Price for Freedom: The Ongoing Detention of
   Victims After Their Trafficking Experience},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2021},
Volume = {43},
Number = {4},
Pages = {781-811},
Month = {NOV},
Abstract = {In 2010 the Human Rights Quarterly published findings of a 2008 study,
   revealing that victims of human trafficking are very often detained in
   public and private shelters following their trafficking experience, for
   months or even years. This article documents progress made in the decade
   since that initial study was conducted. Using desk and field-based
   research across three study countries (Malaysia, the Philippines and
   Thailand) it canvasses efforts made and challenges faced to secure free
   movement of victims. The reasons offered to rationalize the ongoing
   detention of trafficked victims in shelters are interrogated, and
   recommendations are offered towards ending that practice.},
