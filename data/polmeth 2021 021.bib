@article{ WOS:000642523500001,
Author = {Igechi, Gabriel Ozekhome},
Title = {Should Nigeria join the European Union's Economic Partnership Agreement
   with the other ECOWAS states?},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2021},
Volume = {48},
Number = {169},
Pages = {462-472},
Month = {JUL 3},
Abstract = {This debate piece explores the possible consequences for Nigeria in the
   event that she signs up to the Economic Partnership Agreement (EPA)
   between the European Union (EU) and the other Economic Community of West
   African States (ECOWAS) member states). It argues that in a global
   firmament driven by economic conflict, the struggle for resources and
   realist economic nationalism - and with cut-throat competition as the
   norm in international political economy - it will amount to folly for
   the nation to embrace a partnership whose ramifications are at best
   murky. The author thus questions whether the EPA signed by ECOWAS with
   the EU will be favourable to the Nigerian people.},
