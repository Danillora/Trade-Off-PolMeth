@article{ WOS:000744050700019,
Author = {Kaul, Nitasha},
Title = {The Misogyny of Authoritarians in Contemporary Democracies},
Journal = {INTERNATIONAL STUDIES REVIEW},
Year = {2021},
Volume = {23},
