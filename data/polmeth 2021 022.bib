@article{ WOS:000667986900001,
Author = {Asua, Samuel A. and Ugwueze, Michael I. and Onah, Vincent C.},
Title = {Piracy in the Gulf of Guinea: a threat to the means of livelihood of
   artisanal fishers in South South region, Nigeria},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2021},
Volume = {48},
Number = {169},
Pages = {452-461},
Month = {JUL 3},
Abstract = {The rising spate of piracy in the Gulf of Guinea, targeting artisanal
   fishery, is a huge concern regarding security and economy. Relying to a
   major extent on qualitative primary data, this briefing explores the
   economic effects of piracy by providing empirical evidence of how the
   piracy attacks in the Gulf of Guinea contribute to the increasing food
   security crisis among the coastline population in the South South
   region, Nigeria. It argues that the increasing piracy on artisanal
   fishery is a manifest sign of state fragility in the country.},
