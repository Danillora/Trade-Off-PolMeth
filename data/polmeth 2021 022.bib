@article{ WOS:000720754700006,
Author = {Pacewicz, Josh and Robinson, John N.},
Title = {Pocketbook policing: How race shapes municipal reliance on punitive
   fines and fees in the Chicago suburbs},
Journal = {SOCIO-ECONOMIC REVIEW},
Year = {2021},
Volume = {19},
Number = {3},
Pages = {975-1003},
Month = {JUL},
Abstract = {This article investigates a trend in the Chicago region that defies
   conventional accounts of municipal politics and revenue-motivated
   policing: since the Great Recession, higher-income black suburbs have
   sharply increased collection of legal fines and fees. To explain this,
   we draw on a study of municipal officials to develop a racialization of
   municipal opportunity perspective, which highlights how racial
   segregation in the suburbs intersects with policies that encourage
   competition over tax revenue to produce fiscal inequalities that fall
   along racial lines. Officials across the region shared views about
   `good' revenues like sales taxes paid mostly by nonresidents, but those
   in black suburbs were unable to access them and instead turned to `bad'
   revenues like legal fines to manage fiscal crises-even where residents
   were fairly affluent and despite the absence of discriminatory intent at
   the local level. These findings invite inquiry into the racially uneven
   consequences of seemingly colorblind municipal fiscal practices in the
   USA and the distributional consequences of municipal governance in other
   national contexts.},
Publisher = {OXFORD UNIV PRESS},
Address = {GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Pacewicz, J (Corresponding Author), Brown Univ, Dept Sociol, Providence, RI 02912 USA.
   Robinson, JN (Corresponding Author), Washington Univ, Dept Sociol, St Louis, MI 63110 USA.
   Pacewicz, Josh, Brown Univ, Dept Sociol, Providence, RI 02912 USA.
   Robinson, John N., Washington Univ, Dept Sociol, St Louis, MI 63110 USA.},
DOI = {10.1093/ser/mwaa029},
ISSN = {1475-1461},
EISSN = {1475-147X},
