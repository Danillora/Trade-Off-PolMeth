@article{ WOS:000664708100001,
Author = {Veillette, Anne-Marie},
Title = {Racialized Popular Feminism: A Decolonial Analysis of Women's Struggle
   with Police Violence in Rio de Janeiro's Favelas},
Journal = {LATIN AMERICAN PERSPECTIVES},
