@article{ WOS:000628917300001,
Author = {Pukelis, Lukas and Jastramskis, Mazvydas},
Title = {Prime ministers, presidents and ministerial selection in Lithuania},
Journal = {EAST EUROPEAN POLITICS},
Year = {2021},
Volume = {37},
Number = {3},
Pages = {466-480},
Month = {JUL 3},
Abstract = {In the semi-presidential system of Lithuania, a tradition to elect
   non-partisan presidents coexists with re-occurring conflicts between
   prime ministers and presidents over cabinet composition. We investigate
   what factors increase the probability of presidential activism in this
   field, i.e. when the president attempts to affect ministerial selection.
   We explain the outcomes of such activism by the electoral cycles and the
   political weakness of prime ministers and their party governments. More
   specifically, we argue that prime ministers are most vulnerable to
   presidential activism right after presidential elections and when they
   do not enjoy strong support in parliament. We test these hypotheses with
   a data set chronicling all instances of competition between the prime
   ministers and presidents over ministerial selection. The quantitative
   analysis is supplemented with qualitative insights from interviews with
   former cabinet members and high-level officials.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
