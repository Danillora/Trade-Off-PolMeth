@incollection{ WOS:000823723300004,
Author = {Kranich, Maximilian},
Editor = {Hauter, J},
Title = {The Great Patriotic War 2.0 An Analysis of Collective Violence in
   Eastern Ukraine after the Euromaidan Revolution},
Booktitle = {CIVIL WAR? INTERSTATE WAR? HYBRID WAR?: Dimensions and Interpretations
   of the Donbas Conflict in 2014-2020},
Series = {Soviet and Post-Soviet Politics and Society SPPS},
Year = {2021},
Volume = {227},
Pages = {61-86},
Abstract = {This contribution attempts to explain the appearance of the first
   collective violence in the Donbas in spring 2014 by applying a
   theoretical framework that focuses on the role of the myth of the
   ``Great Patriotic War.{''} It argues that a critical mass of the local
   population adopted the narrative of a ``Soviet fighter{''} who uses
   violence to defend his homeland against a ``fascist other.{''} People
   adopted this narrative in order to understand both their own role and
   the role of the new Kyiv authorities and their supporters during the
   Euromaidan revolution. Russian political elites and media played a
   crucial part by providing this narrative in the first place. The
   contribution concludes that local actors received sufficient legitimacy
   for their own resort to violence in the new social structure that was
   created in this process, which, in turn, led to the emergence of such
   violence.},
