@article{ WOS:000643534900007,
Author = {Mace, M. J. and Fyson, Claire L. and Schaeffer, Michiel and Hare,
   William L.},
Title = {Large-Scale Carbon Dioxide Removal to Meet the 1.5°C Limit: Key
   Governance Gaps, Challenges and Priority Responses},
Journal = {GLOBAL POLICY},
Year = {2021},
Volume = {12},
Number = {1, SI},
Pages = {67-81},
Month = {APR},
Abstract = {Parties to the UNFCCC and Paris Agreement have agreed to pursue efforts
   to limit the global average temperature increase to 1.5 degrees C. To
   meet this goal, the international community will have to aggressively
   reduce emissions and also remove CO2 from the atmosphere on an
   unprecedented scale, through an array of biological and technical Carbon
   Dioxide Removal (CDR) options. This paper considers governance
   challenges that arise from the need to rely on CDR to meet the Paris
   Agreement's long-term temperature goal. It looks at how heavy this
   reliance may have to be, over what timeframe, involving what options
   and, crucially, how best to ensure that CDR does not, while trying to
   address one problem, create many other challenges for sustainable
   development. After identifying the potential scale and pace of CO2
   removal needed to meet the 1.5 degrees C goal, we identify key
   governance gaps and challenges that arise from large-scale CDR
   implementation and propose a series of policy responses to be addressed
   by policy makers as a matter of priority, to enable CDR to contribute to
   1.5 degrees C-consistent pathways at the scale and pace required.},
