Affiliation = {Meyer, M (Corresponding Author), PSL Univ, i3 CNRS, Mines ParisTech, Ctr Sociol Innovat,UMR 9217, Paris, France.
   Meyer, Morgan; Vergnaud, Frederic, PSL Univ, i3 CNRS, Mines ParisTech, Ctr Sociol Innovat,UMR 9217, Paris, France.},
DOI = {10.3389/fpos.2021.731496},
