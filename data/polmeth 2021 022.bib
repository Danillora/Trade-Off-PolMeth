@article{ WOS:000620100600001,
Author = {Jacoby, Tim},
Title = {The Islamic ``State{''}: sovereignty, territoriality and governance},
Journal = {INTERNATIONAL POLITICS},
Year = {2023},
Volume = {60},
Number = {1},
Pages = {174-193},
Month = {FEB},
Abstract = {In this paper, four elements of statehood will be used to assess the
   Islamic State's (IS's) June 2014 claim of sovereignty, thereby raising
   important questions over its classification as a ``non{''} state armed
   actor. As such, it seeks to contribute to a small, but growing,
   literature which attempts to look beyond IS's virulent bellicosity and
   to consider the political institutions it established on their own
   terms-not as simply an extension of its prodigious messianic and
   eschatological diatribe or as an interstitial response to the failures
   of the Asad and Maliki regimes. In other words, rather than adding to
   the voluminous literature on the extent to which IS might be regarded an
   Islamic State, this paper considers to what degree it might have
   constituted an Islamic State.},
