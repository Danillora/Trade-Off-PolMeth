@article{ WOS:000733625800001,
Author = {Hedstrom, Jenny},
Title = {On violence, the everyday, and social reproduction: Agnes and Myanmar's
   transition},
Journal = {PEACEBUILDING},
Year = {2021},
Volume = {9},
Number = {4},
Pages = {371-386},
Month = {OCT 2},
Abstract = {This article brings into conversation feminist political economy with
   critical studies in peace and conflict to examine how Myanmar's
   transition is experienced though everyday gendered sites and with what
   consequences for women living in rural areas of the country, where lives
   are shaped as much by the actuality as the possibility of violence. The
   everyday is where these insecurities are felt, feared and negotiated. To
   illustrate this, I draw on the experiences of Agnes, a woman growing up
   within the context of prolonged conflict in rural Myanmar. I demonstrate
   how Agnes's home, and her bodily labour and vulnerability, is at the
   locus of a gendered political economy (re)produced both within the home
   and at the national level. 1 show how the transition has for women like
   Agnes resulted in a continuation of insecurity, challenging the
   legitimacy of Myanmar's neoliberal reform initiatives as a meaningful
   pathway towards sustainable peace and security.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
