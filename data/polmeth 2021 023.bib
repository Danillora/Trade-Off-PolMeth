@article{ WOS:000634477300001,
Author = {Krawatzek, Felix},
Title = {Remembering a Contentious Past: Resistance and Collaboration in the
   Former Soviet Union},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
Year = {2022},
