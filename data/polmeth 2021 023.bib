@article{ WOS:000649582200001,
Author = {Alberti, Carla and Diaz-Rioseco, Diego and Visconti, Giancarlo},
Title = {Gendered bureaucracies: Women mayors and the size and composition of
   local governments},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2022},
Volume = {35},
Number = {3},
Pages = {757-776},
Month = {JUL},
Abstract = {While women are underrepresented in politics, recent improvements in
   women's representation in legislative and executive bodies have spurred
   academic interest in the effects of electing women on a wide array of
   outcomes. Effects on bureaucracies, however, have received less
   attention. Do women mayors reform local bureaucracies differently than
   their men counterparts? We take advantage of rich administrative data
   from Chile to explore the effects of having a woman mayor on the size
   and gender composition of municipal bureaucracies. Using a regression
   discontinuity design in close electoral races, we find that women mayors
   reduce the size of local bureaucracies while simultaneously increasing
   the share of women public employees. Our findings thus show that women
   mayors' approach to bureaucratic reform once in office differs from that
   of their men counterparts, and contribute to existing research on the
   consequences of electing women.},
