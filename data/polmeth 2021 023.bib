@article{ WOS:000656118500001,
Author = {Cornut, Jeremie and Harris Rimmer, Susan Gail and Choi, Ivy},
Title = {The liquidification of international politics and Trump's (un)diplomacy
   on Twitter},
Journal = {INTERNATIONAL POLITICS},
Year = {2022},
Volume = {59},
Number = {2},
Pages = {367-382},
Month = {APR},
Abstract = {Former US President Donald Trump has caused international controversies
   on Twitter, fuelling conflict and spreading misinformation up until the
   suspension of the @realDonaldTrump account in January 2021. This paper
   shows that this Twitter undiplomacy furthered what Bauman called `liquid
   modernity'. For Bauman, new forms of communitarianism appear in liquid
   times, changing the way politics is conducted and how communities engage
   with strangers. We contend that Trump's use of Twitter has been a
   symptom of and an accelerating force in this transformation. Both the
   form-short aggressive messages-and the content-based on fear of
   strangers-of his messages accentuated the liquidification of modern
   society, the banalization of international politics and the uncivil
   treatment of strangers. This analysis advances critical diplomatic
   studies, the study of digital politics and International Relations on
   two levels: it clarifies the nature of the challenges that both Trump
   and Twitter posed to traditional diplomatic norms and practice; it
   builds understanding on how Twitter transforms the international public
   square, and how we might then try to make sense of diplomatic practice
   in the modern digital era. The case of Trump's engagement with the UK in
   2017 is taken as an illustration.},
