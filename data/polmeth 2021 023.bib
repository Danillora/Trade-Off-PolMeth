@article{ WOS:000641376100001,
Author = {Anastasio, Natalie and Perliger, Arie and Shortland, Neil},
Title = {How Emotional Traits and Practices Lead to Support in Acts of Political
   Violence},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2023},
Volume = {46},
Number = {10},
Pages = {1912-1932},
Month = {OCT 3},
Abstract = {The current study aims to assess the emotional processes that foster
   support for political violence. A survey of 1202 respondents was used to
   identify the association between emotional traits, support for extremist
   groups, and willingness to engage in political violence. Additionally,
   we tested how attitudinal traits' interaction with emotional factors may
   impact radicalization. Our findings indicate that aspects of anger and
   anxiety, as well as emotional triggers related to social interactions,
   are associated with increased support for political violence and
   extremism. Moreover, we identified important pathways in which
   attitudinal and emotional traits interact to facilitate radicalization
   further.},
