@article{ WOS:000700005500001,
Author = {Antunes de Oliveira, Felipe},
Title = {Lost and Found: Bourgeois Dependency Theory and the Forgotten Roots of
   Neodevelopmentalism},
Journal = {LATIN AMERICAN PERSPECTIVES},
