@article{ WOS:000667966500001,
Author = {Malone, David M. and Day, Adam},
Title = {THE UN AT 75. HOW TODAY'S CHALLENGES WILL SHAPE THE NEXT 25 YEARS},
Journal = {FORO INTERNACIONAL},
Year = {2021},
Volume = {61},
Number = {3},
Pages = {581-608},
Month = {JUL-SEP},
Abstract = {Against the backdrop of the 75th anniversary of the United Nations
   Organization (UN), celebrated in 2020, this paper takes stock of the
   situation and seeks to show how the strength lost by multilateralism in
   many of the most pressing issues on the global level has had a negative
   impact on several of the organization's fundamental activities. The
   situation is evaluated in four of the United Nation's major fields of
   activity: peace and security, international development, humanitarian
   aid and human rights. The paper argues that several of the significant
   advances made in each area now face the risk of setbacks or dilution. In
   each of these major issues, the main challenges facing the organization
   are analyzed, while proposing a number of lines of action that should be
   developed or towards which the UN should reorient its work.
   This review is of considerable relevance given the challenges the world
   faces today-particularly those related to climate change, but also the
   enormous demographic changes of the decades to come, and the impact of
   technology-can only be addressed through collective action, and the
   United Nations should play a central role in this regard.},
