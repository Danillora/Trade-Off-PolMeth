@article{ WOS:000619031200001,
Author = {Scott, Catherine},
Title = {The gender of dependency theory: women as workers, from neocolonialism
   in West Africa to the implosion of contemporary capitalism},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
