@article{ WOS:000625635700002,
Author = {Fortin, Katharine M. A.},
Title = {To be or not to be?: Legal Identity in Crisis in Non-international Armed
   Conflicts},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2021},
Volume = {43},
Number = {1},
Pages = {29-69},
Month = {FEB},
Abstract = {Examining the situations of Syria, Iraq, and Ukraine, this article
   demonstrates how individuals living outside the control of the de jure
   government struggle to access birth registration and civil status
   documentation in times of non-international armed conflict. Evaluating
   how the right to legal identity is protected in international law, the
   article highlights the need for legal identity to be better protected in
   armed conflict, so that its conferral and denial is not used as a weapon
   by the fighting parties.},
