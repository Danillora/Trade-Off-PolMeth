@article{ WOS:000675704200001,
Author = {Reyes-Carranza, Mariana},
Title = {Racial geographies of the Anthropocene: Memory and erasure in Rio de
   Janeiro},
Journal = {POLITICS},
Year = {2023},
Volume = {43},
Number = {2, SI},
Pages = {250-266},
Month = {MAY},
Abstract = {This paper interrogates the extent to which imaginaries of climate and
   ecological breakdown attend to the memories, knowledges, and experiences
   of communities already impacted by histories of racism, colonialism, and
   poverty. Drawing on insights from Black studies and decolonial thinking,
   the article reflects on how the causes and effects of anthropogenic
   climate change can be mapped onto geographies of racialised violence and
   social dispossession. Specific emphasis is given to Rio de Janeiro,
   notably its port area, a geographical space where future-oriented
   narratives remain oblivious to the city's history of anti-Black violence
   and Indigenous genocide. In parallel, the paper looks at the recently
   built Museum of Tomorrow and its public representations of the
   Anthropocene. Overall, the article contends that pluralising accounts of
   the Anthropocene might offer alternative epistemic entry points for
   understanding and interrupting the mounting ecological catastrophe.},
