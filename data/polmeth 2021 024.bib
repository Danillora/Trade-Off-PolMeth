@article{ WOS:000658034600001,
Author = {Cohen, Nissim and Aviram, Neomi F.},
Title = {Street-level bureaucrats and policy entrepreneurship: When implementers
   challenge policy design},
Journal = {PUBLIC ADMINISTRATION},
Year = {2021},
Volume = {99},
Number = {3, SI},
Pages = {427-438},
Month = {SEP},
Abstract = {Linking street-level bureaucracy with policy entrepreneurship is a new,
   ongoing trend in the literature. We claim that in various cases one must
   consider street-level policy entrepreneurship in order to fully
   understand policy processes. Therefore, we call on public policy and
   administration scholars to devote more resources to this important link.
   The symposium will promote our understanding of this still
   underresearched area. We will explore both theoretically and empirically
   how bureaucrats who interact with the public on a daily basis not only
   implement policy but also influence how it is designed. We will also
   assess the contribution of the symposium papers in light of the existing
   scholarly debate. We will conclude by reflecting on the challenges of
   studying street-level policy entrepreneurship and suggest future avenues
   for research in this field.},
