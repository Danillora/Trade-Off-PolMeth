@article{ WOS:000720377600001,
Author = {Duraskovic, Stevo},
Title = {Vladimir's Bakaric's Idea of a Socialist (Croatian) Nation: From
   Communist Historicism to Depoliticization},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
