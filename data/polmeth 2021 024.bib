@article{ WOS:000695258400001,
Author = {Massala, Thomais and Pearce, Nick},
Title = {Statecraft and incremental change: Explaining the success of pension
   reforms in the United Kingdom},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
