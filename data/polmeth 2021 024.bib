@article{ WOS:000618676800001,
Author = {Gutierrez, Jose Antonio D.},
Title = {Eating, Shitting and Shooting: A Scatological and Culinary Approximation
   to the Daily Lives of Rebels},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2023},
Volume = {46},
Number = {10},
Pages = {1817-1839},
Month = {OCT 3},
Abstract = {Everything that comes in, eventually goes out. One way or another. This
   paper will look into eating and defecating practices among rebels in
   Colombia, in particular, the FARC-EP. I will argue that these practices,
   despite being daily occurrences, have been overlooked in conflict
   studies. I will argue that eating practices in particular reinforced
   emotional bonds within the organization and fostered micro-solidarity.
   At the same time, eating and defecating practices reflected and
   reinforced, at once, organizational practices and ideological
   commitments of the rebels - particularly gender equality and
   collectivism. This paper, by focusing on cherished everyday activities
   which are deeply human, both biological and social, is a call to
   re-humanise this field of study.},
