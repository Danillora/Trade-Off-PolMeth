@article{ WOS:000652118100001,
Author = {Berube, Maxime and Beaulieu, Laurie-Anne and Mongeau, Pierre and
   Saint-Charles, Johanne},
Title = {Identifying Key Players in Violent Extremist Networks: Using
   Socio-Semantic Network Analysis as Part of a Program of Content
   Moderation},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2023},
Volume = {46},
Number = {12},
Pages = {2381-2399},
Month = {DEC 2},
Abstract = {Some moderation strategies of online content have targeted the
   individuals believed to be the most influential in the diffusion of such
   material, while others have focused on censorship of the content itself.
   Few approaches consider these two aspects simultaneously. The present
   study addresses this gap by showing how a socio-semantic network
   analysis can help identify individuals and subgroups who are
   strategically positioned in radical networks and whose comments
   encourage the use of violence. It also made it possible to identify the
   individuals and subgroups who act as intermediaries and whose statements
   are often the most violent.},
