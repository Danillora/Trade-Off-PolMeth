@article{ WOS:000607001400001,
Author = {McLeod, Laura},
Title = {Hoping for change: a snapshot of gender and constitutional reform in
   Bosnia and Herzegovina},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2021},
Volume = {24},
Number = {3},
Pages = {628-650},
Month = {SEP},
Abstract = {In October 2013, following almost two decades of exclusion from
   post-conflict constitutional debates in Bosnia and Herzegovina, gender
   advocates within civil society sought to publicly present a Women's
   Platform for constitutional reform. This article takes a snapshot of
   that moment - the juncture when civil society activists launched the
   Platform - to understand the role of hope at a time when attempts to
   mobilise for change occur. I explore how individual and collective
   performances of hope contribute to the process and practice of
   mobilising within post-conflict political initiatives. Investigating
   this offers us another way of paying attention to how affective
   processes `move' us, enabling the development of a nuanced perception
   about activism beyond simplistic narratives of success and failure in
   post-conflict contexts.},
