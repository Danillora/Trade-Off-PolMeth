@article{ WOS:000651816700007,
Author = {Crilley, Rhys},
Title = {Where We At? New Directions for Research on Popular Culture and World
   Politics},
Journal = {INTERNATIONAL STUDIES REVIEW},
Year = {2021},
Volume = {23},
Number = {1},
Pages = {164-180},
Month = {MAR},
Abstract = {A decade ago, scholars of international relations articulated a research
   agenda for the study of popular culture and world politics (PCWP), and
   since then a burgeoning literature has grown in this area. This article
   critically reflects on the research agenda put forward by Grayson,
   Davies, and Philpott and explores how recent scholarship has furthered
   the study of PCWP. In doing so, this article identifies four limitations
   of current research and suggests that if PCWP scholarship is to remain
   committed to understanding how power, identities, ideologies, and
   actions are made commonsense and legitimate, while also problematizing
   global inequalities and injustices, then it needs to pay greater
   attention to the analysis of four areas. These are (1) race,
   colonialism, and intersectionality in PCWP; (2) the impact of digital
   technology on PCWP; (3) the audience interpretation of PCWP; and (4)
   practices of making and producing PCWP.},
