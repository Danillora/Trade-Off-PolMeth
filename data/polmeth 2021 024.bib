@article{ WOS:000678780000001,
Author = {Ginosar, Avshalom},
Title = {Policy for the public without the public: Net neutrality in
   Israel(sic)(sic)(sic)Palabras clave},
Journal = {REVIEW OF POLICY RESEARCH},
