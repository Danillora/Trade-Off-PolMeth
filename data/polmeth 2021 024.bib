@article{ WOS:000714462900007,
Author = {McMullin, Caitlin and Roy, Michael J. and Curtin, Maeve},
Title = {Institutional logics as a framework for understanding third sector
   development: an analysis of Quebec and Scotland},
Journal = {POLICY AND POLITICS},
Year = {2021},
Volume = {49},
Number = {4},
Pages = {615-632},
Month = {OCT},
Abstract = {We compare the development of the third sector in Scotland and Quebec,
   which have developed ecosystems that distinguish them from the liberal
   non-profit regimes of the UK and Canada. We employ an institutional
   logics framework to consider how the rules, practices, values and
   beliefs of these `stateless nations' have formed unique structures and
   identities of the third sector that diverge from their broader national
   context. Our model demonstrates how the development of the welfare state
   and approaches to implementing social policy, government-third sector
   relationships, civic nationalism and solidarity interact in an iterative
   process to create distinct third sectors.},
