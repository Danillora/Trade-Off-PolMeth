@article{ WOS:000708254300001,
Author = {Silva, Patricia and Costa, Edna and Moniz, Joao},
Title = {A Portuguese Miracle: the Politics of the First Phase of Covid-19 in
   Portugal},
Journal = {SOUTH EUROPEAN SOCIETY AND POLITICS},
Year = {2022},
Volume = {27},
Number = {4, SI},
Pages = {521-549},
Month = {OCT 2},
Abstract = {Facing the first phase of the pandemic with exceptionally low
   casualties, Portugal was internationally praised as an outstanding
   example, particularly when compared to other Southern European
   countries. This article depicts how the pandemic crisis erupted in
   Portugal and how the Socialist government, the opposition and, to an
   important degree, the head of state responded to its demands, curbing
   the initial spread of the infection. The government ensured considerable
   levels of support from citizens and from other parties. The official
   opposition maintained its pragmatic and conciliatory tone during the
   confinement stages of the pandemic. It is argued that lower levels of
   political polarisation and a climate of cross-party collaboration are
   key factors in understanding the `Portuguese miracle'.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
