@article{ WOS:000652537100001,
Author = {Standish, Katerina},
Title = {Encounter Theory},
Journal = {PEACEBUILDING},
Year = {2021},
Volume = {9},
Number = {1},
Pages = {1-14},
Abstract = {This paper proposes Encounter Theory as a first principle of Peace and
   Conflict Studies (PACS). It seeks to challenge the notion that PACS is
   only cohered by a normative pursuit and will demonstrate that although a
   plethora of social scientific theories in PACS endure within the fields'
   diversity and multi-disciplinary make-up, there exists a conceptual
   premise that undergirds and drives much of the field. To establish how
   Encounter Theory functions, this article examines how the field of peace
   education as a process of conflict transformation functions via
   encounter using the work of Paolo Freire. The paper goes on to briefly
   examine four facets of Encounter Theory including connection,
   non-violent engagement, inter-relationship and humanisation to then
   illumine how those facets exist in eight forms of conflict
   transformation: conflict analysis, conflict resolution, mediation,
   negotiation, structured dialogue, intercommunal engagement, restorative
   justice and peace education.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
