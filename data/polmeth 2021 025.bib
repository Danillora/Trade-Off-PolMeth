@article{ WOS:000734307000001,
Author = {Tripathi, Dhananjay},
Title = {So similar yet so distant: border security management between India and
   Pakistan as a laboratory of non-experimentation},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2021},
Volume = {59},
Number = {4, SI},
Pages = {521-538},
Month = {OCT 2},
Abstract = {Borders on the Indian subcontinent are notable for the distinct lack of
   cross-border cooperation. Among the most intractable border dyads in the
   world, it is as volatile as it is securitised. Instead of facilitating
   trade and movement, both sides pursue a border policy to create a strong
   barrier. The puzzle that informs this article is the extent to which the
   bilateral border relationship is characterised by political animosity,
   despite a shared history and culture that the conventional approach to
   security communities would posit as enablers of coordination and
   cooperation.},
