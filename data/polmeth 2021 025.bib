@article{ WOS:000744050700011,
Author = {Wehner, Leslie E. and Thies, Cameron G.},
Title = {Leader Influence in Role Selection Choices: Fulfilling Role Theory's
   Potential for Foreign Policy Analysis},
Journal = {INTERNATIONAL STUDIES REVIEW},
Year = {2021},
Volume = {23},
