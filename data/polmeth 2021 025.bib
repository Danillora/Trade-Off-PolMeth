@article{ WOS:000731014600003,
Author = {Cabelka, Ondrej and Stepanek, Stanislav},
Title = {FRENCH COUNTERINSURGENCY IN MALI BEFORE AND AFTER THE COUP D'ETAT OF
   2020 Fallout from Task Force Takuba's Operational Activities},
Journal = {OBRANA A STRATEGIE-DEFENCE \& STRATEGY},
Year = {2021},
Volume = {21},
Number = {2},
Pages = {43-65},
Abstract = {The civil war in Mali is one of the longest-running conflicts in the
   African Sahel region. The conflict is now a typical example of an
   asymmetric conflict, with nation-state forces on one side and non-state
   armed actors on the other. This article describes the roots of the
   conflict, its evolution, France's involvement in counterinsurgency and
   counterterrorism operations, and the operational activities of Task
   Force Takuba. Finally, it provides an assessment of the overall issue,
   including an assessment of whether the objectives for which Task Force
   Takuba was formed have been met.},
