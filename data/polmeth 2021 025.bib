@article{ WOS:000738944300001,
Author = {Eisikovits, Nir and Feldman, Dan},
Title = {Al and Phronesis},
Journal = {MORAL PHILOSOPHY AND POLITICS},
Year = {2022},
Volume = {9},
Number = {2},
Pages = {181-199},
Month = {OCT 26},
Abstract = {We argue that the growing prevalence of statistical machine learning in
   everyday decision making - from creditworthiness to police force
   allocation - effectively replaces many of our humdrum practical
   judgments and that this will eventually undermine our capacity for
   making such judgments. We lean on Aristotle's famous account of how
   phronesis and moral virtues develop to make our case. If Aristotle is
   right that the habitual exercise of practical judgment allows us to
   incrementally hone virtues, and if AI saves us time by taking over some
   of those practical judgments, or if its pattern recognition capacities
   are very good at learning that kind of behavior - we risk innovating
   ourselves out of moral competence with the introduction of AI.},
