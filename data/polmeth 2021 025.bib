@article{ WOS:000695202700004,
Author = {Kostelka, Filip and Blais, Andre},
Title = {The Generational and Institutional Sources of the Global Decline in
   Voter Turnout},
Journal = {WORLD POLITICS},
Year = {2021},
Volume = {73},
Number = {4},
Pages = {629-667},
Month = {OCT},
Abstract = {Why has voter turnout declined in democracies all over the world? This
   article draws on findings from microlevel studies and theorizes two
   explanations: generational change and a rise in the number of elective
   institutions. The empirical section tests these hypotheses along with
   other explanations proposed in the literature-shifts in party/candidate
   competition, voting-age reform, weakening group mobilization, income
   inequality, and economic globalization. The authors conduct two
   analyses. The first analysis employs an original data set covering all
   post-1945 democratic national elections. The second studies
   individual-level data from the Comparative Study of Electoral Systems
   and British, Canadian, and US national election studies. The results
   strongly support the generational change and elective institutions
   hypotheses, which account for most of the decline in voter turnout.
   These findings have important implications for a better understanding of
   the current transformations of representative democracy and the
   challenges it faces.},
