@incollection{ WOS:000823726700006,
Author = {Horovitz, Liviu},
Editor = {Schmies, O},
Title = {A ``Great Prize,{''} But Not the Main Prize British Internal
   Deliberations on Not-Losing Russia, 1993-1995},
Booktitle = {NATO'S ENLARGEMENT AND RUSSIA: A Strategic Challenge in the Past and
   Future},
Series = {Soviet and Post-Soviet Politics and Society SPPS},
Year = {2021},
Volume = {229},
Pages = {85-111},
Abstract = {Recently declassified documents reveal that British officials recognized
   that Russia's transition in the early and mid-1990s offered unique
   opportunities. They also realized that NATO's potential enlargement
   could imperil the West's relationship with Moscow. Nonetheless, for
   British policymakers, securing America's place in Europe through NATO
   was the main prize in foreign affairs. The eastwards expansion of the
   Alliance was a necessary step to that end. In contrast, good relations
   with Russia were merely a ``great prize,{''} and complicated
   politico-strategic relations within Western Europe, transatlantic
   quagmires, and Russia's own uncertainties rendered that prize
   unattainable.},
