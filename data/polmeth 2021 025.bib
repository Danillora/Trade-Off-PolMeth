@article{ WOS:000686306300001,
Author = {Ball, Sarah and McConnell, Allan and Stark, Alastair},
Title = {Dramaturgy and crisis management: A third act},
Journal = {PUBLIC ADMINISTRATION},
Year = {2022},
Volume = {100},
Number = {3, SI},
Pages = {585-599},
Month = {SEP},
Abstract = {Dramaturgical perspectives have been used successfully in the past by
   crisis management researchers. However, previous contributions have been
   limited because they have been actor-centered, which has meant that they
   have tended to ignore the critical role that an audience can play in the
   drama of a crisis. This article therefore presents a ``third act{''} in
   which dramaturgical perspectives are used to deliver an
   actor-and-audience centered analysis of crisis management. This third
   act is built around the dramaturgical concept of ``characterization,{''}
   which we introduce to assess how an audience receives the symbolic
   outputs and discourses that are produced by crisis actors. After this
   theorizing, we present an analytical model, which will allow future
   researchers to analyze the interplay between actor, audience, and
   legitimacy when examining crisis. We conclude by illustrating the
   model's analytical capacity via an examination of the role of leaders
   and experts during the COVID-19 pandemic.},
