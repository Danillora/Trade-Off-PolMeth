@article{ WOS:000678426800001,
Author = {Malksoo, Maria},
Title = {Captive minds: the function and agency of Eastern Europe in
   International Security Studies},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2021},
Volume = {24},
Number = {4, SI},
Pages = {866-889},
Month = {DEC},
Abstract = {This article unpacks the ways Eastern Europe (broadly conceived) has
   featured as a space, trope, and scholarly origin in major International
   Security Studies (ISS) and International Relations (IR) journals over
   the past three decades. A framing and authorship analysis in 18
   disciplinary journals between 1991 and 2019 demonstrates how the region
   has been instrumental for the ISS subfield as an exemplary student of
   the Western theory and practice of IR. Eastern Europe has served as a
   symbolic space for exercising the civilising mission of the West and
