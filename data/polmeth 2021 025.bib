@article{ WOS:000772830500006,
Author = {Breault, Yann},
Title = {The new start of the Russian-American race for nuclear arsenals},
Journal = {ETUDES INTERNATIONALES},
Year = {2021},
Volume = {52},
Number = {1-2},
Pages = {123-145},
Month = {SPR-SUM},
Abstract = {The New start treaty on intercontinental missile which was due to expire
   in February 2021 has finally been extended for five years, much to the
   relief of many. However, the upcoming negotiations for nuclear arms
   control are going to be most difficult. With the end of the inf Treaty
   on Intermediate-Range Forces in August 2019 and the current reinvestment
   in these formerly banned weapons, talks are taking place in a context
   where arm race is intensifying. But what is its underlying logic ? To
   answer the question, this article traces its roots back to the early
   2000s, against the backdrop of nato enlargement and the deployment of
   the missile defence shield. It then explores the consequences of the
   renegotiation of power relations that took place during the Ukrainian
   and Syrian conflicts. Finally, it considers the impact of the Donald
   Trump's presidency, for whom it is no longer Russia, but communist
   China, that poses the main threat to American interests.},
