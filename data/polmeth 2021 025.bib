@article{ WOS:000651271800001,
Author = {Buzogany, Aron and Cetkovic, Stefan},
Title = {Fractionalized but ambitious? Voting on energy and climate policy in the
   European Parliament},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2021},
Volume = {28},
Number = {7, SI},
Pages = {1038-1056},
Month = {JUL 3},
Abstract = {Analysing roll call votes from the energy and climate policy field in
   the Eighth European Parliament (2014-2019), this article asks why has
   the European Parliament succeeded in maintaining its relatively
   ambitious position and how national and partizan factors explain voting
   behaviour of Members of the European Parliament (MEPs) on EU energy and
   climate legislation. We find the Eurosceptic vs. pro-EU cleavage to be
   the main conflict line structuring voting on energy and climate policy.
   Additionally, EU energy and climate policy has been supported by MEPs
   from member states with a track record of more ambitious climate
   policymaking and those with higher energy dependence. We show that
   increasing party fragmentation in the European Parliament has
   strengthened the influence of some progressive party groups,
   particularly the Greens, and has enhanced the European Parliament's
   ability to mobilize support for a relatively ambitious energy and
   climate legislation.},
