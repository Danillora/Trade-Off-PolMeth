@incollection{ WOS:000823723300009,
Author = {Brik, Tymofii},
Editor = {Hauter, J},
Title = {The Donbas and Social Science Terra Incognita?},
Booktitle = {CIVIL WAR? INTERSTATE WAR? HYBRID WAR?: Dimensions and Interpretations
   of the Donbas Conflict in 2014-2020},
Series = {Soviet and Post-Soviet Politics and Society SPPS},
Year = {2021},
Volume = {227},
Pages = {191-214},
Abstract = {This contribution explores challenges that social science research on
   Ukraine is facing in the light of the Donbas war. The issue is twofold:
   On the one hand, local Ukrainian research has been poorly integrated
   into international scholarship. As a result, international researchers
   experience difficulties identifying reliable local sources while the
   quality of local research has been hampered by outdated methods and
   concepts. On the other hand, nuances of the Ukrainian context have often
   been omitted by international scholars. Thus, theoretical models and
   empirical analyses have been hampered by ill-defined scope conditions
   and measurements. Taken together, this vicious circle has allowed the
   circulation of poorly justified ideas in the scientific discourse.
   Recently, there were some significant advances in breaking this cycle.
   Nevertheless, much more effort is needed to bring together the
   international state of the art regarding methodological standards with a
   rigorous understanding of the local context.},
