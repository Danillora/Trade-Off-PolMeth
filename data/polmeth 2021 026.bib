@article{ WOS:000684047100002,
Author = {Steele, Brent J.},
Title = {Nowhere to run to, nowhere to hide: inescapable dread in the 2020s},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2021},
Volume = {24},
Number = {4, SI},
Pages = {1037-1043},
Month = {DEC},
Abstract = {This article appraises Professor Rumelili's important central focus on
   anxiety by broadening the scope of the challenges the age of anxiety
   poses. With reference to recent events, such as the covid-19 pandemic
   and authoritarian politics, it argues that practices and strategies once
   thought to alleviate anxiety are now resources for it. The article
   concludes by calling for scholars to consider the possibility of anxiety
   as a structural feature of global politics, and organising our
   theoretical interventions, analyses, and politics around that
   constitutive feature. Ontological security, therefore, proves more
   elusive than ever before.},
