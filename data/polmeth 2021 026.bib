@incollection{ WOS:000823723300008,
Author = {Matsiyevsky, Yuriy},
Editor = {Hauter, J},
Title = {Internal Conflict or Hidden Aggression Competing Accounts and Expert
   Assessments of the War in Ukraine's Donbas},
Booktitle = {CIVIL WAR? INTERSTATE WAR? HYBRID WAR?: Dimensions and Interpretations
   of the Donbas Conflict in 2014-2020},
Series = {Soviet and Post-Soviet Politics and Society SPPS},
Year = {2021},
Volume = {227},
Pages = {165-190},
Abstract = {This contribution examines two issues-competing scholarly accounts of
   the military events in the Donbas and Ukrainian experts' opinion on the
   factors that led to the beginning of violence. Its first part reveals
   the existence of two competing academic discourses clustering around the
   concepts of civil war and hybrid war, as well as the so-called pragmatic
   discourse which recognizes the presence of both phenomena at the same
   time. While authors who support the civil war discourse face the
   challenge of accounting for Russia's role in the war, proponents of the
   hybrid war discourse underestimate the loss of Ukraine's monopoly over
   the legitimate use of force during a period of revolutionary change. The
   second part of this contribution surveys the weight of factors
   contributing to the conflict in the assessments of Ukrainian experts.
   Surveys conducted by pairwise comparison in 2015 and 2016 demonstrate
   that most Ukrainian experts assign the largest weight to Russia's direct
   and indirect interference and the destructive role of local elites as
   primary factors behind the conflict. Many of their Western counterparts,
   however, look at the events through the prism of civil war. Differences
   in the two perspectives are, apparently, not due to facts but to
   pre-existing images and ways of thinking which affect the perception of
   these events.},
