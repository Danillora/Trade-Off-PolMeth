@article{ WOS:000632235800011,
Author = {Ganor, Boaz},
Title = {Targeted Killings: Ethical \& Operational Dilemmas},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2021},
Volume = {33},
