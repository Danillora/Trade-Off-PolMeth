@article{ WOS:000731294600006,
Author = {Mazique, Rachel},
Title = {Deaf Rights as Human Rights: Delimiting the Human with Literatures of
   ``The Hearing Line{''}},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2021},
Volume = {43},
Number = {4},
Pages = {736-758},
Month = {NOV},
Abstract = {Bringing together the heretofore separate theorizations of Deafnicity
   and the hearing line, this article shows how reading the hearing line in
   literature becomes crucial to understanding Deafnicity and promoting
   Deaf rights. This article presents Angeline Fuller-Fischer's Scenes in
   the History of the Deaf and Dumb and Annie C. Dalton's To Viola Meynell
   as resistance literature that countered Viola Meynell's advancement of
   the international doctrine of eugenics in We Were Just Saying. Speaking
   to Sign Language Peoples' contemporary arguments for human and group
   rights, Angeline Fuller-Fischer and Annie C. Dalton redefined what it
   means to be deaf, a hearing ally, and ``human.{''}},
