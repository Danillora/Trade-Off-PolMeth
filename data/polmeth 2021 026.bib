@article{ WOS:000716510200001,
Author = {Castro, Lucas Crivelenti e},
Title = {Brazilian Dependent Capitalism Under the Hegemony of Financialized
   Capital},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2022},
Volume = {49},
