@incollection{ WOS:000824835900003,
Author = {Wells, Tamas},
Book-Author = {Wells, T},
Title = {Elucidating the meaning of democracy through narrative},
Booktitle = {NARRATING DEMOCRACY IN MYANMAR: The Struggle Between Activists,
   Democratic Leaders and Aid Workers},
Series = {Global Asia},
Year = {2021},
Pages = {31-53},
Abstract = {As opposed to the assumption of an `ideal type' of democracy - which is
   present in much mainstream democratisation scholarship - the second
   chapter in this book begins by drawing on W. B. Gallie's notion of the
   `essential contestability' of democracy. Meanings given to the word
   `democracy' are inevitably open-ended and dependent on context. Recent
   interpretive studies have established the context- specif ic nature of
   meanings of democracy and sought to `elucidate' meanings of democracy on
   their own terms. Chapter Two extends this work to show how attention to
   narratives can ground and locate meanings of democracy and expose the
   ways in which they can be wielded as political tools.},
