@article{ WOS:000735405300001,
Author = {Elias, Barbara},
Title = {Why rebels rely on terrorists: The persistence of the Taliban-al-Qaeda
   battlefield coalition in Afghanistan},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2022},
Volume = {45},
Number = {2, SI},
Pages = {234-257},
Month = {FEB 23},
Abstract = {Why do some rebels form persistent battlefield coalitions with terrorist
   groups? I argue three factors are likely to condition whether the
   benefits of doing so outweigh the costs: 1) potential rivalry among
   coalition members' political agendas, 2) the military, organizational,
   and political value terrorists can bring to the collective, and 3) the
   degree to which local partnerships are institutionalized or reinforced
   through threats of intra-coalitional violence. Analyzing previously
   classified and captured primary source documents, I examine the
   Taliban's persistence in forming battlefield coalitions with al-Qaeda in
   Afghanistan (1979 - present) and find significant empirical support for
   the argument.},
