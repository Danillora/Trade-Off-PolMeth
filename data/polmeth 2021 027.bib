@article{ WOS:000604695400001,
Author = {Bendik-Keymer, Jeremy},
Title = {Beneficial relations between species \& the moral responsibility of
   wondering},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2022},
Volume = {31},
Number = {2, SI},
Pages = {320-337},
Month = {FEB 23},
Abstract = {Some concepts within the development theory known as the `capabilities
   approach' can articulate multi-species justice. To do so, they should
   begin with `biocentric wonder' and its correlate, the `biocentric
   freedom' of the multiple forms of life on Earth. These lead to
   relational freedom supporting eco-centric considerations of a sort. In
   such freedom, `multi-species fields' are useful for articulating
   multi-species justice when the relational freedom within them indicates
   beneficial relations between species. At the same time, wondering over
   the life forms left out of the field becomes a moral responsibility.
   There are limits to coming to multi-species justice in this way. Here, I
   (1) provide concepts for a `green' capability approach and (2) enable
   the capability approach to make some sense of lands, waters, and skies
   as moral communities as many Indigenous societies do. In this way, the
   development philosophy avoids some hermeneutical injustice toward some
   Indigenous cultures.},
