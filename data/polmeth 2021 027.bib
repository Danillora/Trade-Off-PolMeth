@article{ WOS:000667401900001,
Author = {Jennings, Will and Stoker, Gerry and Valgarosson, Viktor and Devine,
   Daniel and Gaskell, Jennifer},
Title = {How trust, mistrust and distrust shape the governance of the COVID-19
   crisis},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2021},
Volume = {28},
Number = {8, SI},
Pages = {1174-1196},
Month = {AUG 3},
Abstract = {It is commonplace to claim that trust is essential to effective
   governance in many contexts, including that of a public health crisis
   such as the COVID-19 pandemic. We argue that trust is better understood
   as a family of concepts - trust, mistrust and distrust - and each of
   these may have different implications for the governance of COVID-19.
   Drawing on original measures tested through nationally representative
   surveys conducted in Australia, Italy, the UK and the USA between May
   and June 2020, we explore how these distinct types of trust are
   associated with citizens' perceptions of the threat posed by COVID-19,
   and their behavioural responses to it. We show how public policy
   dynamics around the COVID-19 crisis are driven by each of the trust
   family members and that policymakers might gain more from promoting an
   information-seeking and mistrusting society, rather than a trusting one.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
