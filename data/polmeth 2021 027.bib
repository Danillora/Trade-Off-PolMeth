@incollection{ WOS:000823723300007,
Author = {Hauter, Jakob},
Editor = {Hauter, J},
Title = {Delegated Interstate War Introducing an Addition to Armed Conflict
   Typologies},
Booktitle = {CIVIL WAR? INTERSTATE WAR? HYBRID WAR?: Dimensions and Interpretations
   of the Donbas Conflict in 2014-2020},
Series = {Soviet and Post-Soviet Politics and Society SPPS},
Year = {2021},
Volume = {227},
Pages = {147-163},
Abstract = {Drawing the dividing line between civil and interstate war can be a
   difficult task. This task is made even more difficult by a gap in the
   current typology of armed conflict. The conflict studies literature in
   general and the coding rules of the Uppsala Conflict Data Program (UCDP)
   in particular acknowledge that internal conflict can involve external
   actors but ignore that interstate conflict can be disguised as internal
   rebellion. This creates an unnecessary risk of categorization errors and
   a risk of neglecting the potential complexity of interstate conflict in
   the modern world. This contribution uses Idean Salehyan's distinction
   between intervention and delegation, the Nicaragua Judgement of the
   International Court of Justice, and the debate on the causes of the war
   in eastern Ukraine to illustrate this point. On the basis of this
   discussion, it proposes the introduction of a new category-delegated
   interstate conflict-to create a more coherent and symmetrical typology.},
