@article{ WOS:000657995100001,
Author = {Pasieka, Agnieszka Maria},
Title = {Postsocialist and Postcapitalist Questions? Far-Right Historical
   Narratives and the Making of a New Europe},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
Year = {2021},
Volume = {35},
