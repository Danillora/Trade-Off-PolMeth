@article{ WOS:000646389300003,
Author = {Herman, Lior and Fischhendler, Itay},
Title = {Energy as a Rewarding and Punitive Foreign Policy Instrument: The Case
   of Israeli-Palestinian Relations},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2021},
Volume = {44},
Number = {6},
Pages = {495-520},
Month = {MAR 14},
Abstract = {This article examines how and when energy is used as a punitive or
   rewarding measure to advance foreign policy ends under conditions of
   perpetual conflict. Drawing on hundreds of primary governmental and
   commercial documents, and extensive elite interviews, we examine
   Israeli-Palestinian relations over 50 years. We find extensive
   instrumentalization of energy for foreign policy objectives in the
   conflict. We highlight electricity, a neglected area in international
   relations and conflict literatures, as a significant foreign policy
   tool. Our findings emphasize four major variables that shape the timing
   and form of energy measures-politics, regime type, dependence level, and
   energy production chain.},
