@article{ WOS:000678993900001,
Author = {Bang, Insik and Hollibaugh, Gary E.},
Title = {Legislative influence on administrative decision making in
   Pennsylvania's Abandoned and Orphan Well Plugging Program},
Journal = {PUBLIC ADMINISTRATION},
Year = {2022},
Volume = {100},
Number = {3, SI},
Pages = {737-758},
Month = {SEP},
Abstract = {To what extent are elected legislators able to influence administrative
   decision making? Recent research has been divided on this question, with
   some studies suggesting legislators are effectively unable to influence
   administrative outcomes, whereas others suggesting influence is common.
   Here, we examine this question in the context of Pennsylvania's
   Abandoned and Orphan Well Plugging Program (WPP). Despite the WPP using
   objective evaluative criteria to prioritize which abandoned and orphan
   oil and gas wells should be plugged before others, our results suggest
   legislators are able to influence the WPP's decisions. Specifically, we
   find that wells are plugged more quickly when the districts in which
   they are located are represented by (a) more experienced legislators,
   (b) legislators who sit on the state House or Senate Environmental
   Resources and Energy Committees, and (c) more liberal/Democratic
   legislators. These results have implications for those interested and
   involved in the legislative-administrative nexus.},
