@article{ WOS:000692558400033,
Author = {Sterling-Folker, Jennifer and Freyberg-Inan, Annette and Wilcox, Lauren
   and Ozguc, Umut and Shinko, Rosemary E.},
Title = {Forum: Thinking Theoretically in Unsettled Times: COVID-19 and Beyond},
Journal = {INTERNATIONAL STUDIES REVIEW},
Year = {2021},
Volume = {23},
Number = {3},
Pages = {1100-1125},
Month = {SEP},
Abstract = {This collection of essays seeks to theorize the politics of the COVID-19
   pandemic in international relations (IR). The contributions are driven
   by questions such as: How can theorizing help us understand these
   unsettled times? What kind of crisis is this? What shapes its politics?
   What remains the same and what has been unsettled or unsettling? In
   addressing such questions, each of the participants considers what we
   may already know about the pandemic as well as what might be ignored or
   missed. Collectively, the forum pushes at the interdisciplinary
   boundaries of IR theorizing itself and, in so doing, the participants
   hope to engender meaningful understandings of a world in crisis and
   encourage expansive ways of thinking about the times that lie beyond.},
