@article{ WOS:000679064800001,
Author = {Blumenau, Bernhard and Muller, Johannes-Alexander},
Title = {International Organisations and Terrorism. Multilateral Antiterrorism
   Efforts, 1960-1990},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
