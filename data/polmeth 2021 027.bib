@article{ WOS:000636134100001,
Author = {Zhang, Chunman and Fu, Jianjun},
Title = {Non-institutional Factors in Shaping Rural Elections and Governance in
   China: a Pragmatist Perspective},
Journal = {JOURNAL OF CHINESE POLITICAL SCIENCE},
Year = {2021},
Volume = {26},
Number = {4},
Pages = {631-649},
Month = {DEC},
Abstract = {What type of micro individual factors may affect rural elections and
   governance in China? What may affect the dynamics of power politics in
   rural China? Based on intensive field interviews with rural residents,
   our research finds a gap between what good governance requires and what
   fair, competitive elections can provide in rural China. We argue that
   habits, social skills, and chaxu geju ((sic)(sic)(sic)(sic)) are causal
   factors that determine the roles and action orientation of elites in
   rural China, correspondingly shaping the dynamics of power politics in
   rural areas. Rural elections select the elites who are candidates to
   govern the countryside; however, the habits, skills, and chaxu geju of
   rural elites are crucial but neglected factors that can significantly
   affect the outcome of rural governance. This new theoretical framework
   which we call a pragmatist theory of agency and power politics is
   derived from Western pragmatist theory and Chinese indigenous theory.
   This approach of merging Western theories and Chinese indigenous
   theories has substantial potential to provide insights into rural
   politics in contemporary China.},
