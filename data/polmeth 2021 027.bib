@article{ WOS:000772830500008,
Author = {David, Charles-Philippe},
Title = {The American foreign policy is (now) the continuation of the (domestic)
   policy by other means},
Journal = {ETUDES INTERNATIONALES},
Year = {2021},
Volume = {52},
Number = {1-2},
Pages = {171-187},
Month = {SPR-SUM},
Abstract = {U. S. foreign policy no longer aims at the pursuit of a ``national
   interest{''} and international goals based on merit, but is essentially
   a result of political calculus and electoral considerations. This
   reality is not new but has greatly increased under the presidency of
   Donald Trump. The question is whether President Biden can stop this
   trend. The short answer is no. Several domestic factors will affect
   American foreign policy and undermine hopes of a `` return{''} of the
   United States as internationalist leader, the way it has been after the
   end of the Cold War. U.S. foreign policy is now clearly subject to the
   vagaries of domestic politics.},
