@article{ WOS:000707616600001,
Author = {Levy, Ido},
Title = {Lethal Beliefs: Ideology and the Lethality of Terrorist Organizations},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2023},
Volume = {35},
Number = {4},
Pages = {811-827},
Month = {MAY 19},
Abstract = {Open-source data and studies indicate the higher lethality rates of
   jihadist groups versus other types of terrorist organizations. I argue
   jihadist groups are more lethal than other groups due to their higher
   ideological intensity, determined by observing how ideology corresponds
   to a group's goals, internal cohesion, and external affinity. I use data
   from the Global Terrorism Database (GTD) and Revolutionary and Militant
   Organizations dataset (REVMOD) to create two samples of terrorist
   organizations coded by ideological affiliation. My findings confirm the
   higher lethality of jihadists in general and of apocalyptic jihadists in
   particular. Jihadist groups kill more per attack and have higher
   kill-wound ratios than other groups. Attachment to suicide terrorism
   largely accounts for higher jihadist lethality.},
