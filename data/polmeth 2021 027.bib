@article{ WOS:000720754700004,
Author = {Kuypers, Sarah and Figari, Francesco and Verbist, Gerlinde},
Title = {Redistribution in a joint income-wealth perspective: A cross-country
   comparison},
Journal = {SOCIO-ECONOMIC REVIEW},
Year = {2021},
Volume = {19},
Number = {3},
Pages = {929-952},
Month = {JUL},
Abstract = {Redistribution is usually understood in terms of income, as a resource
   used to rank individuals as well as determine tax liabilities or benefit
   entitlements. Yet, it is increasingly argued that more prominence should
   be given to the joint distribution of income and wealth and interest
   into the taxation of wealth for redistributive purposes has largely
   increased. By including income and wealth data from the Eurosystem
   Household Finance and Consumption Survey into the tax-benefit
   microsimulation model EUROMOD, we add two novel aspects to the
   literature. First, we include the analysis of taxes on wealth and wealth
   transfers. Second, we evaluate redistributive effects of tax-benefit
   systems against the joint income-wealth distribution instead of income
   only. We show that expressing living standards in terms of both income
   and wealth results in considerable reranking of individuals, which in
   turn leads to a lower redistributive impact of tax-benefit systems than
   is traditionally considered.},
