@article{ WOS:000632879800002,
Author = {Siegel, Alexandra A. and Nagler, Jonathan and Bonneau, Richard and
   Tucker, Joshua A.},
Title = {Tweeting Beyond Tahrir Ideological Diversity and Political Intolerance
   in Egyptian Twitter Networks},
Journal = {WORLD POLITICS},
Year = {2021},
Volume = {73},
Number = {2},
Pages = {243-274},
Month = {APR},
Abstract = {Do online social networks affect political tolerance in the highly
   polarized climate of postcoup Egypt? Taking advantage of the real-time
   networked structure of Twitter data, the authors find that not only is
   greater network diversity associated with lower levels of intolerance,
   but also that longer exposure to a diverse network is linked to less
   expression of intolerance over time. The authors find that this
   relationship persists in both elite and non-elite diverse networks.
   Exploring the mechanisms by which network diversity might affect
   tolerance, the authors offer suggestive evidence that social norms in
   online networks may shape individuals' propensity to publicly express
   intolerant attitudes. The findings contribute to the political tolerance
   literature and enrich the ongoing debate over the relationship between
   online echo chambers and political attitudes and behavior by providing
   new insights from a repressive authoritarian context.},
