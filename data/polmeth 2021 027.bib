@article{ WOS:000705163500001,
Author = {Tsai, Wen-Hsuan and Tian, Gang},
Title = {The Role of China's County-level Research Offices in Policy Adaptation},
Journal = {JOURNAL OF CHINESE POLITICAL SCIENCE},
Year = {2022},
Volume = {27},
Number = {4},
Pages = {661-679},
Month = {DEC},
Abstract = {Local governments in China have been given a considerable degree of
   flexibility in policy formulation and are permitted to adapt central
   policies to meet the needs of local development. We argue that
   county-level policy research offices play an extremely significant role
   in policy adaptation. From case studies conducted in County A and County
   B, under the jurisdiction of City X in central China, we examine local
   adaptation of the ``targeted poverty alleviation{''} policy. The
   research offices of these two counties studied the policy documents
   issued by their superiors and selected those aspects of the poverty
   alleviation policy that they considered to be conducive to local
   development. In County A, which has a relatively well developed economy,
   the research office chose to drive development through high-tech
   industries, whereas in the poorer of the two counties, County B, the
   research office formulated a policy document that emphasized poverty
   alleviation through the development of production cooperatives. Our
   discussion of these case studies enhances our understanding of how
   Chinese local governments adapt central policies.},
