@article{ WOS:000671668700007,
Author = {Haastrup, Toni and Hagen, Jamie J.},
Title = {Racial hierarchies of knowledge production in the Women, Peace and
   Security agenda},
Journal = {CRITICAL STUDIES ON SECURITY},
Year = {2021},
Volume = {9},
Number = {1},
Pages = {27-30},
Month = {JAN 2},
Abstract = {In this article, we address hierarchies of knowledge production that
   have emerged in the two decades of researching the global normative
   framework, the Women, Peace and Security (WPS) agenda. We argue that
   through the constitution of Centres of Excellence (CoE), the
   well-meaning processes of producing WPS knowledge can reify prevailing
   global racial hierarchies. We find that these processes rather than
   achieving emancipatory feminist outcomes can instead serve to narrow the
   scope of inquiry, pushing marginalised peoples further to the margins.},
