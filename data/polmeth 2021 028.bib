@article{ WOS:000608730800003,
Author = {Sarbahi, Anoop},
Title = {THE STRUCTURE OF RELIGION, ETHNICITY, AND INSURGENT MOBILIZATION
   EVIDENCE FROM INDIA},
Journal = {WORLD POLITICS},
