@article{ WOS:000697452600012,
Author = {Brosig, Malte},
Title = {Fit for Purpose? BRICS and Inner-Group Conflicts},
Journal = {GLOBAL POLICY},
Year = {2021},
Volume = {12},
Number = {4},
Pages = {529-533},
Month = {SEP},
Abstract = {This contribution argues that BRICS is designed to manage inner-group
   tensions indirectly by endorsing a number of pragmatic operating
   principles. However, these principles seem to work best at smaller scale
   conflicts but not larger armed confrontation. The article breifly
