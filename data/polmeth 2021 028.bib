@article{ WOS:000635993500001,
Author = {Weyland, Kurt},
Title = {Populism as a Political Strategy: An Approach's Enduring - and
   Increasing - Advantages},
Journal = {POLITICAL STUDIES},
Year = {2021},
Volume = {69},
Number = {2},
Pages = {185-189},
Month = {MAY},
Abstract = {Responding to Rueda's questions, this essay explains the
   political-strategic approach (PSA) to populism and highlights its
   analytical strengths, which have become even more important with the
   emergence of populist governments across the world. PSA identifies
   populism's core by emphasizing the central role of personalistic leaders
   who tend to operate in opportunistic ways, rather than consistently
   pursuing programmatic or ideological orientations. PSA is especially
   useful nowadays, when scholars' most urgent task is to elucidate the
   political strategies of populist chief executives and their problematic
   repercussions, especially populism's threat to democracy.},
