@article{ WOS:000612787800001,
Author = {Nelson, Steven and Fisk, Jonathan M.},
Title = {End of the (Pipe)Line? Understanding how States Manage the Risks of Oil
   and Gas Wells},
Journal = {REVIEW OF POLICY RESEARCH},
