@article{ WOS:000610204200001,
Author = {Thaut Vinson, Laura and Rudloff, Peter},
Title = {Ethnicities and Conflict: A Survey Experiment on the Effect of Narrative
   Framing on Perceptions in Jos, Nigeria},
Journal = {ETHNOPOLITICS},
Year = {2022},
Volume = {21},
Number = {1},
Pages = {80-101},
Month = {JAN 1},
Abstract = {Where ethnic violence divides groups by both religious and tribal
   affiliation, how does the `ethnic' characterization of conflict affect
   perceptions of the crises? From a survey experiment in Jos, Nigeria, we
   find that priming respondents with religious versus tribal conflict
   frames leads respondents to differently interpret the causes of
   violence, with religious issues viewed as the most salient cause of
   violence and religion the most important solution. The findings
   emphasize that where more than one ethnic identity is salient to
   conflict, the causes of violence take on different meaning depending on
   how individuals interpret the ethnic dimension of local violence.},
