@article{ WOS:000705484200001,
Author = {Giri, Keshab and Haer, Roos},
Title = {Female Combatants and Durability of Civil War},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2024},
Volume = {47},
Number = {5},
Pages = {526-547},
Month = {MAY 3},
Abstract = {How is conflict duration affected by female combatants in rebel group?
   In this study, we advance three possible pathways through which female
   combatants enhance the resilience of the rebel group, thereby
   lengthening the conflict. We explore this association using quantitative
   cross-sectional data on female combatant and conflict duration. The
   positive relationship between female combatants and civil war duration
   from quantitative analysis is substantiated by the qualitative evidence
   collected via in-depth interviews with former male and female combatants
   in the Maoist insurgency in Nepal. This work has important implications
   for the study of armed conflict duration, rebel organizations, and
   post-conflict peacebuilding.},
