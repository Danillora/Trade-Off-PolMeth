@article{ WOS:000731294600004,
Author = {He, Wenwen},
Title = {Unresolved Issues Regarding Hukou Registration Following Implementation
   of China's Universal Two-Child Policy},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2021},
Volume = {43},
Number = {4},
Pages = {699-713},
Month = {NOV},
Abstract = {In January 2016, China's one-child policy was replaced by a universal
   two-child policy. The central government then contended that the
   one-child policy has been the key cause of unregistered children
   (children without a hukou or legal identity, also referred to as heihu).
   The government also contended that adopting a two-child policy and
   eliminating hukou registration barriers related to the one-child policy
   would solve the problem of unregistered children. However, this simple
   claim ignores that parents in violation of family planning policies
   still face financial penalties (social maintenance fees for the second
   child and beyond born prior to 2016 and third child and beyond born
   since 2016) which disincentivizes them from registering their children,
   especially girls. The statement also ignored the legislative and
   administrative barriers that vulnerable populations are challenged by in
   the process of hukou registration. Specifically, three main categories
   of children still face great difficulty in becoming registered: chil-
   dren born out-of-policy (especially those from low-income families who
   cannot afford the social maintenance fee), children of internal
   migrants, and victims of child trafficking and informal adoption. These
   groups continue to have large numbers of individuals with heihu status
   as fundamental issues in the hukou registration process remain
   unaddressed.},
