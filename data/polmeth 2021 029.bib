@article{ WOS:000606508500006,
Author = {Udrea, Andreea and Smith, David},
Title = {Minority Protection and Kin-State Engagement: Karta Polaka in
   Comparative Perspective},
Journal = {ETHNOPOLITICS},
Year = {2021},
Volume = {20},
Number = {1, SI},
Pages = {67-82},
Month = {JAN 1},
Abstract = {In this article, the authors propose a new normative approach that
   recognises and responds more adequately to the quadratic political
   reality of kin-state-kin minorities relations. The authors' point of
   departure is the dual contention that home-states have the primary duty
   to achieve full and effective equality between their citizens, while
   accommodating fairly their internal cultural and linguistic diversity;
   and that kin-states have a legitimate interest in their co-ethnics
   abroad. Building on this foundation, the authors argue that kin-state
   engagement should complement home-states' domestic commitments to
   cultural justice, in order to foster more effective minority protection.
   The authors conclude by outlining a concept of shared responsibility for
   minority protection between kin-state and home-states.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
