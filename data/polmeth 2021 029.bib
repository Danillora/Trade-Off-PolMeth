@article{ WOS:000681124100007,
Author = {Carmody, Michelle},
Title = {Making Human Rights International? Amnesty International, Organizational
   Development, and the Third World, 1970-1985},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2021},
Volume = {43},
Number = {3},
Pages = {586-606},
Month = {AUG},
Abstract = {This article explores the process of internationalizing Amnesty
   International. Looking at this process in the 1970s and early 1980s,
   this article argues that there was a tension between Amnesty's drive to
   professionalize as an NGO, and its desire to internationalize. The
   former brought with it centralization of decision-making power and
   control over the organization's activities along with a reification of
   the concepts of impartiality and effectiveness, whereas the latter
   called for an approach that was responsive to the particular culture and
   concerns of potential members across the world.},
