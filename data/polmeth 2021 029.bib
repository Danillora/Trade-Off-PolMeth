@article{ WOS:000702997800005,
Author = {Maris Juste, Stella},
Title = {SUB-STATE TRANSNATIONAL MANAGEMENT OF ENERGY PROJECTS IN ARGENTINA IN
   RELATION TO CHINESE ACTORS},
Journal = {FORO INTERNACIONAL},
Year = {2021},
Volume = {61},
Number = {4},
Pages = {1003-1035},
Month = {OCT-DEC},
Abstract = {The article proposes to analyze the energy projects of Argentine
   provinces with Chinese actors in the context of the growing tendency of
   substate actors to deploy international actions, instrumentalizing
   foreign policy. The case studies arc the provinces of Jujuy, Neuquen and
   Santa Cruz. The objective is to study Argentine-Chinese relations from a
   sub-state perspective and to study sub-state international management in
   relation to development.
   The methodology is qualitative with interpretation of documents, taking
   into account the contextualization and exploration of each case. The
   analysis makes use of the theories of complex interdependence and
   structuralism to explain the center-periphery dynamics.},
