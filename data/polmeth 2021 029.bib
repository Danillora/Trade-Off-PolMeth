@article{ WOS:000722062100001,
Author = {Ferrill, Jamie and Nicklin, Germana and Legrand, Tim and McComas, Haydn},
Title = {Security beyond the border: exploring Australia and New Zealand
   trans-Tasman relations in a globalized world},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2021},
Volume = {59},
Number = {4, SI},
Pages = {399-423},
Month = {OCT 2},
Abstract = {This article addresses contemporary security challenges related to
   borders within Oceania, focusing on trans-Tasman relations. We address
   border security between Aotearoa New Zealand and Australia as a matter
   of an ascendant relationship, based on coordination and cooperation. By
   interrogating shared and divergent attributes between the nations, we
   are able to provide an overview of approaches towards regional threats.
   Behind these approaches are socio-political contexts, including drivers
   and authorities for cooperation as well as cultural and political
   identities. Importantly, the evolution of shared practices between the
   nations and a future scan of security in the region lead to important
   geopolitical considerations.},
