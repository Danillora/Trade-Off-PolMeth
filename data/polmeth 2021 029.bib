@article{ WOS:000631150600004,
Author = {Osland, Kari M. and Peter, Mateja},
Title = {UN peace operations in a multipolar order: Building peace through the
   rule of law and bottom-up approaches},
Journal = {CONTEMPORARY SECURITY POLICY},
Year = {2021},
Volume = {42},
Number = {2},
Pages = {197-210},
Month = {APR 3},
Abstract = {UN peace operations need a new peacebuilding agenda that acknowledges
   both the transboundary nature of conflict drivers and the multipolar
   nature of the global order. This means casting aside the current
   stabilization approach, but also abandoning the pursuit of liberal
   peacebuilding of the unipolar era. Such a conflict transformation agenda
   would require UN peace operations to prioritize the rule of law and
   bottom-up approaches, thus creating the potential to be embraced by a
   much broader range of member states. In this article, we bring liberal
   peacebuilding critiques into a discussion with debates on the nature of
   the global order. Liberal peacebuilding critiques are rooted in the
   bottom-up problematization of international interventions and show what
   kind of peacebuilding is desirable. Conversely, the debates on the
   multipolar nature of the global order expose the top-down constraints as
   to what kind of peacebuilding is feasible.},
