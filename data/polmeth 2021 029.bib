@article{ WOS:000712732900002,
Author = {Wieclawski, Jacek},
Title = {Historical analogies and general theoretical schemes in the study on
   contemporary international relations: anachronism or opportunity?},
Journal = {INTERNATIONAL POLITICS},
Year = {2022},
Volume = {59},
Number = {6},
Pages = {1210-1231},
Month = {DEC},
Abstract = {This article discusses the plausibility of historical analogies and
   general theoretical schemes in the study on contemporary international
   processes. It explains how the complex character of contemporary
   international relations modifies the previous role of theoretical
   generalizations and lessons from the past. The uncertainty of
   contemporary international processes, including the waves of global
   economic, social and political turbulences, makes the general
   theoretical schemes more tentative. It reduces the role played by
   historical analogies which become more risky and potentially
   inconclusive. They change their initial explanatory functions. The
   novelty and past experience intertwine in the analysis of contemporary
   international phenomena, yet the elements of uncertainty and novelty
   prevail. Theories respond to these challenges with flexibility and
   pragmatism, including some ``middle ground{''} positions. Their ideas
   are valuable, but do not change the article's conclusion that general
   theoretical schemes and historical analogies offer a limited practical
   advice in the study on contemporary international relations.},
