@article{ WOS:000727785100001,
Author = {Arroja, Ricardo and Camoes, Pedro J.},
Title = {What are policymakers waiting for? How trustworthy government can sooth
   tax instability and expenditure arrears COMMENT},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2022},
Volume = {35},
Number = {2},
Pages = {661-671},
Month = {APR},
Abstract = {Tax policies and public spending reflect the taking and giving powers of
   the government. However, when thinking about these, the taking powers of
   government are perceived as the most prominent. Why is the government's
   relationship with society seemingly asymmetric in that give and take?
   What leads to these perceptions? This article's starting point is
   instability in corporate income taxes and government expenditure arrears
   as two features of poor fiscal governance associated with lower
   corporate investment in the market economy. The article then focuses on
   the political and technical relationships comprising budgetary
   governance and proposes feasible policy options. These policy options
   aim toward a path of corporate income tax stability and timely
   government payments through better balancing the give and take involved
   in running the government.},
