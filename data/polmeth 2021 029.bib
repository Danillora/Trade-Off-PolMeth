Author = {Lee, I, Jonathan and Jang, Daisung and Luckman, Elizabeth A. and Bottom,
   William P.},
Title = {Wielding power in multiparty negotiations: the impact of communication
   medium and assertiveness},
Journal = {INTERNATIONAL JOURNAL OF CONFLICT MANAGEMENT},
