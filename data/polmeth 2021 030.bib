Web-of-Science-Categories  = {Political Science; Public Administration},
Author-Email = {schitom@tcd.ie},
Affiliations = {Trinity College Dublin},
Funding-Acknowledgement = {Irish Research Council},
Funding-Text = {I would like to thank Gizem Arikan, Adriana Bunea, Raj Chari, Emanuel
   Coman, Michele Crepaz, Eleonora La Spada, as well as three anonymous
