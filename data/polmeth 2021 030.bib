@article{ WOS:000605763200008,
Author = {Lee, Don S. and Park, Soonae},
Title = {What motivates street-level bureaucrats to implement the reforms of
   elected politicians?},
Journal = {POLICY AND POLITICS},
Year = {2021},
Volume = {49},
Number = {1, SI},
Pages = {141-160},
Month = {JAN},
Abstract = {The aim of this article is to explore the motivations of street-level
   bureaucrats when implementing change initiated by elected politicians.
   We analyse experimental data on more than 1,800 local civil servants
   from all 243 local governments in South Korea and find that street-level
   bureaucrats are more likely to implement change instigated by local
   elected politicians when their own policy positions are reflected in the
   reforms. Moreover, the degree to which street-level bureaucrats are
   likely to execute reforms instigated by local politicians is greater
   when bureaucrats perceive themselves as having more freedom to exercise
   discretion. These findings reveal a behavioural insight into the
   conditions in which bureaucrats are more likely to respond to change
   championed by elected politicians versus conditions where they are more
   likely to follow existing rules in the policy implementation process.},
Publisher = {POLICY PRESS},
