@article{ WOS:000772830500005,
Author = {Pahlavi, Pierre},
Title = {The Iranian policy of the United States: oscillations and continuity
   ({[}1900] 1977- 2020)},
Journal = {ETUDES INTERNATIONALES},
Year = {2021},
