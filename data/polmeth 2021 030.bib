@article{ WOS:000656809200002,
Author = {Zhu, Jiangnan and Mukhin, Nikolai},
Title = {The Modern Regency Leadership Transition and Authoritarian Resilience
   of the Former Soviet Union and China},
Journal = {COMMUNIST AND POST-COMMUNIST STUDIES},
Year = {2021},
