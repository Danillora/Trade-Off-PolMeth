@article{ WOS:000712623900005,
Author = {Gardner, Josie},
Title = {Reclaiming the sacred: towards a holographic notion of power and peace},
Journal = {PEACEBUILDING},
Year = {2021},
Volume = {9},
Number = {3, SI},
Pages = {311-325},
Month = {JUL 3},
Abstract = {This article reframes the relationship between peace and power through
   the notion of the sacred. Both orthodox and critical theorisations of
   power tend to be grounded within a power-as-force discourse, which sees
   peace and power as dichotomous rather than synergistic. In this article,
   I argue that in order to understand and embody power in a new way, we
   must bring attention to the relationship between the inner
   psycho-spiritual realm and the outer socio-political realm. I introduce
   the sacred through a holographic epistemology, which invites three
   conceptual reorientations. First, I posit power as distinct from force
   and compatible with peace. Second, I explore the ways in which power is
   holographically mirrored between inner and outer realms of peace. Third,
   I invite a peace-centric approach to researching conflict. These
   reorientations reclaim the sacred in peace research in order to
   conceptualise and embody a more congruent relationship between power and
   peace.},
