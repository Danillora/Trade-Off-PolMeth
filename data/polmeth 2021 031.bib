@article{ WOS:000654802700001,
Author = {Abbott, Kenneth W. and Faude, Benjamin},
Title = {Hybrid institutional complexes in global governance},
Journal = {REVIEW OF INTERNATIONAL ORGANIZATIONS},
Year = {2022},
Volume = {17},
