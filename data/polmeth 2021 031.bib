@article{ WOS:000788390900009,
Author = {Moore, Winston},
Title = {A reply to `Intensifying Caribbean market share within China: a CARICOM
   case assessment' by Alleyne},
Journal = {GLOBAL DISCOURSE},
Year = {2021},
Volume = {11},
Number = {4, SI},
Pages = {653-655},
Month = {NOV},
Abstract = {This reply responds to Antonio Alleyne's (2021) `Intensifying Caribbean
   market share within China: a CARICOM case assessment', the author's
   attempt to investigate the potential for increasing the market share of
   Caribbean exports in China. The article shows that there are
   opportunities to increase exports from the Caribbean in more than 50 per
   cent of the industries considered. These opportunities, however, have
   been falling over time. The article explores a very interesting topic
   but suffers from some methodological challenges that might limit the
   inferences that can be drawn from the study.},
