@article{ WOS:000670325300006,
Author = {Hudson, Darrell},
Title = {Achieving Health Equity by Addressing Legacies of Racial Violence in
   Public Health Practice},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2021},
Volume = {694},
Number = {1, SI},
Pages = {59-66},
Month = {MAR},
Abstract = {Health equity means that everyone, regardless of their abilities,
   economic status, or race/ethnicity, has the opportunity to reach their
   optimal level of health. However, the inequitable distribution of
   resources, power, and privilege in the United States means that
   historically marginalized communities bear a disproportionate burden of
   poor health and disease. The COVID-19 pandemic has compounded the
   problem for Black Americans: already bearing an unequal burden of
   social, economic, and health inequities and experiencing systemic racism
   in various sectors of American life, Black Americans have been at even
   greater risk of COVID-19 transmission and severity of the disease. I use
   critical race theory (CRT) to show how key social and historical factors
   fuel racial health inequities. Further, I use key tenets of CRT to argue
   that redressing historical legacies of racism cannot be done without
   using a critical, race conscious lens and lifting up the voices of Black
   people.},
