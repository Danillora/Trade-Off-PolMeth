@article{ WOS:000756660000001,
Author = {Hearty, Kevin},
Title = {Truth Beyond the `Trigger Puller': Moral Accountability, Transitional
   (In)Justice and the Limitations of Legal Truth},
Journal = {INTERNATIONAL JOURNAL OF TRANSITIONAL JUSTICE},
Year = {2021},
Volume = {15},
Number = {3},
Pages = {658-677},
Month = {MAR 2},
Abstract = {This article critically examines how existing truth recovery processes
   have, and how proposed truth recovery processes might, address the issue
   of shoot-to-kill in Northern Ireland. In contrast to ongoing legacy case
   prosecutions of British Army veterans, it argues that truth recovery
   processes should adopt a maximalist conceptualization of truth and
   responsibility. This, it is argued, necessitates differentiating between
   legal truth and structural truth, and between legal, political and moral
   responsibility. It is further submitted that such an approach would
   reflect the importance of identifying and understanding patterns
   (looking at similar cases collectively) and context (looking at the
   socio-political-legal backdrop to them) when establishing the `broad
   circumstances' around disputed killings. This approach would allow truth
   recovery to advance beyond low-level `trigger pullers' to ensure `moral
   accountability' for the complicity of other actors within the state
   apparatus.},
