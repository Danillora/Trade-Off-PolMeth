@incollection{ WOS:001116607300005,
Author = {Laine, Tuija},
Editor = {Berge, KL and Roos, M and Edgren, H and Hiidenmaa, P and Matthiesen, C},
Title = {Literacy, Schooling and the Role of Common People in the Educational
   Field in Finland in the Eighteenth Century},
Booktitle = {EXPLORING TEXTBOOKS AND CULTURAL CHANGE IN NORDIC EDUCATION 1536-2020},
Year = {2021},
Pages = {48-60},
Abstract = {One of the main principles of the Reformation was to read the Bible on
   your own. In Lutheran areas, including the Swedish realm and its eastern
   part, Finland, this meant reading the Catechism and the Christian
   Doctrine. Reading the Catechism was important for common people in many
   other ways as well. According to the Swedish Church Law of 1686, it was
   a condition for getting married, for receiving permission to act as a
   godfather or godmother, and for attending the Eucharist. This was a
   strong tool by which the Church and state could control the common
   people.
   The Church likewise had a strong role in the schooling system since the
   main purpose of schooling was to educate future priests. From time to
   time, this gave rise to much discussion among the secular social classes
   because of the need for education before beginning a secular career. The
   common people still had no schools, but it was the duty of parents to
   teach their children to read.
   At the end of the eighteenth century, the Enlightenment impacted even
   the common people. Catechisms for children were published and some
   schools for the poor people were funded by private persons. Discussion
   on the importance of skills, not wealth, in the schooling process was
   also a gesture of goodwill towards the common people.
   Despite several ongoing discussions about the need to change and
   modernise the schooling system during the eighteenth century, no
   remarkable changes took place. The time for change was still to come, in
   the nineteenth century.},
Publisher = {BRILL SENSE},
Address = {P O BOX 9000, LEIDEN, 2300 PA, NETHERLANDS},
Type = {Article; Book Chapter},
Language = {English},
Affiliation = {Laine, T (Corresponding Author), Univ Helsinki, Book \& Church Hist, Helsinki, Finland.
