@article{ WOS:000673063300001,
Author = {Claver, Alexander and van de Meeberg, Huibert M.},
Title = {Devil's Advocacy within Dutch military intelligence (2008-2020) an
   effective instrument for quality assurance?},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2021},
Volume = {36},
Number = {6},
Pages = {849-862},
Month = {SEP 19},
Abstract = {In 2008 a Devil's Advocate (DA) office was established within the Dutch
   Defence Intelligence and Security Service (DISS) to improve quality
   assurance and institutionalise critical reflection in its ranks. Over
   the years, the DA concept developed into a system of critical review of
   operational and strategic processes pertaining to the intelligence cycle
   as a whole. The DA office provides the DISS management with valuable
   feedback on the overall performance of the service (intelligence
   collection and analytical output), and how its output is appreciated by
   its customers. The following paper will describe this approach and
   highlight the results and challenges encountered.gt},
