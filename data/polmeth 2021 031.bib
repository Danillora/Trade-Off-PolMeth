@article{ WOS:000692274600001,
Author = {Yusupova, Guzel},
Title = {How Does the Politics of Fear in Russia Work? The Case of Social
   Mobilisation in Support of Minority Languages},
Journal = {EUROPE-ASIA STUDIES},
Year = {2022},
Volume = {74},
Number = {4},
Pages = {620-641},
Month = {APR 21},
Abstract = {The article examines the social movement of ethnic minorities in defence
   of republican state languages in ethnic regions of the Russian
   Federation in the context of politics of fear. In particular, the
   article focuses on how the repressive context shapes this social
   mobilisation both in offline and online domains. The article offers a
   detailed description of responsive as well as pre-emptive and visible as
   well as covert repressions aimed at ethnic activists and explains how
   these repressive practices change the nature of dissent, pushing it
   online and thus inadvertently leading to the development of grassroots
   activism. It also suggests that the repressive turn in Russia started
   long before the poisoning of Aleksei Naval'nyi in August 2020.},
