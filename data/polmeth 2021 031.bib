@article{ WOS:000675271300001,
Author = {Gamboa, Ricardo and Morales, Mauricio},
Title = {Candidate Gender Quotas and Campaign Spending in Open-List Proportional
   Representation Systems: The Case of Chile},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2021},
Volume = {42},
Number = {3},
Pages = {260-271},
Month = {JUL 3},
Abstract = {This research note analyzes the effect of a candidate gender quota in
   Open-List Proportional Systems and its interaction with electoral
   spending. Focusing on the Chilean parliamentary elections of 2017, our
   evidence shows, first, that the quota law was effective in promoting the
   representation of women, but, second, the differences in the electoral
   performance between men and women grew considerably nevertheless. We
   argue that this result was due mainly to gender inequality in access to
   resources to finance electoral campaigns. Third, we found that this
   inequality exists only for candidates who are challenging incumbents,
   but not for the incumbents themselves: incumbent women are as
   competitive as men.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
