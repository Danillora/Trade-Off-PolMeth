@article{ WOS:000608523000001,
Author = {Krystalli, Roxani C.},
Title = {Narrating victimhood: dilemmas and (in)dignities},
Journal = {INTERNATIONAL FEMINIST JOURNAL OF POLITICS},
Year = {2021},
Volume = {23},
Number = {1},
Pages = {125-146},
Month = {JAN 1},
Abstract = {Feminist researchers are increasingly paying attention to the politics
   of victimhood during transitions from violence. In this article, I
   address the dilemmas of researching victimhood when the researcher
   herself is part of the production of its politics and hierarchies. Based
   on in-depth fieldwork in Colombia, I examine dilemmas related to (1)
   directing the research gaze during transitions from war; (2)
   investigating violence without requiring people to re-narrate harms
   suffered during armed conflict; (3) engaging with both voluntary and
   imposed silences; and (4) navigating the complicated tug of loyalties
   among conflict-affected actors. I argue that ethics and methods are
   inseparable from each other, from the findings of the research, and from
   the meaningful study of power and violence. Collectively, these insights
   contribute to an ongoing interdisciplinary conversation about power and
   politics in the study of violence.},
