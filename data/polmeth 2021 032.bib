@article{ WOS:000616190100001,
Author = {Menkveld, Christiaan},
Title = {Understanding the complexity of intelligence problems},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2021},
Volume = {36},
Number = {5},
Pages = {621-641},
Month = {JUL 29},
Abstract = {The complexity of an intelligence problem determines to a great extent
   the certainty that can be provided by intelligence and security
   services. This article will argue this deductively and validate this
   claim using a set of intelligence reports produced by the Dutch
   Intelligence \& Security Service (AIVD). A better understanding of how
   the level of complexity affects intelligence analysis is helpful for the
   further (academic) study of analysis methodologies, as well as for
   managing expectations of intelligence clients, and for informing the
   debates over legislation that affects intelligence and security
   services.},
