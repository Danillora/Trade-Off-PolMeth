@article{ WOS:000670325300013,
Author = {Simko, Christina},
Title = {From Legacy to Memory: Reckoning with Racial Violence at the National
   Memorial for Peace and Justice},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2021},
Volume = {694},
Number = {1, SI},
Pages = {157-171},
Month = {MAR},
Abstract = {How are the legacies of violent pasts brought into collective memory?
   Even as social scientists have systematically documented the long shadow
   that racially motivated lynching has cast into the present, this history
   has had little place within dominant national narratives. Recently,
   however, the Equal Justice Initiative's National Memorial for Peace and
   Justice in Montgomery, Alabama, has broken through the silence,
   attracting widespread attention and praise. This article examines how
   the memorial creatively adapts morally and emotionally resonant themes
   from dominant national narratives to bridge long-standing mnemonic
   cleavages between Black and white Americans. In addition, the article
   provides preliminary evidence that the legacies of racial violence are
   gaining a place in national memory.},
