@article{ WOS:000731401500011,
Author = {Masullo, Juan and Zulver, Julia and Idler, Annette},
Title = {Community Responses to a Changing Security Landscape at the
   Colombia-Venezuela Border: The Case of La Guajira},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
