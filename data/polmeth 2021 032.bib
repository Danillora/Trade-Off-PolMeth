@article{ WOS:000725280100009,
Author = {Bartha, Eszter and Toth, Andras},
Title = {The Emasculation of Trade Unions and Workers' Drift to Neonationalism in
   Hungary},
Journal = {EUROPE-ASIA STUDIES},
Year = {2021},
Volume = {73},
Number = {9, SI},
Pages = {1726-1747},
Month = {OCT 21},
Abstract = {Theoretically, the essay is built on Karl Polanyi's interpretation of
   disembedding and Chris Hann's application of this model to post-1989
   Eastern Europe. The essay sets out to explain why trade unions failed to
   become a successful countermovement in the Polanyian sense of the word
   by analysing four sources of power available to unions. We go on to
   analyse the social and political consequences of this failure,
   demonstrating through the analysis of life-history interviews how
   `lonely fighters' can become rightwing voters and activists, thanks to
   the rise of a new political culture on the shopfloor.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
