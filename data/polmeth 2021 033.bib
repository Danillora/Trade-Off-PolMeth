@article{ WOS:000693941900001,
Author = {Johnson, Janet Elise and Novitskaya, Alexandra and Sperling, Valerie and
   Sundstrom, Lisa McIntosh},
Title = {Mixed signals: what Putin says about gender equality},
Journal = {POST-SOVIET AFFAIRS},
Year = {2021},
Volume = {37},
Number = {6},
Pages = {507-525},
Month = {NOV 2},
Abstract = {The prevailing wisdom among scholars of gender in Russia is that
   Vladimir Putin - as Russia's ``strongman{''} president - has become an
   agent of traditionalism. Some political scientists, often without a
   gendered lens, have argued that Putin is not so powerful, compelled to
   deploy various tactics and ideologies to balance competing interests
   among elites and retain support from the general public. We
   systematically analyze Putin's statements about gender in two decades of
   his annual speeches (1999-2020) to better understand how Putin rules.
   Coding Putin's remarks on a spectrum from promoting to opposing gender
   equality, we find that there has been no shift toward an explicit
   traditionalism, but rather, an expansion of the
   gender-stereotypical/Soviet views that have dominated Putin's
   pronouncements all along. We argue that Putin's diverse remarks across
   the spectrum of gender (in)equality constitute an important part of his
   efforts to balance diverse elite interests and enlist mass support.},
