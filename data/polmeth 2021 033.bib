@incollection{ WOS:001094748500006,
Author = {Qian, Jiwei},
Book-Author = {Qian, J},
Title = {Feedback on Social Policy Reform and Bureaucrats' Responses},
Booktitle = {POLITICAL ECONOMY OF MAKING AND IMPLEMENTING SOCIAL POLICY IN CHINA},
Series = {Social Policy and Development Studies in East Asia},
Year = {2021},
Pages = {127-146},
Abstract = {This chapter discusses how citizens and workers can respond to social
   policy reforms. Citizens can provide their feedback about policy
   implementation by participating in the policymaking process via online
   notice-and-comment systems. Workers can choose to opt out of (locally
   managed) social insurance. Workers can also choose to migrate to other
   cities if they can obtain a better welfare package/real wage increase
   there. This chapter also discusses bureaucrats' responses to feedback
   mechanisms from a political economy perspective. While these feedback
   mechanisms have the potential to improve policy implementation, I argue
   that bureaucrats are more likely to respond to mechanisms related to
   their career concerns.},
