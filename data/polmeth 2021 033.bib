@incollection{ WOS:001175505500002,
Author = {Ricento, Thomas},
Book-Author = {Ricento, T},
Title = {The Problem},
Booktitle = {REFUGEES IN CANADA: On the Loss of Social and Cultural Capital},
Year = {2021},
Pages = {1-16},
Abstract = {There is a mismatch between Canadian federal government immigration
   policies which are geared to attract high-skilled workers with high
   levels of education and fluency in one of the two official languages and
   the actual experiences of immigrants in Canada, who, controlling for
   education and language, experience unemployment and underemployment at
   significantly higher levels than native-born Canadians. Despite Canada's
   preference for highly skilled immigrants, and despite the fact that
   immigrant professionals bring significant human capital resources to the
   Canadian labor force, a number of studies have shown that many highly
   educated immigrant professionals experience deskilling and devaluation
   of their prior learning and work experience after immigrating to Canada.},
