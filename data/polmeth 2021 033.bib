@article{ WOS:000629711000001,
Author = {Sabbagh-Khoury, Areej},
Title = {Tracing Settler Colonialism: A Genealogy of a Paradigm in the Sociology
   of Knowledge Production in Israel},
Journal = {POLITICS \& SOCIETY},
