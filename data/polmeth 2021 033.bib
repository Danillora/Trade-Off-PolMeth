@article{ WOS:000735214300001,
Author = {Hamrik, Lukas and Kaniok, Petr},
Title = {Who's in the Spotlight? The Personalization of Politics in the European
   Parliament},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
