@article{ WOS:000715896700006,
Author = {Flores, Stella M. and Carroll, Tim and Lyons, Suzanne M.},
Title = {Beyond the Tipping Point: Searching for a New Vision for Latino College
   Success in the United States},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
