@article{ WOS:000642305100003,
Author = {Stephens, Angharad Closs and Coward, Martin and Merrill, Samuel and
   Sumartojo, Shanti},
Title = {Affect and the Response to Terror: Commemoration and Communities of
   Sense},
Journal = {INTERNATIONAL POLITICAL SOCIOLOGY},
Year = {2021},
Volume = {15},
Number = {1},
Pages = {22-40},
Month = {MAR},
Abstract = {This article examines affective responses to terror and the emergence of
   communities of sense in the commemoration of such attacks. We challenge
   the predominant framing of responses to terror which emphasize security
   and identity. We focus on the singular response by the city of
   Manchester in the aftermath of the 2017 Arena bombing, drawing on
   fieldwork conducted at the 1-year anniversary commemorative events. Our
   discussion focuses on the ways improvised, transient communities
   crystallized around the cultural significance of music during these
   events. The article explores these communities of sense through two case
   studies: those drawn together around the figure of Ariane Grande; and
   those assembled through a mass sing-along. In contrast to national or
   municipal responses to terror which orchestrate affect to establish
   narratives about security, borders and identity, we argue for the
   importance of paying attention to the improvised, affective ways in
   which people respond to terror. These plural, affective responses
   suggest another form of collective subjectivity. They also demonstrate
