@article{ WOS:000750189100002,
Author = {Rothwell, Jonathan and Smith, Ember},
Title = {Socioeconomic Status as a Risk Factor in Economic and Physical Harm from
   COVID-19: Evidence from the United States},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2021},
Volume = {698},
Number = {1, SI},
Pages = {12-38},
Month = {NOV},
Abstract = {We provide an empirical summary of the relationship between
   socioeconomic status and the economic and disease burden of the
   SARS-CoV2 pandemic in the United States. We rely on large-scale public
   data, including a zip code database we constructed from public records,
   to investigate the relationship between socioeconomic status and the
   risk of economic harm, COVID-19 infection, or COVID-19-related death. We
   find that low levels of education and income are associated with 1.3 to
   2 times higher risk of economic harm and 2 to 3 times greater physical
   harm. Education and income have a similar effect size to racial and
   ethnic disparities, with many Americans of color facing worse outcomes.
   Using Gallup data to investigate potential mechanisms, we find that
   socioeconomic status is not related to preventative behavior like mask
   use but is related to occupation-related exposure, which puts
   lower-socioeconomic-status households at risk.},
