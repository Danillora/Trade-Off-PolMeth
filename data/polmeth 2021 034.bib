@article{ WOS:000670325300004,
Author = {Zuckerman, Molly K. and Austin, Rita M. and Hofman, Courtney A.},
Title = {Historical Anatomical Collections of Human Remains: Exploring Their
   Reinterpretation as Representations of Racial Violence},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2021},
Volume = {694},
Number = {1, SI},
Pages = {39-47},
Month = {MAR},
Abstract = {We synthesize how the tools of molecular anthropology, integrated with
   analyses of skeletal material, can provide direct insights into the
   context-specific experiences of racial structural violence in the past.
   Our work-which is emblematic of how biological anthropologists are
   increasingly interested in exploring the embodied effects of structural
   and race-based violence-reveals how anthropology can illuminate past
   lived experiences that are otherwise invisible or inscrutable. This kind
   of integrative research is exposing the legacies of structural violence
   in producing anatomical collections and the embodied effects of
   structural violence evident within individuals in those collections.},
