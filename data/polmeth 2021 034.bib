@article{ WOS:000734295300001,
Author = {Kroger, Sandra and Loughran, Thomas},
Title = {The Risks and Benefits of Differentiated Integration in the European
   Union as Perceived by Academic Experts},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2022},
Volume = {60},
Number = {3},
Pages = {702-720},
Month = {MAY},
Abstract = {This article sheds light on how academic experts assess the benefits and
   risks of differentiated integration (DI) in the European Union (EU). DI
   denotes particular member states either being allowed to opt out of
   specific EU policies, or being excluded or exempted from participating
   in them until certain conditions are met. The findings of a novel expert
   survey (n = 95) highlight two key divisions amongst experts, namely,
   first a regional divide between scholars based in Western Europe and
   those based in Eastern Europe, with the former more favourable to DI
   than the latter; and second, a substantive divide between those, more
   numerous, who are favourable to DI, and those who are more critical. For
   the former, the perceived benefits outweigh the perceived risks. What
   drives the support of or objection to DI also differs between experts.
   Whereas supporters favour pragmatic reasons for DI, opponents mostly
   mobilize principled reasons against it.},
