@article{ WOS:000650516900001,
Author = {Barsukova, Svetlana and Denisova-Schmidt, Elena},
Title = {Double Standards as Modus Operandi: Mixing Business and Politics
   in Russia},
Journal = {EUROPE-ASIA STUDIES},
Year = {2022},
Volume = {74},
Number = {6},
Pages = {990-1005},
Month = {JUL 3},
Abstract = {Based on expert interviews (n = 75) conducted between October 2018 and
   July 2019 in selected Russian regions, the authors identify several
   types of strategies used by businesses while dealing with the
   government. These include collusion, revolving-door affiliations,
   conflict of interest, `voluntary' donations and other forms of support
   to the ruling party (and sometimes the opposition parties) in exchange
   for public contracts or preferential treatment. The study outcomes
   suggest that it is almost impossible to do business in Russia without
   using these types of strategies. The authors trace some of these forms
   back to the Soviet period and discuss possible remedies.},
