@article{ WOS:000686808600001,
Author = {Marples, David R. and Laputska, Veranika},
Title = {Maly Trascianiec in the Context of Current Narratives on the Holocaust
   in the Republic of Belarus},
Journal = {EUROPE-ASIA STUDIES},
Year = {2022},
Volume = {74},
Number = {1},
Pages = {31-49},
Month = {JAN 2},
Abstract = {This article examines the current memorialisation of the Holocaust in
   Belarus through the example of the Maly Trascianiec camp, established by
   the Nazi occupation regime just outside Minsk. It traces the changing
   interpretations of the site's history, from neglect of its Holocaust
   dimension to a partial recognition of this in the past few years and the
   establishment of two significant memorials, opened in Maly Trascianiec
   (2015) and Blahauscyna Forest (2018). Building on previous studies, it
   asks whether Belarus may finally recognise the transnational nature of
   the Holocaust and Maly Trascianiec as a key component of the Holocaust
   in Belarus. Such recognition may eventually change the government's
   longstanding focus on victory in World War II as the founding stone of
   modern Belarus.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
