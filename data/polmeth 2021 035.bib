@article{ WOS:000788387100005,
Author = {Leeb, Claudia},
Title = {Castration anxiety, COVID-19 and the extremist right},
Journal = {GLOBAL DISCOURSE},
Year = {2021},
Volume = {11},
Number = {3, SI},
Pages = {387-404},
Month = {MAY},
Abstract = {In this article, I draw on Theodor W. Adorno's psychoanalytically
   inspired works on (neo-) fascism and psychoanalytic theory to outline
   the threat of castration in contemporary capitalist societies on
   economic, interpersonal and bodily levels. I then explain how the
   COVID-19 pandemic has heightened people's castration anxieties on all
   three levels in a class- and gender-specific way. Finally, I expose how
   the right extremist president of the United States, Donald Trump, and
   the right extremist leader of the Austrian Freedom Party, Norbert Hofer,
   utilised castration anxieties in their psychologically oriented tricks
   to strengthen their base and capture new followers.},
