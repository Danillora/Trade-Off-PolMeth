@incollection{ WOS:001094748500004,
Author = {Qian, Jiwei},
Book-Author = {Qian, J},
Title = {Interagency Collaboration in China's Social Policy Reform},
Booktitle = {POLITICAL ECONOMY OF MAKING AND IMPLEMENTING SOCIAL POLICY IN CHINA},
Series = {Social Policy and Development Studies in East Asia},
Year = {2021},
Pages = {81-99},
Abstract = {Social programs in China are managed by different government agencies.
   Also, for a given social program, there usually are multiple government
   departments involved in implementing the program. Given that different
   government agencies have their own policy agenda and mission, the
   effectiveness of interagency coordination is a major issue in social
   policy implementation. In this chapter, I discuss what institutions in
   the Chinese bureaucracy system address this interagency coordination
   issue and whether they work. Two mechanisms are highlighted in this
   chapter: logrolling and separating authority. Further, I argue in this
   chapter that the fragmentary structure of policymaking could be
   endogenous.},
