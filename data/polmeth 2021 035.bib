@article{ WOS:000670452300001,
Author = {Elder, Laurel and Greene, Steven and Lizotte, Mary-Kate},
Title = {Feminist and Anti-Feminist Identification in the 21st Century
   United States},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2021},
Volume = {42},
Number = {3},
Pages = {243-259},
Month = {JUL 3},
Abstract = {Feminism and anti-feminism featured prominently in the 2016 presidential
   election. Hillary Clinton was the first female major party presidential
   candidate and self-identified as a feminist speaking openly about the
   challenges facing women. Clinton faced off against Donald Trump, who was
   on record making sexist statements and arguing that Clinton's success
   was from playing the ``woman card{''}. We ask several questions: who
   identifies as a feminist today and how is this different from who
   identified as a feminist in the previous generation? Who identifies as
   ``anti-feminist{''}? Are anti-feminists simply a mirror reflection of
   feminists or is it a distinctive social identity? Finally, the study
   explores the meaning of these labels by looking at what feminists and
   anti-feminists believe in terms of public policy and attitudes about
   gender equality. Thus, this study provides insights into the state of
   modern feminism and antifeminism in contemporary American politics.},
