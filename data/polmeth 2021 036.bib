@article{ WOS:000727090000001,
Author = {Garcia Cabrera, Marta},
Title = {British geographic intelligence during the Second World War: a case
   study of the Canary Islands},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2022},
Volume = {37},
Number = {2},
Pages = {262-280},
Month = {FEB 23},
Abstract = {The Second World War led to significant developments in operational
   intelligence activities as the belligerent powers collected the
   geographic, military, and socio-economic information that was essential
   for planning military operations. Part of the British strategic agencies
   were dedicated to geographic intelligence through divisions, sections,
   and departments that analysed the terrain over which potential military
   movements could occur. This article provides an analysis of British
   reports on the Canary Islands as a case study of wartime geographic
   intelligence. It shows how the information collected supported the
   design and updating of British invasion plans on the islands between
   1940 and 1943.},
