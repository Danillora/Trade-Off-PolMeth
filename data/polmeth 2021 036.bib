@article{ WOS:000612735200001,
Author = {Best, Jonathan},
Title = {`The tortuousness of our Albanian allies': Special Operations Executive
   in Albania through the eyes of Anthony Quayle's Eight hours from
   England},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2021},
Volume = {36},
Number = {3},
Pages = {358-376},
Month = {APR 16},
Abstract = {This article reveals the issues that SOE operatives faced in Albania by
   exploring Anthony Quayle's 1945 spy novel Eight hours from England,
   which was based on Quayle's experiences as a secret agent in Albania. It
   details SOE's inability to achieve effective relations with communist
   and anti-communist resistance networks which, while claiming to be
   fighting the Axis occupiers, were more interested in fighting each
   other. Also, this article will demonstrate the psychological toll that
   this mission had on Quayle as he changed from being enthusiastic for
   success to expressing an abhorrence for his Albanian allies after
   countless setbacks and betrayals.},
