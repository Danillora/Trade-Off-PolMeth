@incollection{ WOS:001116607300012,
Author = {Hommerstad, Marthe},
Editor = {Berge, KL and Roos, M and Edgren, H and Hiidenmaa, P and Matthiesen, C},
Title = {Politics, Catechisms and Democracy: Political Strife, Popular Power and
   Enlightenment in Norway 1814-1840},
Booktitle = {EXPLORING TEXTBOOKS AND CULTURAL CHANGE IN NORDIC EDUCATION 1536-2020},
Year = {2021},
Pages = {128-143},
Abstract = {This chapter aims to investigate the early attempts at civic education
   in the period after the introduction of the Norwegian constitution of
   1814. In these first decades of the nineteenth century, the education
   system lacked any ambitions of providing a political literacy to the
   children. Instead, one finds that political catechisms introduced
   political literacy into a newfound political public sphere, aimed at the
   adult population. In this field, one experiences a popular movement
   searching after political influence. The authorities had a more limited
   role in this period and did not manage to exercise their control of the
   public schools and its curriculum.},
