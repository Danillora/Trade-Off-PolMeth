@article{ WOS:000624220500001,
Author = {Gartzou-Katsouyanni, Kira and Kiefel, Max and Olivas Osuna, Jose Javier},
Title = {Voting for Your Pocketbook, but against Your Pocketbook? A Study of
   Brexit at the Local Level},
Journal = {POLITICS \& SOCIETY},
Year = {2022},
Volume = {50},
Number = {1},
Pages = {3-43},
Month = {MAR},
Abstract = {In explaining the outcome of the 2016 EU referendum in the United
   Kingdom, can theories emphasizing the importance of economic factors be
   reconciled with the fact that many people appeared to vote against their
   economic self-interest? This article approaches this puzzle through case
   study research that draws on fieldwork and a process of reciprocal
   knowledge exchange with local communities in five local authorities in
   England and Wales. It argues that the Leave vote can be attributed
   partly to political discontent associated with trajectories of relative
   economic decline and deindustrialization. Building on the growing
   literature about the role of narratives and discourses in navigating
   uncertainty, it contends that these localized economic experiences,
   interpreted through local-level narratives, paved the way for
   local-level discourses of resilience and nationwide optimistic messaging
   about the economic impacts of Brexit to resonate. Local and
   national-level discourses discounting the potential economic costs of
   leaving the European Union played a crucial role in giving precise,
   somewhat paradoxical, political content to the sense of discontent. The
   article contributes to the growing focus on place and community in
   understanding political behavior and invites further research on local
   discourses linking macro-level trajectories and micro-level voting
   decisions.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Gartzou-Katsouyanni, K (Corresponding Author), London Sch Econ \& Polit Sci, European Inst, Ctr Bldg,Houghton St, London WC2A 2AE, England.
   Gartzou-Katsouyanni, Kira; Kiefel, Max; Olivas Osuna, Jose Javier, London Sch Econ, London, England.},
DOI = {10.1177/0032329221992198},
EarlyAccessDate = {FEB 2021},
Article-Number = {0032329221992198},
ISSN = {0032-3292},
EISSN = {1552-7514},
