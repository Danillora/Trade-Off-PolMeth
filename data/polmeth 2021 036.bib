@article{ WOS:000704561400001,
Author = {Kleynhans, Evert},
Title = {The Rooseboom operation: uncovering the embryonic German intelligence
   network in South Africa, 1940-1942},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2022},
Volume = {37},
Number = {1},
Pages = {38-56},
Month = {JAN 2},
Abstract = {Germany desired to contact the wartime opposition in South Africa in
   order to obtain accurate political and military intelligence. The
   arrival of Hans Rooseboom in South Africa in 1940 provided the
   Ossewabrandwag (Oxwagon Sentinel), a quasi-cultural and anti-war
   movement, with the means through which to initiate two-way communication
   with Germany. The so-called Rooseboom secret service became the first
   German intelligence network to operate in South Africa during the early
   war years. This article investigates the nature and operation of the
   so-called Rooseboom secret service from 1941 to 1942 against the
   backdrop of the larger intelligence war waged in South Africa.},
