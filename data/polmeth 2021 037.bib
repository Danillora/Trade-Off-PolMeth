@article{ WOS:000788180600005,
Author = {Erikson, Josefina and Freidenvall, Lenita},
Title = {A legislative gender-equality norm as a catalyst for change? Discursive
   convergence in the case of the Swedish Parliament},
Journal = {EUROPEAN JOURNAL OF POLITICS AND GENDER},
Year = {2021},
Volume = {4},
Number = {3},
Pages = {403-421},
Month = {SEP},
Abstract = {This article addresses the establishment of gender-equality norms in a
   case often presented as one of the most gender-equal legislatures in the
   world, namely, the Swedish Parliament (Riksdagen). Based on a series of
   in-depth interviews between 2005 and 2016 with 90 legislators in the
   Swedish Parliament, we ask whether there is agreement over
   gender-equality problems in Parliament that cut across gender and party
   affiliation, and whether there is convergence over time in this regard.
   Our findings show that there is a trend of convergence of the
   gender-inequality framings over time, which indicates the establishment
   of a shared legislative gender-equality norm. We suggest that a
   legislative gender-equality norm might work as a catalyst for
   progressive and continuous work in this area.},
Publisher = {BRISTOL UNIV PRESS \& POLICY PRESS},
Address = {1-9 OLD PARK HILL, UNIV BRISTOL, BRISTOL, BS2 8BB, ENGLAND},
