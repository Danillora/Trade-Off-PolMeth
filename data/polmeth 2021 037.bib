@article{ WOS:000616254500005,
Author = {Feldmann, Magnus and Morgan, Glenn},
Title = {Brexit and British Business Elites: Business Power and Noisy Politics{*}},
Journal = {POLITICS \& SOCIETY},
Year = {2021},
Volume = {49},
Number = {1, SI},
Pages = {107-131},
Month = {MAR},
Note = {Society-for-the-Advancement-of-Socio-Economics (SASE) Annual Meeting,
   Univ Claude Bernard, Lyon, FRANCE, JUN 29-JUL 01, 2017},
Organization = {Soc Advancement Socio Econ},
Abstract = {This article analyzes business power in the context of noisy politics by
   comparing business involvement in two British referendum campaigns: one
   about membership in the European Communities in 1975, and the Brexit
   referendum about European Union membership in 2016. By exploring these
   two contexts, the article seeks to identify the conditions under which
   business elites can and cannot be effective in a context of noisy
   politics. Three key factors are identified as determinants of business
   influence during periods of noisy politics: the incentives to get
   directly involved in noisy politics; the legitimacy of business
   involvement; and, finally, the capacity to act in a cohesive way. The
   article shows that these factors have changed substantially over the
   last four decades because of wider changes in the nature of capitalism,
   and their impact on business power in the United Kingdom and more
   generally is discussed.},
