   Erikson, Josefina, Uppsala Univ, Dept Govt, Uppsala, Sweden.
   Freidenvall, Lenita, Stockholm Univ, Polit Sci, Stockholm, Sweden.},
DOI = {10.1332/251510820X16007079771821},
ISSN = {2515-1088},
EISSN = {2515-1096},
Keywords = {gender-sensitive parliaments; gender equality; norm; discourse;
   convergence; Sweden},
