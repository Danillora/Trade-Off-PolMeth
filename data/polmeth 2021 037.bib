@article{ WOS:000607549400008,
Author = {Hollenbach, Florian M.},
Title = {Elite interests and public spending: Evidence from Prussian cities},
Journal = {REVIEW OF INTERNATIONAL ORGANIZATIONS},
Year = {2021},
Volume = {16},
Number = {1},
Pages = {189-211},
Month = {JAN},
Abstract = {When do economic and political elites demand investment in public goods
   and services? The prevailing view is that non-democratic governments
   engage in low levels of government spending and taxation, because elites
   have interests in low taxation. Non-democracies exhibit significant
   variation in levels of government spending; the causes of these
   discrepancies have thus far not been thoroughly examined. I argue that
   where elites own capital that is conducive to government spending,
   regimes make higher investments. I test this argument using newly
   collected data on government spending as well as political and economic
   characteristics of 110 cities in 19th century Prussia. Using both
   standard regression models and instrumental variable analysis, I show
   that the economic needs of the local elites drove local government
   decisions on public spending.},
