@article{ WOS:000644230700001,
Author = {Hannah, Erin and Roberts, Adrienne and Trommer, Silke},
Title = {Gender in global trade: Transforming or reproducing trade orthodoxy?},
Journal = {REVIEW OF INTERNATIONAL POLITICAL ECONOMY},
Year = {2022},
Volume = {29},
Number = {4},
Pages = {1368-1393},
Month = {JUL 4},
Abstract = {In contrast to fields such as development, peace and security, and
   corporate governance, questions of gender equality have only recently
   begun entering into trade policymaking. In recent years, there has been
   a proliferation of initiatives aimed at leveraging trade for gender
   equality and addressing the adverse impacts of trade on women and other
   vulnerable populations. In this article, we assess whether the new
   gender and trade agenda marks a transformative moment in global trade
   governance or reproduces the trade orthodoxy, thereby perpetuating
   existing inequalities. We build upon critical trade and feminist IPE
   literatures to development an analytical framework to examine whether
   gender and trade initiatives reproduce or disrupt the orthodoxy. Its
   four key markers are: market rationality, technocracy, legalization, and
   abstraction. We find that while most gender and trade initiatives that
   we study reproduce and further entrench the trade orthodoxy, there are
   openings that could lead towards a more transformative trade politics.
   Future research should examine the policy processes that produced these
   openings and investigate if and how they could be transferred into
   policy instruments that lead to real changes to how the world trades.},
