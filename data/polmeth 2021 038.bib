@article{ WOS:000596806500002,
Author = {Barbera, Pablo and Boydstun, Amber E. and Linn, Suzanna and McMahon,
   Ryan and Nagler, Jonathan},
Title = {Automated Text Classification of News Articles: A Practical Guide},
Journal = {POLITICAL ANALYSIS},
