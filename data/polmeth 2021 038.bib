@article{ WOS:000725681500003,
Author = {Lewis, Joanna I.},
Title = {Green Industrial Policy After Paris: Renewable Energy Policy Measures
   and Climate Goals},
Journal = {GLOBAL ENVIRONMENTAL POLITICS},
Year = {2021},
Volume = {21},
Number = {4},
Pages = {42-63},
Month = {NOV 28},
Abstract = {Renewable energy (RE) will play a significant role in national climate
   mitigation strategies, including those put forth in the context of the
   Paris Agreement. This article examines the role of industrial policy in
   supporting renewables and how it compares to the use of other types of
   RE policies in both location and quantity around the world. On the basis
   of an original database of RE policy support measures developed for this
   analysis, the article illustrates which measures are most commonly being
