Author = {Sargentis, G. -Fivos and Iliopoulou, Theano and Dimitriadis, Panayiotis
   and Mamassis, Nikolaos and Koutsoyiannis, Demetris},
Title = {Stratification: An Entropic View of Society's Structure},
