@article{ WOS:000629841400001,
Author = {Baloge, Martin},
Title = {From Brexit to Dexit? Alternative fur Deutschland's Euroscepticism on
   European debates in the Bundestag},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2021},
Volume = {29},
Number = {4, SI},
Pages = {552-566},
Month = {OCT 2},
Abstract = {The 2017 entry of the AfD in the Bundestag is one of the most important
   event in the history of the federal Republic. The far-right party has
   changed the way debates are led on numerous subjects including European
   issues. We analyse how the Brexit debates have shaped AfD's
   Euroscepticism and how other parties have reacted to this rhetoric. We
   investigate how Brexit is mobilized in the Bundestag as a political
   issue. The case of the AfD is particularly relevant since this far-right
   party was originally established in 2013 and based its foundations on a
   critique of the EU construction and particularly its monetary policy.
   However, the party has evolved into a populist, anti-migrant,
   sovereignist party while maintaining its Eurosceptic arguments. The AfD
   is the main opposition force in the Bundestag and has regularly managed
   to influence the media agenda and political debates on various topics,
   including Europe. Specific attention will be given to the debates in the
   plenary to underline how the Bundestag discusses this issue in a chaotic
   atmosphere. This article relies on the analysis of parliamentary debates
   and political programmes of the parties in the Bundestag from September
   2017 to September 2019.},
