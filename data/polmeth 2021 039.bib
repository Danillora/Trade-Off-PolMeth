@article{ WOS:000733242500006,
Author = {Yair, Omer},
Title = {THE HOSTILE MEDIATOR PHENOMENON WHEN THREATENED, RIVAL PARTISANS
   PERCEIVE VARIOUS MEDIATORS AS BIASED AGAINST THEIR GROUP},
Journal = {PUBLIC OPINION QUARTERLY},
