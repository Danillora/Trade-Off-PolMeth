@article{ WOS:000731935300008,
Author = {Dammert, Lucia and Elorrieta, Felipe and Alda, Erik},
Title = {Satisfaction with the Police in Chile: The Importance of Legitimacy and
   Fair Treatment},
Journal = {LATIN AMERICAN POLITICS AND SOCIETY},
Year = {2021},
Volume = {63},
Number = {4},
Pages = {124-145},
Month = {NOV},
Abstract = {This study addresses a void in the literature on public attitudes toward
   police in Latin America. It integrates three theoretical models of the
   determinants of citizen satisfaction with police work in Chile:
   demographic, quality of life in the neighborhood, and experiential. The
   study tested the integrated model using a novel random sample of 996
   individuals living in the Metropolitan Region of Santiago. The results
   underscore the importance of legitimacy centered on fair treatment,
   respect for human rights, and the perception that the police represent
   society. The findings are also significant for the Chilean institutional
   political process and for the Latin American police reform debate.},
