@article{ WOS:000642329300007,
Author = {Urbatsch, Robert},
Title = {REVISITING ``THE FIRST-DAUGHTER EFFECT{''}},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2020},
Volume = {84},
Number = {2},
Pages = {523-537},
Month = {SUM},
Abstract = {A recent Public Opinion Quarterly article argued that men whose first
   children were girls rather than boys-daughters rather than sons-were
   more likely to support gender-equality policies. This note reexamines
   the coding and model specification of that analysis, focusing on the
   definition of ``first daughters{''} and on the inclusion, perilously for
   causal inference, of control variables that are more probably
   consequences than causes of the independent variable of interest. In the
   tested alternative specifications, the predicted effect of first-child
   sex diminishes, usually falling short of standard benchmarks for
   statistical significance.},
