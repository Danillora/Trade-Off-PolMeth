@article{ WOS:000684622100001,
Author = {Chkhaidze, Irakli and Kakitelashvili, Ketevan},
Title = {A national figure as a memory site: reinterpretations of Ilia
   Chavchavadze in the 1910s-1940s},
Journal = {CAUCASUS SURVEY},
