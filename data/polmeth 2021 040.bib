@incollection{ WOS:001116618500006,
Author = {Pienimaki, Mari and Kotilainen, Sirkku},
Editor = {Hoechsmann, M and Thesee, G and Carr, PR},
Title = {Media Education for the Inclusion of At-Risk Youth: Shades of Democracy
   2.0 from Finland},
Booktitle = {EDUCATION FOR DEMOCRACY 2.0: Changing Frames of Media Literacy},
