@article{ WOS:000656203600006,
Author = {Struminskaya, Bella and Toepoel, Vera and Lugtig, Peter and Haan,
   Marieke and Luiten, Annemieke and Schouten, Barry},
Title = {UNDERSTANDING WILLINGNESS TO SHARE SMARTPHONE-SENSOR DATA},
Journal = {PUBLIC OPINION QUARTERLY},
