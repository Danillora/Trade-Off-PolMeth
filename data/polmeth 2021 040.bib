@article{ WOS:000778891900001,
Author = {Schmid, Christian S. and Chen, Ted Hsuan Yun and Desmarais, Bruce A.},
Title = {Generative Dynamics of Supreme Court Citations: Analysis with a New
   Statistical Network Model},
Journal = {POLITICAL ANALYSIS},
