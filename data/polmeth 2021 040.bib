@article{ WOS:001109278200001,
Author = {Darwazeh, Durgham and Clarke, Amelia and Wilson, Jeffrey},
Title = {Framework for Establishing a Sustainable Medical Facility: A Case Study
   of Medical Tourism in Jordan},
