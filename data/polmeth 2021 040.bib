@article{ WOS:000658470300001,
Author = {Lomonosov, Matvey},
Title = {`Ethnic memories' from above? The Kosovo myth among the South Slavs and
   minimalist ethnosymbolism},
Journal = {NATIONS AND NATIONALISM},
