@article{ WOS:000613875500001,
Author = {Bochsler, Daniel and Green, Elliott and Jenne, Erin and Mylonas, Harris
   and Wimmer, Andreas},
Title = {Exchange on the quantitative measurement of ethnic and national
   identity{*}},
