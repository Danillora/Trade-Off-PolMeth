@article{ WOS:000679863400001,
Author = {Szpak, Agnieszka},
Title = {Human rights approach in selected European municipal strategies of
   international relations},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2023},
Volume = {31},
Number = {1, SI},
Pages = {169-183},
Month = {JAN 2},
Abstract = {This article examines and compares strategies of international relations
   of three European cities - Amsterdam, Barcelona and Stockholm. Amsterdam
   was chosen due to its broad human rights agenda and friendly attitude
   towards migrants, Barcelona due to its innovative International
   Relations Master Plan, and Stockholm because of its long-term vision of
   Stockholm for Everyone, which clearly takes a people-centered approach.
   The author attempts to answer the following research questions - which
   of the three strategies is the most developed one in human rights terms?
   Which is the most progressive and which propose something more or new
   rather than only recognize already existing human rights? Research
   methods include desk analysis, comparative analysis, critical analysis
   of literature and online interviews with representatives of the examined
   cities. As a result, the article shows the rising role of cities in
   implementing and promoting human rights and meeting the needs of their
   citizens which fall under the umbrella of people-centered approach.},
