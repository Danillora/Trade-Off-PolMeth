@article{ WOS:000779748500001,
Author = {McMann, Kelly and Pemstein, Daniel and Seim, Brigitte and Teorell, Jan
   and Lindberg, Staffan},
Title = {Assessing Data Quality: An Approach and An Application},
Journal = {POLITICAL ANALYSIS},
Year = {2022},
Volume = {30},
Number = {3},
Pages = {426-449},
Month = {JUL},
Abstract = {Political scientists routinely face the challenge of assessing the
   quality (validity and reliability) of measures in order to use them in
   substantive research. While stand-alone assessment tools exist,
   researchers rarely combine them comprehensively. Further, while a large
   literature informs data producers, data consumers lack guidance on how
   to assess existing measures for use in substantive research. We
   delineate a three-component practical approach to data quality
   assessment that integrates complementary multimethod tools to assess:
   (1) content validity; (2) the validity and reliability of the data
   generation process; and (3) convergent validity. We apply our quality
   assessment approach to the corruption measures from the Varieties of
   Democracy (V-Dem) project, both illustrating our rubric and unearthing
   several quality advantages and disadvantages of the V-Dem measures,
   compared to other existing measures of corruption.},
