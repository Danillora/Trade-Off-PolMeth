@article{ WOS:000745962900004,
Author = {Kao, Jay C.},
Title = {FAMILY MATTERS: EDUCATION AND THE (CONDITIONAL) EFFECT OF STATE
   INDOCTRINATION IN CHINA},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2021},
Volume = {85},
Number = {1},
Pages = {54-78},
Month = {SPR},
Abstract = {When and how does state indoctrination work? Building upon research on
   motivated reasoning and family socialization, I argue that only those
   individuals whose parents have connections to political patronage are
   subject to state indoctrination because their pro-regime biases
   transmitted from parents induce higher receptivity prior to government
   messages. Focusing on political education in China, I conduct a
   quasi-experimental analysis exploiting the sharp variation in textbook
   content generated by China's most recent curriculum reform. Results
   based on a national survey show that the new politics textbooks
   successfully affected only those individuals whose parents had worked
   for the government. The finding survives extensive robustness checks and
   falsification tests. I also consider several alternative explanations of
   the effects: preference falsification, selective attention, parental
   indoctrination, and educational quality. This paper not only highlights
   the role of intergenerational transmission in moderating the
   effectiveness of state indoctrination but also casts doubt on the actual
   degree to which regimes can change minds by changing educational
   content.},
