@article{ WOS:000611729400006,
Author = {Matejova, Miriam and Briggs, Chad M.},
Title = {Embracing the Darkness: Methods for Tackling Uncertainty and Complexity
   in Environmental Disaster Risks},
Journal = {GLOBAL ENVIRONMENTAL POLITICS},
Year = {2021},
Volume = {21},
Number = {1, SI},
Pages = {76-88},
Month = {FEB},
Abstract = {Environmental systems are complex and often difficult to predict. The
   interrelationships within such systems can create abrupt changes with
   lasting impacts, yet they are often overlooked until disasters occur.
   Mounting environmental and social crises demand the need to better
   understand both the role and consequences of emerging risks in global
   environmental politics (GEP). In this research note, we discuss
   scenarios and simulations as innovative tools that may help GEP scholars
   identify, assess, and communicate solutions to complex problems and
   systemic risks. We argue that scenarios and simulations are effective at
   providing context for interpreting ``weak signals.{''} Applying
   simulations to research of complex risks also offers opportunities to
   address otherwise overwhelming uncertainty.},
