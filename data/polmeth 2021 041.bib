@article{ WOS:000701270200008,
Author = {Balkelis, Tomas},
Title = {The Logic of Violence in the Polish-Lithuanian Conflict, 1920-1923},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2021},
Volume = {49},
Number = {5},
Pages = {911-925},
Month = {SEP},
Abstract = {The article is devoted to violence that took place in the
   Polish-Lithuanian borderland after the Great War. Using the theoretical
   insights of Stathis Kalyvas (2006), the author explores violent actors,
   types, and the dynamics of violence in the conflict over the neutral
   zone between Poland and Lithuania between 1920 and 1923. The focus is on
   the experiences of civilians and the social impact of violence on the
   formation of their national identities. The author suggests that violent
   ways of nation making forced the local populace to adopt national
   identities to ensure their security, but the process of forced
   nationalization was limited and may have resulted in the emergence of
   indifference among certain groups of the population.},
Publisher = {CAMBRIDGE UNIV PRESS},
