@article{ WOS:000722276800007,
Author = {Glebov, Sergey},
Title = {Exceptional Subjects: Koreans, Settler Colonialism, and Imperial
   Subjecthood in the Russian Far East, 1860s-1917},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2021},
Volume = {49},
Number = {6},
Pages = {1065-1081},
Month = {NOV},
Abstract = {This article traces debates and policies of the Russian imperial
   administrators toward the Korean population in the Far Eastern provinces
   of the Russian Empire. Koreans were initially treated as de facto
   members of the peasant estate, and in the 1890s many were granted the
   status of Russian subjects. Yet the rise of settler colonialism and a
   nationalizing empire from the 1880s, and especially after the Russian
   revolution of 1905, complicated the issue of Korean subjecthood and led
   to policies that excluded Koreans from the regulations normally
   applicable to peasants, such as the right to increased land allotments.
   At the same time, the neotraditionalist approach to the management of
   difference in the empire was still present in the 1910s, albeit never
   clearly articulated to compete with the nationalizing idiom.},
