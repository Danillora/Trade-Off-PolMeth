@article{ WOS:001109234400001,
Author = {Altikolatsi, Eleni and Karasmanaki, Evangelia and Parissi, Androniki and
   Tsantopoulos, Georgios},
Title = {Exploring the Factors Affecting the Recycling Behavior of Primary School
   Students},
Journal = {WORLD},
Year = {2021},
Volume = {2},
Number = {3},
Pages = {334-350},
Month = {SEP},
Abstract = {Education on recycling must be provided during the first school years
   because environmentally aware students are more likely to become
   environmentally aware citizens. The aim of the present study is to
   investigate the factors affecting the recycling behavior of students
   attending the last grade of primary school in the regional unit of Evros
   in Greece. To select the sample, multistage sampling was used, and to
   collect the research data, a questionnaire with closed-ended items was
   designed. The collected data were analyzed using descriptive statistics,
   the non-parametric Friedman test, and categorical regression. Our
   results showed that the students had positive views, attitudes, and
   behaviors in relation to recycling and were actively involved in it.
   Categorical regression analysis indicated that the other family members
   affected students' recycling behavior to a high degree, whereas
   students' parents' occupation and students' participation in
   environmental education programs at school exerted a lower influence on
   their recycling behavior.},
