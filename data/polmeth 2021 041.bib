@article{ WOS:000652790400001,
Author = {Voorberg, William and Vermeeren, Brenda and Van Buuren, Arwin},
Title = {The clear model as a predictor of candidacy for council membership},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2023},
Volume = {49},
Number = {1},
Pages = {54-77},
Month = {JAN 2},
Abstract = {This paper tests to what extent the dimensionof the CLEAR model can be
   used as reliable predictors of an individual's decision to pursue
   council membership. By using the CLEAR framework as an extension of
   known frameworks such as self-determination theory and public service
   motivation that only focus on motivation, we develop a more
   comprehensive view of an individual's decision to run for candidacy. Our
   research shows that people do not distinguish between the `can do' and
   `like to' dimensions. Rather, these two dimensions must be considered as
   three sub-dimensions in our research coined as amotivation, intrinsic
   motivation, and extrinsic motivation. Furthermore, in explaining why
   people tend to decide to run for council membership, only amotivation,
   intrinsic motivation and social support (enabled dimension) appeared to
   be reliable predictors. To further our analysis, we have analysed how
   these different dimensions differ across different societal groups.},
