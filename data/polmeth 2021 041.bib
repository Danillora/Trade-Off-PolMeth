@article{ WOS:000631951300001,
Author = {Ash, Konstantin},
Title = {How did Tunisians react to Ennahdha's 2016 reforms? Evidence from a
   survey experiment},
Journal = {MEDITERRANEAN POLITICS},
Year = {2023},
Volume = {28},
Number = {1},
Pages = {73-97},
Month = {JAN 1},
Abstract = {This study looks at public reaction to 2016 reforms made by Ennahdha,
   Tunisia's most prominent Islamist party, to formally separate from its
   religious wing. Interviews with Ennahdha's electoral strategists suggest
   they expected resurgent support from both secular Tunisians and
   beneficiaries from the party's social programmes. Nevertheless,
   Ennahdha's vote share declined in 2019's parliamentary elections. A
   survey experiment conducted in 2017 randomly primed Tunisians with news
   of Ennahdha's reforms. While the reforms themselves were viewed
   favourably by secular and educated Tunisians, the party did not gain in
   aggregate favourability and lost support among less-educated and
   non-secular Tunisians.},
