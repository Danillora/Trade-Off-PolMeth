@incollection{ WOS:000787895600002,
Author = {Sneath, David and Humphrey, Caroline and Bulag, Uradyn E.},
Book-Author = {Hoare, JP},
Title = {Gender and Development in Soviet Kyrgyzstan},
Booktitle = {GENDER, ACTIVISM, AND INTERNATIONAL DEVELOPMENT INTERVENTION IN
   KYRGYZSTAN},
Series = {Inner Asia Book Series},
Year = {2021},
Volume = {13},
Pages = {19-61},
Abstract = {In this chapter, I consider the position of different groups of women in
   pre-Soviet Central Asia, and present the gendered impacts of the Soviet
   development project in the Central Asian Soviet Socialist Republics,
   with particular reference to the Bolshevik campaigns in the 1920s to
   liberate Central Asian women and the drive during the Stalinist period
   to bring women into the labour force. I then discuss the gendered social
   contract that emerged in the later Soviet period, focusing on the
   experiences of women in the Kirgiz SSR itself. Finally, I assess the
   gendered impacts of the collapse of the Soviet state, and the transition
   to a market economy. I argue that 70 years of Soviet social policy
   regarding women and the Soviet mission to develop Central Asia resulted
   in a very particular `gender regime{''} and gendered settlement between
   the people and the state at the time of Kyrgyzstan's independence in
   1991. The way that this settlement was so suddenly and violently
   ruptured affected (and continues to affect) how my research respondents
   have subsequently engaged with the new ideas about citizenship, civil
   society, and gender equality that were introduced by international
   organisations arriving in the country in the 1990s, as will be
   considered in subsequent chapters.},
