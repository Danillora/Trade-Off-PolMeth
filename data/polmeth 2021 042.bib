@article{ WOS:000701269700010,
Author = {Reid, Ernest A.},
Title = {Third Rome or Potemkin village: Analyzing the Extent of Russia's Power
   in Serbia, 2012-2019},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2021},
Volume = {49},
Number = {4, SI},
Pages = {728-737},
Month = {JUL},
Abstract = {This past decade has seen the return of Russia into the public
   discourse. Moscow's influence has been making a comeback across its old
   sphere of influence and in other parts of the world. In particular,
   there has been a lot of talk lately of Russian soft power spreading
   across the Balkan region, especially in Serbia. However, the concept of
   power can be rather ambiguous, and only a theoretically informed
   analysis based on empirical methods can lead to genuine conclusions.
   This article aims to apply an existing theoretical framework on power to
   the context of Russian influence in Serbia, in order to find out whether
   Moscow has any power over Belgrade and to gauge its extent.},
