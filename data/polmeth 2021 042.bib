@article{ WOS:000701269300010,
Author = {Ivesic, Tomaz},
Title = {The Yugoslav National Idea Under Socialism: What Happens When a Soft
   Nation-Building Project Is Abandoned?},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2021},
Volume = {49},
Number = {1},
Pages = {142-161},
Month = {JAN},
Abstract = {Following Stalin's interpretations of the Lenin's thesis on the merging
   of the nations, the Yugoslav communists first needed to ``push{''} all
   nations to the same level of development. After the Tito-Stalin split in
   1948, the soft Yugoslav nation-building project was accelerated. During
   the 1950s, national Yugoslavism was stimulated in a latent way through
   language, culture, censuses, and changes in the constitutional and
   socialist system. By the end of the 1950s, the Yugoslav socialist
   national idea reached its peak with the 1958 Party Congress.
   Nevertheless, with the economic crisis in the early 1960s, and the
   famous Cosi-Pirjevec debate on Yugoslavism, the Yugoslav national idea
   declined. This was evident on the level of the personal, national
   identifications of the Party members, but also in the ideological shift
   of the Party's chief ideologue Edvard Kardelj. Yet, the concept of
   Yugoslavism was redefined in the second half of the 1960s without ethnic
   or national connotations. Two Yugoslavisms were created: a socialist one
   propagated by the Party and a national one that lived among the
   population in small proportions. Although the Yugoslavs were never
   recognized as a nation, that did not stop them from publicly advocating
   for their national rights.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {32 AVENUE OF THE AMERICAS, NEW YORK, NY 10013-2473 USA},
Type = {Article},
Language = {English},
Affiliation = {Ivesic, T (Corresponding Author), European Univ Inst, Dept Hist \& Civilizat, Florence, Italy.
   Ivesic, Tomaz, European Univ Inst, Dept Hist \& Civilizat, Florence, Italy.},
DOI = {10.1017/nps.2019.121},
ISSN = {0090-5992},
EISSN = {1465-3923},
Keywords = {nationalism; nation-building; communist; Southeastern Europe; national
   identity},
Keywords-Plus = {IDENTITY},
Research-Areas = {Area Studies; Ethnic Studies; History; Government \& Law},
Web-of-Science-Categories  = {Area Studies; Ethnic Studies; History; Political Science},
Author-Email = {tomaz.ivesic@eui.eu},
Affiliations = {European University Institute},
