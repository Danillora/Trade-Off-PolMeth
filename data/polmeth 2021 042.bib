@article{ WOS:000777858900001,
Author = {Fittante, Daniel},
Title = {Institutional Activism and Ethnic Intermediation in Post-Communist
   Romania},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2022},
Volume = {50},
