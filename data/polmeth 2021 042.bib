@incollection{ WOS:000787895600005,
Author = {Sneath, David and Humphrey, Caroline and Bulag, Uradyn E.},
Book-Author = {Hoare, JP},
Title = {Gender-Focused NGOs in Bishkek},
Booktitle = {GENDER, ACTIVISM, AND INTERNATIONAL DEVELOPMENT INTERVENTION IN
   KYRGYZSTAN},
Series = {Inner Asia Book Series},
Year = {2021},
Volume = {13},
Pages = {121-163},
Abstract = {In the previous chapters, I have considered the historical gender regime
   inherited from the Soviet period, as well as the policy contexts in
   which international donors arriving in Kyrgyzstan have operated since
   the 1990s, and how these shaped development programmes. In this chapter,
   I move on to analyse the local gender-focused NGO sector that has
   developed in this context, and as a direct result of the policies and
   programmes of the international organisations operating in Kyrgyzstan. I
   start by providing an overview of the NGO sector in Bishkek in general
   at the time of the research, and of the characteristics of
   gender-focused NGOs in particular, before introducing my three case
   studies: a women's crisis centre (case study 1); an umbrella
   organisation officially representing over 8o membership organisations
   (case study 2); and a youth-led lesbian, gay, bisexual, and transgender
   (LGBT) rights organisation (case study 3). I then go on to examine
   relationships between gender-focused NGOs (including my case study
   groups), looking in particular at the way that assumed hierarchies and
   processes of inclusion and exclusion are constructed, and highly
   contingent.},
