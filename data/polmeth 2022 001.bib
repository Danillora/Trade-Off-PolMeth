@article{ WOS:000866343000001,
Author = {Saunders-Hastings, Emma},
Title = {The privatized state and our own},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2023},
Volume = {26},
Number = {2},
Pages = {260-266},
Month = {FEB 23},
Abstract = {Chiara Cordelli's The Privatized State offers a powerful critique of
   privatization and an inspiring vision of the kind of democratic
   governance that could secure citizens' equal freedom. This essay raises
   questions about how Cordelli's arguments apply in non-ideal theory. It
   asks whether her arguments about the illegitimacy of privatization
   provide us with adequate reasons to reject ongoing processes of
   privatization. It also queries some of her recommendations for how
   philanthropy should be practiced by individuals and incentivized by the
   state.},
