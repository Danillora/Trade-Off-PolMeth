@article{ WOS:000853797400001,
Author = {Kinsey, Christopher and Mayer, Christopher},
Title = {A Step Too Far How the International Code of Conduct Association's
   Actions Could Unintentionally Help to Privatise War},
Journal = {RUSI JOURNAL},
Year = {2022},
Volume = {167},
Number = {3, SI},
Pages = {54-63},
Month = {OCT 27},
Abstract = {In December 2021, the International Code of Conduct for Private Security
   Service Providers' Association (ICoCA) changed the definition of
   security services, expanding its scope and undertaking roles that
   include direct participation in hostilities. Christopher Kinsey and
   Christopher Mayer argue that, in promoting this broadened scope, the
   ICoCA did so without first identifying important principles and
   standards relevant to those new functions. Critically for the
   international order, the unintended consequences of these changes could
   be far reaching, legitimising quasi-mercenary organisations and fighting
   in combat.},
