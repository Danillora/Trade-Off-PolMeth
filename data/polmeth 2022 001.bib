@article{ WOS:000753529800001,
Author = {Han, Rongbin and Shao, Li},
Title = {Scaling Authoritarian Information Control: How China Adjusts the Level
   of Online Censorship},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2022},
Volume = {75},
Number = {4},
Pages = {1345-1359},
Month = {DEC},
Abstract = {Autocracies can conduct ``strategic censorship{''} online by selectively
   targeting different types of content and by adjusting the level of
   information control. While studies have confirmed the state's selective
   targeting behavior in censorship, few have empirically examined how the
   autocracies may adjust the control level. Using data with a 6-year span,
   this paper tests whether the Chinese state scales up control over
   citizenry complaints in reaction to a series of socio-political events.
   The results show that instead of responding to mass protests and major
   disasters as previous studies have suggested, the state tends to adjust
   the control level because of political ceremonies, policy shifts, or
   leadership changes. The findings help refine the strategic censorship
   theory and offer a granular understanding of the motives and tactics of
   authoritarian information control.},
