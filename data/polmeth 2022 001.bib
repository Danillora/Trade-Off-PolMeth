@article{ WOS:000822303900001,
Author = {Tejkal, Martin and Odehnal, Jakub and Michalek, Jaroslav},
Title = {The Connection between the Risk of Terrorism and Its Economic and
   Political Determinants, Analysis, and Application for Forecasting},
Journal = {DEMOCRACY \& SECURITY},
Year = {2023},
Volume = {19},
Number = {1},
Pages = {28-57},
Month = {JAN 2},
Abstract = {The paper is focused on studying the relationship between the risk of
   terrorism and economic and political development of 13 European
   countries, quantified using a set of socio-economic and political
   indicator variables. Numbers of terrorist attacks are used as a
   terrorist activity indicator. A method of categorization of the numbers
   of terrorist attacks into a level of risk of terrorism index is
   presented. Proportional odds multinomial logit generalized linear model
   is used to describe the connection between the level of risk and the
   socio-economic and political indicator variables. The presented findings
   imply that the connection is significant for 9 out of the 13 studied
   countries. Furthermore, a possible application of the model for
   obtaining short time predictions of the probability of the levels of
   risk is presented using a real data set.},
