@article{ WOS:000895811300007,
Author = {Ghosh, Aryama},
Title = {Nehru's Elephant Envoys: Animal Modernity, Orientalist Gaze and India's
   Soft Power},
Journal = {STUDIES IN INDIAN POLITICS},
