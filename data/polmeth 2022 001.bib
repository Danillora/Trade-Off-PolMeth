@article{ WOS:000807846200003,
Author = {Borovinsky, Tomas},
Title = {Towards a new nomos on the Earth? Sovereignty and Disruption in the
   Light of Carl Schmitt},
Journal = {ANACRONISMO E IRRUPCION},
Year = {2022},
Volume = {12},
Number = {22},
Pages = {32-57},
Month = {MAY-OCT},
Abstract = {Based on the works of Carl Schmitt (1888-1985), this article proposes to
   contribute to the thought about the state of contemporary sovereignty
   and the possibility of a new nomos of the Earth. Although the work of
   Carl Schmitt is a fundamental tool for thinking about the present, in
   recent decades, the advent of the climate crisis and digital disruption
   implies a new challenge for contemporary sovereignty and an opportunity
   to raise the possibility of a new nomos of the Earth.},
