@article{ WOS:000822304400001,
Author = {Joshanloo, Mohsen},
Title = {A global index of anti-immigrant xenophobia: associations with cultural
   dimensions, national well-being, and economic indicators in 151 nations},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2024},
Volume = {12},
Number = {2},
Pages = {494-503},
Month = {MAR 14},
Abstract = {The Gallup World Poll was used to develop a global index of
   anti-immigrant xenophobia. The data were collected in 151 countries
   between 2016 and 2020. Results suggest that xenophobia has stronger
   associations with cultural variables (e.g., power distance and
   allocentrism) and well-being variables (e.g., eudaimonic well-being and
   positive affect) than with economic and social indicators (e.g.,
   national wealth, perceived injustice, and good governance).
   Globalization is not significantly correlated with xenophobia. Results
   indicate that this new global index is a valid measure that provides an
   up-to-date assessment of national xenophobia with much broader coverage
   than previous indices.},
