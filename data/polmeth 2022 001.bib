@article{ WOS:000880517500001,
Author = {Sohnius, Marie-Lou and Gschwend, Thomas and Rittmann, Oliver},
Title = {What Are the Effects of Larger Electoral Districts on Political
   Behavior? An Empirical Contribution to the Electoral Reform Debate},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2022},
Volume = {63},
Number = {4},
Pages = {685-701},
Month = {DEC},
Abstract = {What changes do enlargement of individual electoral districts bring
   about in the relationship between those who vote and those who are
   elected? The German Bundestag continues to grow with each election, but
   this growth is intended to be counteracted, among other things, by the
   work of the Commission for the Reform of Electoral Law and the
   Modernization of Parliamentary Work. One of the most discussed
   recommendations to reduce the size of the Bundestag is to reduce the
   number of electoral districts. However, that measure is often criticized
   because the associated enlargement of individual constituencies could
   lead to less closeness between citizens and the parliamentary members,
   not only in practical terms but also politically. Using data from
   academic postelection studies for all federal elections from 2002 to
   2021 in combination with structural data from all electoral districts of
   the Federal Returning Officer, we investigated the possible consequences
   for voters if the number of German federal electoral districts were
   reduced from the previous 299. The results show that larger electoral
   districts are neither associated with lower voter satisfaction with
   democracy nor political efficacy if either the population or the area of
   electoral districts increase. Thus, if electoral district amalgamation
   helps downsize the Bundestag, there would be no discernible voter
   objections, based on the factors we examined, to that reform.},
