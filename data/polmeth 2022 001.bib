@article{ WOS:000772345500001,
Author = {Jafarov, Anar and Huseynli, Ilkin},
Title = {Republican freedom, domination, and ignorability},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2022},
Volume = {15},
Number = {2},
Pages = {221-234},
Month = {MAY 4},
Abstract = {Some argue that republican freedom is impossible because since it is
   always possible that a person or a group of persons possesses arbitrary
   power to interfere with individuals, no one is free to do anything. To
   avoid this challenge, in their recent article, Sean Ingham and Frank
   Lovett invoke the notion of ignorability in terms of which they offer a
   moderate interpretation of republican freedom. On their view, B is free
   from A to phi if A's possible types who prefer to intervene with B's
   phi-ing are ignorable. They also try to show that freedom is not
   determined by the probability of an intervention. However, we argue that
   they fail to do this and that freedom as non-domination is not entailed
   by ignorability.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
