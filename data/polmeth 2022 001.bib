@article{ WOS:000828952800001,
Author = {Ruiz Collantes, F. Xavier and Sofia Cabezas, Ana},
Title = {Populist discourses and conflicts of democratic legitimacy: the Bolivian
   political crisis of 2019},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2022},
Month = {2022 JUL 16},
Abstract = {This article supports the thesis that so-called `populist' and
   `anti-populist' discourses are really demo-legitimist discourses, i.e.
   discourses in which political actors defend their own democratic
   legitimacy and attack the democratic legitimacy of their antagonist. To
   support this thesis, using a qualitative-narratological approach, we
   analysed the speeches of political leaders of the two opposing sides in
   the 2019 political crisis in Bolivia. The narrative structures of those
   speeches are identical in that they correspond to what Cas Mudde
   identifies as populism. We conclude, therefore, that populist and
   anti-populist discourses are very similar in that they both reflect
   demo-legitimist currents that come into opposition in conjunctural or
   structural situations of crises in democratic legitimacy.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
