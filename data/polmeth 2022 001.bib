@article{ WOS:000895429600001,
Author = {Eilam, Ehud},
Title = {How the Israel Defense Forces Seek to Defeat Non-State Actors},
Journal = {RUSI JOURNAL},
Year = {2022},
Volume = {167},
Number = {4-5},
Pages = {104-111},
Month = {JUL 29},
Abstract = {The Israel Defense Forces (IDF) have been at war with Hizbullah and
   Hamas, two powerful non-state actors, several times throughout Israel's
