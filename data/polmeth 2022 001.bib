@article{ WOS:000901490500001,
Author = {Gibilisco, Michael and Steinberg, Jessica},
Title = {Strategic Reporting: A Formal Model of Biases in Conflict Data},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
