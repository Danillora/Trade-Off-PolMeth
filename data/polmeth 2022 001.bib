@article{ WOS:000967077000003,
Author = {Wehowski, Matthaus},
Title = {``Fading of the state idea{''} and ``song of death{''}: Health crisis
   and Spanish flu in Upper Silesia},
Journal = {TOTALITARISMUS UND DEMOKRATIE},
Year = {2022},
Volume = {19},
Number = {2},
Pages = {257-278},
Abstract = {Upper Silesia was characterized by the struggle between two conflicting
   national movements, German and Polish/Silesian, the dominance of the
   Catholic Church and the accelerated industrialization since the second
   half of 19th century. It experienced a fast population growth,
   accompanied by the spread of infectious diseases. The First World war
   caused even more social disturbances and finally led to the erosion of
   the state's legitimacy. In autumn 1918 the more serious second wave of
   the Spanish Flu caused death and severe illness. The provincial
   administration mostly stood by its own and struggled with the
   identification of the disease and the collection of verifiable numbers.},
