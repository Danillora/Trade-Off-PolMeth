@article{ WOS:000873348100001,
Author = {Bargetz, Brigitte and Eggers, Nina Elena},
Title = {Affective Narratives: Theory and Critique of Political Mediation},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2023},
Volume = {64},
Number = {2},
Pages = {221-246},
Month = {JUN},
Abstract = {The article develops the theoretical approach of affective narratives
   with the aim of better understanding contemporary constructions of the
   political. It argues that affective narratives are a crucial mode of
   political mediation that operates both in the paradigm of a politics of
   feelings and in the paradigm of feeling politics and that touches upon
   three analytical levels: the story, the audience, and the narrators.
   Through this argument, the article also aims to contribute to ongoing
   theoretical debates on narratives and affect in politics, topics that
   have been increasingly subject to scholarly interest in political
   science for quite some time but have rarely been conceptualised in their
   connectedness. Against this background, the article elaborates on their
   mutual entanglement by offering an affective-narrative concept of agency
   that is able to elucidate how affective narratives simultaneously guide
   and legitimise politics. Finally, the article analyses a self-narrative
   by German right-wing politician Bjorn Hocke and his role therein as
   populist leader, narrator, and bearer of affects in order to exemplarily
   elaborate on the analytical benefit of the concept of affective
   narratives.},
