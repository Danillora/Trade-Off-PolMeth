@article{ WOS:000859019000001,
Author = {Richmond, Oliver P.},
Title = {On the ``Greats{''} and peace (part two): The internal and external
   challenges to the embryonic international peace architecture in
   modernity},
Journal = {CIVIL WARS},
Year = {2023},
Volume = {25},
Number = {2-3, SI},
Pages = {520-546},
Month = {JUL 3},
Abstract = {Part II of this article develops the argument that in a century of
   industrialised warfare, the international peace architecture (IPA) was
   caught in a series of contradictions. It was drawn into a delicate
   balancing act of expanding rights and decolonizing former empires,
   building law and international institutions, making peace and managing
   war. Critical arguments emerged about appropriate responses to these
   issues, drawing on, but also heavily constrained by, their genesis in
   the `Greats'. Part II of this article examines this contradictory
   process in greater detail.},
