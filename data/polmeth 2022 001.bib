@article{ WOS:000784083000001,
Author = {Cai, Kevin G.},
Title = {China's Initiatives: A Bypassing Strategy for the Reform of Global
   Economic Governance},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Year = {2023},
Volume = {8},
Number = {1, SI},
Pages = {1-17},
Month = {MAR},
Abstract = {In addition to pushing for the reform of global economic governance
   within the existing international order, China has launched the BRICS,
   the BRI and the AIIB as a bypassing strategy to pursue such a reform
   outside the existing global economic system. While these initiatives,
   particularly the BRI and the AIIB, have helped greatly enhance China's
   influence in the regional and global economy, such increased regional
   and global influence can hardly be directly transformed into the
   nation's enhanced status within the existing global economic order due
   to the limit of these initiatives. However, these initiatives have been
   posing normative and structural challenges to the existing global
   economic order dominated by the Western powers and bringing significant
   implications on the existing global economic architecture in multiple
   important areas, including patterns of regional and global trade,
   investment and infrastructure development, which will, in turn, have
   strategic impact on global economic governance.},
