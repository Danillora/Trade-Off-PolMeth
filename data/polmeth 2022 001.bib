@article{ WOS:000965899000003,
Author = {Tinoco, Cuauhtemoc Ochoa},
Title = {Balances Of The Democratic Revolution: The Politics Cultural Local Of
   The Left In Mexico City},
Journal = {REVISTA MEXICANA DE ANALISIS POLITICO Y ADMINISTRACION PUBLICA},
Year = {2022},
Volume = {11},
Number = {22},
Pages = {67-89},
Month = {JUL-DEC},
Abstract = {Culture had a relatively significant place on the political and social
   left at the time of its opposition to the PRI regime. Although
   programmatically it had a limited place, it was cultural activism,
   alternative proposals, as well as its works and its artists that gave it
   visibility and contributed to the formation of a certain political and
   ideological identity. The convergence of characters and groups of those
   left in the Party of the Democratic Revolution (PRD) and their
   subsequent arrival in the city government and in particular, the
   political delegations of the city, did not have a notable and permanent
   impact on the politics and cultural action of those administrations.
   After more than two decades in power, the balance in this area is
