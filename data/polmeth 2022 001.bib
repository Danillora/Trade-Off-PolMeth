DOI = {10.5817/PC2022--3-281},
ISSN = {1211-3247},
EISSN = {1805-9503},
Keywords = {COVID-19; pandemic; elections; retrospective voting; economic voting},
Keywords-Plus = {COVID-19; POLITICS; CHOICE},
