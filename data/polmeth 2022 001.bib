@article{ WOS:000898851300008,
Author = {Hernandez Cuevas, Luis Armando},
Title = {The Capture and Uprising of David Alfaro Siqueiros. On How to Obtain an
   Own Name in Painting},
Journal = {IZQUIERDAS},
Year = {2022},
Volume = {51},
Month = {SEP},
Abstract = {This paper aims to question what it means to acquire a proper name in
   painting. To do so, we work on a painter who was part of the Mexican
   Renaissance: David Alfaro Siqueiros (1894-1974). So, to resolve the
   leading questioning of this paper, we walk through three paths. The
   first demands to think about the apparatuses of capture with which
   painting is extracted from its revolutionary character. The second of
   them request to think about the repercussions that are at stake at
   painting people for the acquisition of our own name in painting.
   Finally, the third path makes us question the links between the logic of
   self-portraiture and the acquisition of one's own name. It should be
   noted that all the above is carried out through a conceptual work that
   echoes different names of contemporary philosophy such as Gilles
   Deleuze, Felix Guattari, and Georges Didi-Huberman, as well as
   specialists in Mexican painting and culture such as Octavio Paz, Luis
   Cardoza y Aragon, Carlos Monsivais, among others.},
