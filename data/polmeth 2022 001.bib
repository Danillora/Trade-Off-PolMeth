@article{ WOS:000846920600014,
Author = {Racz, Andras},
Title = {War, Change of Course, Continuity Hungary's Ukraine and Russia Policy},
Journal = {OSTEUROPA},
Year = {2022},
Volume = {72},
Number = {4-5},
Pages = {155+},
Abstract = {With Hungary's approval, Ukraine was declared a candidate for EU
   membership on June 23, 2022. This marked a change of course for
   Budapest. Due to bilateral conflicts with Kyiv, Budapest had previously
   blocked all of Ukraine's Euro-Atlantic ambitions. Hungary's Ukraine
   policy remains in flux. Irrespective of the war, Hungary continues to
   pursue its ``opening to the East{''} towards Russia. This strategy aims
   to expand economic cooperation. Hungary has so far supported the EU
   sanctions against Russia, but Viktor Orban makes no secret of the fact
   that he opposes them. Domestically, Orban has used the war to
   consolidate his power.},
