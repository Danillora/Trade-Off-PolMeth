@article{ WOS:000878591800003,
Author = {Gauzin, Lidya Margarita Ugalde and Cid, Gabriel Puron},
Title = {Factors of Citizen Adoption of Digital Government Services},
Journal = {REVISTA MEXICANA DE ANALISIS POLITICO Y ADMINISTRACION PUBLICA},
Year = {2022},
Volume = {11},
Number = {21},
Pages = {29-52},
Month = {JAN-JUN},
Abstract = {One of the purposes of electronic government is to offer a channel where
   the costs of procedures and public services are reduced. However,
   citizens prefer to conduct procedures through other channels, such as
   offices, banks, stores, telephone, among others, only 4.8\% carry out
   procedures on the Internet. To explain this phenomenon, the perception
   of trust towards governments and the satisfaction of expectations have
   been studied. The present investigation takes up these factors and
   presents two proposals: to analyze data on real use and not on the
