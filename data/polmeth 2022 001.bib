@article{ WOS:000911130800003,
Author = {Janusz, Andrew and Jarrin, Andy and Faerman, Tali and Bravo, Karina},
Title = {Electoral colorism: Candidate skin color and list placement in Ecuador},
Journal = {ELECTORAL STUDIES},
Year = {2023},
Volume = {81},
Month = {FEB},
Abstract = {To what extent does candidate skin color influence party list placement
   in proportional representation systems? While candidate skin color is
   increasingly understood to play an important role in politics, the
   extent to which it shapes electoral opportunities and outcomes remains
   unclear. In this paper, we investigate whether party elites in list
   proportional representation systems place darker-skinned candidates in
   lower, less advantageous list positions than their lighter-skinned
   copartisans. Drawing on party lists from Ecuador's 2021 National
   Assembly elections and an original measure of candidate skin color, we
   find evidence that candidate skin color is a significant determinant of
   list placement. This finding indicates that party lists reinforce
   color-based inequalities in political representation and reveals that a
   candidate's skin color shapes their chances of winning elected office.},
