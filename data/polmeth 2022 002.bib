@article{ WOS:000765827600006,
Author = {Ejrnaes, Anders and Jensen, Mads Dagnis},
Title = {Go Your Own Way: The Pathways to Exiting the European Union},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2022},
Volume = {57},
Number = {2},
Pages = {253-275},
Month = {APR},
Abstract = {Studies have suggested that people voting for Brexit were motivated by
   anti-globalization, anti-multiculturalism and anti-elite sentiments.
   However, little is known about how these factors are related and whether
   citizens in other member states share similar reasons for wanting to
   exit the EU. Methodologically, this question is addressed by utilizing
   path models on data from the European Social Survey, with respondents in
   17 countries. Empirically, this article reveals considerable
   cross-country variation, which implies that motivations for voting Leave
   should be assessed on a country-by-country basis. Yet, two main pathways
   are identified. First, lower education is related to more negative
   attitudes towards multiculturalism, which increases the probability of
   voting Leave. Second, lower income decreases the level of trust in the
   political establishment, which again increases the probability of voting
   Leave. Theoretically, this implies that the anti-globalization model is
   subsumed by the anti-multiculturalism and anti-elite models, giving rise
   to two new mechanisms.},
