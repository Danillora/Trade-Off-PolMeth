@article{ WOS:000921358800013,
Author = {Kraev, O. L. and Silantieva, V, M.},
Title = {COLLECTIVE MEMORY FORMATION IN PERU THROUGH THE LENS OF J.P. FELDMAN'S
   RESEARCH},
