@article{ WOS:001279176000007,
Author = {Abib, Tayane Aidar and Ventura, Mauro},
Title = {Peace-oriented media coverage of conflicts: the report of the Catalan
   Bru Rovira},
Journal = {REVISTA DE PAZ Y CONFLICTOS},
Year = {2022},
Volume = {15},
Number = {1},
Month = {JAN-DEC},
Abstract = {In this article, a theoretical and interpretive investigation about
   Peace Journalism is developed, based on the study of the narrative of
   the Catalan reporter Bru Rovira on the conflicts unleashed in Liberia in
   2004. A referential bibliography on the field of Peace Communication is
   revisited in order to identify in the report Liberia: the war of modern
   times , originally published in the Spanish newspaper La Vanguardia ,
   the procedures that characterize peace-oriented journalistic coverage,
   according to Lynch and McGoldrick (2000). It is concluded, from this
   analytical study, that the news production of Rovira presents a complex
   and contextual approach to the Liberian reality in the post-Cold War and
   interested in collecting testimonies from anonymous social actors
   involved in the conflicts.},
