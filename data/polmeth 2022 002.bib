@article{ WOS:000825379100001,
Author = {ANNISON, H. A. R. R. Y. and GUINEY, T. H. O. M. A. S.},
Title = {Populism, Conservatism and the Politics of Parole in England and Wales},
Journal = {POLITICAL QUARTERLY},
