@article{ WOS:000744085600001,
Author = {Qiu, Xiaoyan},
Title = {State Support for Rebels and Interstate Bargaining},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
Year = {2022},
Volume = {66},
Number = {4},
Pages = {993-1007},
Month = {OCT},
Abstract = {Since the end of World War II, leaders have frequently supported rebel
   groups in other countries as a coercive strategy in international
   disputes. However, the strategic rationale by which rebel groups gain
   international support is non-obvious. Many recipient groups are too weak
   to viably win and are hostile to the sponsoring state's goals. Using a
   formal model, I explain that the fundamental objective of transnational
   rebel support is to gain bargaining leverage against a rival state by
   depleting its resources to counter internal and external challenges.
   This subversive effect provides a sufficient incentive for sponsoring
   the rebels even when favourable conditions suggested by previous studies
   are absent. Sponsoring rebels is attractive even if conventional warfare
   is not comparatively costly and even if rebel and sponsor preferences
   diverge. Moreover, given the goal of destabilizing rival regimes,
   potential sponsors prefer to support weaker rebel groups and provide
   more support to them.},
