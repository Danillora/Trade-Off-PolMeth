@article{ WOS:000800275800001,
Author = {Zerilli, Linda M. G.},
Title = {Critical historiography and the problem of judgment},
Journal = {EUROPEAN JOURNAL OF POLITICAL THEORY},
Year = {2023},
Volume = {22},
Number = {3},
Pages = {490-495},
Month = {JUL},
Abstract = {Max Tomba aims to reconstruct how historical actors reconstructed the
   past to open the future in ways that diverged from the trajectory of the
   dominant modernity. Insurgent Universality would break open the dead
   logic of the juridical, political, and economic trajectory of modernity
   that limits what is given and constrains what is possible. This essay
   reflects on the practice and the role of the historian. Beyond merely
   adopting insurgents' perspectives, the historian must engage in a
   practice of critical and reflective judgment. The essay draws on
   Michel-Rolph Trouillot on the silencing of the past, Reinhard Koselleck
   on the priority of the future, and Marisa Fuentes on the limits of the
   archives for voicing marginalized points of view. It concludes by
   calling for judgment and imagination where the archives run dry.},
