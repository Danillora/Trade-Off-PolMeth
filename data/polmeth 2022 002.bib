@article{ WOS:000754976500001,
Author = {Sengul, Irem and Demir, Ebru and Sahin, Bilge},
Title = {Rebuilding peace in exile: bringing together the Women, Peace and
   Security Agenda and the International Refugee Protection Regime in
   Turkey},
Journal = {GLOBAL CHANGE PEACE \& SECURITY},
Year = {2022},
Volume = {34},
Number = {1},
Pages = {53-75},
Month = {JAN 2},
Abstract = {Refugee women are generally depicted as vulnerable and dependent
   subjects and excluded from peacebuilding efforts. This article is a
   response to the need for balancing the protection needs of refugee women
   and their participation in decision-making processes. It brings two
   different but complementary frameworks, namely the International Refugee
   Protection Regime (IRPR) and the United Nations Security Council's
   Women, Peace and Security (WPS) Agenda, into conversation through a
   gender analysis. The article shows that bringing these two frameworks
   together can overcome each other's limitations regarding refugee women's
