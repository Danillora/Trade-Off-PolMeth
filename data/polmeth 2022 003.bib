@article{ WOS:000851316900001,
Author = {Jacobs, Nicholas and Munis, B. Kal},
Title = {Place-Based Resentment in Contemporary US Elections: The Individual
   Sources of America's Urban-Rural Divide},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2023},
Volume = {76},
Number = {3},
Pages = {1102-1118},
Month = {SEP},
Abstract = {Drawing on a unique battery of questions fielded on the 2018 CCES and in
   two separate surveys-one in 2019 and the other during the 2020
   election-we study the extent to which Americans feel animus toward
   communities that are geographically distinct from their own and whether
   these feelings explain Americans' attitudes toward the two major
   political parties and self-reported vote choice. We report results on
   how place-based resentment predicted vote choice in the 2018 midterm and
   2020 general elections and how those feelings relate to other widely
   studied facets of political behavior such as partisanship and racial
   resentment. Rural resentment is a powerful predictor of vote choice in
   both election years examined.},
