@article{ WOS:000866197100001,
Author = {Lay, J. Celeste and Holman, Mirya R. and Greenlee, Jill S. and Oxley,
   Zoe M. and Bos, Angela L.},
Title = {Partisanship on the Playground: Expressive Party Politics Among Children},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2023},
Volume = {76},
Number = {3},
Pages = {1249-1264},
Month = {SEP},
Abstract = {The beliefs and behaviors of U.S. adults are increasingly sorted and
   polarized along partisan lines. We draw on studies of partisanship and
   social identity formation to argue that children develop partisanship as
   a social identity during the political socialization process. For a
   group of children, their partisan social identity produces an affective
   (and largely negative) evaluation of the political world. Analyzing
   survey data collected from 1500+ children ages 6-12 in 2017 and 2018, we
   show that some children develop a partisan identity as they learn about
   politics that operates similarly to other social identities like gender
   and race. Children's partisanship is associated with negative affective
   evaluations of politics, particularly leaders of the other political
   party. Using an innovative measurement tool, we show affective, negative
   reactions in children's open-ended responses, including when they are
   asked to draw a political leader. Other children simply learn about
   politics without developing partisan identities and thus hold more
   positive affective evaluations of the political system.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Lay, JC (Corresponding Author), Tulane Univ, Dept Polit Sci, 316 Norman Mayer Bldg, New Orleans, LA 70118 USA.
   Lay, J. Celeste; Holman, Mirya R., Tulane Univ, New Orleans, LA 70118 USA.
