@article{ WOS:000776278000003,
Author = {Benito Sanchez, Ana Belen},
Title = {Portfolio Salience in Dominant-Presidential Systems: Insights from the
   Dominican Case, 2012-2020},
Journal = {POLITICA Y GOBIERNO},
Year = {2022},
Volume = {29},
Number = {1},
Abstract = {What ministerial attributes were important under Danilo Medina's
   administration (2012-2020)? Based on the analytical framework proposed
   by Camerlo and Martinez-Gallardo (2022), we categorized the relevance of
   the organizational, strategic and discretionary powers of the Dominican
   Republic portfolios, the impact of their policies and their ability to
   influence the political agenda. We identify four groups of ministries:
   the planning group close to the president; a second group responsible
   for the country's productive strategy and also managing high-impact
   policies; a third one of low relevance ministries for the government
   action; and a fourth group of irrelevant portfolios. From 2012 to 2020,
   the coalition partners did not get relevant ministries and the cabinet
   was dominated by the weight of factionalism.},
