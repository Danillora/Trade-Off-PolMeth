@article{ WOS:000776392500001,
Author = {Jacques, Olivier and Belanger, Eric},
Title = {Deficit or Austerity Bias? The Changing Nature of Canadians' Opinion of
   Fiscal Policies},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2022},
Volume = {55},
