@article{ WOS:000824111500010,
Author = {Adeodato, Joao Mauricio},
Title = {ERISTICS IN THE LEGAL DECISION},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2022},
Volume = {124},
Pages = {291-334},
Month = {JAN-JUN},
Abstract = {The paper intends to define what should be understood by theory of legal
   argumentation, focusing the role of eristics and its relationship with
   the concepts of persuasion - to which legal argumentation is
   traditionally reduced - and decision. The methodology consists of an
   etymological archeology or conceptual phenomenology of the main
   concepts, starting from ancient Greece, where rhetoric was born. The
   text also suggests that complex contemporary society pulverizes its
   decision centers and expands the eristic character of the legal
   discourse. The article concludes by placing the anthropological
   foundations of the eristic decision in the impossibility to distinguish
   lying in human language and consequently in communication.},
