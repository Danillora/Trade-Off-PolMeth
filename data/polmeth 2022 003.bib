@article{ WOS:000770255000001,
Author = {Kim, Veda Hyunjin},
Title = {Subimperialism and Perpetual Necropower: Foundational Violence and
   Mnemonic Self-killing on Jeju Island, 1947-Present},
Journal = {JOURNAL OF GENOCIDE RESEARCH},
