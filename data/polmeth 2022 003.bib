@article{ WOS:000805522600001,
Author = {Shaw, Daron R. and Dun, Lindsay and Heise, Sarah},
Title = {Mobilizing Peripheral Partisan Voters: A Field Experimental Analysis
   From Three California Congressional Election Campaigns},
Journal = {AMERICAN POLITICS RESEARCH},
Year = {2022},
Volume = {50},
Number = {5},
Pages = {587-602},
Month = {SEP},
Abstract = {Since the early 2000s, an array of experimental research has
   demonstrated that face-to-face canvassing is the most effective form of
   get-out-the-vote campaigning. Recent scholarship, however, suggests that
   text messaging can also have powerful mobilization effects. Can the
   effects of text messaging match those of canvassing? We present a field
   experiment gauging the effects of text messaging, canvassing, mail, and
   phone calls among medium propensity evangelical Christian voters in
   three California battleground congressional districts for the 2018
   midterm election. The results show significant turnout effects
   associated with texting as well as any form of outreach followed by a
   late-October text message. This challenges the widely held notion that
   personalized contacting is required to get voters to the polls; rather,
   we find that peripheral voters-often targeted by campaigns for
   mobilization-may be receptive to anonymous but timely outreach.},
