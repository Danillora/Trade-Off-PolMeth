@article{ WOS:000863540400001,
Author = {Andrews, Ben and Pruysers, Scott},
Title = {Does Democracy Die in Darkness? An Examination of the Relationship
   between Local Newspaper Health and Turnout in Municipal Politics},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2022},
Volume = {55},
Number = {4},
Pages = {958-971},
Month = {DEC},
Abstract = {This study is, to our knowledge, the first quantitative analyses of the
   relationship between local print newspaper health and voter turnout in
   Canadian municipal elections. Municipal turnout is understudied in
   Canada, and the few studies on the topic overlook the role of local
   media. This cross-sectional study fills a gap in the literature by
   determining the relationship between local print newspaper health and
   municipal election turnout in a unique dataset of 233 populous Canadian
   municipalities. Results reveal a significant positive correlation
   between turnout and two measures of newspaper health: (1) the total
   number of newspapers per population and (2) the publication frequency of
   the largest newspaper in a municipality.},
