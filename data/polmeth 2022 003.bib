@article{ WOS:000803112600004,
Author = {Spector, Celine},
Title = {FROM ROUSSEAU TO RAWLS. THE JUSTICE THEORY AS A ``REALISTIC UTOPIA{''}?},
Journal = {TOCQUEVILLE REVIEW},
Year = {2022},
Volume = {43},
Number = {1},
Pages = {51-68},
Month = {JUN 1},
Abstract = {In the history of the philosophical uses of Rousseau, John Rawls
   undoubtedly deserves special treatment. The author of the Theory of
   Justice assumes Rousseau's legacy - even if the influence of this
   thinker remains lesser in his work than that of Kant. Resolutely turning
   his back on the anti-totalitarian reading of the Social Contract, Rawls
   fully reintegrates Rousseau into the history of liberal thought and
   makes the general will one of the pillars of the ``well-ordered
   society{''}. Rousseau allows us not only to think about justice based on
   the contract, but also the question of the ``stability{''} of a society:
   the contractualist theory exposed in the Social Contract serves to
   specify the stakes of the concept of equality, in its articulation with
   liberty; it envisages the mechanism of the general will whose object is
   to create the social conditions of justice, thanks to the basic
   institutions which ensure its foundation; it finally wonders about the
   best way to create the ``social spirit{''} or the motivations which will
   allow these institutions to perpetuate themselves. Far from being the
   precursor of totalitarianism and the gravedigger of liberty, Rousseau
   thus appears as the proponent of a just and stable society where only
   true liberty can be achieved.},
