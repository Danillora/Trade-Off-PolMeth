@article{ WOS:000745650900002,
Author = {Hankins, Keith and Thrasher, John},
Title = {Hume's Politics and Four Dimensions of Realism},
Journal = {JOURNAL OF POLITICS},
Year = {2022},
Volume = {84},
Number = {2},
Pages = {1007-1020},
Month = {APR 1},
Abstract = {Debates between realists and idealists in contemporary political theory
   have been confused by a tendency to conflate several distinct
   methodological theses. This article distinguishes between four
   dimensions of realism and shows how a novel reading of Hume's politics
   can help us make sense of the importance of these theses and the
   relationships between them. More specifically, we argue that a theory we
   call normative conventionalism can be distilled from two of Hume's more
   surprising and controversial essays, ``The Idea of a Perfect
   Commonwealth{''} and ``That Politics May Be Reduced to a Science.{''}
   This theory views norms and institutions as conventional solutions to
   problems of coordination and conflict that are historically contingent,
   but also provides us with an approach to gaining leverage on our
   practices so that we can say something about which norms and
   institutions are worth emulating.},
