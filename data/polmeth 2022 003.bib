@article{ WOS:000864736400012,
Author = {Reynolds, Bradley and Ketola, Johanna},
Title = {DIALOGUE FOR OSCE RENEWAL - SHIFTING SECURITY BACK TO THE PEOPLE},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2022},
Volume = {21},
Number = {1},
Pages = {111-119},
Month = {SPR},
Abstract = {The OSCE is the largest regional security organization globally, but a
   common understanding of the institution and the security it seeks to
   promote has been in decline since roughly 2008. The war in Ukraine and
   renewed Russian invasion in February 2022 have led many to ask how to
   sustain the OSCE past 2022. One of the conflicts at the core of the
   OSCE's current and existing malaise is the question of what the OSCE of
   today symbolizes. Ahead of the 50th anniversary of the Helsinki Final
   Act in 2025, we argue that better incorporation of the perspectives of
   civil society on the ground may help revive a genuinely comprehensive
   vision of the OSCE. This approach would allow for shifting away from
   compartmentalized security and open discussion for different perceptions
   or how to combine different perceptions of security.},
