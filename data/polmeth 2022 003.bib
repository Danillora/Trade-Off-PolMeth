@article{ WOS:000810429900001,
Author = {Kestila-Kekkonen, Elina and Koivula, Aki and Tiihonen, Aino},
Title = {When trust is not enough. A longitudinal analysis of political trust and
   political competence during the first wave of the COVID-19 pandemic in
   Finland},
Journal = {EUROPEAN POLITICAL SCIENCE REVIEW},
Year = {2022},
Volume = {14},
Number = {3},
Pages = {424-440},
Month = {AUG},
Abstract = {In this study, we explored the longitudinal relationship of political
   trust and subjective political competence during the COVID-19 outbreak.
   We also examined how levels of political trust and political competence
   were associated with compliance with COVID-19 recommendations. We used
   three-wave panel data including 2,205 observations (N = 735) to study
   changes within and between individuals from 2017 to 2020. The last round
   of the survey was conducted at the peak of the first wave of COVID-19.
   The measures included political trust, subjective political competence,
   political interest, multiple background variables, and measures for
   compliance with the social distancing recommendations. We found that
   Finns' political trust increased significantly during the first wave of
   COVID-19. The between-individuals analysis suggested that political
   competence was negatively associated with trust. The results also
   indicated that citizens' compliance with COVID-19 recommendations was
   directly linked to political trust but depended also on the interaction
   between competence and trust.},
