@article{ WOS:000879052500002,
Author = {Brennan, Bernard and Johnson, Kristin and Rundlett, Ashlea},
Title = {Human Rights During Transition: Accountability Mechanisms in Mexican
   States 1997-2008},
Journal = {JOURNAL OF POLITICS IN LATIN AMERICA},
Year = {2022},
Volume = {14},
Number = {3},
Pages = {264-288},
Month = {DEC},
Abstract = {This paper assesses the impact of political and fiscal accountability
   mechanisms on the severity and scope of human rights violations in
   Mexico's states during its democratic transition. First, we argue that
   elites in states with lower levels of electoral accountability are free
   of constraints that would normally result in their removal from office
   should they engage in human rights violations. Similarly, we argue that
   local elites maintain power while evading accountability when supported
   by transfers from the central government, and are thereby freer to use
   force. We find statistical support for our claims that lower levels of
   procedural democracy and higher levels of central transfers are
   associated with higher levels of human rights and physical integrity
   rights violations. These results are robust to subsample and numerous
   economic and other factors. Our findings suggest that particularly
   during democratic transitions, subnational institutions are salient in
   explaining the frequency of human rights violations.},
