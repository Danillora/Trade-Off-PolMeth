@article{ WOS:000821029900001,
Author = {Bures, Oldrich and Stojanov, Robert},
Title = {Securitization of Immigration in the Czech Republic and Its Impact on
   the Czech Migration Policy: Experts' Perceptions},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2023},
Volume = {70},
Number = {6, SI},
Pages = {679-689},
Month = {NOV 2},
Abstract = {This article offers an exploratory analysis of experts' perceptions of
   securitization of immigration in the Czech Republic and its impact on
   Czech migration policy in the aftermath of the 2015-2016 European
   ``migration crisis.{''} Our findings indicate that the interviewed
   experts' perceptions correspond more to the logic of the exception than
   the routine: the importance of day-to-day management is less frequently
   emphasized than the elite-level security-oriented discourses and their
   acceptance by the Czech public. Our respondents also identify the
   relative stability and continuity of the Czech migration policy as a
   positive repercussion of securitization.},
