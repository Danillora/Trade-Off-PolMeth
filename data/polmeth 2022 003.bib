@article{ WOS:000744094400001,
Author = {Godefroidt, Amelie},
Title = {How Terrorism Does (and Does Not) Affect Citizens' Political Attitudes:
   A Meta-Analysis},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
