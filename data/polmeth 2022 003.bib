@article{ WOS:000923769500014,
Author = {Kirval, Levent},
Title = {THE IMPACT OF TURKIYE AND ISRAEL'S RAPPROCHEMENT ON THE MARITIME
   DELIMITATION DISPUTE IN THE EASTERN MEDITERRANEAN},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2022},
Volume = {21},
Number = {3},
Pages = {133-140},
Month = {FAL},
Abstract = {Turkiye and Israel relations have recently improved with ambassadors'
   mutual appointment to the respective countries. This recent
   rapprochement between the two essential states of the Middle East also
   influences the ongoing political disputes in the seas of this region. As
   is well known, Turkiye, Greece, and Cyprus (North and South) have been
   in conflict regarding the delimitation of maritime borders in the
   Eastern Mediterranean. In this conflict, Greece and Southern Cyprus have
   developed strong alliances with western countries, the most important
   ones being the U.S. and the EU. The recent rapprochement between Turkiye
   and Israel may change the game in the region and improve Turkiye's hand
   with regard to the Eastern Mediterranean dispute.},
