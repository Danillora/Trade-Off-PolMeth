@article{ WOS:000911309000013,
Author = {Capelos, Tereza and Salmela, Mikko and Krisciunaite, Gabija},
Title = {Grievance Politics: An Empirical Analysis of Anger Through the Emotional
   Mechanism of Ressentiment},
