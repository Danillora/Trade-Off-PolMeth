@article{ WOS:000854505300001,
Author = {Floyd, Jonathan},
Title = {Can real actions justify realist principles? Normative behaviourism as a
   member of the realist family},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2023},
Volume = {26},
Number = {3, SI},
Pages = {356-375},
Month = {APR 16},
Abstract = {If Alison McQueen is right that there is a broad `family' of realist
   approaches to political theory, then it follows there are several ways
   of `doing' realism, as illustrated by this collection. Here, I set out
   one such way, normative behaviourism, by explaining its realist
   character on four fronts: Its starting point; its values; its ambitions;
   and its treatment of a shared problem. The argument then considers two
   key objections to the described approach, both of which affect a range
   of possible realisms.},
