@article{ WOS:000845933400001,
Author = {Albrecht, Holger and Loewe, Markus},
Title = {States or social networks? Popular attitudes amid health crises in the
   Middle East and North Africa},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
