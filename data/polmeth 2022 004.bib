@article{ WOS:000759629800001,
Author = {Friesen, Paul},
Title = {The partisan consequences of liberation mentality: Ruling party support
   in Zimbabwe across space and time},
Journal = {PARTY POLITICS},
Year = {2023},
Volume = {29},
Number = {2},
Pages = {306-321},
Month = {MAR},
Abstract = {In autocratic-leaning countries, the level of genuine public support for
   ruling parties and reasons driving it are often unclear. While such
   parties often rely on violence, electoral manipulation, and corruption
   to help maintain power, maintaining legitimate popularity is the ideal.
   Parties that emerged from national liberation struggles and other
   extraordinary events are thought to have developed strong branding and
   organizational structures. This paper tests the determinants of ruling
   party identification among Zimbabweans through seven geo-coded rounds of
   Afrobarometer data. First, genuine ruling party support is estimated to
