@article{ WOS:000921362000011,
Author = {Markedonov, S. M. and Neklyudov, N. Y. and Suchkov, M. A.},
Title = {NON-MILITARY ASPECTS OF POLITICAL INTERACTION IN THE CAUCASUS:
   COMPARATIVE ANALYSIS OF US AND RUSSIA APPROACHES},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2022},
