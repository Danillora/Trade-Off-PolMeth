@article{ WOS:000837182800031,
Author = {Voronkova, Valentyna and Nikitenko, Vitalina and Bilohur, Vlada and
   Oleksenko, Roman and Butchenko, Taras},
Title = {Conceptualization of smart-philosophy as a post-modern project of
   non-linear pattern development of the XXI century},
