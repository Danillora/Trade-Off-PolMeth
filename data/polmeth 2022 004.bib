@article{ WOS:000752865800001,
Author = {Dada, Zubair Ahmad and Wani, Mehraj Din and Shah, Shamim Ahmad},
Title = {The impact of pilgrim tourism on the bilateral diplomacy of India and
   Nepal and the mediation effect of people-to-people contact},
Journal = {ASIAN JOURNAL OF COMPARATIVE POLITICS},
