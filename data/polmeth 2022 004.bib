@article{ WOS:000872153500008,
Author = {Jablonski, Ryan S. and Buntaine, Mark T. and Nielson, Daniel L. and
   Pickering, Paula M.},
Title = {Individualized Text Messages about Public Services Fail to Sway Voters:
   Evidence from a Field Experiment on Ugandan Elections},
Journal = {JOURNAL OF EXPERIMENTAL POLITICAL SCIENCE},
Year = {2022},
Volume = {9},
Number = {3},
Pages = {346-358},
Abstract = {Mobile communication technologies can provide citizens access to
   information that is tailored to their specific circumstances. Such
   technologies may therefore increase citizens' ability to vote in line
   with their interests and hold politicians accountable. In a large-scale
   randomized controlled trial in Uganda (n = 16,083), we investigated
   whether citizens who receive private, timely, and individualized text
   messages by mobile phone about public services in their community
   punished or rewarded incumbents in local elections in line with the
   information. Respondents claimed to find the messages valuable and there
   is evidence that they briefly updated their beliefs based on the
   messages; however, the treatment did not cause increased votes for
   incumbents where public services were better than expected nor decreased
   votes where public services were worse than anticipated. The
   considerable knowledge gaps among citizens identified in this study
   indicate potential for communication technologies to effectively share
   civic information. Yet the findings imply that when the attribution of
   public service outcomes is difficult, even individualized information is
   unlikely to affect voting behavior.},
