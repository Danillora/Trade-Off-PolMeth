@article{ WOS:000860928200005,
Author = {Kosevich, Ekaterina},
Title = {Russia's Relations with the Countries of Latin America at the Beginning
   of the 21st Century: Four Levels of Interstate Interaction},
Journal = {RUSSIAN POLITICS},
Year = {2022},
Volume = {7},
Number = {3},
Pages = {450-484},
Month = {SEP},
Abstract = {This article examines the main stages in the development of relations
   between Russia and Latin America from 2OOO to 2O22. The research covers
   the entire set of bilateral cooperation between Russia and Latin
   American countries, and the internal and external factors influencing
   their evolution. The article presents the author's view of interstate
   interactions from two perspectives - the foreign policy of Russia, and
   the national and international objectives of leading Latin American
   countries. The author concludes that for Russian foreign policy in the
   21st century, all the countries of this region can be divided into four
   conditional groups: traditional partners, ideological allies, trade
   partners, and low priority states. This mapping of the region can
   explain the peculiarities of the formation of dialogue between Moscow
   and Latin American countries, and the possibilities and limits of
   interstate and interregional cooperation.},
