@article{ WOS:001017290500002,
Author = {Vommaro, Pablo and Rovelli, Laura},
Title = {Challenges of research assessment oriented towards knowledge
   mobilization in the transition to open science: an analysis based on the
   case of the Working Groups from the Latin American Council of Social
   Sciences},
Journal = {ANALECTA POLITICA},
Year = {2022},
Volume = {12},
Number = {23},
Month = {JUL-DEC},
Abstract = {Various recent debates and initiatives have once again brought to the
   forefront the need to reform research evaluation and its greater link
   with society, based on a growing openness, collaboration and
   participation in the field of knowledge, following some of the
   principles of transition to open science. The foregoing entails multiple
   challenges for the design of research promotion instruments and their
