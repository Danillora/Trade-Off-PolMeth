@article{ WOS:000980036600004,
Author = {Truccone, Marilina},
Title = {Rights and Social Conflicts in Peronism?s Letters in the Province of La
   Rioja- Argentina (1951-1952)},
Journal = {CIENCIA POLITICA},
Year = {2022},
Volume = {17},
Number = {34},
Pages = {81-111},
Month = {JUL-DEC},
Abstract = {The present paper addresses a problematization between the demands for
   rights and the emergence of social conflicts during the first Peronism
   in the province of La Rioja. In the arti-cle, we propose to analyze the
   dimensions reached by the democratization of the social wel-fare in this
   provincial context, based on the conflictive expression of demands for
   new rights. Reflecting on the sending of letters to government
   authorities, our analysis is based on letters from the campaign ``Peron
   wants to know what his people need{''} to warn of the emergence of
   diverse conflicts that recover the daily experiences of men and women;
   in which identity processes are manifested between these conflicts and
   political experience of Peronism. Thus, it can be anticipated that, in
   these sources, the modulation of demands is composed, on the one hand,
   around the expression of social injustices and, on the other hand, in
   the delimita-tion of the community.},
