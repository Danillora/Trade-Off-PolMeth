@article{ WOS:000765823200010,
Author = {Grosfoguel, Ramon},
Title = {The four genocides/epistemicides of the long sixteenth century and the
   racist/sexist knowledge structures of modernity in the occidental
   university},
Journal = {IZQUIERDAS},
Year = {2022},
Volume = {51},
Month = {MAR},
Abstract = {This article analyzes the foundational epistemic racism/sexism in the
   knowledge structures of the westernized university. It argues that the
   epistemic privilege of Western man in the knowledge structures of
   Westernized universities is the result of four genocides/epistemicides
   in the 16th century: against the Jewish and Muslim population in the
   conquest of Al-Andalus, against the indigenous peoples in the conquest
   of the American continent, against kidnapped and enslaved africans in
   the American continent and against burned alive women accused of being
   witches in Europe. Which were the historical processes that produced
   knowledge structures founded on epistemic racism/sexism? Since the
   Cartesian legacy has had such an influence on Western knowledge
   structures, the article begins with a discussion of Cartesian
   philosophy. The second part deals with the conquest of Al-Andalus. The
   third on the conquest of the American continent and its implications for
   the population of Jewish and Muslim in 16th century in Spain, as well as
   for the African population forcibly removed from Africa and enslaved on
   the American continent. The fourth part deals with the
   genocide/epistemicide against Indo-European women burned alive by the
   Christian church on charges of witchcraft. The last part deals with
   Enrique Dussel's ``transmodernity{''} project and what it means to
   decolonize the westernized university.},
