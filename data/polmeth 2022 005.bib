@article{ WOS:000891666900001,
Author = {Ammassari, Sofia},
Title = {It depends on personal networks: Feelings of stigmatisation among
   populist radical right party members},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
