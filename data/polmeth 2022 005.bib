@article{ WOS:000864772300001,
Author = {Lucas, Caleb and Appel, B. J. and Prorok, A. K.},
Title = {Not Too Distant: Grievance, Opportunity, and the Onset of Civil War},
Journal = {CIVIL WARS},
Year = {2022},
Volume = {24},
Number = {4},
Pages = {497-523},
Month = {OCT 2},
Abstract = {Grievance and opportunity theories dominate research on the causes of
   civil war. However, theoretical and empirical problems limit their
   ability to explain variation in conflict onset. We argue that these
   problems partly stem from treating them as largely independent
   explanations. We integrate grievance and opportunity into a unified
   theory and argue that they are complements that jointly predict
   conflict. We apply insights from the interstate war literature to
   develop and test a theory of conflict onset that argues that the
   probability of civil war increases as the disparity between relative
   power and the status quo distribution of benefits increases.},
