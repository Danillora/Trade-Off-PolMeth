@article{ WOS:000855810400001,
Author = {Kearney, Jarlath and Shirlow, Peter and Tannam, Etain},
Title = {Partition to Partnership to Brexit Strategically Reinvigorating the
   Belfast Good Friday Agreement},
Journal = {RUSI JOURNAL},
Year = {2022},
Volume = {167},
Number = {3, SI},
Pages = {14-26},
Month = {OCT 27},
Abstract = {Dublin Castle stands as a signifier of both conflict and conciliation
   between Ireland and Britain over recent centuries. On 16 January 2022,
   all of Ireland's mainstream political parties gathered there on the
   centenary of the castle's handover by the last lord lieutenant of
   Ireland, Viscount FitzAlan of Derwent, to the provisional government of
   Ireland led by Michael Collins, which took place after the Dail endorsed
   the 1921 Treaty. Jarlath Kearney, Peter Shirlow and Etain Tannam show
   that a century after that treaty and the partition of the island, the
   facts and themes flowing from that period - particularly ongoing
