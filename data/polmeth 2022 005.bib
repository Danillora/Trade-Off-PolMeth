@article{ WOS:000904716400003,
Author = {Pletenac, Tomislav},
Title = {Call to Remain Responsible as a Sign of Cultural Trauma},
Journal = {POLITICKA MISAO-CROATIAN POLITICAL SCIENCE REVIEW},
