@article{ WOS:000724706300002,
Author = {Kukovic, Simona},
Title = {HOW NOVEL CORONAVIRUS HAS SHAKEN PUBLIC TRUST IN DECISION-MAKING
   INSTITUTIONS: COMPARATIVE ANALYSIS OF SELECTED EUROPEAN UNION MEMBERS},
