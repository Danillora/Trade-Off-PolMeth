@article{ WOS:000863767800001,
Author = {Ryan, Megan and Mai Van Tran},
Title = {Democratic backsliding disrupted: The role of digitalized resistance in
   Myanmar},
Journal = {ASIAN JOURNAL OF COMPARATIVE POLITICS},
Year = {2024},
Volume = {9},
Number = {1, SI},
Pages = {133-158},
Month = {MAR},
Abstract = {More than one year since its coup, the Myanmar military has neither
   established effective control of the territory nor crushed online
   dissent. What factors have enabled the resistance forces to deny the
   consolidation of military rule? We address this question by building a
   novel theoretical framework that incorporates the role of long-standing
   digitalized pro-democracy activism and by conducting a mixed methods
   analysis that includes an original, largely representative sample of
   public Facebook posts in post-coup Myanmar. We find that the development
   of online and hybrid pro-democracy activism against digital abuse and
   other illiberal policies under previous quasi-civilian governments
   enabled anti-coup resistance forces to thwart the military's attempt at
   authoritarian revival in 2021. Our research findings deepen
   understanding of Myanmar's post-coup contestation dynamics as well as
   other cases of unpopular autocratization in the digital age.},
