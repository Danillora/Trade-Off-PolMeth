@article{ WOS:000885341300001,
Author = {Wagner, John K. and Wiezel, Adi},
Title = {Do Partisans Always Like Their Inparty and Dislike Their Outparty? An
   Analysis of Partisans Across the Affective Spectrum},
Journal = {AMERICAN POLITICS RESEARCH},
