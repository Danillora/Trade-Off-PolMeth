@article{ WOS:000827031700001,
Author = {Tchouikina, Sofia},
Title = {Exhibitions on Soviet Repressions at the Moscow NGO Memorial
   (2011-2021): Transforming Family Memory into Cultural Memory},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2023},
Volume = {70},
Number = {5, SI},
Pages = {557-569},
Month = {SEP 3},
Abstract = {The article analyzes the curation of the history of Soviet repressions
   at the temporary exhibitions organized by the International Memorial NGO
   in Moscow from 2011 to 2021. Compared with other Russian museums,
   Memorial relies mainly on family archives and family memories. The
   repressions are treated as a collective experience that affected not
   only victims, those executed or imprisoned, but also their relatives and
   descendants. Seen from this perspective, the carceral past is directly
   linked to today's population. The article explores the various ways in
   which Memorial draws on families' memories of repression to create a
   museum statement.},
