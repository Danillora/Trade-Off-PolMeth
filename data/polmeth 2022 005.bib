@article{ WOS:000756187400001,
Author = {Fernandez de Mosteyrin, Laura and Martini, Alice},
Title = {Meaning and context in analysing extremism: the banalisation of the
   far-right in Spanish public controversies},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2022},
Volume = {15},
Number = {1, SI},
Pages = {38-60},
Month = {JAN 2},
Abstract = {Drawing on the analysis of public controversies around violence, this
   article examines the construction of ``extremism{''} in Spain. We
   explore how differences in public understanding of the limits of the
   (un)acceptable end up normalising the far right. Our article examines
   four public controversies: a public debate on terrorism glorification;
   an episode of collective violence; the deployment of violent speech in
   social media by a group of retired military personnel; and an episode of
   anti-Semitic speech in a Francoist memorialisation. We argue that the
   singularities of each country's political culture play a role in the
   understanding of extremism, so while certain political expressions are
   constructed as extreme, others remain in ``the realm of the normal{''}.
   Our findings shed light on the process in which far-right discourse is
   softened and clarifies some challenges faced by Preventing Violent
   Extremism (PVE) paradigms.},
