@article{ WOS:000773507400003,
Author = {Ramos, Jennifer M. and Raab, Nigel},
Title = {Russia Abroad, Russia at Home: The Paradox of Russia's Support for the
   Far Right},
Journal = {RUSSIAN POLITICS},
