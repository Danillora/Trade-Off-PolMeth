@article{ WOS:000744610600001,
Author = {Piper, Christopher},
Title = {Going for Goals: Presidential Appointments and Agency Goal Change},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2022},
Volume = {52},
Number = {1},
Pages = {140-167},
Month = {MAR},
Abstract = {With dozens of mandates, federal agencies have considerable discretion
   in how they focus their time and resources. In this article, I explain
   how presidents influence agency goals, focusing on how appointee teams
   are essential to prioritizing and resetting agency goals. Using original
   data on federal agency goals from the Obama administration to the Trump
   administration, my analysis of strategic and priority goals across 56
   agencies reveals that agencies in which the president has a higher
   percentage of key appointed positions filled experienced higher levels
   of long-term goal change. This finding highlights the president's
   immediate need to get their appointee teams in place to alter the course
   of agency policymaking for their whole first term.},
