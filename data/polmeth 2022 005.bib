@article{ WOS:000817270500001,
Author = {Nwakanma, Adaugo Pamela},
Title = {Theorizing justice from the margins: Black feminist insights on
   political (protest) behavior},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2023},
Volume = {11},
Number = {5},
Pages = {1190-1202},
Month = {OCT 20},
Abstract = {Variations in perceptions of and attitudes towards justice animate how
   different people decide to engage in politics. In the US, political
   attitudes typically vary across salient identity markers such as race,
   gender, and socio-economic class. Drawing on insights from the Black
   Lives Matter movement, this research note advances a theoretical
   framework for understanding justice and political protests through a
   transnational Black feminist lens. Here, I consider political behavior
   through the Black feminist frame of interlocking identities and the
   notion of ``the personal being political.{''} I examine the diverse ways
   in which Black feminism provides novel insights for better understanding
   justice and political behavior in terms of theory, methodology, and
   praxis.},
