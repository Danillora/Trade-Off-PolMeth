@article{ WOS:000822503100002,
Author = {Camino, Edorta},
Title = {The non-response to ideological self-placement: a social distribution of
   silence},
Journal = {REVISTA DE INVESTIGACIONES POLITICAS Y SOCIOLOGICAS},
Year = {2022},
Volume = {21},
Number = {1},
Abstract = {This paper analyzes the incidence of non-response (don't know/no
   opinion) in the variable on ideological self-study raised in the surveys
   of the Centro de Investigaciones Sociologicas (CIS). The study seeks to
   find the profile of people who opt for this option of silence in the
   face of the dichotomous continuum between having left wing or right wing
   ideology. Through a statistical analysis of binary logistic regression,
   the work asks if behind this silence there is a majority proportion of
   people who have an apolitical ideology and/or they abstain in the
   electoral elections. A silence, moreover, that occurs more frequently
   among groups less favored by the world economic system: women, the
   elderly, workers, peasants and people with primary education.},
