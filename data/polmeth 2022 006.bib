@article{ WOS:000793713700031,
Author = {Didkivska, Galina and Miroshnychenko, Serhiy and Zavydniak, Iryna and
   Biriukova, Inna and Hmyrin, Andrii and Lopashchuk, Dmitry},
Title = {International Cooperation in Investigating Economic Crimes of
   Transnational Nature},
Journal = {CUESTIONES POLITICAS},
Year = {2022},
Volume = {40},
Number = {72},
Pages = {531-542},
Month = {JAN-JUN},
Abstract = {The purpose of the research is to highlight international cooperation in
   investigation of economic crimes of a transnational nature. The main
   content. it is emphasized that the concept of ``international
   cooperation in fight against crimes{''} is in the sphere of action of
   various legal systems of both international public law and domestic law
   of states (countries) taking part in cooperation.{''}. It is established
   that it is necessary to clearly delineate the subject of international
   legal regulation of this type of interaction between states and
   international organizations. Methodology. Review of materials and
   methods based on analyzing documentary materials of international
   cooperation in investigating economic crimes of transnational nature.
   Conclusions. Due to objective reasons and circumstances modern
