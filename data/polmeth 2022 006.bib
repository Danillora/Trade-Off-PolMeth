@article{ WOS:000880001200036,
Author = {Kovalska-Pavelko, Iryna and Vyhivska, Oksana and Voropayeva, Tatiana and
   Olyanych, Valentyna and Babichev, Oleksandr},
Title = {The Russian-Ukrainian War of 2014-2022: A Historical Retrospective},
