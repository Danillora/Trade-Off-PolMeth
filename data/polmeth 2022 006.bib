@article{ WOS:000898638000001,
Author = {Lehmann, Felix},
Title = {Talking about Europe? Explaining the salience of the European Union in
   the plenaries of 17 national parliaments during 2006-2019},
Journal = {EUROPEAN UNION POLITICS},
Year = {2023},
Volume = {24},
Number = {2},
Pages = {370-389},
Month = {JUN},
Abstract = {To provide accountability, Members of Parliament (MPs) are expected to
   publicly communicate European Union (EU) affairs. Yet, parliamentary
   communication of EU affairs remains underexplored and existing analyses
   are conflicting. Against this background, this article sheds light on
   what drives MPs to talk about the EU, utilizing a novel dataset based on
   over 20,000 plenary protocols from 17 parliaments during 2006-2019. The
   study highlights that EU mentions increase in connection to referendums,
   summits, treaty ratifications, and European elections. Additional
   drivers include transfers of authority to the EU level, EU legislation,
   and political incentives. The findings provide some nuance to more
   pessimistic evaluations of the EU's accountability deficit, but major
   legitimacy challenges remain concerning national elections and Eurozone
   integration.},
