@article{ WOS:000875979300001,
Author = {Passarelli, Gianluca and Bergman, Matthew},
Title = {Runoff Comebacks in Comparative Perspective: Two-Round Presidential
   Election Systems},
Journal = {POLITICAL STUDIES REVIEW},
