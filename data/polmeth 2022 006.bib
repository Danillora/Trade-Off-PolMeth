@article{ WOS:000880001200044,
Author = {Drozd, Valentyna and Tsutskiridze, Maksym and Burlaka, Vladyslav and
   Romanov, Maksym and Pohoretskyi, Mykola},
Title = {Considerations on the reform of Ukraine's wartime criminal justice
   system},
Journal = {CUESTIONES POLITICAS},
