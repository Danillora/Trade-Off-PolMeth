@article{ WOS:000824647800001,
Author = {Zafirovski, Milan},
Title = {Toward a new political democracy index: Construction, validation, and
   calculation for individual societies and types of society},
Journal = {POLITICS \& POLICY},
Year = {2022},
Volume = {50},
Number = {4},
Pages = {663-699},
Month = {AUG},
Abstract = {The article proposes a new composite index of political democracy. It
   first constructs, elaborates, and rationalizes a comprehensive democracy
   index composed of 20 indicators and covariates. It then describes data
   on the democracy indicators and their sources and properties, including
   their metrics and range or scale which it makes uniform and detailed for
   precision and discrimination in index computation and estimation. It
   reports the results of an empirical analysis. Preliminary statistical
   analyses test for validation and their results indicate that the
   democracy indicators and index have high validity and reliability and
   confirm that its dimensions express the same latent variable.
   Exploratory substantive analyses compute present absolute and
   standardized democracy indexes both for individual societies such as 36
   OECD countries and for types of society, specifically varieties of
   modern capitalism. The findings indicate wide variations among both
   individual societies and types of society, notably that coordinated,
   welfare capitalism surpasses the other varieties of capitalism in
   liberal political democracy. The article seeks to contribute to a better
   conceptualization, representation, and measurement of democracy in
   relation to society and its types.},
