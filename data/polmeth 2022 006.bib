@article{ WOS:000752139400001,
Author = {van der Does, Ramon and Kantorowicz, Jaroslaw},
Title = {Political exclusion and support for democratic innovations: evidence
   from a conjoint experiment on participatory budgeting},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2023},
Volume = {11},
Number = {4},
Pages = {947-955},
Month = {OCT},
Abstract = {Citizens that tend to experience political exclusion are often more
   supportive of direct and participatory forms of decision-making. We
   empirically verify two competing explanatory logics for such high
   support: the ``antiestablishment{''} logic, which expects politically
   excluded citizens to unconditionally express more support than their
   fellow citizens for democratic innovations (DIs); and the
   ``instrumental{''} logic, which expects politically excluded citizens to
   only express more support for DIs than other citizens when these
   innovations offer procedural control and favorable outcomes. Based on a
   conjoint analysis of Dutch citizens' preferences for participatory
   budgeting, we find no support for the anti-establishment logic and
   partial support for the instrumental logic. We show how measures of
   citizens' own feelings of exclusion help to explain the results.},
