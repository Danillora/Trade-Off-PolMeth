@article{ WOS:000905161900006,
Author = {Fuentes, Israel Arcos},
Title = {Conciliatory politics and politics as hostility. A reading about Carl
   Schmitt and Richard Rorty},
Journal = {REVISTA DE INVESTIGACIONES POLITICAS Y SOCIOLOGICAS},
Year = {2022},
Volume = {21},
Number = {2},
Abstract = {This article presents Richard Rorty's and Carl Schmitt's political
   conceptions. In particular, we draw from Schmitt's politics of
   hostility, founded on antagonism and the need to configure an enemy to
   reaffirm an essentialist `demos'. On the contrary, we show Rorty's
   conciliatory politics, which seeks the fraternity between strangers,
   inviting them to form part of a liberal, contingent, and open community
   to eradicate suffering and injustices. Considering both perspectives of
   political theory, human nature, society, and alterity, we analyze the
   present time in the light of these authors' legacy and reflections.},
