@article{ WOS:000798561100014,
Author = {Sierra Caballero, Francisco and Moreno Galvez, Francisco Javier},
Title = {Bolivar Echeverria and the critique of modernity: Contributions to a
   Marxist theory of culture},
Journal = {IZQUIERDAS},
Year = {2022},
Volume = {51},
Month = {MAR},
Abstract = {This work centres on the contribution of Bolivar Echeverria to critical
   theory and in particular the relevance of his challenge to Eurocentrism,
   an important element of Marxist debates in and about Latin America.
   Echeverria's cultural materialism has two key foundations: the
   recognition of the use-value or natural form of social reproduction and
   his critique of the one-dimensional conception of capitalist modernity.
   His argument for the existence of diverse modernities and a particular
   form of baroque ethos involves a decolonial critique of power and a
   deconstruction of Eurocentrism that brings to the forefront the
   contradiction, conflict and forms of resistance that persist in the
   dispute for the construction of alternative modernities.},
Publisher = {ARIADNA EDICIONES \& SAINT PETERSBURG STATE UNIV},
