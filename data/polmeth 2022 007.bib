@article{ WOS:000747778400005,
Author = {Foos, Florian and Bischof, Daniel},
Title = {Tabloid Media Campaigns and Public Opinion: Quasi-Experimental Evidence
   on Euroscepticism in England},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
