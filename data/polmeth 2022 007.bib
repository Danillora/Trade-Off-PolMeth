@article{ WOS:000868564700003,
Author = {Feddersen, Alexandra and Adams, James F.},
Title = {Public opinion backlash in response to party messages: A case for party
   press releases},
Journal = {ELECTORAL STUDIES},
Year = {2022},
Volume = {80},
Month = {DEC},
Abstract = {Empirical studies report only weak and inconsistent evidence that
   citizens react to parties' policy statements in election manifestos. We
   extend this research to analyze citizens' reactions to party messages
   published in press releases. We code over 1500 press releases about
   migration policy that Swiss parties released between 2007 and 2017, and
   we analyze how citizens adjusted their policy views in response to
   parties' stated positions. We identify party persuasion effects whereby
   citizens take policy cues from their preferred party, and strong
   backlash effects whereby citizens shift their positions away from the
   stated positions of parties they dislike. The finding of backlash
   effects in (relatively) consensual Swiss politics substantiates that
   this phenomenon is not confined to polarized political contexts, such as
   the US or to public reactions to extremist parties.},
