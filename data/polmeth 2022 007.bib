@article{ WOS:000806492000004,
Author = {Wuest, Joanna},
Title = {After Liberation: Sex, Social Movements, and Capital Since the New Left},
Journal = {POLITY},
Year = {2022},
Volume = {54},
Number = {3},
Pages = {478-502},
Month = {JUL 1},
Abstract = {This article asks what New Left era liberation theory can illuminate
   about capitalism, corporate power, and minority oppression in American
   politics today. Over the past forty years, rising rates of legal rights
   and social acceptance of LGBTQ+ persons have accompanied parallel trends
   in economic immiseration and business influence in politics. Given that
   sexual and gender minorities are disproportionately affected by economic
   inequalities, scholars and activists alike have invoked the New Left
   experience to distinguish a radical liberation politics from an
   assimilationist and pluralist interest-group agenda. They have done so
   with the expectation that liberation theory offers a robust account of
   capitalism, patriarchy, and racial oppression's interlocking natures as
   well as a means of charting a path beyond this uneven egalitarianism. In
   reassessing the legacy of New Left political thought, this article
   posits that such accounts-which have cast capitalism as a system
   maintaining a strict regulation of gender and sexual conformity and have
   exalted the wealth redistributionist potential of identity-based
   movements-are ill-equipped to explain the complex relationship between
   corporate power and minority political struggles. Throughout, the
   article draws out lessons for those seeking to interpret and to
   transform the uneven egalitarian state of contemporary American
   politics.},
