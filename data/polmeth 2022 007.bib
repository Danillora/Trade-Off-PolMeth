@article{ WOS:000818531700001,
Author = {Berdahl, Loleen and Malloy, Jonathan and Young, Lisa},
Title = {Doctoral Mentorship Practices in Canadian Political Science},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2022},
Volume = {55},
Number = {3},
Pages = {709-720},
Month = {SEP},
Abstract = {Supervisors shape the PhD student experience and play a critical role in
   students' development. To what extent and in what ways are faculty
   engaged in mentorship? Are faculty mentoring more or differently now
   than in the past? This study of political science faculty from political
   science departments offering PhD programs in the English language finds
   that graduate supervision is changing over time, with mentorship
   practices becoming both more common and more varied. Supervisors do not
   appear to be simply replicating their own limited experience of
   mentorship as a PhD student. Instead, supervisors are becoming more
   actively and directly involved in their students' research careers in
   ways that increase their students' career opportunities. There is
   opportunity for institutions, at both the university and department
   level, to further invest in building the capacity and ability of
   supervisors to be effective mentors.},
