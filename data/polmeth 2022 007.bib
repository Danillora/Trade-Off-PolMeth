@article{ WOS:000835277600008,
Author = {Trak, Juan Manuel},
Title = {Truncated Opening? The Failed Attempt to Democratic
   Reinstitutionalization in Venezuela 2021},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2022},
Volume = {42},
Number = {2},
Pages = {461-488},
Abstract = {In 2021, the Venezuelan government took steps towards an apparent
   process of political opening. The National Assembly elected in 2020
   appointed new electoral authorities with the participation of civil
   society, who called subnational elections with the presence of
   in-ternational observation. Additionally, in Mexico, the government and
   opposition started a negotiation process that aimed to
   re-institutionalize the country. However, even when the opposition
   returned to the electoral arena, the political opening was truncated
   when the government withdrew from the negotiations in Mexico, annulled
   the electoral result in Barinas, and maintained the authoritarian
   practices of previous years. This paper aims to analyze the
   sociopolitical context in Venezuela, considering this liberalization
   attempt.},
