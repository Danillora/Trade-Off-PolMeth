@article{ WOS:000864736400011,
Author = {Kuhn, Ulrich},
Title = {WAR, PEACE, AND (IN)JUSTICE IN THE NUCLEAR AGE},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2022},
Volume = {21},
Number = {1},
Pages = {103-110},
Month = {SPR},
Abstract = {Russia's war against Ukraine has created a watershed moment for Europe.
   The coming weeks and months will decide what kind of peace and security
   order will shape the old continent. The prominence of nuclear weapons
   will likely increase in the years ahead. Mutual deterrence between NATO
   and Russia will be the military component to what may soon turn out to
   be a new Cold War dividing Europe. While this is already bad news news,
   Ukraine's fate has again exposed the system of nuclear deterrence as
   highly unjust and precarious. The consequences of that sorry state of
   affairs might soon create additional injustices on a global scale. A
   realistic view must come to the conclusion that we do not have the time
   to fight another Cold War.},
