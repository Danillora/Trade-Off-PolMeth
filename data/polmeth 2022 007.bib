@article{ WOS:000980036600002,
Author = {Castillo, Alberto David Martinez},
Title = {Populism in Latin America: the Eternal Return},
Journal = {CIENCIA POLITICA},
Year = {2022},
Volume = {17},
Number = {34},
Pages = {19-44},
Month = {JUL-DEC},
Abstract = {Latin America has been a fertile land for populist regimes. This study
   aims to explain the recurrence of populist regimes in the region,
   focusing mainly on the case of Venezuela, paradigm of the XXI Century
   populism. The main conclusion is that the frequency of populist regimes
   in Latin America is greatly due to the combination of weak democracies
   with high income inequality. Moreover, the article suggests that these
   regimes have a cycle that begins when populism emerges from its dormant
   state due to deep democratic deficits, but once in the government
   disregard of the economic limits of politics leads to a sustainability
   crisis and populism returns to its dormant state.},
