@article{ WOS:000883756600001,
Author = {Friedner Parrat, Charlotta},
Title = {Interpretivists in the English School: Aren't we all?},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
Year = {2023},
Volume = {19},
Number = {2},
Pages = {221-241},
Month = {JUN},
Abstract = {This article is a reply to Bevir and Hall, who recently argued in this
   journal that the English School needs to reflect more on its philosophy.
   They are right. Yet, their preferred distinction between a structural
   and an interpretivist strand of the School is not a constructive way
   forward. This is because their distinction between a structural and an
   interpretivist strand of the school is too stark, their chosen
   dimensions for sorting through the School are arguably not the most
   fruitful, and the inclusion of the English School's normative agenda
   must remain independent of whether one is inclined to start from
   structure or from agency. After elaborating these points, the article
   moves on to suggesting a number of other philosophical issues which
   would be more relevant for the English School to work through. It ends
   with an empirical illustration of what an integrated English School
   approach, inspired by structuration, could look like.},
