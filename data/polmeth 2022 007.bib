@article{ WOS:000986557200002,
Author = {Banyer, Emma},
Title = {A `cabinet without a parliament'? Scrutinising the National Cabinet},
Journal = {AUSTRALASIAN PARLIAMENTARY REVIEW},
Year = {2022},
Volume = {37},
Number = {1},
Pages = {8-29},
Month = {FAL-WIN},
Abstract = {The National Cabinet represents a dynamic and innovative approach to
   crisis management across the Federation. However, it cannot be said to
   be a cabinet in the Westminster sense. Lacking cabinet solidarity, and
   answerable to nine separate legislatures, its members have nevertheless
   been bound by the rules of cabinet secrecy, at times frustrating
   parliamentary scrutiny of its decisions. In the wake of a successful
   challenge to its foundations, an attempt was made to embed the National
   Cabinet -and its secrecy provisions- through legislation.},
