@article{ WOS:000927881000004,
Author = {Rodriguez, Angel},
Title = {From the Judiciary to politics and vice versa. Returning to
   jurisdictional functions after hol- ding other public offices},
Journal = {TEORIA Y REALIDAD CONSTITUCIONAL},
Year = {2022},
Volume = {50},
Pages = {185-217},
Abstract = {The Spanish Constitution forbids judges to exercise jurisdictional
   functions and at the same time holding other public offices. However,
   the Organic Law of the Judicial Power stipulates that judges can easily
   return to the judiciary afterwards. This paper reviews pros and cons of
   that regulation, arguing on the principle of separation of powers,
   judicial pluralism, and fundamental rights (both of judges and citizens)
   involved. The conclusion is reached that, in the present context of <<
   politization of Justice >>, it is advisable to strengthen conditions
   under which judges could return to be judges after being for some time
   politicians.},
