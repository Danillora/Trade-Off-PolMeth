@article{ WOS:000831100400008,
Author = {Pizzorno, Pablo},
Title = {The Radical Dispute. Unionist and Intransigents in the Peronist Years
   (1946-1955)},
Journal = {TEMAS Y DEBATES},
Year = {2022},
Number = {43},
Pages = {175-200},
Month = {JAN-JUN},
Abstract = {This paper analyzes the dispute within the Radical Civic Union
   throughout the years it became the main opposition party to Peronism.
   The previous division between Unionists and Intransigents was
   resignified due to the anti-Peronist experience, deepening the dispute
   over the meaning of the Radical tradition that each sector claimed for
   itself. While Unionists remained faithful to the spirit of the
   Democratic Union and accompanied the growing opposition radicalization,
   Intransigents tried to dispute the national and popular discourse of a
   government whose victory they considered a consequence of the loss of
   Radical identity.},
