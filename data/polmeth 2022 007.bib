@article{ WOS:001286239900001,
Author = {Azam, Aizah and Khan, Aarish U.},
Title = {India's election upset: What next!},
Journal = {GLOBAL CHANGE PEACE \& SECURITY},
Year = {2022},
Volume = {34},
Number = {2-3},
Pages = {187-191},
Month = {SEP 2},
Abstract = {The surprising result of India's 2024 election has reaffirmed the
   unpredictable nature of Indian politics. Although Prime Minister
   Narendra Modi secured a third term, his Bharatiya Janata Party (BJP)
   failed to achieve an outright parliamentary majority, falling short of
   its 370-seat target in the 543-seat Lok Sabha. Despite Modi's central
   role in the campaign, this outcome has damaged the Modi brand. The BJP's
   power proving to leverage India's rising global status and Hindu
   nationalist credentials was overshadowed by local livelihood issues,
   which proved decisive for voters. This study delves into Modi's third
   term's political, economic, and foreign policy implications,
   highlighting India's challenges and opportunities under a weakened
   BJP-led coalition government. The study underscores the critical nature
   of Modi's third term, analysing its potential impact on India's
   socio-political fabric and geopolitical strategies. It highlights the
   importance of understanding these dynamics to anticipate the future
   trajectory of India's domestic policies and international relations.},
