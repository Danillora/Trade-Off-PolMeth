@article{ WOS:000875542900001,
Author = {Gardner, Karl},
Title = {Rethinking sanctuary cities in Canada: reflecting on a decade of
   municipal access without fear policies},
Journal = {CITIZENSHIP STUDIES},
Year = {2023},
Volume = {27},
Number = {1},
Pages = {1-18},
Month = {JAN 2},
Abstract = {This article reflects on the commitments and outcomes of municipal
   access without fear policies in Canada over the past decade. The
   potentials and limitations faced by sanctuary cities are evaluated
   through an analysis of municipal policies, peer-reviewed scholarship,
   and grey literature. In particular, I focus on three conceptual framings
   that are commonly associated with sanctuary city
   policies--sanctuary-as-defiance, sanctuary-as-protection, and
   sanctuary-as-inclusion--and ask: to what extent can these be observed in
   the discourses and actions of Canadian sanctuary cities? I find that
   while municipal access without fear policies do constitute valuable
   expressions of solidarity with residents without immigration status,
   municipalities have faced several challenges in living up to their
   expectations. I conclude by suggesting that we might look toward
   grassroots social movements, advocacy organizations, and community
   networks for examples of these fundamental commitments to defiance,
   protection, and inclusion.},
