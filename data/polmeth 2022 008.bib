@article{ WOS:000837182800003,
Author = {Shulga, Maryna and Deliia, Oksana and Krasnozhon, Neonila and Kuchyk,
   Oleksandr and Kravchuk, Olha and Kostyrya, Inna},
Title = {Regional leadership as an element of Ukraine's geopolitical strategy},
Journal = {CUESTIONES POLITICAS},
