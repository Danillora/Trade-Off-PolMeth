@article{ WOS:000885636900004,
Author = {Palomera, Adriana},
Title = {Theoretical and Historical Considerations and Tensions around Asylum and
   Refuge},
Journal = {IZQUIERDAS},
Year = {2022},
Volume = {51},
Month = {SEP},
Abstract = {In recent decades, the increase in forcibly displaced people seeking
   refuge and protection, along with restrictions installed at many borders
   to prevent their entry, has meant that millions of human beings face a
   humanitarian crisis. The multiple realities that refugees live, in
   addition to stressing the conceptualizations and regulations about
   asylum and refuge, force its permanent revision. The objective of this
   article is to examine the theoretical and historical considerations of
   asylum and refuge, which show the edges and scope of the discussion on
   the subject.},
