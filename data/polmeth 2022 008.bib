@article{ WOS:000912179700001,
Author = {Loeffler, Hannah},
Title = {Does a universal basic income affect voter turnout? Evidence from Alaska},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2023},
Volume = {11},
Number = {3},
Pages = {521-536},
Month = {JUL},
Abstract = {Does a universal basic income (UBI) affect voter turnout? This article
   argues that the introduction of an unconditional cash payment-where
   citizens receive money independent of employment status, age, or
   indigence-can have a turnout-enhancing effect. I evaluate the argument
   using the introduction of the Permanent Fund Dividend in Alaska.
   Differences-in-differences estimates covering November general elections
   from 1978 to 2000 provide compelling evidence that the Alaskan UBI has a
   significant positive effect on turnout. The results further suggest that
   the turnout increase was not a one-off effect but persists over a period
   of almost 20 years. Thus, a UBI has the potential to positively affect
   turnout among an entire electorate, adding to the discussion around
   potential welfare reforms in western democracies.},
