@article{ WOS:000994594500008,
Author = {Neves, Maria Lucia Correa and Dandolini, Gertrudes Aparecida and de
   Souza, Joao Artur},
Title = {INNOVATION MODEL THROUGH OWN R\&D FOR ORGANIZATION OF PUBLIC SERVICES},
