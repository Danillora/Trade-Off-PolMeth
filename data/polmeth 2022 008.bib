@article{ WOS:000794076300001,
Author = {Karinkurayil, Mohamed Shafeeq},
Title = {`Dubai' as a Place of Memory in Malayalam Cinema},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
