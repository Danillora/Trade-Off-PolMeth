@article{ WOS:000831125700001,
Author = {Ubaydullaeva, Dilnoza},
Title = {Ideological discourses in post-Soviet Uzbekistan: internationalization
   of higher education as a threat},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
