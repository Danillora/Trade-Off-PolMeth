@article{ WOS:000821312300001,
Author = {Kugelberg, Henrik D.},
Title = {Is Political Liberalism Antidemocratic?},
Journal = {JOURNAL OF POLITICS},
Year = {2022},
Volume = {84},
Number = {4},
Pages = {2283-2287},
Month = {OCT 1},
Abstract = {Political liberals have regularly been accused of being antidemocratic.
   The standard version of this objection maintains that in prioritizing
   liberal rights, political liberals leave insufficient room for
   democratic self-legislation. Leading political liberals like Jonathan
   Quong have denied the force of this objection, by arguing that certain
   liberal rights are beyond reasonable dispute and that there is nothing
   objectionably undemocratic about having constitutional guarantees in
   place to protect them. In this article, I argue that the democratic
   problem with Quong's political liberalism is not the emphasis on
   constitutional safeguards for liberal rights. Rather, Quong's theory
   cannot ensure that even a basic desideratum for democratic
   decision-making is met. We are thus faced with the choice of granting
   citizens the right to make democratic decisions and Quong's
   antidemocratic political liberalism.},
