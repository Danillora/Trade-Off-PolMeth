@article{ WOS:000884078700006,
Author = {Agbaba, Romina},
Title = {SUSTAINABLE-RESPONSIBLE TOURISM VALUES AND CRISIS MANAGEMENT: ISSUES AND
   CRITICAL REFLECTION},
Journal = {EUROPEAN JOURNAL OF TRANSFORMATION STUDIES},
