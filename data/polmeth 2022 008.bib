@article{ WOS:000750176400003,
Author = {Masecchia, Anna},
Title = {DAD. Disembodied Alienated Didactics},
Journal = {LABORATOIRE ITALIEN-POLITIQUE ET SOCIETE},
Year = {2022},
Number = {27},
Abstract = {The paper analyses transformations in higher education, and specifically
   in Italian universities, during and after the Covid-19 crisis. The main
   focus is on visual and digital media and on their role in reshaping the
   relationship between teachers and students: on the one hand, teachers
   have to act as technological performers, following a method in which
   teaching becomes a show and an event; on the other, students watch on
   the other side of the screen in a voyeuristic, consumerist way. As a
   result, education as a lived experience gives way to a mechanical
   transfer of standardised content, designed as products and commodities
   in a video platform market.},
