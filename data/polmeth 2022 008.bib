@article{ WOS:000810390200001,
Author = {Cooley, Jason},
Title = {Why Did the American Transfer during the Korean War Succeed?},
Journal = {DEMOCRACY \& SECURITY},
Year = {2022},
Month = {2022 JUN 13},
Abstract = {For the first year of the Korean War, American troops led the effort to
   halt communist aggression. As the fighting continued, though, the
   members of the South Korean Army assumed control of the majority of the
   sectors on the defensive line by the thirty-eighth parallel. During a
   review of strategic studies publications, it becomes clear that most
   scholars believe only three factors enabled this security transfer to
   succeed. Through a more rigorous analysis of the case, this article will
   demonstrate that other determinants actually played a role in the
   auspicious outcome. If all of the contributing factors are identified,
   current and future policymakers will have an opportunity to see what
   conditions tend to produce useful transfers in conventional conflicts.},
