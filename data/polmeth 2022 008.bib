@article{ WOS:000773895100001,
Author = {Stiers, Dieter and Hooghe, Marc},
Title = {Ethnic Diversity, Anti-Immigrant Sentiments, and Radical Right Voting in
   the Flemish Region of Belgium},
Journal = {SWISS POLITICAL SCIENCE REVIEW},
Year = {2022},
Volume = {28},
Number = {3},
Pages = {496-515},
Month = {SEP},
Abstract = {A standard assumption in realistic threat theories is that the presence
   of ethnic minorities is associated with a rise of anti-immigrant
   sentiments. However, we do not know whether this presence has a specific
   local effect, or whether one can detect a more general nationwide
   perception of threat. Using data from a recent Belgian population
   survey, we assess the association between ethnic diversity within the
   local community and anti-immigrant sentiments. Results suggest a strong
   negative association between the level of ethnic diversity and
   anti-immigrant sentiments. Furthermore, while we do not find evidence
   for an association between ethnic diversity and radical right voting on
   the individual level, there is a strong negative correlation on the
   aggregate level. We conclude with some speculation about how
   anti-immigrant sentiments are created in areas with a very low levels of
   ethnic diversity, and what this implies for the electoral potential of
   radical-right parties.},
