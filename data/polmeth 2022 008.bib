@article{ WOS:000825088900001,
Author = {Molder, Martin and Enyedi, Zsolt and Bertoa, Fernando Casal},
Title = {Party system closure and the liberal dimension of democracy: a
   double-edged sword},
Journal = {ITALIAN POLITICAL SCIENCE REVIEW-RIVISTA ITALIANA DI SCIENZA POLITICA},
Year = {2023},
Volume = {53},
Number = {1},
Pages = {104-120},
Month = {MAR},
Abstract = {An institutionalized party system is often regarded as a precondition
   for a well-functioning democracy. Recent recesses in democracy and, in
   particular, in the liberal dimension of democracy in relatively
   established party systems, however, warrant a fresh look into how party
   system institutionalization shapes liberal democracy. We use a dataset
   that covers 58 European party systems over more than a century to assess
   how party system institutionalization in the governmental arena -
   closure - is related to more robust liberal democracy. Our results show
   that stable coalition combinations are conducive to higher levels of
   liberalism, while infrequent government changes and the exclusion of new
   parties from the governmental arena have a detrimental role.},
