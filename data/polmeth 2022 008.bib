@article{ WOS:000826884600009,
Author = {Vinale, Adriano},
Title = {Post-truth and Conspiracy Fantasies. Conceptual Coordinates and
   Historical-Political Traces},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
Year = {2022},
Volume = {34},
Number = {66},
Pages = {125-142},
Abstract = {This article aims to offer a historical reconstruction and theoretical
   analysis of two concepts that have pervaded contemporary political
   debate: post-truth and conspiracy theories. In doing so, the main goal
   is to refute the alleged filiation between post-structuralism and
   post-truism. This attempt is grounded on the hypothesis that the shift
   from a society of secrecy to a society of transparency has actually
   produced an invisibilisation of the real through its documedial
   overexposure.},
