@article{ WOS:000790123700001,
Author = {Ryan, Kevin and Whelan, Fiona},
Title = {Boys in the Making: an arts and education programme in
   conversation with `the biology of adversity and resilience'},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2022},
Volume = {15},
Number = {2},
Pages = {337-357},
Month = {MAY 4},
Abstract = {Anchored in a `core story' of childhood development, `the biology of
   adversity and resilience' (TBOAR) is an emerging scientific paradigm
   that seeks to explain how and why adverse childhood experiences have
   consequences in the form of negative `outcomes' that span health and
   behavioural problems. In this article, Two Fuse bring TBOAR into
