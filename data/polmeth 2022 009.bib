@article{ WOS:000924612600005,
Author = {Anderlini, Michel Vincent},
Title = {A reaction to the French `` non{''}? Or a case of institutional
   bricolage? A discursive institutionalist approach to the revised EU
   enlargement methodology},
Journal = {POLITIQUE EUROPEENNE},
Year = {2022},
Volume = {76},
Number = {3},
Pages = {142-169},
Abstract = {How has the European Commission designed its new enlargement methodology
   in times of enlargement fatigue? Several studies have pointed out the
   French opposition to launching accession negotiation with North
   Macedonia and Albania in 2019 as a significant factor explaining the
   content of the new methodology. Traditionally, enlargement policy has
   been seen through the lenses of rational choice institutionalism: from
   this perspective, the new methodology would be resulting from such an
   external shock. This paper argues instead, using the discursive
   institutionalism framework and the concept of institutional bricolage,
   that the new methodology is the product of four different discourses on
   enlargement layered on top of each other.},
