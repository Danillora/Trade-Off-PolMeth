@article{ WOS:000825374900001,
Author = {Rivera, Jorge Andres Lopez},
Title = {Give me a reason. On power in Rational Choice Theory},
Journal = {POLITICA Y SOCIEDAD},
Year = {2022},
Volume = {59},
Number = {2},
Abstract = {This paper claims that a conception that understands practical reasoning
   in terms of a maximizing instrumental rationality, as done by the
   Rational Choice Theory (RCT), is insufficient for the analysis of social
   power. Under the premise that action and power are tightly related
   concepts and taking rationality as a key category for understanding
   power exercises, the concept of power offered by RCT is revised, in
   order to expose its limitations. The paper concludes proposing that
   identifying RCT conceptual limitations opens a pathway of research that
   links the philosophy of action with literature about power.},
