@article{ WOS:000816846100021,
Author = {Goetz, Roland},
Title = {Sanctions and Reactions The Impact on Russia's Economy},
Journal = {OSTEUROPA},
Year = {2022},
Volume = {72},
Number = {1-3},
Pages = {197+},
Abstract = {In reaction to Russia's attack on Ukraine, the European Union, the
   United States and other countries imposed numerous economic sanctions on
   Moscow between late February and mid-March 2022. The most serious ones
   are those targeting Russia's commercial banks and central bank. The
   trade sanctions, on the other hand, have an effect only on special
