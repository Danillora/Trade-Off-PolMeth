@article{ WOS:000854031800001,
Author = {Ahrens, Petra and Elomaki, Anna},
Title = {Researching without asking actors directly: the value of interpretative
   methods in studying gender mainstreaming implementation in the European
   Parliament},
Journal = {POLITICAL RESEARCH EXCHANGE},
Year = {2022},
Volume = {4},
Number = {1},
Month = {DEC 31},
Abstract = {This article discusses interpretative methods as well-suited approaches
   to researching norm- and value-driven topics, such as gender
   mainstreaming (GM). Using the European Parliament as a case study, the
   article introduces the idea of `flipped expert interviews', extending
   the interview population to non-experts, as an approach to study GM. It
   interrogates the advantages and limitations of this method in capturing
   power relations and the formal and informal practices and processes in
   the EP's committees and political groups. The article suggests that
   going beyond `usual suspects' when selecting the interview population
   helps to paint a broader picture of contextual factors and
