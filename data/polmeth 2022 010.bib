@article{ WOS:000780917200001,
Author = {Fox, Amos C.},
Title = {Manoeuvre is Dead? Understanding the Conditions and Components of
   Warfighting},
Journal = {RUSI JOURNAL},
Year = {2022},
Volume = {166},
Number = {6-7},
Pages = {10-18},
Month = {MAY 26},
Abstract = {For the better part of the 20th century, manoeuvre held primacy in
   Western military thinking. Yet, modern technological advances, in
   today's period of limited war, have suffocated the conditions and
   components that manoeuvre requires to exist. As a result, it is dead.
   Amos C Fox argues that, instead of lamenting this, the defence and
   security studies communities should celebrate manoeuvre's death as a
   liberating event and begin looking at alternative theories and ideas for
   the prosecution of war.},
