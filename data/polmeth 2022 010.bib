@article{ WOS:000920386200005,
Author = {Peyrouse, Sebastien},
Title = {China's Impact on Democracy and Human Rights in Central Asia},
Journal = {SECURITY AND HUMAN RIGHTS},
Year = {2022},
Volume = {32},
Number = {1-4, SI},
Pages = {53-68},
Month = {SEP},
Abstract = {This article analyzes the impact of China's policy on politics and human
   rights in Central Asia. First, it discusses how Beijing's narrative has
   supported authoritarianism in the region. Second, it analyzes some of
   the tools of authoritarianism China has exported to support the
   political legitimacy of Central Asian authorities and their efforts to
   monitor their citizens. In conclusion, this article argues that although
   China has had a tangible impact on human rights in Central Asia, other
   elements also need to be taken into consideration to understand
   authoritarian tendencies in the region, including the influence of other
   foreign actors such as Russia as well as the goals of the Central Asian
   leaders themselves, who are not passive recipients of Chinese policy but
   rather have embarked on their own on road of authoritarianism since
   independence.},
