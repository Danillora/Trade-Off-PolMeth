Title = {Party-led innovation of state-sponsored intermediary organizations in
   Beijing, Shanghai, and Guangdong of China: Effects and managerial
   strategies},
Journal = {ASIAN POLITICS \& POLICY},
Year = {2022},
Volume = {14},
