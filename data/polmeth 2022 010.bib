@article{ WOS:000925804800007,
Author = {Ronis, Matias Paschkes},
Title = {BE A RESIDENT. THE EXPERIENCE OF OLD AGE IN A PUBLIC GERIATRIC RESIDENCE
   IN BUENOS AIRES},
Journal = {REVISTA ESTUDOS POLITICOS},
Year = {2022},
Volume = {13},
Number = {26},
Pages = {118-140},
Abstract = {This article is based on an investigation that aimed to investigate and
   explore the experience of living in a public geriatric residence managed
   by the Government of the City of Buenos Aires. If generally these
   institutions were studied as areas of solitude and passivity in which
   elderly people who can no longer fend for themselves and are in their
   last stage of life are confined, this work poses the challenge of
   approaching them as a world of life. The research approach was carried
