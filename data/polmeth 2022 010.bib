@article{ WOS:000771913600001,
Author = {Achury, Susan and Casellas, Jason P. and Hofer, Scott J. and Ward,
   Matthew},
Title = {The Impact of Racial Representation on Judicial Legitimacy: White
   Reactions to Latinos on the Bench},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2023},
Volume = {76},
Number = {1},
Pages = {158-172},
Month = {MAR},
Abstract = {Despite evidence that racial diversification has increased support for
   the judiciary, political scientists know little about the heterogeneous
   effects of diversification across different population segments.
   Previous research illustrates that including Black judges increases
   judicial legitimacy among the Black population, but it decreases the
   legitimacy of the courts among the White population. We expand on this
   knowledge by examining the impact of adding Latinos to the bench. Our
   survey experiment compares White respondents' perception of the courts
   based on differing levels of Latino representation in the ruling panel.
   Does descriptive representation in the racialized issue area of
   immigration signal fairness and legitimacy to White respondents? Or does
