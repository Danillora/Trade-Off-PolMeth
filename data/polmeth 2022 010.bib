@article{ WOS:000923769500010,
Author = {Daniel, Remi and Lindenstrauss, Gallia and Guzansky, Yoel},
Title = {COMPLEMENTARY FACETS: TURKIYE-ISRAEL RELATIONS AND THE ABRAHAM ACCORDS},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2022},
Volume = {21},
Number = {3},
Pages = {95-104},
Month = {FAL},
Abstract = {The Abraham Accords between Israel and four Arab states in 2020 were
   seen negatively and heavily criticized by Turkiye. At the time they were
   signed, some interpreted them as a balancing act not only against the
   rising Iranian threat, but also against Turkiye's growing activism in
   the region. However, less than two years later, after Turkiye began
   normalizing its relations with the United Arab Emirates and with Israel,
   there appear to be several complementary aspects between the Abraham
   Accords and the Turkiye-UAE and Turkiye-Israel normalization processes.
   Still, there is room to push even further for trilateral cooperation
   between Turkiye-Israel-UAE, as these are among the economic powerhouses
   in the region.},
