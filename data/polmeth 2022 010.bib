@article{ WOS:000898851300002,
Author = {Sofia Jemio, Ana},
Title = {The workers of the Monte Grande farm (Tucuman, Argentina). From
   productive reconversion to genocide (1966-1976)},
Journal = {IZQUIERDAS},
Year = {2022},
Volume = {51},
Month = {SEP},
Abstract = {In February 1975, in a small province in northern Argentina called
   Tucuman, a military operation was launched that marked the beginning of
   the genocide in the country. One of the many points where this happened
   was the Monte Grande lemon farm, a farm from which 25 workers were
   kidnapped and in which a Clandestine Detention Center operated. Taking
   these facts as a starting point, the article proposes to reconstruct the
   history of that territory, characterizing the social fabric of which
   these victims were part, the historical process by which that fabric
   came to be, and the policies used to attack it.},
