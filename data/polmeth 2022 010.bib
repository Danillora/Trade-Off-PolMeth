@article{ WOS:000859797900001,
Author = {Clementson, David E. and Xie, Tong},
Title = {Are Presidential Candidates Impervious to Deception Detection? A Test of
   Voters' Truth-Default},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2022},
Volume = {52},
Number = {4},
Pages = {905-924},
Month = {DEC},
Abstract = {This study applies truth-default theory (TDT) to presidential
   candidates. TDT holds that people tend to passively believe others
   without consciously considering whether they are being told the truth.
   But do voters have a truth-default toward presidential candidates? In an
   experiment, voters across the United States (N = 294) watched a news
   interview in which a presidential candidate was either honest or
   deceptive. Party affiliation was also manipulated. Consistent with TDT,
   thought-listing tasks revealed that most voters did not mention
   deception after exposure to the presidential campaign interview. Voters
   largely defaulted to the truth even when sustaining outgroup partisan
   exposure and deception, and when asked about the candidate's demeanor.
   Filling out closed-ended scales, though, voters reported distrust,
   suspicion, and perceiving deceptive messaging. The discussion concerns
   the implications of voters' perceptions of a presidential candidate's
   veracity varying based on how voters are prompted.},
