@article{ WOS:000779663700001,
Author = {Stoeckel, Florian and Ceka, Besir},
Title = {Political tolerance in Europe: The role of conspiratorial thinking and
   cosmopolitanism},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
Year = {2023},
