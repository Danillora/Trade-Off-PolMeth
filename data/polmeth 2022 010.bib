@article{ WOS:000905421200036,
Author = {Tykhonenko, Iryna and Trygub, Oleksandr and Bohdanova, Tetiana and
   Shevchuk, Oleksandr},
Title = {Ukraine's security and Pakistan's security: mechanisms to overcome
   threats},
Journal = {CUESTIONES POLITICAS},
