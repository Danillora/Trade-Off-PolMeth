@article{ WOS:000772013600001,
Author = {Naess, Ole-Andreas Elvik},
Title = {Inefficient voting with identical voters},
Journal = {JOURNAL OF THEORETICAL POLITICS},
Year = {2022},
Volume = {34},
Number = {2},
Pages = {175-190},
Month = {APR},
Abstract = {This paper constructs a two-period electoral model where the future is
   uncertain and the policy in each period is decided by majority rule. The
   voters' optimal future policy takes into account all possible future
   realizations, while the future pivotal median voter only cares about the
   median value of the future distribution. This dynamic conflict of
   interest implies that the electoral outcome may be strictly
   Pareto-dominated by other policies even in cases where all voters are
   identical and have the same beliefs for the future. I apply the model to
   analyze the dynamic incentives for redistribution.},
