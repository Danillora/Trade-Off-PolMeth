@article{ WOS:000834647300001,
Author = {Hong, Ji Yeon and Park, Sunkyoung and Yang, Hyunjoo},
Title = {In Strongman We Trust: The Political Legacy of the New Village Movement
   in South Korea},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
Year = {2023},
Volume = {67},
Number = {4},
Pages = {850-866},
Month = {OCT},
Abstract = {This study explores how authoritarian distributive policies may not only
   generate political support for the autocrat but may also help sustain
   powerful and lasting authoritarian legacies. We use microlevel data from
   South Korea's New Village Movement, a 1970s rural development program
   implemented under dictator Park Chung-hee and widely touted as
   contributing to the country's rapid economic development. Our analysis
   shows that townships in receipt of larger cash transfers cast more votes
   for Park's incumbent party in the subsequent election. More importantly,
   we show that the effects of the subsidies still appeared almost four
   decades later in 2012, when the dictator's daughter was democratically
   elected as the president of South Korea. We show that these effects were
   not driven by villagers' long-term income gains or enhanced social
   capital due to the program but by the unwavering support of the
   beneficiary villagers for the dictator, whose legacy remained strong
   long after democratization.},
