@article{ WOS:000857904700001,
Author = {Barry, Nicholas and Sanchez-Urribarri, Raul},
Title = {Populist politics, COVID-19, and fake news: The case of Craig Kelly},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2022},
Volume = {57},
Number = {3, SI},
Pages = {280-296},
Month = {JUL 3},
Abstract = {One of the marked features of the COVID-19 pandemic has been the
   proliferation of fake news about the virus. Although commentary on this
   issue has generally focused on the dissemination of online material by
   private citizens and organised groups, politicians have often played a
   major role as well, exacerbating divisions and impairing the effective
   implementation of measures against the pandemic. This article examines
   this issue, focusing on Australian politician Craig Kelly, with
   references to the cases of Donald Trump (US) and Jair Bolsonaro
   (Brazil). Despite operating in very different contexts, we find a high
   degree of similarity in the messages they conveyed, which echoed
   misleading claims being circulated online, and constituted a form of
   medical populism. These findings show that the problem of fake news is
   not simply about the activities of shadowy groups online; it is also
   enabled by the public comments of populist politicians.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
