@article{ WOS:000799439600001,
Author = {Umit, Resul and Schaffer, Lena Maria},
Title = {Wind Turbines, Public Acceptance, and Electoral Outcomes},
Journal = {SWISS POLITICAL SCIENCE REVIEW},
Year = {2022},
Volume = {28},
Number = {4},
Pages = {712-727},
Month = {DEC},
Abstract = {Despite a widespread public support for wind energy in general, wind
   turbine proposals attract a considerable amount of public opposition. At
   a time of political commitments to building more wind turbines for
   climate risk mitigation, we study the potential causes of this
   opposition and its electoral effects. Our analysis draws on a survey
   experiment in Switzerland, where the number of wind turbines will grow
   from a couple of dozens to many hundreds in the next three decades. We
   find that exposure to wind turbines increases public acceptance, but
   this affect does not translate into electoral turnout or vote choice.
   Moreover, locality or politicisation does not seem to have an effect at
   all-neither on acceptance nor on electoral outcomes. Our results suggest
   that voters do not reward or punish political parties for their
   positions on wind energy, even when turbines might soon be rising in
   their local area.},
