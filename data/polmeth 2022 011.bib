@article{ WOS:000864013500007,
Author = {Khader, Jamil},
Title = {Welcome to the Metaverse: Social Media, the Phantasmatic Big Other, and
   the Anxiety of the Prosthetic Gods},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2022},
Volume = {34},
Number = {3},
Pages = {397-405},
Month = {JUL 3},
Abstract = {In Algorithmic Desire: Toward a New Structuralist Theory of Social
   Media, Matthew Flisfeder develops an alternative structuralist theory of
   social media. As a Zizekian analysis of social media, this book is an
   important contribution to the field of Zizekian studies. While Flisfeder
   theorizes social media in relation to Slavoj Zizek's idea of the big
   Other, he nonetheless proposes an unorthodox understanding, importantly
   choosing not to engage Zizek's analysis of technology, which develops in
   the context of his discussion of artificial intelligence and virtual
   reality in relation to Freud's ``prosthetic God{''} and Lacan's
   lathouses. The present essay suggests that Zizek's analysis of
   technology has radical implications for understanding social media,
   providing an important perspective that shifts the emphasis of
   theorizing social media from desire to anxiety in a zone ``beyond the
   pleasure principle,{''} an analysis that Zizek situates in the context
   of the global COVID-19 pandemic and its impact on the big Other.},
