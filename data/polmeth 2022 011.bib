@article{ WOS:000798343200001,
Author = {Erisen, Cengiz},
Title = {Psychological foundations and behavioral consequences of COVID-19
   conspiracy theory beliefs: The Turkish case},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2023},
Volume = {44},
Number = {4},
Pages = {587-606},
Month = {SEP},
Abstract = {This article offers a comprehensive approach to understanding
   pandemic-related behavior by tackling both the psychological predictors
   of belief in conspiracy theories about COVID-19 and their potential
   consequences for trust in experts and institutions, compliance behavior
   and vaccine acceptance. Drawing from an observational survey (n = 1028)
   in Turkey, this article examines: (a) the psychological precedents of
   believing COVID-19 conspiracy theories; (b) how trust in experts,
   scientists and health authorities is influential in controlling these
   beliefs; (c) to what extent conspiracy theory beliefs affect compliance
   with protective measures; and (d) who is more likely to accept
   vaccination. Results demonstrate that one of the potential solutions to
   the influence of conspiracy theory beliefs across an array of attitudes
   and behaviors is to establish trust in experts, scientists and health
   authorities.},
