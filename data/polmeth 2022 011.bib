@article{ WOS:000760930100001,
Author = {Homola, Jonathan and Rogowski, Jon C. and Sinclair, Betsy and Torres,
   Michelle and Tucker, Patrick D. and Webster, Steven W.},
Title = {Through the ideology of the beholder: how ideology shapes perceptions of
   partisan groups},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2023},
Volume = {11},
Number = {2},
Pages = {275-292},
Month = {APR},
Abstract = {Growing attitudinal and affective differences across party lines and
   increasing social polarization are often attributed to the strengthening
   of partisanship as a social identity. Scholars have paid less attention
   to personal preferences as a contributor to these phenomena. Our focus
   is on how citizens' policy beliefs-their operational ideologies-are
   associated with their views of partisan groups. We examine our
   perspective with two studies. In the first, we find that the attribution
   of ideologically extreme political views to an individual's peer
   significantly reduces interest in interpersonal interaction but find
   limited evidence that partisan group membership alone induces social
   polarization. In the second, we show that citizens' policy views are
   strongly associated with their perceptions of their own partisan group
   as well as their counterpartisans. Together, our results have important
   implications for understanding the consequences of increased
   polarization and partisan antipathy in contemporary politics.},
