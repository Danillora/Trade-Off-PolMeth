@article{ WOS:000769898500001,
Author = {Cisneros, Angel Saavedra and Carey, Tony E. and Rogers, Darrin L. and
   Johnson, Joshua M.},
Title = {One size does not fit all: core political values and principles across
   race, ethnicity, and gender},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2023},
Volume = {11},
Number = {4},
Pages = {793-812},
Month = {AUG 8},
Abstract = {An understanding of the structure and implications of political values
   across groups is necessary in order to explore their political role
   across an increasingly diverse American public. In this study, we
   examine the consistency and political consequences of several core
   values at the intersection of race, ethnicity, and gender. To explore
   this topic, we estimate measurement invariance for beliefs and values of
   authoritarianism, egalitarianism, limited government, and moral
   traditionalism, across racial and gender groups in the United States.
   While we uncover substantial variations in internal consistency across
   measures, we also discover a striking degree of measurement invariance.
   We find, with a few important exceptions, that core values and
   principles constrain political attitudes in similar ways across groups,
   while having distinct effects on political attitudes. This suggests
   researchers must be aware of group heterogeneity when applying core
   political values to examine political attitudes and behaviors in a
   diverse United States of America.},
