@article{ WOS:000793810200001,
Author = {Ikuta, Jennie C.},
Title = {On the Uses of Acknowledgment for Injustice: Disavowal and Deflection in
   Baldwin's Thought},
Journal = {POLITY},
Year = {2022},
Volume = {54},
Number = {3},
Pages = {435-456},
Month = {JUL 1},
Abstract = {This article challenges a pervasive optimism-shared by both the growing
   anti-racism industry and scholars of James Baldwin's thought-regarding
   the motivational power of acknowledgment. Through a reading of Baldwin's
   essays, I argue that acknowledgment is unlikely to motivate white
   Americans to dismantle structures of racial dominance. As Baldwin
   observes, white Americans who acknowledge their implication in racial
   dominance engage in practices of disavowal and deflection in order to
   protect their attachment to its benefits even as they proclaim a
   commitment to racial equality. Despite their claims to the contrary,
   then, Baldwin suggests that many white Americans are attached to racial
   dominance in a way that goes beyond their willingness to acknowledge
   their implication in it. This indicates that any proposal to motivate
   white Americans to act for racial justice that does not sever their
   attachment to racial dominance is unlikely to succeed.},
