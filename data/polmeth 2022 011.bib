@article{ WOS:000884173500001,
Author = {Dobbins, Michael and Labanino, Rafael Pablo and Riedel, Rafal and
   Czarnecki, Szczepan and Horvath, Brigitte and Szyszkowska, Emilia},
Title = {Organized interests in post-communist policy-making: a new dataset for
   comparative research},
Journal = {INTEREST GROUPS \& ADVOCACY},
Year = {2023},
Volume = {12},
Number = {1},
Pages = {73-101},
Month = {MAR},
Abstract = {This article familiarizes readers with the international research
   project `The Missing Link: Exploring Organized Interests in
   Post-Communist Policy-Making' (OrgInt-CEE). The project team has focused
   on how populations of organized interests in the region have evolved,
   how they interact with state institutions as well as the group-specific
   characteristics driving access to policy-makers. The project also
   explores how Europeanization has affected post-communist interest groups
   as well as other factors contributing to their ``coming-of-age.{''} We
   provide a comprehensive overview of the population ecology and survey
   datasets, while shedding light on the challenges during the data
   collection process. After a short overview of the project context and
   structure, we present some country-specific aggregated data on
   organizational densities and their political activity. We also reflect
   on potential uses for the data, before wrapping up the article with a
   self-critical assessment of what could have been done differently as a
   roadmap for future research.},
