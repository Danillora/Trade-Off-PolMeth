Title = {Learning about political systems while playing: Testing short-term
   knowledge retention through a role-play classroom game de conocimientos
   a corto plazo mediante un juego de rol en el aula},
Journal = {REVISTA ESPANOLA DE CIENCIA POLITICA-RECP},
Year = {2022},
