@article{ WOS:000908351500001,
Author = {Chrvalova, Lujza},
Title = {DEVELOPMENT OF THE DISCUSSIONS ON LETHAL AUTONOMOUS WEAPON SYSTEMS UNDER
   CONVENTIONON CERTAIN CONVENTIONAL WEAPONS},
