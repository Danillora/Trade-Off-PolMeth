@article{ WOS:000930882500005,
Author = {Grajales Zarate, Stephania and Felipe Caicedo, Daniel},
Title = {Historization of the Colombian Student Movement: Six Generations of
   Struggle from 1900 to 2014},
Journal = {CIENCIA POLITICA},
Year = {2022},
Volume = {17},
Number = {33},
Pages = {105-137},
Month = {JAN-JUN},
Abstract = {This research seeks to identify ideals of struggle characteristic of
   each era of the student movement in Colombia from 1903 to 2014,
   concluding that the student movement can be temporarily divided into six
   generations of struggle. Each one with an intergenerational pause where
   the movement is strongly weakened, but from which it always tends to be
   reborn with force, generally thanks to identifiable leaders who see in
   it a form of political platform or political parties and groups that see
   it as an agitator. perfect social platform to catapult your position in
   electoral contests. Each generation has been assigned a name in
   reference to the particular characteristics of those who compose it or
   the ideologies that govern them and that differ in some cases from the
   official historicization.},
