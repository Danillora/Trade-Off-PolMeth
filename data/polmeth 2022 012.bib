@article{ WOS:000878591800004,
Author = {Ibarra, Griselda Alicia Macias and Morales, Adan Baca},
Title = {Electronic Government or Open Government in Mexico: Between the
   Instrument and A New Model of Public Management},
Journal = {REVISTA MEXICANA DE ANALISIS POLITICO Y ADMINISTRACION PUBLICA},
Year = {2022},
Volume = {11},
Number = {21},
Pages = {53-80},
Month = {JAN-JUN},
Abstract = {The concept of Open Government has emerged as a new approach that brings
   together effort to improve government capabilities and modernize
   administrations public, under the principles of transparency and
   accountability, in addition to the openness, participation and
   collaboration. It consists of a new paradigm on the governance that
   establishes the idea of a new way of governing based on a relationship
   strong and constant between civil society, citizens, and government to
   create solutions to the public problems. The objective of this work is
   to specify a delimitation concept of this new paradigm and its
   differentiation with the Electronic Government. In addition to
   addressing, as a first approach, from the statistical analysis of some
   main surveys, indices and indicators, the status of the implementation
   of the Open Government in Mexico.},
