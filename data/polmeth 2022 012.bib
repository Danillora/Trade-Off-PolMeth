@article{ WOS:000928342700005,
Author = {Casalini, Brunella},
Title = {Oppression, Resistance and Emancipation in Maria Lugones},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
Year = {2022},
Volume = {34},
Number = {67},
Pages = {75-90},
Abstract = {Lugones's theory of oppression focuses on the need for the oppressed to
   cultivate a different logic of reality. The conditions that allow the
   emergence of a resistant consciousness rest on a pluralist ontology and
   on the idea of a multiple self-capable of traveling between << worlds
   >>. A key role in this theory is occupied by the space of limen. The
   limen constitutes a portal to liberation, but it does not guarantee its
   outcome. In fact, the political project of emanci-pation from the
   multiple oppressions requires the creation of coalitions. A goal that
   can only be achieved through complex communication, which abandons the
   liberal logic of transparency.},
