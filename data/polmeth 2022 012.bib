@article{ WOS:000860069700001,
Author = {Vale, Sergio R. and Marques, Moises S.},
Title = {Impact of environmental quality indicators on soft power: a few
   empirical estimates},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2022},
Volume = {15},
Number = {3, SI},
Pages = {514-531},
Month = {SEP 2},
Abstract = {Environmental issues have become unavoidable over the last few decades.
   Countries are increasingly undergoing decision-making processes that
   involve impacts on the environment. Among the many possible costs of
   misuse of the environment, one that has drawn attention is with regard
   to soft power. In other words, can environmental degradation actually
   lead to a worsening of countries' soft power? The empirical answer
   suggests that it does. In addition, we attempt to identify which
   specific factors within the environmental issue can have the greatest
   impact on countries' soft power. Furthermore, measures that affect
   people's health seem to be especially important points of impact on
   countries' soft power.},
