@article{ WOS:000868482800006,
Author = {Waisova, Sarka and Cabada, Ladislav},
Title = {CHINA?S ENGAGEMENT IN CENTRAL EUROPE: WORDS SPEAK LAUDER THAN ACTIONS},
Journal = {POLITICKE VEDY},
Year = {2022},
Volume = {25},
Number = {2},
Pages = {151-176},
Abstract = {Until the end of the 1980s the role of the United States (US), European
   Communities (EC) and China in Central Europe (CE) was marginal. However,
   with the end of the Cold War the situation dramatically changed. The
   present article collects and analyses the evidence about the rising
   Chinese engagement in CE, particularly in the Visegrad Group (V4)
   countries Czechia, Hungary, Poland and Slovakia and about its political,
   societal and economic effects in the region. The research is designed as
   a three-level analysis of Chinese activities and interests in CE:
   firstly, attention will be paid to individual CE countries, secondly, to
   V4 which is historically the longest joint political project of CE
   countries, and thirdly, to the impact of the situation in CE on the EU
   level. We found out that pro-Chinese forces recruit from the populist
   and anti-liberal groups, Chinese activities and influence in individual
   CE countries differ, even though Chinese strategies to penetrate
   politics and business are similar, and that China ` s ``words speak
   lauder than actions{''}. We conclude that even when V4 countries alone
   are sobering up from the `Chinese dream', they use the `China card'
   within the EU and towards EU authorities to get what they want.},
Publisher = {BELIANUM-MATEJ BEL UNIV PRESS},
Address = {FAC POLITICAL SCIENCE \& INT RELATIONS, KUZMANYHO 1, BANSKA BYSTRICA,
   974 01, SLOVAKIA},
