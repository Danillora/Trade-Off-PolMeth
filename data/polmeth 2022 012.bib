@article{ WOS:000800340000001,
Author = {Jackson, Patrick Thaddeus and Heo, Sujin},
Title = {Working on relationalism},
Journal = {NEW PERSPECTIVES},
Year = {2022},
Volume = {30},
Number = {2},
Pages = {157-169},
Month = {JUN},
Abstract = {In this essay, we elucidate the insufficiency of a simple binary choice
   between substantialism and relationalism or between dualist and
   nondual/incarnational sensibilities. We highlight the ways that all
   translations of these sensibilities into the scholarly form of life
   encounter characteristic problems and challenges precisely because the
   very practice of scholarly writing is always and already inclined
   towards a more dualist way of going on.},
