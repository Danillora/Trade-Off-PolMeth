@article{ WOS:000828569800001,
Author = {Bitschnau, Marco and Mussotter, Marlene},
Title = {(National) pride and (conceptual) prejudice: critical remarks on the
   distinction between patriotism and nationalism},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2024},
Volume = {29},
Number = {1},
Pages = {64-78},
Month = {JAN 2},
Abstract = {Patriotism and nationalism are typically conceptualized as two distinct
   if not conflicting forms of national attachment. Carrying a substantial
   normative dimension, the former is often linked to an enlightened sense
   of community that takes pride in the commitment to democratic values,
   whereas the latter is understood as an exclusionary and uncritical
   attachment to the nation. It is this dichotomy and the categorical
   assumptions at its core which this article subjects to critical
   appraisal largely missing from the debate. We first (a) address the
   confusion that comes from constructing national attachment as fixed and
   dualistic. Then, we (b) highlight the lack of contextual awareness in
   empirical research that employs scales and items regardless of
   geographical or temporal peculiarities. Finally, we (c) cast light on
   the implications entailed, in particular, the legitimacy provided to the
   civic ideal of a good and reasonable patriotism.},
