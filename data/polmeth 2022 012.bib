@article{ WOS:000765823200020,
Author = {Liao, Yue and Zhang, Xinyu},
Title = {Chinese studies on populism: a bibliometric analysis from 2000 to 2020},
Journal = {IZQUIERDAS},
Year = {2022},
Volume = {51},
Month = {MAR},
Abstract = {The 21st century has witnessed the resurgence of populism. Using
   bibliometric method, this article aims to review the research, major
   topics and main trends in this field by Chinese academic journals during
   2000 and 2020. The analysis shows an exponential increase in research in
   the analyzed period. As part of the research results, most journals that
   published populism-related articles are comprehensive journals and there
   is little cooperation between different Chinese researchers or research
   teams. Most of the papers published in the field are of political
   science, whose major topics include historical research, definition of
   populism, digital populism in China, as well as the characteristics and
   impacts of contemporary populism. While ``Mao Zedong{''} is the earliest
   and the constant research focus, ``European Union{''} emerges as the
   latest focus in Chinese populism studies.},
