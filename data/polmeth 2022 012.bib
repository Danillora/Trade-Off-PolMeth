@article{ WOS:000762208000001,
Author = {Carty, R. Kenneth},
Title = {Into the void: the collapse of Irish party democracy},
Journal = {IRISH POLITICAL STUDIES},
Year = {2022},
Volume = {37},
Number = {2, SI},
Pages = {303-325},
Month = {APR 3},
Abstract = {For most of the twentieth century the Irish party system was stable and
   unchanging, centred on Fianna Fail, Europe's most electorally successful
   political party. That system suddenly collapsed in 2011 when Irish
   electoral politics became unusually volatile, ending decades of
   one-party dominance and leaving democratic elections unable to provide
   for government choice and accountability. This paper traces this
   transformation of the party system, unmasking the altered dynamics of
   the country's electoral competition, through a focus on its
   long-dominant natural party of government. It concludes by considering
   the challenges and prospects for Ireland's party democracy.},
