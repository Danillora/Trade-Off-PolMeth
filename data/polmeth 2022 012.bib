@article{ WOS:000842870800001,
Author = {Gunes, Ali and Ozkaleli, F. Murat},
Title = {An intertemporal statistical analysis to ideal point estimates: EU
   cohesion at the UNGA revisited},
Journal = {COMPARATIVE EUROPEAN POLITICS},
Year = {2023},
Volume = {21},
Number = {3},
Pages = {333-355},
Month = {JUN},
Abstract = {This paper offers an intertemporal statistical analysis employing ideal
   point estimates based on the United Nations General Assembly roll- call
   data to investigate the European Union's overall voting cohesion along
   with utilizing a new taxonomy that distinguishes member deviations and
   non- member adhesions. The intertemporal cardinal analysis adopted here
   presents a viable alternative to the earlier calculations of EU
   cohesion. Our analysis captures the higher cohesion of EU relative to
   non- EU states and critical junctures such as Brexit. In comparison to
   the existing literature, our calculations show that deviations of France
   and Greece have been statistically insignificant, except for few acute
   years, whereas the UK has been the only deviant member chronically. Our
   observations reveal that Turkey and Norway are adhesive to the EU,
   whereas superpowers, the US, Russia, and China, are located afar from
   the EU cohesion, locating the EU as a separate actor in the emerging
   multipolar world.},
