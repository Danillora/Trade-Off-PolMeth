@article{ WOS:000837182800040,
Author = {Kopcha, Vasyl and Mamontov, Ihor and Reznik, Hanna and Semko, Vadym and
   Yakhno, Olekij},
Title = {Forms of implementation of legal policy in the field of civil law},
Journal = {CUESTIONES POLITICAS},
Year = {2022},
Volume = {40},
Number = {73},
Pages = {693-712},
Month = {JUL-DEC},
Abstract = {One of the most pressing modern problems of international law is the
   study of the characteristics of the regulation of civil law, as well as
   the forms of application of legal policy in the field of civil law. The
   guidelines for the development of private law policy are not only
   related to the development of legislation and the improvement of civil
   law doctrine, but also to the reform of judicial approaches in the
   examination of civil law disputes. The aim of the study is to form a
   scientific understanding of legal policy in the field of civil law,
   taking into account its current state. The multiplicity of objectives is
   dictated by the search for legal tools that optimize the existing
   mechanism of regulation of civil law as a solid basis to guarantee the
   effective application and full protection of the subjective rights and
   legitimate interests of civil law. In carrying out the study of the
   subject, the traditional scientific methods of knowledge were applied in
   jurisprudence, whose basis is the method of materialist dialectics,
   which allows to provide a comprehensive analysis of the processes under
   study in their conditionality and historical interrelation.},
