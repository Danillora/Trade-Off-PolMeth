@article{ WOS:000973381800004,
Author = {Guevara, Carolina and Salazar Mendez, Yasmin},
Title = {The influence of cities and their environmental management on life
   satisfaction: urban and rural perspectives},
Journal = {AMERICA LATINA HOY-REVISTA DE CIENCIAS SOCIALES},
Year = {2022},
Volume = {92},
Pages = {67-94},
Month = {DEC},
Abstract = {This study analyses the influence of city size on life satisfaction
   among the Ecuadorian population. It distinguishes rural from urban
   residents and looks at the effect of environmental management. Using
   individual and canton-level variables, an ordinal logit model is
   estimated. Results suggest that individuals living in the most populated
   cantons report a lower level of life satisfaction. This effect varies
   for urban and rural residents. Environmental management is positive for
   life satisfaction.},
