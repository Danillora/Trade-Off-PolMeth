@article{ WOS:000805498400001,
Author = {Ismail, Muhammad and Shah, Azhar and Idrees, Muhammad},
Title = {The Dynamics of Anti-Americanism Among Educated Youth in Pakistan},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Year = {2024},
Volume = {9},
Number = {4},
Pages = {508-528},
Month = {DEC},
Abstract = {Internal and external sources cause anti-Americanism. This study does
   not justify any terrorist and extremist activities, whether against the
   US or other states. In Pakistan, these factors shape the anti-American
