@article{ WOS:000747826700010,
Author = {Murphy, Wendy Wiedenhoft},
Title = {Accounting for Justice: Citizen Public Debt Audits and the Case of
   Puerto Rico},
Journal = {STUDIES IN SOCIAL JUSTICE},
Year = {2022},
Volume = {16},
Number = {1},
Pages = {182-199},
Abstract = {A Citizen Public Debt Audit (CPDA) is an emancipatory praxis that can
   mobilize citizens to make legible public debt that has been accrued in
   their name. Ideally, it should hold creditors accountable for debt that
   is determined to be odious. This study examines the public debt crisis
   in Puerto Rico to illustrate the historically unjust circumstances under
   which public debt was accumulated on the island in the context of US
   federal taxation and economic policies. It explains how citizens are
   mobilizing via a CPDA to make these circumstances legible and argues
   that citizens should not be obliged to service debt that was accrued
   contrary to their own welfare, especially if conditions of repayment
   threaten their current and future well-being.},
Publisher = {UNIV WINDSOR, CENTRE STUDIES SOCIAL JUSTICE},
