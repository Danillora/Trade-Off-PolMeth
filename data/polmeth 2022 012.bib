@article{ WOS:000905368000001,
Author = {Vinogradov, Mikhail Yu. and Suslova, Anastasia A.},
Title = {THE PHENOMENON OF SOCIAL APATHY AND ITS RELEVANCE IN MODERN RUSSIA},
