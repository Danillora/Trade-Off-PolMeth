@article{ WOS:000793713700006,
Author = {Bandura, Oleg and Lytvynov, Oleksandr and Maksymov, Sergiy and
   Pavlyshyn, Oleg and Smaznova, Irina},
Title = {Semiotics of law in modern philosophical and legal research},
