@article{ WOS:000933743900001,
Author = {Munoz, Lourdes Gaitan},
Title = {Debates and challenges in the sociology of childhood facing a new era},
Journal = {POLITICA Y SOCIEDAD},
Year = {2022},
Volume = {59},
Number = {3},
Abstract = {The sociology of childhood has been explicitly recognized by some
   authors as the structuring element of current childhood social studies,
   characterized by three key features: the very concept of childhood as a
   social category, the recognition of children as subjects with a role in
   society and the generational character of the childhood-society and
