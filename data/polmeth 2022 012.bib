@article{ WOS:000865612500001,
Author = {Egami, Naoki and Hartman, Erin},
Title = {Elements of External Validity: Framework, Design, and Analysis},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2023},
Volume = {117},
