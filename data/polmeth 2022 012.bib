@article{ WOS:000766738800005,
Author = {Luis Ledesma, Jose},
Title = {JUSTICE BEYOND THE STATE OUTER MARGINS: CONTENDING FORMS OF ANTIFASCIST
   VIOLENCE AND JUDICIARY PRACTICES IN THE SPANISH CIVIL WAR (1936-39) AND
   LIBERATION-FRANCE (1944-45)},
Journal = {PARTECIPAZIONE E CONFLITTO},
Year = {2022},
Volume = {15},
Number = {1, SI},
Pages = {72-87},
Abstract = {Although civil war scholarship focuses on the study of violence in
   internecine conflict, little attention has been paid to the ways in
   which extra-judiciary violence and the legal administration of justice
   interact. This article explores the relationship between state justice
   and extra-judiciary practices in two settings: Republican-held
   territories during the Spanish Civil War (1936-39) and the later stages
   of WWII and Liberation-France (1944-45). This paper challenges the view
   that the violence perpetrated by armed non-state agents flared and
   existed only because of state institutional justice was lacking, and
   until it was replaced by it. After a general overview of violence and
   justice in civil war settings, this article summarizes the differences
   and blurred boundaries between extra-judiciary violence and legal
   justice in both case studies; finally, it argues that exploring the
   complex links between ``legal justice{''} and ``illegal revolutionary
   violence{''} is a useful way to improve our understanding of revolutions
   and civil wars, as violence in those contexts constitutes a challenge to
   existing law and seeks to create new and non-conventional ways to
   implement justice.},
