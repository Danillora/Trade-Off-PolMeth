@article{ WOS:001128417100005,
Author = {Fernando Vargas-Alzate, Luis},
Title = {The Decisional Factor in Colombian Foreign Policy: Colombia's Entry into
   the Pacific Alliance},
Journal = {DESAFIOS},
Year = {2022},
Volume = {34},
Number = {SI},
Abstract = {This article delves into a topic that has been little addressed in
   previous studies on Colombian foreign policy: decision-making processes.
   I rely on two classic decision-making models (Putnam and Allison's) and
   use them to analyze the decisional procedure that led Colombia to join
   the Pacific Alliance in 2012. The case study shows that certain actors
   in the private sector (mainly economic associations) strongly influenced
   the executive branch's actions regarding Colombian foreign policy.
   Generating such strategies in the country departs from traditional views
   of how public policy is generated, showing that in this particular
   field, it can be influenced by the privileged actors closest to the
   President. This research highlights the need and the various
   opportunities for further work on Foreign Policy Analysis (FPA) for the
   Colombian case.},
