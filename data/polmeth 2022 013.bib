@article{ WOS:000779978800005,
Author = {Chalupecky, Petr},
Title = {Thinking and Work of Jaroslav Nebesar after World War II},
Journal = {POLITICKA EKONOMIE},
Year = {2022},
Volume = {70},
Number = {1},
Pages = {97-113},
Abstract = {The article deals with the fate of the first governor of the
   Czechoslovak Central Bank after World War II. It analyses his economic
   thinking and the sources that influenced it. The text sees him as deeply
   rooted in non-liberal Central European tradition but with a strong
   impact of Anglo-Saxon economics of the interwar period. The article also
   explores his contribution to the transition of the Czechoslovak economy
   to Soviet-type central planning and proves the hypothesis that his
   influence was much smaller than existing literature indicates.},
