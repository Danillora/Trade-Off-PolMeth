@article{ WOS:000740407800001,
Author = {Olson, Michael P. and Stone, Andrew R.},
Title = {The Incumbency Advantage in Judicial Elections: Evidence from Partisan
   Trial Court Elections in Six US States},
Journal = {POLITICAL BEHAVIOR},
