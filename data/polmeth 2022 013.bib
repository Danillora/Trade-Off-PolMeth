@article{ WOS:000816846100017,
Author = {Gol'c, Aleksandr},
Title = {The Merciless Honesty of War},
Journal = {OSTEUROPA},
Year = {2022},
Volume = {72},
Number = {1-3},
Pages = {169+},
Abstract = {Like no other event, war tests the ability of political leaders to act
   calmly and rationally. It unrelentingly punishes those who do not bring
   their ambitions into line with their capabilities. One should start a
   war only when the vital interests of the state are actually threatened
   and all non-military means have been exhausted beforehand. Before making
   the decision to go to war, one must make sure one has complete political
   support at home and abroad. With his decision to launch a special
   military operation, Vladimir Putin disregarded all of these principles.},
