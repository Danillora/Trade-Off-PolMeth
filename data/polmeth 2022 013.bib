@article{ WOS:000788543900001,
Author = {Tank, Lukas},
Title = {Against the budget view in climate ethics},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2024},
Volume = {27},
Number = {7},
Pages = {1075-1088},
Month = {NOV 9},
Abstract = {The extent of our duties to mitigate climate change is commonly
   conceptualized in terms of temperature goals like the 1.5 degrees C and
   the 2 degrees C target and corresponding emissions budgets. While I do
   acknowledge the political advantages of any framework that is relatively
   easy to understand, I argue that this particular framework does not
   capture the true extent of our mitigation duties. Instead I argue for a
   more differentiated approach that is based on the well-known distinction
   between subsistence and luxury emissions. At the heart of this approach
   lies the argument that we have no budget of substantial, net-positive
   luxury emissions left. In a world in which dangerous climate change has
   begun, we must expect all further substantial, net-positive luxury
   emissions to cause harm. Since they lack the kind of justification
   needed for them to be nevertheless permissible, I conclude that we must
   stop emitting them with immediate effect. I also briefly discuss the
   difficult case of subsistence emissions and offer some first thoughts on
   the morality of a third category of emissions, what I call `transition
   emissions'.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
