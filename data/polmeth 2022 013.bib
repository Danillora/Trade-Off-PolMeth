@article{ WOS:001318499400006,
Author = {Hopper, Tilman and Ticona, Juan},
Title = {Parliamentary Inquiries: an Underestimated Anticorruption Tool},
Journal = {INTERNATIONAL JOURNAL OF PARLIAMENTARY STUDIES},
