@article{ WOS:000880001200018,
Author = {Fedchyniak, Artur and Danylevskyi, Andrii and Nazymko, Yehor and
   Kuleshov, Oleksandr and Makarenko, Tamara},
Title = {Military administrations as an element of national security under
   international and Ukrainian legislation},
