@article{ WOS:000911075100001,
Author = {Renwick, Alan and Dobrianska, Nadia and Kelly, Conor J. and Kincaid,
   Charlotte},
Title = {Public attitudes to referendums on Irish unification in Northern
   Ireland: evidence from an online consultation},
Journal = {IRISH POLITICAL STUDIES},
Year = {2023},
Volume = {38},
Number = {4},
Pages = {413-437},
Month = {OCT 2},
Abstract = {Debate on Irish unification has increased in recent years, yet public
   attitudes on the processes that might lead to it are underexplored. This
   article examines the results of an online public consultation carried
   out in Northern Ireland in the summer of 2020 on how any future
   referendums on the unification question would best be conducted. It
   shows that hopes and fears on this question exist across society in
   Northern Ireland. It also reveals that public views are yet to
   crystallise on many of the design features of a referendum. There are
   some exceptions to this: there is a widespread feeling that the Brexit
   referendum process should not be replicated; and there are signs of
   divergent views emerging between communities on the franchise,
   referendum threshold, and use of citizens' assemblies. Nevertheless, we
   conclude that the lack of hardened views on most questions of process
   points to an opportunity for policymakers and impartial observers to
   foster agreement on how a referendum process should be conducted.},
