@article{ WOS:000769286100001,
Author = {Jonelis, Andrew and Suwanprasert, Wisarut},
Title = {Protection for sale: evidence from around the world},
Journal = {PUBLIC CHOICE},
Year = {2022},
Volume = {191},
Number = {1-2},
Pages = {237-267},
Month = {APR},
Abstract = {The ``protection-for-sale{''} motive introduced by Grossman and Helpman
   (Am Econ Rev 84: 833-850, 1994) has been adopted widely in the
   literature, but only a few papers test the theory empirically. To
   provide empirical evidence for the protection-for-sale theory, we
   proceed in three steps. First, we argue that among all existing
   theories, only the mechanism in the protection-for-sale theory depends
   on the government's political strength. Second, we develop a theoretical
   model to rationalize the connection between political strength and
   import tariffs. Our extended protection-for-sale model predicts that a
   government with greater political power generally imposes higher
   tariffs. Third, we propose that political strength can be proxied by the
   share of legislative seats held by the governing party or coalition. We
   test the model prediction using panel data covering 95 product
   categories and 105 countries, from 1996 to 2014. Our estimates provide
   support the protection-for-sale theory. The estimated effects of
   political strength on tariffs are larger in small and democratic
   countries.},
Publisher = {SPRINGER},
