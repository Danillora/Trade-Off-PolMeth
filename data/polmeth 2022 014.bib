@incollection{ WOS:000945558200021,
Author = {Plockinger, Othmar},
Editor = {Michalczyk, JJ and Bryant, MS and Michalczyk, SA},
Title = {A resume of the reception of Mein Kampf in Germany (1925-1945)},
Booktitle = {HITLER'S MEIN KAMPF AND THE HOLOCAUST: A Prelude to Genocide},
Series = {Perspectives on the Holocaust},
Year = {2022},
Pages = {253-263},
Abstract = {After the Second Word-War the reception of Hitler's Mein Kampf was part
   of the discussion about National-Socialism in Germany. The legend became
   prevalent that the book was an ``unread bestseller{''}. But
   investigating the real circumstances in detail another picture emerges.
   Before 1933 many newspapers and magazines wrote about the book, Mein
   Kampf became essential in the public discussions about
   National-Socialism and several state-institutions and ministries studied
   the book. Remarkably anti-Semitism didn't play an important role in all
   those texts. After 1933 the distribution of Mein Kampf did not only rise
   extraordinary but also was controlled strictly in order to give it the
   sacrosanct status of ``The book of the Germans{''}. All in all the
   reception of Mein Kampf on public, state and private level was
   considerable. For that reason a nuanced scientific research to this
   topic can give a useful contribution to the study of National-Socialism.},
