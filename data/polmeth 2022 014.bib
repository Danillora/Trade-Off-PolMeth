@article{ WOS:000779978800003,
Author = {Barak, Vladimir and Krebs, Vojtech and Mitwallyova, Helena},
Title = {SUPPORT FOR INFORMAL CARERS: HAS THE NEW BENEFIT IMPROVED THEIR ABILITY
   TO CARE?},
Journal = {POLITICKA EKONOMIE},
Year = {2022},
Volume = {70},
Number = {1},
Pages = {51-76},
Abstract = {The purpose of this article is to evaluate the subjective impact of
   provided care on the quality of life of informal carers, to assess the
   institution of long-term carer's allowances from the viewpoint of
   informal carers, and to identify additional social policy tools that
   could, in carers' opinion, improve the provision of care. Our research
   shows that provision of care leads to a reduced quality of life for a
   significant number of respondents. It is confirmed that respondents'
   welfare is negatively influenced by a lack of funding and weak
   development of social services. It is not proven that the long-term
   carer's allowance is a comprehensible benefit increasing the carers'
   quality of life providing enough motivation to care. This sickness
   insurance benefit is intended primarily for a temporary lack of
   self-sufficiency, with the prospect of future improvement. Long-term or
   permanent lack of self-sufficiency and the related care must be secured
   by social support and assistance mechanisms, including respite care, and
   ought to be funded outside the framework of sickness insurance.},
