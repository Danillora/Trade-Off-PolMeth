@article{ WOS:000923776300001,
Author = {Marechal, Francois and Moran, Pierre-Henri},
Title = {Should bid Evaluation Formulas for Choosing the Economically Most
   Advantageous Tender in Public Procurement Contracts be Changed?},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2022},
Volume = {132},
Number = {5},
Pages = {697-722},
Month = {SEP-OCT},
Abstract = {This article analyzes the methods of selecting the most economically
   advantageous tender in public procurement. It has two objectives.
   Firstly, it evaluates the properties of the relative bid evaluation
   formulas used in practice for the criteria for selecting the bids.
   Through examples, it underlines that these bid evaluation formulas turn
   out to be rather arbitrary, manipulable, and do not reveal the
   preferences of public purchasers. The article then presents the steps
   for the construction of a scoring rule allowing to put bidders in
   competition in a transparent way by choosing for each criterion a bid
   evaluation formula and a weighting that reflect the preferences of the
