@article{ WOS:000843398900001,
Author = {Chou, Chung-Hui},
Title = {Does possessing an installed base induce a proprietary software producer
   to act aggressively or mildly in pricing and intrinsic quality
   provision?},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2023},
Volume = {70},
Number = {2},
Pages = {133-143},
Month = {MAY},
Abstract = {This paper investigates competition between open-source software (OSS)
   and proprietary software (PS) which possesses an installed base. We find
   that an increase in PS installed base does not necessarily induce the PS
   producer to increase price or reduce intrinsic quality. Specifically, if
   OSS is weakly (moderately) compatible with PS, then an increase in PS
   installed base induces the PS producer to invest less (more) in
   intrinsic quality. Regarding the price of PS, if OSS is highly
   compatible with PS possessing a large installed base, then an increase
   in installed base induces the PS producer to sell products for a lower
   price.},
