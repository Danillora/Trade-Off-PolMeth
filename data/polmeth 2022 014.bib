@article{ WOS:000878420400001,
Author = {Bustos, Sebastian and Morales-Arilla, Jose},
Title = {Gains from globalization and economic nationalism: AMLO versus NAFTA in
   the 2006 Mexican elections},
Journal = {ECONOMICS \& POLITICS},
Year = {2024},
Volume = {36},
Number = {1},
Pages = {202-244},
Month = {MAR},
Abstract = {Do gains from globalization erode support for economic nationalism? We
   study how North American Free Trade Agreement (NAFTA)-enhanced local
   access to US markets affected Mexican demands for protectionist
   platforms. The left-led by Andres Manuel Lopez Obrador
   (AMLO)-underperformed in cities benefiting from export-access gains
   during the 2006 presidential elections. This effect is observed strictly
   in 2006-the only post-NAFTA election in which debates over trade
   integration played a salient role. Our findings are robust to controls
   for import-competing pressures from NAFTA and the China shock. AMLO's
   2006 protectionist platform likely cost him that year's election, and
   campaign media strategies in 2012 map to this earlier backlash.},
