@article{ WOS:000869865600002,
Author = {Giovanis, Eleftherios and Ozdamar, Oznur},
Title = {The nexus between business-investment climate and firm performance in
   the Middle East and North Africa (MENA) region},
Journal = {REVIEW OF ECONOMICS AND POLITICAL SCIENCE},
