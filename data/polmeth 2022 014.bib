@article{ WOS:000861935000006,
Author = {Gasche, Malte and Prempain, Laurence},
Title = {(Dis)Playing Exotic Otherness in the Circus: The Bouglione Wild West
   Show},
Journal = {EUROPEAN HISTORY QUARTERLY},
