@article{ WOS:000801587700005,
Author = {Ruckteschler, Christian and Malik, Adeel and Eibl, Ferdinand},
Title = {Politics of trade protection in an autocracy: Evidence from an EU tariff
   liberalization in Morocco},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2022},
Volume = {71},
Month = {JAN},
Abstract = {Although non-tariff measures (NTMs) have surpassed tariffs as the most
   prevalent instrument of trade protection globally, our knowledge of what
   drives these NTMs is extremely limited. This paper sheds light on the
   political determinants of non-tariff protection using a rich empirical
   setting in Morocco. Taking advantage of a bilateral EU-Morocco trade
   agreement that resulted in an across-the-board tariff cut and a
   subsequent rise in NTMs, we use a difference-in-differences regression
   framework to show that sectors with close prior political connections to
   the royal family received disproportionately higher levels of non-tariff
   protection after the EU Agreement than unconnected sectors. We also
   demonstrate that, in the wake of the EU-induced tariff cut, connected
   sectors were mainly compensated through technical barriers to trade that
   depend on administrative oversight and are vulnerable to political
   influence.},
Publisher = {ELSEVIER SCIENCE INC},
