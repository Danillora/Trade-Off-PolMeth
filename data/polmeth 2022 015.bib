@article{ WOS:000776632400001,
Author = {Orchard, Ximena and Gonzalez-Bustamante, Bastian},
Title = {Power Hierarchies and Visibility in the News: Exploring Determinants of
   Politicians' Presence and Prominence in the Chilean Press (1991-2019)},
Journal = {INTERNATIONAL JOURNAL OF PRESS-POLITICS},
