@article{ WOS:000782664600001,
Author = {Yen, Wei-Ting},
Title = {Shaky social insurance foundation: Income instability and pension
   participation in South Korea},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2022},
Volume = {103},
Number = {3},
Pages = {494-508},
Month = {MAY},
Abstract = {Objective Many welfare states in young democracies feature low effective
   social insurance coverage, especially in old-age pension. What explains
   the ineffective coverage? This article shows individual income
   instability is a microlevel factor contributing to low effective
   coverage. Method The article used 15 waves of the Korean Labor Income
   Panel Study and constructed a fine-grained individual-level income
   instability measure, which was used to predict individual participation
   in state-run public pension using (multinomial) logistic models with
   industry, occupation, and cohort fixed effects. Result The article shows
   that income instability is negatively associated with public pension
   participation, and the negative relationship becomes larger as income
   level increases. Moreover, income instability inhibits only
   participation in state-led risk pooling, but not in private
   self-insuring. Conclusion The article sheds light on disentangling an
   individual-level factor buttressing young welfare states and bears
   implications for the future contour of welfare states in young
   democracies.},
