@article{ WOS:000900756800001,
Author = {Jiang, Liang},
Title = {Exposure to Campaign and Partisan Voting: The Moderating Roles of
   Voters' Political Predispositions},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2022},
Volume = {68},
Number = {4},
Pages = {566-592},
Month = {DEC},
Abstract = {How do partisan citizens respond to campaign information and make voting
   decisions? We theorise that the association between exposure to campaign
   information and partisan voting is moderated by citizens' political
   predispositions - strength of partisanship, political sophistication,
   and election interest. To test our expectations, we used Australian
   Election Studies data from 2013, 2016, and 2019. We found that partisans
   who are exposed to campaign information tend to follow their party
   preference when strength of partisanship is high. For political
   sophistication, we found that the influence of campaign information on
