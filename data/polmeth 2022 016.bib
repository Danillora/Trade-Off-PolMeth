@article{ WOS:000881863600001,
Author = {Chen, Melinda},
Title = {The compounded2 nature of the Covid pandemic on survivors of
   sexual violence},
Journal = {SOCIAL MOVEMENT STUDIES},
Year = {2022},
Month = {2022 NOV 11},
Abstract = {Alarm has been raised by the mainstream anti-rape movement in the United
   States at the compounded nature of sexual violence experienced by
   survivors during the Covid pandemic. While these efforts show renewed
   attention at the problem of compounded sexual violence, these framings
   of sexual violence as compounded miss the intersectional oppressions
   experienced by marginalized survivors, such as survivors of color living
   through a pandemic. This profile advocates for an intervention, asking
   anti-rape activist-advocates to turn toward an anti-rape movement that
   addresses the `compounded-squared' effects of sexual violence during a
   pandemic.},
