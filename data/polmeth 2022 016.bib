@article{ WOS:000789674000008,
Author = {Cohle, Zachary and Ortega, Alberto},
Title = {Life of the party: The polarizing effect of foreign direct investment},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2022},
Volume = {72},
Month = {MAR},
Abstract = {As countries are becoming increasingly connected with one each other, it
   is essential to studythe influence multinational firms have on political
   outcomes in the countries where they locate.This paper examines the
   effects of foreign direct investment (FDI) on party vote shares in149
   countries between 1990 and 2017. We construct a database by cataloging
   political partiesinto specific political positions. We use a measure of
   linguistic distance between countries toconstruct a novel instrumental
   variable using the average exchange rate of the surroundingregion. With
   this IV, we capture the relative attractiveness of FDI in host
