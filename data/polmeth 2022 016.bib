@article{ WOS:000822385800001,
Author = {Kim, Hye-Sung and Lee, Youngchae and Huffmon, Scott},
Title = {Public opinion on US investment in foreign countries: Survey evidence
   from 11 southern states in the United States},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2022},
Volume = {103},
Number = {5},
Pages = {1113-1124},
Month = {SEP},
Abstract = {Objective Despite a growing literature on the impact of economic
   nationalism on public attitudes toward foreign direct investment (FDI)
   inflows, public attitudes toward U.S. FDI outflows have been overlooked.
   We examine U.S. residents' attitudes toward FDI outflows to two host
   countries viewed unfavorably by the American public, Mexico and China,
   and the extent to which providing accurate information on FDI flows
   between the United States and these countries affects the biases toward
   American investment in these countries. Methods We implemented a
   vignette experiment on respondents from 11 southern states in the United
   States. Results We find little support for the existence of bias toward
   Mexico or China as destinations for American investment, nor do we find
   that providing accurate information on bilateral FDI flows affects
   perceptions of U.S. FDI outflows to these countries. Instead,
   individuals' economic self-interest and sociotropic concerns influence
   their attitudes toward FDI outflows. Conclusions Individuals'
   preferences toward U.S. FDI outflows are not shaped by biases toward
