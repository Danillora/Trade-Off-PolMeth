@article{ WOS:000904203100001,
Author = {Trere, Emiliano and Bonini, Tiziano},
Title = {Amplification, evasion, hijacking: algorithms as repertoire for social
   movements and the struggle for visibility},
Journal = {SOCIAL MOVEMENT STUDIES},
Year = {2024},
