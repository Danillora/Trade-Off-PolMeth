@article{ WOS:000801587700016,
Author = {Baraldi, Anna Laura and Immordino, Giovanni and Stimolo, Marco},
Title = {Self-selecting candidates or compelling voters: How organized crime
   affects political selection},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2022},
Volume = {71},
Month = {JAN},
Abstract = {Using data on mayoral candidates and elected mayors in Italian
   municipalities, this paper aims to unveil the mechanism underlying the
   evidence that organized crime lowers politicians' quality. We exploit
   the dissolution of city governments for presumed mafia infiltration as
   an exogenous shock to the presence of organized crime in local politics.
   We implement a difference-in-differences methodology to test whether
   organized crime discourages qualified individuals to run for elections.
   Our results indicate that the active presence of organized crime does
   not prevent highly qualified candidates from running but does induce
   citizens to vote for less qualified ones. The identification of the
   channel through which organized crime influences local politics is
   important to better direct public intervention against mafia
   infiltration.},
