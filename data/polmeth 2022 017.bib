@article{ WOS:000810357000004,
Author = {Mondello, Gerard},
Title = {Construction of Belief Systems and Controversies around Covid-19},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2022},
Volume = {132},
Number = {1},
Pages = {79-111},
Month = {JAN-FEB},
Abstract = {The Covid-19 pandemic saw the emergence of public opinion in the public
   health decision that often was strongly contested. Relying on both
   recent work in behavioral psychology and psycho-sociology, as well as on
   the theory of ambiguity, this contribution attempts to analyze the
   reasons for this doubt. Two types of patients will be distinguished,
   ``analytical{''} and ``intuitive{''}, to explain the choices for a given
   action. This a priori distinction is not sufficient to explain the
   choices of a given type because the degree of aversion to ambiguity and
   the attitude towards ambiguity can modify the latter. It is in fact the
   belief in the capacity of one treatment to heal in relation to another
   that proves to be fundamental in determining the choice for one of them.},
