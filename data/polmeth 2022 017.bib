@article{ WOS:000789785900001,
Author = {Carvalho, Antonio and Ferreira, Vera},
Title = {Climate crisis, neoliberal environmentalism and the self: the case of
   `inner transition'},
Journal = {SOCIAL MOVEMENT STUDIES},
Year = {2024},
