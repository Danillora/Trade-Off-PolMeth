@article{ WOS:000893018500015,
Author = {Chen, Qianmiao and Huang, Qingyang and Liu, Chang and Wang, Peng},
Title = {Career incentives of local leaders and crisis response: A case study of
   COVID-19 lockdowns in China},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2022},
Volume = {75},
Month = {DEC},
Abstract = {This paper studies the role of local Chinese leaders' career incentives
   in decisions regarding large-scale crises such as the COVID-19 pandemic.
   Most local leaders were reluctant to impose lock -downs at the beginning
   of the pandemic, because their promotions rely on posting strong numbers
   for economic growth in their region, while lockdowns can suppress
   growth. Once the nation's top leader warned that local leaders who
   failed to control the disease would be removed from office, many rapidly
   implemented resolute measures. However, we find that local leaders with
   larger promotion incentives were still more likely to downplay the virus
   by avoiding or minimizing lockdowns.},
