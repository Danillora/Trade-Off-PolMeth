@article{ WOS:000843743000001,
Author = {Reardon, Louise and Marsden, Gregory and Campbell, Morgan and Gupta,
   Sanjay and Verma, Ashish},
Title = {Analysing multilevel governance dynamics in India: exercising hierarchy
   through the Smart Cities Mission},
Journal = {TERRITORY POLITICS GOVERNANCE},
Year = {2024},
Volume = {12},
Number = {8},
Pages = {1217-1235},
Month = {SEP 13},
Abstract = {This paper provides a theoretically informed empirical analysis of how
   Narendra Modi's claim of `more governance, less government' is felt in
   practice in India. It does so through a qualitative case study of the
   Smart Cities Mission and its implementation in four cities, applying
   multilevel governance theorizing. It finds centralizing dynamics at play
   through the use of `special purpose vehicles', which bypass local-level
   institutions. The paper highlights how multilevel governance dynamics
   can be shaped through institutional design, a point overlooked in the
   literature. It also raises questions about the risks of such
   arrangements for long-term, democratic development.},
