@article{ WOS:000766770600001,
Author = {Kurtzberg, Terri R. and Naquin, Charles E. and Ameri, Mason},
Title = {Deception by device: are we more self-serving on laptops or cell phones?},
Journal = {INTERNATIONAL JOURNAL OF CONFLICT MANAGEMENT},
