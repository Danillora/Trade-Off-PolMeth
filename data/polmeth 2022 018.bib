@article{ WOS:000758847600001,
Author = {Bandula-Irwin, Tanya and Gallien, Max and Jackson, Ashley and van den
   Boogaard, Vanessa and Weigand, Florian},
Title = {Beyond Greed: Why Armed Groups Tax},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2024},
Volume = {47},
Number = {12},
Pages = {1599-1622},
Month = {DEC 1},
Abstract = {Based on a review of the diverse practices of how armed groups tax, we
   highlight that a full account of why armed groups tax needs to go beyond
   revenue motivations, to also engage with explanations related to
   ideology, legitimacy, institution building, legibility and control of
   populations, and the performance of public authority. This article
   builds on two distinct literatures, on armed groups and on taxation, to
   provide the first systematic exploration of the motivations of armed
   group taxation. We problematize common approaches toward armed group
   taxation and state-building, and outline key questions of a new research
   agenda.},
