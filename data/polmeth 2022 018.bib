@article{ WOS:000822049300001,
Author = {Weaver, Duncan},
Title = {Extinction Rebellion: greening vanguardism?},
Journal = {SOCIAL MOVEMENT STUDIES},
Year = {2024},
Volume = {23},
Number = {1, SI},
Pages = {113-121},
Month = {JAN 2},
Abstract = {This profile offers an analysis of Extinction Rebellion (XR) from the
   vantage point of vanguardism. Vanguardism is a Leninist approach that
   marshals universal(isable) grievances to mobilise mass-constituencies.
   XR is used as a conduit through which to raise awareness of
   vanguardism's environmental applications. XR is introduced before a
   theoretical framework for understanding vanguardism is offered. The
   article then discusses XR's politics, before discussing its protest
   repertoire. It is found that whilst vanguardism is ordinarily associated
   with the left, XR contributes to its `greening', taking the pursuit of
   uprising beyond the left.},
