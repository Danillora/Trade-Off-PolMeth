@article{ WOS:000823009200001,
Author = {Kapounek, Svatopluk and Hanousek, Jan and Bily, Frantisek},
Title = {Predictive Ability of Altman Z-score of European Private Companies},
Journal = {POLITICKA EKONOMIE},
Year = {2022},
Volume = {70},
Number = {3},
Pages = {265-287},
Abstract = {The paper investigates the relationship between the financial distress
   of European private companies identified by the Altman Z-score and real
   bankruptcy. We extend the traditional Z-score with the asymmetric effect
   of economic activity. Our results show higher forecasting performance of
   the Altman Z-score of large companies in a three-year projection. We
   argue that our results differ from Altman (1968) because of specific
   market conditions in Europe that enable prolongation of activity after
   financial distress is identified. We also emphasize the role of
   liquidity, size, performance and indebtedness in increasing financial
   distress forecasting performance. Finally, we extend our prediction
   model with selected indicators of quality and development of the
   institutional environment.},
