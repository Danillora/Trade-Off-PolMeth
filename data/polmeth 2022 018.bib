@article{ WOS:000807230500006,
Author = {Quinn, Genevieve},
Title = {Evolution and Electoral Implications of Congressional Gun Control Issue
   Framing: ``From Crime Control to Mass Shootings{''}},
Journal = {JOURNAL OF POLICY HISTORY},
Year = {2022},
Volume = {34},
Number = {3},
Pages = {440-472},
Month = {JUL},
Abstract = {This paper traces the political development of Congressional gun control
   issue framing (with a specific focus from the early 1990s to the
   present), demonstrating that there have been two primary contexts in
   which gun control policy has been debated over this time frame: as a
   component of general crime control and as a specific response to mass
   shooting events. It identifies the primary historical, political, and
   electoral forces shaping the gun control debate in a given period while
   distinguishing the critical changes that drove the evolution from a
   crime control to a mass shooting focus. It assesses the degree of policy
   coherence and electoral salience specific to each context, illuminating
   why ``gun control as crime control{''} had bipartisan Congressional
   support in the early 1990s and identifying what comparative
   disadvantages hinder the mass shooting focus of the present while also
   recognizing that the latter unfolded against a backdrop of heighted
   partisan polarization. The paper concludes that although one cannot
   compare the crime control and mass shooting framing contexts in a
   political vacuum, the electoral implications particular to each are
   relevant for understanding legislative action or inaction in Congress
   over the past thirty years, if more so in some periods than others.},
