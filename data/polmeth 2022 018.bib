@article{ WOS:000893797900001,
Author = {Soni, Anmol and Jose, Justina and Kingsley, Gordon A.},
Title = {When cities take control: Explaining the diversity of complex local
   climate actionsPalabras Clave(sic)(sic)(sic)},
Journal = {REVIEW OF POLICY RESEARCH},
