@article{ WOS:000893215000012,
Author = {De Navascues, Santiago},
Title = {Yeast and dough: Failed discussions on the European spirit
   at the International Institute of Intellectual Cooperation (1933-1936)},
Journal = {HISTORIA Y POLITICA},
Year = {2022},
Number = {48},
Pages = {343-366},
Month = {JUL-DEC},
Abstract = {This article examines the main discussions about the European spirit in
   the intellectual debates sponsored by the League of Nations. Using as a
   reference the congresses on intellectual cooperation held in various
   European capitals between 1933 and 1936, we will analyse the
   perspectives and limits of the reflections on the Europeanism of the
   intellectuals in the 1930s. Through the documentation of the
   International Institute of Intellectual Cooperation, you can reconstruct
