@article{ WOS:000869865700005,
Author = {el-Shafei, Alyaa Wagdy and Metawe, Mohamed},
Title = {China drive toward Africa between arguments of neo-colonialism and
   mutual-beneficial relationship: Egypt as a case study},
Journal = {REVIEW OF ECONOMICS AND POLITICAL SCIENCE},
