@article{ WOS:000789674000016,
Author = {Chambers, Dustin and O'Reilly, Colin},
Title = {Regulation and income inequality in the United States},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2022},
Volume = {72},
Month = {MAR},
Abstract = {Income inequality in the United States has risen over the past several
   decades. Over the same period, federal regulatory restrictions have
   increased. An emerging literature shows that regu-lations can have
   regressive effects on the distribution of income, exacerbating
   inequality. The Federal Regulation and State Enterprise (FRASE) index
   quantifies the regulatory restrictions that apply to each US state by
   industrial composition. We construct a panel of 50 US states from 1997
   to 2015 to test whether states exposed to more federal regulatory
   restrictions have higher levels of income inequality. The results
   indicate that a 10 percent increase in federal regulation is asso-ciated
   with an approximate 0.5 percent increase in income inequality as
   measured by the Gini coefficient. When states are rank-ordered by
   average Gini coefficient, a 0.5 percent increase in income inequality
   will typically result in a two-position decline in state ranking.},
