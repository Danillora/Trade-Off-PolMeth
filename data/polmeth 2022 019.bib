@article{ WOS:000907694400001,
Author = {Larsen, Jeppe Fuglsang and Jensen, Sune Qvotrup},
Title = {The New Far Right, Subcultural Theory, and the Sociology of Emotions},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2025},
Volume = {48},
Number = {10},
Pages = {1079-1099},
Month = {OCT 3},
Abstract = {This article argues that a theoretical synthesis between subcultural
   theory and the sociology of emotions can provide a way to grasp the
   esthetic and emotional appeal of the new far right. This is illustrated
   through a multimodal analysis of fashwave music. We find that the
   central emotion conveyed by the music is a variation of restorative
   nostalgia accompanied by a sense of loss and anger. The analysis
   demonstrates the potential of the theoretical perspective and
   contributes empirically to studies of the new far-right movement.},
