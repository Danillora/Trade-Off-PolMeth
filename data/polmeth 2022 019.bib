@article{ WOS:000880563700001,
Author = {Molloy, Sean},
Title = {Hierarchy and status: a response},
Journal = {INTERNATIONAL POLITICS},
Year = {2023},
Volume = {60},
Number = {1},
Pages = {236-244},
Month = {FEB},
Abstract = {Siba Grovogui's essays present an opportunity for us to think about the
   current war in Ukraine. The article recapitulates Grovogui's argument
   before using it as a platform to discuss the centrality of status and
   hierarchy to Russia's decision to pursue its `special military
   operation' in Ukraine. At the core of Putin's war policy is a resentment
   of Ukraine's effrontery and a perception of Western post-Cold War
   dismissal of Russia's status as a leading global power. At root, Putin
   is motivated by a fear of humiliation. The article concludes with a
   reflection on the Afro-idealist position outlined by Martin Kimani, the
   Kenyan Ambassador to the UN, who argues in favour of repsect for borders
   as a first step to transcending the cycles of violence that border
   disputes occasion in world politics.},
