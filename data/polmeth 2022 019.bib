@article{ WOS:000890751700001,
Author = {Orliange, Philippe},
Title = {International development cooperation as a global governance policy},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2022},
Volume = {65},
Number = {2},
Abstract = {The three major international texts adopted in 2015 - Addis Ababa Action
   Agenda on financing for development, 2030 Agenda for Sustainable
   Development and Paris Climate Agreement- place national public policies
   and actors at the heart of international development cooperation (IDC).
   By bringing IDC closer to national institutions, particularly
   development banks, and taking it further away from traditional foreign
   policy, this shift has challenged long-held IDC narratives and
   structures, both among developed and developing countries. It has also
   laid the foundations for a degree of structured and universal
   accountability within the international development cooperation system,
   setting the stage for a wider shift within global governance.},
