@article{ WOS:000789797100001,
Author = {Hadjigeorgiou, Nasia and Kapardis, Dina},
Title = {Police Cooperation in Cases of Unrecognised Secessions: The Joint
   Communications Room in Cyprus},
Journal = {ETHNOPOLITICS},
Year = {2023},
Volume = {22},
Number = {5},
Pages = {527-549},
Month = {OCT 20},
Abstract = {Since 1974 the Republic of Cyprus only exercises effective control in
   the south, while the unrecognised `Turkish Republic of Northern Cyprus'
   exercises effective control in the north of the island of Cyprus. An
   increasing realisation among the two sides that effective policing
   requires the collaboration of their respective law enforcement agencies,
   led to the creation of the Joint Communications Room (JCR). The article
   frames the JCR as an example of engagement without recognition and
   assesses its effectiveness through an analysis of the cases it has been
   involved in between its creation in 2009 and 2018.},
