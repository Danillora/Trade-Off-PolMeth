@article{ WOS:000869173600001,
Author = {Guo, Wen},
Title = {Policy beliefs, policy learning, and risk perception: Exploring the
   formation of local creative Placemaking-catalyzed policy networkPalabras
   Clave(sic)(sic)(sic)},
Journal = {REVIEW OF POLICY RESEARCH},
