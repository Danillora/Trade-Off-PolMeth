@article{ WOS:000802093200001,
Author = {Chen, Wenna and Dong, Binzizi and Hsieh, Chih-Wei and Lee, M. Jin and
   Liu, Ning and Walker, Richard M. and Wang, Yao and Wen, Bo and Wen, Wen
   and Wu, Peiyi and Wu, Xia and Zhang, Jiasheng},
Title = {A replication of ``an experimental test of the
   expectancy-disconfirmation theory of citizen satisfaction{''}},
Journal = {PUBLIC ADMINISTRATION},
Year = {2022},
Volume = {100},
Number = {3, SI},
Pages = {778-791},
Month = {SEP},
Abstract = {To better understand citizen satisfaction with public services, public
   administration research has adopted the expectancy-disconfirmation model
   in recent years. This model proposes that satisfaction is a function of
   perceived performance and expectations. Recent quantitative and
   experimental studies of the expectancy-disconfirmation model have
   supported the framework. However, few replications have been conducted
   and none outside western contexts. We conducted two narrow, robust
   experimental replications of Van Ryzin (2013, Journal of Policy Analysis
   and Management, 32(3), pp. 597-614) in the Chinese cities of Hong Kong
   (in 2017) and Shenzhen (in 2021). We found support for the findings
   reported in Van Ryzin (2013) and concluded that the
   expectancy-disconfirmation model holds promise in a variety of settings
   as a framework for measuring citizen satisfaction with public services.},
