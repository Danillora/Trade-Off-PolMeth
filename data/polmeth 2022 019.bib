@article{ WOS:000826174300001,
Author = {Choi, Yongjin and Fox, Ashley M.},
Title = {Fact-value framework for adjudicating public health policy
   debates(sic)(sic)(sic)Palabras clave},
Journal = {REVIEW OF POLICY RESEARCH},
