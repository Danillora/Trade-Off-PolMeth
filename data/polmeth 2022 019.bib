@article{ WOS:000762456300001,
Author = {Viano, Cristina and Avanzo, Sowelu and Cerutti, Monica and Cordero, Alex
   and Schifanella, Claudio and Boella, Guido},
Title = {Blockchain tools for socio-economic interactions in local communities},
