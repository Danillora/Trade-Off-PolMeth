@article{ WOS:000761202600001,
Author = {Rodriguez, Cristian G. and Gadarian, Shana Kushner and Goodman, Sara
   Wallace and Pepinsky, Thomas B.},
Title = {Morbid Polarization: Exposure to COVID-19 and Partisan Disagreement
   about Pandemic Response},
Journal = {POLITICAL PSYCHOLOGY},
