@article{ WOS:000850139600001,
Author = {Klosek, Kamil C. and Souleimanov, Emil A.},
Title = {One or Many? Disentangling the Puzzle of Pro-Government Militia
   Deployment},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2025},
Volume = {48},
Number = {1},
Pages = {67-91},
Month = {JAN 2},
Abstract = {Why do some countries harbor pro-government militias (PGMs), while
   others do not? We assert that the deployment of PGMs depends on
   topographic, social, and political structures within which governments
   and rebels operate. Drawing on the concept of opportunity structures, we
   postulate that structural conditions within which governments are
   embedded in constitute a contributing factor to the existence and
   multiplicity of PGMs. Data from the Pro-Government Militia Dataset along
   with a two-stage hurdle model reveal that personalist regime type and
   civil wars increase the likelihood of PGM emergence. In contrast, ethnic
   fractionalization, onshore oil fields, drug production, number of rebel
   groups, and military prowess influence the number of PGMs.},
