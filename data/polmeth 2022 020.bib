@article{ WOS:000923471100003,
Author = {Fjaestad, Kristin and Naterstad, Tora Berge},
Title = {What Now, Russologists?},
Journal = {INTERNASJONAL POLITIKK},
Year = {2022},
Volume = {80},
Number = {4},
Pages = {431-440},
Abstract = {Russia's war against Ukraine has enormous consequences. First and
   foremost for Ukraine, but also for Russia and its neighboring states.
   The war has not only changed European and Norwegian security and foreign
   policies, it will also have a significant impact on Norwegian research
   on and knowledge about Russia. The opportunities for doing research in
   Russia have become more limited in recent years. After February 2022, it
   has become impossible. At the same time, knowledge about Russia is
   important for Norway, which shares a border and administers critical
   resources in cooperation with Russia. This will continue to be the case.
   The question now is how this knowledge is to be created given that the
   framework conditions under which Norwegian research on Russia has been
   produced during the last 30 years have dramatically changed. How are we
   going to update Norwegian knowledge about Russia in the coming years?
   What methods and data are available, and what can we expect from these?},
