@article{ WOS:000842245700001,
Author = {Horiuchi, Yusaku and Ono, Yoshikuni},
Title = {Susceptibility to threatening information and attitudes toward refugee
   resettlement: The case of Japan},
Journal = {JOURNAL OF PEACE RESEARCH},
