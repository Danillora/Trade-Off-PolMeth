@article{ WOS:000842274700001,
Author = {Ugur-Cinar, Meral and Sensonmez, Gokhan},
Title = {Making Sense of Senseless Times:Religious Narratives and Identity in the
   Memoirs of Far-right Militants in Turkey},
Journal = {POLITICS RELIGION \& IDEOLOGY},
Year = {2022},
Volume = {23},
Number = {3},
Pages = {327-348},
Month = {JUL 3},
Abstract = {This article traces the autobiographical narratives of the far-right
   Ulkucu militants in Turkey, based on memoirs written in the aftermath of
   the 1980 coup. The Ulkucus, who had fought on the streets against the
   leftist during the 1970s in the name of the state and nation,
   experienced a great rupture in their lives following their torturous
   treatment at the hands of the junta. This article examines the ways in
