@article{ WOS:000855822400021,
Author = {Braunstein, Juergen},
Title = {Crisis and state investment funds with expiration dates: Risks and
   opportunities in a decarbonization context},
Journal = {GLOBAL POLICY},
Year = {2022},
Volume = {13},
Number = {4},
Pages = {579-584},
Month = {SEP},
Abstract = {Since the beginning of the COVID-19 crises the establishment of new
   state-owned funds with expiration dates has accelerated. This new type
   of fund acquires shares or silent partnerships, or it would take over
   other components of a company's equity. Through recapitalization, the
   state funds' assets grow when the injection of capital is needed and the
   funds shrinks during periods of economic recovery. In other words, the
   expiration date ensures the eventual transfer of equity stakes to
   private ownership. While this is a sensible choice for avoiding generous
   public handouts for ailing firms some unintended long-term risks remain.
   Many debt and equity investments have been directed into sectors such as
   the automotive, energy, or aviation industries. These sectors will face
   increasing pressure in the future due to structural and policy-induced
   changes (e.g., decarbonization efforts to reduce climate change).},
