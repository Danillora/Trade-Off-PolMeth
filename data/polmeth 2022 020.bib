@article{ WOS:000764133100001,
Author = {Destradi, Sandra and Plagemann, Johannes and Tas, Hakki},
Title = {Populism and the politicisation of foreign policy},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
