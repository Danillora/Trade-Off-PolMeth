@incollection{ WOS:000977218700006,
Author = {Korolev, Alexander},
Book-Author = {Korolev, A},
Title = {Comparative Mapping: US-India and China-Russia Alignments},
Booktitle = {CHINA-RUSSIA STRATEGIC ALIGNMENT IN INTERNATIONAL POLITICS},
Year = {2022},
Pages = {157-188},
Abstract = {To better understand the extent of China-Russia alignment, it is useful
   to assess it in relative terms. This chapter applies the alignment
   criteria developed and tested in the previous chapters to US-India
   alignment. US-India alignment is a new development in the post-Cold War
   international politics formed in the context of rising China and
   believed to share certain features with China-Russia post-Cold War
   alignment. This makes it a useful reference point for assessing the
   relative depth of China-Russia strategic cooperation. The goal of the
   comparison is twofold. First, it aims to see how China-Russia alignment
   fares vis-a-vis US-India alignment and where it is ahead or behind. The
   second goal is to execute a plausibility probe for the developed
   framework by applying it to a new case of strategic cooperation.},
Publisher = {AMSTERDAM UNIV PRESS},
Address = {PRINSENGRACHI 747-51, AMSTERDAM 1071, NETHERLANDS},
Type = {Article; Book Chapter},
