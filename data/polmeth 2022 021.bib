@article{ WOS:000944245900007,
Author = {Ko, Ernie},
Title = {Leadership and anti-corruption reforms in Taiwan: a comparison of Chen
   Shui-bian and Ma Ying-jeou},
Journal = {PUBLIC ADMINISTRATION AND POLICY-AN ASIA-PACIFIC JOURNAL},
Year = {2022},
