@article{ WOS:000766946500001,
Author = {You, Jongeun and Heikkila, Tanya and Weible, Christopher M. and Kim,
   Serena and Park, Kyudong and Yordy, Jill and Smolinski, Sharon L.},
Title = {The distribution of conflict and attention across energy infrastructure},
Journal = {PUBLIC ADMINISTRATION},
Year = {2023},
Volume = {101},
Number = {3},
Pages = {1033-1054},
Month = {SEP},
Abstract = {Across the world, public administration and policy decisions are related
   to diverse levels of conflict and attention. However, the degree and
   variance of conflict and attention remain largely unspecified. This
   article examines how types of energy infrastructure and characteristics
   of project location are associated with the distribution of conflict and
   attention around the energy infrastructure siting process. Our empirical
   focus is on gas pipelines, electricity transmission lines, solar power
   projects, and wind power projects across the United States in 2018.
   Primarily relying on regression analysis and interviews, this article
   finds differences in the distribution of conflict and attention
   intensity within and between these energy infrastructure types, with gas
   pipelines and wind power projects presenting relatively higher conflict
   and attention intensities. However, conflict and attention are skewed to
   low intensities across infrastructure types. Characteristics of project
   locations that are positively associated with high conflict and
   attention intensity include the proportion of Democratic voters and the
   level of urbanization in the places where projects are sited. In
   contrast, the proportion of Black or Hispanic residents is negatively
   associated with high conflict and attention intensity.},
