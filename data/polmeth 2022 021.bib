@article{ WOS:000826569800001,
Author = {Godechot, Olivier and Neumann, Nils and Apascaritei, Paula and Boza,
   Istvan and Hallsten, Martin and Henriksen, Lasse and Hermansen, Are and
   Hou, Feng and Jung, Jiwook and Kodama, Naomi and Krizkova, Alena and
   Lippenyi, Zoltan and Marta, Elvira and Melzer, Silvia Maja and Mun,
   Eunmi and Sabanci, Halil and Soener, Matthew and Thaning, Max},
Title = {Ups and downs in finance, ups without downs in inequality},
Journal = {SOCIO-ECONOMIC REVIEW},
Year = {2023},
Volume = {21},
Number = {3},
Pages = {1601-1627},
Month = {JUL 26},
Abstract = {The upswing in finance in recent decades has led to rising inequality,
   but do downswings in finance lead to a symmetric decline in inequality?
   We analyze the asymmetry of the effect of ups and downs in finance, and
   the effect of increased capital requirements and the bonus cap on
   national earnings inequality. We use administrative
   employer-employee-linked data from 1990 to 2019 for 12 countries and
   data from bank reports, from 2009 to 2017 in 13 European countries. We
   find a strong asymmetry in the effect of upswings and downswings in
   finance on earnings inequality, a weak, if any, mitigating effect of
   capital requirements on finance's contribution to inequality, and a
   restructuring but no absolute effect of the bonus cap on financiers'
   earnings. We suggest that while rising financiers' wages increase
   inequality in upswings, they are resilient in downswings and thus
   downswings do not contribute to a symmetric decline in inequality.},
