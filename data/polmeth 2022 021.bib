@article{ WOS:000994950800001,
Author = {Vestergren, Sara and Uysal, Mete Sefa and Tekin, Selin},
Title = {Do disasters trigger protests? A conceptual view of the connection
   between disasters, injustice, and protests-The case of COVID-19},
Journal = {FRONTIERS IN POLITICAL SCIENCE},
Year = {2022},
Volume = {4},
Month = {AUG 9},
Abstract = {People around the globe are affected by disasters far beyond the
   disaster properties. Given that certain social groups are affected
   disproportionately, disasters need to be considered as political events
   which may cause political actions. Therefore, we aim to discuss, from a
   social psychological perspective, how and why protests might occur
   during or after a disaster. We argue for an elaborated model of
   collective action participation suggesting that disasters enhance the
   predictors of protest mobilization and participation though emerged or
   enhanced social injustice. We also suggest that disaster properties can
   be used to delegitimise protests and social movements, limiting the
   mobilization and collective resilience during and after a disaster.
   Finally, we discuss the gaps in current research and emphasize the need
   for more attention to the disaster-protest link as we can expect more
   disasters due to climate crisis, likely to lead to more protests and
   political collective action.},
Publisher = {FRONTIERS MEDIA SA},
Address = {AVENUE DU TRIBUNAL FEDERAL 34, LAUSANNE, CH-1015, SWITZERLAND},
Type = {Article},
Language = {English},
Affiliation = {Uysal, MS (Corresponding Author), Friedrich Schiller Univ Jena, Inst Psychol, Fac Social \& Behav Sci, Jena, Germany.
   Vestergren, Sara, Keele Univ, Sch Psychol, Newcastle Upon Tyne, England.
   Uysal, Mete Sefa, Friedrich Schiller Univ Jena, Inst Psychol, Fac Social \& Behav Sci, Jena, Germany.
