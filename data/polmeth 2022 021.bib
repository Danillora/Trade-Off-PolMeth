@article{ WOS:000825041600001,
Author = {Negro, Gianluigi},
Title = {China's Perspective on Internet Governance: a more Integrated Role in
   the Global Discussion?},
Journal = {JOURNAL OF CHINESE POLITICAL SCIENCE},
Year = {2023},
