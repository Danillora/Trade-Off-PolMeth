@article{ WOS:000782995200001,
Author = {van Prooijen, Jan-Willem and Rosema, Sabine and Chemke-Dreyfus, Axel and
   Trikaliti, Konstantina and Hormigo, Rita},
Title = {Make It Great Again: The Relationship Between Populist Attitudes and
   Nostalgia},
Journal = {POLITICAL PSYCHOLOGY},
Year = {2022},
Volume = {43},
