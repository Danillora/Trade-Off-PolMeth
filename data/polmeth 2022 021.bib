@article{ WOS:000857958600001,
Author = {Ornston, Darius and Camargo, Lorena},
Title = {The large firm dilemma: anchor embeddedness and high-technology
   competition},
Journal = {SOCIO-ECONOMIC REVIEW},
Year = {2024},
Volume = {22},
Number = {1},
Pages = {61-80},
Month = {APR 10},
Abstract = {Do large firms support or suppress regional entrepreneurship? Comparing
   Ottawa and Waterloo, two entrepreneurial ecosystems dominated by large,
   flagship firms, Nortel and Research in Motion (RIM), we demonstrate how
   an anchor firm's relationship to the entrepreneurial community shapes
   entrepreneurial activity. In Ottawa, an engaged anchor delivered the
   public goods to overcome the negative externalities associated with
   large firm growth. In the long run, however, this vibrant, startup scene
   proved surprisingly vulnerable to Nortel's decline. In Waterloo, in
   contrast, new firm formation plummeted under a detached anchor. RIM's
   isolation, however, enabled Waterloo to construct the independent,
   entrepreneurial infrastructure it needed to capitalize on its collapse.
   Illuminating the mechanisms behind `entrepreneurial recycling', or the
   reallocation of labor from declining anchor firms, we argue that regions
   confront a strategic dilemma in how they engage large firms.},
Publisher = {OXFORD UNIV PRESS},
Address = {GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Ornston, D (Corresponding Author), Univ Toronto, Munk Sch Global Affairs \& Publ Policy, Toronto, ON M5S 3K7, Canada.
