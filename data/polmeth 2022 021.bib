@article{ WOS:000834871800001,
Author = {Golub, Grant},
Title = {The Eagle and the Lion: Reassessing Anglo-American strategic planning
   and the foundations of US grand strategy for World War II},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2023},
Volume = {46},
Number = {5},
Pages = {921-948},
Month = {JUL 29},
Abstract = {Many accounts of the formation of American and British grand strategy
   during World War II between the fall of France and the Pearl Harbor
   attacks stress the differences between the two sides' strategic
   thinking. These accounts argue that while the Americans favored a
   `direct' Germany-first approach to defeating the Axis powers, the
   British preferred the `indirect' or `peripheral' method. However, a
   review of Anglo-American strategic planning in this period shows that
   before official U.S. wartime entry, both sides largely agreed the
   British `peripheral' approach was the wisest grand strategy for winning
   the war.},
