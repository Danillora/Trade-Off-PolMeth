@article{ WOS:000820487200001,
Author = {Medeiros, Marcelo de Almeida and de Brito Carvalho, Lucas Jose and de
   Oliveira Rocha, Felipe Ferreira},
Title = {Subnational policies and migration in the USA: post-national citizenship
   or sovereignty?},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2022},
Volume = {65},
Number = {1},
Abstract = {This paper aims to: (i) investigate whether Republican legislatures were
   more inclined than Democratic ones to uphold laws of national
   sovereignty and whether Democratic legislatures were more likely than
   Republican ones to prioritise bills linked to the post-national
   perspective; (ii) assess the US migration laws by considering how
   state-level legal systems affect migrants' lives. Using data from the
   National Conference of State Legislatures and Legiscan and techniques
   like clustering, statistical, and geospatial analysis, we found that
   bills sponsored by the Democratic Party are indeed more likely to belong
   to the post-national category than bills by the Republican Party.},
Publisher = {INST BRASILEIRO RELACOES INT},
Address = {CAIXA POSTAL 4400, BRASILIA DF, 70919-970, BRAZIL},
