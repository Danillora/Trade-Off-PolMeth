@article{ WOS:000823182700001,
Author = {Ridenti, Marcelo},
Title = {Brazilian Students in the United States: A Forgotten Chapter of the
   Cultural Cold War during the Rebel Years},
Journal = {LATIN AMERICAN PERSPECTIVES},
