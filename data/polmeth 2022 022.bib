@article{ WOS:000763180600001,
Author = {Wuthnow, Joel and Fravel, M. Taylor},
Title = {China's military strategy for a `new era': Some change, more continuity,
   and tantalizing hints},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2023},
Volume = {46},
Number = {6-7},
Pages = {1149-1184},
Month = {NOV 10},
Abstract = {In 2019, China's Central Military Commission adopted a new strategy for
   the People's Liberation Army, titled the `military strategic guidelines
   for the new era.' This was consistent with the past but framed by Xi's
   political consolidation, growing threats from the United States and
   Taiwan, and a new military structure. This article documents the
   strategy and asks what would drive a more fundamental adjustment. It
   concludes that the strategy reflected a determination to focus the PLA
   on the necessary and the achievable, but a new direction could be
   influenced by changes in the strategic landscape, rapid modernization,
   or new operational concepts.},
