@article{ WOS:000745460900001,
Author = {Zhu, Xiao and Qiu, Taotao and Liu, Dongshu},
Title = {Resisting public monitoring in authoritarian regimes: Evidence from
   local environmental litigation in China},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2023},
Volume = {36},
Number = {2},
Pages = {459-477},
Month = {APR},
Abstract = {Civic activism is increasingly popular for authoritarian regimes to
   ensure local compliance and improve local governance, but it also places
   pressure on local officials. How do local officials respond to such
   pressures? With a unique dataset of city-level environmental
   investigations and public interest litigation in China, we conduct a
   generalized difference-in-difference analysis and find that public
   interest litigation, a new legal channel for social actors to monitor
   environmental governance, can significantly reduce information
   disclosure related to investigations. Furthermore, it changes the focus
   of investigations: the number of low-profile investigations is
   increased, while the number of high-profile cases is decreased. This
   finding reveals how local officials avoid public monitoring by reducing
   information disclosure but maintaining a certain level of enforcement,
   which implies that the effect of civic engagement on the accountability
   of local governments in authoritarian regimes is dynamic with the
   coevolution of strategies of local officials and civil societies.},
