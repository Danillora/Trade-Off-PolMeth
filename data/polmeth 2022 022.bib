@article{ WOS:000844035800001,
Author = {Zamchiya, Phillan},
Title = {Mining, capital and dispossession in post-apartheid South Africa},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
Year = {2022},
Volume = {49},
Number = {173},
Pages = {417-435},
Month = {JUL 3},
Abstract = {Some Marxist political economists use accumulation by dispossession to
   explain processes in which natural resources are enclosed and their
   users dispossessed through extra-economic means. However, accumulation
   by dispossession takes an overly omnibus and materialistic approach in
   trying to cover a wide range of global processes. This article therefore
   distils accumulation by dispossession's three central features of
