@article{ WOS:000858312200003,
Author = {Gellwitzki, C. Nicolai L.},
Title = {Stimmung and ontological security: anxiety, euphoria, and
   emerging political subjectivities during the 2015 `border opening' in
   Germany},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2022},
Volume = {25},
Number = {4, SI},
Pages = {1101-1125},
Month = {DEC},
Abstract = {This article draws on Heidegger's notion of Stimmung (mood, attunement,
   atmosphere) to further develop the study of public moods in IR. To that
   end, it synthesises two recent developments in ontological security
   studies (OSS), the decentred Deleuzian approach that emphasises the role
   of affective environments in subjects' sense of and search for
   ontological security and Heideggerian readings of anxiety as (public)
   mood. The developed framework maintains OSS' conceptual focus on anxiety
   whilst centring the locus of analysis around dynamic affective
   environments rather than individual subjects. This framework allows for
   exploring the relationship between anxiety and the radical agency,
   emerging political subjectivities, and intense (positive) moods it can
   facilitate. The empirical added value of this framework is illustrated
   through an analysis of the public mood of anxiety that preceded and
   enabled the ``border opening{''} in Germany during the so-called
   migration crisis and the subsequent euphoria it engendered.},
