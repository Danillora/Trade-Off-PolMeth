@article{ WOS:000862618900001,
Author = {Lubina, Michal and Kozlowska, Magdalena},
Title = {Unwillingly Orientalist: Gustaw Herling-Grudzinski's Travel Diary to
   Burma as an Example of an Unusual Postcolonial Eastern European Account},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
Year = {2023},
Volume = {37},
Number = {3},
Pages = {789-812},
Month = {AUG},
Abstract = {This article explores Gustaw Herling-Grudzinski's largely forgotten
   travel diary ``A Journey to Burma.{''} It argues that the strategy
   Herling-Grudzinski employed to describe the postcolonial world was a
   unique one. It did not belong to the ``socialist postcolonialism{''}
   narrative, nor was it a part of Western orientalist discourse.
   Herling-Grudzinski's Eastern European past and identity deeply
   influenced his account. Despite some simplification, he was able to
   summarize Burmese reality surprisingly accurately. The article claims
   that Herling-Grudzinski's travelogue represents a new angle to the
   narrative of the relations between Eastern European people of letters
   and inhabitants of colonized countries.},
