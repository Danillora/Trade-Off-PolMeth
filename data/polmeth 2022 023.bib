@article{ WOS:000919843500001,
Author = {Achilleos-Sarll, Columba and Thomson, Jennifer and Haastrup, Toni and
   Farber, Karoline and Cohn, Carol and Kirby, Paul},
Title = {The Past, Present, and Future(s) of Feminist Foreign Policy},
Journal = {INTERNATIONAL STUDIES REVIEW},
