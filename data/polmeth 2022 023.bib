@article{ WOS:000896566900001,
Author = {Sandvik, Kristin Bergtora and Bjorhaug, Ingunn and Espegren, Astrid and
   Garnier, Adele},
Title = {Protecting skilled Afghan women: Brain save and the politics of
   vulnerability},
Journal = {GLOBAL POLICY},
Year = {2023},
Volume = {14},
Number = {1},
Pages = {5-15},
Month = {FEB},
Abstract = {Considering the Afghan evacuation of 2021 and its aftermath, this
   article suggests the term `brain save' to characterise emerging
   protection discourses and practices concerning the resettlement of
   skilled women refugees. Resettlement has traditionally focused on women
   as vulnerable because of their gender. Drawing on examples of the
   evacuation and prospective resettlement of Afghan women professionals,
   the article develops the analytic concept of brain save to label these
   discourses and practices. Unlike `brain drain', brain save challenges
   established politics of vulnerability and has progressive potential for
   resettlement as a durable solution. However, it also implies problematic
   prioritisation of particular resettlement candidates.},
