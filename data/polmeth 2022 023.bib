@article{ WOS:000771822500001,
Author = {Mensah, Isaac},
Title = {The African Union's Policy Frameworks, Institutional Mechanisms and
   Challenges in Countering Terrorism in Africa},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2023},
