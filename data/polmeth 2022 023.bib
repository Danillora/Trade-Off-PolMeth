@article{ WOS:000800853400001,
Author = {Erlich, Aaron and Garner, Calvin and Pennycook, Gordon and Rand, David
   G.},
Title = {Does Analytic Thinking Insulate Against Pro-Kremlin Disinformation?
   Evidence From Ukraine},
Journal = {POLITICAL PSYCHOLOGY},
