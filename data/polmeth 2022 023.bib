@article{ WOS:000889244200001,
Author = {Deng, Jianwei and Hao, Xueting and Yang, Tianan},
Title = {The increase of counterproductive work behaviour from organizational and
   individual level due to workplace conflict: a sequential moderated
   mediation model},
