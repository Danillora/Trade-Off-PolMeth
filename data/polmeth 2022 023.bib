DOI = {10.1093/isr/viac010},
Article-Number = {viac010},
ISSN = {1521-9488},
EISSN = {1468-2486},
Keywords = {polarization; US foreign policy; two-level game; <bold>Palabras
   clave</bold>; polarizacion; politica exterior de EE; UU; juego a dos
