@article{ WOS:000868441500001,
Author = {Rauh, Christian},
Title = {Clear messages to the European public? The language of European
   Commission press releases 1985-2020},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2023},
Volume = {45},
Number = {4},
Pages = {683-701},
Month = {MAY 19},
Abstract = {The public politicisation of European integration indicates a growing
   demand for public communication of supranational politics. This paper
   highlights that the messages the European Commission sends to its
   citizens do not meet this demand. A text analysis of almost 45,000 press
   releases the Commission has issued during 35 years of European
   integration rather indicates an extremely technocratic style of
   communication. Benchmarked against large samples of national executive
   communication, public political media, and scientific discourse, the
   Commission used and notably continues to use very complex language,
   specialized jargon, and a nominal style that obfuscates political
   action. This appears disadvantageous in a politicized context and more
   research on the reasons for this apparent communication deficit is
   needed.},
