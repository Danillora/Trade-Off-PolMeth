@article{ WOS:000883180800005,
Author = {Alim, Mastura and Due, Clemence and Strelan, Peter},
Title = {Understanding and Restoring Justice: A Qualitative Study of Uyghurs in
   Australia},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2022},
Volume = {44},
Number = {3},
Pages = {592-611},
Month = {AUG},
Abstract = {An abundance of research has sought to understand how people seek to
   restore justice in the face of injustices. However, there is little
   understanding of how people with refugee and migrant backgrounds respond
   to systemic injustice and how this may shape their understandings of
   justice. This study qualitatively explored the meaning of justice and
   the pathways to restore justice for Uyghur people resettled in
   Australia. Twenty-seven interviews were conducted and analyzed
   thematically. The results highlight that justice is understood as equal
   rights, truth, and freedom. Restoring justice included acknowledging
   injustices, right of return, safety, self-determination, and political
   intervention.},
