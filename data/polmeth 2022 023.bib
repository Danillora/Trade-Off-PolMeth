@article{ WOS:000765721900001,
Author = {Hywel Nicholls, Daniel},
Title = {Roles and locations in relational hierarchies: imperial role-authority
   order in Cold War Latin America},
Journal = {INTERNATIONAL POLITICS},
Year = {2023},
