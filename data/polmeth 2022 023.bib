@article{ WOS:000769078800001,
Author = {Avram, Silvia},
Title = {Unstable jobs and time out of work: evidence from the UK},
Journal = {SOCIO-ECONOMIC REVIEW},
Year = {2022},
Volume = {20},
Number = {3},
Pages = {1151-1171},
Month = {SEP 29},
Abstract = {This article tests the hypothesis that unstable jobs with variable hours
   or pay enhance the job-finding chances of the working-age non-employed
   in the UK, by using a combination of the UK Household Longitudinal Study
   and the Labour Force Survey data and a discrete time model. We find no
   evidence on the share of unstable jobs in the non-employed person's
   local labour market impacts on the probability to move into employment.
   This result holds both for men and women and for groups with low
   employability such as the low educated and the long-term unemployed. It
   is robust to alternative ways of defining unstable jobs and to the
   inclusion of unobserved heterogeneity. Overall, findings cast doubt on
   the importance of unstable jobs for employment creation in the UK.},
