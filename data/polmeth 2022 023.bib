@article{ WOS:000827033900001,
Author = {Rosset, Jan},
Title = {Perceptions of (unequal) responsiveness and their consequences for
   electoral participation},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2023},
Volume = {30},
Number = {8},
Pages = {1565-1587},
Month = {AUG 3},
Abstract = {Research on a variety of democracies has shown that there is a link
   between mass preferences and public policy. It has also been documented
   that public policy tends to be more responsive to the preferences of
   affluent citizens. We know little, however, about citizens' perceptions
   of policy responsiveness and their consequences for electoral
   participation. This manuscript addresses this issue by analysing
   citizens' perceptions of policy responsiveness and of unequal policy
   responsiveness, the extent to which these perceptions vary by income, as
   well as the effect of those perceptions on turnout across various income
   groups. Using data from an original survey conducted in 13 western
   democracies in 2019, the analyses reveal that most citizens from all
   income groups are skeptical about their influence on policy and perceive
   a bias in policy making that favors the rich. These perceptions are
   associated with lower levels of participation, especially among the
   low-income groups.},
