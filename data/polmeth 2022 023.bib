@article{ WOS:000888053500020,
Author = {Waldoch, Jacek},
Title = {Legal and Political Methods of Weakening the Local Government in the
   Second Polish Republic After 1926},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2022},
Volume = {20},
Number = {4},
Pages = {1191-1208},
Month = {OCT},
Abstract = {The subject of the article is the issue of strengthening the position of
   the government administration in relation to local government in the
   Second Polish Republic. The author presented the evolution of the legal
   basis for the local self-government system, starting with the
   constitution of 1921, in which there was a mention of ``extensive local
   self-government{''}. Subsequent acts on this subject come from the
   period after 1926, when a party supporting Jozef Pilsudski came to power
   as a result of a coup d'etat. The crowning moment of the systemic
   changes was the constitution of 1935, in which the issue of local
   self-government was almost omitted. The article is enriched with
   opinions of representatives of the legal doctrine.},
