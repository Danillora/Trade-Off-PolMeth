@article{ WOS:000768786200010,
Author = {Brajnik, Irena Baclija and Prebilic, Vladimir and Kronegger, Luka},
Title = {Explaining Strategies in Setting Own Local Taxes in Slovenia},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2022},
Volume = {20},
Number = {1},
Pages = {239-257},
Month = {JAN},
Abstract = {There are numerous ways for localities to exercise tax powers. However,
   not all localities utilize their taxing powers. Taxation strategies are
   numerous and are influenced by the size of local government, the
   percentage of taxes that the local government takes, share and the local
   government's autonomy over taxation. Local governments in Slovenia have
   some tax autonomy, but local taxes only represent about 13 percent of
   municipal total tax revenue. This article reviews taxation by local
   governments in Slovenia as well as some potential reasons for their
   under-taxing. We can conclude that local governments in Slovenia use
   local taxes as a fiscal measure and that share of (all) own taxes
   positively correlate to the size of the municipality. Small
   municipalities tend to under-tax (or avoid taxing completely), and since
   equalisation mechanisms do not reward taxation inactivity, the most
   probable explanation for this practice is closeness to the electorate.},
