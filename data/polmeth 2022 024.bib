@article{ WOS:000846796300001,
Author = {Shkolnik, Michael},
Title = {``The Road to Jerusalem Goes through Cairo{''}: Exploring Israel's
   Counter-Militancy Efforts against Sinai Peninsula-Based Threats},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2022},
Month = {2022 AUG 24},
Abstract = {This article explores Sinai Peninsula-based militant threats to Israeli
   national security and Israel's efforts to address these threats. Though
   Israel primarily relies on coercive actions like targeted killings and
   airstrikes in other hostile fronts, Israel prioritizes defensive
   measures (i.e. barrier construction, missile defence systems) and
   intelligence cooperation with Egypt to counter a mutual enemy. Sinai
   attacks targeting Israel, which fluctuate based on organizational
   objectives and capabilities, aim to provoke Israeli retaliation and
   disrupt bilateral ties with Egypt - a cornerstone of regional interstate
   stability. However, Israel's pragmatic counterterrorism measures have
   helped contain cross-border threats and strengthen Egypt-Israel
   relations to new heights. This case sheds light on how other strong
   states could face militant threats from territory of a neighboring
   partner while pursuing a delicate balance to preserve bilateral ties and
   ensure national security.},
