@article{ WOS:000786389400001,
Author = {Celik, Feyyaz Baris},
Title = {The EU's different faces in climate diplomacy: leadership, interests,
   and responsibilities},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2022},
Volume = {44},
Number = {8},
Pages = {1019-1039},
Month = {NOV 17},
Abstract = {Climate change politics is a key area where the EU demonstrates an
   ambition for global leadership. Yet, analysts and practitioners point to
   significant disconnections between the EU's discourse and practice in
   this area. In what particular ways do these disconnections occur? This
   article uses the concepts of `organised hypocrisy' and `organisational
   facades' to address this question. Offering a framework to analyse the
   EU's talk, decisions, and actions in climate diplomacy, these two
   concepts provide a nuanced understanding of the way in which the Union
   handles conflicting demands from strategic actors in global climate
   politics. This article argues that engaging in hypocrisy by portraying
   different facets of the Union has been a primary way in which the EU has
   managed to adapt its negotiation positions in climate diplomacy by
   addressing conflicting external conditions.},
