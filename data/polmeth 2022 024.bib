@article{ WOS:000898236900001,
Author = {Hitman, Gadi},
Title = {The Formation of the National Palestinian Ethos},
Journal = {ETHNOPOLITICS},
Year = {2024},
Volume = {23},
Number = {3},
Pages = {254-272},
Month = {MAY 26},
Abstract = {This study seeks to examine how the Palestinian national ethos was
   formed based on both primordialism and territorial perceptions. It is
   based on primary sources-memoirs, public statements, and Palestinian
   media releases-dating back to the British Mandate for Palestine and
   examines the use of the basic elements of the ethos: victimhood, victory
   at all costs, and universal justice. The paper discerns two stages in
   the process of ethos formation: the first started in the 1910s and ended
   when the British Mandate was over. The second began after the civil war
   of 1947-1949 in Palestine. The loss of the Arab-Palestinian side and the
   consequences of the Nakba has not terminated yet. While the first stage
   focuses on injustice, the second phase includes all three features of
   the national ethos.},
