@article{ WOS:000791961600001,
Author = {Henriksen, Lasse Folke and Seabrooke, Leonard and Young, Kevin L.},
Title = {Intellectual rivalry in American economics: intergenerational social
   cohesion and the rise of the Chicago school},
Journal = {SOCIO-ECONOMIC REVIEW},
