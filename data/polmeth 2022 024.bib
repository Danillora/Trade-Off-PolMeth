@article{ WOS:000782867700001,
Author = {Drulak, Petr},
Title = {Technocracy that fails: a Czech perspective on the EU},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2022},
Volume = {25},
Number = {3},
Pages = {739-760},
Month = {SEP},
Abstract = {The migration crisis of 2015/2016 deepened a major fault line inside the
   EU, the one between its Western part and its new members from Central
   and Eastern Europe. Analysing the case of Czechia this article points to
   the role of politicisation and technocracy in this split. It is argued
   here that technocratic ideas and practices are deeply rooted in the
   Czech public life and that a technocratic symbiosis existed between the
   EU and Czechia. However, this symbiosis was disturbed by the way the
   European Commission was politicised before and during the crisis while
   it clashed with Czech technocratic positions.},
