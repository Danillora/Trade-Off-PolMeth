@article{ WOS:000841530300001,
Author = {Harstad, Renate M. B. and Vik, Jostein},
Title = {Political parties' framing of farm animal welfare: A fragmented picture},
Journal = {EUROPEAN POLICY ANALYSIS},
Year = {2023},
Volume = {9},
Number = {1},
Pages = {30-47},
Month = {FEB},
Abstract = {In this article, we draw from qualitative interviews with political
   parties' representatives and a content analysis of party programs to
   identify how political parties frame animal welfare policies in Norway.
   In analyzing the framing of animal welfare and its significance for
   understanding agricultural post-exceptionalism, we found that though
   they frame animal welfare issues in conflicting ways, most see Norwegian
   animal welfare as reasonably good but perceive potential for
   improvement. They also generally understand societal claims as lacking
   or having an unclear factual basis, which has legitimized their
   nonaction as political parties. Even so, because of active new actors
   such as animal welfare organizations and retailers, the parties do not
   believe that public pressure will wane. Although introducing new farm
   animal welfare policies can represent a move towards post-exceptionalism
   in Europe's agri-food sector, the discourse on animal welfare policies
   amongst political parties implies that such is not the case in Norway.},
