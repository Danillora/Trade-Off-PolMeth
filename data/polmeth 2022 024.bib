@article{ WOS:000800839000001,
Author = {Alexandroff, Alan S.},
Title = {Director, Global Summitry Project, teaches at the Munk School of Global
   Affairs \& Public Policy, University of Toronto A Conclusion: Informal
   actors and their current impact on the global order},
Journal = {INTERNATIONAL POLITICS},
Year = {2022},
Month = {2022 MAY 22},
Abstract = {The focus of this conclusion is on the SI contributions. All the
   contributors focus on key international actors and their actions and
   their necessary interaction with a significant evolving order. The
   evolution and the interaction continues without pause. While there is
   some confusion between structure and behavior such that formal
   institutions may act more formally but also informally, all acknowledge
   that there is value in distinguishing formal from informal institutions.
   There is a great deal of valuable analysis in the contributions around
   these formal and informal institutions: what identifies them and how
   they operate. There is an effort as well to be comparative and also
   quantitative and qualitative. In the end structural analysis would be
   even further advanced by distinguishing regional from global and
   particularly leader-led from other informals. Finally, today's global
   order requires us to accept the influence of nonstate actors as well as
   substate actors. The order evolves.},
