@article{ WOS:000879526200001,
Author = {Faulkner, Christopher M. and Welsh, Blair},
Title = {Rebel Child Soldiering and Conflict-Related Sexual Violence},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
