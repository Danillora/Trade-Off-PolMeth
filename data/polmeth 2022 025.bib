@article{ WOS:000800928300001,
Author = {McBeth, Mark K. and Warnement Wrobel, Megan and van Woerden, Irene},
Title = {Political ideology and nuclear energy: Perception, proximity, and
   trust(sic)(sic)(sic)Palabras clave},
Journal = {REVIEW OF POLICY RESEARCH},
