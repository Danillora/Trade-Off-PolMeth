@article{ WOS:000858461900001,
Author = {Kreuder-Sonnen, Christian and Rittberger, Berthold},
Title = {The LIO's growing democracy gap: an endogenous source of polity
   contestation},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
