@article{ WOS:000871359200001,
Author = {Hague, Alice and Bomberg, Elizabeth},
Title = {Faith-based actors as climate intermediaries in Scottish climate policy},
Journal = {POLICY STUDIES},
Year = {2023},
Volume = {44},
