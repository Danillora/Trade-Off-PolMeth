@article{ WOS:000751704000001,
Author = {Bury, Patrick},
Title = {Post-Fordism and the Transformation of Transatlantic Counter-Terrorism},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2024},
Volume = {47},
Number = {10},
Pages = {1261-1285},
Month = {OCT 2},
Abstract = {There is growing evidence that, led by the U.S., a transformation of
   trans-Atlantic counter-terrorism (CT) has occurred since 9/11. However,
   a theoretically informed framework for understanding how this has
   happened remains elusive. Using post-Fordist industrial theory and new
   evidence collected from senior practitioners across the trans-Atlantic
   space, this article provides a novel conceptual framework for
   understanding how states have transformed CT organization since 9/11,
   primarily in response to global jihadism. It shows how many of these
   solutions are remarkably similar and based on post-Fordist business
   and/or military best practices, including centralization and
   decentralization; integration of core and periphery workforces; a
   network approach to knowledge sharing; and outsourcing. The conclusion
   discusses the effectiveness of these solutions and their use in meeting
   new policy challenges.},
