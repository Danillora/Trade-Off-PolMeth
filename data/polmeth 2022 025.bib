@article{ WOS:000815544500001,
Author = {Wayne, Carly and Zhukov, Yuri M.},
Title = {Never Again The Holocaust and Political Legacies of Genocide},
Journal = {WORLD POLITICS},
Year = {2022},
Volume = {74},
Number = {3},
Pages = {367-404},
Month = {JUL},
Abstract = {Do individuals previously targeted by genocide become more supportive of
   other victimized groups? How are these political lessons internalized
   and passed down across generations? To answer these questions, the
   authors leverage original survey data collected among Holocaust
   survivors in the United States and their descendants, Jews with no
   immediate family connection to the Holocaust, and non-Jewish Americans.
   They find that historical victimization is associated with increased
