@article{ WOS:000917537100001,
Author = {Saraiva, Miriam Gomes},
Title = {South America at the core of Brazilian foreign policy during Bolsonaro's
   administration (2019-2022)},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2022},
Volume = {65},
Number = {2},
Abstract = {The arrival of Jair Bolsonaro to the Brazilian presidency brought many
   changes to foreign policy. Based on new ideas in a new foreign
   policymaking format, several patterns of international behavior were
   questioned and replaced by new guidelines and actions that created
   friction with international partners. Brazil's behavior towards South
   America was one of the areas most impacted by this shift. This paper
   reflects upon Bolsonaro's foreign policy for the region, influenced by
   these changes and marked by disinterest on policymakers' part,
   highlighting how ideational factors underpinned behaviors, actors, and
   actions.},
Publisher = {INST BRASILEIRO RELACOES INT},
