@article{ WOS:000798776700001,
Author = {Braghiroli, Stefano and Makarychev, Andrey},
Title = {Conservative populism in Italy and Estonia: playing the multicultural
   card and engaging ``domestic others{''}},
Journal = {EAST EUROPEAN POLITICS},
Year = {2023},
Volume = {39},
Number = {1},
Pages = {128-149},
Month = {JAN 2},
Abstract = {U-turns by populist parties are not a new phenomenon. The 2021 electoral
   campaign in Estonia was marked by episodes that combined cultural
   hybridity and political opportunism. The nationalist Conservative
   People's Party of Estonia (EKRE) was reprimanded by the Language
   Inspectorate for using Russian-language campaign posters with no
   Estonian translation. The same party was celebrating Estonian
   independence with a concert performing Soviet-time popular music. These
   episodes appeared quite surprising in the Estonian context, but not
   unique in a wider European perspective. We tackle the following
   question: why and how national conservative parties appeal to groups
   previously treated as domestic others.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
