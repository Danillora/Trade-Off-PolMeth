@article{ WOS:000840126000001,
Author = {Lindvall, Johannes and Rogers, Katren},
Title = {The changing faces of the modern state},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2023},
Volume = {36},
Number = {3},
Pages = {973-993},
Month = {JUL},
Abstract = {This paper describes the changing nature of the English and Swedish
   states between 1800 and 2020 from the perspective of ordinary citizens.
   We identify 13 typical life situations and then describe the types of
   citizens who interacted with the state, the number and types of public
   officials with whom they interacted, and where those interactions took
   place. We distinguish among three typical social groups: the poor, the
   middle, and the rich. We find, among other things, that the poor became
   objects of government policy much earlier than other groups, but they
   also remained in a world of parochial poor relief even as the middle and
   the rich began to interact with a more functionally differentiated,
   professional bureaucracy. These findings have important implications for
   scholarly debates on when the state began to interact directly with the
   civilian masses and the unevenness of state activities and capacities
   within countries.},
Publisher = {WILEY},
