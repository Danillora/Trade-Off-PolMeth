@article{ WOS:000758598100001,
Author = {McManus, Cathal},
Title = {Political Interventions to `Ripen' Peace Initiatives: An Analysis of the
   Northern Ireland and Israeli/Palestinian Conflicts},
Journal = {ETHNOPOLITICS},
Year = {2023},
Volume = {22},
Number = {3},
Pages = {335-352},
Month = {MAY 27},
Abstract = {As President of the United States, Donald Trump's decision to recognise
   Jerusalem as the capital of Israel served to heighten tensions in the
   Middle East and instantly undermined his stated goal of bringing peace
   to the troubled region. This paper will argue that there are important
   lessons to be gained from the political process in Northern Ireland to
   help bolster a peace process in the Middle East. Through an analysis of
   Unionist politics pre- and post- the Anglo-Irish Agreement, the paper
   will argue that there is a need to not only encourage militants to make
   peace, but also to incentivise regional hegemons into meaningful
   negotiations. The paper will highlight the important role played by
   international hegemonic powers as they seek to create a Strategically
   Benefical Moment for all sides in the conflict as they seek to `ripen'
   the conditions for peace.},
