@article{ WOS:000759308700001,
Author = {Kumove, Michael},
Title = {Does Language Foster Reconciliation? Evidence From the Former Yugoslavia},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2022},
Volume = {66},
Number = {4-5},
Pages = {783-808},
Month = {MAY},
Abstract = {Despite a surge in research on post-conflict reconciliation, the
   specific factors which promote reconciliation remain a subject of
   debate. In particular, the possible role of shared language in fostering
   reconciliation has received little scholarly attention. This paper
   examines two possible channels through which shared language may
   facilitate reconciliation, and tests these using a new survey dataset of
   446 individuals from Serbia. As expected, the results indicated that
   shared language reduces the negative effect of conflict on intergroup
   trust and friendship, two crucial components of reconciliation.
   Furthermore, the results suggest that in the former Yugoslavia this
   effect is generated by the communication-enabling aspects of a shared
   language, rather than its other properties such as acting as a marker of
   ethnic or cultural identity.},
