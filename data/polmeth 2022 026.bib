@article{ WOS:000826174200001,
Author = {Otsuka, Kenji},
Title = {Co-optation in co-production: Maintaining credibility and legitimacy in
   transboundary environmental governance in East AsiaPalabras
   clave(sic)(sic)(sic)},
Journal = {REVIEW OF POLICY RESEARCH},
