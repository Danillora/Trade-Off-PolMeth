@article{ WOS:000771519400001,
Author = {Bussu, Sonia and Bua, Adrian and Dean, Rikki and Smith, Graham},
Title = {Embedding participatory governance},
Journal = {CRITICAL POLICY STUDIES},
Year = {2022},
