@article{ WOS:000761624800001,
Author = {Migone, Andrea and McGregor, Michael R. and Brock, Kathy and Howlett,
   Michael},
Title = {Super-users and hyper-experts in the provision of policy advice:
   Evidence from a survey of Canadian academics(sic)(sic)(sic)Palabras
   clave},
Journal = {EUROPEAN POLICY ANALYSIS},
Year = {2022},
Volume = {8},
Number = {4},
Pages = {370-393},
Month = {DEC},
Abstract = {The relationships of influence and activity between academics and other
   actors (public, private, and non-governmental) in the policy process are
   complex. Although older work often argued academic research at best had
   an indirect ``environmental{''} or ``enlightenment{''} effect on
   policy-makers, (May et al. (2016). Journal of Public Policy, 36, 195)
   recently argued that in the US case previous studies misconstrued the
   role of academic policy advice because they surveyed ``average{''}
   academics and in so doing missed the significant impact of a small elite
   group of ``hyper-experts{''} within an already small group of
   ``super-users{''} interacting on a constant basis with government
   policy-makers. This article draws upon data from a survey of academics
   in four fields (Business, Engineering, Health and Politics) in six major
   Canadian Universities to map out the relationships existing between
   academics and other actors in the public, private, and non-governmental
   sectors and test for the existence of this elite pattern of interaction
   in a second country.},
