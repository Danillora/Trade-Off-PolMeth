@article{ WOS:000745321300001,
Author = {Wullweber, Joscha},
Title = {Democracy, economic inequality, and mending},
Journal = {CRITICAL POLICY STUDIES},
Year = {2022},
Volume = {16},
Number = {2},
Pages = {229-232},
Month = {MAY 2},
Abstract = {The article argues that while the book brilliantly illustrates the many
   ways of democratic mending, there is an essential blind spot that runs
   through the entire book: economic inequality. Democratic processes seem
   strangely disconnected from the conditions under which people live.
   However, there can be no democratic equality and no just democracy as
   long as living conditions in society and among societies remain highly
   unequal. Accordingly, to unleash its potential the concept of mending
   should be enriched with an analysis and an understanding of the global
   political economy of inequality.},
