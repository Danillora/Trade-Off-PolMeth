@article{ WOS:000851457100001,
Author = {Yang, Wenhui and Zhao, Jing},
Title = {Institutions, interests, and policy support: Experimental evidence from
   China},
Journal = {PUBLIC ADMINISTRATION},
Year = {2023},
Volume = {101},
Number = {4},
Pages = {1309-1325},
Month = {DEC},
Abstract = {Policy support matters for the success of public policies. It is still
   unclear how governments can garner support for policies with high costs.
   Using a conjoint experiment in China, we demonstrate that governments
   can encourage policy support by offering institutional services and
   material interests to policy targets. In particular, citizens become
   more willing to support policies when governments timely disclose policy
   information and respond to and incorporate their voices in the policy
   design. Government subsidies in both the short and long runs also
   increase citizens' policy support. In addition, government transparency
   and long-run subsidies are complementary to enhancing policy support;
   the role of institutions is strengthened when citizens are exposed to
   severe policy problems.},
