@article{ WOS:000769381600001,
Author = {Dooley, Neil},
Title = {Frustrating Brexit? Ireland and the UK's conflicting approaches to
   Brexit negotiations},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2023},
Volume = {30},
Number = {5},
Pages = {807-827},
Month = {MAY 4},
Abstract = {While Ireland and Northern Ireland barely featured during the 2016
   referendum campaign, they have been central to Brexit negotiations. For
   some, Ireland's prominence in talks represents core EU values of
   solidarity and peace. For others, Ireland has been `used' as a
   bargaining chip to `frustrate Brexit'. In contrast, this paper shows how
   conflicting policy styles had an impact on the outcome of Brexit
   negotiations on the border in Ireland. Drawing on the literatures on
   Brexit negotiations, British policy style, and new intergovernmentalism,
   it shows that Ireland pursued a deliberative approach, contributing to
   its negotiating success. This is contrasted with three, relatively
   ineffectual, British approaches to Ireland, `lack of engagement',
   `magical thinking', and `delayed deliberation'. The paper draws on
   original interviews conducted with Irish politicians during
   negotiations, and interviews with senior British political figures
   contained in the UK in a Changing Europe Witness Archive.},
