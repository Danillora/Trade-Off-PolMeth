@article{ WOS:000944245900002,
Author = {Johnston, Michael},
Title = {It takes a whole society: why Hong Kong's ICAC cannot succeed alone},
Journal = {PUBLIC ADMINISTRATION AND POLICY-AN ASIA-PACIFIC JOURNAL},
Year = {2022},
Volume = {25},
Number = {2},
Pages = {109-123},
Month = {AUG 16},
Abstract = {PurposeTo show that establishing an anti-corruption agency (ACA), by
   itself, is not likely to be a successful anti-corruption strategy.
   Instead, a solid base of social trust, participation, and support is
   just as essential as creating an ACA with significant powers and
   resources.Design/methodology/approachThis is a historical review of the
   origins and development of the Independent Commission Against Corruption
   (ICAC) of Hong Kong, and an assessment of its three-pronged approach
   strategy at both strategic and tactical levels.FindingsAn ACA requires
   broad and deep social support and participation if it is to succeed.
   That the ICAC remains nearly unique in developing such a base of support
   provides important lessons for those countries with ineffective
   ACAs.Originality/valueA reassessment of the ICAC after a half-century
   shows that the Hong Kong case, despite its familiarity, has much to
   teach us if we understand it in its broadest implications.},
