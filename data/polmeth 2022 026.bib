@article{ WOS:000741188500001,
Author = {Sharma, Amogh Dhar},
Title = {`Mr. Clean' and his `computer boys': technology, technocracy, ana
   de-politicisation in the Indian National Congress (1981-1991)},
Journal = {COMMONWEALTH \& COMPARATIVE POLITICS},
Year = {2022},
Volume = {60},
