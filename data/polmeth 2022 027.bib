@article{ WOS:000801936700001,
Author = {Di Giulio, Marco and Vecchi, Giancarlo},
Title = {How ``institutionalization{''} can work. Structuring governance for
   digital transformation in ItalyPalabras clave(sic)(sic)(sic)},
