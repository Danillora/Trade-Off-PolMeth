@article{ WOS:000811605900001,
Author = {Tallis, Benjamin},
Title = {Kraftwerk and the international `re-birth of Germany':
   Multiplicity, identity and difference in music and International
   Relations},
Journal = {COOPERATION AND CONFLICT},
