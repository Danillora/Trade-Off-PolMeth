@article{ WOS:000760086700001,
Author = {Benson, Michelle and Tucker, Colin},
Title = {The Importance of UN Security Council Resolutions in Peacekeeping
   Operations{*}},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
