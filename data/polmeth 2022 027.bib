@article{ WOS:000854246000001,
Author = {Thomson, Andrew and Pankhurst, Dale},
Title = {From Control to Conflict: A Spectrum and Framework for Understanding
   Government-Militia Relationships},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
