@article{ WOS:001160710300003,
Author = {Ramirez-Alujas, Alvaro V.},
Title = {Open Government Policies in Ibero-America. Exploring a Decade of Changes
   in the Context of the Open Government Partnership (2011-2021)},
Journal = {REVISTA DEL CLAD REFORMA Y DEMOCRACIA},
