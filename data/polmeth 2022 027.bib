@article{ WOS:000820288500001,
Author = {Moravec, Lukas and Kukalova, Gabriela and Filipova, Dana Bina and
   Vankova, Zdenka},
Title = {The Effectivity of Local Fee Collection: Case Study in Selected
   Municipalities of the South Bohemian District, Czech Republic},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2022},
Volume = {20},
Number = {3},
Pages = {479-499},
Month = {JUL},
Abstract = {Local fees and tax income are one of the income sources of a
   municipality budget. Their size and collection is regulated by the
   municipality and a particular state. 12 municipalities of the South
   Bohemian District were chosen according to their size to find out the
   efficiency of the collection of the higher mentioned income. Next, a
   relationship between individual shares and size of selected
   municipalities in number of inhabitants was calculated based on shares
   among city fees, tax income and total income. The size of yields in
   local fees differs and the size of municipality (based on the number of
   inhabitants) is not the main factor of the difference. Yields of
   municipalities are also influenced by geographical and economic factors
   (e.g.: growth of tourism, ??esk?? Krumlov). Furthermore, absence of any
   relationship among local fees, tax income and total income was found
   within selected municipalities.},
