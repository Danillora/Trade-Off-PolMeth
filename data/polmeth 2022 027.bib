@article{ WOS:000850048700001,
Author = {Prisiazhniuk, Daria and Holavins, Arturs},
Title = {Active Ageing and Social Services: The Paradox of Empowerment in Russia},
Journal = {EUROPE-ASIA STUDIES},
Year = {2023},
Volume = {75},
Number = {2, SI},
Pages = {309-329},
Month = {FEB 7},
Abstract = {The essay describes the adoption of an active ageing policy framework in
   Russia. Based on semi-structured interviews with elderly Russians, the
   essay provides evidence of confusion and uncertainty on how to perceive
   one's own ageing. Research participants understood that the
   `paternalistic' view of old age as a time of troubles was now largely
   viewed as obsolete, yet the new `optimistic' view of old age as a time
   of freedom and opportunities often did not reflect their experiences.
   This mismatch in discourses and practices reflects how participatory
   empowerment rhetoric, which promotes active ageing, is becoming a
   justification for more modest state social service provision.},
