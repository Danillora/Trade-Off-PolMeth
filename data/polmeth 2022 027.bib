@article{ WOS:000827518200001,
Author = {Hassan, Muhammad Haniff and Abdullah, Walid Jumblatt},
Title = {The Model Muslim Minority: Wasatiyah (Justly-Balanced) as a
   Counter-Ideology Tool in Singapore},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
