@article{ WOS:000858415900001,
Author = {Naczyk, Marek and Ban, Cornel},
Title = {The Sputnik V moment: biotech, biowarfare and COVID-19 vaccine
   development in Russia and in former Soviet satellite states},
Journal = {EAST EUROPEAN POLITICS},
Year = {2022},
Volume = {38},
Number = {4, SI},
Pages = {571-593},
Month = {OCT 2},
Abstract = {Why have Russia and Cuba developed and produced vaccines against
   COVID-19 while Central and Eastern European (CEE) countries that are
   members of the European Union have only played a marginal role in the
   global supply of such vaccines? We argue that the answer is to be found
   in the capacity of Russia's national security state and entrepreneurs to
   mobilise historic Soviet advantages as part of a broader security
   motivated statecraft. CEE countries lacked this legacy and this drive.
   Similarly, they failed to massively invest - as for example, Cuba has -
   into potential synergies between public health systems and biotech
   firms.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
