@article{ WOS:000842112100001,
Author = {Smith, Jessica C.},
Title = {Representation in times of crisis: women's executive presence and
   gender-sensitive policy responses to crises},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2023},
Volume = {30},
Number = {10},
Pages = {1984-2009},
Month = {OCT 3},
Abstract = {This paper shows how the gender composition of executive government
   impacts national responses to crises through the case study of the
   COVID-19 pandemic. Building on descriptive accounts of women's
   underrepresentation in COVID-19 decision-making I consider the causes
   and consequences of their (non)presence. Using data from the UN COVID-19
   Global Gender Response Tracker, I find that: (i) across 62 countries,
   women average 25 per cent of members of government taskforces responding
   to the crisis and are siloed into advisory as opposed to decision-making
   positions; and, (ii) Women leaders shape who is present in
   policy-making, and policy outcomes. Women-led countries, although
   limited in number, have higher proportions of women on taskforces,
   especially decision-making ones. A country being woman-led has a large,
   although non-significant, positive effect on whether a policy response
   is gender-sensitive, whilst a higher proportion of women on all
   taskforces combined has a significant, yet small, positive effect.},
