@article{ WOS:000842353300001,
Author = {Jo, Eun A.},
Title = {Memory, Institutions, and the Domestic Politics of South Korean-Japanese
   Relations},
Journal = {INTERNATIONAL ORGANIZATION},
Year = {2022},
Volume = {76},
Number = {4},
Pages = {767-798},
Abstract = {How does collective memory shape politics in the domestic and
   international spheres? I argue that collective memory-an intersubjective
   understanding of the past-has no inherent meaning and its salience is
   entirely contextual. What it means politically depends on the historical
   trajectory through which it came to form and the political exigency for
   which it is mobilized in the present. I propose three strategies by
   which social actors mobilize collective memory: framing-negotiating how
   the past can be interpreted; accrediting-redefining which narrators are
   authorized to speak; and binding-enforcing the narrative bounds to which
   narrators must conform. Using this framework, I reassess the failure of
   South Korea-Japan reconciliation and find that it has as much to do with
   the mobilization of collective colonial memory in South Korea over the
   course of its democratization as with Japanese impenitence.
   Anti-Japanese memory reflects continued domestic political contestation
   about how South Korea remembers and identifies itself.},
