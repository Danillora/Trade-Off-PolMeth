@article{ WOS:000740595700001,
Author = {Alter, Karen J.},
Title = {The promise and perils of theorizing international regime complexity in
   an evolving world},
Journal = {REVIEW OF INTERNATIONAL ORGANIZATIONS},
Year = {2022},
Volume = {17},
Number = {2, SI},
Pages = {375-396},
Month = {APR},
Abstract = {As the world becomes more complicated, so too does global governance.
   The political consequences of the rising density of institutions,
   policies, rules and strategies to address global phenomena has been a
   central focus of the scholarship on international regime complexity.
   This conclusion to a special issue grapples with the promise and perils
   of theorizing about international regime complexity in a constantly
   evolving world. It discusses the special issue contributions while
   uniting the different conversations about the increasingly complex
   global governance space we refer to as international regime complexity.
   The goal is to bridge existing debates about global governance, to
   expand the scholarly conversation by drawing from and better connecting
   to IR debates, and to ensure that we can address practical and pressing
   global governance challenges.},
