@article{ WOS:000802980300001,
Author = {Kraft, Patrick and Dolan, Kathleen},
Title = {Glass Half Full or Half Empty: Does Optimism about Women's
   Representation in Elected Office Matter?},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2023},
Volume = {44},
Number = {2},
Pages = {139-151},
Month = {APR 3},
Abstract = {Recent years have seen an unprecedented number of women candidates
   running for public office. Does the resulting potential for greater
   gender equality in political representation have downstream effects on
   individual-level political attitudes, particularly among women voters?
   Given the partisan imbalance in women's candidacies, do Republican and
   Democratic voters experience the growing gender parity in political
   representation differently? We explore these questions by employing a
   survey experiment in the 2018 Cooperative Election Study (CES) that
   manipulates the perceived trajectory of women's representation in
   politics. Our results suggest that priming future optimism as compared
   to pessimism in women's representation has little overall effect on the
   gender gap in political efficacy and interest, but that party
   affiliation can be a moderator in this context. We discuss the broader
   implications of our findings for women's engagement in politics.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
