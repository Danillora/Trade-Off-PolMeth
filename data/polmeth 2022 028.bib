@article{ WOS:000765214900001,
Author = {Pili, Giangiuseppe},
Title = {Deciphering intelligence analysis - The synthetic nature of the core
   intelligence function},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2023},
Volume = {38},
Number = {1},
Pages = {128-142},
Month = {JAN 2},
Abstract = {Intelligence analysis is one of the most explored topics in intelligence
   studies. However, decoding its nature is still challenging. A unifying
   question must be considered: `Is intelligence analysis - analysis?'
   Unfolding the problem leads to an extreme conclusion: intelligence
   analysis is a way to structure sensory data collection and reduction. It
   is, namely, synthesis. A systematic scrutiny of the general nature of
   analysis is considered to compare it to what intelligence analysis is
   intended to be. As it will turn out, intelligence analysis is much more
   synthesis - namely, structuring sensory data collection - than analysis
   per se, which is the main conclusion of the argument.},
