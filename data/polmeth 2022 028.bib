@article{ WOS:001012497700012,
Author = {Schmidt, Carissa J. and Rauk, Leigh and Cunningham, Rebecca M. and
   Zimmerman, Marc A. and Roche, Jessica S. and Carter, Patrick M.},
Title = {How Multi-disciplinary Research Centers and Networks Can Advance the
   Science of Firearm Injury Prevention},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2022},
Volume = {704},
Number = {1, SI},
Pages = {242-266},
Month = {NOV},
Abstract = {Firearm injuries are a leading cause of injury-related deaths in the
   U.S., but there is a dearth of federal funding for research on firearm
   injury prevention, compared to funding for research on other injuries
   and diseases. University research centers/institutes (C/Is) are in a
   position to help generate such research: they are multidisciplinary,
   have the capacity to conduct large-scale research projects, and commonly
   address complex health and social problems. In this article, we trace
   the history of research on firearm injury prevention, describe the
   different kinds of C/Is and C/I networks that have been developed to
   build knowledge in other areas of critical social concern, and argue for
   the development of a national coordinated network of firearm injury
   prevention C/Is.},
