@article{ WOS:000859202600007,
Author = {Vandenhole, Wouter},
Title = {The Committee on the Rights of the Child-Size Matters Sometimes,
   Somewhat: A Comment†},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2022},
Volume = {14},
Number = {1, SI},
Pages = {101-107},
Month = {SEP 26},
Abstract = {The Convention on the Rights of the Child is the most widely ratified
   core United Nations human rights treaty. Does size matter for
   normativity and compliance? Size does matter sometimes in a positive
   way: the quasi-universal ratification of the CRC gives that treaty
   unprecedented normative legitimacy and legal reach. Often, size adds
   however a layer of complexity: in negotiations, in implementation, in
   monitoring. Size matters somewhat, although factors other than size
   matter more: normative loopholes and black holes; underlying normative
   disagreement; political calculation; capacities; overstated expectations
   about the role of law in social change; as well as mismatches between
   legal standards and real-life issues.},
