@article{ WOS:000844535900001,
Author = {Oleart, Alvaro and Theuns, Tom},
Title = {`Democracy without Politics' in the European Commission's Response to
   Democratic Backsliding: From Technocratic Legalism to Democratic
   Pluralism},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2023},
Volume = {61},
Number = {4},
Pages = {882-899},
Month = {JUL},
Abstract = {The debate on EU responses to democratic backsliding in EU member states
   has mostly been anchored on technocratic appeals to the rule of law and
   judicial independence, and on Poland and Hungary. In this article, we
   ask: What understandings of democracy have shaped the European
   Commission's response to democratic backsliding in recent years? After
   developing an understanding of pluralist democracy, we uncover the way
   in which the European Commission conceives of democracy through a
   discourse analysis of European Commissioners' speeches (2018-21) and a
   normative-theoretical analysis. We identify the Commission's conception
   as a form of `democracy without politics', and argue that it matches the
   EU's policy choices in regard to democratic backsliding. We argue that a
   fuller, healthier and normatively more attractive conception of
   democracy encompasses more attention to political pluralism, agonistic
   contestation and the vibrancy of civil society.},
