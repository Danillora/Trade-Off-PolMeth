@article{ WOS:000787177300001,
Author = {Prezelj, Iztok and Ristevska, Teodora Tea},
Title = {Intelligence scandals: a comparative analytical model and lessons
   learned from the test case of North Macedonia},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2023},
Volume = {38},
Number = {1},
Pages = {143-170},
Month = {JAN 2},
Abstract = {Most countries that possess intelligence services have encountered
   intelligence scandals involving actual or alleged intelligence
   activities. This paper offers a definition of intelligence scandal and
   creates a new analytical model for assessing intelligence scandals.
   Application of the analytical model to six key intelligence scandals in
   North Macedonia shows that various intelligence and political actors can
   commit a wide range of transgressions in largely internal and external
   political contexts with very broad political and security impacts. The
   case also shows the competitive nature of the triggers of the scandals,
   various kinds of actors' defences, and the scandals' long duration.},
