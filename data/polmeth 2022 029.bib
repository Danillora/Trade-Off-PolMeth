@incollection{ WOS:001097180000004,
Author = {Tas, Diren},
Editor = {Borsuk, I and Dinc, P and Kavak, S and Sayan, P},
Title = {Displacing Resistance in Kurdish Regions: The Symbiosis of Neoliberal
   Urban Transformation and Authoritarian State in Sur},
Booktitle = {AUTHORITARIAN NEOLIBERALISM AND RESISTANCE IN TURKEY: Construction,
   Consolidation, and Contestation},
Year = {2022},
Pages = {81-104},
Abstract = {Many states seek to shape their own space with mass-scale neoliberal
   urban transformation projects that result in the widespread displacement
   and dispossession of subaltern classes from inner-city neighbourhoods.
   This chapter focuses on how the AKP government, which has been at the
   vanguard of an authoritarian resurgence in Turkey, facilitates
   post-conflict neoliberal urban transformation projects as a revanchist
   mechanism to suppress oppositional politics and grassroots resistance in
   Kurdish cities. Based on an ethnographic case study in the Sur district
   of Diyarbakir, this chapter shows how legal and coercive state
   apparatuses are immersed in neoliberal urban transformation projects as
   counter-insurgency tools of security, control, and power, to displace
   and dispossess resistant neighbourhoods. Hence, neoliberal
   transformation projects enable the authoritarian state to dominate,
   frame, and reconfigure localities to eliminate any forms of opposition
   or challenge to its existing hegemony.},
