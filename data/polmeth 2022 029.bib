@article{ WOS:000766233900007,
Author = {Dhima, Kostanca},
Title = {Do Elites Discriminate against Female Political Aspirants? Evidence from
   a Field Experiment},
Journal = {POLITICS \& GENDER},
Year = {2022},
Volume = {18},
Number = {1},
Pages = {126-157},
Month = {MAR},
Abstract = {Do elites exhibit gender bias when responding to political aspirants?
   Drawing on theories of gender bias, group attachment, and partisan
   identity, I conduct the first audit experiment outside the United States
   to examine the presence of gender bias in the earliest phases of the
   political recruitment process. Based on responses from 1,774 Canadian
   legislators, I find evidence of an overall gender bias in favor of
   female political aspirants. Specifically, legislators are more
   responsive to female political aspirants and more likely to provide them
   with helpful advice when they ask how to get involved in politics. This
   pro-women bias, which exists at all levels of government, is stronger
   among female legislators and those associated with left-leaning parties.
   These results suggest that political elites in Canada are open to
   increasing female political representation and thus should serve as
   welcome encouragement for women to pursue their political ambitions.},
