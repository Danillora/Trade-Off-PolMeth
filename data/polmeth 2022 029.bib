@article{ WOS:000901304900001,
Author = {Lesschaeve, Christophe},
Title = {War as an Enduring Determinant of Party Choice in Postwar Southeast
   Europe},
Journal = {EUROPE-ASIA STUDIES},
Year = {2023},
Volume = {75},
Number = {10},
Pages = {1597-1625},
Month = {NOV 26},
Abstract = {In postwar elections, voter choices are often shaped by the memory of
   past violence. Taking Bosnia \& Hercegovina and Croatia as case studies,
   this study examines war as an enduring determinant of party choice among
   the age cohorts who lived through the wars of the 1990s, and the cohorts
   who were born after. Based on a representative survey of over 5,000
   citizens, the results show that in Bosnia \& Hercegovina, war-related
   issues and social divisions continue to inform party preferences in the
   postwar generation as much as they did in the generations that came
   before. In Croatia, by contrast, war-related issues are showing signs of
   diminishing political relevance.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
