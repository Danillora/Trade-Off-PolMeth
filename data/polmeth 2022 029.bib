@article{ WOS:000803294700001,
Author = {Kuteleva, Anna and Chernilevskaya, Klavdiya and Salnikova, Polina and
   Shevchuk, Egor},
Title = {Russia's Actually (Non-)Existent Neoliberalism: The Development of the
   Russian Far East as Discourse and Practice},
Journal = {EUROPE-ASIA STUDIES},
Year = {2022},
Volume = {74},
Number = {6},
Pages = {968-989},
Month = {JUL 3},
Abstract = {The article explores Russia's `turn to the East' and examines different
   and often conflicting visions of development that emerged in the process
   of reconstructing the Russian Far Eastern development strategy at
   regional and federal levels. It draws on a `thick' case study of the
   special investment regime, Free Port Vladivostok, which exemplifies
   simultaneously a new approach to regional development and the
   contradictions spawned by it. Analysis of Free Port Vladivostok
   represents an entry point into a discussion of Russian neoliberalisation
   and `actually (non-)existent neoliberalism'.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
