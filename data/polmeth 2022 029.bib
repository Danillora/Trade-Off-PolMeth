@article{ WOS:000871330500001,
Author = {Gotz, Elias},
Title = {Taking the Longer View: A Neoclassical Realist Account of Russia's
   Neighbourhood Policy},
Journal = {EUROPE-ASIA STUDIES},
Year = {2022},
Volume = {74},
Number = {9, SI},
Pages = {1729-1763},
Month = {OCT 21},
Abstract = {This essay complements the other contributions to this special issue by
   placing Russia's neighbourhood policy into a broader temporal and
   theoretical perspective. It shows that Russia's political elite during
   the last three decades has been largely united behind the goal of
   establishing a Moscow-centred regional security order. Yet, despite this
   broad-based consensus, Russia's policy in the former Soviet area has
   varied markedly across time and space. To account for this pattern, the
   essay develops and tests a neoclassical realist approach that explains
   why, how and when major powers such as Russia pursue regional primacy.},
