@article{ WOS:001330570500011,
Author = {Ramanathan, Kumar and Kalmoe, Nathan P.},
Title = {Partisanship and Racial Attitudes in US Civil War Enlistment},
Journal = {JOURNAL OF RACE ETHNICITY AND POLITICS},
