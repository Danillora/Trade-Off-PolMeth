@article{ WOS:000784751500001,
Author = {Shahid, Rudabeh and Turner, Joe},
Title = {Deprivation of Citizenship as Colonial Violence: Deracination and
   Dispossession in Assam},
Journal = {INTERNATIONAL POLITICAL SOCIOLOGY},
Year = {2022},
Month = {2022 APR 1},
Abstract = {This article argues that deprivation of citizenship is an ongoing force
   of colonial violence. By exploring the case of citizenship stripping in
   India's northeastern state of Assam, the article proposes that the
   removal of citizenship rights is not merely an aberration of the
   ``normal{''} rules of citizenship but bound up with ongoing forms of
   colonial dispossession informed by racial hierarchies, the regulation of
   belonging and mobility. Interdisciplinary scholarship on deprivation of
   citizenship remains largely Euro/Western-centric and fails to consider
   how deprivation works as part of broader patterns of colonial-modern
   dispossession. By drawing on Gurminder K. Bhambra's (2015) work on the
   colonial constitution of citizenship and Aurora Vergara-Figueroa's
   (2018) work on deracination, we treat deprivation of citizenship as a
   legacy of the colonial and racialized structure of citizenship itself.
   By using Assam as a case study, the article examines how practices of
   deprivations are tied to the histories of dispossession, extraction, and
   control, which underpinned the historical emergence of citizenship in
   (post)colonial India and beyond.},
