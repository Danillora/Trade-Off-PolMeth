@article{ WOS:000751541300001,
Author = {Bonacker, Thorsten and Lottholz, Philipp},
Title = {The Post-Imperial Politics of Security and Depoliticisation: Comparing
   Discourses and Practices of Ordering Across Central Asia},
Journal = {EUROPE-ASIA STUDIES},
