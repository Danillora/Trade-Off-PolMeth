@article{ WOS:001012497700006,
Author = {Zeoli, April M. and Mccourt, Alexander D. and Paruk, Jennifer K.},
Title = {Effectiveness of Firearm Restriction, Background Checks, and Licensing
   Laws in Reducing Gun Violence},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2022},
Volume = {704},
Number = {1, SI},
Pages = {118-136},
Month = {NOV},
Abstract = {We present the rationale behind four types of laws that restrict access
   to firearms for those who are deemed to be a high risk for future gun
   violence and two types of laws that implement firearm purchase
   prohibitions. We also present evidence on the effectiveness of these
   laws. Broadly, these are laws that restrict access for domestic violence
   abusers, individuals convicted of misdemeanor violence, and individuals
   at high risk of violence against themselves or others. We briefly
   discuss relinquishment of firearms by those who are newly restricted,
   but we focus mainly on how purchase restrictions are implemented by the
   federal government and across states. Extant research shows that
   well-implemented firearm policy that is based on evidence-based risk
   factors can be effective in reducing firearm injury.},
