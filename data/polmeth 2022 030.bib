@article{ WOS:000904745500001,
Author = {Suttill, Francis J.},
Title = {Was the Prosper French resistance circuit betrayed by the British in
   1943?},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2023},
Volume = {38},
Number = {4},
Pages = {654-661},
Month = {JUN 7},
Abstract = {SOE's resistance circuit Prosper was destroyed suddenly and rapidly. In
   the absence of any information as to the causes of this collapse,
   conspiracy theories developed suggesting that the British had somehow
   deliberately sacrificed the circuit as part of a deception plan. Newly
   released information enables the sequence of events that led up to this
   disaster to be set out in detail, making it clear that the circuit was
   not, and could not have been, betrayed as part of a deception plan nor
   could anyone in Britain have played any part in the speed with which the
   circuit was rounded up.},
