@article{ WOS:000737557200001,
Author = {Raj-Reichert, Gale and Staritz, Cornelia and Plank, Leonhard},
Title = {Conceptualizing the Regulator-Buyer State in the European Union for the
   Exercise of Socially Responsible Public Procurement in Global Production
   Networks},
