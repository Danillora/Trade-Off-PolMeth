@article{ WOS:000823632000001,
Author = {LASSILA, J. U. S. S. I. and SANINA, A. N. N. A.},
Title = {Attitudes to Putin-Era Patriotism Amongst Russia's `In Between'
   Generation},
Journal = {EUROPE-ASIA STUDIES},
Year = {2022},
Volume = {74},
Number = {7, SI},
Pages = {1190-1209},
Month = {AUG 9},
Abstract = {This essay focuses on the role of patriotic socialisation amongst a
   selected sample from the cohort born in the early 1980s. This
   understudied group occupies a unique space in Russia's societal and
   political developments, including Putin-era patriotic education. The
   qualitative case study shows that respondents' personal and positive
   memories of the Soviet-era patriotism have no positive connection with
   the current patriotic education. While they acknowledge the virtues of
   official patriotism, they are also critical of certain aspects, in
   particular, its militaristic emphasis.},
