@article{ WOS:000763878600001,
Author = {Hearne, Siobhan},
Title = {Sanitising Sex in the USSR: State Approaches to Sexual Health in the
   Brezhnev Era},
Journal = {EUROPE-ASIA STUDIES},
Year = {2022},
Volume = {74},
Number = {10},
Pages = {1793-1815},
Month = {NOV 26},
Abstract = {This article examines Soviet approaches to sexual health in the Brezhnev
   era (1964-1982), specifically venereal diseases (VD). After the death of
   Stalin, the Soviet leadership adopted new methods for regulating the
   behaviour of Soviet citizens based on collective adherence to specific
   moral codes. State ministries took tentative steps towards mass sex
   education. In the 1960s, the Soviet leadership relaunched the `struggle
