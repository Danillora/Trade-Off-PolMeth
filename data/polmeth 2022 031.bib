@article{ WOS:000897156800001,
Author = {Lee, Yaechan and Grimes, William W. W.},
Title = {Manifesting the embedded developmental state: the role of South Korea's
   National Pension Service in managing financial crisis},
Journal = {REVIEW OF INTERNATIONAL POLITICAL ECONOMY},
Year = {2023},
Volume = {30},
Number = {5},
Pages = {1933-1956},
Month = {SEP 3},
Abstract = {Financial liberalization has noticeably reduced the role of the state in
   effectively influencing the economy in post-developmental states. Yet
   many studies have found that the legacies of the developmental model
   continue to influence the policies, institutions, and socioeconomic
   challenges that are faced by the states that previously adopted the
   model. These studies, however, do not clearly identify when and how such
   legacies may be manifested in state behavior. This paper contributes to
   filling this gap in the literature by arguing that financial crises can
   serve as a trigger to more clearly reveal the structural evidence of the
   legacy in institutions that were previously established and utilized for
   developmental objectives. By conducting a rigorous case analysis using
   historical and market data on the crisis responses of South Korea's
   public pension fund, this paper finds that South Korea's developmental
   legacy remains passively embedded in the governance structure of the
   pension fund in non-crisis times but manifests during financial crises.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
