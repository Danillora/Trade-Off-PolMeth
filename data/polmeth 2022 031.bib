@article{ WOS:000789282300001,
Author = {Sukalo, Alexandra},
Title = {Learning to Think and Talk Like the Locals: The Soviet Political
   Police's Efforts to Adapt in Lithuania and Ukraine, 1944-1949},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2022},
Volume = {37},
Number = {4, SI},
Pages = {556-568},
Month = {JUN 7},
Abstract = {Following the Second World War, anti-Soviet groups emboldened by their
   wartime experience challenged the Soviets' efforts to regain control
   over the Lithuanian and Ukrainian Soviet Socialist Republics. The Soviet
   domestic intelligence officers, or political police, realized that if
   they were to have any hope of pacifying the local population and
   destroying these anti-Soviet groups, they needed to speak and think like
   the people they were surveilling. This article explores the attempts of
   the political police leadership to adapt their methods and organization
   to meet new challenges and succeed in the postwar operational
   environment.},
