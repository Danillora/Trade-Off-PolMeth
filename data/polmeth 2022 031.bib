@incollection{ WOS:001097180000007,
Author = {Murat, Ezgi},
Editor = {Borsuk, I and Dinc, P and Kavak, S and Sayan, P},
Title = {Education for the Marginalised as Resistance Under Authoritarian
   Neoliberalism: The Case of People's Houses Summer School},
Booktitle = {AUTHORITARIAN NEOLIBERALISM AND RESISTANCE IN TURKEY: Construction,
   Consolidation, and Contestation},
Year = {2022},
Pages = {151-170},
Abstract = {Education has always been contested, and while it can become an
   instrument for strengthening the power and dominance of the state, it
   can also become an area of resistance. This chapter analyses how
   education becomes an area of resistance by focusing on one of the few
   alternative education practices in Turkey, People's Houses Summer
   School, which has organised free summer schools for primary and
   secondary school children in disadvantaged neighbourhoods since 2008.
   The chapter underlines how such organised educational activity becomes a
   resistance tool and space against not only mainstream education but also
   the increasing authoritarianism appearing in daily life in Turkey. This
   ethnographic study answers the following questions: how and why did it
   become difficult to practice summer schools in a politically
   marginalised neighbourhood, despite the increasing popularity of the
   movement? Does this show a failure of the education activity or does it
   show the contradictions that are inherent to any resistance movement?},
