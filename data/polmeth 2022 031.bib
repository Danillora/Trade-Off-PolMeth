@article{ WOS:000825435700001,
Author = {Parent, Genevieve},
Title = {Identity and Trust in Bosnia \& Hercegovina: Redrawing Broader
   Boundaries Through Meaningful Experiences},
Journal = {EUROPE-ASIA STUDIES},
Year = {2023},
Volume = {75},
Number = {1},
Pages = {88-109},
Month = {JAN 2},
Abstract = {Political elites continue to instil notions of fear of the Other in
   Bosnia \& Hercegovina (BiH), which could lead to renewed violence in a
   context of growing socio-economic and political instability.
   Considerable literature on BiH indicates that more should have been done
   to foster reconciliation between the antagonists. The purpose of this
   article is to clarify the relationship between trust, a crucial element
   of reconciliation, and identity. Based on a series of semi-structured
   interviews conducted in BiH, this article also seeks to deepen our
   understanding of significant factors to consider for building trust
   through an incremental process.},
