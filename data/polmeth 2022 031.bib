@article{ WOS:000858001500001,
Author = {Soyaltin-Colella, Digdem},
Title = {The EU Accession Process, Chinese Finance and Rising Corruption in
   Western Balkan Stabilitocracies: Serbia and Montenegro},
Journal = {EUROPE-ASIA STUDIES},
Year = {2023},
Volume = {75},
Number = {8},
Pages = {1311-1335},
Month = {SEP 14},
Abstract = {As front runners in the EU accession process, Serbia and Montenegro have
   failed to control corruption yet managed to advance along the path to EU
   integration. Although they have stronger state capacity compared to
   other Western Balkan countries, the two have recently reverted to
   previous poor governance practices after initial headway meeting EU good
   governance conditions. An in-depth analysis of the two countries
   explains this paradox as the result of the combination of the EU
   accession process and increasing Chinese finance. While the EU's
   stability-oriented strategy helped to consolidate these authoritarian
   regimes, Chinese funding policies have further strengthened patronage
   networks, fuelling corruption in the EU's `stabilitocracies'.},
