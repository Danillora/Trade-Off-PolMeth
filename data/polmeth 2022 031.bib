@incollection{ WOS:001097180000009,
Author = {Ozduzen, Ozge},
Editor = {Borsuk, I and Dinc, P and Kavak, S and Sayan, P},
Title = {DIY Media and Urban Citizenship: Intersectional Post-Occupy Media
   Activism in Turkey},
Booktitle = {AUTHORITARIAN NEOLIBERALISM AND RESISTANCE IN TURKEY: Construction,
   Consolidation, and Contestation},
Year = {2022},
Pages = {191-210},
Abstract = {This chapter studies the media activism of urban and media collectives
   and image activists against authoritarian policies in Turkey in the
   post-Occupy period (2013-2019). It identifies the roles of creativity,
   community-making, and intersectionality in recent protest movements in
   Turkey, which took their cues from earlier global protests such as the
   Arab Spring and the Occupy movements. The post-Occupy urban activism in
   Istanbul mimicked and critically engaged with the Gezi protests by
   focusing on transient and participatory do-it-yourself (DIY) media
   practices that consist of sharing and creating social media content;
   producing videos, podcasts, and images; as well as organising
   semi-professional concerts, radio shows, and film screenings. Through an
   analysis of these DIY media production and circulation, the chapter
   frames how the use of urban and digital spaces following the Gezi
   protests marked a continuous activist effort in signposting the heart
   of.Istanbul as a `rebel city' against the top-down control of urban
   space as well as the government's media ownership and regulation.},
