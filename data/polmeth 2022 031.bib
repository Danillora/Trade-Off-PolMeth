@article{ WOS:000893072800005,
Author = {Walby, Sylvia},
Title = {Crisis and society: developing the theory of crisis in the context of
   COVID-19},
Journal = {GLOBAL DISCOURSE},
Year = {2022},
Volume = {12},
Number = {3-4, SI},
Pages = {498-516},
Month = {SEP},
Abstract = {The theory of crisis and society is advanced by developing a complex
   systems analysis arid is applied to the COVID-19 pandemic. Five issues
   are identified, discussed and resolved: the definition of crisis;
   whether a crisis is treated as real or as a socially constructed
   narrative, or both; the underlying concept and theory of society and its
   alternative forms; and the different kinds of change in relationship
   between crisis and society (recuperation, intensification,
   transformation and catastrophe). This complex systems approach to crisis
   is applied to the COVID-19 pandemic, analysing the cascade of the crisis
   through institutional domains and the consequent changes to multiple
   regimes of inequality.},
