@article{ WOS:001330324000002,
Author = {Carlson, Kirsten Matoy},
Title = {Beyond descriptive representation: American Indian opposition to federal
   legislation},
Journal = {JOURNAL OF RACE ETHNICITY AND POLITICS},
Year = {2022},
Volume = {7},
Number = {1},
Pages = {65-89},
Month = {MAR},
Abstract = {This study explores how American Indians use interest group strategies
   to block federal legislation. Unlike other disadvantaged groups, who
   have influenced public policymaking through descriptive representation,
   American Indians have turned to interest group strategies to protect
   their interests in Congress. Using original data collected from American
   Indian testimony at congressional hearings on 266 bills during five
   Congresses, this study tests interest group hypotheses about how and
   when active opposition affects bill enactment. It finds that American
   Indians can block federal legislation harmful to their interests when
   they unify against a bill and that members of Congress frequently
   respond to American Indian opposition by amending bills to alleviate
   American Indian concerns.},
