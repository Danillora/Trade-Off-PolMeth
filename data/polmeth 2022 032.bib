@article{ WOS:000825803800001,
Author = {Kenny, John},
Title = {The Formation of British Attitudes towards the Common Market: 1957-72},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2023},
Volume = {61},
Number = {2},
Pages = {434-450},
Month = {MAR},
Abstract = {This paper uses an extensive collection of historical surveys that have
   only recently been made available to researchers to examine the
   formation of British attitudes towards the then European Economic
   Community. It demonstrates that - up until 1967 - the demographic
   predictors of support for UK membership were unstable. Thereafter,
   coinciding with the second UK application, these attitudes started to
   stabilise and harden, with support becoming highest among men, the
   youngest age cohorts, the middle class and those with greater education.
   The renewed politicization of the issue in 1967 also coincided with
   Labour voters becoming significantly more likely to support membership.
   Following the change in the parties' positions, Labour voters
   subsequently become substantially less supportive than Conversative
   voters. The paper thus supports existing analyses on the role of elite
   cues, while providing new, robust evidence of the change in demographic
   associations over this formative period.},
