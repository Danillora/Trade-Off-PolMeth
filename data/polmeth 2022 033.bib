Title = {Britain's key counter-subversion instrument before the 1971 withdrawal
   from the Gulf: Voice of the Coast {[}Sawt Al
   Saahil] Arabic radio station},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2023},
