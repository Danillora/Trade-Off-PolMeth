@article{ WOS:000897644700001,
Author = {Borrell, Joan-Ramon and Garcia, Carmen and Luis Jimenez, Juan},
Title = {The difficult road to a better competition policy: How do competition
   authorities reforms affect antitrust effectiveness?},
Journal = {REGULATION \& GOVERNANCE},
Year = {2024},
Volume = {18},
Number = {1},
Pages = {203-225},
Month = {JAN},
Abstract = {This paper estimates the impact of reforming competition authorities on
   perceived antitrust effectiveness using methods of causal inference. We
   study how 20 countries reformed their competition authorities in depth
   between 1995 and 2020, and what has been the outcome of such reforms in
   the perceived competition policy effectiveness by the business community
   compared with 18 control countries in a balanced panel. As the political
   economy literature warned, we find that reforms paradoxically have not
   always improved antitrust effectiveness. Some of the reforms approved
   stalled or backlashed as politicians opted for a Machiavelli option:
   undertaking ``counter-reforms{''} even in the name of an apparent but
   deceptive progressiveness and pro-competition drive.},
