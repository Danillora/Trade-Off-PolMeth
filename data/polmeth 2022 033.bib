@article{ WOS:000871333800001,
Author = {Aslett, Kevin and Magistro, Beatrice},
Title = {Principal-Agent Problems with EU Funds: A Case Study of Patronage in
   Hungary},
Journal = {EUROPE-ASIA STUDIES},
Year = {2023},
Volume = {75},
Number = {8},
Pages = {1286-1310},
Month = {SEP 14},
Abstract = {EU funds have been linked to high levels of corruption, even given
   substantial administrative and regulatory requirements and extensive
   domestic monitoring. We posit that this divergence between actual
   outcomes and preferred policies can be attributed to the co-optation of
   the auditing and monitoring processes by member-state governments. We
   outline the importance of the auditing process and flow of information
   to the European Commission using a delegation model and then test what
   occurred when this process was co-opted in Hungary. We find that the
   co-optation of the auditing process results in a rise in patronage and
   corruption.},
