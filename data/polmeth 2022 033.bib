@article{ WOS:000770300600001,
Author = {Briggs, Chad M. and Matejova, Miriam and Weiss, Robert},
Title = {Disaster intelligence: developing strategic warning for national
   security},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2022},
Volume = {37},
