@article{ WOS:000889069500001,
Author = {Hornat, Jan and Slosarcik, Ivo and Tomalova, Eliska and Vaska, Jan},
Title = {International Organisations as Status Enhancers: The Case of the Czech
   Republic},
Journal = {EUROPE-ASIA STUDIES},
Year = {2023},
Volume = {75},
Number = {10},
Pages = {1626-1650},
Month = {NOV 26},
Abstract = {In 2015 and 2017 the Czech Republic adopted two strategic documents that
   were aimed to support Czech nationals in positions within international
   organisations (IOs). In this article, we explore the rationale of the
   Czech government in doing so. Considering the Czech Republic as a small
   state, we approached the question from the perspective of status-seeking
   and observed the mechanisms of personnel support in selected IOs. We
   argue that the policy of promoting nationals in the ranks of prioritised
   IOs was intended to enhance the country's international status and also
   formed part of domestic legitimation games.},
