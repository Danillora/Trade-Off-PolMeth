@article{ WOS:000788250400001,
Author = {Klassen-Molyneaux, Riley},
Title = {Doing Good and Feeling Good: A Critical Analysis of Human Rights
   Research},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2022},
Volume = {14},
Number = {2, SI},
Pages = {513-534},
Month = {OCT 7},
Abstract = {The question guiding this article is: `when is nongovernmental
   organization (NGO) research useful?'. The first section after the
   introduction addresses this question directly. It does so by drawing on
   the author's personal experience and by considering what scholars say
   about research done by NGOs on human rights. It then considers Bent
   Flyvbjerg's grounded conception of the social sciences as well as what
   good a checklist could do to curb human rights violations in the
   maritime sector. The second section of the paper asks a more difficult
   question: when is NGO research not useful? Drawing on Thomas Sowell's
   critique of intellectuals in Intellectuals and Society, this section
   aims to show that, although people tend to think that working at an NGO
   is an unassailable good, hidden motivations can-and maybe
   should-sometimes cast doubt on this assumption. The third section of the
   paper synthesizes the first two and draws some additional conclusions
   about other-perhaps better-ways to improve the human condition. The
   section then concludes that while NGO research might be useful for some
   specific purposes, well-meaning scholars should not assume that their
   research into human rights is always useful.},
