@article{ WOS:000787580600001,
Author = {Nortio, Emma and Jasinskaja-Lahti, Inga and Hamalainen, Mikko and
   Pakkasvirta, Jussi},
Title = {Fear of the Russian bear? Negotiating Finnish national identity online},
Journal = {NATIONS AND NATIONALISM},
Year = {2022},
Volume = {28},
Number = {3},
Pages = {861-876},
Month = {JUL},
Abstract = {National identities are an important tool for collective political
   persuasion and mobilisation among political elites and lay people.
   Recent research on nationalism has shown that the negotiating of
   national identities, like any political deliberations and operations,
   increasingly occur on the Internet. In this study, we contribute to this
   research by examining the relational construction of Finnish identity
   online. More specifically, we focus on how the users of the largest
   discussion forum in Finland constructed Russia as a threat. A massive
   dataset spanning 12 years enables us to map the recurring patterns and
   temporal shifts in the discussions. We show that the construction of
   Russia as a threatening national other was used to both oppose and
   support Finland's alliance with the West, namely, becoming a member of
   NATO.},
