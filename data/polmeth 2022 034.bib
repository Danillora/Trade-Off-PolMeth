@incollection{ WOS:000907348600004,
Author = {Plummer, Franziska},
Book-Author = {Plummer, F},
Title = {Making Border Politics: State Actors \& Security in the Chinese Border
   Regime},
Booktitle = {RETHINKING AUTHORITY IN CHINA'S BORDER REGIME: Regulating the Irregular},
Series = {New Mobilities in Asia},
Year = {2022},
Pages = {131-174},
Abstract = {This chapter elaborates how responsibility over immigration and border
   management is shared among government levels and how they conceptualise
   and practice `security' and `development' at the border. I depict how
   border politics are regulated in a decentralised and transnationalised
   system that emphasises central rule over the immigration system while
   deliberately allowing leeway for local actors (i.e. local governments
   and local Public Security Bureaus) to find solutions that are legal,
   yet, different from standard immigration procedures. The role of local
   governments as `scalar managers' both within the security field of
   border control and within domestic development campaigns is discussed.},
