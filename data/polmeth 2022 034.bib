@article{ WOS:000788398700009,
Author = {Rosman, Michael E.},
Title = {Federalism and female circumcision in the US: a reply to `The
   prosecution of Dawoodi Bohra women' by Richard Shweder},
Journal = {GLOBAL DISCOURSE},
