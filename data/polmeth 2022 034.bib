@article{ WOS:000754553100003,
Author = {Taiwo, Olufemi O. and Talati, Shuchi},
Title = {Who Are the Engineers? Solar Geoengineering Research and Justice},
Journal = {GLOBAL ENVIRONMENTAL POLITICS},
Year = {2022},
Volume = {22},
Number = {1},
Pages = {12-18},
Month = {FEB 4},
Abstract = {Solar geoengineering research is a small but growing field as concerns
   arise that reducing emissions will not be sufficient to limit severe
   climate impacts. With this increasing attention, ensuring that the field
   advances equitably and inclusively is of immense importance. This
   commentary is a response to arguments that advocate for abandoning solar
   geoengineering research altogether because it perpetuates colonialism
   and promotes injustice. We find, however, that this brand of argument is
   itself performatively colonial and recommend a more inclusive framework
   for solar geoengineering governance that integrates existing research on
   relevant structures.},
