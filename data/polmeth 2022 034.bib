@article{ WOS:000849177300001,
Author = {Govindasamy, Geetha},
Title = {Regime Change and Malaysia-South Korea Relations: Domestic and
   Structural Explanations of Mahathir Mohamad and Moon Jae-in's Foreign
   Policy Reorientations},
