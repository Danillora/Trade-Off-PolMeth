@article{ WOS:000739723300001,
Author = {Gamkrelidze, Tamar},
Title = {Georgia's external frontier on Russia sedimented and unmalleable:
   engagement politics and the impact of the three-tier warfare},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2023},
Volume = {31},
Number = {2},
Pages = {536-555},
Month = {APR 3},
Abstract = {The article analyses the Georgian Dream (GDDG)-driven engagement
   politics towards the Kremlin and in this context Georgia-Russia
   relations, which deteriorated during the pandemics, and this is amid
   repeated accusations towards the GDDG by the political opponents on
   being pro-Russian. What happened, why has GDDG failed to shift external
   frontier from Russia by practicing the engagement politics, i.e.
   filtered political discourse and cooperative initiatives? The research
   claims that the external frontier is sedimented and unmalleable within
   existing political arrangements with the Kremlin. Hence, despite
   targeted efforts and success in establishing small `islands of
   cooperation' with Russia, the GDDG-driven engagement politics failed to
   redress external frontier. The difficulties to move external frontier
   became obvious gradually through years of no substantive results in
   mitigation of the three-tier warfare: (1) territorial, (2) cyber and (3)
   disinformation by the Kremlin. Russia initiated full-scale
   disinformation warfare, however, against the Richard Lugar Public Center
   for Public Health Research has been arguably a turning point for the
   GDDG's decision to retire the proactive engagement politics and maintain
   a status quo without falling a victim to Russia's full-scale military
   intervention or/and provocations instead. Such an approach can be best
   described as pro-pragmatic `wait and see' policy towards the Kremlin.},
