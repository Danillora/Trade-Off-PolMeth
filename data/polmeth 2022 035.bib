@incollection{ WOS:001329617700007,
Author = {Kocak, Muhammet},
Editor = {Lavallee, MJ},
Title = {The erosion of Western hegemony by Russia and Turkey: the tripartite
   relations of US, Russia, and Turkey},
Booktitle = {END OF WESTERN HEGEMONIES?},
Series = {Series in Politics},
Year = {2022},
Pages = {145-166},
Abstract = {This chapter will investigate the causes of the diminishing of US
   hegemonic influence over the Turkey-Russia nexus. Turkey and Russia
   increasingly contest the US influence through cooperation in the Black
   Sea and the Middle East. This chapter will consider the factors which
   enabled and fostered this erosion of US influence. It will argue that
   the Iraq invasion had a disruptive impact on the post-Cold War
   international system, which encouraged Turkey and Russia to develop and
   follow independent foreign policy orientations. In order to demonstrate
   how the post-Cold War international system constrains the US hegemonic
   influence, this chapter will rely on a neoclassic realist framework. It
   will investigate how the Iraq invasion acted as a critical juncture in
   accelerating this trend by tracking and analyzing the evolution of the
   tripartite relations of the US, Russia, and Turkey. This study will rely
   on official documents, including trade data, leaders' speeches, and
   treaties.},
