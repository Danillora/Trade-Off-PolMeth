Title = {Analysis of Corporate Governance, Organisational Resilience and
   Sustainable Practices Developed by Brazilian Companies during the
   COVID-19 Pandemic: An Exploratory Study},
Journal = {WORLD},
Year = {2022},
Volume = {3},
