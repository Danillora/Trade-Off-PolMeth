@incollection{ WOS:001329617700006,
Author = {Korhonen, Juho},
Editor = {Lavallee, MJ},
Title = {The liberal international order from the standpoint of the Russian,
   Soviet Empires and post-socialism},
Booktitle = {END OF WESTERN HEGEMONIES?},
Series = {Series in Politics},
Year = {2022},
Pages = {117-144},
Abstract = {This chapter assesses the role of Russian and Soviet Empires as
   co-constitutive in the history and making of the liberal international
   order (LIO). I will contend that the LIO does not refer solely to a
   given configuration of the international arena. It is also an
   ideological and hegemonic conceptual framework that influences how we
   perceive, analyze, and classify actors and units of world politics.
   From this perspective the LIO is understood simultaneously as both a
   world order as well as the knowledge of it. As such, analysis of it
   should take into account its constitutive others and the classificatory
   struggles that have lent it power. This also opens on forms of
   contestations which, while not deriving from the LIO itself, do not
   simply challenge the hegemonic posture of the LIO with regard to power
   relations or as knowledge of these relations, but which challenges the
   very relation between power and knowledge.
   For instance, the current perceived crisis of the LIO has framed the
   discussions as one of `threats' like migration, populist politics, and
   institutional legitimacy. By constricting analytical units, it results
   in problematic causal relations exemplified by statements like this:
   ``Americans and others may not notice the security and prosperity that
   the liberal order provides until they are gone-but by then, it may be
   too late.{''}1 From a sociological perspective, such temporally
   totalizing analytic hints at a strong relation between distinction in
   political power and analytical thought, i.e. an order whose reproduction
   is secured by the reproduction of knowledge of it. This relation between
   the LIO and our knowledge of it provides a fruitful ground for a
   comparative-historical analysis that contextualizes that contradiction
   and historicizes its context.},
Publisher = {Vernon Press},
Address = {1000 N West Street, Ste 1200, Wilmington, DE, UNITED STATES},
