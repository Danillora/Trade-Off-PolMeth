@article{ WOS:000830113400004,
Author = {Jernnas, Maria and Lovbrand, Eva},
Title = {Accelerating Climate Action: The Politics of Nonstate Actor Engagement
   in the Paris Regime},
Journal = {GLOBAL ENVIRONMENTAL POLITICS},
Year = {2022},
Volume = {22},
Number = {3},
Pages = {38-58},
Month = {AUG 1},
Abstract = {The 2015 Paris Agreement is often depicted as a turning point for global
   climate governance. Following years of diplomatic gridlock, it laid the
   foundations for a new global climate regime that invites states to
   partner with nonstate actors in the transition to the low-carbon
   society. This article critically examines the political rationalities
   that inform the pluralization of climate politics after Paris and the
   turn toward cooperative modes of governing. Drawing on an analysis of
   initiatives led by the United Nations Framework Convention on Climate
   Change that were launched to engage nonstate actors in the evolving
   Paris regime, we identify a global governmentality that mobilizes
   nonstate actors as active and responsible partners in the quest for
   rapid and deep decarbonization. In its search for cooperative and
   efficient forms of problem management, we argue, this form of rule
   nurtures a global space free from friction and opposition where
   businesses, investors, and industry are elevated as the real partners of
   government.},
