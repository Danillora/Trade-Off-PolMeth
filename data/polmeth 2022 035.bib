@incollection{ WOS:001329617700008,
Author = {Korhonen, Pekka},
Editor = {Lavallee, MJ},
Title = {Passing of the idea of Japan as a bridge between the East and the West},
Booktitle = {END OF WESTERN HEGEMONIES?},
Series = {Series in Politics},
Year = {2022},
Pages = {167-189},
Abstract = {The end of Western hegemonies is best seen in the peripheries of the
   world system led by the United States, such as in East Asia, where
   countries like South Korea, Japan, Australia, and New Zealand used to be
   categorized as members of the Western alliance in a politico-military
   sense. Japan has been in a special role in this because of its intimate
   relations with Europe and the United States since the late nineteenth
   century. Its situation has not been clear cut; rather than the West
   itself, a usual Japanese categorization of the nation has been that of a
   `bridge between the East and the West,' implying a location in the
   West's periphery, but a central position in Asia. This situation lasted,
   with twists and turns, for more than a century, until China, during the
   past two decades, has replaced Japan as the East Asian biggest economy
   and political central state. The paper analyses the theoretical and
   argumentative dimensions of this change. The source material is
   historical Japanese scholarly arguments and modern Japanese and Chinese
   foreign political rhetoric.},
