@article{ WOS:000746675300001,
Author = {de Freitas, Marco Roque and de Carvalho, Joao Soeiro},
Title = {Performing a culture, staging the revolution: Choral singing and
   traditional music as nation-building tropes in post-colonial Mozambique},
