@article{ WOS:000763264600001,
Author = {Liu, Lina},
Title = {Network analysis: a cross-country comparison of peer attention among EU
   countries in response to China with the media data},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
