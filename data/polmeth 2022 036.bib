@article{ WOS:000884293400005,
Author = {Gard-Murray, Alexander},
Title = {De-risking Decarbonization: Accelerating Fossil Fuel Retirement by
   Shifting Costs to Future Winners},
Journal = {GLOBAL ENVIRONMENTAL POLITICS},
Year = {2022},
Volume = {22},
Number = {4},
Pages = {70-94},
Month = {NOV 10},
Abstract = {Fossil fuel-rich developing countries should be included in climate
   change mitigation. But they need money, above what rich countries have
   been willing to provide, to do so. I argue that existing approaches to
   international and intergenerational transfers have failed to bridge the
   gap between developing and developed countries' interests. I present a
   new model in which the costs of present climate action are distributed
   not according to present or historical wealth but according to future
   wealth. I demonstrate the basic feasibility of this approach with an
   implementation I call income-adjusted guarantees. I argue that such an
   arrangement can help accelerate fossil fuel retirement in developing
   countries and break the deadlock over global climate financing.},
