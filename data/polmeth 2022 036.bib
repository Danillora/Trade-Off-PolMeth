@article{ WOS:000813937900005,
Author = {Laksmana, Evan A.},
Title = {Remodelling Indonesia's Maritime Law Enforcement Architecture:
   Theoretical and Policy Considerations},
Journal = {CONTEMPORARY SOUTHEAST ASIA},
Year = {2022},
Volume = {44},
Number = {1},
Pages = {122-149},
Month = {APR},
Abstract = {Indonesia, the world???s largest archipelagic state, tends to punch
   below its weight in regional maritime affairs. Part of the explanation
   for this puzzle lies in Indonesia???s under-developed and ineffective
   maritime governance system. This article seeks to develop ways to
   overhaul Indonesia???s maritime governance by focusing on one specific
   but potentially strategic area: maritime law enforcement in the
   country???s Exclusive Economic Zone (EEZ). The article analyses the
   institutional challenges surrounding Indonesia???s EEZ maritime law
   enforcement and develops theoretical models and policy options drawn
   from the experiences of other Asian maritime states. It argues that
   Indonesia needs to move from its current ???division of labour??? model,
   where there are multiple agencies with multiple tasks, to a ???unified
   command??? model, where different maritime assets are unified under a
   single operational command. The article suggests that Indonesia should
   consider a ???dual agency??? architecture where the Indonesian Navy and
   the Maritime Security Agency become the two primary EEZ maritime law
   enforcement authorities. The article aims to contribute to broader
   debates surrounding maritime security governance and Indonesia???s
   future trajectory as a maritime power in the Indo-Pacific.},
