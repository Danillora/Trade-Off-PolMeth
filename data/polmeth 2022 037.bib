@article{ WOS:001121043900001,
Author = {Shaiq, Muhammad Asef and Barati, Ali Akbar and Kalantari, Khalil and
   Asadi, Ali},
Title = {Dimensions of Poverty in Kunduz Province of Afghanistan},
