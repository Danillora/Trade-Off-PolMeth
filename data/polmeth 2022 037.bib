@incollection{ WOS:001329617700011,
Author = {Ajagbe, Samsondeen},
Editor = {Lavallee, MJ},
Title = {Knowledge production and the linguistic market place: the case of
   Nigeria},
Booktitle = {END OF WESTERN HEGEMONIES?},
Series = {Series in Politics},
Year = {2022},
Pages = {247-271},
Abstract = {Using Nigeria as a case study, this chapter explores knowledge
   production in a multilingual postcolonial setting, with a particular
   emphasis on Nigerian Pidgin, a widespread contact language of mixed
   European and African origin. In its theoretical approach, the study is
   informed by Bourdieu's studies of the linguistic marketplace and on
   Mignolo's critique of Western hegemony in education and knowledge
   production. In addition, I will review a few major (socio)linguistic
   studies on Nigerian Pidgin. This study will also be based on language
   biographical interviews with Nigerian informants. These interviews were
   part of the research project ``West African Englishes on the Move: New
   Forms of English as a Lingua Franca (ELF) in Germany.{''}
   The analysis will make evident that both Nigerian Pidgin and English are
   unifying languages in the Nigerian context, but that they play their
   unifying roles in very different ways. The economic and educational
   systems inherited from the colonial era have pushed the English language
   to the top of Nigeria's language hierarchy. However, Pidgin's
   progressive normalization challenges this hegemony across a wide range
   of public domains and in all social echelons. Pidgin now assumes a
   significant role, complementary to English in communication. Beyond the
   questioning of a Western language's enduring privilege, this phenomenon
   points to a crisis of Western cultural hegemony.
   While Pidgin is a useful means of communication for people of different
   ethnic and linguistic groups in Nigeria, comparing this language
   unfavorably to English remains current among Nigerians. This negative
   perception is an offshoot of colonialism that propped language hierarchy
   in the colonies and left behind a deep-seated assumption that English
   would be the only legitimate language for official use by the people.
   Promotion of the English language in Nigeria, and its enduring hegemony,
   was part of a process connected to colonial power and to the oversea
   expansion of capitalism, a process Walter Mignolo has referred to as the
   `coloniality of power.' Could the boom in Pidgin facilitate `knowledge
   emancipation' through shaping people's perceptions and actions about
   being Nigerian?},
Publisher = {Vernon Press},
Address = {1000 N West Street, Ste 1200, Wilmington, DE, UNITED STATES},
Type = {Article; Book Chapter},
Language = {English},
Affiliation = {Ajagbe, S (Corresponding Author), Univ Freiburg, Dept English, Freiburg, Germany.
