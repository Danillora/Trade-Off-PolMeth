@incollection{ WOS:001329617700010,
Author = {El Younssi, Anouar},
Editor = {Lavallee, MJ},
Title = {Islam and Muslims: a threat to, or a victim of, Western hegemony?},
Booktitle = {END OF WESTERN HEGEMONIES?},
