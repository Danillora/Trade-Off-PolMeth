@incollection{ WOS:001329617700012,
Author = {Muhonen, Riikkamari Johanna},
Editor = {Lavallee, MJ},
Title = {Soviet educational cooperation with the developing world during the Cold
   War: Contesting Western hegemony?},
Booktitle = {END OF WESTERN HEGEMONIES?},
Series = {Series in Politics},
Year = {2022},
Pages = {273-294},
Abstract = {This chapter looks at Soviet internationalism in the sphere of higher
   education during the Cold War. Higher education in the newly independent
   developing world was a thoroughly Western-dominated field due to the
   colonial past of these areas. Soviet Union was keen to contest this
   hegemony by offering opportunities for foreign students to gain degrees
   in Soviet universities. The education provided was similar to Western
   education in terms of quality, but different in the values it promoted.
   This chapter analyzes the processes of student selection, contents of
   study programs and outcomes of the Soviet educational cooperation
   project in the 1960s and 1970s.},
