@article{ WOS:000769885800001,
Author = {Petrovic, Nikola and Raos, Viseslav and Fila, Filip},
Title = {Centrist and Radical Right Populists in Central and Eastern Europe:
   Divergent Visions of History and the EU},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
