@incollection{ WOS:001329617700003,
Author = {Mihatsch, Moritz and Mulligan, Michael},
Editor = {Lavallee, MJ},
Title = {Sovereignty in Africa and the specter of Wilson},
Booktitle = {END OF WESTERN HEGEMONIES?},
Series = {Series in Politics},
Year = {2022},
Pages = {35-61},
Abstract = {Sovereignty is at the core of many contemporary political debates in
   Africa. The chapter argues that sovereignty, while often portrayed as
   immutable, fixed, or clearly defined, is in fact a construct dependent
   on historical and geographical context. However, in the case of Africa,
   the Western conception of sovereignty, which the chapter refers to as `
   Wilsonian sovereignty', was hegemonically imposed through the process of
   colonization and decolonization. The chapter analyzes this process and
   how sovereignty as a hegemonic norm has become natural, disembodied, and
   internalized, still shaping post-colonial developments on the continent.
   Good governance discourses, as well as recent migration policies,
   continue to enforce hegemonic sovereignty. Nevertheless, since the end
   of the Cold War, Wilsonian sovereignty has become increasingly
   contested, with alternative imaginaries starting to emerge.},
