@article{ WOS:000754553100002,
Author = {Chan, Nicholas},
Title = {The Paris Agreement as Analogy in Global Environmental Politics},
Journal = {GLOBAL ENVIRONMENTAL POLITICS},
Year = {2022},
Volume = {22},
Number = {1},
Pages = {4-11},
Month = {FEB 4},
Abstract = {The Paris Agreement is increasingly being used as an analogy in global
   environmental politics to discuss issues beyond climate change. This
   Forum article explores the two main ways in which this analogy has been
   discursively employed: as a symbol of diplomatic success to be emulated
   and as a model for institutional treaty design. It illustrates the
   broader meanings associated with the Paris Agreement, reflecting its
   preeminent public and political profile among environmental issues just
   a few years into its history and its potential significance in shaping
   subsequent global environmental negotiations.},
