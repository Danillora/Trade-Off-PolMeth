@article{ WOS:000837631500001,
Author = {Ciot, Melania-Gabriela and Ghidiu, Iulia-Anamaria},
Title = {The influence of European and American non-state actors on the
   integration of the transatlantic market after the Cold War - A Romanian
   perspective},
