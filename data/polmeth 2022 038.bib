@article{ WOS:000851329000001,
Author = {Korosteleva, Elena and Petrova, Irina},
Title = {Power, People, and the Political: Understanding the Many Crises in
   Belarus},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2023},
