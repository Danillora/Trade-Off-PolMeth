@article{ WOS:000920342500003,
Author = {Sakal, Halil Burak},
Title = {The Risks of Hydro-Hegemony: Turkey's Environmental Policies and Shared
   Water Resources in the South Caucasus},
Journal = {CAUCASUS SURVEY},
Year = {2022},
Volume = {10},
