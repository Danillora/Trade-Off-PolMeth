@article{ WOS:000827272600001,
Author = {Correa-Cabrera, Guadalupe and Schaefer, Kathleen Blair},
Title = {Notes on a Perilous Journey to the United States: Irregular Migration,
   Trafficking in Persons, and Organized Crime},
Journal = {LATIN AMERICAN POLITICS AND SOCIETY},
Year = {2022},
Volume = {64},
Number = {3},
Pages = {142-160},
Month = {AUG},
Abstract = {This article examines the interplay between transnational criminal
   actors (essentially human smugglers), local crime groups, and drug
   cartels in the phenomenon of trafficking in persons coming from Central
   America along Mexico's eastern migration routes. The analysis focuses on
   sex trafficking, compelled labor for criminal activities, and other
   forms of labor trafficking. Through qualitative research that involved
   336 semistructured interviews with migrants, activists, and other
   persons familiar with the subject, this work describes and maps
   trafficking trends throughout Mexico's eastern migration routes. It also
   sheds light on the role of drug cartels and other crime groups (local
   and transnational) in these activities.},
