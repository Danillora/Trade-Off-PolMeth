@article{ WOS:000766699500010,
Author = {Pang, Xun and Liu, Licheng and Xu, Yiqing},
Title = {A Bayesian Alternative to Synthetic Control for Comparative Case Studies},
Journal = {POLITICAL ANALYSIS},
