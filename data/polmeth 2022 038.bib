@article{ WOS:000821454300004,
Author = {Kamaruddin, Nurliana and Huda, Mohd Ikbal Mohd and Deivasagayam, Aaron
   Denison and Xia, Menghu},
Title = {Engaging North Korea: Malaysia's Quest for the Middle Path},
Journal = {ISSUES \& STUDIES},
Year = {2022},
Volume = {58},
Number = {02},
Month = {JUN},
Abstract = {The assassination of Kim Jong Nam in 2017 brought about a significant
   change in the diplomatic relationship between Malaysia and North Korea.
   Although diplomatic relations between the two countries were downgraded
   due to the incident, current changes would likely encourage the
   resumption of diplomatic ties. This study applies a poliheuristic
   decision-making analysis of Malaysia's diplomatic relations with North
   Korea. It argues that Malaysia's choice to establish diplomatic ties
   with North Korea was made consistent with the strong sense of neutrality
   that has shaped Malaysia's foreign policy. Despite the increased
   security threat North Korea is perceived as globally, it was not until
   the assassination attempt that there was any regression in diplomatic
   relations between the two countries. Poliheuristic theory can also be
   utilized to explain why Malaysia will most likely proceed with reopening
   the Malaysian Embassy in Pyongyang as this is an alternative that would
   present Malaysia with continued gain in consideration of both its
   emphasis on diplomacy and neutrality in its foreign policy.},
