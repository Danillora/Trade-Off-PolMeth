@incollection{ WOS:001329617700004,
Author = {Yildirim, Yavuz},
Editor = {Lavallee, MJ},
Title = {Dealing with Western Hegemony: Turkish politics in search of the level
   of contemporary civilizations},
Booktitle = {END OF WESTERN HEGEMONIES?},
Series = {Series in Politics},
Year = {2022},
Pages = {63-90},
Abstract = {This chapter will explore an ever-controversial issue based on the
   Turkish case: is it possible to root (liberal) Western democracy, its
   institutions, and procedures in a non-typical Western society? Through
   landmarks, events, figures, and phenomena, it will analyze
   Turkish-Western relationships and the hegemonic patterns that have
   characterized them since the late nineteenth century. This study's main
   argument is that the Western hegemony over the Turkish democratization
   process did integrate the country into international capitalist politics
   but did not help turn Turkey into a consolidated democracy. The equation
   between capitalism and democracy was at the very root of the Western
   model of liberal democracy. However, in the post-Cold War setting and
   especially in the context of intensified globalization, these phenomena
   tend to be conflictual. Democracy also is made up of discussions between
   citizens and articulations with equal others, to borrow Laclau and
   Mouffe's concept. It is a system that people can alter and improve
   directly through social movements or institutions. Democratic society
   becomes hegemonic through these processes. This is especially
   problematic in the case of Turkey's relationship with the West. The
   emergence of democracy in Turkey, as elsewhere, occurred against a
   background of Western influence and interference. The chapter will apply
   the concept of hegemony to Western-Turkish relations but will use it to
   make sense of internal political shifts.},
Publisher = {Vernon Press},
Address = {1000 N West Street, Ste 1200, Wilmington, DE, UNITED STATES},
