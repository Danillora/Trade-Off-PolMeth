@article{ WOS:000763803400001,
Author = {Esteban, Mario and Armanini, Ugo},
Title = {The limits of China's engagement in the Mediterranean: Role conflict and
   emerging mistrust in Spain-China relations},
Journal = {MEDITERRANEAN POLITICS},
Year = {2023},
Volume = {28},
Number = {5, SI},
Pages = {812-833},
Month = {OCT 20},
Abstract = {Spain has recently endorsed a more critical stance towards China. This
   article explains this posture through the application of role theory and
   the concept of (mis)trust. Role conflict has intensified from the
   interplay between China's new roles of great power and economic and
   technological competitor, Spain's higher expectations towards China, and
   Spain's self-identified role, greatly influenced by its relations with
   its traditional European and US allies. This has generated mistrust, and
   Spanish authorities have shown greater concerns about Chinese behaviour.
