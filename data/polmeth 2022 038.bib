@article{ WOS:000746459300001,
Author = {Peshkopia, Ridvan and Trahan, Adam},
Title = {Does the EU message impact public attitudes toward the death penalty in
   EU membership-aspiring countries? Lessons from Albania},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2023},
Volume = {31},
Number = {3, SI},
Pages = {754-767},
Month = {JUL 3},
Abstract = {The dominant knowledge in the extant literature is that citizens in EU
   member countries are not receptive toward the EU message, but rather
   perceive it from a domestic politics perspective. However, recent
   research has shown that people in EU membership-aspiring countries from
   Southeastern Europe might not be as intransigent toward the EU message.
   What that literature has not highlighted thus far is the type of EU
   message. Whereas EU normative messages might hold sway among people's
   normative attitudes, they might not affect their policy preferences. We
   argue that citizens are receptive to the EU's support of death penalty
   abolition and tend to adjust their normative views about the death
   penalty accordingly. In an original experimental survey conducted in
   Albania, we find that respondents who received an EU framing of the
   death penalty exhibited lower levels of personal support for the death
   penalty. However, we did not find evidence that this result holds in
   regards to levels of support for the reinstatement of the death penalty,
   which remained similar among respondents who received and did not
   receive the EU framing.},
