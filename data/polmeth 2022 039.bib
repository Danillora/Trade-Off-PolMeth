@article{ WOS:000847594400001,
Author = {Sorokina, Anna and Kasamara, Valeria and Maximenkova, Marina},
Title = {Between Hard and Soft Power: British and Russian Youth on National Pride},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2023},
Volume = {51},
Number = {5, SI},
Pages = {1164-1188},
Month = {SEP},
Abstract = {This study examines the collective memory of British and Russian youth.
   We used the results of a comparative survey conducted among Russian and
   British students. The study focuses primarily on pride in the collective
   memory of young people with the aim of analyzing the category of pride
   among young people across several dimensions. First, we look at the
   qualitative content of national pride: pride in the realization of tasks
   related to ``soft power{''} (for example, culture, education, sports),
   and pride in manifestations of ``hard power{''} (for example, pride in
   military victories or power politics). Second, we analyze the temporal
   localization of national pride: where are the main events,
   personalities, and phenomena study participants take pride in, both in
   the past and in the present. Third, an important element of
   understanding pride in a country is the relationship between pride and
   shame: what events are mentioned more often: shameful or
   pride-inspiring.},
