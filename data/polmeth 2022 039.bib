@article{ WOS:000793048300001,
Author = {Klatt, Martin},
Title = {Schleswig and Non-Territorial Autonomy - The Territorial Trap?},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
