@article{ WOS:000784530200001,
Author = {Bielewska, Agnieszka},
Title = {Place Relations of Mobile People: National and Local Identification of
   Highly Skilled Migrants in Wroctaw, Poland},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2023},
Volume = {51},
Number = {6},
Pages = {1302-1318},
Month = {NOV},
Abstract = {This article discusses the difference between the construction of
   national and local identifications related to the new place of
   residence. It shows that local identification is more inclusive than
   national, and therefore may be a key to strengthening social cohesion.
   National and local identities can both be seen as forms of place
   identification (i.e., of spatial or territorial identity). The article
   builds on qualitative research on highly skilled migrants living in
   Wroclaw, Poland. The empirical data shows that these migrants would
   rather obtain a city identification and call themselves Wroclawianie
   (inhabitants of Wroclaw), and do not want, or only partially want,
   Polish national identity. Living in and experiencing Wroclaw makes them
   feel like insiders, while experiencing Poland positions them as
   outsiders. While national identity is built around the difference
   between ``us{''} and ``them{''}, local identity focuses on gaining
   knowledge about the particularity of a place and therefore allows for
   acceptance of heterogeneity and is easier for migrants to obtain.},
