@article{ WOS:001296528400008,
Author = {Cardozo-Uzcategui, Alejandro},
Title = {From popular empowerment to criminal governance: discourse and praxis in
   Venezuela (1999-2022)},
Journal = {REVISTA CIENTIFICA GENERAL JOSE MARIA CORDOVA},
Year = {2022},
Volume = {20},
Number = {40},
Month = {OCT-DEC},
Abstract = {This article analyzes popular empowerment and its Latin American
   understanding of the concept. Based on a casuistic and discursive
   analysis of Hugo Ch \& aacute;vez and Chavismo, it examines the
   epistemological and empirical errors in the use of this concept since
   the dawn of the Bolivarian or Chavista revolution to understand how the
   transition towards a model of participatory democracy and, therefore, of
   popular empowerment results in criminal governance. This result derives
   from the classist nature of Chavez's discourse, in which the bourgeois
   condition of middle and upper sectors of Venezuelan society is
   criminalized.},
