@article{ WOS:000834623000001,
Author = {Poniedzialek, Jacek},
Title = {Florian Znaniecki's Culturalistic Sociology of Nation},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2023},
Volume = {51},
Number = {3, SI},
Pages = {665-683},
Month = {MAY},
Abstract = {The aim of this article is to characterize the culturalist theory of the
   nation by Florian Znaniecki. Opposing the sociological theory dominated
   by Western, particularly Anglo-Saxon, thinkers, Znaniecki rejected the
   view of the nation as a state society. He believed that the nation is a
   type of community constituted by a specific culture that is created by
   its intellectual leaders. To derive his findings, he used the knowledge
   gained from the experiences of the nations of Central and Eastern
   Europe. He believed that their specific history required the development
   of a sociological theory that was adequate for such research, not
   dominated by the findings of Western sociology.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {EDINBURGH BLDG, SHAFTESBURY RD, CB2 8RU CAMBRIDGE, ENGLAND},
