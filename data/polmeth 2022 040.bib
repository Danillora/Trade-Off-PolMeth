@article{ WOS:001315149800005,
Author = {Torres, Parcival Pena and Betancourt, Claritza Marles and Alfonso,
   Orlando Valera},
Title = {Scenario planning as a tool for peace-building in Caqueta},
Journal = {REVISTA CIENTIFICA GENERAL JOSE MARIA CORDOVA},
Year = {2022},
Volume = {20},
Number = {37},
Month = {JAN-MAR},
Abstract = {This article seeks to determine future scenarios as an alternative for
   peace-building in Caquet \& aacute;, one of the regions most affected by
   the armed conflict in Colombia and facing enormous challenges in the
   post-conflict period. To this end, it presents a descriptive study with
   a mixed approach based on the phases of the prospective cross-impact
   matrix technique, involving a sample of regional development actors from
   the knowledge, power, productive, and community sectors. The results
   reveal that future events in the dimensions of institutional capacities,
   illicit crops issues, victims of the conflict, and the influence of
   higher education, constitute the most dynamic scenarios for
   peace-building in the region.},
