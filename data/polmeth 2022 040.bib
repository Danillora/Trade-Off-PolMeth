@article{ WOS:001295432500004,
Author = {Patino, Lorena Andrea Erazo and Camero, Faiver Coronado},
Title = {The relationship between conflict and development in Colombia predicated
   on a public policy precedent},
Journal = {REVISTA CIENTIFICA GENERAL JOSE MARIA CORDOVA},
Year = {2022},
Volume = {20},
Number = {39},
Month = {JUL-SEP},
Abstract = {A BsTRACT . It has been traditionally established that the relationship
   between armed conflicts and development is inversely proportional; the
   greater the conflict's intensity, the lower the country's development.
   However, the Colombian case shows that this is not necessarily true.
   This article examines the National Rehabilitation Plan's precedent to
   understand how public policies developed in the context of the conflict
   have helped identify territorial characteristics that show a nexus
   between development and conflict in the areas most affected by violence.
   Thus, this precedent of differential development policies in Colombia is
   analyzed to understand their scope, territorial approach, and
   limitations, given the territory's specific economic, social, and
   geographic dynamics.},
Publisher = {ESCUELA MILITAR CADETES GENERAL JOSE MARIA CORDOVA},
