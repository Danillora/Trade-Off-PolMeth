@article{ WOS:000919937200009,
Author = {Soare, Sorina and Tufis, Claudiu D.},
Title = {Saved by the diaspora? The case of the Alliance for the Union of
   Romanians},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2023},
Volume = {22},
Number = {1, SI},
Pages = {101-118},
Month = {MAR},
Abstract = {This article analyses and explains the rapid electoral success of an
   emergent populist radical right party in Romania-the Alliance for the
   Union of Romanians (AUR). AUR's definition of `a transnational people'
   relates to a multi-layered community-native Romanians within Romania,
   co-ethnic communities in neighbouring countries, and Romanians living
   and working abroad-that create, together, a Romanianness in need of
   representation, and preservation. By focusing on AUR's definition of
   `the people' and on (trans)national mobilization, we unwrap a new
   dimension of populism which refers to broader fields of connectedness
   across borders in which bottom-up and top-down dynamics co-exist, acting
   as rhetorical and operational openings for the party's development and
   success in the homeland arena.},
