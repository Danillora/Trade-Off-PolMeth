@article{ WOS:000962007600009,
Author = {Paquin, Stephane and Plouffe-Malette, Kristine},
Title = {The WTO and the Covid-19?Vaccine Apartheid?: Big Pharma and the
   Minefield of Patents},
Journal = {POLITICS AND GOVERNANCE},
