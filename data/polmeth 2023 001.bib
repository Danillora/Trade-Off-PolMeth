@article{ WOS:000911890100001,
Author = {Jansa, Joshua M. M. and Ringsmuth, Eve M. M.},
Title = {Disrupted Learning about Democracy: Instructor Strategies for Navigating
   Temporary Modality Shifts},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2023},
Volume = {56},
Number = {2},
Pages = {315-320},
Month = {APR},
Abstract = {Within-semester shifts in course modality in response to pandemics,
   weather, or accommodation for travel and health are increasingly common
   and can interrupt student learning. We tracked temporary modality
   changes across 10 sections of ``Introduction to American Government{''}
   to examine the extent to which instructors have tools to help students
   successfully navigate such changes and mitigate learning loss. We find
   that students rated instructors' handling of shifts well if they made
   course material engaging, communicated clearly, and effectively used
   technology. The analysis suggests that instructors can mitigate the
   impact of unplanned changes to modality on students' learning when there
   are three or fewer shifts during a semester.},
