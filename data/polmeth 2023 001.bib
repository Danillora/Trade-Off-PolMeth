   Dentler, Klara, GESIS Leibniz Inst Social Sci, Unter Sachsenhausen 6-8, D-50667 Cologne, Germany.},
DOI = {10.1016/j.electstud.2023.102699},
EarlyAccessDate = {NOV 2023},
Article-Number = {102699},
