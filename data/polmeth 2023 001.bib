@article{ WOS:000963929100001,
Author = {Becker, Karin L. and Gilbert, Danielle and Bezerra, Paul},
Title = {Promoting College Reading Completion and Comprehension with Reading
   Guides: Lessons Learned Regarding the Role of Form, Function, and
   Frequency},
Journal = {JOURNAL OF POLITICAL SCIENCE EDUCATION},
Year = {2024},
Volume = {20},
Number = {1},
Pages = {14-30},
Month = {JAN 2},
Abstract = {College faculty often struggle with getting their students to read
   assigned materials. Even if students do read, they may not read closely
   or critically. Not only does the lack of effective reading undermine
   understanding, but it also hampers class discussions and engagement. To
   promote close and critical reading in a required, upper-division
   International Security Studies course, we offered optional reading guide
   worksheets as tools to increase students' reading comprehension and
   completion. While our reading guides helped students focus on key terms
   and lesson objectives, flaws in our implementation produced a lack of
   perceptual value and extrinsic motivation in using the reading guides.
   In this article, we offer our lessons learned from the use of reading
   guides, focusing on their form, function, and frequency. These findings
   equip faculty with useful guidance in how to design and implement
   effective reading guides across the disciplines.},
