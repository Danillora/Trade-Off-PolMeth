@article{ WOS:001168003100001,
Author = {Garsten, Bryan},
Title = {The great reconciliation of reason and myth},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2025},
Volume = {28},
Number = {2},
Pages = {361-365},
Month = {FEB 23},
Abstract = {This paper examines the suggestion in Tae-Yeoun Keum's Plato and the
   Mythic Tradition in Political Thought that myth and reason can be
   reconciled in a practice of philosophy. If that practice remains
   centered around critical reasoning, and myth is resistant to such
   reasoning, then Keum's argument seems to suggest that resistance to
   critical reasoning must be somehow necessary to critical reasoning. How
   to make sense of this apparent contradiction? Exploring the ambiguous
   relationship between myth and truth, I explore several ways of
   understanding Keum's contribution.},
