@article{ WOS:001118313500001,
Author = {Gorusuk, Levent},
Title = {A hundred-years legacy of Turkish Democracy: the impact of democracy by
   rationalism in politics under Atatürk and Erdoğan},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2023},
Volume = {17},
Number = {4, SI},
Pages = {365-387},
Month = {DEC},
Abstract = {The present study aims to trace the political theory of Michael
   Oakeshott, one of the leading critics of rationalism in politics in the
   20th century, both in the founding stage of the Modern Republic and in
   the policies of the AK Party Government, as its final stage. Political
   rationalism, which Oakeshott is skeptical of, is an understanding in
   which the traditional is destroyed and a new political order and society
   is created rather than repaired. Political rationalism, which has an
   understanding of abstraction, universality and perfectionism, especially
   turns into a politics of absolute faith and becomes a unity of
   enterprise in which law and freedom are suspended through the unity of
   powers. From this perspective, the rationalist policies of the Ataturk
   and Erdogan eras are analyzed. In particular, the transformation of the
   concept of unity of powers into the politics of faith, the existence of
   new political orders (Republic, Presidential Government System) and new
   social structures (Asim's generation instead of Kemalist generation)
   reveals the importance of rationalism in politics in order to understand
   the structure of politics in Turkey.
   Die vorliegende Studie zielt darauf ab, die politische Theorie von
   Michael Oakeshott, einem der fuhrenden Kritiker des Rationalismus in der
   Politik des 20. Jahrhunderts, sowohl in der Grundungsphase der Modernen
   Republik als auch in der Politik der AKP-Regierung als deren Endphase
   nachzuzeichnen. Der politische Rationalismus, dem Oakeshott skeptisch
   gegenubersteht, ist ein Verstandnis, in dem das Traditionelle zerstort
   und eine neue politische Ordnung und Gesellschaft geschaffen wird,
   anstatt es zu reparieren. Der politische Rationalismus, der ein
   Verstandnis von Abstraktion, Universalitat und Perfektionismus vertritt,
   verwandelt sich vor allem in eine Politik des absoluten Glaubens und
   wird zu einer Unternehmungseinheit, in der Recht und Freiheit durch die
   Einheit der Gewalten aufgehoben werden. Aus dieser Perspektive wird die
   rationalistische Politik der ara Ataturk und Erdogan analysiert.
   Insbesondere die Umwandlung des Konzepts der Einheit der Gewalten in
   eine Politik des Glaubens, die Existenz neuer politischer Ordnungen
   (Republik, Prasidialregierungssystem) und neuer sozialer Strukturen
   (Asims Generation anstelle der kemalistischen Generation) offenbart die
   Bedeutung des Rationalismus in der Politik, um die Struktur der Politik
   in der Turkei zu verstehen.},
