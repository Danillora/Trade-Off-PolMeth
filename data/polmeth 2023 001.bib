@article{ WOS:001058129300001,
Author = {Wayne, Carly and Damann, Taylor J. and Fachter, Shani},
Title = {The Holocaust, the Socialization of Victimhood and Outgroup Political
   Attitudes in Israel},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2025},
Volume = {58},
Number = {14},
Pages = {3179-3214},
Month = {DEC},
Abstract = {How does historical victimization and its memorialization impact
   present-day outgroup attitudes in conflict-riven societies? This study
   explores this question using a survey experiment with a representative
   sample of 2000 Jewish Israelis-half of whom are direct descendants of
   Holocaust survivors-and a content analysis of 98 state-approved school
   textbooks, examining how histories of victimization become socialized
   and shape political attitudes. We find that, in Israel, family
   victimization during the Holocaust plays surprisingly little role in
   shaping present-day attitudes toward outgroups. Rather, perceived
   historical victimization of the Jewish and Israeli people is broadly
   socialized among the Israeli public and is a stronger predictor of
   outgroup (in)tolerance. These findings shed light on the power of
   societal victimhood narratives-even in the absence of personal family
   histories of victimization-to shape political attitudes in conflict
   contexts, with long-term implications for intergroup cooperation and
   conflict.},
