@article{ WOS:001020648200010,
Author = {Cichocki, Marek},
Title = {Asymmetry without Centre and Periphery The Unreconciled German-Polish
   Relationship},
Journal = {OSTEUROPA},
Year = {2023},
Volume = {73},
Number = {1-2},
Pages = {143+},
Abstract = {The relationship between Germany and Poland was long considered one of
   centre and periphery. Bilateral reconciliation, the transformation of
   the Polish economy and politics, as well as embedment in European
   integration were supposed to lead successively to overcoming
   developmental differences and enabling the convergence of both
   countries. This model is obsolete. In economic, European, and security
   policy, Poland is now pursuing its own interests, and these collide with
   those of Germany. German-Polish relations are characterized by
   ``strategic asymmetries{''}. Russia's war against Ukraine is reminding
   both ``central European powers{''} what is at stake: the security of
   Europe.},
