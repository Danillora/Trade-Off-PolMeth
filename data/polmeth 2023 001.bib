@article{ WOS:001095969100001,
Author = {Graham, Hannah},
Title = {Diary of an SNP First Minister: A Chronopolitics of Proximity and
   Priorities},
Journal = {POLITICAL QUARTERLY},
Year = {2023},
Volume = {94},
Number = {4},
Pages = {547-555},
Month = {OCT},
Abstract = {This article provides a content analysis of Nicola Sturgeon's first
   ministerial diaries in the final two years of her leadership (April
   2021-March 2023). As first minister, to whom and what did she give her
   time-which issues and interest groups had access? Which didn't? Or, who
   and what may be missing? An audience with a national leader can be
   indicative of priorities and potential for influence-as can its absence.
   The lens of chronopolitics-the politics of time-is used here to consider
   twenty-four months of diaries, with 681 entries. Some key social and
   political issues in Scotland were kept away from the FM's meetings and
   delegated to other ministers to oversee, whereas other issues appear to
   be signature priorities, including climate and the environment, economy
   and finance, culture, and health and Covid-19.},
