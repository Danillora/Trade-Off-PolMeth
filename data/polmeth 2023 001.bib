@article{ WOS:001091423300007,
Author = {Gomez Diaz, Abelardo},
Title = {Contamination Effects in Mixed-member Electoral Systems: Qualitative
   Evidence from the Mexican Case},
Journal = {POLITICA Y GOBIERNO},
Year = {2023},
Volume = {30},
Number = {2},
Month = {OCT},
Abstract = {In mixed-member electoral systems, there is an interaction between the
   single-seat district arena and the proportional representation arena.
   The literature suggests that this interaction (or contamination) raises
   the number of parties in the single-seat district arena above what
   Duverger's Law and the M+1 rule suggest. However, the literature is
   exclusively quantitative and has enough limitations to doubt if what is
   observed is actually contamination, and if contamination is actually
   responsible for the increase in the number of parties. This contribution
   aims to reinforce the literature with evidence from a series of
   interviews with party strategists and coordinators in Mexico. The
   results indicate that, even with other factors at play, contamination
   exists as believed: parties' behaviour in the single-seat district arena
   is based on what they project in the proportional representation arena.},
Publisher = {CENTRO DE INVESTIGACION Y DOCENCIA ECONOMICAS},
