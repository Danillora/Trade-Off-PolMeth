@article{ WOS:000952917300032,
Author = {Fursykova, Tetiana and Chystiakova, Liudmyla and Shlianchak, Svitlana
   and Kravchenko, Olena and Kuris, Yurii},
Title = {Artificial intelligence and new forms of governance in the digital age},
Journal = {CUESTIONES POLITICAS},
