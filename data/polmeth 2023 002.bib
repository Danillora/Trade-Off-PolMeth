@article{ WOS:000945791200001,
Author = {Zhao, Minjuan and Shi, Rui and Du, Ruirui and Yao, Liuyang},
Title = {The Pathway to China's Carbon-Neutral Agriculture: Measures, Potential
   and Future Strategies},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
