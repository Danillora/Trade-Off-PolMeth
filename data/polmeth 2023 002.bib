@article{ WOS:000945222200001,
Author = {Suzuki, Motoshi},
Title = {The punitive impact of radical right populism on foreign aid:
   immigration pressure and mainstream partnership},
Journal = {EUROPEAN POLITICAL SCIENCE REVIEW},
Year = {2023},
Volume = {15},
Number = {4},
Pages = {542-561},
Month = {NOV},
Abstract = {This study investigates populist radical right (PRR) influence on aid
   amid widespread concerns about a potential connection between its rise
   and the reduction of aid allocation. Previous studies failed to address
   these concerns owing to the disuse of immigrant inflows as an
   intervening variable and a bilateral framework capable of investigating
   properties in donor and recipient countries. By analyzing panel data on
   Western European parliamentary democracies, the study demonstrates the
   PRR's reducing effect via a coalitional pathway on bilateral aid to the
   recipients, failing to stem emigration into the donor countries.
   Further, analysis shows that such reduction intensifies in conjunction
   with the donors' weak pluralistic institutions and the recipients'
   sociocultural characteristics different from the ordinary citizens
   represented by the PRR. The findings make a novel contribution to the
   expanding literature on the PRR to integrate insights on the
   aid-immigration nexus, strategies for policy influence, and ideational
   profiles.},
