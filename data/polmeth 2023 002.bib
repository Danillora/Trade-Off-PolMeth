@article{ WOS:001009987000001,
Author = {Zhong, Shi},
Title = {Coordinated Co-governance and Grassroots Innovation: A Case Study of
   Shenzhen's Livelihood-Related Project Voting System},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Year = {2024},
Volume = {9},
Number = {4},
Pages = {577-597},
Month = {DEC},
Abstract = {Multi-stakeholder co-governance is essential for boosting governance
   performance and fostering grassroots governance innovation. In exploring
   Shenzhen's livelihood-related project voting system promoted by the
   Municipal People's Congress, this article discusses the latest
   advancements in multi-stakeholder co-governance models in China. The
   unequal power distribution among stakeholders has eroded the
   effectiveness of co-governance models, causing a rift between the
   government and its constituents. By introducing local People's
   Congresses as pivotal players in grassroots governance, the
   livelihood-related project voting system helps address this inadequacy.
   The Shenzhen case shows that this voting system establishes a
   coordinated co-governance structure that involves public participation,
   local People's Congress decision-making, and local government execution,
   thus achieving a balance of power among stakeholders and enhancing
   governance performance. The practical experience of Shenzhen has some
   implications for understanding the changing role of multi-stakeholders
   in China's co-governance model. It exemplifies the potential of local
   People's Congresses in grassroots innovation, the reallocation of local
   government authority and responsibility, and the tendency of citizens'
   dependence in co-governance.},
