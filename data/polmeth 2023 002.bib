@article{ WOS:000947920400001,
Author = {Holley, Jared},
Title = {Racial Equality and Anticolonial Solidarity: Antenor Firmin's Global
   Haitian Liberalism},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2024},
Volume = {118},
Number = {1},
Pages = {304-317},
Month = {FEB},
Abstract = {This article recovers Antenor Firmin's contribution to anticolonial
   political thought by excavating his liberal worldmaking project of
   global racial equality and anticolonial solidarity. I assess Firmin's
   contrast between ``true{''} and ``false{''} liberalism in Haiti,
   reconstructing his understanding of true Haitian liberalism as committed
   to the core ideas of historical progress, national regeneration, and
   rehabilitation of the Black race globally. I contextualize his Equality
   of the Human Races in metropolitan Paris during his first exile, arguing
   that his critique of anthropological racism should be seen as integral
   to his commitment to Haitian liberalism. I then situate his discussion
   of what he called ``European Solidarity{''} in wider legitimating
