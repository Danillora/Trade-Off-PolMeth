@article{ WOS:001134196500005,
Author = {Abadi, Florencia},
Title = {Motherless Conceived: the Patriarchal Absolute},
Journal = {ANACRONISMO E IRRUPCION},
Year = {2023},
Volume = {14},
Number = {25},
Pages = {132-138},
Month = {NOV-MAY},
Abstract = {This essay examines the figure of Dionysus as a symbol of a higher order
   than the political and domestic one, which is an order of sacred
   madness. This aspect implies both an understanding of the sacred that
   integrates sacrilege as a connection with the divine, and a critique of
