@article{ WOS:001136385900023,
Author = {Krusmann, Valentin and Langbein, Julia and Ocakli, Beril and Peragovics,
   Tamas},
Title = {The New Silk Road Ten Years On Results from Hungary, Serbia, Georgia,
   and Kazakhstan},
Journal = {OSTEUROPA},
Year = {2023},
Volume = {73},
Number = {7-9},
Pages = {357+},
Abstract = {It has been said that the New Silk Road allows China to assert its
   interests by financing and implementing major infrastructure projects.
   Further, within its partner countries, China raised expectations of
   ``win-win{''} situations that were not fulfilled. Results in Hungary,
   Serbia, Georgia, and Kazakhstan show that local actors and conditions
   influence Chinese engagement more than was previously assumed. China is
   expanding its presence in countries and regions that are also of
   particular interest to Russia. By doing so, it is becoming a competitor
   for Russia, not a partner.},
