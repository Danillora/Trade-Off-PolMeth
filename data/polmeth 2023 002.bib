@article{ WOS:001072821400001,
Author = {Dawkins, Ryan and Nemerever, Zoe and Munis, B. Kal and Verville,
   Francesca},
Title = {Place, Race, and the Geographic Politics of White Grievance},
Journal = {POLITICAL BEHAVIOR},
Year = {2024},
