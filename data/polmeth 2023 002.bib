@article{ WOS:000920504200001,
Author = {Shetler-Jones, Philip},
Title = {UK-Japan Relations and the Indo-Pacific Tilt: The Cornerstone},
Journal = {RUSI JOURNAL},
Year = {2022},
Volume = {167},
Number = {6-7, SI},
Pages = {44-52},
Month = {NOV 10},
Abstract = {The UK's relationship with Japan is the cornerstone of the Indo-Pacific
   `tilt' and provides unique insights into its motivating logic and the
   UK's role in world politics, Philip Shetler-Jones argues. First, it
   exhibits the importance the UK gives to universal principles
   unrestricted by cultural or geographic markers in its choice of
   `like-minded' partners - an aspect of Global Britain that is
   reciprocated in Japanese policy, informing how the `tilt' is received in
   the region. Second, it represents the prime example of how the tilt
   maintains the equilibrium of UK foreign policy in the midst of a
   shifting global power balance, by means of a more diversified, flatter
   framework of `quasi alliance' relations built on the foundation of the
   transatlantic alliance. Third, the foremost commitment in the Integrated
   Review that the UK remain a leading technology power is reflected in the
   defence technology and industry partnership that is becoming central to
   the UK-Japan relationship.},
