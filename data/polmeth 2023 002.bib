@article{ WOS:001171118900017,
Author = {Fernandez Mora, Vicente de Jesus},
Title = {VOLUNTARY SERVITUDE AND GOVERNABILITY. A PROPOSAL FOR A FOUCAULDIAN
   READING OF ETIENNE DE LA BOETIE},
Journal = {REVISTA INTERNACIONAL DE PENSAMIENTO POLITICO},
Year = {2023},
Number = {18},
Pages = {327-349},
Abstract = {The aim of this paper is to introduce a proposal for the interpretation
   of Etienne de La Boeie's classic text on voluntary servitude from the
   perspective of the State and power developed by Foucault at the end of
   the 1970s based on the concept of governmentality. The comparative study
   of these two authors, infrequent in the Spanish bibliography, can enrich
   not only the understanding from a different perspective of the emergence
   of the modern state, the power relations it establishes and its
   survival, but also critically illuminate the proliferation in our days
   of complex devices of subjectivation that produce the will to serve.},
Publisher = {UNIV PABLO OLAVIDE},
