@article{ WOS:001020788000001,
Author = {Moloney, David and Princen, Sebastiaan},
Title = {Assessing the role of the European Council and the European Commission
   during the migration and COVID-19 crises},
Journal = {WEST EUROPEAN POLITICS},
Year = {2024},
Volume = {47},
Number = {7},
Pages = {1556-1587},
Month = {NOV 9},
Abstract = {Over the past decades, `emergency politics' has become a quasi-permanent
   feature of the European Union (EU). According to some, this has
   reinforced the trend towards a greater role for the European Council
   (EUCO) in EU agenda-setting, to the detriment of the European Commission
   (Commission). In this article, this claim is critically assessed by
   analysing two major crises: the 2015-2016 migration crisis and the
   COVID-19 pandemic. By systematically tracing the various agenda-setting
   roles played by EU actors in these crises, two claims are made. First,
   using a more fine-grained typology of agenda-setting roles, the
   relationship between EUCO and the Commission is shown to be more nuanced
   than is often suggested. Second, EUCO and the Commission cannot be
   considered monolithic players. Instead, actors within these institutions
   operate outside of formal channels to purse their own policy goals. This
   puts in doubt the usefulness of focussing on the EUCO-Commission
   relationship in a purely inter-institutional sense.},
