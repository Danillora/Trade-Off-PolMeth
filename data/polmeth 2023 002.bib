@article{ WOS:001061870200047,
Author = {Huallpa, Jorge Jinchuna and Sosa, Luis Enrique Fernandez and Garabito,
   Ninfa Dalila Carrizales},
Title = {Simultaneous control for timely decision-making in public management,
