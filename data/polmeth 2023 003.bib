
@article{ WOS:001085732900001,
Author = {Bar-On, Tamir and Paradela-Lopez, Miguel},
Title = {Limitations and inconsistencies of using the four waves of modern
   terrorism to understand Latin America},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2023},
Volume = {16},
Number = {4},
Pages = {744-766},
Month = {OCT 2},
Abstract = {Rapoport's theory of waves of terrorism has been considered a milestone
   when studying terrorist movements worldwide. This theory states that
   terrorism develops due to temporal, ideological and technological
   trends, which stimulate or diminish expressions of terrorism. More
   concretely, Rapoport argues that since the 19th century, global events
   have generated four waves of terrorism: anarchist, anticolonial, New
   Left, and religious. Using the Latin American experience of terrorist
   movements, this article explores inconsistencies and two main
   limitations in Rapoport's theory. Firstly, it shows how Latin America's
   historical and political particularities led to the weakness or absence
   of the anarchist and religious waves. Secondly, this article evidences
   that Rapoport ignored state terrorism and narcoterrorism, both extremely
   relevant to understanding terrorism in the region. Consequently, this
   study concludes that, despite it constituting a powerful theory for
   understanding terrorism from a global perspective, it requires a more
   nuanced regional approach and attention to more exhaustive expressions
   of the terrorist phenomenon.},
