@article{ WOS:000919343400001,
Author = {Petersen-Overton, Kristofer J.},
Title = {The Harmonious Classroom: Teaching Political Theory With Period Music},
Journal = {JOURNAL OF POLITICAL SCIENCE EDUCATION},
