@article{ WOS:001105870600001,
Author = {Lipsitz, Keena and Pop-Eleches, Grigore and Robertson, Graeme B.},
Title = {Local Norms, Political Partisanship, and Pandemic Response: Evidence
   from the United States},
Journal = {PERSPECTIVES ON POLITICS},
Year = {2024},
Volume = {22},
Number = {1, SI},
Pages = {247-262},
Month = {MAR},
Abstract = {A growing literature focuses on the role of political partisanship in
   shaping attitudes and behaviors during the COVID-19 pandemic in the
   United States. We provide a different perspective, by developing a
   theory of how partisanship interacts with another important factor that
   shapes how people think and behave in the context of the pandemic-local
   norms. Using a combination of survey data and a survey experiment, we
   demonstrate the importance of norms in shaping both support for social
   distancing and reported social-distancing behavior, particularly amongst
   independents and Republicans. We then confirm that perceptions of norms
   are indeed tied to what is actually happening around people-that their
   partisanship does not blind them to reality. Our analysis is the first
   to examine how partisanship and norms interact with each other and helps
   to explain why partisan differences matter more in some places than in
   others.},
