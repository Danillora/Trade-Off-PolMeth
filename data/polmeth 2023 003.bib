@article{ WOS:001039536200001,
Author = {Visconti, Francesco},
Title = {Voting across borders? The electoral consequences of individual
   transnationalism},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2024},
Volume = {34},
Number = {4},
Pages = {656-675},
Month = {OCT 1},
Abstract = {This article investigates the association between individual-level
   transnationalism, interactions and cultural skills cutting across
   European Union (EU) member states, and voting behaviour in the 2019
   European elections. The process of EU integration has expanded
   significantly virtual and physical forms of cross-border mobility in the
   last decades. Previous works have shown that transnational social
   exchange furthers Europhile attitudes and dilutes fears for immigrants.
   Against this backdrop, it is argued that people with high levels of
   transnationalism have the incentive to vote for parties with pro-EU
   integration and pro-immigration platforms. Empirical analyses conducted
