@article{ WOS:000930656100001,
Author = {Franco, Clara and Fuenfgeld, Anna and Garcia Pinzon, Viviana and Klenke,
   Jan and Lohmann, Rebecca and Reder, Desiree and Schirmer, Swantje},
Title = {Introducing ScrumAdemia: An Agile Guide for Doctoral Research},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2023},
Volume = {56},
Number = {2},
Pages = {251-258},
Month = {APR},
Abstract = {Pursuing a PhD is an exigent endeavor entailing various psychological
   and organizational challenges. This article proposes our adaptation of
   the agile method Scrum, which we call ``ScrumAdemia,{''} as a solution.
   We illustrate how ScrumAdemia helps us to overcome the challenges
   related to workflows, working conditions, and mental health during
   doctoral research. We use data from a self-evaluation survey executed
   over 18 months, as well as an extended focus-group discussion to assess
   ScrumAdemia's usefulness. Our experiences show that ScrumAdemia helps us
   to (1) overcome limitations of organization and structure relating to
   working conditions; (2) solve time-management problems affecting
   workflows; and (3) remedy the lack of support. This study has broader
   implications for doctoral research: more attention should be given to
   creating structures for peer-to-peer support.},
