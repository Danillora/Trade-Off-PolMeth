@article{ WOS:001064103900003,
Author = {Higgot, Richard and Reich, Simon},
Title = {TOWARDS A FUZZY WORLD ORDER: WHAT ROLE FOR NATO?},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2023},
Volume = {22},
Number = {1},
Pages = {17-25},
Month = {SPR},
Abstract = {Bi-polarity, multi-polarity, the end of globalization, or Cold War v.
   2.0. New explanations of world order abound. Often, they are asserted
   rather than reasoned by pundits looking for their 15 minutes of fame.
   This paper is much more modest. It eschews grand statements about world
   order. Rather we demonstrate that the future is far less clear cut and
   predetermined. Yes, a global binary is emerging around U.S.-China
   competition, but it is what we describe as fuzzy bifurcation. If our
   analysis is accurate, then it affects the behavior of other actors
   differently than a tight bi-polar system. In this paper we look at the
   implications of a fuzzy bifurcation of world order for that most
   important of alliances - NATO.},
