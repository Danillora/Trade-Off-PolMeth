@article{ WOS:000915332700001,
Author = {Rodriguez, Pedro L. and Spirling, Arthur and Stewart, Brandon M.},
Title = {Embedding Regression: Models for Context-Specific Description and
   Inference},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2023},
Volume = {117},
Number = {4},
Pages = {1255-1274},
Month = {NOV},
Abstract = {Social scientists commonly seek to make statements about how word use
   varies over circumstances-including time, partisan identity, or some
   other document-level covariate. For example, researchers might wish to
   know how Republicans and Democrats diverge in their understanding of the
   term ``immigration.{''} Building on the success of pretrained language
   models, we introduce the a la carte on text (conText) embedding
   regression model for this purpose. This fast and simple method produces
   valid vector representations of how words are used-and thus what words
   ``mean{''}-in different contexts. We show that it outperforms slower,
   more complicated alternatives and works well even with very few
   documents. The model also allows for hypothesis testing and statements
   about statistical significance. We demonstrate that it can be used for a
   broad range of important tasks, including understanding US polarization,
   historical legislative development, and sentiment detection. We provide
   open-source software for fitting the model.},
