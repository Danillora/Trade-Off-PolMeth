@article{ WOS:000996305400016,
Author = {Alonso, Jorge Perez},
Title = {Judicial Battle around Abortion: From Roe v. Wade to Dobbs v. Jackson},
Journal = {TEORIA Y REALIDAD CONSTITUCIONAL},
Year = {2023},
Volume = {51},
Pages = {529-564},
Abstract = {For the last fifty years, Roe v. Wade was the leading case that
   recognized the woman's right to get an abortion. But since the moment it
   was released, it was subjet to a severe controversy. Those who are pro
   choice defended strongly the woman's right to decice if she wants to end
   her pregnancy, while pro life supporters defended the right of the
   unborn. After four decades of litigation over the suject, and a set of
   deci-sions that manteined the woman's right to get an abortion although
   restricting the conditions to get it, Supreme Court of United States in
   a 6-3 mayority opinion Dobbs v. Jackson, overruled Roe, returning the
   issue to the states. This article explains the evolution of Supreme
   Court abortion jurisprudence.},
Publisher = {UNIV NACIONAL EDUCACION DISTANCIA},
Address = {PO SENDA REY,BRAVO MURILLO 38, MADRID, 7-28040, SPAIN},
