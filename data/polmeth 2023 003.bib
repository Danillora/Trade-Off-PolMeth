@article{ WOS:001024137500001,
Author = {Eschenauer-Engler, Tanja and Herre, Bastian},
Title = {Coup leaders: a new comprehensive dataset, 1950-2020},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2024},
Volume = {23},
