@article{ WOS:001148044700009,
Author = {Liu, Licheng and Pang, Xun},
Title = {A Bayesian multifactor spatio-temporal model for estimating time-varying
   network interdependence},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2023},
Volume = {11},
Number = {4},
Pages = {823-837},
Month = {OCT},
Abstract = {This paper proposes a Bayesian multilevel spatio-temporal model with a
   time-varying spatial autoregressive coefficient to estimate temporally
   heterogeneous network interdependence. To tackle the classic reflection
   problem, we use multiple factors to control for confounding caused by
   latent homophily and common exposures. We develop a Markov Chain Monte
   Carlo algorithm to estimate parameters and adopt Bayesian shrinkage to
   determine the number of factors. Tests on simulated and empirical data
   show that the proposed model improves identification of network
   interdependence and is robust to mis-specification. Our method is
   applicable to various types of networks and provides a simpler and more
   flexible alternative to coevolution models.},
