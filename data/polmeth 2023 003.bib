@article{ WOS:001104404500001,
Author = {Foley, Lauren S. and Jordan, Marty P.},
Title = {Institutionalizing Internships: Enhanced Civic Culture via State Capital
   Internship Programs},
Journal = {JOURNAL OF POLITICAL SCIENCE EDUCATION},
