   Pearse, Harry, Univ Cambridge, POLIS, Cambridge, England.
   Pearse, Harry, Univ Cambridge, POLIS, Cambridge CB2 1TN, England.},
DOI = {10.1177/14789299231195454},
EarlyAccessDate = {AUG 2023},
ISSN = {1478-9299},
EISSN = {1478-9302},
Keywords = {universal suffrage; democracy; voting; children},
Keywords-Plus = {CLIMATE-CHANGE; AGE; EQUALITY; GOVERNMENT; DEMOCRACY; CRITIQUE; JUSTICE;
