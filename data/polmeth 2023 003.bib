@article{ WOS:001043445700001,
Author = {Hoang, Bai Linh},
Title = {Does Race Affect Public Evaluations of Constituent Messages in Local
   Government Meetings? Results from an Experiment},
Journal = {POLITICAL STUDIES REVIEW},
Year = {2024},
Volume = {22},
Number = {2},
Pages = {436-446},
Month = {MAY},
Abstract = {Constituents in the United States have used local public meetings in
   recent years to shape policy on some of the most high-profile
   race-related issues. However, public meeting participation remains less
   studied relative to other modes of participation. This study
   investigates the extent to which race shapes the way a message is heard
   and evaluated by the public audience and the degree to which its impact
   depends on the issue of the message. To carry out the study, I set up a
   2 x 3 experiment containing six short treatment videos in which I
   manipulated the race of the actor/speaker (Black or White) and the
   message issue (innocuous service request, race-related policy, and
   race-neutral policy). Subjects were randomly assigned to watch one of
   the six videos and then asked to evaluate the message in the video. The
   early results of this study show that unlike previous studies revealing
   racial bias in a range of political contexts in the United States, race
   exerts neither independent nor conditional effects on the evaluation of
   the message as reasonable. I discuss the limitations and implications of
   these findings and useful considerations to think about in expanding
   this study.},
