@article{ WOS:000958542600001,
Author = {Xu, Lingyu},
Title = {The Evolution of China's Foreign Talent Policy: the Case Study of
   Beijing},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Year = {2024},
Volume = {9},
Number = {2},
Pages = {200-221},
Month = {JUN},
Abstract = {This paper aims to explore the evolution of China's foreign talent
   policy (FTP) and the factors that contribute to those policy changes.
   Selecting Beijing as a case study, this paper explores Beijing's FTP and
   divides it into three periods (1949-1978; 1978-2000; 2000 until now). It
   argues that at the point when the policy system becomes more
   institutionalized, more simplified measures are adopted. In particular,
   this paper applies a three-tier framework to detect the factors
   contributing to the policy changes as follows: (1) the deregulation of
   China's scientific and educational systems, (2) emerging actors in the
   FTP institutional setting (Zhongguancun Science Park, private think
   tanks and social groups), and (3) market-oriented reform of the policy
   tools, e.g., China's permanent residence system.},
