@article{ WOS:001112733900001,
Author = {Cacciatore, Federica and Bitonti, Alberto and Sgueo, Gianluca},
Title = {Interest groups and the implementation of electronic health records in
   the Italian NRRP, between policy and politics},
Journal = {CONTEMPORARY ITALIAN POLITICS},
