@article{ WOS:000972144700006,
Author = {Hernandez, Yannick and Loudcher, Jean-Francois and Suchet, Andre},
Title = {Territorialization of national educational public action and
   decentralization in France and Spain: a comparative study of sports
   activities in the school sector},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2023},
Volume = {56},
Number = {1},
Pages = {113-135},
Month = {MAR},
Abstract = {This study analyzes the so-called periscolaires sports activities in
   France and extraescolares sports activities in Spain, which are based on
   their respective national educational legislative frameworks. The
   different dynamics of decentralization in each country are then
   identified. This article examines how these two cases diverge in terms
   of the territorialization of public action. The role of the states and
   the participation of sub-state partners in the management of these
   intersectoral activities are analyzed as well. This work demonstrates
   the disengagement of the national administrations of both countries in
   the management of these activities. There are reasons for this that go
   beyond just decentralization. Paradoxically, proven decentralization
   does not always result in the territorialization of education. Moreover,
   decentralization can be surprisingly restrictive.},
