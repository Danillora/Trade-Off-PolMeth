@article{ WOS:001016296500007,
Author = {Kropotkin, Piotr},
Title = {What Geography Ought to Be},
Journal = {GEOPOLITICAS-REVISTA DE ESTUDIOS SOBRE ESPACIO Y PODER},
Year = {2023},
Volume = {14},
Number = {1},
Pages = {117-132},
Abstract = {The natural sciences developed greatly in the last years of the 19th
   century -consequently, the interest in Geography in schools rose-. But
   to improve its impact, Geography would need to engage in in-depth reform
   tasks. The first task would be to make the reform of Geography teaching
   the first step towards a global reform affecting the entire educational
   system. Geography's second task would be to instruct children in the
   idea of universal brotherhood regardless of nationality. Thirdly,
   Geography would need to eradicate the prejudices, myths and constant
   degradations that the western colonizers disseminated about the
   so-called ``inferior races{''}.},
