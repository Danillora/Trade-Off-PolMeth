@article{ WOS:001067489000009,
Author = {Rodriguez, Diana Isabel Molina and Mantilla, Ana Patricia Pabon},
Title = {Legal feminism and the denaturalization of socio-economic, political,
   identity and vital injustices against women},
Journal = {CIENCIA POLITICA},
Year = {2023},
Volume = {18},
Number = {35},
Pages = {211-233},
Month = {JAN-JUN},
Abstract = {The feminization of justice has been shaped by various narratives in
   recent years. From studies on institutions that normalized femicide to
   the recent contributions of constitutional feminism, including
   interesting characterizations of legal discourses that discipline women
   in domestic spaces, all of them took a turn with the first waves of
   enlightened vindication and suffragettes that contested spaces of public
   reason and access to social and political rights. They document the
   structural discontent of culture and the situated relationship between
   discipline and dispossession of women, amidst the epistemological
   contexts of critical currents.This article aims to address the question
   of what contribution gender studies have made to law under the influence
   of corrective and transformative contingencies in social contexts
   through the critique of legal practices. To do so, it will showcase
   several studies, particularly those of critical legal feminism, that
   have explored the relationships shaped by sex-gender, highlighting
   conditions of non-neutrality and, thus, the inadequacy of the classical
   legal discourse of formal equality in overcoming capitalist and
   patriarchal violence and discrimination.},
