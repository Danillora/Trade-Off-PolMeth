@article{ WOS:001064093000009,
Author = {Hanser, Amy and Boisson, Helene},
Title = {The rice bowl of its kind. Sexual Politics of Service Work in Urban
   China},
Journal = {POLITIX},
Year = {2023},
Number = {141},
Pages = {149-172},
Abstract = {This article examines new conceptions of gender and sexuality observable
   in China, seeking to understand why and how these elements could take on
   such importance in the organization of service work. Drawing on
   ethnographic data collected in three urban retail establishments, this
   article demonstrates how powerfully essentialized conceptions of gender
   and sexuality convey class distinctions in service establishments,
   linked to the image of a China moving from socialism to a market
   economy. The transition from the ``iron rice bowl{''} to the ``rice bowl
   of youth{''} confers value on young, feminine and urban bodies, while at
   the same time devaluing rural women of `middle age. This article argues
   that an essentializing discourse on gender legitimizes new inequalities
   in urban China, while masking the class distinctions simultaneously
   produced.},
