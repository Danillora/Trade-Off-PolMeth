@article{ WOS:001167365000009,
Author = {Zapata-Cortes, Olga Lucia},
Title = {Challenges of heritage protection in Colombia. The case of the
   municipality of Concepcion.},
Journal = {REFLEXION POLITICA},
Year = {2023},
Volume = {25},
Number = {52},
Pages = {111-126},
Month = {DEC},
Abstract = {One form of protection of cultural heritage in Colombia is the
   declaration of a National Asset of Cultural Interest (BICN). This
   provides incentives for its owners, but prohibits the demolition,
   displacement, restoration, intervention or destruction of the property
   without government authorization. This text presents, through a
   documentary analysis, the main challenges of public policy for heritage
   protection in the case of the municipality of Concepcion. This case is
   significant because it was one of the first municipalities to declare
   its historic center a BINC, but it took more than 20 years to formulate
   its management plan. Thus, the lack of preparation on the part of the
   local government and the late formulation of the management plan had an
   impact on the increase of risks for the protection of this heritage, as
   well as on the lack of citizen empowerment.},
