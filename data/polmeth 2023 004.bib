@article{ WOS:001014511700001,
Author = {Arnesen, Sveinung and Christensen, Dag Arne and Finseraas, Henning},
Title = {Look to Denmark or not? An experimental study of the Social Democrats'
   strategic choices},
Journal = {ELECTORAL STUDIES},
Year = {2023},
Volume = {84},
Month = {AUG},
Abstract = {Social Democratic parties struggle to maintain their strong electoral
   position, as political competition has shifted from the traditional
   left-right dimension to the cultural dimension. This has led to a debate
   on what would be the most viable electoral strategy for these parties in
   terms of adjusting their policies. Some propose a ``New Left{''} policy
   platform that combines social investment and progressive cultural
   policies; others an ``Old Left{''} policy platform that combines
   traditional redistribution policies and social-conservative cultural
   policies. We conducted a survey experiment to test the effects of these
   two platforms on support for the Norwegian Labour party. Our results
   show that the New Left platform is more popular among current Labour
   voters and voters from competing left-wing parties, and the two policy
   platforms are equally popular among the total electorate.},
