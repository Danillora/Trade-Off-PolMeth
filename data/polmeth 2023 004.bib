@article{ WOS:001132506200001,
Author = {Gherghina, Sergiu and Pilet, Jean-Benoit and Mitru, Bettina},
Title = {Big ideas, little detail: how populist parties talk about referendums in
   Europe},
Journal = {CONTEMPORARY POLITICS},
Year = {2024},
Volume = {30},
Number = {4, SI},
Pages = {437-455},
Month = {AUG 7},
Abstract = {The idea of increasing the power of the `pure people' at the expense of
   a `corrupt elite' lies at the core of populism. One way for populist
   parties to do this is to push for a greater use of referendums. Previous
   research shows that populist parties mention in general in their
   communications the referendums as suitable avenues for the direct
   involvement of the people in the decision-making process. However, we
   miss details about how they refer to referendums. This article addresses
   this gap in the literature and explores how populist parties talk about
   referendums in their election manifestos. It seeks to identify what type
   of referendum populist parties tend to support, and to analyze whether
   their support for referendums is generic or policy-specific. Our
