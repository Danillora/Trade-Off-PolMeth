@article{ WOS:001133785200002,
Author = {Indriasih, Dewi and Mulyani, Sri and Fajri, Aminul and Rimbawan, Teguh},
Title = {Bridging the gender gap: women in fisheries industry policy on the North
   Coast of West Java, indonesia},
