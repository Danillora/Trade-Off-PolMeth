@article{ WOS:001083330800006,
Author = {Blanco, Domitille},
Title = {The Cost of Family Memory. A Study of the Publicisation of Memories of
   the Tutsi Genocide in France},
Journal = {SCIENZA \& POLITICA-PER UNA STORIA DELLE DOTTRINE},
Year = {2023},
Volume = {35},
Number = {68},
Pages = {125-142},
Abstract = {This paper highlights the expression of memories of the Tutsi genocide
   in the public space, in Auvergne -Rhone-Alpes (France). We will see that
   the form and actors of genocide commemorations have evolved in recent
   years, with the absence of testimonies of survivors as only constant.
   This will lead us to question the conditions of elaboration and
   transmission of individual and family memories of the event. The data
   and analyses that are produced here come from a sociology-anthropology
   PhD thesis, whose fieldwork was carried out between 2014 and 2019.},
