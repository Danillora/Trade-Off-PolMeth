@article{ WOS:001136712500001,
Author = {Garcia, Christian Camilo Castano},
Title = {The Current geopolitical crisis: Imperialism and the persistence of the
   unipolar ``moment{''}},
Journal = {CIENCIA POLITICA},
Year = {2023},
Volume = {18},
Number = {36},
Pages = {47-78},
Month = {JUL-DEC},
Abstract = {In this reflective article, the argument is presented that the current
   international crisis is not an exception to the system but one of its
   inherent characteristics, namely its imperialistic nature. Drawing on a
   characterization of the term from the perspective of the theory of `new
   imperialism,' the article examines the question of the supposed shift
   from a unipolar international system to a multipolar one. The conclusion
   is that the present situation exhibits continuity of unipolarity with
   the dominance of the United States, which is reinforced in the current
   context.},
