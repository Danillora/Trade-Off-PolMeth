@article{ WOS:000949264000001,
Author = {Azevedo, Flavio and Micheli, Leticia and Bolesta, Deliah Sarah},
Title = {Does Stereotype Threat Contribute to the Political Knowledge Gender Gap?
   A Preregistered Replication Study of Ihme and Tausendpfund (2018)},
Journal = {JOURNAL OF EXPERIMENTAL POLITICAL SCIENCE},
Year = {2024},
Volume = {11},
Number = {1},
Pages = {102-113},
Month = {SPR},
Abstract = {The gender gap in political knowledge is a well-established finding in
   Political Science. One explanation for gender differences in political
   knowledge is the activation of negative stereotypes about women. As part
   of the Systematizing Confidence in Open Research and Evidence (SCORE)
   program, we conducted a two-stage preregistered and high-powered direct
   replication of Study 2 of Ihme and Tausendpfund (2018). While we
   successfully replicated the gender gap in political knowledge - such
   that male participants performed better than female participants - both
   the first (N = 671) and second stage (N = 831) of the replication of the
   stereotype activation effect were unsuccessful. Taken together (pooled N
   = 1,502), results indicate evidence of absence of the effect of
   stereotype activation on gender differences in political knowledge. We
   discuss potential explanations for these findings and put forward
   evidence that the gender gap in political knowledge might be an artifact
   of how knowledge is measured.},
