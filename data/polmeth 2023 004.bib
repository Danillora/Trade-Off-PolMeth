@article{ WOS:001091355500001,
Author = {Cinar, Asli Ceren and Kibris, Ozgur},
Title = {Persistence of voice pitch bias against policy differences},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2024},
Volume = {12},
Number = {3},
Pages = {591-605},
Month = {JUL},
Abstract = {We use an online experiment to study the relative effect on voter
   behavior of a candidate's voice pitch and policy stance. We demonstrate
   a strong voice-pitch bias: between candidates who are identical in every
   other aspect, voters are more likely to choose the one with the lower
   voice-pitch, and more so in elections between men than women candidates.
   We then introduce a novel phenomenon: persistence of voice-pitch bias is
   the amount of policy difference needed to compensate for voice-pitch
   bias. While persistence is also gender-dependent, the effect is now
   reversed: voice-pitch bias is more persistent in elections between women
   than men candidates. As a possible mechanism, we show that voters
   perceive candidates with lower voice-pitch as more competent and
   trustworthy.},
