@article{ WOS:001133655300007,
Author = {Lloyd, Moya},
Title = {Becoming Visible Corporeal Politics, Spaces of Appearance, and the Miss
   America Protest},
Journal = {DEMOCRATIC THEORY-AN INTERDISCIPLINARY JOURNAL},
Year = {2023},
Volume = {10},
Number = {2},
Pages = {18-30},
Month = {DEC},
Abstract = {Jacques Ranciere's discussion of disidentification provides an important
   account of how existing inegalitarian structures and hierarchically
   ordered identities may be challenged. However, Ranciere treats
   disidentifi- cat ion as a discursive phenomenon, centered on naming. As
   an explanation of how the invisible might become visible, it is
   problematic to overlook the body, since appearance requires our bodies
   to be seen, to become visible. Drawing on discussions of the
   subject-in-process and the idea of identity as both enfleshed and
   performatively constituted, this article seeks to enrich Ranciere's
   discussion of disidentification by focusing attention on its em- bodied
   dimensions. It does so by exploring, through an analysis of the Miss
   America protest of 1968, the role of corporeality both in constituting
   spaces of appearance and in articulating democratic demands for
   visibility.},
