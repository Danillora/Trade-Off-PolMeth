@article{ WOS:001035764100006,
Author = {Kemp, Kenny},
Title = {HOW HOPE AND OPPORTUNITY WERE SQUANDERED: FERRIES AND SHIPYARD
   REGENERATION ON INVERCLYDE},
Journal = {SCOTTISH AFFAIRS},
Year = {2023},
Volume = {32},
Number = {3},
Pages = {365-386},
Month = {AUG},
Abstract = {In 2015 Ferguson's, the only remaining shipyard on the Lower Clyde, was
   awarded the contract to build two passenger ferries for the Hebridean
   routes. The vessels are uncompleted, the MV Glen Sannox having been
   `launched' in 2017, and Hull 802 much further behind. Delivery is
   already five years overdue, and costs have spiralled to an estimated
   350m pound: a staggering amount that might have been spent in better
   ways. Whilst the underlying story is likely to remain unknown without a
   full public inquiry, this essay recounts how the story has developed,
   how the project sank into rancorous failure, and the ways in which the
   people of Inverclyde have been sold short yet again.},
