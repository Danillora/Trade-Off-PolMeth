@article{ WOS:000912128100001,
Author = {Trimcev, Eno},
Title = {Drinking Wine with Friends: Plato's Lesson for Contemporary Democratic
   Theory},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
