@article{ WOS:001006095800001,
Author = {Taylor, Zack and Lucas, Jack and Kirby, J. P. and Hewitt, Christopher
   Macdonald},
Title = {Canada's Federal Electoral Districts, 1867-2021: New Digital Boundary
   Files and a Comparative Investigation of District Compactness},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2023},
Volume = {56},
Number = {2},
Pages = {451-467},
Month = {JUN},
Abstract = {We introduce new data resources to enable spatial and nonspatial
   research on Canadian elections, electoral history and political
   geography. These include a comprehensive set of distinct identification
   codes for every federal electoral district in Canada from 1867 to the
   present, a complete set of digital boundary files for these electoral
   districts, historical census data aggregated to federal electoral
   districts, and tools to connect our district identification codes to
   federal election results. After describing the construction and content
   of these new resources, we provide an example of their use in a
   comparative-historical analysis of district compactness in Canada and
   the United States. We find that, in contrast to the United States,
   postwar institutional changes to district boundary-drawing processes had
   little effect on district compactness in Canada.},
