@article{ WOS:001084878800001,
Author = {Fletcher, Louis},
Title = {A modern theodicy: John Rawls and `The Law of Peoples'},
Journal = {EUROPEAN JOURNAL OF POLITICAL THEORY},
Year = {2023},
Month = {2023 OCT 16},
Abstract = {John Rawls' The Law of Peoples has typically been read as an
   intervention in the field of `global justice'. In this paper, I offer a
   different and widely overlooked interpretation. I argue that The Law of
   Peoples is a secular theodicy. Rawls wants to show that the `great
   evils' of history do not condemn humankind by using a secularised form
   of moral faith to search for signs that the social world allows for the
   possibility of perfect justice. There are, I show, striking homologies
   between this argument and the Christian theodicy that Rawls wrote in
   1942, A Brief Inquiry into the Meaning of Sin and Faith. Perhaps more
   significantly, I draw out how there is, as Rawls himself appears to
   acknowledge, an intimate relationship between this redemptive project
   and Rawls' idealistic and moralistic approach to political philosophy.},
