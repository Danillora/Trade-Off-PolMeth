@article{ WOS:001058013500001,
Author = {Joest, Prisca and Kronke, Matthias and Lockwood, Sarah J. and Lust,
   Ellen},
Title = {Drivers of Political Participation: The Role of Partisanship, Identity,
   and Incentives in Mobilizing Zambian Citizens},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2024},
Volume = {57},
Number = {9},
Pages = {1441-1474},
Month = {AUG},
Abstract = {Scholars and policymakers widely view identity as a key driver of
   African citizens' political engagement. In doing so, however, they have
   emphasized ethnicity and largely sidelined other identities, including
   gender, local origin, shared residency, and partisanship. In this paper,
   we explore which identities drive political engagement and why they do
   so. We employ an original survey experiment that includes various
   identities and other incentives that may drive citizens' participation
   around Zambia's 2021 national elections. We find that partisanship most
   influences individuals' stated willingness to campaign for a candidate
   or meet with an MP, while ethnicity and social incentives play less
   significant roles. Finally, we explore the mechanisms underpinning these
   results and find that citizens anticipate sanctions if they fail to
   support a co-partisan but not a co-ethnic candidate. These findings have
   important implications for understanding political engagement and
   democratic development throughout the region.},
