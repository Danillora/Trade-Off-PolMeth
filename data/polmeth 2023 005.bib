@article{ WOS:001136989200001,
Author = {Kashyap, Amit Kumar and Lunagariya, Simran},
Title = {Trade finance evolution: a comparative study of regulatory reforms in
   factoring policies for SMEs in India and China},
Journal = {OTORITAS-JURNAL ILMU PEMERINTAHAN},
