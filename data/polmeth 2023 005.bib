@article{ WOS:000996305400014,
Author = {Vitoria, Ignacio Garcia},
Title = {The exclusion of offenders from parliament: between political freedom
   and the rule of law},
Journal = {TEORIA Y REALIDAD CONSTITUCIONAL},
Year = {2023},
Volume = {51},
Pages = {441-480},
Abstract = {The power of the state to exclude criminals from parliament is limited
   by the constitutional and European guarantee of the right to passive
   suffrage. The main points of friction between the restrictions
   established in the Spanish legal system will be identified, considering
   the fundamental principles of the Euro-pean constitutional heritage. The
   case law of the Constitutional Court, the European Court of Human Rights
   and the Court of Justice of the European Union will then be examined to
   analyze the difficulties posed by the judicial review of transcendental
   areas of the political process.},
