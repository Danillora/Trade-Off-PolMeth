@article{ WOS:001054253100001,
Author = {Jeifets, Victor},
Title = {From dependence to (dis)obedience: Relations between the Mexican
   Communist Party and the Communist Party of the Soviet Union during the
   Cold War},
Journal = {IZQUIERDAS},
Year = {2023},
Volume = {52},
Month = {AUG},
Abstract = {The article analyzes the evolution of the character of relations between
   the Mexican Communist Party (PCM) and the Communist Party of the Soviet
   Union (CPSU) in the period that covers the times from the end of the
   Second World War till 1981 when the PCM ceased to exist and fusions with
   some other Left groups within almost the Socialist Unificated Party of
   Mexico. The author focuses, above all, on the analysis of the degree of
   independence and autonomy of the PCM's attitude toward Moscow taking as
   the main points of research the attitudes of the PCM over the
   Sino-Soviet rupture within the International Communist movement, the
   issue of the invasion into Checoslovakia and supposed evolution of the
   PCM toward ``Eurocommunist{''} ideas. Based on the documents of the
   International Department of the CC of the CPSU kept in Moscow and some
   published memories of the militants of the PCM, the author recover this
   aspect of the Party History which is not well-researched in the existing
   historiography.},
