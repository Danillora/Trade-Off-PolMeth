@article{ WOS:001165595700002,
Author = {Hidalgo-Capitan, Antonio Luis and Cubillo-Guevara, Ana Patricia},
Title = {Social Progress in 21st-Century Costa Rica. The Fading of the ⟪Tico
   Dream⟫},
