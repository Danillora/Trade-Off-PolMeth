@article{ WOS:000981560200001,
Author = {Manow, Philip and Fluegel, Dominik},
Title = {``Kulturkampf Legacy{''} or Consequence of Programmatic Repositioning?
   Catholic Voters and the AfD-A Replication Study},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2023},
Volume = {64},
Number = {3},
Pages = {585-601},
Month = {SEP},
Abstract = {This research note replicates Lukas Haffert's 2022 article The Long-Term
   Effects of Oppression: Prussia, Political Catholicism, and the
   Alternative fur Deutschland and tests that article's thesis about the
   long-term effects of the Kulturkampf ({''}culture struggle{''}) on the
   voting behaviour of Catholic voters today. We offer an alternative
   explanation for the finding that the voting behaviour of Catholics with
   regard to the Alternative for Germany party (AfD) varies regionally
   between former Prussian and former non-Prussian areas. Our explanation
   focuses on differences in the programmatic-ideological position of the
   federal and state Christian Democratic Union associations and the
   Christian Social Union in the 2017 and 2021 federal elections. We find
   empirical evidence consistent with this alternative explanation, while
   the ``Kulturkampf thesis{''} is not confirmed upon closer examination.},
