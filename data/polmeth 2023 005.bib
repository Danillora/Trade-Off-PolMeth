@article{ WOS:001025641100002,
Author = {Munoz, Cristina Sanchez},
Title = {Women's Rights, Peace, and Democracy: A Problematic Relationship},
Journal = {POLITICKA MISAO-CROATIAN POLITICAL SCIENCE REVIEW},
Year = {2023},
Volume = {60},
Number = {2},
Pages = {31-51},
Month = {JUN},
Abstract = {This paper analyses the reformulations of citizenship, positive peace
   and democracy of both contemporary feminist theory and the feminist
   movement. In doing so, it will explain the interrelationship between the
   feminist movement as a democratic movement and its proposals for
   reformulating concepts and ideas that make up what we can call the
   ``grammar of politics{''}. Following Seyla Benhabib, the ``democratic
   iterations{''} that have been achieved in the framework of human rights
   will be presented, giving some examples at national and international
   levels. Finally, the changes and challenges in women's demands for
   rights in current scenarios will be analysed.},
