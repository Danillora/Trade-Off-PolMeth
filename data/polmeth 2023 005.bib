@article{ WOS:000928685300001,
Author = {Reda, Amir Abdul and Fraser, Nicholas A. R. and Khattab, Ahmed},
Title = {Does Social Mobility Matter? The Kafala System and Anti-Immigrant
   Sentiment},
Journal = {POLITICAL STUDIES REVIEW},
Year = {2023},
Volume = {21},
Number = {4},
Pages = {801-824},
Month = {NOV},
Abstract = {Existing studies argue that anti-immigrant sentiment stems from threat
   perception. Yet, conventional theoretical approaches cannot fully
   explain hostility toward immigrants in the Middle East and North Africa,
   where low-skilled foreign workers occupy an inferior social and legal
   status vis-a-vis natives under the kafala system. Building on existing
   studies of immigration politics, we theorize how immigration policies
   can either facilitate or prevent the social mobility of foreign workers.
   Exploring immigration attitudes in 14 Middle East and North Africa
   countries using an original dataset that matches survey responses with
   host country-specific factors, we find that extreme rights-restricting
   immigration policies (such as the kafala system) encourage wealthier
   natives to be more hostile than their lower-class counterparts. Our
   study suggests that anti-immigrant sentiment is context-specific and
   influenced by local institutions.},
