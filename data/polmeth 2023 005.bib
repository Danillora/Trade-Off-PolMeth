@article{ WOS:000952917300026,
Author = {Donkov, Serhii and Nykytchenko, Nataliia and Mishchuk, Mykhailo and
   Lysa, Mariia and Kurliak, Marian},
Title = {Disciplinary liability of insolvency officers: current challenges},
Journal = {CUESTIONES POLITICAS},
