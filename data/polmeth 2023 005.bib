@article{ WOS:001012633500002,
Author = {Noesselt, Nele},
Title = {Chinese Contemplations on Utopian and Dystopian Democratic Governance},
Journal = {DEMOCRATIC THEORY-AN INTERDISCIPLINARY JOURNAL},
Year = {2023},
Volume = {10},
Number = {1},
Pages = {1-18},
Abstract = {This article addresses the Chinese debates on utopian and dystopian
   modes of democracy. It opens the black box of the one-party state and
   delves below the surface of the People's Republic of China's of-ficial
   statements on ``democracy{''} (e.g., ``people's democracy,{''}
   ``democratic centralism{''}) by focusing on the often-overlooked
