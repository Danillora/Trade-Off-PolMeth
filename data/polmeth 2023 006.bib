@article{ WOS:001156745000005,
Author = {Lorenzini, Maria Elena and Doval, Gisela Pereyra},
Title = {Forty Years of Democratic Foreign Policy: Regional Integration as State
   Policy},
Journal = {TEMAS Y DEBATES},
Year = {2023},
Number = {45, 1},
Pages = {113-123},
Abstract = {Argentine foreign policy has taken on different contours and had
   different objectives during these 40 years of democratic life. For this
   reason, many authors have described it as inconsistent. However,
   domestic and systemic conditions that affect its implementation have led
   to adjustments in foreign policy. Nevertheless, upon careful
   examination, it is possible to identify some structuring axes that we
   recognize as State policy, such as in the case of regional integration.
   Regional integration has remained on the foreign policy agenda despite
   the different priorities it has adopted over time.},
