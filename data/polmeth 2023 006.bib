@article{ WOS:001156745000009,
Author = {Pesuto, Carolina},
Title = {Reflections on Paradiplomacy after forty Years of Democracy in Argentina},
Journal = {TEMAS Y DEBATES},
Year = {2023},
Number = {45, 1},
Pages = {153-158},
Abstract = {This article is intended to reflect how the return of the democratic
   order in the 1980s and the constitutional reform of 1994 were
   contributing factors for the development of paradiplomacy in politics.
   The complexity and multiplicity of edges that the phenomenon of
   paradiplomacy has acquired, understood as sub-statehood in international
   relations, will be described taking into account how the regulatory and
   institutional frameworks contextualized and expanded the powers and
   margins of action of subnational governments towards
   internationalization.},
