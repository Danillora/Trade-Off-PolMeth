@article{ WOS:000956722100001,
Author = {Nam, Aerang and Weible, Christopher M.},
Title = {Examining experts' discourse in South Korea's nuclear power policy
   making: An advocacy coalition framework approach to policy
   knowledgePalabras Clave(sic)(sic)(sic)},
