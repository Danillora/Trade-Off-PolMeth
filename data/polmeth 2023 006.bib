@article{ WOS:001171029900005,
Author = {Maldonado, Pedro Huerta},
Title = {Updates and displacements of sense in the political category of ``class
   struggle{''} in Grupo Comuna (Bolivia)},
Journal = {IZQUIERDAS},
Year = {2023},
Volume = {52},
Pages = {24},
Month = {OCT},
Abstract = {The article examines some updates and displacements of meaning in the
   political category of ``class struggle{''} of Karl Marx and Friedrich
   Engels in the contemporary authors of the Grupo Comuna alvaro Garcia
   Linera, Luis Tapia, Raquel Gutierrez and Raul Prada. This article works
   with the positions developed by these authors in their first collective
   book published in 1999, El Fantasma Insomne, as well as other texts
   published outside the production of this group of intellectuals. It
   seems relevant to us to examine the positions of these Latin American
   intellectuals to the extent that from their positions one can see the
   formulation of a critical Marxism from the periphery of the so-called
   world -system (Wallerstein, 1979). The intention is not to show the
   logical coherence of his approaches with respect to the early Marxist
   thinkers, but rather to open up to a contemporary political horizon that
   thinks about the ``class struggle{''} under the conditions of its
   contemporary theoretical relevance.},
