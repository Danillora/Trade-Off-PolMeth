@article{ WOS:001128658900010,
Author = {Rodriguez, Lorenzo Cachon},
Title = {The Souls of Black Folk and the art of fugue by W. E. B. Du Bois},
Journal = {REVISTA DE ESTUDIOS POLITICOS},
Year = {2023},
Number = {202},
Pages = {77-103},
Month = {OCT-DEC},
Abstract = {120 years ago, it was published The Souls of Black Folk by W. E. B. Du
   Bois. It is a well-known book in the United States both inside and
   outside of academia. This article examines the historical context in
   which it appeared, a context marked by racial segregation and traces of
   slavery; then the meaning of the book is exposed, proposing that it be
   read as a kind of << the art of the fugue >> around a theme: the
   struggles of black folk to achieve freedom and equality with respect to
   whites. The variants of the theme in the fourteen chapters of the book
   are analyzed below and it is shown how a new language appears that will
   be the basis of the civil rights movement of the sixties of the
   twentieth century and also some parts of the speech << I have a dream >>
   by Martin Luther King Jr.},
Publisher = {CENTRO ESTUDIOS POLITICOS CONSTITUCIONALES},
Address = {PLAZA DE LA MARINA ESPANOLA 9, 28071 MADRID, SPAIN},
Type = {Article},
Language = {English},
Affiliation = {Rodríguez, LC (Corresponding Author), Univ Complutense Madrid, Madrid, Spain.
   Rodriguez, Lorenzo Cachon, Univ Complutense Madrid, Madrid, Spain.},
DOI = {10.18042/cepc/rep.202.03},
ISSN = {0048-7694},
