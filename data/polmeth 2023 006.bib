@article{ WOS:001126258500001,
Author = {Haponenko, Vira and Rykhlik, Volodymyr and Shulga, Marina and Bulbeniuk,
   Svitlana and Naumenko, Olha},
Title = {Informal institutionalization in modern Ukraine: Technological aspect},
