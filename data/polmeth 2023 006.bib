@article{ WOS:001146783800001,
Author = {Basu, Chitralekha},
Title = {How the media matters for the economic vote: Evidence from Britain},
Journal = {ELECTORAL STUDIES},
Year = {2024},
Volume = {87},
Month = {FEB},
Abstract = {Existing research finds that economic news can influence how citizens
   evaluate their governments, but the relative importance of different
   channels through which this effect arises, and how this may vary across
   contexts, remains unclear. Drawing on media dependency theory, I argue
   that we should observe larger media effects on citizens' economic
   evaluations during periods of economic stability than during crises.
   Moreover, during crises, we should observe larger media effects on
   citizens' evaluations of governing parties' responsibility for, and
   handling of, the economic situation, than on their subjective economic
   evaluations. Additionally, these effects should be stronger among
   governing party supporters. Analysis of British public opinion leading
   up to and following the 2007-8 global financial crisis provides
   empirical support for this theory. These findings have implications for
   our understanding of how the media matters for the economic vote, as
   well as voters' ability to use elections as instruments of
   accountability during crises.},
