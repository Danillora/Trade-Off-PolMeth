@article{ WOS:001237680100012,
Author = {Packova, Miroslava},
Title = {RUSSIAN ACTIVE MEASURES IN CYBERSPACE THROUGH THE LENS OF SECURITY
   SECTORS},
Journal = {POLITICKE VEDY},
Year = {2023},
Volume = {26},
Number = {4},
Abstract = {This article examines Russian active measures in cyberspace, focusing on
   the implications for across security sectors. Initially, the study
   contextualizes the increasing reliance on cyberspace for conducting
   subversive activities that target military, political, societal, and
   economic structures, particularly after the Ukrainian Euromaidan in
   2013. Utilizing a qualitative analysis of open -source data, the
   research identifies the diverse tactics employed by Russian actors,
   including sophisticated use of code and content to manipulate public
   perception and governmental processes. Key findings reveal that while
   activities in the military and political sectors are characterized by a
   high degree of technological sophistication, societal sectors
   predominantly face threats from disinformation campaigns. Notably, no
   significant cyber active measures were detected within the environmental
   sector. The study concludes that Russian cyber tactics are deeply
   embedded in their broader geopolitical strategy, necessitating robust
   countermeasures. Enhanced cybersecurity policies and international
   cooperation are recommended to bolster resilience against such foreign
   malign influences.},
