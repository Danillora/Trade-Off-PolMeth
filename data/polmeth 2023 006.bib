@article{ WOS:001020370200001,
Author = {Pugliese, Matteo},
Title = {The International Legion of Ukraine Exploring the Background and
   Motivations of Foreign Volunteers},
Journal = {RUSI JOURNAL},
Year = {2023},
Volume = {168},
Number = {3},
Pages = {46-58},
Month = {APR 16},
Abstract = {In the aftermath of Russia's full-scale invasion of Ukraine in February
   2022, President Volodymyr Zelensky established an International Legion
   and thousands of foreign volunteers came to defend the country. Matteo
   Pugliese examines the structure of the Legion, its recruitment process,
