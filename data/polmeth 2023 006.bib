@article{ WOS:000991340700001,
Author = {Kelbel, Camille and Navarro, Julien and Neihouser, Marie},
Title = {Great minds think alike? A new measure of MEPs-voters congruence
   following the 2019 European Parliament elections},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2023},
Volume = {22},
Number = {4},
Pages = {476-495},
Month = {DEC},
Abstract = {Do citizens and Members of the European Parliament agree on the format
   and future of the EU? While the literature has emphasized the gap
   between pro-European elites and increasingly Eurosceptic electorates,
   this article relies on a novel dataset to explore the implications of
   measures of such (in)congruence at the EU level. We compare the
   preferences of elected representatives and of voters on a wide range of
   issues: democracy at the supranational level, the reform of EU policies,
   as well as institutions. The empirical analysis relies on a citizen
   survey and on an MEP survey conducted in the framework of the RECONNECT
   project. We evidence that the level of congruence varies across issues
   and that it is the representatives, not the citizens, who drive
   polarization.},
