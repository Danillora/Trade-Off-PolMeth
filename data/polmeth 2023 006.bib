DOI = {10.18042/cepc/rep.200.05},
ISSN = {0048-7694},
EISSN = {1989-0613},
Keywords = {Foucault; Spaemann; freedom; emancipation; negative freedom; political
   community; love of friendship; recognition; relationality},
Research-Areas = {Government \& Law},
Web-of-Science-Categories  = {Political Science},
Author-Email = {vivianag@ucm.es},
Affiliations = {Complutense University of Madrid},
