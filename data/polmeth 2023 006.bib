@article{ WOS:001000720300006,
Author = {Kendrick, Jamie},
Title = {Environmental Protest as a Transformative Force for Democracy: The Cases
   of Poland and Serbia and the Birth of a Different Democratic Matrix},
Journal = {TOCQUEVILLE REVIEW},
Year = {2023},
Volume = {44},
Number = {1},
Pages = {89-111},
Month = {JUN 1},
Abstract = {The contribution of environmental movements to eastern Europe's recent
   history is underexamined and widely seen as marginal. However,
   environmental movements were important forces in the democratic
   transition and continue to guard against authoritarian drift and the
   weakening of the rule of law. Especially after the Chernobyl disaster,
   environmental movements in 1980s Poland organised opposition against the
   military dictatorship and helped lay the ground for the democratic
   transition. In 2020s Serbia, environmental movements protesting against
   infrastructure projects that undermine the wellbeing and livelihoods of
   local populations have formed a democratic alternative to established
   political elites. Both cases demonstrate the historical and ongoing
   significance of environmental movements to eastern Europe politics and
   society and underline how environmental movements expose the flaws and
   vulnerabilities of undemocratic systems. Drawing on historical research
   and the experiences of participants, this article shows how
   environmental movements protect, deepen and shape democracy as practised
   and experienced.},
