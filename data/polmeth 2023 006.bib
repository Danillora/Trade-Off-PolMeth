@article{ WOS:001127944300003,
Author = {Cotta, Benedetta and Domorenok, Ekaterina},
Title = {Conclusion: bringing together the eco-social debate and established
   political science perspectives: synergies and new research pathways},
Journal = {EUROPEAN POLITICAL SCIENCE},
Year = {2024},
Volume = {23},
Number = {1, SI},
Pages = {80-85},
Month = {MAR},
Abstract = {This symposium makes a first step in bridging the emerging eco-social
   debate and the established political science theories and concepts,
   indicating the mutually beneficial analytical perspectives and common
   research pathways that may arise. In addition to identifying several
   aspects in the policy, politics and polity dimensions that appear to be
   particularly relevant in view of the emerging eco-social policies, this
   collection of articles points out two cross-cutting themes, namely the
   transformation of the welfare state set-ups, and new cleavages and power
   relations, which pose new questions and open a promising research agenda
   for political scientists.},
