@article{ WOS:001240954000003,
Author = {Elsbach, Sebastian},
Title = {The Reichsbanner Schwarz-Rot-Gold: Defensive Democrats in the Weimar
   Republic},
Journal = {TOTALITARISMUS UND DEMOKRATIE},
Year = {2023},
Volume = {20},
Number = {2},
Pages = {171-192},
Abstract = {The Reichsbanner Black-Red-Gold was a private organisation for the
   protection of the Weimar Republic. It was founded in 1924. The
   Reichsbanner set itself the goal to encounter right-wing violence as
   well as left wing violence and sought the cooperation with the
   understaffed authorities. The activists of the Reichsbanner sought to
   protect political meeting of democratic parties, the pacifist movement
   and the Jewish community. Additionally, the goal was to strengthen the
   legitimacy of the republic through festivities, publications and
   day-to-day items. However, the internal division of the Reichsbanner
   with its 1,5 million members lead to a purely defensive course, which
