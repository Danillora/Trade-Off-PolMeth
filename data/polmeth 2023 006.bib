@article{ WOS:001114310700001,
Author = {Gash, Alison and Tichenor, Daniel},
Title = {Dreamers, Queer Kids, and American Culture Wars: Centering Young People
   in Identity Politics},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
Year = {2024},
Volume = {21},
Number = {3},
Pages = {455-473},
Month = {JAN 5},
Abstract = {Children routinely sit at the center of American culture wars, as
   parents, officials, and other adults battle over what kids learn in the
   classroom, which bathrooms they can use, whether they must wear masks or
   get vaccines, what sports teams they can join, and which schools they
   can attend. In Democracy's Child, we describe the crucial reciprocal
   relationship that exists between children and politics: young people are
   constituted by their political environs, but liberal democratic politics
   in turn are shaped and influenced by their young constituents. In this
   article we ask: what happens when the youngest voices in the most
   contentious policy battles take the reins - with a specific focus on
   youth leaders who are challenged by multiple identity-based structural
   barriers. We train a spotlight on how two youth-led movements - gay
   straight or gender-sexuality alliances (GSAs) and Dreamers - influenced
   the rights of LGBTQIA+ people and immigrants amidst opposition from
   adult adversaries and allies.},
