@article{ WOS:001071704600002,
Author = {Bein, Simon},
Title = {Collective political identities and their contribution to the
   explanation of governmental performance in democracies},
Journal = {ZEITSCHRIFT FUR VERGLEICHENDE POLITIKWISSENSCHAFT},
Year = {2023},
Volume = {17},
Number = {3},
Pages = {225-260},
Month = {SEP},
Abstract = {The socio-cultural foundations of a society play an important role in
   explaining performance in democratic orders, as Alexis de Tocqueville
   already recognized. However, with a few exceptions, this approach has
   not been systematically developed further, which is especially true for
   the subject of collective political identities. Building on the
   theoretical premise that a comprehensive anchoring of the liberal
   paradigm in the collective political identity of a democratic society
   emphasizes economic performance and prosperity, while a strengthening of
   republican-community-oriented elements places the focus on
   socio-economic security and equality, this study aims to contribute to
   closing this research gap. The object of the empirical analysis is a
   fuzzy-set QCA that tests whether liberal and republican political
   identities are necessary or sufficient conditions for economic
   prosperity and social equality. The two political identities are
   measured using the World Values Survey and the European Values Study for
   the period 2005 to 2021, while the basis for the performance data are
   the Sustainable Governance Indicators of the Bertelsmann Foundation for
   the years 2018 to 2022. As a result, liberal political identity can be
   seen as part of a combinatorial explanation for economic performance,
   while the explanatory contribution of republican identity to
   sociopolitical identity can only be partially demonstrated. Overall, an
   equifinal variety of solution paths for performance in Western
   democracies and the explanatory potential of both liberal and republican
   political identities emerges.},
Publisher = {SPRINGER VIEWEG-SPRINGER FACHMEDIEN WIESBADEN GMBH},
