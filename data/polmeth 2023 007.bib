@article{ WOS:000967530800002,
Author = {Battaglini, Marco and Sciabolazza, Valerio Leone and Patacchini,
   Eleonora},
Title = {Abstentions and Social Networks in Congress},
Journal = {JOURNAL OF POLITICS},
Year = {2023},
Month = {2023 APR 1},
Abstract = {We study the extent to which personal connections among legislators
   influence abstentions in the US Congress. Our analysis is conducted by
   observing representatives' abstentions for the universe of roll call
   votes held on bills in the 109th-113th Congresses. Our results show that
   a legislator's propensity to abstain increases when the majority of his
   or her alumni connections abstains, even after controlling for other
   well-known predictors of abstention choices and a large set of fixed
   effects. We further reveal that a legislator is more prone to abstain
   than to take sides when the demands from personal connections conflict
   with those of the legislator's party.},
