@article{ WOS:001128552500001,
Author = {Yoshida, Akinaga},
Title = {Beyond Non-bindingness: States' Implementation of UN Human Rights
   Treaties Bodies' Concluding Observations},
Journal = {NORDIC JOURNAL OF HUMAN RIGHTS},
