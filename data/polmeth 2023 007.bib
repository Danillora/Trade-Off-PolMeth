@article{ WOS:000935489500001,
Author = {Ling, Shuang},
Title = {What Drive People to Successfully Protest China's Environmental Project
   in Social Media Era? A Fuzzy-Set Qualitative Comparative Analysis},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Year = {2024},
Volume = {9},
Number = {4},
Pages = {551-576},
Month = {DEC},
Abstract = {Previous literatures have indicated a positive link between social media
   and social protest. However, there has been no clear elaboration on how
   social media influence the result of environmental protest. In this
   article, by analyzing 20 cases of environmental protest in China using
   fuzzy-set qualitative comparative analysis (fsQCA), we aim to explore
   which combinations of diverse conditions drive citizens to successfully
   protest China's environmental project in the age of social media. The
   findings indicated that the internet policy, mobilization pattern and
   visibility can be a key condition of successful protest in social media.
   This combination of high visibility, resonance and legitimacy levels can
   facilitate favorable outcomes. The tripartite relations drive successful
   protest to China's environmental project in the social media age among
   political opportunities, mobilization structure, as well as framing
   process that social media created. Three contextualized paths can be
   summarized to explain how social media influence the result of
   environmental protest in China. These findings increase our
   understanding of protest outcomes in China and further enrich the
   literature about the relationship between social media and protest
   outcomes.},
