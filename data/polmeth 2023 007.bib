@article{ WOS:001180544100005,
Author = {Samoylovskaya, N. A.},
Title = {ON THE PROHIBITION OF NUCLEAR TESTS IN THREE ENVIRONMENTS: BETWEEN FEAR
   AND DOUBT},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2023},
Number = {6},
Pages = {52-65},
Abstract = {The article examines the formation of the negotiation process between
   the USSR and the USA on nuclear disarmament and non-proliferation in the
   context of agreements on the prohibition of nuclear tests in three
   environments. The main approaches of Russia and the United States to the
   problem of nuclear disarmament were formulated in the 1950s, and the
   analysis of historical events allows us to answer the main research
   question: how, in conditions of an irreconcilable ideological struggle,
   distrust and competition, the two powers managed to take the first steps
   to find compromise solutions that led to the creation of the nuclear
   disarmament and non-proliferation regime. The historical analysis allows
   us to create analogies with the current situation and look at the
   current crisis in the field of nuclear disarmament retrospectively.
   Special attention is paid in the article to 2nd track diplomacy and to
   the political activity of scientists connected with the development of
   atomic and hydrogen weapons, that, according to the author, allowed to
   formulate the concept of nuclear responsibility, which was an important
   component of the policy of nuclear deterrence. Drawing an analogy with
   the current situation, the author comes to the conclusion that the
   crisis in the field of nuclear disarmament is associated with a return
   to the policy of deterrence without its key component - nuclear
   responsibility, which allowed to balance the power policy and keep
   nuclear powers from plunging the world into an uncontrolled conflict.},
