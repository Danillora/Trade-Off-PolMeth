@article{ WOS:001011906500001,
Author = {Dahbi, Khalil},
Title = {Relevance and electoralism in the study of political oppositions: the
   case of Morocco's radical left},
Journal = {DEMOCRATIZATION},
Year = {2023},
Volume = {30},
Number = {6},
Pages = {969-988},
Month = {AUG 18},
Abstract = {Relevance is a key concept in the study of political parties. This
   article argues that the conceptions of relevance used in the study of
   political oppositions in authoritarian settings need to be clarified and
   reformulated. More specifically, and in light of current knowledge on
   electoral authoritarianism, the notion of relevance must go beyond the
   electoralist conceptions traditionally used when studying democratic
   contexts. Drawing on the case of Morocco, the article highlights the
   obscuring effect that such conceptions can have. It then provides an
   alternative framework, adjusting the concept of relevance to the
   specificities of the context and the evolving nature of the actors
   operating within it. The case of al-Nahj al-Dimoqrati, a Moroccan
   radical-leftist opposition party, is used to illustrate the analytical
   power of the framework.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
