@article{ WOS:000877215700001,
Author = {MacMullen, Ian},
Title = {Justified Coercion, Political Cooperation, and Exemption from General
   Laws},
Journal = {JOURNAL OF POLITICS},
Year = {2023},
Month = {2023 JAN 1},
Abstract = {When, if ever, should people be exempted from a law because they cannot,
   given their particular beliefs and values, see the force of the reasons
   for that law? Such exemptions, often requested by religious and cultural
   minority groups, enhance the cooperative character of democratic
   government by eliminating instances in which some citizens must
   experience their subjection to state power as mere coercion. I defend
   three claims. First, the value of political cooperation is sometimes but
   not always pivotal in the moral evaluation of mere coercion. Second,
   political cooperation has both intrinsic and instrumental value, but the
   latter should not usually determine judgments about exemptions. Third,
