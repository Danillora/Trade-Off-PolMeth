@article{ WOS:000992102000001,
Author = {Wunsch, Natasha and Belanger, Marie-Eve},
Title = {Radicalisation and discursive accommodation: responses to rising
   Euroscepticism in the European Parliament},
Journal = {WEST EUROPEAN POLITICS},
