@article{ WOS:001217617800001,
Author = {Avila, Elizabeth Monica Torrico and Guerra-Mejia, Roberto},
Title = {Measuring the Reversal of the Displacement of the Ckunza Language of San
   Pedro de Atacama: a First Approximation},
Journal = {AMERICA LATINA HOY-REVISTA DE CIENCIAS SOCIALES},
Year = {2023},
Volume = {93},
Month = {APR},
Abstract = {The Ckunza language of the Chilean Lickan Antai is regarded as extinct.
   This community has worked on its revival since 2010. This descriptive
   -quantitative study explores the knowledge and use of the language among
   the members of the Atacamenean community. The results reveal an emerging
   reversal of the displacement of the Ckunza language. This article
   contributes to support the struggle for language revitalisation by
   supporting language planning as well as research in this field.},
