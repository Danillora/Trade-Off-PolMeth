@article{ WOS:001031613000007,
Author = {Dikaios, George and Blavoukos, Spyros},
Title = {Influencing the International Transport Regime Complex: The EU's Climate
   Action in ICAO and IMO},
Journal = {POLITICS AND GOVERNANCE},
Year = {2023},
