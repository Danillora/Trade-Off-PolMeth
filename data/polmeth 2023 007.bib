@article{ WOS:001262099800005,
Author = {Buu-Sao, Doris and Patinaux, Leny},
Title = {The inertia of transition: the reproduction capitalist landscapes in
   Europe, from copper mines to wind turbines},
Journal = {POLITIX},
Year = {2023},
Number = {144},
Pages = {99-123},
Abstract = {This article analyzes the role of social, economic, and environmental
   inertia in the industrial renewal resulting from the contemporary
   ``energy transition{''}. To do so, it draws on a comparison of two case
   studies: the reopening of a copper mine in Andalusia and the massive
   installation of wind turbines in northern France. A localized analysis
   of industrial trajectories in these two regions shows, on the one hand,
   how zones of sacrifice propitious to industrial renewal are constructed
