@article{ WOS:001208996600001,
Author = {Cicek, Aylin Ece},
Title = {Nuclear Powerplay: Traditional and Emerging State Dynamics in the United
   Nations General Assembly (1990-2017)},
Journal = {SIYASAL-JOURNAL OF POLITICAL SCIENCES},
Year = {2023},
Volume = {33},
Number = {1},
Abstract = {This article deals with the specific topic of nuclear non-proliferation
   under the umbrella of global governance- namely the United Nations'
   General Assembly. First, this work will present an account of the
   evolution of the nuclear nonproliferation regime and, secondly, examine
   the contemporary situation through a realist lens. More specifically, it
   is aimed to test powerful states' probability to shape the global
   agenda, through alignment. By establishing the theoretical bases and
   proposition on which the study is grounded, the following part will
   focus on methodology and hypotheses. By utilizing ANOVA analysis and
   Ordinary Least Square (OLS) regression, this paper aims to uncover the
   relationship -if any- between the factors affecting state behavior on
   global platforms under the issue area of nuclear weapons and
   nonproliferation efforts.},
