@article{ WOS:001041968600001,
Author = {Garcia-Ponce, Omar and Wantchekon, Leonard},
Title = {Critical Junctures: Independence Movements and Democracy in Africa},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
Year = {2024},
Volume = {68},
Number = {4},
Pages = {1266-1285},
Month = {OCT},
Abstract = {We show that current levels of democracy in Africa are linked to the
   nature of its independence movements. Using different measures of
   political regimes and historical data on anticolonial movements, we find
   that countries that experienced rural insurgencies tend to have
   autocratic regimes, while those that faced urban protests tend to have
   more democratic institutions. The association between the type of
   independence movement and democracy is statistically significant for the
   post-Cold War period and robust to a number of potential confounding
   factors and sensitivity checks. We provide evidence for causality in
   this relationship by using an instrumental variables approach and a
   difference-in-differences design with fixed effects. Furthermore, we
   adjudicate between two potential mechanisms and find support for a
   behavioral path dependence hypothesis. Urban protests enabled
   participants to develop norms of peaceful political behavior, which
   provided cultural bases for liberal democracy. In contrast, armed
   rebellions generated behavioral patterns that perpetuated political
   exclusion and the use of violence as a form of political dissent.},
