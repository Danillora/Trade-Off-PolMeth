@article{ WOS:001000536400001,
Author = {Bracic, Ana and Israel-Trummel, Mackenzie and Johnson, Tyler and Tipler,
   Kathleen},
Title = {``Because He Is Gay{''}: How Race, Gender, and Sexuality Shape
   Perceptions of Judicial Fairness},
Journal = {JOURNAL OF POLITICS},
Year = {2023},
Volume = {85},
Number = {4},
Pages = {1167-1181},
Month = {OCT 1},
Abstract = {How does a judge's identity affect perceptions of the ability to preside
   fairly? We theorize that identity categories operate as ideological cues
   and that the public views judges perceived as ideologically proximate to
   be fairer, more impartial, and more inspiring of trust in courts
   broadly. Using a conjoint survey experiment with a diverse national
   sample, we find support for this theory and show that race, gender, and
   especially sexuality are used as ideological cues. The effect of
   identities is conditioned by respondent partisanship. Democratic
   respondents trust judges with marginalized identities more than judges
   with dominant identities. Republicans are relatively indifferent to
   judges' race or gender but are significantly less trusting of gay
   judges. We also uncover limited effects when judges preside over a case
   in which their identity is salient. These results suggest that the
   public does not seek descriptive representation as such but uses
   identity categories to achieve ideological congruence.},
