@article{ WOS:001194153700006,
Author = {Barrie, Christopher and Fleming, Thomas G. and Rowan, Sam S.},
Title = {Does Protest Influence Political Speech? Evidence from UK Climate
   Protest, 2017-2019},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2024},
Volume = {54},
Number = {2},
Pages = {456-473},
Month = {APR},
Abstract = {How does protest affect political speech? Protest is an important form
   of political claim-making, yet our understanding of its influence on how
   individual legislators communicate remains limited. Our paper thus
   extends a theoretical framework on protests as information about voter
   preferences, and evaluates it using crowd-sourced protest data from the
   2017-2019 Fridays for Future protests in the UK. We combine these data
   with similar to 2.4m tweets from 553 legislators over this period and
   text data from similar to 150k parliamentary speech records. We find
   that local protests prompted MPs to speak more about the climate, but
   only online. These results demonstrate that protest can shape the timing
   and substance of political communication by individual elected
   representatives. They also highlight an important difference between
   legislators' offline and online speech, suggesting that more work is
   needed to understand how political strategies differ across these
   arenas.},
