@article{ WOS:001012633500004,
Author = {Masabo, Conrad John},
Title = {Nyerere and the African Theory of Democracy},
Journal = {DEMOCRATIC THEORY-AN INTERDISCIPLINARY JOURNAL},
Year = {2023},
Volume = {10},
Number = {1},
Pages = {36-51},
Abstract = {Statecraft, under democratic principles in Tanzania in partic-ular, is
   often considered as a total heritage from former colonial masters.
   Ju-lius Kambarage Nyerere (1922-1999) disputed this by advancing an
   African theory of democracy, articulated to inform modern statecraft in
   Tanzania. His theory advances a form of democracy characterized by a
   merger of some prac-tices from the African past and others from the
   western world. In this way, he articulated the centrality of democracy
   in organizing public affairs without compromising its African origin but
   also acknowledging the influence of other democratic cultures in the
   modern organization of a polity. This article articu-lates Nyerere's
   contribution to African democratic discourse and the extent to which his
   theory of democracy is relevant in the organization of contemporary
   politics and democratic trajectories in Tanzania and Africa in
   particular.},
