@article{ WOS:000992104900001,
Author = {Bischof, Daniel and Kurer, Thomas},
Title = {Place-Based Campaigning: The Political Impact of Real Grassroots
   Mobilization},
Journal = {JOURNAL OF POLITICS},
Year = {2023},
Month = {2023 JUL 1},
Abstract = {Generations of research have incrementally identified the circumstances
   under which electoral campaigns matter. Direct interpersonal contact
   within local networks is commonly seen as conducive to campaign impact,
   but empirical evidence is scarce because of demanding data requirements.
   We advance the literature by studying the Movimento Cinque Stelle (M5S),
   an important challenger party in Italy, which followed the unusual
   practice of coordinating political activities on a public online
   platform. We web scraped the entire history of the movement's more than
   1,000 local branches with over 200,000 geocoded political activities, to
   study the effect and mechanisms of their no campaign in the 2016
   constitutional referendum. Relying on regression, matching, and
   instrumental variable models, we demonstrate that local M5S mobilization
   had substantial campaign effects. Our results have important
   implications, as they highlight the effectiveness of locally rooted
   campaigns and the particular potency of place-based political
   mobilization.},
