@article{ WOS:000961708800001,
Author = {Thomsen, Mette Kjaergaard and Opstrup, Niels},
Title = {Priming citizens to think about public participation initiatives: Does
   it impact their view of government?},
Journal = {SCANDINAVIAN POLITICAL STUDIES},
Year = {2023},
Volume = {46},
Number = {1-2},
Pages = {23-51},
Month = {MAR},
Abstract = {Prior research has shown that public participation initiatives may have
   positive effects on how participating citizens view government. However,
   little is known about whether and how priming citizens to think about
   such initiatives influences the view of government among the larger
   public. Using a survey experiment, we find that the effect of priming
   citizens to think of a public participation initiative that includes a
   small group of citizens on their view of government is conditional on
   how proximate the service, which the initiative resolves around, is to
   citizens. Notably, across outcome measures we a find a negative priming
   effect among those for which the service is not proximate. These results
   are highly important, as they suggest that among the larger public,
   public participation initiatives-that involve few citizens-may have
   mixed results on the view of government, thus questioning the legitimacy
   of decisions reached by such initiatives.},
Publisher = {WILEY},
