@article{ WOS:001088030500001,
Author = {Taiclet, Anne-France and Delaporte, Arthur},
Title = {From stronghold to marginal constituency: the case of the 18th
   arrondissement in Paris (2014-2022)},
Journal = {FRENCH POLITICS},
Year = {2023},
Volume = {21},
Number = {4, SI},
Pages = {398-418},
Month = {DEC},
Abstract = {As many urban centers, the 18th arrondissement of Paris has changed in
   two important ways over the last fifteen years. The first is the
   socio-urban transformation of the territory itself: gentrification and
   its deepening of social divisions in the area. The second is the
   oscillations in voting patterns of residents. As a consequence, this
   territory which used to be a stronghold of the Socialist Party has
   gradually become more of a marginal constituency, along with uncertain
   electoral outcomes and harsher competition. A localized survey thus
   makes it possible to highlight this socio-spatial mosaic and explain the
   variations observed in electoral results since 2014. Considering the
   2020 municipal elections in a sequence going from 2014 to 2022 makes it
   possible to show the oscillations, the recompositions, which notably
   take place in the struggles between segments of the left, corresponding
   to struggles between fractions of classes, which the localized approach
   makes it possible to grasp more finely.},
