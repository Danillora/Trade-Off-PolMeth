@article{ WOS:000956800200005,
Author = {Cornejo, Rodrigo Castro},
Title = {The AMLO Voter: Affective Polarization and the Rise of the Left in
   Mexico},
Journal = {JOURNAL OF POLITICS IN LATIN AMERICA},
