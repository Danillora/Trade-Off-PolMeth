@article{ WOS:001073133500001,
Author = {Gulotty, Robert and Yu, Arthur Zeyang},
Title = {Must watch propaganda: the marginal treatment effect of foreign media
   among always-takers},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2025},
Volume = {13},
Number = {1},
Pages = {132-149},
Month = {JAN},
Abstract = {Studies of political persuasion often use an exogenous encouragement as
   an instrument for persuasive messaging. However, for some people, such
   encouragement is insufficient, while for others, it is unnecessary.
   These individuals are excluded from methods that only estimate a
   treatment effect among compliers. Using the marginal treatment effect
   framework, we extend research finding that exposure to West German
   television increases support for communism. We find that, because of
   self-selection, for those who watch West German TV regardless of signal
   quality, i.e. always-takers, cutting off West German television would
   have increased support for communism. Our extrapolation shows that media
   choices reinforce, rather than mollify, political preferences.},
