@article{ WOS:001078994100003,
Author = {Arancibia-Bustos, Alvaro and Flores-Capriles, Jose and
   Gutierrez-Penaloza, Susana and Galanakis-Valenzuela, Lucas},
Title = {The dispute over the content of the Constitution. Class struggle in the
   Chilean Constitutional Convention (2021-2022)},
Journal = {IZQUIERDAS},
Year = {2023},
Volume = {52},
Month = {AUG},
Abstract = {The present investigation analyzes the class struggle present in the
   dispute over the content within the Chilean Constitutional Convention of
   2021-2022. For this, a multivariate statistical analysis (Multiple
   Correspondence Analysis) is used in a database that collects information
   from different sources on the conventional ones, and their behavior
   within the instance. The results show a polarization of political
   positions based on class material interest. The foregoing refutes part
   of the discussion regarding the Chilean Social Outbreak (2019), which
   poses the conflict as an expression of identity struggles.},
Publisher = {ARIADNA EDICIONES \& SAINT PETERSBURG STATE UNIV},
