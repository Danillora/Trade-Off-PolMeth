@article{ WOS:000930639300001,
Author = {Ylisalo, Juha and Kulha, Katariina and Leino, Mikko and Siren, Rasmus
   and Rapeli, Lauri and Setala, Maija},
Title = {Acceptability of mandatory vaccination: a survey experiment on the
   effects of thresholds and justifications},
Journal = {POLITICAL RESEARCH EXCHANGE},
