@article{ WOS:001127266600001,
Author = {Bogumil, Joerg and Graefe, Philipp},
Title = {Inter-administrative Relations in the Federal System},
Journal = {POLITISCHE VIERTELJAHRESSCHRIFT},
Year = {2024},
Volume = {65},
Number = {3},
Pages = {417-446},
Month = {SEP},
Abstract = {Problems in administrative implementation have been increasingly
   prominent in recent years, particularly in the context of crises. Many
   of the problems in migration and integration policy, healthcare policy
   (COVID-19), social policy, or the slow progress of digitalizing public
   services in Germany can be traced back to issues in the interplay
   between administrations. Drawing on the concept of
   ``inter-administrative relations,{''} this article develops an
   analytical concept for enforcement actions that focuses on both the
   interlocking structure and interface processing as analytical
   dimensions. It is argued that analyzing these dimensions allows for
   systematically exposing and explaining the structural problems that
   regularly lead to coordination deficits in policy implementation across
   various administrative domains.},
