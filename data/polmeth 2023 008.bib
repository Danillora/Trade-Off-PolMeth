@article{ WOS:001237680100008,
Author = {Jakabovic, Lukas and Necas, Pavel},
Title = {POLITICAL AND MILITARY IMPLICATIONS OF PRIVATE MILITARY CONTRACTORS IN
   RUSSIA},
