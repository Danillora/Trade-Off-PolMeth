@article{ WOS:001127525900001,
Author = {Plakoudas, Spyridon and Sofitis, Vasileios},
Title = {Explaining the Bayraktar Paradox},
Journal = {RUSI JOURNAL},
Year = {2023},
Volume = {168},
Number = {6},
Pages = {42-52},
Month = {SEP 19},
Abstract = {The conflicts in Libya, Syria and the Caucasus catapulted Turkey to the
   status of a `drone power' thanks to its armed Bayraktar TB2. Owing to
   its reputation as a low-budget and combat-tested drone, Ukraine added it
   to its arsenal to counter Russia's escalating aggression. When the
   invasion started in February 2022, the so-called `game changer' drone
   was expected to help defeat the Russian army. The drone was less
   successful than anticipated and yet, in the same year, Baykar (the
   drone's manufacturer) registered the best sales ever. Spyridon Plakoudas
   and Vasileios Sofitis explore this paradox and how it might be
   explained.},
