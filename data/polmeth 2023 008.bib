@article{ WOS:001078994100002,
Author = {Duhalde, Byron},
Title = {Theoretical-Conceptual Reflections of Historical Institutionalism in
   International Relations. Contributions and Challenges to a Dynamic Field
   of Study.},
Journal = {IZQUIERDAS},
Year = {2023},
Volume = {52},
Month = {AUG},
Abstract = {This article reflects on the relevance of Historical Institutionalism
   (HI) in International Relations (IR), focusing on the study of
   International Regimes and Institutions. It highlights how HI explains
   persistence and gradual institutional change through its analytical
   tools and boundary conceptualizations based on endogenous change. The
   article concludes by identifying the challenges and opportunities of HI
   in IR, emphasizing the need to expand its application to the study of
   endogenous international institutional dynamics, where States generate
   effects on the social reality sought by International Institutions.},
