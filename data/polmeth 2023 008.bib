@article{ WOS:001346338800001,
Author = {Del Tredici, Romina and Gonzalez, Lucas and Zarazaga, Rodrigo},
Title = {Buying Social Peace: the Distribution of Social Plans During Cristina
   Kirchner and Mauricio Macri's Governments},
Journal = {REVISTA SAAP},
Year = {2023},
Volume = {17},
Number = {1},
Pages = {13-34},
Month = {MAY},
Abstract = {This work analyzes the distribution of social programs in Argentina and
   present a counterintuitive observation. The Justicialista Party of
   Argentina, which the literature describes as clientelistic, increased
   programmatic funds during the government of Cristina Fern \& aacute;ndez
   de Kirchner, while the Cambiemos coalition, which presented itself as
   programmatic, invested more in discretionary funds during the government
   of Mauricio Macri. The study presents a quantitative analysis of a total
   of 364 national government programs between 2008 and 2019, explores the
   processes that led to the results in both administrations using
   secondary sources, and concludes with some comparative implications. The
   main argument is that both parties in government distributed
   discretionary funds through social movements to contain possible social
   unrest, exchanging them for social peace and governability.},
