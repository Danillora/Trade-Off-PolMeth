@article{ WOS:001084591300003,
Author = {Lilja, Mona and Baaz, Mikael and Hassellind, Filip Strandberg},
Title = {De/Politicization as resistance: the `missing women' in India},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2023},
Volume = {16},
Number = {2, SI},
Pages = {177-192},
Month = {MAY 4},
Abstract = {Resistance is often depicted in terms of antagonism, in which its core
   is the politicization of different issues or identities. This paper,
   however, seeks to add to previous research by displaying how
   depoliticization could also be understood as a form of resistance.
   De/Politicization has previously been addressed in the Social Sciences
   as a power tactic. But, by bringing insight from, as well as
   illuminating, the `missing women' situation in India, we suggest that
   depoliticization could also be considered a tactical form of dissent.},
