@article{ WOS:001254748300008,
Author = {Serengil, Yusuf and Papageorgiou, Stavros},
Title = {THE CONTRIBUTION OF TURKIYE'S FORECAST FOR ACHIEVING CARBON NEUTRALITY
   IN 2053},
Journal = {TURKISH POLICY QUARTERLY},
Year = {2023},
Volume = {22},
Number = {3},
Pages = {76-85},
Month = {FAL},
Abstract = {Turkiye's forestry sector can play a vital role in helping to achieve
   the country's net zero target through absorbing greenhouse gases. By
   acting as a carbon sink, it relieves the need to undertake the most
   expensive abatement in hard-to-abate sectors, significantly reducing the
   cost of achieving carbon neutrality. This article outlines current
   trends in the forestry sector and what is needed for forestry to play
   its important role in reaching climate targets while improving land-use
   management.},
