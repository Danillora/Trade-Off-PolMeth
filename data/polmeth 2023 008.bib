@article{ WOS:000956800200002,
Author = {Barrientos, Armando},
Title = {Social Assistance Expansion and Political Inclusion in Latin America},
Journal = {JOURNAL OF POLITICS IN LATIN AMERICA},
Year = {2023},
Volume = {15},
Number = {1},
Pages = {25-46},
Month = {APR},
Abstract = {Has social assistance expansion contributed to political inclusion in
   Latin America? The current literature favours a ``policy exchange{''}
   approach, hypothesising that social assistance is an electoral asset
   exploited by governing coalitions. The findings from this literature are
   mixed. The article proposes an alternative approach emphasising
   political inclusion. In unequal societies where economic cooperation is
   regulated by institutions generating inequality and disadvantage, social
   assistance contributes to the political inclusion of disadvantaged
   groups. Analysis of Latin American Public Opinion Project data for 2010
   to 2019 data finds support for this hypothesis.},
