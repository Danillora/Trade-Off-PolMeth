@article{ WOS:001025563300001,
Author = {Montigny, Eric},
Title = {A new party system linked to the evolution of Quebec nationalism},
Journal = {FRENCH POLITICS},
Year = {2023},
Volume = {21},
Number = {3},
Pages = {249-268},
Month = {SEP},
Abstract = {The Quebec election of 2022 brought the Coalition Avenir Quebec (CAQ)
   back to power with an even bigger victory than its first victory in
   2018. This confirms the major realignment of the Quebec party system.
   The main aim of this article is to focus on this evolution in relation
   to Quebec nationalism. Using the theoretical framework of dominant
   parties developed by Vincent Lemieux (Les partis et leurs
   transformations. Le dilemme de la participation. Les Presses de
   l'Universite Laval, Quebec, 2005), it is argued that this party system
   reconfiguration is based on an evolution of Quebec nationalism rather
   than on its decline. First, this article situates the change in the
   party system over time and explains the breakdown of a party system that
   had been shaped for most of the last 50 years by the pro-independence
   Parti Quebecois (PQ). To understand this change, survey data are also
   used to illustrate the declining salience of the independence issue in
   public opinion while explaining the nature of the new autonomist policy
   of the CAQ. Finally, this article analyses the consequence of the rise
   of Quebec solidaire for the pro-independence movement and the future of
   the PQ as a sovereigntist intraparty coalition.},
