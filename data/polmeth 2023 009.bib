@article{ WOS:001013178300001,
Author = {Kucherenko, Sergey A.},
Title = {THE CONCEPT OF POWER AND ITS TRANSFORMATION IN POLITICAL REALISM},
Journal = {POLITEIA-JOURNAL OF POLITICAL THEORY POLITICAL PHILOSOPHY AND SOCIOLOGY
   OF POLITICS},
Year = {2023},
