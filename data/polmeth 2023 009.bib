@article{ WOS:001088045200001,
Author = {Zhelyazkova, Asya and Thomann, Eva and Ruffing, Eva and Princen,
   Sebastiaan},
Title = {Differentiated policy implementation in the European Union},
Journal = {WEST EUROPEAN POLITICS},
Year = {2024},
