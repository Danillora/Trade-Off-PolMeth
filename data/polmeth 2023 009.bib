@article{ WOS:001171118900022,
Author = {Navarro Caro, Valentin},
Title = {THE INDOMITABLE MASS. NOTES ON THE CONCEPT OF DEMOCRACY IN ORTEGA Y
   GASSET},
Journal = {REVISTA INTERNACIONAL DE PENSAMIENTO POLITICO},
Year = {2023},
Number = {18},
Pages = {429-446},
Abstract = {Jose Ortega y Gasset is one of the most brilliant philosophers of the
   20th century in Spain. His work, however, so highly praised in fields
   such as anthropology and epistemology, has been criticised and
   denigrated in the political sphere for its defence of the theory of the
   elite, to the extent that some have seen him as a precursor of fascism.
   The aim of these pages is, on the contrary, to show how, contrary to
   what is traditionally held, Ortega was a staunch supporter of the
   democratic system, to the defence of which he devoted his life and not
   only his work.},
