@article{ WOS:001179236800003,
Author = {Gorgi, Hossein Siamian and Ardestani, Ahmad Bakhshaishi and Fam,
   Mohammad Toheed},
Title = {Analyzing the effective factors in the failure of the constitutional
   revolution based on the structure-agent model},
