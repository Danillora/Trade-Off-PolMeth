@article{ WOS:001009633400001,
Author = {Sances, Michael W. W.},
Title = {Defund My Police? The Effect of George Floyd's Murder on Support for
   Local Police Budgets},
Journal = {JOURNAL OF POLITICS},
Year = {2023},
Month = {2023 JUL 1},
Abstract = {Did George Floyd's murder and the subsequent protests affect attitudes
   toward local police budgets? Surveys conducted after Floyd's murder may
   miss attitudinal change among urban residents, and past studies of
   police violence have not examined attitudes toward budgets. I examine
   two cities where millions in police spending is regularly subject to
   popular vote and where external events forced both cities to hold
   referendums just weeks after Floyd's murder. Police spending is
   overwhelmingly popular in both cities before 2020, with majorities in
   nearly all precincts voting in favor; in 2020, average support drops and
   there is considerable heterogeneity across precincts. Areas with more
   Black residents and closer to local police killings see especially large
   decreases. In these two cities, the immediate effect of the Floyd
   protests was to modestly reduce support for police spending overall and
   to polarize support along racial lines.},
