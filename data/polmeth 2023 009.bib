@article{ WOS:001151035600001,
Author = {Hoekstra, Kinch},
Title = {Philosophical discourse and myth},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2025},
Volume = {28},
Number = {2},
Pages = {355-360},
Month = {FEB 23},
Abstract = {This paper questions the argument in Tae-Yeoun Keum's Plato and the
   Mythic Tradition in Political Thought about the relation between myth
   and philosophical discourse. In the context of Plato's Republic in
   particular, myth remains but one of the several devices that are
   presented as acceptable only if they are vetted by and consistent with
   reason. More generally, I ask about the justification or grounding of
   myth, and how we can know that myth is warranted if it is
   incommensurable with reason.},
