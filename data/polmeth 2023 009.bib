@article{ WOS:000967610400008,
Author = {Hoellerbauer, Simon},
Title = {Why Join? How Civil Society Organizations' Attributes Signal Congruence
   and Impact Community Engagement},
Journal = {JOURNAL OF EXPERIMENTAL POLITICAL SCIENCE},
Year = {2023},
Volume = {10},
Number = {1},
Pages = {88-99},
Abstract = {Civil society organizations (CSOs) can facilitate collective action.
   This makes understanding what shapes whether people are likely to engage
   with CSOs critically important. This paper argues that whether an
   organization is perceived as congruent - similar to an individual in
   values - is a key determinant of whether individuals will engage with
   it. I use a conjoint survey experiment to test how organizational
   attributes signaling congruence influence respondents' willingness to
   attend a hypothetical organization's meetings. I find that individuals
   are more likely to choose organizations that are more likely to be
   congruent with them, except when it comes to funding. These findings
   imply that an individual's level of comfort with a CSO matters for
   engagement; thus, CSOs need to consider how they match to their publics
   when reaching out to potential joiners. Furthermore, donors seeking to
   support CSOs need to pay attention to their impact on perceptions of
   congruence.},
