@article{ WOS:001077820700004,
Author = {Titeca, Kristof},
Title = {Russian Influence, anti-Western Sentiments, and African Agency: The
   Struggle for Influence in the Democratic Republic of Congo},
Journal = {STRATEGIC REVIEW FOR SOUTHERN AFRICA},
Year = {2023},
Volume = {45},
Number = {1},
Pages = {49-68},
Abstract = {Russia has been trying to extend its influence over Africa over the last
   years; a process which became intensified due to the Russia-Ukraine war.
   Through the case of the Democratic Republic of Congo (DRC), this article
   shows that although Russia plays an active role in this quest for
   influence by offering incentives (such as shipments of weapons),
   anti-Western sentiments in the DRC are at least as important. These
   sentiments have been magnified by the M23 rebellion and perceptions of
   Western complicity in this crisis. This has resulted in pressure within
   the Congolese administration, particularly from the security forces, to
   ``shift to Russia,{''} as well as Western efforts to counter this
   influence. At the same time, this `neo-Cold War' has offered
   opportunities for political leverage by the Congolese government to
   exercise pressure on Western governments-something which has not always
   been successful.},
