@article{ WOS:001039347400008,
Author = {Coticchia, Fabrizio and Moro, Francesco N.},
Title = {The Italian armed forces and the new conflicts in Europe},
Journal = {CONTEMPORARY ITALIAN POLITICS},
Year = {2023},
Volume = {15},
