@article{ WOS:001064093000007,
Author = {Clair, Isabelle and Rault, Wilfried and Tissot, Sylvie and Maudet,
   Marion and Monteil, Lucas},
Title = {Investigating sexuality and social class},
Journal = {POLITIX},
Year = {2023},
Number = {141},
Pages = {123-144},
Abstract = {Studies of sexuality and social class have different theoretical,
   epistemological and methodological roots. In this interview, we retrace
   three innovative lines of research that have marked French sociological
   and political science thinking on class/sexuality relations over the
   past fifteen years. Isabelle Clair, Sylvie Tissot and Wilfried Rault
   explain the stages and conditions involved in making the articulation of
   these two social relations of power a problem, and, more broadly, the
   kitchens of scientific production, offering a highly reflexive look back
   at their careers, which have been marked by major cultural, social and
   academic transformations.},
