@article{ WOS:001124991300001,
Author = {Pfeifer, Hanna and Schwab, Regine},
Title = {Re-examining the State/Non-State Binary in the Study of (Civil) War},
Journal = {CIVIL WARS},
Year = {2023},
Volume = {25},
Number = {2-3, SI},
Pages = {428-451},
Month = {JUL 3},
Abstract = {One of the fundamental distinctions informing studies on civil war is
   that between state and non-state actors as parties to an armed conflict.
   As we argue, however, this binary has recently come under increased
   scrutiny in light of real-world developments in armed conflicts. The
   article builds on newer scholarly contributions that have exposed the
   porous boundaries between state and non-state actors and orders while
   demonstrating a striking convergence in their behaviour. Drawing on
   examples from conflict zones in West Asia and North Africa, we
   investigate phenomena in civil wars that uncover the tenuity of
   state/non-state distinction.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
