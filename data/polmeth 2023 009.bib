@article{ WOS:001128756800007,
Author = {Barroso, Borja Sanchez},
Title = {Euthanasia and assisted suicide: a comparative analysis of recent
   developments in Germany, Austria, Portugal and Spain},
Journal = {TEORIA Y REALIDAD CONSTITUCIONAL},
Year = {2023},
Volume = {52},
Pages = {579-608},
Abstract = {Spain and three of its closest neighbours (Germany, Austria and
   Portugal) have recently undergone drastic legal changes regarding
   euthanasia and assisted suicide . Our article performs a Compar-ative
   Law analysis of the solutions adopted in each country. It seems
   especially relevant if we consider that, despite their apparent
   similarity at first sight, those solutions present significant
   differences among them. These differences also show some broader
   constitutional issues underlying the central problem, which are pointed
   out pending further discussion.},
