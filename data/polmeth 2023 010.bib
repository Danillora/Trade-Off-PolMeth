@article{ WOS:000995613200005,
Author = {Caldevilla-Dominguez, David and Barrientos-Baez, Almudena and
   Padilla-Castillo, Graciela},
Title = {Dilemmas Between Freedom of Speech and Hate Speech: Russophobia on
   Facebook and Instagram in the Spanish Media},
Journal = {POLITICS AND GOVERNANCE},
