@article{ WOS:001334756200001,
Author = {Lawson, Fred H. and Legrenzi, Matteo},
Title = {How conciliating Taliban-led Afghanistan ended up endangering Iran},
Journal = {GLOBAL CHANGE PEACE \& SECURITY},
Year = {2023},
Volume = {35},
Number = {1},
Pages = {79-93},
Month = {JAN 2},
Abstract = {Iran found itself in a perilous situation following the Taliban's return
   to power in Afghanistan. As a way to promote Iranian security, Tehran
   maintained the conciliatory policy toward the Taliban that it had
   pursued over the preceding decade. This undertaking not only failed to
   mitigate the threats facing Iran, but also generated further dangers.
   Interactions between the two states highlight an often overlooked aspect
   of the security dilemma: Measures that a state implements under anarchic
   conditions to enhance its security relative to an adversary generate
   unintended consequences that put its safety in further jeopardy, whether
   such measures are antagonistic or conciliatory.},
