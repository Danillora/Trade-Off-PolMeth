@article{ WOS:001104336400011,
Author = {Loner, Enzo},
Title = {Winners or losers? Using Twitter to evaluate how the COVID-19 pandemic
   changed the images of political leaders in Italy},
Journal = {CONTEMPORARY ITALIAN POLITICS},
