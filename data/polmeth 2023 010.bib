@article{ WOS:001269517900009,
Author = {Leroy, Alice and Bachschmidt, Carlo Augusto},
Title = {Genoa for the record},
Journal = {LABORATOIRE ITALIEN-POLITIQUE ET SOCIETE},
Year = {2023},
Number = {31},
Abstract = {Nell'agosto del 2022, Carlo Augusto Bachschmidt, regista e militante, ha
   partecipato al festival \& Eacute;tats g \& eacute;n \& eacute;raux du
   film documentaire de Lussas , nell'Ard \& egrave;che francese, per
   presentare l'archivio che aveva contribuito a costituire all'interno del
   Genoa Legal Forum, un gruppo di coordinamento nato dopo il Genoa Social
   Forum per accompagnare gli avvocati delle parti civili e degli imputati
   nel lungo procedimento giudiziario seguito al contro-vertice del 2001.
   Per avviare la discussione sulle questioni giudiziarie e memoriali
   legate all'uso di questo archivio, ha portato con s \& eacute; due suoi
   film: OP Genova (2007), realizzato durante il processo sulla base di
   materiali audiovisivi archiviati, e Black Block (2011), realizzato dopo
   i processi con alcune delle vittime della Diaz. La discussione con il
   pubblico di Lussas \& egrave; stata l'occasione per ripercorrere la
   creazione di questo archivio, che non ha precedenti per le sue
   dimensioni e la diversit \& agrave; dei suoi formati -documentari, video
   militanti, riprese dei canali televisivi locali, registrazioni audio
   delle comunicazioni radio della polizia - parlare anche dei montaggi e
   delle ricostruzioni a cui ha dato luogo, non solo nel contesto
   processuale ma anche nei circoli militanti. Il progetto di questo
   archivio e i suoi usi possono quindi essere intesi come un'impresa non
   di istituzionalizzazione ma di rilettura critica d'un materiale :
   raccogliere e conservare, s \& igrave;, ma al servizio di un'ambizione
   inconciliabile con il quadro delle istituzioni politiche e giudiziarie.},
