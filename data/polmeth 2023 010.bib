@article{ WOS:001037182300001,
Author = {Linhart, Eric and Jankowski, Michael and Tepe, Markus},
Title = {Electoral system preferences of citizens compared: evidence from a
   conjoint experiment in Germany, the Netherlands, and the United Kingdom},
Journal = {EUROPEAN POLITICAL SCIENCE REVIEW},
Year = {2023},
Volume = {15},
Number = {4},
Pages = {671-689},
Month = {NOV},
Abstract = {Electoral systems fulfill different functions. Typically, they cannot
   meet all demands at the same time, so that the evaluation of specific
   electoral systems depends on subjective preferences about the single
   demands. We argue that it is the electorate which transfers its power to
   representatives and, therefore, its preferences should be considered in
   debates about electoral systems. Consequently, our contribution presents
   results of citizens' demands regarding electoral system attributes.
   Specifically, we rely on a large-scale conjoint experiment conducted in
   Germany, the Netherlands, and the UK in which subjects were asked to
   choose between two electoral systems which randomly differed on a set of
   attributes referring to electoral systems' core functions. Our results
   show that all core functions are generally of importance for the
   respondents but reveal a higher preference for proportional electoral
   systems. These preferences are largely stable for citizens in different
   countries but also for other subgroups of subjects.},
