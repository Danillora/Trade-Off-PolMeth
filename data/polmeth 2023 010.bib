@article{ WOS:001064949000006,
Author = {Giacomini, Gabriele},
Title = {Scenarios of female participation in politics. From the past to the
   digital future},
Journal = {STORIA DEL PENSIERO POLITICO},
Year = {2023},
Volume = {12},
Number = {2},
Pages = {275-294},
Month = {MAY-AUG},
Abstract = {Female participation in the digital age is crucial for an equal
   democracy. Starting with feminist political thought and conducting an
   environmental scan of the most relevant forces in digital society, four
   possible futures for women's political participation online are
   explored. The preferable scenario, synergistic progress, sees equality
   achieved through inclusion and open dialogue. Key strategies include
   support for female participation, access to technology, policies
   promoting freedom and equality for both genders, and fostering
   dialogical and inclusive environments. Scenarios stimulate reflection
   and encourage conscious choices for a more equitable and inclusive
   future.},
