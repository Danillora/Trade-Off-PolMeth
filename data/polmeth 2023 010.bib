@article{ WOS:001034311000001,
Author = {Tynes, Robert},
Title = {American-Made Child Soldiers},
Journal = {CIVIL WARS},
Year = {2024},
Volume = {26},
Number = {3, SI},
Pages = {542-570},
Month = {JUL 2},
Abstract = {In America, there is a pervasive social force constantly at work,
   reinforcing the norm that children should be soldiers. With the U.S.
   case, we see that it takes a society to produce children on the
   battlefield, and that American culture is weakening the global norm
   against child soldiering. American schools, summer camps, and media are
   generating narratives that help soldier the minds of children. The U.S.
