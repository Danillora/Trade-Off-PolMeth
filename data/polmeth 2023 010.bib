@article{ WOS:001006088000001,
Author = {Hendrix, Cullen S. and Macdonald, Julia and Powers, Ryan and Peterson,
   Susan and Tierney, Michael J.},
Title = {The Cult of the Relevant: International Relations Scholars and Policy
   Engagement Beyond the Ivory Tower},
