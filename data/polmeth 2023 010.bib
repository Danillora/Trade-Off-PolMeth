@article{ WOS:000983079600001,
Author = {Bigman, Akiva and Lebel, Udi},
Title = {`Dying Was Never a Preference' - Post-War Ethical Discourse (PWED):
   Casualty Aversion and the War Legitimizing Projects},
Journal = {DEMOCRACY \& SECURITY},
Year = {2023},
Volume = {19},
Number = {2, SI},
Pages = {161-182},
Month = {APR 3},
Abstract = {Modern case studies are presented to show how the idea of war in Western
   societies is legitimized through an ethical script by which war and
   warfare result in few losses to both sides. A Post-War Ethical Discourse
   model is used to describe the prevalent postwar condition of casualty
   aversion, by which modern leaderships attempt to convince the public of
   the need to justify the idea of military empowerment and to legitimize
   the idea of war, through technological revolutions and doctrines aimed
   at assuring that the next war will be more precise and more ethical,
   distancing soldiers from the battle field and thereby resulting in less
   casualties to both sides. The paper illustrates this condition by
   analyzing events that took place after World War 1, World War 2 and the
   Vietnam War, as well as an Israeli case study.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
