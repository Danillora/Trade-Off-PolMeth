@article{ WOS:001054253100003,
Author = {Oyarzo, Cristina},
Title = {Political stories in Silvia Rivera Cusicanqui: passages of a dislocated
   enunciation, 1984-1988},
Journal = {IZQUIERDAS},
Year = {2023},
Volume = {52},
Month = {AUG},
Abstract = {In the 1980s, Aymara sociologist Silvia Rivera Cusicanqui (SRC) played
   an important role in debates that questioned topics such as racism,
   coloniality, and the inability of states to manage ethnic differences in
   Bolivia, but also with strong connections to Latin America as a whole.
   This article focuses on the author's production during that period,
   concentrating on 2 books and one theoretical-methodological article
   published between 1984 and 1988, with the aim of analyzing her political
   narratives. The hypothesis is that this writing marks an initial stage
   in Rivera's thinking, materializing a growing politicization and
   indigenization while constructing a dislocated enunciation from
   conventional thought, and at the same time, situated in time and space.
   Using intellectual history as the theoretical framework, the goal is to
   access the text, context, and authorship, under the premise that the
   discursive act reflects and modifies social reality.},
