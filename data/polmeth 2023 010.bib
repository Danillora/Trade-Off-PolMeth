@article{ WOS:001083047200001,
Author = {Pickup, Mark and Kimbrough, Erik O. and de Rooij, Eline A.},
Title = {Crossing the Line: Evidence for the Categorization Theory of Spatial
   Voting},
Journal = {BRITISH JOURNAL OF POLITICAL SCIENCE},
Year = {2024},
Volume = {54},
Number = {3},
Pages = {946-956},
Month = {JUL},
Abstract = {Bolstad and Dinas (2017) propose a model of spatial voting, based on
   social identity theory, that suggests supporting a candidate/policy on
   the other side of the ideological spectrum has a disutility that is not
   accounted for by common spatial models. Unfortunately, the data they use
   cannot speak directly to whether the disutility arises because
   individuals perceive their ideology as a social identity. We present the
   results of an experimental study that measures the norm against crossing
   the ideological spectrum; tests the cost of doing so, controlling for
   spatial effects; and demonstrates that this cost increases with the
   salience and strength of identity norms. By demonstrating the norm
   mechanism for the disutility of crossing the ideological spectrum, we
   provide strong support for B\&D's model.},
