@article{ WOS:001024010200001,
Author = {Herron, Erik S. S. and Pelchar, Khrystyna},
Title = {Undivided Ukraine: Evidence of Rallying Effects and an Emerging National
   Consensus},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2024},
Volume = {71},
Number = {4},
Pages = {283-295},
Month = {JUL 3},
Abstract = {The escalation of Russia's eight-year war against Ukraine in February
   2022 did not produce the outcomes that Russian political elites
   anticipated. This paper uses almost a decade of public opinion data
   collected by the Ukrainian agency Info Sapiens and a recent survey
   conducted by the Kyiv International Institute of Sociology to analyze
   changes in attitudes that can help explain the responses of citizens. We
   explore how key events have produced rallying effects and changes in
   behavior. Further, we explore the implications for postwar
   reconstruction efforts. Our findings suggest that the challenges of the
   last decade have unified Ukrainians.},
