@article{ WOS:001112742300001,
Author = {Gerez, Julian E. and Miller, Michael G.},
Title = {The Electoral Dynamics of Capital Punishment Commutations},
Journal = {STATE POLITICS \& POLICY QUARTERLY},
Year = {2024},
Volume = {24},
Number = {1},
Pages = {99-109},
Month = {MAR},
Abstract = {We explore electoral explanations for U.S. governors' willingness to
   commute death sentences in their state. Across descriptive tests and
   pre-registered regression specifications, we find little evidence that
   election timing or term limits affect either the probability of
   commuting death sentences or the proportion of such sentences governors
   might commute. However, we do find evidence that governors are more
   likely to commute sentences - and commute sentences for a higher
   proportion of defendants - during the ``lame duck{''} period after their
   successor's election but before their inauguration.},
