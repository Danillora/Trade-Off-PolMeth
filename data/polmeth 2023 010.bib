@article{ WOS:001051106800001,
Author = {Keum, Tae-Yeoun},
Title = {A reply to my critics},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2025},
Volume = {28},
Number = {2},
Pages = {366-373},
Month = {FEB 23},
Abstract = {In this article, I respond to critics of Plato and the Mythic Tradition
   in Political Thought. First, I defend my choice to carve up the concept
   of myth into deep myths and literary myths. I address concerns that the
   effective focus of my book on literary myths risks jettisoning the
   political stakes of myth, and that it may not solve the definitional
   problems the move seeks to mitigate. Second, I answer my critics'
   invitation to elaborate on the relationship between myth and critical
   reason. In particular, I attempt to resolve the tension they perceive
   between my definition of myth, as a medium that is distinctively opaque
   to criticism, and my argument that it can complement critical thought.},
