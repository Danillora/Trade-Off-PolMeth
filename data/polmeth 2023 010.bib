@article{ WOS:001152542300001,
Author = {Keum, Tae-Yeoun},
Title = {Introduction: Myths of Plato, myths of modernity},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2025},
Volume = {28},
Number = {2},
Pages = {331-338},
Month = {FEB 23},
Abstract = {This introduction presents an overview of Plato and the Mythic Tradition
   in Political Thought and its central arguments. I situate the
   contributions of the book within theoretical work on political myth,
   both traditional and more recent, and also within scholarship on the
   philosophical function of Plato's myths. Whereas political theorists
   have long conceived of myth in pathological terms, Plato and the Mythic
   Tradition joins a growing body of work envisioning a more constructive
   role for myth in politics and philosophy. This was a task that
   preoccupied Plato and some of his most significant modern readers.
   Recovering their common project, moreover, helps us tell a different
   story about Plato's philosophical myths and their reception in modern
   political thought.},
