@article{ WOS:001105161700001,
Author = {Johnson, Janet Elise},
Title = {Russia's Authoritarian Policymaking: The Politics of Domestic Violence
   after Partial Decriminalization},
Journal = {PROBLEMS OF POST-COMMUNISM},
