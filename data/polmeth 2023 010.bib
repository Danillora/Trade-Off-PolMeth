@article{ WOS:001099990300001,
Author = {Bvirindi, Tawanda Ray and Chadambuka, Patience and Tombindo, Felix},
Title = {Remembering Lumumba's dismembered body-polity through Amin},
Journal = {POLITIKON},
Year = {2023},
Volume = {50},
Number = {4, SI},
Pages = {407-418},
Month = {OCT 2},
Abstract = {The paper is nested within the decolonial theme by repositioning Patrice
   Lumumba's decolonial thought within the radical writings of Samir Amin.
   As seen in his famous independence speech, Patrice Lumumba is arguably
   one of the African decolonial giants who championed the remembering of
   the black bodies by challenging their dismembering facilitated and
   sustained by the grand construction of the human by Euro-modernity.
   Rereading his decolonial ideas through repositioning it in Amin's works
   comes at an opportune time given the recent burial of his last known
   remains - a tooth that was removed from his body as a trophy by a
