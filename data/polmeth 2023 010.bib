@article{ WOS:001136385900018,
Author = {Umarov, Temur},
Title = {Cooperation instead of Conflict Russia and China in Central Asia},
Journal = {OSTEUROPA},
Year = {2023},
Volume = {73},
Number = {7-9},
Pages = {287+},
Abstract = {China's importance in Central Asia is growing. This applies both in
   terms of economics and security policy. All the same, Russia remains an
   important player in the region. The shift in power does not lead to
   conflicts between Beijing and Moscow. Common interests in Central Asia
   and global politics override potential competition. In particular,
   maintaining the stability of the region's authoritarian regimes and
   shielding them from Western influences is a stated common goal.
