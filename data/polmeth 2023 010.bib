@article{ WOS:001013075300001,
Author = {Haas, Violeta I. and Stoetzer, Lukas F. and Schleiter, Petra and
   Kluever, Heike},
Title = {Can wedge strategies by mainstream parties cross-cut the
   anti-immigration far right vote?},
Journal = {ELECTORAL STUDIES},
Year = {2023},
Volume = {83},
Month = {JUN},
Abstract = {Anti-immigration campaigns have helped far right parties to establish
   themselves in party systems around the world. We examine whether
   mainstream parties can employ wedge issue campaigns that divide the far
   right anti-immigration vote to win back electoral support. Wedge issues
   that cross-cut the anti-immigration vote may enhance the electoral
   support of mainstream parties, as long as they do not simultaneously
   alienate proimmigration voters. We evaluate this expectation using a
   panel survey experiment conducted during the 2021 German federal
   election. The first wave allows us to identify wedge issues that the
   mainstream CDU/CSU can stress to cross-cut the anti-immigration vote.
   The second wave raises the salience of these issues by manipulating the
   perceived issue agenda of the CDU/CSU using hypothetical campaign
   posters. While our results show that wedge issue strategies are not
   effective on average, exploratory analyses reveal the potential of
   strategically targeted messaging in winning back support of some
   anti-immigration voters.},
