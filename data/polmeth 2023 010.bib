@article{ WOS:001087788300001,
Author = {Schulpen, Lau and van Kempen, Luuk and Elbers, Willem and van Uden,
   Daniella},
Title = {Beyond the Immediate Effects of COVID-19: Exploring the Consequences of
   the Pandemic on the Southern NGO Partners of Dutch INGOs},
Journal = {JOURNAL OF CIVIL SOCIETY},
Year = {2024},
Volume = {20},
Number = {1},
Pages = {74-92},
Month = {JAN 2},
Abstract = {Covid-19 has a significant impact on societies and individuals but also
   on organizations. Using a survey and focus group discussions, this
   article examines (perceptions of) Covid-19's impact on the Southern NGO
   partners of Dutch INGOs in the context of international development. The
   analysis explores how Covid-19 has affected Southern NGOs' (SNGOs)
   organizational capacity and relations with donors. This article finds
   that Covid-19 has had substantial and largely negative consequences on
   SNGOs' financial situation, staffing, and activities. Furthermore, the
   findings suggest that Covid-19 contributed (at least temporarily) to
   more equal relations between Southern NGOs and donors. The results
   emphasize the need for donors, including INGOs, to offer financial
   safety nets to ensure that SNGOs remain operational when crises hit and
   the need for both sides to keep up the momentum to shift the power
   relation.},
