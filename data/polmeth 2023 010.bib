@article{ WOS:001035803900002,
Author = {Sanchez, Ana Sanchez-Sierra},
Title = {The vital teaching of Jose Ortega y Gasset in the work of
   Luis Diez del Corral},
Journal = {REVISTA DE ESTUDIOS POLITICOS},
