@article{ WOS:000932082100001,
Author = {Burni, Aline and Stockemer, Daniel and Hackenesch, Christine},
Title = {Contagious politics and COVID-19: does the infectious disease hit
   populist supporters harder?},
Journal = {CONTEMPORARY POLITICS},
Year = {2023},
Volume = {29},
Number = {4},
Pages = {466-491},
Month = {AUG 8},
Abstract = {As an example of a typical right-wing populist, Jair Bolsonaro
   downplayed Covid-19 and rejected scientific evidence to address the
   pandemic. We argue that both his communication style and approach to
   crisis management had consequences for the behavioural patterns of his
   followers, which, in turn, had public health implications. Building on
   survey research, we demonstrate how Bolsonaro's supporters were less
   likely to consider the pandemic as a key challenge for the country, less
   worried about getting infected and less likely to wear masks. We show
   that this `riskier' behaviour had concrete repercussions. Even after
   controlling for confounders such as population density, age, education
   and wealth, municipalities with higher aggregate support for Bolsonaro
   had higher Covid-19 infection rates in 2020 and saw more people dying
   from the virus.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Hackenesch, C (Corresponding Author), German Inst Dev \& Sustainabil IDOS, Tulpenfeld 6, D-53113 Bonn, Germany.
   Burni, Aline; Hackenesch, Christine, German Inst Dev \& Sustainabil IDOS, Bonn, Germany.
   Stockemer, Daniel, Univ Ottawa, Fac Social Sci, Polit Studies, Ottawa, ON, Canada.
