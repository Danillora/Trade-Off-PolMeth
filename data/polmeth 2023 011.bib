@article{ WOS:000946791300001,
Author = {Smetana, Michal and Vranka, Marek and Rosendorf, Ondrej},
Title = {The ``Commitment Trap{''} Revisited: Experimental Evidence on Ambiguous
   Nuclear Threats},
Journal = {JOURNAL OF EXPERIMENTAL POLITICAL SCIENCE},
Year = {2024},
Volume = {11},
Number = {1},
Pages = {64-77},
Month = {SPR},
Abstract = {In this paper, we provide an empirical test for the theoretical claim
   that ambiguous nuclear threats create a ``commitment trap{''} for
   American leaders: when deterrence fails, supposedly they are more likely
   to order the use of nuclear weapons to avoid domestic audience costs for
   backing down. We designed an original survey experiment and fielded it
   to a sample of 1,000 U.S. citizens. We found no evidence of a commitment
   trap when ambiguous nuclear threats are made. Unlike explicit threats,
   ambiguous ones did not generate domestic disapproval when the leader
   backed down; the decision to employ nuclear weapons led to more public
   backlash for the leader than being caught bluffing; and the threats did
   not influence public preference for nuclear use across our scenarios.
   Our findings contribute to the scholarly literature on nuclear crisis
   bargaining and policy debates over the future of US declaratory policy.},
