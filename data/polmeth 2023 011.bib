@article{ WOS:001167720700004,
Author = {Hoeffler, Catherine},
Title = {Arming Fortress Europe? Spaces and Instruments of Economic Patriotism in
   EU Armament Policy},
Journal = {POLITICS AND GOVERNANCE},
