Author = {Melville, A. Yu. and Malgin, A. V. and Mironyuk, M. G. and Stukal, D. K.},
Title = {``POLITICAL ATLAS OF THE MODERN WORLD 2.0{''}. FORMULATION OF THE
   RESEARCH PROBLEM},
