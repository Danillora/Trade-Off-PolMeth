@article{ WOS:000952917300045,
Author = {Lehkodukh, Viktor and Horielov, Volodymyr and Marchenko, Svitlana and
   Khromova, Olga I. and Shchyhelska, Halyna},
Title = {On the issue of historical origins and civilizational preconditions of
   the Russian-Ukrainian war of 2014-2022: attempts of scientific
   reflection br},
Journal = {CUESTIONES POLITICAS},
