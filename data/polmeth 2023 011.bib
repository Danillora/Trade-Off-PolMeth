@article{ WOS:000970985600001,
Author = {Yoshimoto, Iku},
Title = {Do aid projects from World Bank and China impact state legitimacy
   differently? An exploratory analysis in Tanzania},
Journal = {INTERNATIONAL POLITICAL SCIENCE REVIEW},
Year = {2024},
Volume = {45},
Number = {4},
Pages = {455-471},
Month = {SEP},
Abstract = {This study examines how aid activities by the World Bank and China,
   which take two competing approaches to building state capacity, affect a
   recipient government's effort to build state legitimacy vis-a-vis its
   population. The study operationalized legitimacy drawing on Levi's
   `quasi-voluntary compliance,' and empirically analyzed the two
   approaches' effects at the local level. The study used the Afrobarometer
   survey conducted in Tanzania in 2014 and geocoded datasets for aid
   projects. Chinese aid, characterized by its `Developmental State'
   approach, was associated with higher projection of state legitimacy at
   local level. Meanwhile, the World Bank's aid, which encourages building
   state legitimacy in a `contractarian' way, did not demonstrate such a
   correlation, and there was limited evidence that the co-location of
   projects from both donors may condition the effects of the World Bank
   aid.},
Publisher = {SAGE PUBLICATIONS LTD},
Address = {1 OLIVERS YARD, 55 CITY ROAD, LONDON EC1Y 1SP, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Yoshimoto, I (Corresponding Author), Univ Tokyo, Komaba Campus,3-8-1 Komaba,Meguro Ku, Tokyo 1538902, Japan.
   Yoshimoto, Iku, Univ Tokyo, Grad Sch Arts \& Sci, Dept Adv Social \& Int Studies, Tokyo, Japan.
   Yoshimoto, Iku, Univ Tokyo, Komaba Campus,3-8-1 Komaba,Meguro Ku, Tokyo 1538902, Japan.},
