@article{ WOS:001123495000001,
Author = {Carella, Leonardo},
Title = {Who Runs for Higher Office? Electoral Institutions and Level-Hopping
   Attempts in Germany's State Legislatures},
Journal = {LEGISLATIVE STUDIES QUARTERLY},
