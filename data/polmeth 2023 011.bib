@article{ WOS:001180544100012,
Author = {Latov, Yu V.},
Title = {DYNAMICS OF MASS CONSCIOUSNESS OF RUSSIANS: EXTRAORDINARY SITUATION OR
   BEGINNING OF A NEW CYCLE?},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2023},
Number = {6},
Pages = {161-179},
Abstract = {Based on the results of the all-Russian sociological survey based on a
   representative sample, conducted by the Institute of Sociology of the
   Federal Scientific Research Center of the Russian Academy of Sciences in
   June 2023 and compared with previous surveys of the 2010-2020s. the
   extent to which the mass consciousness of modern Russians meets the
   objective requirements of national mobilization development is
   considered. The dynamics of indicators of mass consciousness that
   characterize citizens' assessments of the situation in the country
   (including, in particular, assessments of social tension), as well as
   the compliance of this situation with their normative expectations (for
   example, assessments of Russia's development path, a request for change)
   is analyzed. It is concluded that, in comparison with the survey data in
   March 2022, almost all the indicators considered have improved. It is
   legitimate to consider the current situation as a phase of a new cycle
   of changes in social well-being, which is largely similar to the
   previous cycle of 2014-2016/2017. The question was raised about the
   alternation of long (4-6 years) cyclical periods associated with shock
   events, and intermediate short ``calm{''} periods as the main form of
   the dynamics of the mass consciousness of Russians in the conditions of
   modern mobilization development.},
