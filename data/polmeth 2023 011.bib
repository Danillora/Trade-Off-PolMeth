@article{ WOS:001096628700009,
Author = {Georgiou, Christakis},
Title = {Fiscal Rules and Federal Capacity in American Fiscal History: Lessons
   for Europe?},
Journal = {POLITICS AND GOVERNANCE},
Year = {2023},
Volume = {11},
Number = {4},
Pages = {82-91},
Abstract = {Recent comparative fiscal federalism work has noted how the US displays
   a mix of substantial federal capacity and no federal fiscal rules for
   sub-federal units as opposed to the EU's mix of regulation and lack of
   capacity. The difference is explained by the lack of federal capacity in
   the EU case, which presumably creates a need for regulation. However,
   these studies are cross-sectional. This carries the obvious drawback of
   abstracting the actual political and historical processes that have
   given rise to the respective mixes of regulation and capacity in the two
   polities. In this article, I trace the historical process by which the
   specific mix of no rules and capacity became entrenched in the US in the
   second half of the 20th century and ask whether that political-economic
   history has any lessons for the EU today.},
