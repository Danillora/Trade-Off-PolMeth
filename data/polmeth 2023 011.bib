@article{ WOS:000925789000001,
Author = {Rashkova, Ekaterina R. and Erzeel, Silvia},
Title = {Measuring the Substantive Representation of Women Cross-Nationally:
   Towards a Composite Index},
Journal = {POLITICAL STUDIES REVIEW},
Year = {2023},
Volume = {21},
Number = {3},
Pages = {523-531},
Month = {AUG},
Abstract = {Recent scholarship on women's substantive representation has expanded
   from its initial focus on gender inequalities in parliaments and now
   studies (1) various group interests, (2) in different venues, (3) across
   different contexts and (4) using different methods and approaches.
   Building on these advances, we present a new comparative measure of
   women's substantive representation. This `Substantive Representation
   Index' combines eight indicators, linked to two key dimensions of
   women's substantive representation, in one composite quantitative index.
   As such, we offer a consistent modality that allows scholars to measure
   women's substantive representation systematically and comparatively
   across and within democratic countries and over time.},
