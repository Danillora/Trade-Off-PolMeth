@article{ WOS:001014457100001,
Author = {Liu, Hongsong},
Title = {Chinese academic representations of the rise of populism in Europe},
Journal = {COMPARATIVE EUROPEAN POLITICS},
Year = {2023},
Volume = {21},
Number = {5, SI},
Pages = {687-701},
Month = {OCT},
Abstract = {The rise of populist parties in some European countries and their
   competition with mainstream parties have brought European populism to
   the attention of Chinese academics, creating discourses that address
   this topic. These narratives represent Chinese academics'
   representations of the origins of European populism and its impact on
   European politics and European integration. Different perspectives bring
   about a plurality of narratives of European populism by Chinese
   academics. The rise of European populism is attributed to the worse
   economic situation, the high unemployment rate, decreasing social
   welfare and the rapid increase of foreign immigration. Although Chinese
   academics debate the question of whether populism has changed Europe's
   political ecology, the predominant discourses emphasize the negative
   impact of populism on European politics and European integration as well
