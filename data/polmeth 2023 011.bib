@article{ WOS:001092653500010,
Author = {Vairo, Daniela and Antia, Florencia},
Title = {Uruguay 2022: Between the Referendum, Strategic Reforms and Political
   Scandals},
Journal = {REVISTA DE CIENCIA POLITICA},
Year = {2023},
Volume = {43},
Number = {2},
Pages = {377-401},
Abstract = {This article examines the third year of the center-right coalition
   government, led by President Lacalle Pou in Uruguay. After two years
   where the pandemic and a referendum against the Urgent Consideration Law
