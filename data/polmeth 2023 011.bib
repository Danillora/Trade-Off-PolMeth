@article{ WOS:000943839600001,
Author = {Gainous, Jason and Velez Serrano, Mayra and Wagner, Kevin M.},
Title = {Protesting With Feeling in Puerto Rico: Twitter and El Verano Del 19},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2023},
Volume = {76},
