@article{ WOS:001372288300019,
Author = {Ardakani, Hossein Fattahi},
Title = {Explaining the Taliban's Revival: Thomas Spragens's Crisis Theory},
Journal = {CHINA QUARTERLY OF INTERNATIONAL STRATEGIC STUDIES},
