@article{ WOS:001086177700001,
Author = {Al-Aloosy, Massaab},
Title = {Parasitical elitism in a sectarianized political system with a rentier
   economy: The power and practice of the Iraqi political elite after 2003},
Journal = {POLITICS \& POLICY},
Year = {2023},
Volume = {51},
Number = {6},
Pages = {1136-1154},
Month = {DEC},
Abstract = {This article theorizes the role of the elite in a sectarianized
   political system with a rentier economy. It argues that by virtue of the
   sectarian and ethnic representation of the sub-identities, the long-term
   interests of society are neglected. The reliance on natural resources
   creates a competition within the elite to seize the state's revenue for
   narrow interests. Finally, in such a setting, the elite attempts the
   monopolization of the flow of information to the public to shore up its
   legitimacy. The case study is Iraq since it is one of the few, if not
   the sole, sectarianized political system with a rentier economy. The
   political elite, despite an abysmal performance for nearly two decades,
   has been resilient and garnered more power. The continuous malpractice
   encompasses all the political players who agree on the governing rules
   by which they maintain their position within the system, and with
   limited circulation of the elite.},
