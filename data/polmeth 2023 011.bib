@article{ WOS:000943883600001,
Author = {Borodina, Svetlana},
Title = {Russia's Domestication of Disability Inclusion},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2025},
Volume = {72},
Number = {3},
Pages = {262-272},
Month = {MAY 4},
Abstract = {This article examines the institutional conditions and discursive
   aspects of the depoliticized project of disability inclusion in the late
   2010s in Russia. It suggests that the depoliticization of disability
   inclusion in Russia occurred by outsourcing the task of instituting
   disability inclusion to Russian NGOs and rendering disability inclusion
   a ``social{''} problem solvable through these NGOs' tools. The case of
   postsocialist disability inclusion in Russia provides an insight into
   how state-socialist legacies and intensifying postsocialist
   neoliberalization appropriate such progressive tools of social change as
   disability inclusion and produce a locally specific discursive formation
   of this global symbol of development.},
