@article{ WOS:001156745000022,
Author = {Caminotti, Mariana},
Title = {Gender, Politics, and Democracy in Argentina (1983-2023)},
Journal = {TEMAS Y DEBATES},
Year = {2023},
Number = {45, 1},
Pages = {47-52},
Abstract = {Gender inequalities and policies have a prominent place in the Argentine
   political debate. In the last forty years a series of legal,
   institutional, and social changes expanded women's and LGBTIQ+'s rights.
   However, there are still significant gender gaps and inequalities. This
   article analyzes participation and political representation of women in
   recent Argentine history. First, it seeks to connect current feminist
   agendas with previous activism in society and legislative politics.
   Second, it identifies a series of political and programmatic challenges
   for the achievement of democratic parity.},
