@article{ WOS:001021921000001,
Author = {Linden, Felipe Rafael},
Title = {How `Empty' is the signifier `the people'? Impasses of the
   poststructuralist approach in Ernesto Laclau's political ontology},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2025},
Volume = {30},
Number = {2},
Pages = {327-351},
Month = {MAY 4},
Abstract = {This article looks at the `empty signifier', a central but nonetheless
   elusive concept for understanding the logic that underpins the political
   construction of `the people' as it was theorized in On Populist Reason.
   It begins by examining its epistemological foundations (which revisit
   the principles of Saussurean modern linguistics) and limitations based
   on Vincent Descombes's critique of (post)structuralist literature. Among
   the authors that Laclau mobilized, particular attention is paid to the
   `logic of the signifier' in Lacanian theory. The article then moves on
   to an analysis of the concept within the broader framework of the
   political ontology of which it is part ('discourse theory'). This is
   followed by a discussion of recent political events and the construction
   of concrete `popular' or `populists' identities The question is whether
   the notion of `empty' and `floating' signifier is truly empty and how
   Laclau explain this `emptiness'. More globally, our aim is to identify
   the impasses in Laclau's theory of political subjectivation around empty
   signifiers (the `populist logic'). In conclusion, this article argue
   that there is some problems associated with the conception of
   signifiers, such as that `empty' seems to refer to different `degrees'
   of emptiness leading to some theoretical and practical consequences.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
