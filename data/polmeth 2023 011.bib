@article{ WOS:001036942600001,
Author = {Driscoll, Amanda and Nelson, Michael J.},
Title = {Are courts ``different?{''} Experimental evidence on the unique costs of
   attacking courts},
Journal = {RESEARCH \& POLITICS},
Year = {2023},
Volume = {10},
Number = {3},
Month = {JUL 8},
Abstract = {U.S. courts have long been thought to be held in special regard by the
   American public, and public support is theorized to protect institutions
   from interbranch aggression. At the same time, recent research
   underscores that institutional fealty and public reaction to court
   curbing is shaped by partisan concerns. Drawing on a survey experiment
   fielded in the U.S., we evaluate whether (1) the public is uniquely
   punitive toward incumbents who seek to undermine a court rather than an
   agency and (2) the extent to which these penalties are dependent upon
   shared partisanship with the proposer. We demonstrate that the public is
   less supportive of efforts to strip judicial power than analogous
   efforts to strip power from an executive agency, but that this penalty
   for court curbing dissipates in the face of copartisanship. This
   substantiates previous claims regarding the role of partisanship on
   shaping public attitudes about high courts but underscores that the
   American public may still hold the courts in unique regard.},
