@article{ WOS:001061870200011,
Author = {Rogatinska, Nina and Halahan, Oleksandr and Protsiuk, Oleh and Galagan,
   Sergii and Fierieva, Natalia},
Title = {War crimes and crimes against humanity in Ukraine: Legal qualification
   and features of documentation},
Journal = {CUESTIONES POLITICAS},
Year = {2023},
Volume = {41},
Number = {78},
Pages = {143-165},
Month = {JUL-SEP},
Abstract = {This scientific article is devoted to the interpretation of war crimes
   and crimes against humanity, peculiarities of regulation of these
   illegal acts and organization of their investigation in Ukraine. Using
   the dialectical method, it emphasizes the necessity of using the
   resources of the International Criminal Court ICC in Ukraine and
   establishing a Special International Tribunal to hold personally
   responsible for the commission of crimes of aggression the actors
   involved. The expediency of creating a special state institute for
   interaction with the ICC with the appointment of national coordinators
