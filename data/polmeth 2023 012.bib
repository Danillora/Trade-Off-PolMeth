@article{ WOS:001365984800002,
Author = {Duarte-Abadia, Bibiana and Boelens, Rutgerd and Buitrago, Emerson},
Title = {Neoliberal commensuration and new enclosures of the commons: mining and
   market-environmentalism governmentalities},
