@article{ WOS:001109419700007,
Author = {Gabdrakhmanova, G. F.},
Title = {INSTITUTIONALIZATION OF THE RUSSIAN NATIONAL SCHOOL IN THE FIELD OF
   POLITICS (case Sakha- and Tatar-speaking schools of the republics of
   Sakha (Yakutia) and Tatarstan)},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
Year = {2023},
Number = {3},
Pages = {98-113},
Abstract = {The article problematizes the issue of the subjects of language policy.
   The author proposes to consider ruling and non-ruling actors in this
   capacity. The special institutional agreements on the use of national
   languages, developed as a result of their interaction, set the framework
   for the linguistic territorial regime. One stand-alone segment is the
   question of the use of languages as a means of teaching in the education
   system. The authorities of the federal and regional levels, ethnic
   groups, parental and pedagogical communities participate in its
   discussion. Their ideas about national languages as a means of education
   and the actions taken have led to the formation of a national school as
   an institution in the field of politics. The study of the linguistic
   territorial regime was undertaken based on the example of schools in the
   Russian republics of Sakha (Yakutia) and Tatarstan, where education is
   conducted in the Yakut and Tatar languages. The purpose of the article
   is to reveal the evolution of the institutionalization of the Russian
   national school in the post-Soviet period in the context of the
   linguistic territorial regime. To analyze this process, the ideas of
   historical institutionalism, comparative political science and the
   method of binary analysis are used. The institutionalization of the
   Russian NS takes place in the context of a complex intertwining of ideas
   and actions of the actors of the linguistic territorial regime, their
   contradictory communicative interactions, ongoing dependence on the
   Soviet paradigm of national education and the current language policy.
   The discrepancy is reflected in the negative dynamics of the number of
   Russian national schools and students studying in them.},
