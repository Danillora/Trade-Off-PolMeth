@article{ WOS:000935096000025,
Author = {Makita, Jun},
Title = {Institutional development of legislative supporting agencies (LSAs) from
   a perspective of difference between presidential and parliamentary
   systems},
Journal = {ASIAN JOURNAL OF COMPARATIVE POLITICS},
Year = {2023},
