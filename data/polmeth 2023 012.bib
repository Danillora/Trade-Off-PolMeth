@article{ WOS:001074726900001,
Author = {Jackson, Christopher and Philips, Andrew Q.},
Title = {Synthesize This: Meta-Analysis as a Dissertation Tool},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
