@article{ WOS:001136734800013,
Author = {Alves, Marco Antonio Sousa and Patente, Zilda Manuela Onofri},
Title = {What remainsfrom ``senzala{''}?},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2023},
Volume = {127},
Month = {JUL-DEC},
Abstract = {This paper seeks to analyze the war on drugs as a necropolitics device,
   taking into account the territorialization of vulnerable populations in
   post-colonial cities' urban centers and the implication of security
   devices to racialize the inequality and lengthen the extermination. For
   this, we propose a theoretical investigation of biopolitics as State
   racism, starting from street drug markets as a field of study. Thus, it
   becomes possible to understand the critical conclusions that link the
   Brazilian republican project with the perpetuation of camps of
   exception.},
