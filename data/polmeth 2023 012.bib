@article{ WOS:001150400700007,
Author = {Wendland, Anna Veronika},
Title = {The Zaporizhhia Nuclear Power Plant Theatre of War and Test Case for
   Reactor Safety},
Journal = {OSTEUROPA},
Year = {2023},
Volume = {73},
Number = {10-11},
Pages = {125+},
Abstract = {Russia's war of aggression against Ukraine is the first war in human
   history in which civilian nuclear facilities have been attacked. The
   Russian-occupied Zaporizhzhia nuclear power plant is a theatre of war
   and a test case for reactor safety under wartime conditions. It is
   representative of the safety concerns facing the other similarly
   constructed nuclear power plants in Ukraine. Numerous abnormal operating
   situations have taken place in Zaporizhzhia as a result of the war. The
   staff must ensure the safety of the facility under the terror of the
   occupiers. Emergency situations have so far gone smoothly. However,
   there is concern that there will be a major reactor accident in
   Zaporizhzhia. The fear of such an accident is also a tool of hybrid
   warfare.},
Publisher = {BWV-BERLINER WISSENSCHAFTS-VERLAG GMBH},
