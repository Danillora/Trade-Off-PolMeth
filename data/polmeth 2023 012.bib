@article{ WOS:000975223500001,
Author = {Levin, Andrew},
Title = {External State Support for Rebel Groups and the Occurrence of UN
   Peacekeeping Operations},
Journal = {CIVIL WARS},
Year = {2024},
Volume = {26},
Number = {4},
Pages = {630-655},
Month = {OCT 1},
Abstract = {Why does the UN deploy peacekeepers to some civil wars, but not others?
   I hypothesise that peacekeeping is more likely when external states
   support armed groups, and particularly when external states send troops
   to assist rebels. Facing a heightened threat, governments are more
   likely to consent to UN intervention in an effort to bolster security.
   From the supply side, external state support increases the probability
   that the UN deploys peacekeepers given the threats posed to
   international security. Analysis of intrastate conflicts between
   1991-2010 indicates the likelihood of UN peacekeeping increases when
   external state forces fight in support of rebels.},
