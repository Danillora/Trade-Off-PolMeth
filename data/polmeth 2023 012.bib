@article{ WOS:000962060200001,
Author = {Doudaki, Vaia and Carpentier, Nico},
Title = {Behind the narratives of climate change denial and rights of nature:
   sustainability and the ideological struggle between anthropocentrism and
   ecocentrism in two radical Facebook groups in Sweden},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2025},
Volume = {30},
Number = {1},
Pages = {200-219},
Month = {JAN 2},
Abstract = {In this article, we study two Facebook groups in Sweden that address
   environmental issues, which adopt opposingly radical positions as it
   regards sustainability: one Facebook group supports climate change
   denialism, aligning with anthropocentrism, and one promotes the rights
   of nature movement and its ideas, embedded in ecocentrism. Employing
   discourse analysis, we exemplify how the radical positions that the two
   Facebook groups adopt demarcate the margins and the breadth of the
   spectrum of discourses around sustainability, but also how these
   positions signify the ideological struggle between anthropocentrism and
   ecocentrism, which lies behind the narratives of climate change denial
   or rights of nature, and structures their claims and radicality.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
