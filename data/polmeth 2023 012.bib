@article{ WOS:000978183800001,
Author = {Hopkins, Vincent and Lawlor, Andrea},
Title = {Behavioural Insights and Public Policy in Canada},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2023},
Volume = {56},
Number = {2},
Pages = {435-450},
Month = {JUN},
Abstract = {Much of political science rests on assumptions about how policy makers
   and citizens behave. However, questions remain about how public policy
   can improve the government-citizen relationship. In this research note,
   we present behavioural insights (BI) as one way to address this gap.
   First, we argue that BI can be strategically used both to alleviate
   administrative burdens and to enhance citizen experience. Second, we
   argue that BI interventions can assist in several stages of the policy
   process, strengthening causal inferences about policy efficacy. Third,
   we present original data from Canada's ongoing experimentation with BI
   across multiple jurisdictions and areas of public policy. We conclude by
   acknowledging the myriad pathways through which BI research can engage
   with public policy to support the enhancement of citizen-oriented
   service delivery.},
