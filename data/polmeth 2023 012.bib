@article{ WOS:001115076400005,
Author = {Duclerc, Mathilde Fois and Maclouf, Marguerite},
Title = {The Basque eusko, a divisive currency. Mobilizing and contesting
   throught market in farming world},
Journal = {POLITIX},
Year = {2023},
Number = {142},
Pages = {125-149},
Abstract = {The local currencies that have proliferated in France since 2010 are
   part of a broader set of collective actions based on a critique of the
   globalisation of economic activities, and in particular of agri-food
   distribution chains. This article analyses the contestation logics
   underlying the participation of farmers in such a movement. Using the
   case of the eusko in the Basque Country as a starting point, the article
   explains how a mobilised fraction of the agricultural world is
   integrated into an alternative monetary network. We show that this
   process is the result of an interaction between the militant work
   carried out by the association that produces the local currency and
   forms of collective action specific to the local rural world. This paper
   shows that the acceptance of this currency is linked to positions in
   territorial trade union divisions, and thus contributes to the analysis
   of the social anchoring of technical devices.},
