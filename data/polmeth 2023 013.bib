@article{ WOS:001046653300008,
Author = {Seve, Lucien},
Title = {Marxist Analysis of Alienation (1973)},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2023},
Volume = {31},
Number = {1},
Pages = {245-296},
Month = {MAR},
Abstract = {Lucien Seve (1926-2020) was one of the foremost Marxist theoreticians of
   the Parti Communiste Francais. An indomitable opponent of both
   structural and humanist Marxism, his 1973 article reprinted below
   represents the core of his conception of alienation. For Seve, whilst
   the mature Marxism of Das Kapital is fundamentally distinct from the
   speculative humanism of the 1844 Manuscripts in placing capital, not
   abstract labour, at the heart of alienation, this reinforces, rather
   than replaces, the role of alienation at the centre of Marx's mature
   thought, and hence of Marxism itself.},
