Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Roccato, M (Corresponding Author), Univ Torino, Dept Psychol, Via Verdi 10, I-10126 Turin, Italy.
   Cena, Lorenzo; Roccato, Michele, Univ Torino, Dept Psychol, Via Verdi 10, I-10126 Turin, Italy.},
DOI = {10.1111/ssqu.13254},
EarlyAccessDate = {MAR 2023},
