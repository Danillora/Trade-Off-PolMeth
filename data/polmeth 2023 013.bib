@article{ WOS:000924395500001,
Author = {Pressman, Jeremy and Devin, Elannah},
Title = {`Profile: the diffusion of global protests after George Floyd's murder'},
Journal = {SOCIAL MOVEMENT STUDIES},
Year = {2024},
Volume = {23},
Number = {4, SI},
Pages = {558-565},
Month = {JUL 3},
Abstract = {In what countries did people protest after George Floyd's death? This
   article offers a detailed count of ninety-three countries and
   territories where antiracism protests took place outside the United
   States after George Floyd's murder on 25 May 2020. We suggest that media
   coverage - or non-relational diffusion - may explain this rapid spread.
   But we also note that the protests were not simply amplifying US
   grievances. Rather, in many news reports we found evidence of the
   localization of protests. The US protests opened the door to a wave of
   events that allowed local activists and protestors to air their concerns
   around racism, police brutality, monuments, and related issues.},
