@article{ WOS:000964528000001,
Author = {Cella, Michela and Manzoni, Elena},
Title = {Gender bias and women?s political performance},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2023},
Volume = {77},
Month = {MAR},
Abstract = {We model voters' gender bias as a prejudice on women's competence coming
   from a distorted prior. We analyse the effect of this bias in a
   two-period two-party election model in which voters care about both
   policy preference and competence. We find that, if voters (wrongly)
   believe that women are drawn from a distribution of competences with
   higher weights on lower values, female politicians are less likely to
   win office but, when elected, they are on average more competent than
   male elected officials. As a consequence, female incumbents seek
   re-election more often.},
