@article{ WOS:000980522000002,
Author = {Barcia, Lucia Suarez},
Title = {FinTech and Financial Inclusion: Analysis from a Financial Dignity
   Approach},
Journal = {REVISTA ESTUDIOS SOCIO-JURIDICOS},
Year = {2023},
Volume = {25},
Number = {2},
Pages = {26-28},
Month = {JUL-DEC},
Abstract = {Authorities have identified technological innovation as a tool for
   financial inclusion. However, Financial Technology (FinTech) also has
   the potential to support a broader concept: financial dignity. This
   study analyzes three financial inclusion policies from three regions and
   their metrics to identify whether the supervisors are embedding FinTech
   in their current strategies and metrics. These metrics are evaluated
   from the short-and long-term perspectives, identified as financial
   dignity. Finally, a proposal structure of key performance indicators is
   made.},
