Author = {Popogbe, Oluwaseyi Omowunmi and Akinleye, Simeon Oludiran and Oke, David
   Mautin},
Title = {A tripartite approach to social inclusion in selected slums in Lagos
   State, Nigeria},
Journal = {REVIEW OF ECONOMICS AND POLITICAL SCIENCE},
