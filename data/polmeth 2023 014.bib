@article{ WOS:001174209800002,
Author = {Engster, Frank},
Title = {Sohn-Rethel's Unity of the Critique of Society and the Critique of
   Epistemology, and His Theoretical Blind Spot: Measure},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2023},
Volume = {31},
Number = {4},
Pages = {160-205},
Month = {DEC},
Abstract = {Sohn-Rethel's great idea was to `socialise' Kant's transcendental
   subject by combining it with Marx's commodity-form. In so doing, he took
   on three challenges simultaneously: a) the timeless validity of modern
   natural science; b) the social genesis of empirically pure forms of
   cognition; and c) socialisation occurring through a purely social
   synthesis. However, Sohn-Rethel construed Marx's value-form analysis as
   an empirical exchange of commodities and held that such exchange
   performs a real abstraction in this way, he laboured under the very
   semblance that money engenders at the surface of society by virtue of
