@article{ WOS:001052000900001,
Author = {Magness, Phillip W. W. and Carden, Art and Murtazashvili, Ilia},
Title = {Gordon Tullock and the economics of slavery},
Journal = {PUBLIC CHOICE},
Year = {2023},
Volume = {197},
Number = {1-2},
Pages = {185-199},
Month = {OCT},
Abstract = {This paper investigates Gordon Tullock's unpublished manuscripts that
   proposed a public choice interpretation of American slavery. Drafted in
   response to Conrad and Meyer's seminal 1958 article on the economics of
   slavery, Tullock's writings influenced the early debate over slavery
   through his University of Virginia colleague John E. Moes. This paper
   uses Tullock's surviving writings to map out his theory of slavery and
   situate it in the broader economic analysis of the institution and
   identify the links between the economics of slavery and the public
   choice research tradition.},
