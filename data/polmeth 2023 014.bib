@article{ WOS:001112529300007,
Author = {De Genova, Nicholas},
Title = {A Racial Theory of Labour: Racial Capitalism from Colonial Slavery to
   Postcolonial Migration},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2023},
Volume = {31},
