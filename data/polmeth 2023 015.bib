@article{ WOS:001015227100001,
Author = {Wickes, Judi and Robinson, Kella and Aberdeen, Lucinda},
Title = {Living Under Aboriginal Exemption: Negotiating State Governments'
   Policies and Practices},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2023},
Volume = {69},
Number = {1, SI},
Pages = {1-5},
Month = {MAR},
Abstract = {This volume of the Australian Journal of Politics and History presents
   an edited collection of papers delivered by emerging and established
   researchers at the Second Rethinking \& Researching 20th Century
   Aboriginal Exemption Symposium, co-hosted by the University of the
   Sunshine Coast with La Trobe University in October 2021. The papers
   reveal the human costs, hardships and legacies of the state policies of
   Aboriginal Exemption last century which supposedly offered the promise
   of freedom to Indigenous Australians confined to reserves and missions.
   Equally, the papers explore innovative and culturally safe ways to
   investigate and further understand Aboriginal exemption that ensure
   Ancestors and Elders, who actively negotiated, resisted and subverted
   its use, are recognised and honoured.},
