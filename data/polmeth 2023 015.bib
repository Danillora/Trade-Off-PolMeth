@article{ WOS:001046653300005,
Author = {Wald, Alan M.},
Title = {Missives for the Future? Michael Lowy's Close Encounters with the US
   Left},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
