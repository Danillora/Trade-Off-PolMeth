@article{ WOS:001048818400001,
Author = {Stringham, Edward Peter},
Title = {Banking regulation got you down? The rise of fintech and
   cryptointermediation in Africa},
Journal = {PUBLIC CHOICE},
Year = {2023},
Volume = {197},
Number = {3-4, SI},
Pages = {455-470},
Month = {DEC},
Abstract = {Lack of access to financial services is a problem for many; worldwide,
   one in four adults does not have a checking or savings account.
   Financial institutions with active, low-balance accounts often face
   considerable challenges compared to larger financial institutions
   (Black, 1979). Adding a fixed regulatory cost of servicing accounts can
   affect relative costs and thus generate an Alchian-Allen effect (the
   third law of demand) that leads financial institutions to quit servicing
   these low-balance accounts. Regulators may prefer this outcome as it is
   easier to regulate and extract rents from large financial institutions.
   Data indicates more underbanking in countries with lower income and more
   regulation. This article looks at the barriers traditional financial
   intermediaries often face in less developed nations and then looks at
   how fintech and cryptocurrencies enable bank alternatives to lower
   barriers to entry and expand financial inclusion.},
