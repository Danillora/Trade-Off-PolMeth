@article{ WOS:000949432900001,
Author = {Lizotte, Christopher},
Title = {The Prosaic Stateness of Secularism: Diversity, Incoherence and
   Divergence in the Application of laicite},
Journal = {GEOPOLITICS},
Year = {2024},
Volume = {29},
Number = {3, SI},
Pages = {962-986},
Month = {MAY 26},
Abstract = {France's idiosyncratic form of secularism, laicite, is a highly
   contested principle bound up in centuries of conflict over the state's
   relationship to religious practice. Of late, commentators have
   interpreted it as taking somewhat of an authoritarian turn as it is used
   to disproportionately identify and suppress signs of overt Muslim
   religiosity, including notably within the public school system. I apply
   Painter's `prosaic stateness' framework to understand how educators
   interpret and rework laicite within the French public school to produce
   `actually-existing' laicite. In particular, I show that educators are
   situated within three `spatialities' through which laicite inhabits
   French schooling: the idealised space of the French republic set against
   religious obscurantism, schools as spaces of inviolable neutrality, and
   risky bodies who refuse to conform to norms of republican citizenship.
   Within these spatialities, educators enact a prosaic stateness of
   laicite through their experiences of diversity of the student
   populations they serve, the incoherence of the institution they work
   for, and the divergence of the policies they end up applying.},
