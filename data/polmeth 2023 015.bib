@article{ WOS:001019176700002,
Author = {Jacobson, Stephen},
Title = {WHO WERE THE VOLUNTEERS? A SPANISH REVOLUTIONARY AND WANDERER IN
   ARGENTINA, 1857-1860},
Journal = {HISTORIA Y POLITICA},
