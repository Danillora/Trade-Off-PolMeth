@article{ WOS:000986035200001,
Author = {Steele, Janet},
Title = {What Can We Learn From the Short History of Independent Media in Serbia?
   Radio B92, George Soros, and New Models of Media Development},
Journal = {INTERNATIONAL JOURNAL OF PRESS-POLITICS},
Year = {2024},
Volume = {29},
