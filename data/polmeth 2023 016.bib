@article{ WOS:001111098800001,
Author = {Shamir, Assaf and Oser, Jennifer and Grinberg, Nir},
Title = {Who Is Curating My Political Feed? Characterizing Political Exposure of
   Registered US Voters on Twitter},
Journal = {INTERNATIONAL JOURNAL OF PRESS-POLITICS},
Year = {2024},
