@article{ WOS:001081776300005,
Author = {Abass, Abdallah Mohamed and Pop, Adrian},
Title = {Triggers for Basel III Countercyclical Capital Buffers: A Counterfactual
   Analysis},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2023},
Volume = {133},
Number = {4},
Pages = {601-644},
Month = {JUL-AUG},
Abstract = {We propose a critical assessment of the Basel Committee's
   macro-prudential proposal to mitigate the pro-cyclicality of
   risk-sensitive capital regulations. We compare first the credit-to-GDP
   gap and other macroeconomic and financial variables in their ability to
   predict banking sector vulnerabilities. Then, based on a counterfactual
   empirical implementation, we test a statistical procedure aiming at
   identifying a set of triggers for countercyclical capital buffers. We
   test our proposal using data characterizing the South Africa banking
   sector.},
