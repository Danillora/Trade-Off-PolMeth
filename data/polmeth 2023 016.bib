@article{ WOS:000994330900010,
Author = {Jasinska-Biliczak, Anna},
Title = {How to Ensure the Development of SMEs in Post-Pandemic Economy? A
   Start-ups Case Study},
Journal = {TALTECH JOURNAL OF EUROPEAN STUDIES},
