@article{ WOS:001126048400016,
Author = {Pierre, Arnaud},
Title = {The fall of Cea Bermúdez and the end of absolutism. Networks of
   relationships and action of the elites},
Journal = {HISTORIA Y POLITICA},
Year = {2023},
Number = {50},
Pages = {187-219},
Month = {JUL-DEC},
Abstract = {The fall from power of Francisco de Cea Bermudez, Ferdinand VII's last
   minister, represented the definitive end of absolutism in Spain, and
   marked the beginning of the process which led to the liberal society, in
   the context of the carlist civil war. This article contends that this
   break-up was imposed to the regent Maria Cristina de Borbon by a strong
   movement within public opinion, channelled by the family and affinity
   networks in the << political class >> of the Monarchy, whose members
