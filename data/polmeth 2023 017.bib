@article{ WOS:000906657900001,
Author = {Dionigi, Filippo},
Title = {Displacement as Heterotopic Space: The Syrian Refugee Crisis in Jordan},
Journal = {GEOPOLITICS},
Year = {2024},
Volume = {29},
Number = {1, SI},
Pages = {203-229},
Month = {JAN 1},
Abstract = {The idea of a sovereign territorial order dominates representations of
   space in International Relations through ubiquitous dichotomies such as
   international/domestic, inside/outside, and citizen/foreigner. Yet,
   phenomena of forced displacement question the perceptiveness of these
   binaries justifying an enquiry into the possibility of different
   accounts of the type of space that displacement constitutes. This essay
   revisits critically the Foucauldian concept of heterotopic space and
   proposes its redefinition. It then uses the revised concept for the
   reconstruction of the Syrian displacement crisis in Jordan. The
   objective is to show the validity of heterotopic space as a concept to
   represent the site that states, refugees, and international
   organisations constitute through their interactions in displacement
   response. The argument is that interpreting displacement as heterotopic
   space allows for a more credible representation of this phenomenon that
   supplants the assumptions of sovereign territoriality. This leads to an
