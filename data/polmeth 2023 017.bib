@article{ WOS:001117529900002,
Author = {Kubbara, Ola Ashraf},
Title = {International actors of democracy assistance in Egypt post 2011: German
   political foundations},
Journal = {REVIEW OF ECONOMICS AND POLITICAL SCIENCE},
