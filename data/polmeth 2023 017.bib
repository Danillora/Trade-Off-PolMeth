@article{ WOS:000926292800001,
Author = {Altay, Sacha and Nielsen, Rasmus Kleis and Fletcher, Richard},
Title = {News Can Help! The Impact of News Media and Digital Platforms on
   Awareness of and Belief in Misinformation},
Journal = {INTERNATIONAL JOURNAL OF PRESS-POLITICS},
