@article{ WOS:000998181100001,
Author = {Borwein, Sophie and Lucas, Jack},
Title = {Asymmetries in urban, suburban, and rural place-based resentment},
Journal = {POLITICAL GEOGRAPHY},
Year = {2023},
Volume = {105},
Month = {AUG},
Abstract = {In many advanced economies, citizens are deeply politically divided on
   the basis of where they live, and there is growing concern that these
   divisions are reinforced by place-based resentment - hostility toward
   place-based out-groups that are perceived to be unfairly advantaged.
   Drawing on two large-scale surveys of the Canadian public, this paper
   investigates the size and socio-demographic correlates of place-based
   resentment in urban, suburban, and rural areas. First, we examine how
   place resentment varies across all possible combinations of urban,
   suburban, and rural in-groups and out-groups. Second, we explore if
   high-resentment individuals in urban, suburban, and rural areas share
   similar socio-demographic and political characteristics. We then turn to
   an examination of political implications, investigating how citizens'
   satisfaction with their elected representatives, and positions on
   contentious and important policy issues, relate to place resentment. We
   find that place-based resentment is highly asymmetric: resentment is
   strongest among rural residents regardless of the target (suburban or
   urban) of their resentment, whereas urban and suburban residents tend to
   resent each other more than either resents rural areas. We also find
   substantial asymmetries in the correlates and political implications of
   place resentment. High-resentment rural residents tend to be older, more
   conservative in their attitudes, and less satisfied with democracy,
   while high-resentment urban resenters are generally younger, higher in
   income and education, and more progressive in their attitudes and
   satisfied with democracy. Our findings suggest that place resentment is
   an important and politically consequential phenomenon across all place
   types, but that the character of this resentment differs in rural,
   suburban, and urban places.},
