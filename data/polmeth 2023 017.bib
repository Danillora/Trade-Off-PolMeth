@article{ WOS:001059053900002,
Author = {Knox, Robert and Kumar, Ashok},
Title = {Reexamining Race and Capitalism in the Marxist Tradition- Editorial
   Introduction},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2023},
Volume = {31},
