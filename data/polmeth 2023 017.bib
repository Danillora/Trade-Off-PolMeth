@article{ WOS:000943038500001,
Author = {Crouze, Ronald and Godard, Louise and Meurs, Pieter},
Title = {Learning democracy through activism: the global climate strike movement
   and Belgian youth's democratic experience in times of environmental
   emergency},
Journal = {SOCIAL MOVEMENT STUDIES},
Year = {2024},
Volume = {23},
Number = {1, SI},
Pages = {56-71},
Month = {JAN 2},
Abstract = {The climate protests are one of the most impressive social movements of
   this time and are historical in various respects: the global and
   widespread scale, the employed protest tactics, and its young
   demographic character. This study explicitly takes the position that the
   climate marches are opening new avenues of politization with a radical
   educational democratic potential. Building on the conceptual framework
   of social movement learning, the educational experiences of recurring
   participants of the protests are explored in terms of civic education.
   This contributes not only to the understanding of the possibility of the
   climate movement as educational spaces for learning about politics and
   democracy, but also to a better understanding of the educational
   potential of social movements in general. The research design consists
   of a single country case study with individual interviews and focus
   groups with frequent participants of the climate protests in Belgium.
   The findings indicate that the climate protests are an especially
   fruitful place for political engagement acting as a catalysator for
   politization. It fosters a process with reflections on individual
   agency, collective responsibility, current political structures, and the
   democratic ideals of our society.},
