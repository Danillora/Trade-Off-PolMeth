@article{ WOS:001118924200006,
Author = {Jekabsone, Inga},
Title = {Selected Legal Issues in Online Adult Education: Compliance of Online
   Learning and Teaching Process with GDPR},
Journal = {TALTECH JOURNAL OF EUROPEAN STUDIES},
