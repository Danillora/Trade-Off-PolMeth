@article{ WOS:001104796800001,
Author = {Carvalho, Thales},
Title = {Mechanisms of defense policy diffusion in South America: evidence from
   the South American Defense Council},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2023},
Volume = {66},
Number = {1},
Abstract = {The South American Defense Council (SADC) aimed to coordinate regional
   defense policies and build confidence among its members. It diffused
   practices among its members, such as a standard methodology to report
   defense expenditures. However, we still need a proper understanding of
   how it happen. In this article, I contribute to understanding defense
   cooperation in South America by answering which mechanisms allowed the
   Council to diffuse policies. Using evidence from the Council's meeting
   records and process tracing, I show that SADC boosted interactions and
   set and monitored the implementation of particular practices on defense
   policies.},
