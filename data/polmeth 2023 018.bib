@article{ WOS:001077415000001,
Author = {Giedraityte, Ieva},
Title = {Beyond hierarchy: regional orders in the twenty-first century},
Journal = {INTERNATIONAL POLITICS},
Year = {2023},
Month = {2023 OCT 3},
Abstract = {Arguing that current analytical tools are insufficient to understand and
   compare existing and emerging regions, this article proposes a new
   typology of regional orders based on the interplay of two
   differentiation principles: stratificatory and functional. The
   `differentiation-based' typology distinguishes nine ideal-type regional
   orders based on the level of domination that regional power has or aims
   to have and the functional complexity of the region. The focus on
   differentiation principles brings the diffuse debate on regional
   functions and hierarchies within a single framework and allows proposing
   a coherent way of identifying and classifying regional interactions.
   Moreover, the article proposes ways to operationalize the typology,
   making it suitable for analyzing existing and desired regional orders.
   Consequently, it allows the comparison of different regional orders or
   `regional visions' of different regional powers, tracing their changes
   over time or capturing discrepancies between these visions and reality.},
