@article{ WOS:001091540900001,
Author = {Abramson, Yehonatan and Baram, Gil},
Title = {Saving face in the cyberspace: responses to public cyber intrusions in
   the Gulf},
Journal = {CONTEMPORARY SECURITY POLICY},
