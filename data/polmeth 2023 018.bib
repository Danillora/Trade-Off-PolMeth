@article{ WOS:001129353400001,
Author = {Leite, Higor and Hodgkinson, Ian R. and Volochtchuk, Ana Vitoria
   Lachowski},
Title = {The impact of service separation on value: A longitudinal study of user
   and provider experiences in a mental health service},
Journal = {PUBLIC ADMINISTRATION},
Year = {2024},
Volume = {102},
Number = {3},
Pages = {1233-1256},
Month = {SEP},
Abstract = {Digital innovations are changing how public organizations deliver
   services through the creation of service separation. Service separation
   refers to the decoupling of service production and consumption in
   digital platforms, which affects both users and providers alike. Since
   the global pandemic, service separation has become established in many
   healthcare settings, though little is known about how it has been
   experienced by service actors in specialist service settings such as
   mental healthcare. Positioned at the (sub)micro-level of service
   interaction, our study provides a dual view of service separation to
   reveal its influence on value (co)creation and potential for value
   destruction in a 12-month longitudinal study. The findings draw on three
   waves of semi-structured interviews (n = 67), an end-of-study focus
   group with patients and psychologists, and qualitative diary entries.
   Integrating service actors' attitudes, beliefs, and experiences over
   time, the article presents the Service Separation Value Journey to show
   how digital service separation shapes value creation/destruction in
   specialized healthcare settings.
   As inovacoes digitais estao mudando como as organizacoes publicas
   prestam os servicos por meio da criacao do fenomeno da separacao de
   servicos. A separacao de servicos refere-se a dissociacao entre a
   producao e o consumo de servicos em plataformas digitais, o que afeta
   tanto os usuarios como os prestadores de servico. Desde a pandemia
   global, o fenomeno da separacao de servicos estabeleceu-se em varios
   ambientes de servico de saude, porem pouco ainda e conhecido sobre como
   os atores de servicos tem experimentado esse fenomeno em ambientes de
   servicos especializados como o de saude mental. Posicionado no nivel
   (sub)micro da interacao de servicos, nosso estudo fornece uma visao
   dupla da separacao de servicos para revelar a sua influencia na
   (co)criacao e potencial destruicao de valor em um estudo longitudinal de
   12 meses. Os resultados baseiam-se em tres ondas de entrevistas
   semiestruturadas (n = 67), um grupo focal realizado no final do estudo
   com pacientes e psicologos, e anotacoes qualitativas em diarios.
   Integrando atitudes, crencas e experiencias dos atores do servico ao
   longo do tempo, o artigo apresenta a Jornada de Valor da Separacao de
   Servicos para mostrar como a separacao digital de servicos molda a
   criacao/destruicao de valor em ambientes de saude especializados.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
