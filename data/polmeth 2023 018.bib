Author = {Campos-Alba, Cristina M. and Chica-Olmo, Jorge and Perez-Lopez, Gemma
   and Zafra-Gomez, Jose L.},
Title = {Modeling political mimetic isomorphism versus economic and quality
   factors in local government privatizations},
Journal = {PUBLIC ADMINISTRATION},
