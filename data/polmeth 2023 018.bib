@article{ WOS:000916556200001,
Author = {Chang, Yang-Ming and Sellak, Manaf},
Title = {Conflict and agreement in the collective choice of trade policies:
   implications for interstate disputes},
Journal = {PUBLIC CHOICE},
Year = {2024},
