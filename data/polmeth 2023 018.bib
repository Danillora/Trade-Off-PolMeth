@article{ WOS:001137737700005,
Author = {Abdi, Ahmed Nor Mohammed},
Title = {The mediating role of perceptions of municipal government performance on
   the relationship between good governance and citizens' trust in
   municipal government},
Journal = {GLOBAL PUBLIC POLICY AND GOVERNANCE},
Year = {2023},
Volume = {3},
Number = {3},
Pages = {309-333},
Month = {SEP},
Abstract = {Evidence from existing literature shows that good governance practices
   impact citizens' attitudes and behaviors toward the governing
   authorities. However, the situation in post-conflict countries like
   Somalia is largely unexplored and unknown. Therefore, this study
   examines how good governance elements (public participation,
   accountability, and transparency) influence citizens' trust in municipal
   government in Somalia. Furthermore, this study investigates the
   mediating role of perceptions of municipal government performance on the
   relationship between good governance elements and trust in municipal
   governments. Following a cross-sectional research design, data were
   collected from 357 residents of Mogadishu municipality in Somalia.
   Partial least squares structural equation modeling was used by applying
   SmartPLS4 to analyze the data and evaluate the measurement and
   structural models. The results show that perceived good governance
   elements (public participation, accountability, and transparency)
   positively and directly influence citizens' trust in municipal
   government. Furthermore, perceptions of municipal government performance
   partially mediate the relationship between good governance elements and
   citizens' trust in municipal authorities. This study is one of the first
   to provide empirical evidence of the mediating role of perceptions of
   municipal government performance on the relationship between good
   governance elements and citizens' trust in municipal governments.},
