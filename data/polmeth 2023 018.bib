@article{ WOS:000935792400001,
Author = {Ramiarison, Maholisoa H.},
Title = {The US-China economic tensions and China's growing influence in East
   Asia: Japan's eclectic approach to the free and open Indo-Pacific},
Journal = {INTERNATIONAL POLITICS},
Year = {2023},
Month = {2023 FEB 20},
Abstract = {This paper discusses the changing dynamics in Japan's economic and
   foreign policies in the context of US-China trade tensions and China's
   greater economic and political prominence in East Asia. It investigates
   the different phases of Japan's foreign policy since the World War II.
   It argues that the post-war order provided Japan with an environment
   that favored the use of positive economic statecraft and saw the
   expansion of her trade and investment with partner countries, including
   China. However, the increasing economic competition from China coupled
   with the recent escalation of US-China rivalry has brought about changes
   in Japan's national and economic security. These changes have in turn
   led to several reforms giving Japan's policy a more nuanced and eclectic
   features. We demonstrate that rather than signaling a clear position
   against or in favor of China, Japanese policymakers have been engaging
   in a mixed strategy involving military balancing, hedging, and economic
   cooperation.},
