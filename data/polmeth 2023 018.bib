@article{ WOS:001105882000001,
Author = {Schwartz, Jared and Biberman, Yelena and Hotz, Samantha},
Title = {India as a restorationist state: implications for US-India relations and
   South Asian politics},
Journal = {INTERNATIONAL POLITICS},
Year = {2023},
Month = {2023 NOV 21},
Abstract = {India is a pivotal state in US strategy of ``out-competing{''} Beijing
   in the Indo-Pacific and globally. Experts generally agree that
   Washington and New Delhi share a common interest in deepening the
   bilateral partnership as India grows as one of the world's great powers.
   While some have pointed out the strategic limitations of the
   partnership, this article argues there is also an important political
   challenge. Since 2014, India has increasingly embraced a restorationist
   vision-a national project to restore past glory. By instrumentalizing
   culture and heritage as the wellspring of sovereignty, while promoting a
   narrative of victimization, India's ruling political party, Bharatiya
   Janata Party, has created the conditions for a restorationist foreign
   policy. If this approach continues to develop, a new system in Asia may
   emerge in which the regional powers (India, China, and Russia)
   prioritize righting perceived historical wrongs over respecting their
   neighbors' sovereignty.},
