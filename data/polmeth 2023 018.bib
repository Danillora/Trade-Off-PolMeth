Author = {Hussain, Fiaz and Tsang, Denise and Rafique, Zain},
Title = {Policy advisory systems and public policy making: Bibliometric analysis,
   knowledge mapping, operationalization, and future research agenda},
Journal = {REVIEW OF POLICY RESEARCH},
