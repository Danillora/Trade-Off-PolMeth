@article{ WOS:001083996600001,
Author = {Li, Tan and Luo, Jar-Der and Zheng, Enying},
Title = {Revolving around political connections: the negative effect of
   government venture capital backing on IPO valuation},
Journal = {SOCIO-ECONOMIC REVIEW},
