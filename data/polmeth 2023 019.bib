@article{ WOS:001096473500001,
Author = {Brock, Clare and Mallinson, Daniel},
Title = {Measuring the stasis: Punctuated equilibrium theory and partisan
   polarization},
Journal = {POLICY STUDIES JOURNAL},
Year = {2024},
Volume = {52},
