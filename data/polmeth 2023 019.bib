@article{ WOS:001166904800001,
Author = {Hui, Victoria Tin-bor},
Title = {`The rise and Fall of Eurasian world orders', the Francesco
   Guicciardini prize forum},
Journal = {CAMBRIDGE REVIEW OF INTERNATIONAL AFFAIRS},
Year = {2023},
Month = {2023 OCT 21},
Abstract = {Ayse Zarakol's Before the West: The Rise and Fall of Eastern World
   Orders successfully delivers what it promises: it challenges
   Eurocentrism in international relations theory by providing a better
   alternative. The alternative is the Chinggisid sovereignty model that
   yoked Eurasia into successive world orders from the thirteenth to the
   sixteenth centuries. The Chinggissid model undercuts another strand of
   ethnocentrism: Sinocentrism. The book does not set out to examine where
   the Chinggissid model came from, or what happened BEFORE `Before the
   West' in prior millennia. A more longue duree perspective suggests that
   elements of the Chinggissid model had Chinese precedents, which were
   themselves Eurasian-Chinese hybrids. The breadth and depth of Chinggisid
   elements suggest that they fell but did not disappear AFTER `Before the
   West'. They may, indeed, have risen again in today's world.},
