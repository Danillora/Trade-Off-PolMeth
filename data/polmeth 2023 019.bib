@article{ WOS:001025100100001,
Author = {Coetzee, Wayne Stephen and Larsson, Sebastian and Berndtsson, Joakim},
Title = {Branding `progressive' security: The case of Sweden},
Journal = {COOPERATION AND CONFLICT},
Year = {2024},
Volume = {59},
Number = {1},
Pages = {86-106},
Month = {MAR},
Abstract = {Contemporary research on so-called Nordic branding has provided crucial
   insights into the social power of states and how various actors use and
   circulate `progressive' nation brand tropes for political and commercial
   goals. Hitherto, the literature on Nordic branding has focused on a wide
   range of substantive issues, among other things, human rights, gender
   equality, social welfare and foreign aid, but considerably less
   attention has been paid to the topic of security. The present article
   adds to a small but established literature on how the security sphere is
   increasingly entangled with nation branding. In the Nordic region, we
   argue, the latter is particularly evident in the case of Sweden - one of
   the world's largest per-capita arms exporters in the post-Cold War era
   but also a country known and often revered for its peaceful and
   progressive image. Focusing on the case of Sweden, the article
   contributes to knowledge of how defence industry-related actors (both
   public and private) draw on and frame nation branding tropes to sell and
   legitimise their products and services to both insiders (domestic
   constituents) and outsiders (the global security market).},
