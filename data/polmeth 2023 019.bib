@article{ WOS:001049144000001,
Author = {Chenzi, Vincent and Ndamba, Admire},
Title = {Surviving the Covid-19 lockdown: Zimbabwe's informal sector, 2020-2021},
Journal = {REVIEW OF AFRICAN POLITICAL ECONOMY},
