@article{ WOS:001080019900001,
Author = {Fernandez, Didimo Castillo},
Title = {The Pandemic, the Crisis of the Neoliberal Model, and the Emergence of
   Digital Capitalism: The New Labor Market Environment},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2023},
Volume = {50},
Number = {4, SI},
Pages = {88-102},
Month = {JUL},
Abstract = {The pandemic caused by the rapid spread of the SARS-CoV-2 coronavirus
   and the associated COVID-19 disease brought to light not only the
   foundations of Western modernity and its assumptions about economic and
   social development but also the huge contradictions of the prevailing
   globalizing model. The pandemic exposed the structural and intrinsic
   contradictions of neoliberal globalization. The current economic and
   social consequences are related to the deepening of that model and
   particularly to the history of the prevailing employment structures and
   social inequalities. The emerging scenarios are uncertain, but the
   acceleration of an already advanced process of digital capitalism
   appears in sight. This process will entail, as part of the ``new
   normal,{''} new forms of precarity and social inequality.La pandemia
   causada por la rapida propagacion del coronavirus SARS-CoV-2 y la
   asociada enfermedad COVID-19 puso en evidencia no solo los fundamentos
   de la modernidad occidental y sus supuestos sobre el desarrollo
   economico y social, sino tambien las enormes contradicciones del modelo
   globalizador imperante. La pandemia expuso las contradicciones
   estructurales e intrinsecas de la globalizacion neoliberal. Las
   consecuencias economicas y sociales actuales estan relacionadas con la
   profundizacion de dicho modelo y, en particular, con la historia de las
   estructuras de empleo prevalecientes y las desigualdades sociales. Los
   escenarios emergentes son inciertos, pero la aceleracion de un proceso
   ya avanzado de capitalismo digital es inminente. Este proceso implicara,
   como parte de la ``nueva normalidad{''}, nuevas formas de precariedad y
   desigualdad social.},
