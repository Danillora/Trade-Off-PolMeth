@article{ WOS:000999708500001,
Author = {Oskolkov, Petr},
Title = {Estonianness in the Making: Transformations of Ethnic Democracy Model
   and Nationalism in Estonia},
Journal = {ETHNOPOLITICS},
Year = {2024},
Volume = {23},
Number = {5},
Pages = {467-486},
Month = {OCT 19},
Abstract = {Estonian nationalism has undergone a transformation from `state-seeking'
   to `state-led' since acquiring independence, which has affected the
   Estonian national identity. Estonia is commonly labelled as an `ethnic
   democracy'; however, we propose a dichotomy between open and closed
   ethnic democracies. The process of national state-building in Estonia is
   still going on, with the concept of `Estonianness' consisting of the
   `Finno-Ugric,' `Nordic/Scandinavian,' and `European' identity
   structures. These form the basis for an inclusive, or open ethnic
   democracy. Though Estonianness is primarily defined in cultural and
   linguistic terms, a newcomer may ascribe oneself to it and become a
   member of the ethno-civic nation.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
