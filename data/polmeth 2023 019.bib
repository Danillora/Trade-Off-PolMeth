@article{ WOS:001004298100001,
Author = {Sarturi, Greici and Barakat, Simone R. R. and Gomes, Ricardo Correa},
Title = {Stakeholder theory in the public sector domain: A bibliometric analysis
   and future research agenda(sic)(sic)(sic)Palabras clave},
Journal = {REVIEW OF POLICY RESEARCH},
Year = {2025},
Volume = {42},
Number = {3},
Pages = {736-756},
Month = {MAY},
Abstract = {Stakeholder theory has been widely studied and used in private companies
   because of its potential to achieve satisfactory organizational outcomes
   and create sustainable value among different stakeholder groups.
   However, the stakeholder theory principles still need to be consolidated
   in the public administration field. To advance this discussion, this
   study undertook a bibliometric analysis to collect, analyze, and
   synthesize the research that had already been developed in this regard.
   We analyzed 1805 articles published between 1982 and 2021. Our findings
   show that even though some ideas of stakeholder theory seem to be
   associated with important topics in the public administration field,
   such as participation and governance, some of the central principles of
   stakeholder theory have still not been explored in depth in studies of
   the public sector. A proposed research agenda indicates the need for
   stakeholder management theories and their interaction with public value,
   performance, sustainability and innovation.},
