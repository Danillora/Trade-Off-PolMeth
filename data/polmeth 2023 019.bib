@article{ WOS:001026059800003,
Author = {Fons, Antonio Gil},
Title = {THE RELATION BETWEEN THE KINGDOM OF MOROCCO AND THE STATE OF ISRAEL: A
   PRAGMATIC AND LONGSTANDING GEOPOLITICAL COOPERATION},
Journal = {FORO INTERNACIONAL},
