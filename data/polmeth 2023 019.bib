@article{ WOS:001016607000001,
Author = {Kakhishvili, Levan},
Title = {Do domestic institutional actors matter in Georgian foreign policy?
   Unpacking national role conceptions of bureaucracies and political
   parties in Georgia},
Journal = {EAST EUROPEAN POLITICS},
Year = {2024},
Volume = {40},
Number = {2},
Pages = {219-238},
Month = {APR 2},
Abstract = {How do national role conceptions (NRCs) of domestic institutional actors
   change in response to external and internal shocks? Using role theory in
   foreign policy analysis, this article explores how bureaucratic agencies
   and political parties in Georgia respond to shocks, such as the 2008
   Russo-Georgian War and changes in political leadership. Content analysis
   of 18 hand-coded documents, including all security, military, and
   foreign policy strategies as well as ten party manifestos of three
   ruling parties of Georgia, shows that NRCs are modified in response to
   shocks. While bureaucracies are more stable, party responses have varied
   between radicalisation and compensation strategies.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
