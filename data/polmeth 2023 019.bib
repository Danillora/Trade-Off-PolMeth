@article{ WOS:001068160400004,
Author = {Overton, Kathryn L. and Sharif, Sally},
Title = {Agents with Principles? Preventing Conflict-Related Sexual Violence with
   Human Rights Laws and Norms},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2023},
Volume = {42},
Number = {3},
Pages = {487+},
Month = {AUG},
Abstract = {Focusing on conflict-related sexual violence (CRSV), we posit that
   behavioral expectations in the form of norms and laws have a visible
   impact on government militaries. Using data from over 2,000
   actor-incidents between 1989 and 2015, we find a robust negative
   relationship between physical integrity norms and CRSV. CRSV is often a
   gendered act, but its prevention is not. Our analysis suggests that CRSV
   prevention rests with the imperative to respect physical integrity
   rights. This inherent respect restrains government actors even when
   elements of formal military control are absent or only inconsistently
   enforced.},
