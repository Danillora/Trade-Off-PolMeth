@article{ WOS:001163538900003,
Author = {Stepniewski, Tomasz},
Title = {Conflict as a Strategic Imperative in Putin's Political Agenda: A
   Fundamental Component of Russia's Strategic Framework},
Journal = {STUDIA EUROPEJSKIE-STUDIES IN EUROPEAN AFFAIRS},
Year = {2023},
Volume = {27},
Number = {4},
Pages = {57-69},
Abstract = {This paper delves into the nuanced motivations underpinning Russia's
   2014 invasion of Ukraine, centered on preserving stability for Putin's
   regime. Examining the intricate interplay between domestic and foreign
   concerns, the invasion emerged as a pivotal strategy to bolster domestic
   support and reinforce Russian leadership. Post-2012, Putin, in
   consolidating power through internal policies and external
   assertiveness, responded to catalysts - opposition protests in 2012 and
   Ukraine's ``Revolution of Dignity{''} in 2014. State-controlled media
   played a crucial role in shaping narratives, framing the invasion as a
   defense against alleged Western ``imperialist{''} threats. The paper
   scrutinises Putin's strategic evolution, highlighting the convergence of
   historical events, opposition dynamics, and media narratives in shaping
   the rationale behind Russia's impactful incursion into Ukraine.},
