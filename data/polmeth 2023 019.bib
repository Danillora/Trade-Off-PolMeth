@article{ WOS:001106232500001,
Author = {Ballekura, Balakrishna and Vilvanathan, Lavanya},
Title = {Does family incivility trigger suicidal ideation? The role of emotional
   exhaustion at work and regulation of emotion},
Journal = {INTERNATIONAL JOURNAL OF CONFLICT MANAGEMENT},
