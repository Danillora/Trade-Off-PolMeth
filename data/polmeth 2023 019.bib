@article{ WOS:000924293400001,
Author = {Fiori, Juliano and Arantes, Pedro Fiori},
Title = {Brazil's Cultural Battleground: Public Universities and the New Right},
Journal = {LATIN AMERICAN PERSPECTIVES},
