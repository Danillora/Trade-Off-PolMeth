@article{ WOS:001066737800001,
Author = {De Castro, Renato Cruz},
Title = {The Philippines in the face of the Ukraine-Russia War: from confusion
   and disorientation to critical neutrality},
Journal = {INTERNATIONAL POLITICS},
Year = {2024},
Volume = {61},
Number = {5, SI},
Pages = {1002-1017},
Month = {OCT},
Abstract = {This article examines the Philippines' policy of critical neutrality in
   the face of the Ukraine-Russia War. This policy is characterized by the
   refusal to take any side in the conflict between Ukraine and Russia,
   occasional criticism of Russia's handling of the war, and efforts to
   offer the U.S. military facilities in the Philippines if the war spreads
   and spills over to the East Asia. In conclusion, the article argues that
   the Philippines is taking advantage of the best of two worlds, namely
   (a) taking the moral high ground in criticizing Russia's war against
   Ukraine and (b) enhancing its alliance with the U.S. as a hedge to the
   possibility that China might take a page from Russia's playbook in
   Ukraine by pursuing its irredentist goals in the Taiwan Straits and the
   South China Sea.},
