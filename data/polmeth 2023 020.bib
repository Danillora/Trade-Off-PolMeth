@article{ WOS:000979998000001,
Author = {Miller, Samuel},
Title = {Supreme Gentlemen: The Path of Radicalization for the Incel Community's
   Lone Wolves},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2024},
Volume = {36},
Number = {6},
Pages = {818-833},
Month = {AUG 17},
Abstract = {Several men committed mass violence in the past decade because they
   believed women were denying them the sex they felt they deserved. In the
   wake of their attacks, investigators found references to an online
   community known as Incels in their manifestoes and social media posts.
   Members of this community typically adhere to misogynistic beliefs that
   deny women their autonomy, deride sexually active men, and condone
   violence against both. Using Hamm and Spaaij's lone-wolf radicalization
   model, this article argues that the most violent members of the Incel
   community engaged in lone-wolf terrorism based upon their grievances
   with women. Furthermore, this article suggests that the influential
   impact of misogyny as a political element can contribute to lone-wolf
   terrorism. Accounting for these aspects will require modifications to
   the radicalization model that include factors such as domestic violence
