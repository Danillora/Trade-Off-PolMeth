@article{ WOS:000971611100001,
Author = {Thompson, Sara K. and Grossman, Michele and Thomas, Paul},
Title = {Needs, Rights and Systems: Increasing Canadian Intimate Bystander
   Reporting on Radicalizing to Violence},
