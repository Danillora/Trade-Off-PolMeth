@article{ WOS:001172560900009,
Author = {Farsund, Arild Aurvag},
Title = {Agriculture and Fisheries in Norway. Outside, but Increasingly Inside
   the EU?},
Journal = {INTERNASJONAL POLITIKK},
Year = {2023},
Volume = {81},
Number = {4},
Pages = {631-643},
Abstract = {This article draws attention to developments in agricultural and
   fisheries policy in Norway and the EU after the implementation of the
   EEA Agreement in 1994. It is argued that the policy in both areas has
   become more similar, and this is explained by the fact that both Norway
   and the EU must follow the same international agreements, that both
   share many of the same ideas regarding what constitutes proper policies,
   and that Norway is increasingly importing laws and regulations from the
   EU. In conclusion, it is argued that continued EEA association will be
   far less challenging for the agricultural and fishing industries than a
   new free trade agreement or EU membership.},
