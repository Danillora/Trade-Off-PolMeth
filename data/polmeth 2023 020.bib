@article{ WOS:001127201500008,
Author = {Hokayem, Emile},
Title = {The Gaza War and the Region},
Journal = {SURVIVAL},
Year = {2023},
Volume = {65},
Number = {6},
Pages = {57-66},
Month = {NOV 2},
Abstract = {Hamas's 7 October 2023 attack and the subsequent war in Gaza has forced
   the Palestinian question back to the top of the Middle Eastern agenda
   after years of neglect. The crisis has also confirmed Hamas's identity
   as an agent of resistance rather than governance and shattered Israel's
   perceptions of its own power, the competence of its security services
   and political leadership, and the manageability of its immediate
