@article{ WOS:001046417700001,
Author = {Khan, Aasim and Fatimah, Midhat and Dureja, Kabir and Jumle, Vedant},
Title = {Decoding the star system: Twitter and its impact on journalism in the
   global South},
Journal = {GLOBAL POLICY},
