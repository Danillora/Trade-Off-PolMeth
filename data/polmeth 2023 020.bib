@article{ WOS:000934363600001,
Author = {Longman, Timothy},
Title = {Researching under constraints: Recent books on post-genocide Rwanda},
Journal = {JOURNAL OF HUMAN RIGHTS},
Year = {2023},
Volume = {22},
Number = {3},
Pages = {425-430},
Month = {MAY 27},
Abstract = {Rwanda has been a focus of substantial scholarly attention, but recent
   regulations there have made conducting research increasingly
   challenging. Four books from diverse disciplines show that, despite the
   ways in which the authoritarian context places constraints on what
   research can be undertaken and how it can be done, solid scholarship on
   Rwanda can continue to be produced. They also show that the 1994
   genocide against the Tutsi remains the focal point of nearly every book
   on the country, even those focused on society since 1994.},
