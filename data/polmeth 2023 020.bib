@article{ WOS:001137737700003,
Author = {Woods, Dwayne},
Title = {When the chips are down: shifting policy signals in a globalized
   technology regime},
Journal = {GLOBAL PUBLIC POLICY AND GOVERNANCE},
Year = {2023},
Volume = {3},
Number = {3},
Pages = {268-291},
Month = {SEP},
Abstract = {The escalating rivalry between the United States and China in the
   semiconductor industry has critical implications for global technology
   policy and governance. This paper proposes an innovative approach to
   modeling the strategic interactions between these superpowers using
   concepts from reinforcement learning and game theory. Specifically, we
   apply Coordinating Q-Learning, an algorithm adept at handling complex
   multi-agent systems, to simulate signaling games between the U.S. and
   China. This technique avoids problematic rationality assumptions that
   often hinder empirical verification of signaling models. Through
   iterative learning based on environmental rewards, the model provides
   insights into how semiconductor strategy may evolve. We present
   simulated outcomes that reveal potential scenarios and strategic
   decisions shaping the U.S.-China relations in this sphere. The model
   highlights the risks of undermining existing multilateral governance
   regimes and fracturing global technology ecosystems.},
