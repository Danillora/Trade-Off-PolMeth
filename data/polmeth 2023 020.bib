@article{ WOS:000911184600001,
Author = {Zhuang, Meixi and Wang, Zhengxu and Li, Xiaoyuan},
Title = {Social Embeddedness and Protest Avoidance: Evidence from China},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2023},
Volume = {67},
Number = {9},
Pages = {1704-1730},
Month = {OCT},
Abstract = {Social capital is generally believed to facilitate protest mobilization.
   We examine, however, whether and how it might reduce an individual's
   tendency to join a protest. Analyzing survey data of a migrant
   population in Beijing, China's capital, we find that social capital
   operating as better embeddedness in local social networks lowers the
   likelihood of protest participation. We reason that, structurally,
   embeddedness in local networks provides access to support and
   assistance, reducing the attractiveness of disruptive means of
   problem-solving. Psychologically, such embeddedness mitigates the
   feeling of relative deprivation, reducing the possibility of such
   feelings accumulating and eventually translating into protesting
   actions. Regressions and structural equation models confirm our
   hypotheses, and we are also able to check the robustness of our findings
   with a different, much larger, cross-national dataset.},
