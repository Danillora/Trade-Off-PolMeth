@article{ WOS:001112713500001,
Author = {Carcaba, Ana and Gonzalez, Eduardo and Arrondo, Ruben},
Title = {Effects of the political configuration of local governments on
   subjective well-being},
Journal = {POLICY STUDIES},
Year = {2025},
Volume = {46},
Number = {2},
Pages = {202-220},
Month = {MAR 4},
Abstract = {Determining what makes a person happy is an extremely complicated task.
   The objective of this article is to identify the effects of the
   composition and orientation of governance bodies in municipalities on
   individual subjective well-being. We connect the data from a large
   Spanish welfare survey to municipal data covering the aforementioned
   dimensions of political configuration. Unlike previous country-level
   studies, we find no significant effects of political orientation when
   applied to municipal data. In contrast, political alternation emerges as
   a relevant driver of subjective well-being, especially when corrupt
   local governments are replaced. Furthermore, the fragmentation in the
   Spanish political landscape after the 2015 elections improved the level
   of political competition, which, in turn, exerted a positive effect on
   subjective well-being.},
