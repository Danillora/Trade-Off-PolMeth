@article{ WOS:001103498900004,
Author = {Roth, Hans Ingvar},
Title = {PC Chang and Charles Malik: The Two Philosophers of the Universal
   Declaration of Human Rights},
Journal = {HUMAN RIGHTS QUARTERLY},
