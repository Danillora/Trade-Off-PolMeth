@article{ WOS:000980661500001,
Author = {Jastram, Sarah Margaretha and Berberyan, Zara},
Title = {Democratic Stakeholder Representativeness},
Journal = {JOURNAL OF PUBLIC ADMINISTRATION RESEARCH AND THEORY},
Year = {2023},
Volume = {33},
