@article{ WOS:001109562400002,
Author = {Moene, Kalle},
Title = {Market, Power, and Countervailing Forces - in the Time of Adam Smith,
   Henry Hunt, and Henry George},
Journal = {INTERNASJONAL POLITIKK},
Year = {2023},
Volume = {81},
Number = {3},
Pages = {379-391},
Abstract = {Henry Hunt, Henry George The English Corn Laws, 1815-1846, were
   initiated to protect landowners' commercial rents; the 1770 food crisis
   in France to raise the surplus on the King's land; the 1770 famine in
   Bengal to raise the profits of the East India Company. These examples
   indicate how poverty and starvation in the period 1750-1850 could be
   caused by commercial power abuse, harming the public but benefiting
   landowners and commercial interests. The essay emphasizes how
   progressive economists at the time recommended free trade to tame the
   commercial abuse. The left agitated for markets rather than monopoly.
   The right agitated for privileges rather than competition.},
