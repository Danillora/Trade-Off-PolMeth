@article{ WOS:000963656200002,
Author = {Dana, Tariq},
Title = {The EU and its paradoxes: enforcing stability not promoting democracy},
Journal = {INTERNATIONAL POLITICS},
Year = {2023},
Volume = {60},
Number = {3},
Pages = {762-767},
Month = {JUN},
Abstract = {This critical analysis explores the contradictions between the European
   Union's (EU) democracy promotion rhetoric and the underlying motivations
   behind these efforts, specifically in the context of the 2006
   Palestinian elections. Contrary to the traditional perception that EU
   democracy promotion aims to establish and strengthen democratic
   institutions and practices, this review argues that the primary goal is
   often to enforce stability under the expansive settler-colonial
   structure. The Palestinian elections of 2006 serve as a pertinent case
   study to illustrate this paradox. The EU's response to the victory of
   Hamas was marked by inconsistencies and contradictions, revealing the
   tension between its democracy promotion rhetoric and the prioritization
   of a kind of stability based on Israeli terms.},
