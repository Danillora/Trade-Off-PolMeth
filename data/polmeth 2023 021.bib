@article{ WOS:001040704600001,
Author = {Tolis, Valeria},
Title = {A Method for Change. Lacanian Discourse Analysis: A Glimpse into Climate
   Policy},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
