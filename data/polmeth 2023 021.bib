@article{ WOS:000909876700001,
Author = {Mantilla, Giovanni},
Title = {Deflective Cooperation: Social Pressure and Forum Management in Cold War
   Conventional Arms Control},
Journal = {INTERNATIONAL ORGANIZATION},
