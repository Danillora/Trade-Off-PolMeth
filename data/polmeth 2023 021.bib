@article{ WOS:001106063400001,
Author = {Gu, Yanfeng and Zheng, Bingdao},
Title = {The Double-Edged Sword of State Media Credibility: Experimental Evidence
   from China},
Journal = {JOURNAL OF CHINESE POLITICAL SCIENCE},
Year = {2023},
Month = {2023 NOV 22},
