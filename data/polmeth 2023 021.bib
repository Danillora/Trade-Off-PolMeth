@article{ WOS:001111254100001,
Author = {Luciano, Bruno Theodoro and Ayuso, Anna},
Title = {Revista Brasileira de Política Internacional},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2023},
Volume = {66},
Number = {2},
Abstract = {This article assesses how environmental issues have been incorporated
   into the strategic association between the European Union and Latin
   America and the Caribbean. It analyses whether the 2030 Agenda and the
   Sustainable Development Goals are reflected in the EU-CELAC
   declarations, and what bi-regional programmes and projects have been
   developed in relation to the environmental SDGs. The analysis shows how
