@article{ WOS:000974345500001,
Author = {Mena, Ferran Perez},
Title = {Farewell to revolution: the `Chinese School of IR' and the
   depoliticisation of IR theory in post-Mao China},
Journal = {INTERNATIONAL POLITICS},
Year = {2024},
Volume = {61},
Number = {4},
Pages = {703-717},
Month = {AUG},
Abstract = {During the last years, coinciding with the rise of non-Western IR theory
   and Global IR, IR scholars have become interested in the study of the
   `Chinese School of IR'. Mainstream literature on its advent primarily
   focuses on its development as the result of a revisionist China that
   seeks to challenge the liberal international order in the material and
   ideational spheres. This article seeks to question this narrative.
   Through a critical IR approach that considers the nexus between the
   production of IR theory, geopolitics and the development of state
   ideology in China after 1979, I contend that the CSIR should be
   understood as a complex, and sometimes contradictory, group of Chinese
   IR scholars that seek to provide depoliticised IR theories to legitimise
   non-Marxian views of the Chinese state and protect its political
   autonomy in global politics, whilst contributing to the reimagination of
   what means to advance IR theory from China.},
