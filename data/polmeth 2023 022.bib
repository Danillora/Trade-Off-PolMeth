@article{ WOS:001056809400001,
Author = {Kersting, Joel B.},
Title = {An environmental education: how the education realignment polarized
   Congress on the environment},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2024},
Volume = {33},
Number = {4},
Pages = {612-634},
Month = {JUN 6},
Abstract = {As the international community attempts to unite to combat climate
   change, American party politics could hardly be more divided on this
   issue. This paper offers an additional explanation for how US
   congressional politics on environmental policy has polarized: the
   ongoing education realignment in American party politics. As the
   Democratic Party increasingly relies on college-educated voters and the
   opposite is true for the Republican Party, this can affect the parties'
   positions on environmental policy based on public opinion research which
   finds a positive relationship between education and pro-environment
   attitudes. Using League of Conservation Voters legislative scorecards
   from 1983 to 2020, this paper finds the education realignment
   contributed to the removal of pro-environment Republicans and
   anti-environment Democrats in Congress in recent decades; and this
   primarily occurred through elite replacement rather than conversion.},
