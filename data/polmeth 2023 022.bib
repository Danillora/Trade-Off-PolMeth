@article{ WOS:000914293200001,
Author = {Bailey, Kathleen S. and Yi, Hongtao and Heikkila, Tanya and Weible,
   Christopher M.},
Title = {Policy conflicts in shale development in China and the United
   StatesPalabras Clave(sic)(sic)(sic)},
Journal = {REVIEW OF POLICY RESEARCH},
