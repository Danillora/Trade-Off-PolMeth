Author = {Hartzell, Caroline and Hoddie, Matthew and Page, Douglas},
Title = {Civil War Settlements, Perceived Agreement Fairness, and Trust in
   Political Authorities: Investigating the Influence of Power Sharing on
   Public Opinion in Divided Societies},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2024},
Volume = {68},
