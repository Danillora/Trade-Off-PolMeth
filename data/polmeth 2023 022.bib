@article{ WOS:001041411300001,
Author = {Campbell, Heather E. and Kim, Sekwen and Johnson, Shawnika and Caceres,
   Claudia},
Title = {Environmental racism and air pollution: Pre and post the COVID-19
   economic shutdown(sic)(sic)(sic)Palabras clave},
Journal = {REVIEW OF POLICY RESEARCH},
