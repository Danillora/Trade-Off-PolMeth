@article{ WOS:000963936000001,
Author = {Persson, Mikael},
Title = {Who got what they wanted? Investigating the role of institutional agenda
   setting, costly policies, and status quo bias as explanations to income
   based unequal responsiveness},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2024},
Volume = {31},
Number = {7},
Pages = {1879-1901},
Month = {JUL 2},
Abstract = {Previous research has shown that elected officials are more responsive
   to the opinions of high-income citizens than to those of middle and
   working-class citizens in the United States. This is often explained by
   the fact that economic elites make campaign contributions to political
   elites, leading to decision-making that aligns with the preferences of
   the affluent. This paper examines the opinion-policy link in Swedish
   politics, where campaign contributions are relatively low. Despite this,
   the study finds that high-income citizens still receive the most policy
   responsiveness. Three alternative possible explanations are discussed.
   Do high-income citizens receive more responsiveness because (a) they are
   better able to put issues on the political agenda, (b) because they are
   easier to satisfy and prefer `cheaper' symbolic policy reforms, while
   low-income citizens prefer more costly policies or (c) because the
   status quo bias works to the advantage of high-income citizens?},
