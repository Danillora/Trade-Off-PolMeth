@article{ WOS:001024930600001,
Author = {Mantilla, Jorge},
Title = {Disruptive hope: the communal repertoires of violence resistance in
   Cucuta},
Journal = {PEACEBUILDING},
Year = {2023},
Volume = {11},
Number = {4, SI},
Pages = {358-380},
Month = {OCT 2},
Abstract = {This paper looks at the micro-politics of crime disorder and everyday
   peacebuilding in Colombia and Venezuela's contested urban systems,
   particularly Cucuta. Despite being plagued with crime, Cucuta is also a
   space of hope where resilience and coexistence shape the survival
   strategies of thousands of marginalised people who transit or live in
   this contested city. Focusing on how residents, including immigrants,
   navigate informality and crime at the street and neighbourhood level,
   this study contributes to the emerging literature on how people resist
   criminal governance in violently contested cities by exploring the
   overlap between violent crime and everyday peace practices. Notably, it
   examines how mobility restrictions, and contested informalities come
   into play in places where social order is bargained between non-state
   armed groups and the state. Building on Urban Criminology and Peace
   Studies, I show how communities navigate and resist violence and crime
   in a violently contested city.},
