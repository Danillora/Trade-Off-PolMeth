@article{ WOS:001537752900001,
Author = {Levy, Yagil},
Title = {Legitimacy of violence: extra-military versus intra-military legitimacy},
Journal = {CRITICAL MILITARY STUDIES},
Year = {2023},
Volume = {9},
Number = {2},
Pages = {216-235},
Month = {APR 3},
Abstract = {This article is conceptually motivated. By drawing on cases from the
   U.S. and Israeli militaries, it aims at demonstrating the existence of
   two separate systems of legitimacy of military violence - extra-military
   and intra-military - and mapping the gaps between them. It
   conceptualizes the legitimacy of violence and then maps seven conditions
   under which gaps are created between the two systems, as follows: the
   uniqueness of military culture, the extent to which the military does
   not mirror society, field command's broadening improvisation and
   interpretation, ambiguous political directives, the extent to which the
   military leverages a legitimacy dispute, troops' resistance, and the
   diachronic systems of legitimacy. The appearance of these gaps is more
   likely to present with weakening of civilian control and the break-up of
   military hierarchy.},
