@article{ WOS:001488752800002,
Author = {Schmidt, Elizabeth and Lada, Jenna and Donahoe, Amanda E. and Hancock,
   Landon E.},
Title = {The Power of Voice: Storytelling as Peacebuilding in Post-Troubles
   Northern Ireland},
Journal = {JOURNAL OF PEACEBUILDING \& DEVELOPMENT},
Year = {2023},
Volume = {18},
Number = {1},
Pages = {36-52},
Month = {APR},
Abstract = {The Northern Ireland peace process has been long and tumultuous, yet 20
   years after the Good Friday Agreement, peace has not become a reality
   for many Northern Irish citizens. For those who have been excluded from
   the state and the institution-led peace process, grassroots methods of
   peacebuilding are needed to create an everyday peace that is owned by
   and includes all Northern Irish citizens. A narrative approach to
   peacebuilding is one that addresses many of the challenges to everyday
   peace following the ``Troubles.{''} By exploring the use and impact of
   storytelling and narrative on everyday experiences, the narrative
   approach proves to be a viable tool for elevating and giving ownership
   of the peace process to marginalized members of Northern Irish society.},
