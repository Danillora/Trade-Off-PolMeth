@article{ WOS:001021174600001,
Author = {Weimann, Gabriel and Pack, Alexander},
Title = {TamTam: The Online Drums of Hate},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2023},
Month = {2023 JUN 14},
Abstract = {Recognizing the correlation between the growing spread of violent
   extremist and terrorist content on the internet and online platforms and
   the significant increase in attacks inspired by these posts prompted
   governments, security agencies, and private companies to launch various
   countermeasures to reduce the spread and impact of such material.
   Struggling to adjust to these countermeasures and maintain their online
   presence, terrorists and violent extremists have migrated from
   mainstream online platforms to alternative online channels like 4chan,
   8chan, Telegram, TikTok, and most recently to the new platform TamTam.
   This paper describes the migration to and within social media generally,
   with a specific focus on the newly discovered migration to the platform
   TamTam and presents the results of our extensive content analysis of
   hundreds of posts and shares containing terrorist and violent extremist
   content on this platform. These findings are discussed in terms of
   ethical and practical implications.},
