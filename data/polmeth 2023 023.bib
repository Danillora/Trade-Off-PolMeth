@article{ WOS:000949315100001,
Author = {Ronnblom, Malin and Carlsson, Vanja and Ojehag-Pettersson, Andreas},
Title = {Gender equality in Swedish AI policies. What's the problem represented
   to be?Palabras Clave(sic)(sic)(sic)},
Journal = {REVIEW OF POLICY RESEARCH},
