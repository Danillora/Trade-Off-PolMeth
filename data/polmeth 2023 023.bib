@article{ WOS:001131752800001,
Author = {Gavric, Sasa and Vojvodic, Anja},
Title = {Are consociations bad for women? Assessing the effectiveness of gender
   quotas in Bosnia and Herzegovina and North Macedonia},
Journal = {EAST EUROPEAN POLITICS},
Year = {2024},
Volume = {40},
Number = {3},
Pages = {412-430},
Month = {JUL 2},
Abstract = {To overcome ethno-national segregation, conflict resolution
   practitioners have promoted consociational power-sharing. After
   power-sharing arrangements were instituted, to address women's
   under-representation in parliaments legislated gender quotas were also
   introduced. Gender quotas have been adopted in many post-conflict
   settings, but their effects remain understudied. In this article, by
   combining consociationalism and gender quotas theory, and applying a
   feminist institutionalist analytical approach, we examine the disparate
   effectiveness of gender quotas in two consociations: Bosnia and
   Herzegovina and North Macedonia. We pair the two countries to evaluate
   why gender quotas have produced different outcomes and demonstrate that
   formal and informal institutions matter for quota effectiveness.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
