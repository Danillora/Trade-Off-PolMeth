@article{ WOS:000985745400001,
Author = {Oseka, Piotr},
Title = {``Secret Services Are Meant To Serve{''}: State Violence in the
   Autobiographic Memory of Secret Police Officers in Communist Poland},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
Year = {2023},
Volume = {37},
