@article{ WOS:000974345700001,
Author = {Casas-Cortes, Maribel and Cobarrubias, Sebastian},
Title = {A corona-carnival? A carnivalesque interpretation of (im)mobilities
   under COVID-19 lockdowns},
Journal = {POLITICS},
