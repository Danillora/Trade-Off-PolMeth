@article{ WOS:001060694800001,
Author = {Brooks, Risa and White, Peter B.},
Title = {The military before the march: Civil-military grand bargains and the
   emergence of nonviolent resistance in autocracies},
Journal = {JOURNAL OF PEACE RESEARCH},
Year = {2024},
Volume = {61},
