@article{ WOS:001029915300001,
Author = {Lee, Jaeyong and Jin, Myung H.},
Title = {Fostering employee innovation: Linking person-organization fit to
   innovative behavior through knowledge sharing and reward perception},
Journal = {PUBLIC ADMINISTRATION},
