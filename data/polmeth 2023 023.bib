@article{ WOS:001069130200001,
Author = {Spaniel, William and Savun, Burcu},
Title = {Less Is More? Shifting Power and Third-Party Military Assistance},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2024},
Volume = {68},
Number = {7-8},
Pages = {1332-1357},
Month = {AUG},
Abstract = {In contexts as diverse as civil war interventions, alliances, and
   military coalitions, states often militarily assist proteges to
   counteract unfavorable power shifts. Existing theoretical work finds
   that such assistance mitigates commitment problems and reduces the
   probability of war. We develop a model that captures this but also
   includes information asymmetries. In many cases, overcoming the
   commitment problem encourages the protege to take greater risks in
   bargaining, thereby increasing the probability of war due to incomplete
   information. Using mechanism design, we show that in some cases, no
   feasible transfer can reduce the probability of war to zero. More
   broadly, our results indicate that encouraging policymakers to further
   expand military assistance can backfire despite empirical relationships
   that may appear otherwise.},
