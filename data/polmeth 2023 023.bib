Author = {Jarvis, Lee and Whiting, Andrew},
Title = {(En)Gendering the Dead Terrorist: (De)Constructing Masculinity in
   Terrorist Media Obituaries},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
