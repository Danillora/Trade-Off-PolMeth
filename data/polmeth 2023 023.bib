@article{ WOS:001068160400002,
Author = {Hagen, Jamie J. and O'Rourke, Catherine},
Title = {Forum-Shifting and Human Rights: Prospects for Queering the Women, Peace
   and Security Agenda},
Journal = {HUMAN RIGHTS QUARTERLY},
