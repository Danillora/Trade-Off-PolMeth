@article{ WOS:000936231300001,
Author = {Hobson, Ronit Berger and Matesan, Ioana Emy},
Title = {The Impact of Diasporas on the Tactics of Rebel Groups: The Case of the
   IRA and Noraid},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2025},
Volume = {48},
Number = {11},
Pages = {1235-1260},
Month = {NOV 2},
Abstract = {Do diasporas affect rebel tactics in the home country? We argue that
   diaspora organizations can influence tactics when rebel leaders are
   divided, and domestic political opportunity structures are restrictive.
   Drawing on a variety of primary sources, we analyze the influence of the
   Irish-American group Noraid on the Provisional IRA tactics during four
   episodes of the Troubles. When the military and political leadership of
   the Republican movement diverged over tactics, Noraid's militant stance
   and support for Gerry Adams incentivized the simultaneous adoption of
   both violence and cooperation. Over time, the group also helped
   consolidate Adams's leadership and overcome an organizational crisis.},
