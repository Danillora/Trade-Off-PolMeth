@article{ WOS:001026943600001,
Author = {Galloway, Ben and Zanocco, Chad and Song, Geoboo and Jones, Michael},
Title = {The effect of policy narratives on policy elite versus public
   preferences for hydraulic fracturing regulation(sic)(sic)(sic)Palabras
   clave},
Journal = {REVIEW OF POLICY RESEARCH},
