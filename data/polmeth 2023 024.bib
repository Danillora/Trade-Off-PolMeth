@article{ WOS:001019649200001,
Author = {Suh, Kyungwon},
Title = {Achieving Minimum Deterrence: A New Dyadic Dataset on Strategic Nuclear
   Weapons Delivery Capabilities},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
