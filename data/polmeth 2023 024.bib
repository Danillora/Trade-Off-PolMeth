ISSN = {0305-5736},
EISSN = {1470-8442},
Keywords = {\&nbsp; policymaking venues; policy advocacy; advocacy tactics; advocacy
   strategies; oil and gas industry; environmental policy; stakeholder
   engagement; Colorado},
Keywords-Plus = {PROCEDURAL JUSTICE; STAKEHOLDER PARTICIPATION; CITIZEN PARTICIPATION;
