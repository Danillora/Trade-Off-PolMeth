@article{ WOS:001023808400001,
Author = {Jeffery, Renee},
Title = {Addressing psychosocial trauma in post-conflict peacebuilding: Emotions
   in narrative and arts-based approaches},
Journal = {COOPERATION AND CONFLICT},
Year = {2024},
Volume = {59},
Number = {2, SI},
Pages = {171-189},
Month = {JUN},
Abstract = {Drawing on contemporary research on the nature and treatment of trauma
   in psychology, neuroscience and the emerging field of art therapy, this
   article adopts a psychosocial approach to examine the role that the arts
   may play in assisting individuals and societies to recover from the
   trauma of violent conflict and contribute to the establishment of
   sustainable peace. Taking a broad understanding of the arts to include a
   range of creative endeavours, it primarily focuses on non-verbal art
   forms, including but not restricted to music, painting, sculpture,
   drawing and dance, as well as traditional arts and crafts. It
   demonstrates that many art forms have the potential to make specific
   contributions to the amelioration of conflict-related trauma by
   addressing pervasive non-verbal memories that typically stand outside
   the remit of more conventional psychosocial practices, such as
   truth-telling and storytelling. They do so, it argues, by providing a
   means of expressing, evoking, regulating and transforming the emotions
   in ways that allow individuals and societies to confront and acknowledge
   their violent pasts, develop supportive relationships in the present and
   draw on their creativity to imagine a better future.},
