@article{ WOS:000975117900001,
Author = {Solodoch, Omer},
Title = {Overburdened? How refugee dispersal policies can mitigate NIMBYism and
   public backlash},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2024},
Volume = {31},
Number = {9},
Pages = {2455-2482},
Month = {SEP 1},
Abstract = {With growing numbers of forcibly displaced people and their tendency to
   spatially cluster, destination countries around the world consider
   dispersing them over their territory. While the egocentric
   not-in-my-back-yard syndrome (NIMBYism) predicts that dispersion will
   spark a public backlash, sociotropic considerations and appeals to civic
   fairness predict the contrary. I theorise that the institutional set-up
   determines which force prevails. Although the local proximity of
   refugees triggers public opposition, it can be substantially countered
   by tighter regulation on refugee dispersion. Setting clear guiding
   rules, such as an upper limit or proportional allocation can enhance
   both burden-sharing in the accommodation of refugees and public support
   for their incorporation. Evidence from survey experiments conducted in
   Norway and Israel supports these theoretical accounts. The findings have
   implications for understanding how countries can mitigate public
   backlash against immigrants and refugees while maintaining their
   admission and integration.},
