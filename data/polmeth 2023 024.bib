@article{ WOS:000982917000001,
Author = {Ryan, Holly Eva and Mazzilli, Caterina},
Title = {Twinning and development: a genealogy of depoliticisation},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
Year = {2023},
Volume = {26},
Number = {3},
Pages = {433-452},
Month = {SEP},
Abstract = {One of the latest methods being trialled across the development sector
   to help advance progress towards achievement of the Sustainable
   Development Goals (SDGs) is `twinning'. In this equation, twinning is
   rendered as a broadly replicable methodology for improving development
   outcomes, with a particular emphasis on building up human resources and
   technical capacity within governments and national bureaucracies. It is
   time-bound, target driven and depoliticised. However, the relationship
   between twinning and development has not always looked this way. Our
   paper uses a genealogical approach to unpack and illuminate the
   historical circumstances and politico-economic conditions under which
   these discourses have previously converged. It documents the gradual
   historical trajectory of the phenomenon of twinning from an overt
   political act to a largely apolitical tool of development practitioners.
   In so doing, it denaturalises the status quo and prompts reflection on
   alternative pathways, politics and practices of development.},
