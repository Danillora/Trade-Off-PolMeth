@article{ WOS:001066012600001,
Author = {Nantermoz, Olivia and Turan, Aslihan},
Title = {Humanitarian logics in the evolution of international society},
Journal = {INTERNATIONAL POLITICS},
Year = {2025},
Volume = {62},
