@article{ WOS:001111919800001,
Author = {Youe, Chris},
Title = {THE ROYAL AFRICAN SOCIETY, AFRICAN AFFAIRS AND APARTHEID: THE MUSTOE
   CONTROVERSY OF 1970},
Journal = {AFRICAN AFFAIRS},
Year = {2023},
