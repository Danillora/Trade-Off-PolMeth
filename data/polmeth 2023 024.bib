@article{ WOS:001103000400009,
Author = {Lameck, Wilfred Uronu},
Title = {Political decentralisation and political-administrative relation in the
   local councils in Tanzania},
Journal = {PUBLIC ADMINISTRATION AND POLICY-AN ASIA-PACIFIC JOURNAL},
