@article{ WOS:001150762300001,
Author = {Chalmers, Adam and Klingler-Vidra, Robyn and van den Broek, Onna Malou},
Title = {From Diffusion to Diffuse-ability: A Text-as-Data Approach to Explaining
   the Global Diffusion of Corporate Sustainability Policy},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
Year = {2023},
Volume = {68},
Number = {1},
Month = {DEC 28},
Abstract = {This paper argues that attributes of diffusion objects, in their own
   right, shape the form and extent of policy diffusion. To date, diffusion
   scholarship focuses on actor-level attributes (e.g., connections,
   culture, physical proximity, etc.) to explain what is diffused and how
   much. Extending existing theory on the impacts of policies' textual
   properties on diffusion patterns, we argue that policies that are easier
   to understand, specific in their applicability, and that do not mandate
   specific behavior have their text diffused with less adaptation,
   regardless of the attributes of the authoring organization. We test our
   argument in the context of the global diffusion of corporate
   sustainability policy (CSP), analyzing a novel dataset of 1,429 CSPs
   from 100 countries, 20 international organizations, and 12 regional
   organizations over a 65-year period. Offering a precise measure of
   diffusion as the extent to which a source text is copied into an adopter
   text, we find statistical support for our hypothesis. We contribute to
   diffusion scholarship by helping to mainstream natural language
   processing methods and by theorizing how attributes of policy documents
   affect how much adaptation occurs.
   Este articulo argumenta que lo que determina la forma y el alcance de la
   difusion de las politicas son, por derecho propio, los atributos de los
   objetos de difusion. Hasta la fecha, los estudios de difusion se centran
   en los atributos a nivel de cada agente (por ejemplo, conexiones,
   cultura, proximidad fisica, etc.) con el fin de explicar que es lo que
   se difunde y cuanto. Extendemos la teoria existente sobre los impactos
   de las propiedades textuales de las politicas en los patrones de
   difusion con el fin de argumentar que la difusion de los textos de las
   politicas que son mas faciles de entender, que son especificas en su
   aplicacion y que no exigen un comportamiento especifico, se difunden con
   un menor grado de adaptacion, independientemente de los atributos de la
   organizacion autora. Demostramos este argumento en el contexto de la
   difusion global de la Politica de Sostenibilidad Corporativa (CSP, por
   sus siglas en ingles), analizando un nuevo conjunto de datos procedente
   de 1429 CSP de 100 paises, 20 organizaciones internacionales y 12
   organizaciones regionales durante un periodo de 65 anos. Encontramos
   apoyo estadistico para nuestra hipotesis, a traves de una medida precisa
   de difusion como es el grado en que un texto fuente se copia en un texto
   adoptante. Contribuimos a los estudios sobre difusion ayudando a
   incorporar los metodos de procesamiento del lenguaje natural y
   teorizando como los atributos de los documentos de las politicas afectan
   al grado de adaptacion que se lleva a cabo.
   Cet article affirme que les attributs des objets de diffusion en
   eux-memes faconnent la forme et la portee de la diffusion politique. A
   ce jour, la recherche sur la diffusion se concentre sur les attributs de
   l'acteur (par ex., connexions, culture, proximite physique, etc.) pour
   expliquer ce qui est diffuse et dans quelle mesure. Nous elargissons la
   theorie existante sur les consequences des proprietes textuelles des
   politiques sur les schemas de diffusion pour affirmer que les politiques
   qui sont les plus faciles a comprendre, a l'applicabilite specifique et
   qui ne requierent pas de comportement particulier voient leur texte
   diffuse avec moins d'adaptation, independamment des attributs de
   l'organisation qui les diffuse. Nous evaluons notre argument dans le
   contexte de la diffusion mondiale de la politique de developpement
   durable d'entreprise (PDDE), en analysant un ensemble de donnees inedit
   de 1 429 PDDE issues de 100 pays, 20 organisations internationales et 12
   organisations regionales sur 65 ans. En proposant la mesure dans
   laquelle un texte source est copie dans le texte adopte comme indicateur
   precis de diffusion, nous trouvons des elements statistiques qui etayent
   notre hypothese. Nous contribuons a la recherche sur la diffusion en
   participant a la generalisation des methodes de traitement du langage
   naturel et en theorisant les effets des attributs des documents
   politiques sur la portee de l'adaptation.},
Publisher = {OXFORD UNIV PRESS},
Address = {GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND},
Type = {Article},
Language = {English},
