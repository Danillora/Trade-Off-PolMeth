@article{ WOS:001002155300001,
Author = {Seitzer, Helen and Baek, Chanwoong and Steiner-Khamsi, Gita},
Title = {Instruments of lesson-drawing: comparing the knowledge brokerage of the
   OECD and the World Bank},
Journal = {POLICY STUDIES},
