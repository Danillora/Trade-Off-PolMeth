@article{ WOS:001022594800001,
Author = {Stavro, Martin and Welch, Ryan M.},
Title = {Does Police Militarization Increase Repression?},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2024},
Volume = {68},
Number = {5},
Pages = {964-992},
Month = {MAY},
Abstract = {Does police militarization influence government use of repression? While
   comparative work examines police militarization, violence, human rights
   abuses, and torture, it does not integrate militarized police within
   existing theories of repression. Connecting police militarization and
   repression, this article argues that police militarization increases the
   police's perception of threat as well as their coercive capacity,
   thereby increasing their willingness to repress. As such, we hypothesize
   police militarization increases repression. To evaluate the theory, we
   conduct an international analysis using existing datasets covering 102
   countries from 1994 to 2010. Employing several statistical models, the
   data supports the conclusion that police militarization increases the
   likelihood of government repression, specifically through extrajudicial
   killing and torture. Aside from highlighting a consequence of police
   militarization policy, the findings point to police militarization as an
   important omitted variable in cross-national models of repression and
   human rights.},
