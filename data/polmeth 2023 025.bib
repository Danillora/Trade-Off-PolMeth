@article{ WOS:001107293400004,
Author = {Lagunes-Gomez, Isabel and Hernandez-Orduna, Maria Graciela and
   Martinez-Escalante, Gustavo Alonso and Hernandez-Pitalua, Daniel},
Title = {Performance Evaluation of Institutional Capacity Indices in
   Municipalities of the State of Veracruz, Mexico},
