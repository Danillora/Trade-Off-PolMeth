@article{ WOS:000970332200001,
Author = {Schindler, Sebastian},
Title = {Post-truth politics and neoliberal competition: the social sources of
   dogmatic cynicism},
Journal = {INTERNATIONAL THEORY},
Year = {2024},
Volume = {16},
Number = {1},
Pages = {102-121},
Month = {MAR},
Abstract = {From Trump's America to Putin's Russia, from climate change denial to
   corona denial, so-called post-truth politics are experiencing a global
   rise. How can we understand and explain this phenomenon? In the attempt
   to answer this question, this article advances two core claims. First,
   it suggests that post-truth politics is (despite its name) marked not
   only by the denial of claims to objective truth, but also by the
   naturalization of one specific truth claim: namely, the cynical belief
   that self-interests are behind all public discourse. Second, it locates
   the social sources of this dogmatic cynicism in the global expansion of
   neoliberal competition.},
