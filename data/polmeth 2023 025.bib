Type = {Article},
Language = {English},
Affiliation = {Szulecka, B (Corresponding Author), Polish Acad Sci, Inst Philosophy \& Sociol, Warsaw, Poland.
   Szulecka, Beata, Polish Acad Sci, Inst Philosophy \& Sociol, Warsaw, Poland.},
DOI = {10.1177/08883254221147544},
EarlyAccessDate = {MAR 2023},
