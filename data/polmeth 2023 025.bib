Title = {Explaining Ukraine's resilience to Russia's invasion: The role of local
   governance},
Journal = {GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS},
Year = {2024},
Volume = {37},
