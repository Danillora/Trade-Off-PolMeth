@article{ WOS:001424810500002,
Author = {Rangoni, Bernardo},
Title = {The Experimentalist Future of Regulation: Digital and Pharmaceutical
   Regulation in the European Union},
Journal = {RIVISTA ITALIANA DI POLITICHE PUBBLICHE},
Year = {2023},
Number = {1},
Pages = {67-96},
Month = {APR},
Abstract = {For decades, strategies granting discretion to `lower-level actors' to
   propose solutions themselves had become preferred to conventional
   command and control - the hierarchical imposition and enforcement of
   stable rules. Yet after the 2008 financial crisis, also these
   alternatives (e.g., principles-based regulation) are under pressure.
   Which strategy prevails today, and why? By comparing two crucial yet
   different cases, the article finds that, in both EU digital and
   pharmaceutical regulation, the dominant strategy is experimentalist.
   This emphasises not only discretion but also regular revisions based on
   review of implementation experience. It is driven by uncertainty about
   the best solutions, as perceived by the relevant actors. Since
   uncertainty pervades most contemporary policy challenges, the future of
   regulation and public policy more broadly is thus likely to be
   experimentalist.},
