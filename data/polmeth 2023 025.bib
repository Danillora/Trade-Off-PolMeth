@article{ WOS:000998301100001,
Author = {Sankaran, Jaganath},
Title = {State or soldier? Explaining China's decisionmaking in India-China
   border crises},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2023},
Volume = {46},
Number = {6-7},
Pages = {1215-1244},
Month = {NOV 10},
Abstract = {Prevailing explanations for India-China border crises rely on rational
   actor models and assumes centralized decisionmaking. Instead, I argue
   that the several recent India-China border crises cannot be understood
   without separately interrogating the role of the PLA in China's national
   security policymaking. I argue that the PLA uses its operational
   independence to continuously challenge Indian forces on the contested
   border. The Indian forces, when confronted, pushed back. The several
   recent border standoffs are a manifestation of the interactions between
   the two armed forces.},
