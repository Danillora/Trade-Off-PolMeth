@article{ WOS:001114745600003,
Author = {Schunz, Simon},
Title = {The European Union's roles in global atmospheric governance: evolving
   towards a `spacifier'?},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2023},
Volume = {45},
Number = {8, SI},
Pages = {1161-1180},
Month = {NOV 17},
Abstract = {This contribution examines how the roles of the European Union (EU) in
   the governance of the atmosphere - the global ozone and climate regimes
   - as a contested Global Space have evolved over time. To do so, it first
   combines theoretical considerations regarding foreign policy roles and
   global justice. It then traces EU role conceptions and role performance
   in these regimes from their inception until today. The article argues
   that, as the two regimes have matured, the EU has increasingly been
   adopting the role of a `spacifier', pursuing a substantive justice
   position dedicated to fully protecting the atmosphere ('leaving the
   space in peace') while actively striving to reconcile other actors'
   positions. This role embodies the EU's simultaneous quest for embracing
   the European Green Deal's socio-ecological transformation agenda while
   becoming a geopolitical actor. It can be interpreted as an expression of
   `principled pragmatism' under the conditions of the Anthropocene.},
