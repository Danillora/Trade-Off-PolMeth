@article{ WOS:001107293400012,
Author = {Sindleryova, Ivana Butoracova and Cibik, Lukas and Turcan, Kamil},
Title = {Effects of Indebtedness of Self-governing Regions: Comparison Between
   Selected Central European Countries},
