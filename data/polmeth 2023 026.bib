@article{ WOS:000980754100007,
Author = {Costa, Oriol and Barbe, Esther},
Title = {A moving target. EU actorness and the Russian invasion of Ukraine},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2023},
Volume = {45},
Number = {3, SI},
Pages = {431-446},
Month = {APR 3},
Abstract = {The war in Ukraine sends mixed signals about the capacity of the EU to
   be a relevant actor. Despite steps forward over defense, strategic
   autonomy has been seen as a `pipe dream' that has encountered a `reality
   check'. Key member states are in a similar predicament. Despite talk of
   a Zeitenwende, Germany has been dimed a `reluctant giant'. France has
   allegedly seen discourse on European sovereignty vindicated, but at the
   same time has managed to alienate a few EU countries. We interpret this
   ambivalence as an effect of the fragmentation of the liberal
   international order, accelerated by war in Ukraine, and claim that this
   process is increasing the requirements for EU actorness. We then
   identify a range of reactions to such situation. We map them and
   leverage the mapping to offer a research agenda on the politics of EU
   foreign policy.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
