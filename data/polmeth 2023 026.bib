@article{ WOS:001109600900001,
Author = {Sahoo, Rojalin and Sahoo, Chandan Kumar},
Title = {Bolstering employee relations in a public sector undertaking: validation
   of CODE and PLE model},
Journal = {INTERNATIONAL JOURNAL OF CONFLICT MANAGEMENT},
