@article{ WOS:001071542900001,
Author = {Krumbein, Frederic},
Title = {Leaving the dragon's shadow - Normative Power Europe and the emergence
   of a Taiwan policy in the EU?},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2024},
Volume = {46},
Number = {2},
Pages = {171-190},
Month = {FEB 17},
Abstract = {For a long time, Taiwan was confined to being an object of the EU's
   China policy, but the EU increasingly treats Taiwan as a partner in its
   own right. EU-Taiwan relations are in their most dynamic stage since the
   EU opened its representative office in Taipei in 2003. But the `One
   China policy' still constrains EU-Taiwan relations, as the EU only
   recognizes the People's Republic of China as a sovereign state and not
   Taiwan. The article analyzes EU-Taiwan relations through the lens of
   Normative Power Europe. Taiwan has gained more attention and support in
   the EU, because Taiwan is Asia's most liberal democracy. Furthermore,
   Taiwan also fits well into the EU's geo-economic strategy of de-risking.
   China's increasingly aggressive policy towards Taiwan and strong support
   from the United States for Taiwan strengthen the EU's support for
   Taiwan, too.},
