@article{ WOS:001031673300003,
Author = {Carnegie, Allison and Clark, Richard},
Title = {REFORMING GLOBAL GOVERNANCE Power, Alliance, and Institutional
   Performance},
Journal = {WORLD POLITICS},
Year = {2023},
