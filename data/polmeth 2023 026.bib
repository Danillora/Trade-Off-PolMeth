@article{ WOS:000979819300001,
Author = {Wang, Xin-Yi and Chen, Bo},
Title = {World Power Trends and International Relations: Measuring Power with an
   Entropy-QAP Approach},
Journal = {JOURNAL OF CHINESE POLITICAL SCIENCE},
