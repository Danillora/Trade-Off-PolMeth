@article{ WOS:000994719100001,
Author = {Smith, Martin A.},
Title = {Always part of a vision? The US and the Israel-Palestinian dispute under
   George W. Bush and after},
Journal = {INTERNATIONAL POLITICS},
Year = {2024},
Volume = {61},
Number = {3, SI},
Pages = {637-659},
Month = {JUN},
Abstract = {Having inherited an Israel-Palestinian peace process in poor shape in
   January 2001, George W. Bush did little to advance it during his first
   term. His approach was motivated by political and diplomatic expediency,
   rather than deep-seated convictions or principles. 9/11 did not have a
   decisive impact on US policy. During his second term, however, the
   president authorised a new formal negotiating process. Although this has
   received little scholarly attention hitherto, it remains the last
   serious American effort to broker Israeli-Palestinian peace. Barack
   Obama made no attempt to continue from where Bush left off in early
   2009. Furthermore, Obama's preferred approach backfired, to the extent
   that serious US engagement in Israeli-Palestinian dialogue was
   effectively over before the end of his first term. This facilitated the
   approach subsequently pursued by President Donald Trump, who downgraded
   and side-lined the Israel-Palestinian dispute in favour of other
   regional priorities in the Middle East.},
