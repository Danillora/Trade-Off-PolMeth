@article{ WOS:001022047600001,
Author = {Biskupska, Kamilla},
Title = {The Imagined City: Great Flood of 1997 as a Foundation Story of Wroclaw},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
