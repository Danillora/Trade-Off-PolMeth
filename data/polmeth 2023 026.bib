@article{ WOS:001067677400001,
Author = {Shang, Lijun and Sheff, Lincoln and Dando, Malcolm},
Title = {A key role for scientists in strengthening the Biological Weapons
   Convention},
Journal = {FRONTIERS IN POLITICAL SCIENCE},
Year = {2023},
Volume = {5},
Month = {SEP 7},
Abstract = {It is not easy to be optimistic about international security at the
   present time, but 9th Review Conference of the Biological Weapons
   Convention in December 2022 agreed a renewed effort to strengthen the
