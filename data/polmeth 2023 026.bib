Address = {GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Blair, CW (Corresponding Author), Princeton Univ, Dept Polit, Polit, Princeton, NJ 08544 USA.
