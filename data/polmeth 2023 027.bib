@incollection{ WOS:001172351200007,
Author = {Reger, Jo},
Editor = {Maher, TV and Schoon, EW},
Title = {MOMENTS OF INTERROGATION: DOING FEMINIST ETHNOGRAPHY IN THE ARCHIVES},
Booktitle = {METHODOLOGICAL ADVANCES IN RESEARCH ON SOCIAL MOVEMENTS, CONFLICT, AND
   CHANGE},
Series = {Research in Social Movements Conflicts and Change Series},
Year = {2023},
Volume = {47},
Pages = {121-142},
Abstract = {In this chapter, I address the issues in doing a feminist ethnography
   based on the archives of a lesbian feminist community structured around
   music. In doing so, I grapple with questions central to archival
   research with marginalized social movement communities. I pose these
   questions as moments of interrogation where I draw on personal
   experience as well as literature on archival research to create a
   framework aimed at social movement researchers who are considering or
   doing archival work. These interrogations cover three broad areas and
   apply to different moments in the research process. First, what are the
   origin stories of social movement archives? Second, how can researchers
   construct the stories of marginalized and vulnerable communities? Third,
   how can we access the voices of community members found in the archives?
   To answer these questions, I identify how conceptual tools such as
   identifying community boundaries and documenting types of interaction
   can aid a scholar in the research process.},
