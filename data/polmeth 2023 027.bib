@article{ WOS:001040998800002,
Author = {Vartasova, Anna and Cervena, Karolina},
Title = {Position of the Local Development Fee in the System of Local Taxes and
   Fees in the Slovak Republic},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
