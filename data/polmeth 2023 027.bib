@article{ WOS:000975456200001,
Author = {Hillier-Smith, Bradley},
Title = {Does a State's Right to Control Borders Justify Harming Refugees?},
Journal = {MORAL PHILOSOPHY AND POLITICS},
Year = {2024},
Volume = {11},
Number = {1},
Pages = {195-226},
Month = {APR 25},
Abstract = {Certain states in the Global North have responded to refugees seeking
   safety on their territories through harmful practices of border
   violence, detention, encampment and containment that serve to prevent
   and deter refugee arrivals. These practices are ostensibly justified
   through an appeal to a right to control borders. This paper therefore
   assesses whether these harmful practices can indeed be morally justified
   by a state's right to control borders. It analyses whether Christopher
   Heath Wellman's account of a state's right to freedom of association,
   which represents the most restrictive account of a state's right to
   control borders available in the literature, can extend to justify
   current harmful practices against refugees. If not, then no available
   justification will be able to do so, and thus contemporary harmful
   practices used against refugees cannot be justified by a state's right
   to control borders.},
