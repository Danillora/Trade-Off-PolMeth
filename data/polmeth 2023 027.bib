@article{ WOS:000945534100001,
Author = {Garlick, Jeremy and Qin, Fangxing},
Title = {China's Normative Power in Central and Eastern Europe: `16/17+1'
   Cooperation as a Tale of Unfulfilled Expectations},
Journal = {EUROPE-ASIA STUDIES},
Year = {2023},
Volume = {75},
Number = {4},
Pages = {583-605},
Month = {APR 21},
Abstract = {Normative power China (NPC) has characteristics distinct from Manners'
   conception of normative power Europe (NPE). While NPE attempts to
   establish rules for interaction, NPC introduces practices to be
   co-constituted via regional platforms through a process of `earned
   recognition'. In Central and Eastern Europe (CEE), NPC's regionalising
   `group cooperation diplomacy' has taken the form of the `16/17 + 1'
   cooperation framework. Using normative power theory, the article
   assesses, via a critical discourse analysis of speeches and interviews,
   how and why China's attempts to shape practices, earn recognition and
   create a community of practice in CEE have met with-at best-only limited
   success.},
