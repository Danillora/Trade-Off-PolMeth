@article{ WOS:000980898800007,
Author = {Turaga, Rama Mohana R. and Mittal, Harsh},
Title = {The policy process of adopting environmental standards for coal plants
   in India: accommodating transnational politics in the Multiple Streams
   Framework},
Journal = {POLICY AND POLITICS},
