@article{ WOS:000946084700001,
Author = {Ordonez-Vargas, Laura and Gonzalez, L. C. Peralta and Prieto-Rios,
   Enrique},
Title = {An Econcentric Turn in the Transitional Restorative Justice Process in
   Colombia},
Journal = {INTERNATIONAL JOURNAL OF TRANSITIONAL JUSTICE},
Year = {2023},
Volume = {17},
Number = {1},
Pages = {107-122},
Month = {MAY 22},
Abstract = {This article reflects on how Colombia, as an important laboratory in
   transitional restorative justice, a 60-year long internal conflict, is
   experiencing an `ecocentric turn.' This transition is not free from
   contradiction, ambivalence or great challenges. For this article,
   `ecocentric turn' means an epistemological movement from
   anthropocentrism to ecocentrism, with biocentrism as the middle stage
   between the two ends. We argue that the ecocentric turn does not
   exclusively imply postures that are purely anthropocentric or
   ecocentric, but also ones that are hybrid and eclectic, which for the
   purposes of this article will be called biocentric positions. The
   ecocentric turn is reviewed on two levels: the first is the
   institutional level, focusing on the Special Jurisdiction for Peace (JEP
   for its name in Spanish); and the second level corresponds to the
   experience of Palestina, Huila, a non-ethnic campesino community.},
