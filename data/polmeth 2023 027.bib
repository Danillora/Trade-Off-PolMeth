@article{ WOS:000979770100001,
Author = {Pomeroy, Caleb and Rathbun, Brian C.},
Title = {Just business? Moral condemnation and virtuous violence in the American
   and Russian mass publics},
Journal = {JOURNAL OF PEACE RESEARCH},
