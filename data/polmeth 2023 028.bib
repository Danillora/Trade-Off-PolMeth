@article{ WOS:001129468200001,
Author = {Depoorter, Charline and Marx, Axel},
Title = {Fostering compliance with voluntary sustainability standards through
   institutional design: An analytic framework and empirical application},
Journal = {REGULATION \& GOVERNANCE},
