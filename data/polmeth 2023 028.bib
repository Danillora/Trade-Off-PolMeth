@article{ WOS:001104431800001,
Author = {Levine, Robert},
Title = {Probability or confidence, a distinction without a difference?},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2024},
Volume = {39},
Number = {4},
Pages = {729-741},
Month = {JUN 6},
Abstract = {How analytic uncertainty is presented can affect critical policymaker
   decisions. Analysts are told to offer separate: (1) assessments of the
   likelihood an analytic statement is true and (2) confidence statements
   that express how strongly analysts believe their assessments. The
   distinction between likelihood and confidence sounds clear but is not.
   The IC defines confidence poorly. Most intelligence products fail to
   distinguish likelihood from confidence and use the terms interchangeably
   to mean probability. The IC should abandon its confusing shorthand
   confidence descriptions. Instead, analysts should provide explanations
   of the nature and strength of evidence, and the reasoning behind
   analytic judgments.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
