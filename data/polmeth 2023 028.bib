@article{ WOS:000978535500001,
Author = {Mertens, Charlotte},
Title = {In the ruins of empire: historicizing sexual violence in Congo},
Journal = {INTERNATIONAL FEMINIST JOURNAL OF POLITICS},
Year = {2023},
Volume = {25},
Number = {3},
Pages = {529-550},
Month = {MAY 27},
Abstract = {Sexual violence in the conflict in eastern Democratic Republic of Congo
   is predominantly understood and represented as a ``native{''} phenomenon
   - a direct consequence of the economic civil war - unrelated to colonial
   violence. This article offers a historicized perspective on Congo's
   enduring conflict in which sexual violence continues to ruin lives. I
   argue that sexual violence does not follow but structures colonialism as
   part of a continuum of violence. Drawing on extensive archival research,
   I analyze patterns of everyday gender-based violence in the Congo Free
   State (1885-1908) and identify the fusion of terror and pleasure as key
   aspects of a capitalist and patriarchal gender order. Staging an
   encounter between historical and contemporary dynamics of sexual
   violence in Congo, this article addresses and reconceptualizes
   contemporary sexual violence in relation to past colonial violence.},
