@article{ WOS:001107158000001,
Author = {Eberlee, Sam},
Title = {Scholar, diplomat, Intelligence pioneer: Herbert Norman and Canada's
   Special Intelligence Section, 1942-1945},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
