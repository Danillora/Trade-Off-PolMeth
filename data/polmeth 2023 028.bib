@article{ WOS:001111291000001,
Author = {Tamo-Larrieux, Aurelia and Guitton, Clement and Mayer, Simon and Lutz,
   Christoph},
Title = {Regulating for trust: Can law establish trust in artificial
   intelligence?},
Journal = {REGULATION \& GOVERNANCE},
Year = {2024},
