@article{ WOS:001003692900001,
Author = {Lundgren, Magnus and Squatrito, Theresa and Sommerer, Thomas and
   Tallberg, Jonas},
Title = {Introducing the Intergovernmental Policy Output Dataset (IPOD)},
Journal = {REVIEW OF INTERNATIONAL ORGANIZATIONS},
