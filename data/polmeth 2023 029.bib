@article{ WOS:001153792300007,
Author = {Houghton, Ruth and O'Donoghue, Aoife},
Title = {Introduction to special issue: Feminist manifestos and global
   constitutionalism},
Journal = {GLOBAL CONSTITUTIONALISM},
Year = {2023},
Volume = {12},
