@article{ WOS:001105728700001,
Author = {Buck, Andrew},
Title = {Critical Letter Writing and Deliberation During the Brezhnev Era},
Journal = {EUROPE-ASIA STUDIES},
Year = {2024},
Volume = {76},
Number = {4},
Pages = {590-612},
Month = {APR 20},
Abstract = {This article analyses letters sent by Soviet citizens to newspapers and
   authorities during the Brezhnev era, supposedly a period of political
   stagnation. Citizens wrote critical letters, individually and
   collectively, signed and anonymously, to have problems solved.
   Authorities responded to citizen concerns, which were mostly very
   specific and non-systemic, and also used the letters to monitor public
   opinion. The practice of letter writing in the Soviet Union in this
   period enabled citizens to hold authorities to account while also
   providing leaders with the opportunity to legitimise their authority by
   taking action.},
