@incollection{ WOS:001116896100017,
Author = {Langill, Jennifer C.},
Editor = {Brunn, SD and Kehrein, R},
Title = {The Nature of Gender Relations: The Changing Language of Gender amid
   Anthropogenic Climate Change in Amazonian Peru},
Booktitle = {LANGUAGE, SOCIETY AND THE STATE IN A CHANGING WORLD},
Year = {2023},
Pages = {385-404},
Abstract = {Gender is one of the most defining factors of human interaction
   worldwide. Gender roles, identities, and relations represent fundamental
   spoken and unspoken means of communication. Feminist scholars have
   illustrated how human-environment relationships and rural livelihoods
   are highly gendered as well, shaping who has access to and who is
   responsible for which livelihood activities. Intra-household division of
   labor is often divided along gender lines as well as by age. However, on
   a global scale, anthropogenic climate change is rapidly redefining
   human-environment relationships as both natural resources and human
   societies are greatly impacted by a changing climate, environmental
   hazards, and greater environmental unpredictability. Western Amazonia is
   projected to experience many of these outcomes, particularly changes to
   the annual flood cycle, which is integral to floodplain livelihoods for
   a large portion of the rural population. This chapter integrates
   feminist approaches to understanding gendered rural livelihoods with
   environmental science regarding climate change in the Peruvian Amazon to
   question the intimate and everyday impacts of anthropogenic climate
   change. Attention to gendered livelihoods, gendered ecological
   knowledge, and gendered human-environmental relations illustrates how
   the certainty of anthropogenic climate change causing disruption in
   rural Amazonia is coupled with many everyday unknowns of how these
   impacts will redefine the language of gender roles, gender relations,
   and household division of labor.},
