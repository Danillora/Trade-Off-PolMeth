@article{ WOS:001578911600006,
Author = {Prieto, Antonio David Berning},
Title = {The use of systems based on artificial intelligence by public
   administrations: current situation and some ad futurum proposals for
   responsible use},
Journal = {REVISTA DE ESTUDIOS DE ADMINISTRACION LOCAL Y AUTONOMICA},
Year = {2023},
Number = {20},
Month = {OCT},
Abstract = {The use by the central, regional and local administration of systems
   based on artificial intelligence represents a clear advance and multiple
   benefits related to the effectiveness and efficiency in its operation,
   but it is accompanied by multiple risks that must be managed from the
   moment the decision is made. decision about its development or
   acquisition, until its commissioning and during its life cycle. Given
   that there is currently no adequate regulation that provides legal
   certainty and guarantees the rights of citizens when decisions that
   affect them are made, this paper aims to address certain issues that
   must be regulated in the future so that public administrations can use
   this type of system with the proper guarantees.},
