@article{ WOS:001041392800001,
Author = {Walsh, Patrick F. and Ramsay, James and Bernot, Ausma},
Title = {Health security intelligence capabilities post COVID-19: resisting the
   passive ``new normal{''} within the Five Eyes},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2023},
Volume = {38},
