@article{ WOS:000961870600001,
Author = {Economides, Spyros and Featherstone, Kevin and Hunter, Tom},
Title = {The Changing Discourses of EU Enlargement: A Longitudinal Analysis of
   National Parliamentary Debates},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
Year = {2024},
Volume = {62},
Number = {1},
Pages = {168-185},
Month = {JAN},
Abstract = {Domestic support for EU enlargement across existing member states is
   understood to have shifted significantly since 2004. But, how? Here, we
   explore how enlargement has been framed discursively in the national
   parliaments of eight member states. Our dataset comprises over 18,000
   statements from 1989 to 2019. We link our exploration to alternative
   explanations for change: IR-based shifts in how applicant states are
   perceived and, from comparative politics, the `politicisation' of EU
   membership in domestic party systems. We analyse changes in salience,
   sentiment and thematic foci. Our results confirm shifts in the
   perceptions of individual applicant states over time and an increased
   concern with issues of `identity' in relation to enlargement. We find
   that whilst salience has declined, sentiment has turned negative and
   explore the distinctive discourses of `challenger parties' in this
   regard.},
