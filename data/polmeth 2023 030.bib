@article{ WOS:000983603300001,
Author = {Marchio, James},
Title = {`Making the intelligence product of greater use to those for whom it is
   produced': lessons from the National Security Council Intelligence
   Committee, 1971-1976},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2023},
Volume = {38},
Number = {6},
Pages = {853-871},
Month = {SEP 19},
Abstract = {This article examines the role played by the National Security Council
   Intelligence Committee (NSCIC) and its working group in responding to
   senior policymaker criticism of IC analytic products. Established in
   December 1971 and abolished four years later, the NSCIC has received
   scant attention from intelligence historians despite representing in
   some ways the most ambitious initiative ever attempted to involve
   consumers in determining what intelligence was produced and evaluating
   its quality and usefulness. The NSCIC's problematic history highlights
   the challenges still confronting the IC and the study of it may suggest
   ways to make future intelligence products more useful to consumers.},
