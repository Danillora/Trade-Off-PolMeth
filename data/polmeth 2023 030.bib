@article{ WOS:001130515100002,
Author = {Doromal, Justin B. and Michie, Molly and Kegley, Grace and Bassok,
   Daphna},
Title = {Reducing Complexity to Support Families Navigating Early Care and
   Education Systems},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
