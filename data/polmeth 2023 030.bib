@article{ WOS:001112756000001,
Author = {Koeker, Philipp and Weiher, Nele and Schollmeyer, Anja},
Title = {The Gender Gap in Civil State Decorations: A Comparative Study of the
   Baltic States, 1994-2020},
Journal = {POLITICS \& GENDER},
Year = {2024},
Volume = {20},
Number = {2},
Pages = {363-390},
Month = {JUN},
Abstract = {Systems of state decorations have often been overlooked by political
   scientists. However, they are highly indicative of dominant social norms
   and power differentials. While historical research has highlighted
   gender disparities in award bestowals in individual countries,
   comparative perspectives and cross-national analyses are still missing.
   This article provides the most comprehensive comparative analysis of the
   gender gap in state decorations. Using an original data set of all
   11,559 recipients of civil awards in Estonia, Latvia, and Lithuania from
   1994 to 2020, it shows that women received significantly fewer awards
   than men across the three countries, with only moderate progress over
   time. Even where women and men were recognized in equal numbers, women
   remained underrepresented among higher classes of awards and were more
   likely to be recognized for achievements in stereotypically feminine
   fields. Our findings contribute to research on gendered institutions and
   highlight the usefulness of award bestowals as an indicator of
   sociopolitical phenomena.},
