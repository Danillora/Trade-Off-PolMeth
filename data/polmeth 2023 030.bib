@article{ WOS:001330570000003,
Author = {Alao, Temi},
Title = {Diasporic Consciousness in African Immigrants' Support for
   \#BlackLivesMatter},
Journal = {JOURNAL OF RACE ETHNICITY AND POLITICS},
