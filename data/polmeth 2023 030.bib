@article{ WOS:000946777500004,
Author = {Williams, Brad},
Title = {Why the Five Eyes? Power and Identity in the Formation of a Multilateral
   Intelligence Grouping},
Journal = {JOURNAL OF COLD WAR STUDIES},
Year = {2023},
Volume = {25},
Number = {1},
Pages = {101-137},
Month = {MAR 3},
Abstract = {Adopting an analytically eclectic approach that draws on theories of
   realist bargaining, identity, and socialization, this article
   investigates the early Cold War origins of the Five Eyes intelligence
   grouping (the United States, Canada, Great Britain, Australia, and New
   Zealand). An understanding of identity grounded in culture suggests a
   natural process of international intelligence community building, but
   this was not the case with the Five Eyes. The formation of the grouping
