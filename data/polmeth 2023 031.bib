@article{ WOS:001245323300011,
Author = {Scharrer, Tabea},
Title = {Of Mukhalas and Magafe: Somali Migrants Navigating the
   Dangers of Ransom Smuggling in Northern Africa},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2023},
Volume = {709},
Number = {1, SI},
Pages = {86-104},
Month = {SEP},
Abstract = {In the late 2000s, after the implementation of European policies to
   prevent and criminalize irregular migration, a new form of human
   smuggling-ransom smuggling-appeared in North Africa. This article is an
   analysis of ransom smuggling of Somali migrants, based on
   anthropological fieldwork in Germany and Kenya. Ransom smuggling is
   dangerous and complex, involving both human smugglers and hostage
   takers, but I argue that Somali migrants-especially younger ones-make
   use of this form of smuggling despite its risks because it is cheaper
   and easier to organize than other options. I describe the major elements
   of Somali ransom smuggling, which involves transit through the Sahara
   and Libya. I focus on the various actors involved, and the
   responsibilities and burdens that accrue to the migrants who transit a
   system that accounts for their lives as commodities for trade.},
