@article{ WOS:000998770700001,
Author = {Brattvoll, Joakim and Castro, Vic},
Title = {How intelligence tales are made real: Le Bureau des legendes as a
   cover story for the French DGSE},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2023},
Volume = {38},
Number = {7},
Pages = {1112-1126},
Month = {NOV 10},
Abstract = {The French spy series Le Bureau des legendes (2015-2020) has been
   acclaimed for its allegedly realistic depiction of French foreign
   intelligence. Drawing on the concept of `legend', this article adapts
   actor-network theory to understand how Le Bureau was able to make a
   significant impact on public discussions of secret intelligence in
   France. The article shows how the series, was constituted as a
   `virtually true' cover story for the Direction generale de la securite
   exterieure (DGSE), which supported the series' production, and how this
   story impacted the DGSE itself.},
