@article{ WOS:001069896500001,
Author = {Holmsten, Stephanie and Hughes, Melanie M. and Moser, Robert},
Title = {Invisibility or Inclusion? Ethnic Parties, Ethnic Seats, and Gender
   Quotas and the Representation of Minoritized Women},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
Year = {2024},
Volume = {45},
Number = {2},
Pages = {191-212},
Month = {APR 2},
Abstract = {This article seeks to understand the circumstances under which
   minoritized women are descriptively represented. Drawing from a unique
   dataset of 7,978 legislators in 37 countries, we conduct the first
   cross-national examination of minoritized women's representation at the
   level of individual legislators. We find that gender quotas, ethnic
   parties, and ethnic seats are effective at enhancing minoritized women's
   political representation across different electoral systems, especially
   when clustered together. And, although ethnic parties and ethnic seats
   promote the representation of both minoritized women and men, ethnic
   seats provide a more level playing field between minoritized women and
   men than ethnic parties.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
