@article{ WOS:001021738200007,
Author = {Zhao, Xuegong},
Title = {The Limits of Confrontation: Nuclear Weapons, the 1958 Taiwan Strait
   Crisis, and China-US Relations},
Journal = {JOURNAL OF COLD WAR STUDIES},
Year = {2023},
Volume = {25},
Number = {2},
Pages = {112-149},
Month = {JUN 23},
Abstract = {During the 1958 Taiwan Strait crisis, the Eisenhower administration used
   nuclear weapons to deter Chinese shelling of the Quemoy islands. Zhao
   claims that the oblique nuclear threats had no effect on Beijing's
   decisions and operations and instead created problems for the Eisenhower
   administration by generating widespread opposition at home and abroad.
   Based on recently declassified U.S. and Chinese materials, this article
   examines U.S. and Chinese leaders' perspectives on nuclear weapons
   during the crisis and other features of U.S.-China relations in the late
   1950s.},
