@article{ WOS:001003887200001,
Author = {Crawshaw, Marilyn and Eriksson, Sanna and Brown, Margot},
Title = {Using a Volunteer Friends Support Scheme in a Temporary Relocation
   Programme},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
