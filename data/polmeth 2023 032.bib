@article{ WOS:001070386500007,
Author = {Smidrkalova, Michaela},
Title = {Atoms for Socialism: The Birth of a Czechoslovak-Soviet Nuclear Utopia},
Journal = {JOURNAL OF COLD WAR STUDIES},
