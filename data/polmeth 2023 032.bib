@article{ WOS:001330569200003,
Author = {Smith, Candis Watts},
Title = {The Making of a Mantra: Americans' Racial Ideologies in the Era of
   Black, Blue, and All Lives Matter},
Journal = {JOURNAL OF RACE ETHNICITY AND POLITICS},
Year = {2023},
Volume = {8},
Number = {3},
Pages = {371-396},
Month = {NOV},
Abstract = {In any racialized social system, a dominant racial ideology will emerge
   to uphold it, but it is always contested by and in dialog with others.
   This article leverages conversations around Black Lives Matter, All
   Lives Matter, and Blue Lives Matter as a site of racetalk. By moving
   beyond narrow conceptualizations and measures of racial attitudes, this
   paper pinpoints a myriad of racially based frameworks, or ways of
   talking and thinking about structural racism, white grievance,
   state-sanctioned social control, and the matter of Black lives. It
   analyzes 1,000 Americans' open-ended responses to a question around
   these contested mantras with the Fightin' Words algorithm alongside an
   inductive analysis to illuminate the use of circulating racial
   ideologies. In addition to outlining the components of four racial
   ideologies-colorblind racism, diversity ideology, white protectionism,
   and anti-racism-the article assesses how they are deployed among
   Americans to uphold or challenge the racial status quo.},
