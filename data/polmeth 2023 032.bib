@article{ WOS:001088031500001,
Author = {Kjaerum, Morten},
Title = {Build Forward Fairer: Covid-19, Lessons Learned},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2023},
Volume = {15},
Number = {3, SI},
Pages = {727-735},
Month = {DEC 30},
Abstract = {The article discusses the importance of adopting a human rights-based
   approach in addressing the lessons learned from the Covid-19 pandemic.
   The article highlights the case of a human rights-based hospital in
   Sweden that actively reached out to vulnerable individuals in the
   community during the pandemic, emphasizing how the human rights-based
   approach contributes to building trust and thereby realizing the right
   to life and health. Based on this example it explores the need for a
   human rights-based approach in crisis management including
   climate-induced disasters. The article emphasizes the interconnectedness
   of all human rights and the need for economic prioritizations that align
   with social justice commitments. It highlights the role of institutions,
   legal frameworks, and collaborative efforts at various levels to ensure
   the realization of human rights and the principle of `leave no one
   behind'. The article concludes by advocating for the integration of
   human rights principles into legislation, policies, and actions to build
   a fairer and more resilient society.},
