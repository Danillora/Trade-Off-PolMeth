@article{ WOS:001087649800001,
Author = {Bouza Garcia, Luis and Oleart, Alvaro},
Title = {Regulating Disinformation and Big Tech in the EU: A Research Agenda on
   the Institutional Strategies, Public Spheres and Analytical Challenges},
