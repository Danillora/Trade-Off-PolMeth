@article{ WOS:001110456000001,
Author = {de Jong, Sara},
Title = {Translating migrant Muslim men: strategies of conditional inclusion by
   Afghan interpreters employed by Western armies},
Journal = {EUROPEAN JOURNAL OF POLITICS AND GENDER},
Year = {2024},
Volume = {7},
Number = {1, SI},
Pages = {9-26},
Month = {FEB},
Abstract = {This article investigates refugees' labour to gain inclusion within the
   `host' community, drawing on interviews with male Afghan former
   interpreters employed by Western armies. It makes an empirical
   contribution by centring them as active agents rather than as passive
   tropes in the racialised and gendered discourses of the `War on Terror'
   and Western migration policies. It offers a synthesis between concepts
   from three fields: migration as translation, migrant masculinities and
   the battleground of conditional inclusion. By focusing on migrants' self
   translations in dialogue with translations of their bodies and stories
   by host-country institutions, I trace three strategies: insertion,
   subversion and exemption. While Afghan interpreters largely fail to be
   recognised as needing protection from harm, their insertion and
   subversion of discourses of protection based on service are more
   successful. Finally, they counter their interpellation as dangerous
   bodies with a strategy of exemption that can be momentarily successful
   but remains ultimately precarious.},
