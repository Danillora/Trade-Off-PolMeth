@article{ WOS:001168646300005,
Author = {Napoli, Philip M.},
Title = {What Is Media Policy?},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2023},
Volume = {707},
Number = {1, SI},
Pages = {29-45},
Month = {MAY},
Abstract = {This article offers a definition of media policy and, through a
   discussion of specific areas of public policy, describes the scope of
   media policy as an area of potential government intervention. I argue
   for a more expansive conceptualization of media policy than has
   typically guided the work of 21st-century media policymakers. I also
   argue that there has been a troubling tendency to apply a technology
   policy framework to contexts that would be better served by a media
   policy analytical framework. The result has been a neglect of core media
   policy principles that are focused on cultivating an informed citizenry
   and ensuring that communities' information needs are being met.},
