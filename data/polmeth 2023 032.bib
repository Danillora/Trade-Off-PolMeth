@article{ WOS:000939476000001,
Author = {Endo, Yuya and Ono, Yoshikuni},
Title = {Opposition to Women Political Leaders: Gender Bias and Stereotypes of
   Politicians Among Japanese Voters},
Journal = {JOURNAL OF WOMEN POLITICS \& POLICY},
