@article{ WOS:001144344400002,
Author = {Kollar, Eszter and Boucher, Francois},
Title = {Introduction: Voluntariness and Migration},
Journal = {ETHICS \& INTERNATIONAL AFFAIRS},
Year = {2023},
Volume = {37},
Number = {4},
Pages = {401-405},
Abstract = {The concept of voluntariness permeates the ethics and politics of
   migration and is commonly used to distinguish refugees from migrants.
   Yet, neither the precise nature and conditions of voluntariness nor its
   ethical significance for migrant rights and state obligations has
   received enough attention. The articles in this collection move the
   debate forward by demonstrating the complex ethical judgments involved
   in delineating voluntary from forced migration and in drawing out its
   political and institutional implications. In addition to highlighting
   the interplay between the voluntary and nonvoluntary elements of
   migration over time and across different sites of the migration journey,
   they provide a nuanced account of the various conditions of
   voluntariness and they challenge common ideas about its normative
   political consequences.},
