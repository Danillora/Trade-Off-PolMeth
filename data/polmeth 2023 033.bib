@article{ WOS:001082919600001,
Author = {Lim, Guanie and Hoon, Chang-Yau and Zhao, Kaili},
Title = {Foreign Investment, State Capitalism, and National Development in
   Borneo: Rethinking Brunei-China Economic Relations},
