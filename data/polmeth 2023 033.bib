@article{ WOS:000951099900001,
Author = {MacLean, Ken},
Title = {Interactive Digital Platforms, Human Rights Fact Production, and the
   International Criminal Court},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2023},
