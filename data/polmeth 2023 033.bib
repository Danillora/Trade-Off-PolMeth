@article{ WOS:000949385500001,
Author = {Axelsson, Tobias and Martins, Igor},
Title = {Resilience to Shrinking as a Catch-Up Strategy: a Comparison of Brazil
   and Indonesia, 1964-2019},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
