@article{ WOS:000928452000001,
Author = {Johnsen, Alex Gron and Hall, John A.},
Title = {Liberalism, nationalism and federalism: Reflections on the life and work
   of Meir Goldschmidt},
Journal = {NATIONS AND NATIONALISM},
Year = {2023},
