@incollection{ WOS:001080764000035,
Author = {Perelman, Laura},
Editor = {Ness, I},
Title = {THE GIG ECONOMY AND THE FORMATION OF NEW PLATFORM TRADE UNIONS IN SOUTH
   AMERICA},
Booktitle = {ROUTLEDGE HANDBOOK OF THE GIG ECONOMY},
Series = {Advances in Evaluation and Development},
Year = {2023},
Volume = {11},
Pages = {500-515},
Abstract = {Although the increasing precarisation of work is a structural trend of
   neoliberal globalisation, the gig economy has gone further on this path,
   massively relying on spurious self-employment and limiting access to
   labour rights and union protections. In this context, the legal status
   and labour conditions of platform workers have been a contentious field,
   crossed by growing protests and conflicts in several countries.
   Gradually, workers' collective action has led to organising efforts and,
   in some cases, the formation of new labour unions. This process
   developed against a challenging background, marked by notorious market
   power asymmetries, lack of legal protection and governments' reluctance
   to regulate. Also, in most countries, traditional unions' responses to
   gig workers' organising efforts have been quite ambiguous.
   This work addresses the rise and establishment of the Argentinian
   Association of Platform Workers (APP), the first union of delivery
   platform workers in Latin America. This case study will be informed by
   the power resources theory (PRT), which centres on the dynamics of power
   accumulation by workers' organisations in the context of specific
   market, institutional, associational and social conditions (R Berins
   Collier, VB Dubal, and C Carter, 2017). Moreover, this article seeks to
   advance a conceptual framework for a comparative analysis of
   unionisation across countries in order to identify the structure of
   opportunities and challenges for gig workers' organisation (S Etchemendy
   and L Perelman, 2018).},
