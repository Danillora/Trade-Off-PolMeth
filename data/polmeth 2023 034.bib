@incollection{ WOS:001370117000005,
Author = {Malik, Adeel and Mirza, Rinchan and Platteau, Jean-Philippe},
Editor = {Faguet, JP and Pal, S},
Title = {Devolution under autocracy: evidence from Pakistan},
Booktitle = {DECENTRALISED GOVERNANCE: Crafting Effective Democracies Around the
   World},
Year = {2023},
Pages = {99-134},
Abstract = {Authoritarian regimes often direct the course of electoral politics in
   ways that allow them to concentrate and consolidate power. This
   observation applies well to Pakistan and its three autocratic regimes
   under military rulers General Ayub Khan (1958-69), Zia-ul-Haq (1977-88),
   and General Parvez Musharraf (1999-2008). The political reforms enacted
   by Zia-ul-Haq, his devolution programme, and his mode of channelling
   development funds via elected politicians exerted an enduring impact on
   the country's political system. Specifically, we argue that
   institutional changes under Zia's regime have stimulated the rise of
   family politics in replacement of party politics, resulting in the
   formation and consolidation of political dynasties. They have also
   contributed to the capture of local bureaucracy by elected politicians
   thereby entrenching clientelism.},
Publisher = {LONDON SCH ECONOMICS \& POLITICAL SCIENCE-LSE PRESS},
