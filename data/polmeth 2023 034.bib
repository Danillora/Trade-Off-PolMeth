@incollection{ WOS:001080764000029,
Author = {Shibata, Saori},
Editor = {Ness, I},
Title = {FICTITIOUS AUTONOMY AND NEGOTIATED CONSENT},
Booktitle = {ROUTLEDGE HANDBOOK OF THE GIG ECONOMY},
Series = {Advances in Evaluation and Development},
Year = {2023},
Volume = {11},
Pages = {401-418},
Abstract = {Japan's gig economy has grown significantly over the last decade.
   Government and business elites have actively sought to promote gig work,
   highlighting the opportunities it offers in terms of greater autonomy
   for workers. This is despite a number of detrimental consequences
   arising from the further flexibilisation of labour markets associated
   with gig work. This chapter explores these developments, discussing how
   the integration of new digital technologies has become a key measure
   through which efforts are being made to increase productivity and
   further growth in Japan's ailing economy. This has acted to deprive gig
   workers of skills and lock them into low-paid and precarious jobs, as
   well as increasing workplace surveillance and control. Reflecting the
   precarious nature of gig work, some of the gig workers have opposed the
   unstable and hazardous nature of precarious work. Nevertheless, social
   conflict in the Japanese gig economy has been limited. This is partly
   due to a high level of consent among gig workers, which has also emerged
   as a concomitant phenomenon of the gig economy.},
