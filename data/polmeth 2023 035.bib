@incollection{ WOS:001514619900004,
Author = {Lordos, Alexandros and Louise, Christopher and Symeou, Maria and Guest,
   Alexander and Dagli-Hustings, Ilke},
Editor = {Rettberg, A and Ugarriza, JE},
Title = {Pathways to Resilient Citizenship: A Cross-National Investigation Based
   on SCORE Data From Six Conflict-Affected Countries in Africa, Asia, and
   Europe},
Booktitle = {RECONCILIATION: Experiences in Colombia and the World},
Series = {Coleccion General},
Year = {2023},
Pages = {17-49},
Abstract = {People in countries affected by violent social and political conflict
   often experience a multitude of adversities across several life domains.
   In such emergency contexts, individual citizens are called to play a
   role in the lives of their communities, even as they themselves might be
   experiencing consequences of the conflict. Under such extreme pressures,
   social cohesion easily breaks down and people can lose sight of
   constructive approaches to conflict management and resolution,
   especially in situations where the rule of law fails or is perceived to
   have failed. Thus, it is of critical importance to identify factors that
   facilitate the emergence of constructive and non-violent active
   citizenship in populations that are conflict-prone and where peace
   remains fragile.
   The Social Cohesion and Reconciliation index (SCORE) was developed in
   2012 as a participatory and empirical framework to identify drivers of
   social cohesion, peaceful citizenship, and community resilience in
   conflict-affected populations. Since its inception, the SCORE has been
   implemented in numerous countries across Africa, Asia, and Europe. For
   the purposes of this study, data from six countries where the SCORE
   index has been deployed-specifically, Liberia, South Sudan, Nepal,
   Malaysia, Ukraine, and Bosnia-Herzegovina-were analyzed to identify
   cross-national factors, at the individual, community, and institutional
   level, that predict constructive and non-violent citizenship, as
   contrasted against violent or passive citizenship.},
