@article{ WOS:000976167500001,
Author = {Oceno, Marzia and Yen, Wei-Ting},
Title = {The Impact of Racial Descriptive Norms on Vaccination against COVID-19},
Journal = {PUBLIC OPINION QUARTERLY},
