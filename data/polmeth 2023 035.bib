@article{ WOS:001120543100001,
Author = {Ahmed, Hamza and Miller, Erika E.},
Title = {Quantifying the Economic Impact on Farmers from Agricultural Machinery:
   A Case Study of Farmers in Sudan},
Journal = {WORLD},
Year = {2023},
