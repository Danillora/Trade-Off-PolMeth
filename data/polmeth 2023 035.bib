@article{ WOS:001131143300001,
Author = {Jahaniaghdam, Tannaz and Mamdoohi, Amir Reza and Kheyrabadi, Salman
   Aghidi and Mehryar, Mehdi and Ciari, Francesco},
Title = {Preferences for Alternative Fuel Trucks among International Transport
   Companies},
