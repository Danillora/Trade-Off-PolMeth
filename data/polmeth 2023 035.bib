@article{ WOS:001065958900001,
Author = {Claassen, Christopher and Magalhaes, Pedro C.},
Title = {Public Support for Democracy in the United States Has Declined
   Generationally},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2023},
Volume = {87},
Number = {3},
Pages = {719-732},
Month = {NOV 21},
Abstract = {Support for democracy in the United States, once thought to be solid,
   has now been shown to be somewhat shaky. One of the most concerning
   aspects of this declining attachment to democracy is a marked age gap,
   with younger Americans less supportive of democracy than their older
   compatriots. Using age-period-cohort analysis of 12 national surveys
   collected between 1995 and 2019, we show that this age gap is largely a
   function of a long-term generational decline in support for democracy,
   with little evidence of an independent life-cycle effect apparent. The
   combination of generational decline without a positive and
   counterbalancing life-cycle effect offers a sober prognosis of how
   support for democracy in the United States might look in the future.},
