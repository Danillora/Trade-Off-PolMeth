@article{ WOS:001047750700001,
Author = {Schmoll, Moritz and Swenson, Geoffrey},
Title = {Avoiding the Political Resource Curse: Evidence from a Most-Likely Case},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
