@incollection{ WOS:000994426900006,
Author = {Spicker, Paul},
Book-Author = {Spicker, P},
Title = {The scope of legitimate action},
Booktitle = {STATES AND WELFARE STATES: Government for the people},
Year = {2023},
Pages = {69-82},
Abstract = {Any adequate account of the state should be able to determine what a
   government is legitimately able to do, and this chapter considers four
   different views which offer answers to that. Liberalism saw the duty of
   the state as being to enhance the rights and freedoms of individuals;
   collective action could be taken by consent. Social democrats use the
   state as an instrument to make principled change. Communitarian
   conservatives stress particular duties rather than general rights,
   working with the grain of the social order. Neoliberals advocate
   laissez-faire and free markets; the legitimate role of the state is
   consequently limited.},
