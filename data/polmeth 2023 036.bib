@article{ WOS:001023902300001,
Author = {Nisch, Stefan},
Title = {Public opinion about Finland joining NATO: analysing Twitter posts by
   performing natural language processing},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2024},
Volume = {32},
Number = {1},
Pages = {272-290},
Month = {JAN 2},
Abstract = {The announcement of Finland's decision to join NATO in May 2022 has
   provoked many public discussions and debates both inside and outside
   Finland. The aim of this study is to process natural language in Twitter
   data collected between 12 May and 3 April 2023 and to analyse the public
   sentiment on Finland's decision to join NATO. A total of 28,993 tweets
   were collected and analysed using the VADER algorithm for sentiment
   analysis and the LDA algorithm for topic modelling. The results show
   that overall sentiment towards Finland's NATO membership was mostly
   positive. Topic modelling revealed that the most prevalent topics of the
   tweets were related to Finland's alliances, concerns, and general
   opinions of the NATO membership. The results suggest that the public
   discussion on Twitter focuses on the potential benefits and risks of
   Finland's decision to join NATO, especially considering the recent
   tensions with Russia. The study provides insights into public opinion
   about Finland's NATO membership and highlights the potential of natural
   language processing techniques for the analysis of social media data.
   These findings could help in further policy discussions about Finland
   joining NATO and provide a basis for further research into public
   opinion on NATO and other international organizations.},
