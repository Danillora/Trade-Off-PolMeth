@article{ WOS:001007699200001,
Author = {Verner, Marija},
Title = {Political Trust and Ecological Crisis Perceptions in Developing
   Economies: Evidence from Ecuador},
Journal = {LATIN AMERICAN POLITICS AND SOCIETY},
Year = {2023},
Volume = {65},
Number = {4},
Pages = {147-175},
Month = {NOV},
Abstract = {Could an individual's perception of the possibility of a future
   ecological crisis be linked to their level of political trust? Studies
   of environmental attitudes have identified political trust as an
   important predictor of support for environmental taxation or risk
   perceptions surrounding specific local environmental hazards, but less
   is known about its role when environmental risks are perceived as
   diffuse and distant. Using original survey data from Ecuador, this
   article finds that political distrust predicts heightened ecological
   crisis perceptions and that higher educational attainment intensifies
   this relationship. A follow-up analysis of the AmericasBarometer's
   Ecuador survey shows that political distrust also predicts higher
   concern about climate change. These findings suggest that when
   evaluations of political institutions reflect perceptions of
   environmental risks, individuals blame the government for environmental
   failures. The implications of this study are particularly relevant for
   political institutions in developing economies, where the public sector
   often spearheads development efforts.},
