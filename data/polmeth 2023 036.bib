@article{ WOS:001034291700001,
Author = {Toluk, Pembe and Ozad, Bahire},
Title = {Medical aid under the shadow of the Covid-19: A political crisis among
   the leadership in North Cyprus},
Journal = {MEDITERRANEAN POLITICS},
Year = {2024},
Volume = {29},
Number = {5},
Pages = {755-780},
Month = {OCT 19},
Abstract = {Accepting Greek Cypriot administration's (Republic of Cyprus) medical
   aid by the Turkish Cypriots during COVID-19 pandemic in April 2020 has
   caused a conflict in North Cyprus print media. During the pandemic,
   medical aid has been provided by Greek Cypriot administration with the
   initiatives of the President of the time in North Cyprus, Mustafa
   Akinci. The Prime Minister of the time in North Cyprus, Ersin Tatar
   opposed to this medical aid. This study focuses on the most circulated
   two North Cyprus newspapers' (Kibris and YeniDuzen) discourse regarding
   the aid. The medical aid sent by the Greek Cypriot administration
   (Republic of Cyprus) is important for developing mutual trust and
   enhances the already existing networks between the two sides in the
   island. The reactions to this aid are also important due to approaching
   Presidential elections in North Cyprus since both the President and the
   Prime Minister were candidates in the elections. The study presents the
   results of Critical Discourse Analysis conducted for analysing the news
   stories related to the medical aid under the light of Social Capital
   Theory put forth by Robert Putnam.},
