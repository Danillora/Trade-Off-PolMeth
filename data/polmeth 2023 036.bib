@article{ WOS:000983580800001,
Author = {Helms, Benjamin},
Title = {Exit or Voice? Corruption Perceptions and Emigration},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
Year = {2024},
Volume = {59},
Number = {1},
Pages = {138-175},
Month = {MAR},
Abstract = {Existing research suggests that people concerned about corruption use
   elections to punish corrupt incumbents. I argue that when people grow
   concerned with corruption, they may turn to exit rather than voice,
   using emigration to escape corruption. Highly educated citizens in
   particular, who are most informed about corruption and hold valuable
   human capital, likely see exit as attractive in the face of persistent
   corruption. Using a wealth of micro-level data on potential emigration,
   I show that when people perceive widespread corruption, they are more
   likely to have taken concrete steps to emigrate. Advanced degree holders
   are much more likely to have prepared to emigrate in response to
   corruption. I also show that corruption-concerned potential emigrants
   choose less corrupt destination countries and are less likely to have
   faith, and participate, in electoral institutions. My argument and
   findings hold implications for the corruption literature and generate
   new questions about the relationship between emigration and
   accountability.},
