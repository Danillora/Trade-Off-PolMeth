@article{ WOS:001118288900001,
Author = {Mehmood, Usman and Aslam, Muhammad Umar and Javed, Muhammad Adil},
Title = {Associating Economic Growth and Ecological Footprints through Human
   Capital and Biocapacity in South Asia},
Journal = {WORLD},
Year = {2023},
Volume = {4},
