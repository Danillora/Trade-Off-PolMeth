@article{ WOS:001103639300001,
Author = {Malmberg, Fredrik and Karv, Thomas},
Title = {Jacks of all trades, masters of none? Multiple-mandate holding and trust
   in local government},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2024},
Volume = {50},
Number = {5},
Pages = {871-892},
Month = {SEP 2},
Abstract = {Multiple-mandate holding, i.e., the holding of more than one directly
   elected mandate simultaneously on different government tiers, is a
   controversial practice with potential consequences on political trust.
   The research purpose of this study is thus to contribute to this line of
   research by exploring how the extent of this practice at the local level
   reflects on the opinions of ordinary citizens and their confidence in
   local government. The aim of this paper is therefore to examine this
   question utilising a vignette survey experiment carried out in Finland.
   The findings presented contribute to shining a light on the potential
   consequences of this practice for trust in the local government by
   exposing one way in which negative evaluations of politicians are
   formed.},
