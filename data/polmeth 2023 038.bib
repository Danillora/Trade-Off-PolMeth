@incollection{ WOS:001285542200002,
Author = {Wenzlhuemer, Roland},
Editor = {Steger, MB and Benedikter, R and Pechlaner, H and Kofler, I},
Title = {Dis:connectivity in Global History},
Booktitle = {GLOBALIZATION: Past, Present, Future},
Year = {2023},
Pages = {11-26},
Abstract = {This chapter takes a historical perspective on processes of
   globalization. It evaluates how the historical sciences have hitherto
   applied the concept of globalization in historical research and examines
   some of the discontents that have emerged among historians in this
   context. The chapter claims that such discontents are often the
   consequence of an oversimplified, unidirectional understanding of
   globalization that ignores the role of dis-connections in processes of
   global entanglement. It advocates a stronger focus on the interplay
   between global connectivity and disconnectivity (captured by the term
   dis:connectivity) and develops this claim with the help of examples from
   the history of telegraphy.},
