@article{ WOS:000973032000001,
Author = {Patsias, Caroline and Patsias, Sylvie},
Title = {Politicisation at the local political level within representative and
   participative institutions: the example of a poor Montreal borough},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2024},
Volume = {50},
Number = {2},
Pages = {329-351},
Month = {MAR 3},
Abstract = {The majority of studies on local politics and politicisation have
   focused on the study of electoral participation, political parties or
   civil society. The qualitative dimension of politicisation within local
   institutions has thus been overlooked. This research addresses this
   dimension by comparing how citizens politically discuss the world in a
   representative and a participative institution (The borough council and
   the participatory budget respectively) of local democracy in one of the
   poorest areas of the city of Montreal in Canada. In order to grasp the
   politicisation within political institutions, we link the notion to a
   rise in generality echoing a conflictualisation of stakes or/and a
   reference to (in)justice. Our analysis reveals that both institutional
   designs examined acts in complementary ways. By giving utterance to
   conflict, the borough council permits the expression of all
   neighbourhood issues while the participatory budget allows for citizens
   to dream the future by bringing out the consensus of the moment.},
