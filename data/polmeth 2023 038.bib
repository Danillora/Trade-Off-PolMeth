@article{ WOS:001018618600001,
Author = {Yusupova, Guzel and Matveev, Ilia},
Title = {Nationalism and Inequality Scholarship in the Age of Populism: Bringing
   Territory Back In?},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
