@article{ WOS:000995978700001,
Author = {Henriksson, Linnea and Christensen, Henrik Serup},
Title = {Miles' law in Finnish municipalities: where decision-makers stand
   depends on where they sit},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2024},
Volume = {50},
Number = {2},
Pages = {405-426},
Month = {MAR 3},
Abstract = {Miles' Law, i.e., the notion that institutional position affects
   attitudes, is considered a truism, but not much attention has been given
   to properly test or develop it. Here, we argue that Miles' Law is
   effective not only for civil servants but among municipal
   decision-makers in different positions. We examine how well
   institutional position can explain differences in attitudes to welfare
   service provision among decision-makers on different levels. Differences
   in opinion depending on institutional position are usually overlooked
   when attitudes on welfare are explained. Using survey data (N = 1,578)
   from 42 municipalities in Finland, our analyses show that institutional
   position does affect attitudes to service provision. However, the
   differences are contingent on the mode of service provision under
   consideration and are moderated by the size of the municipality and the
   economic status. This shows that it is important to consider the
   position of local government decision-makers to understand their
   attitudes.},
