@article{ WOS:000961404800001,
Author = {Ariely, Gal},
Title = {Testing the National Identity Argument in a Time of Crisis - Evidence
   from Israel},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2024},
Volume = {52},
Number = {1},
Pages = {205-221},
Month = {JAN},
Abstract = {This article explores the national identity argument in unsettled times
   by using the COVID-19 pandemic as a test case. It uses a longitudinal
   survey among Jewish Israelis to examine whether the pandemic influenced
   levels of national identity and solidarity and whether it altered their
   relationship. The findings indicate a clear reduction in levels of
   solidarity, national attachment, and national chauvinism over time. They
   also show that the positive connection between national attachment and
   solidarity grew stronger, while the connection between national
   chauvinism and solidarity became weaker and insignificant. These
   findings provide complex evidence for the national identity argument.},
