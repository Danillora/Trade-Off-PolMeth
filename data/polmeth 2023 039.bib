@article{ WOS:001297639200004,
Author = {Mesa, Fabio Andres Hincapie and Roldan, Juan Carlos Londono},
Title = {The impact of income inequality on life expectancy and health in
   Colombia},
Journal = {REVISTA CIENTIFICA GENERAL JOSE MARIA CORDOVA},
Year = {2023},
Volume = {21},
Number = {42},
Month = {APR-JUN},
Abstract = {Reducing inequalities in income and health, within and between
   countries, is a global priority for the WHO. This study seeks to examine
   income inequality as a predictor of life expectancy in Colombia. To
   achieve this, a cross-sectional panel data model was developed using the
   DANE's Quality of Life Survey. The predictor variables of life
   expectancy were the Gini coefficient, GDP per capita and, in a novel
   way, monetary poverty, between 2012 and 2019. The results indicate that
   these variables have a significant impact on life expectancy, while no
   significant impact on social mobility was found. The findings can guide
   public policy and governmental decision-making in Colombia.},
Publisher = {ESCUELA MILITAR CADETES GENERAL JOSE MARIA CORDOVA},
