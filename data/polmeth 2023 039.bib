@article{ WOS:001315150000013,
Author = {Ramirez, Daniel Felipe Paez and Mosquera, Hector Antonio Dominguez and
   Sierra-Zamora, Paola Alexandra and Castano-Bedoya, Alejandro},
Title = {Political participation and electoral systems: the case of Colombia,
   Argentina and Mexico, 1910-1917},
Journal = {REVISTA CIENTIFICA GENERAL JOSE MARIA CORDOVA},
Year = {2023},
Volume = {21},
Number = {41},
Month = {JAN-MAR},
Abstract = {Between 1910-1917, different antagonistic processes, represented in
   political-ideological positions, shaped the electoral and political
   participation systems in Colombia, Mexico and Argentina. This article
   describes the contexts of the development of electoral systems and of
   rights and freedoms in these countries, according to two political and
   ideological positions. In this sense, the tensions of power are
   presented between a vision of a liberal State, which privileges
   individual freedoms and rights, and the perspective of the unitary and
   centralist State, which suppresses any type of expression different from
   the established order.},
