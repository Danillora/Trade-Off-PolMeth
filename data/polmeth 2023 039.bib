@article{ WOS:001295047700007,
Author = {Sarmiento, Reynell Badillo and Rosero, Luis Fernando Trejos},
Title = {The Gaitanista Self-Defense Forces of Colombia as a politicized armed
   group: a new paradigm of organized crime},
Journal = {REVISTA CIENTIFICA GENERAL JOSE MARIA CORDOVA},
Year = {2023},
Volume = {21},
Number = {42},
Month = {APR},
Abstract = {. Since the demobilization of the AUC and the FARC-EP, several armed
   actors have emerged in Colombia that have distanced themselves from the
   insurgency -paramilitary logic that dominated the analysis of the war in
   the country. However, some of these groups have also assumed political
   discourses that question the denomination of criminals by some political
   and academic sectors. This article seeks to go beyond these discussions
   through the politicized armed groups (GAP) category. To this end, a case
   study of the Gaitanista Self -Defense Forces of Colombia, the largest
   armed organization active in Colombia, is elaborated. The organization's
   published discourses are analyzed, in order to understand its
   politicization process, while explaining its criminal dimension.},
Publisher = {ESCUELA MILITAR CADETES GENERAL JOSE MARIA CORDOVA},
