@article{ WOS:001045959300001,
Author = {Deets, Stephen},
Title = {Liberal Democratic Mayors in Illiberal Populist Regimes: Istanbul's
   Challenges},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2024},
Volume = {52},
Number = {4},
Pages = {889-906},
Month = {JUL},
Abstract = {After Ekrem Imamoglu won as Istanbul's mayor, the contestation between
   the city and central government resembled other cases of liberal mayors
   winning in illiberal populist regimes. To expand liberal democracy, the
   mayor sought to reinvigorate effective democratic citizenship by
   increasing access to information, creating more inclusive governance
   networks through public participation opportunities, limiting the
   regime's clientelism and rent-seeking opportunities in real estate and
   contracting, and (re)creating social solidarity. The national government
   responded by extensively covering ``scandals{''} and continuing populist
   rhetoric to maintain polarization, limiting the city's financial
   resources, and moving power and projects to agencies they still
   controlled. This article uses Istanbul to develop this model and
   illustrate how Imamoglu has made progress in each area despite the
   central government's effort to constrain his administration.},
