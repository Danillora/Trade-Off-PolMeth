@article{ WOS:001353206900001,
Author = {Ralaigh, Chris O.},
Title = {The north of Ireland during the interregnum: a Gramcian analysis of
   power and crisis},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2025},
Volume = {18},
Number = {1, SI},
Pages = {93-111},
Month = {JAN 2},
Abstract = {The North of Ireland was induced in to existence over a century ago, yet
   the polity lacked societal-wide legitimacy. The outbreak of conflict in
   1968 signalled the beginning of an interregnal period in which the old
   order was substantially challenged. Whilst the Good Friday Agreement was
   idealised as an end to this interregnum, Brexit has re-opened the battle
   for politico-ideological supremacy. This paper traces the contours of
   the hegemony-seeking strategies of the various political actors from
   1968-present and assesses whether we are in the crucial and final phase
   of the organic crisis of the North of Ireland.},
