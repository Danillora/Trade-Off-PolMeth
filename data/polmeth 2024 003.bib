@article{ WOS:001349478700001,
Author = {Enyedi, Zsolt},
Title = {Concept and Varieties of Illiberalism},
Journal = {POLITICS AND GOVERNANCE},
Year = {2024},
Volume = {12},
Abstract = {This article discusses various conceptualizations of illiberalism and
   adopts a definition that equates the concept with the negation of three
   liberal democratic principles: limited power, a neutral state, and an
   open society. The second part of the article explores the implications
   of this definitional strategy for empirical research, describes the
   relationship between populism, authoritarianism, and illiberalism, and
   identifies nine distinct routes to illiberalism: authoritarian,
   traditionalist, religious, libertarian, nativist-nationalist, populist,
   paternalist, materialist-technocratic, and left-wing.},
