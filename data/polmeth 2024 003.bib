@article{ WOS:001190780600001,
Author = {Davila, Salvador Cazzato and Machado, Camilo Vargas},
Title = {Class consciousness and the historical-philosophical contributions of
   Gyõrgy Lukács from historical today : the Case of Colombia},
Journal = {IZQUIERDAS},
Year = {2024},
Volume = {53},
Month = {MAR},
Abstract = {The decision not to consider the validity of the socio-historical
   contributions of the Austro-Hungarian Gyorgy Lukacs is irreversible if
   it does not attend to their relevance. His accredited work: History and
   Class Consciousness is a classic that is difficult to ignore from the
   moment that a critical investigation is undertaken into a society that
   is the persistent object of the interests of oligarchy/bourgeois elites.
   Deserved conceptual distinctions such as Class Consciousness and many
   others validate this historiographic validity. Reason why the
   dialectical method, dialectical materialism and other particular
   contributions of Marx have been addressed by virtue of the essential
   contributions of Lukacs to analyze the societies subjugated by the
   interests of elitist power such as the case of the Republic of Colombia,
   which despite not Having passed a year of his respective government
   period, he and his party Colombia Humana have led reforms and certain
   transformations that, through the dialectic of Lukacs's purposeful
   historical materialism, achieve substrates for social change.},
