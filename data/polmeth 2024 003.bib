@article{ WOS:001350197400001,
Author = {Bell, Macalester},
Title = {The emotion(s) of offense and victimhood culture},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2024},
Month = {2024 NOV 7},
Abstract = {Emily McTernan's book, On Taking Offense, makes an original and
   important contribution to the moral psychology of offense. I argue that
   we may have reasons to question McTernan's view that offense is best
   characterized as a single, discrete, emotion. Moreover, I argue that the
   account of fittingness that McTernan describes deepens concerns some
   have raised regarding an apparent shift in the culture of offense.},
