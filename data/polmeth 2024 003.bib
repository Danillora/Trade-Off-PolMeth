@article{ WOS:001375860600001,
Author = {Tang, Min and Han, Sung Min and Huhe, Narisong},
Title = {Paradox of choices: inclusiveness of mainstream party systems, cultural
   grievance about immigration and populist support among European
   countries},
Journal = {CONTEMPORARY POLITICS},
Year = {2024},
Month = {2024 DEC 13},
Abstract = {This study explores the interactive effect of individual and
   institutional factors of public support for populist parties. Focussing
   on European countries, it investigates how multiplicity and variability
   of mainstream political parties moderate the effect of citizens'
   grievances related to immigration on their populist support. We
   hypothesise that while a more inclusive party system may decrease the
   effect of citizens' economic anxiety on populist support, it cannot
   mitigate the effect of cultural concern about immigrants. Instead, it
   may amplify that effect. A comparative multilevel analysis of 20
   European countries for a period of 15 years yields supportive findings
   for our hypothesis.},
