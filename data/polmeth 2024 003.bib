Title = {COVID-19, Public Responses and Government Models: A Comparative Analysis
   of Quito and Guayaquil},
Journal = {GEOPOLITICAS-REVISTA DE ESTUDIOS SOBRE ESPACIO Y PODER},
Year = {2024},
