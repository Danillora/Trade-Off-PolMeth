@article{ WOS:001350625900001,
Author = {Lever, Annabelle},
Title = {Democracy and the ethics of voting},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2025},
Volume = {28},
Number = {5, SI},
Pages = {831-847},
Month = {JUL 29},
Abstract = {This paper provides an overview of the ethical challenges facing voters
   in democratic elections. It starts by examining the assumptions that
   underpin contemporary claims about the moral and epistemic advantages of
   lotteries as compared to elections and shows their similarities to
   arguments for `unveiling the vote', as Brennan and Pettit put it. (G.
   Brennan \& Pettit, 1990) It looks at the empirical and normative
   difficulties of these claims and highlights the risk of confusing
   morally misguided voting with injustice, and of supposing that it must
   be politically consequential. Finally, the paper casts doubt on the
   claim that democracy implies that voters should be publicly accountable
   to each other for their vote and that the secret ballot is, therefore, a
   regrettable, if necessary, concession of principle to sad reality. It
   concludes by drawing out the implications of its analysis for the ethics
   of voting in ideal, non-ideal and highly non-ideal situations.},
