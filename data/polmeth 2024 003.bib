@article{ WOS:001454616300005,
Author = {Blehova, Maria and Pikner, Ivo},
Title = {UNDERSTANDING THE FUTURE OPERATING ENVIRONMENT: EXPLORING HUMAN FACTOR
   IN THE MILITARY},
Journal = {POLITICKE VEDY},
Year = {2024},
Volume = {27},
Number = {4},
Abstract = {The future security environment outlined in the various strategic
   documents of selected countries and NATO has been extensively analyzed
   to understand the possible challenges for the future armed forces.
   However, while these documents emphasize number of external factors and
   technological developments, less attention has been given to the human
   factor and its role in shaping future military effectiveness. This
   article seeks to bridge this gap by identifying the intersections
   between emerging operational environments and the behavioural
   characteristics required for the future forces. Through an examination
   of anticipated changes and challenges, the study focuses on how the
   human factor must evolve to meet the demands of increasingly complex
   security contexts. The findings highlight key recommendations for
   strengthening behavioral and interpersonal traits in armed forces which
   are essential for operational effectiveness in the future. These
   recommendations, aimed particularly at senior military leadership, offer
   a practical guide for preparing personnel to thrive in the rapidly
   evolving security environment and ensuring mission success in the face
   of emerging challenges.},
