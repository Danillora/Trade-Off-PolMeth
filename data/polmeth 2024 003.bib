@article{ WOS:001285388200002,
Author = {Harris, Neal and Jervis, Robin},
Title = {Worker Cooperatives for a Democratic Economy A Conversation with
   Critical Theory},
Journal = {DEMOCRATIC THEORY-AN INTERDISCIPLINARY JOURNAL},
