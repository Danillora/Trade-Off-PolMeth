@article{ WOS:001184463100011,
Author = {McKelvie, Christina},
Title = {`STOP THE WORLD, SCOTLAND WANTS TO GET ON' - REFLECTIONS FROM A CAREER
   DRIVING FORWARD CHANGE FOR WOMEN IN SCOTLAND},
Journal = {SCOTTISH AFFAIRS},
Year = {2024},
Volume = {33},
Number = {1},
Pages = {99-104},
Month = {FEB},
Abstract = {Since being elected as an MSP in 2007, I have always tried to use the
   platform that comes as a politician to promote and help drive forward
   the work of others - particularly those working to tackle inequality and
   promote social justice. What inspired me personally to get involved in
   politics was an innate understanding that for women in politics,
   particularly working-class women in politics, we were simply not
   represented in a meaningful way. This piece charts my early days getting
   involved in politics in Scotland, reflecting on some of my most
   challenging personal experiences, as well some of the political
   achievements in which I have most pride. While women have contributed to
   modern Scotland in ways that we never fully celebrate, we need more
   women in the corridors of power to ensure that the voices of women are
   never overlooked again.},
