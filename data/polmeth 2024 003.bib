@article{ WOS:001197250100001,
Author = {Papp, Zsofia and Navarro, Julien and Russo, Federico and Nagy, Laura
   Emoke},
Title = {Patterns of democracy and democratic satisfaction: Results from a
   comparative conjoint experiment},
Journal = {EUROPEAN JOURNAL OF POLITICAL RESEARCH},
