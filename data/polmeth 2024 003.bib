@article{ WOS:001370479000001,
Author = {Roche, Gerald},
Title = {``Conquered Primitives Have No Written Language{''}: Language
   Revitalization, Reactionary Settler Colonialism, and Perpetual Genocide},
Journal = {GENOCIDE STUDIES INTERNATIONAL},
