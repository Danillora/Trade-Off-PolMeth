@article{ WOS:001207842200001,
Author = {Matthews, Alan},
Title = {Farmer Protests and Income Developments in the EU},
Journal = {POLITICAL QUARTERLY},
Year = {2024},
Volume = {95},
Number = {2},
Pages = {344-349},
Month = {APR},
Abstract = {Farmer protests are currently taking place in many European countries.
   There are multiple motives behind these, but low prices and incomes are
   frequently cited. This can be hard to understand, as agricultural
   incomes in the past three years in the EU have reached their highest
   ever levels, largely the result of a decline in the number of farms and
   farmers. Farms are also very heterogeneous. Those farms producing the
   majority of agricultural output have the necessary scale to provide a
   decent income, but the majority of farms are not able to remunerate
   their labour input adequately. The continued existence of a productivity
   gap between farm and non-farm sectors suggests farm consolidation will
   continue, but stronger environmental interventions could ensure this
   process occurs in a sustainable way.},
