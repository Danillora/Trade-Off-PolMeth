@article{ WOS:001385710000001,
Author = {Lee, Sumin and Kim, Minkyu},
Title = {Do petitions work? A follow-up study of Japanese local assembly
   petitions, following their adoption},
Journal = {JAPANESE JOURNAL OF POLITICAL SCIENCE},
Year = {2024},
Volume = {25},
Number = {3},
Pages = {206-222},
Month = {SEP},
Abstract = {Many democracies have questioned the policy effectiveness of
   parliamentary petitions because the legislative approval of petitions
   does not legally bind follow-up measures that lead to petition
   realization. This study examines the connection between a legislature's
   adoption of petitions and ex-post policy implementation, using data from
   an original mail survey of previous petitioners at the prefectural level
   in Japan. Regression analysis and empirical insights from case studies
   suggest that the disclosure and transparency of the petition process,
   especially the issuance of progress reports, enhance officials'
   commitment to petitions' broad goals, leading to greater accountability,
   which, in turn, facilitates increased petition realization. These
   findings imply that citizens in Japan can expect democratic
   responsiveness to function, partly through the parliamentary petition
   system after an election.},
