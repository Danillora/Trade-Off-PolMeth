@article{ WOS:001342497100001,
Author = {Valencia, David Alejandro},
Title = {Does populism require a normative complement? Ideas for democratising
   populism by Ernesto Laclau},
Journal = {IZQUIERDAS},
Year = {2024},
Volume = {53},
Pages = {1-20},
Month = {SEP},
Abstract = {in the past 20 years, Ernesto Laclau's theorization of populism as a
   tool for emancipatory political projects has become dominant in
   political theory. Many authors following Laclau's footsteps agree that
   this theory is sufficient as a principle of democratization and
   inclusion of social sectors that do not participate in political life;
   in some sense, populism is, for Laclau, an advancement in what he
   considered a socialist strategy. However, a critical review of Laclau's
   argumentative structure shows that creating a political identity based
   on a discursive process is not enough to make this an instrument of
   emancipation. On the contrary, we propose that for populism to serve
   causes such as radical democracy or the struggle against capitalism, it
   needs to be accompanied by a complementary normative principle. This
   principle is yet to be determined, but we advance a proposal: Nancy
   Fraser's idea of participatory parity, which, due to its negative
   ontology and procedural nature, fits with the basic assumptions of
   Laclau's theory.},
