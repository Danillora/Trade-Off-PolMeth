@article{ WOS:001420868400005,
Author = {Partlova, Petra and Horvathova, Zuzana and Abrham, Josef},
Title = {INTERNATIONAL SECURITY AND TOURISM (CASE STUDY OF THE RUSSIAN-UKRAINIAN
   CONFLICT)},
Journal = {POLITICKE VEDY},
Year = {2024},
Volume = {27},
Number = {3},
Abstract = {Political instability and security threats have a significant impact on
   the development of the world economy and international economic
   relations. The aim of this study is to investigate the effects of
   political-security risks on tourism based on a case study of the
   Russian- Ukrainian conflict. The research combines quantitative data on
   air transport and qualitative analysis of specialist literature with the
   aim of providing a comprehensive view of the issue. The data used in
   this analysis are drawn from the interactive Eurostat tool, which
   provides detailed information on air transport in EU countries, EFTA,
   Turkey and other selected countries (Serbia, Montenegro, North
   Macedonia). This tool makes it possible to analyze the effects of a
   military conflict based on the number of passengers carried and flights
   made. The data covers the years 2018 - 2022. The results of this paper
   will contribute to a deeper understanding of the impact of security
   factors on global tourism and offer recommendations for the formulation
   of effective recovery and adaptation strategies in the post-war period.
   The revitalization of tourism can be one of the factors in the
   development of international trade in services and the economic recovery
   of Ukraine.},
