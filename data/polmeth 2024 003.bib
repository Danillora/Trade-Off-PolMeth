@article{ WOS:001223148400002,
Author = {Souris, Renee Nicole},
Title = {``The Cruelty is the Point{''}: Virtue Ethics and Immigration},
Journal = {POLITY},
Year = {2024},
Volume = {56},
Number = {3},
Pages = {434-455},
Month = {JUL 1},
Abstract = {Existing scholarly debate on immigration is dominated by the concepts of
   justice, right, and duty. Missing from our debates is consideration of
   the virtues that states should cultivate, and more importantly, the
   vices they should avoid toward politically vulnerable migrants. To
   rectify this, I advance a shift toward a virtue ethics of immigration.
   To motivate this shift, I rely on the recent popularization of the
   phrase ``the cruelty is the point{''} that has been used to describe our
   treatment of migrants at the southern border to develop an account of
   cruelty as a distinctly political vice that has become embedded within
   our polity. My project joins Judith Shklar in ``putting cruelty
   first{''} and I extend Shklar's analysis of cruelty to our treatment of
   politically vulnerable migrants as something that we can see and hear in
   the everyday life of our democracy. Moreover, I construe political vices
   as public vices that can become embedded in socio-political groups and
   institutions, rather than as personal vices of individuals. In
   developing my account of cruelty as a political vice, I also distinguish
   it from the more Machiavellian sense in which cruelty could plausibly be
   considered a political virtue.},
