@article{ WOS:001290734800001,
Author = {Mensah, Anna Naa Adochoo},
Title = {Gendered Perspectives on the Recruitment and Use of Child Soldiers in
   Early Warning},
Journal = {CIVIL WARS},
Year = {2024},
Volume = {26},
Number = {3, SI},
Pages = {502-520},
Month = {JUL 2},
Abstract = {The recruitment and use of children by armed groups continue to threaten
   the effective protection of children. Preventive approaches to child
   protection, call for robust early warning mechanisms that do not only
   focus on the recruitment and use of children, but capture the root
   causes and vulnerabilities of both boys and girls for recruitment. A
   comprehensive early warning mechanism for prevention requires a
   gender-sensitive approach. This paper seeks to examine the gender
   dynamics of the recruitment and use of children by armed groups, and how
   that can inform a gender sensitive data collection and analysis to
   improve early warning.},
