@article{ WOS:001368070800001,
Author = {Jou, Willy},
Title = {How do Australians and New Zealanders view China? A comparative study},
Journal = {POLITICAL SCIENCE},
Year = {2024},
Volume = {76},
Number = {2-3},
Pages = {189-208},
Month = {SEP 1},
Abstract = {The Australian and New Zealand economies have long benefitted from
   China's rapid growth, but both countries have recently adjusted their
   relations with China in reaction to its more assertive foreign policy.
   While many studies have examined the causes and implications of these
   changing relationships, relatively few works focus on the attitudes of
   ordinary Australians and New Zealanders towards the rising superpower.
   Through analysis of a cross-national survey, the present study aims to
   fill this gap by exploring factors affecting public opinion, not only on
   general views of China but also on assessments of specific policy
   dimensions of trade, military and technology, as well as views on
   Chinese people. Results show that older respondents are consistently
   more sceptical of China and that perception of Chinese economic power
   contributes to more positive evaluations, whereas perception of Chinese
   military power is less frequently significant. We also note divergences
   between the trans-Tasman neighbours, including more positive attitudes
   shown by New Zealanders towards China and differential impacts of
   variables such as partisanship and patriotism.},
