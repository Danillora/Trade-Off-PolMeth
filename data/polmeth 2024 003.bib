@article{ WOS:001297996100001,
Author = {Meeks, Lindsey},
Title = {The (vice) presidential pivot? Examining Kamala Harris's messaging
   before and after the 2022 midterms},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2024},
Volume = {54},
Number = {4},
Pages = {503-522},
Month = {DEC},
Abstract = {Aspirational vice presidents need to continuously support the president
   yet still make a name for themselves for future elections. To understand
   how US VP Kamala Harris attempted to communicatively enact this balance,
   this study content-analyzed Harris's official VP and personal Twitter/X
   accounts leading up to and following the 2022 midterms to determine how
   she shifted her message emphases before and after the elections and
   across officeholder and personal accounts. Results indicate she played
   her ``party role{''} pre-midterms and ``executive branch role{''}
   post-midterms, championed Joe Biden and Democrats but not herself, and
   used her personal account for campaigning.},
