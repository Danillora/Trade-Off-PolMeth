@article{ WOS:001353437800001,
Author = {Green, Matthew N. and Gawehns, Florian},
Title = {From Congress to the States: Explaining the Emergence and Membership of
   Freedom Caucuses in State Legislatures},
Journal = {STATE POLITICS \& POLICY QUARTERLY},
Year = {2024},
Volume = {24},
Number = {4},
Pages = {370-388},
Month = {DEC},
Abstract = {Since 2017, Republican lawmakers in a growing number of US states have
   formed ideological intraparty organizations, modeled after the US House
   Freedom Caucus, that seek to move state policy further rightward. What
   explains the appearance of these state freedom caucuses, and what kinds
   of lawmakers are more likely to join them? We show that the creation of
   these caucuses was initially motivated by concerns that state-level
   legislative Republican parties are too ideologically heterogeneous but
   has since been driven by conservative entrepreneurs seeking to spread
   freedom caucuses nationally. We also provide evidence that conservative
   legislators are more likely to join a new state freedom caucus, as one
   would expect, but also that, in a few states, lawmakers who are more
   electorally vulnerable lawmakers or lack internal influence have also
   been more likely to join. These findings underscore how state-level
   ideological caucuses can appeal to members' multiple goals and serve as
   instruments of vertical polarization in a federal system.},
