@article{ WOS:001267271600015,
Author = {Kissinger, Henry},
Title = {The Search for Stability},
Journal = {GEOPOLITICAS-REVISTA DE ESTUDIOS SOBRE ESPACIO Y PODER},
Year = {2024},
Volume = {15},
Number = {1},
Pages = {275-293},
Abstract = {In the following work Henry Kissinger reflects on the German question
   and the challenge to Western Europe and the United States posed by the
   November 27, 1958 ultimatum issued by Soviet leader Nikita Khrushchev.
   The ultimatum called for ceding control of access to Berlin to the
   German Democratic Republic and the demilitarization of the western part
   of the city. Moscow even granted a six-month deadline to France, the
   United Kingdom and the United States to establish their own relations
   with the GDR. With the aim of ``seeking stability{''} and balance within
   the West and with regards to the Soviet Union, Kissinger examines the
   importance of diplomacy and the scenario of possible and desired
   options. In doing this, Kissinger invokes action principles and
   behavioral logics that maximize the U.S. position while reducing the
   crisis context generated by this situation.},
