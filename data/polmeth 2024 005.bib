@article{ WOS:001331214600001,
Author = {Jarviniemi, Jon},
Title = {The anatomy of populist communication during COVID-19},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2024},
Month = {2024 OCT 9},
Abstract = {Times of crisis have been argued to function as a springboard for
   populist communication. But what about a crisis that triggers a
   rally-around-the-flag effect, which causes a period of consensus-seeking
   politics and increased trust in institutions and experts? The study
   delves into the impact of the rally-around-the-flag effect triggered by
   the outbreak of the COVID-19 pandemic on the use of populist
   communication in Finland. The findings reveal a notable and temporal
   decrease in populist communication across party lines and various topics
   during the initial year of the pandemic. The study provides novel
   empirical insights into the anatomy of populist communication during the
   pandemic's initial year, highlighting that a crisis not only serves as a
   window of opportunity for populism but can also act as a deactivator of
   such communication.},
