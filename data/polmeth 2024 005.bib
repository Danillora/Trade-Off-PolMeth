@article{ WOS:001274890700011,
Author = {Pranchere, Jean-Yves},
Title = {CAN LIBERALISM AND DEMOCRACY DIVORCE?},
Journal = {TOCQUEVILLE REVIEW},
Year = {2024},
Volume = {45},
Number = {1},
Pages = {13-34},
Month = {JUL 1},
Abstract = {The current deconsolidation of liberal democracies has brought to the
   surface a discourse which was current in the 19th century but seemed
   refuted by the experience of 20th century totalitarianism: the
   affirmation of the absence of a necessary link between liberalism and
   democracy and, consequently, of the possibility of their being placed in
   antithesis. The diffusion of the phrases ``illiberal democracy{''} and
   ``authoritarian liberalism{''} illustrates the success of the idea
   according to which we are witnessing, after a long marriage, a divorce
   of liberalism and democracy. This idea neglects, however, that a
   dissociation of liberalism and democracy would mean a regression of
   both, their involution towards embryonic or underdeveloped forms.
   Certainly, because the liberal idea and the democratic idea do not have
   the same center of gravity, their link is dialectical; but it is only
   dialectical because it is based on conceptual solidarity.},
