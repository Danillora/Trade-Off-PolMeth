@article{ WOS:001162054400001,
Author = {Stockemer, Daniel and Reidy, Theresa},
Title = {Introduction: Pandemic and Post-Pandemic Publication Patterns in
   Political Science},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
