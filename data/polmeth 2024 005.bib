@article{ WOS:001376755700001,
Author = {Martigny, Vincent and Peters, B. Guy},
Title = {All the President's Men: Comparing the Secretary General of the French
   Presidency and the US Chief of Staff in an Era of Presidentialization},
Journal = {GOVERNMENT AND OPPOSITION},
Year = {2025},
Volume = {60},
Number = {2},
Pages = {477-495},
Month = {APR},
Abstract = {This article compares the Secretary General of the Elys \& eacute;e
   Palace and the US Chief of Staff,central political advisors to the
   French and US presidents. Our aim is twofold. Firstly, weidentify the
   precise roles of these advisors. By mapping their respective powers, we
   dem-onstrate their importance in presidential decision-making. By
   examining what the FrenchElys \& eacute;e Secretary General and the US
   White House Chief of Staff have in common and howthey differ, we develop
   a comparative understanding of the mechanisms of the
   presiden-tialization of political executives. We show that the
   similarities of the two offices are linkedto the ongoing
   presidentialization of the French and American political systems, which,
   bygiving greater power to heads of state, also strengthens their
   advisors. This commonalitydoes not rule out marked differences between
   these officials, demonstrating that presiden-tialization takes distinct
   forms, reflecting distinct political cultures as well as different
   bal-ances of power within each institutional system.},
