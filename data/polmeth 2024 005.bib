@article{ WOS:001319340400001,
Author = {Kwong, Ying-ho and Wong, Mathew Y. H.},
Title = {Gambling Economy and Social Protests in Macao: A Happy Problem?},
Journal = {POLITICAL STUDIES REVIEW},
