Author = {Cunha, Bruno Queiroz and Silva, Mauro Santos},
Title = {Incredibly Committed? Assessing Regulatory Agency Financial Autonomy in
   Brazil From a Critical Perspective},
Journal = {JOURNAL OF POLITICS IN LATIN AMERICA},
