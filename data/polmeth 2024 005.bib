@article{ WOS:001335520400001,
Author = {Talamayan, Fernan and Candelaria, John Lee},
Title = {Populist desires, nostalgic narratives: the Marcos golden age myth and
   manipulation of collective memories on YouTube},
Journal = {ASIAN JOURNAL OF POLITICAL SCIENCE},
