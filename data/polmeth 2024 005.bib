@article{ WOS:001434363700003,
Author = {Koestem, Burc},
Title = {The Consummative Mood of Authoritarianism: The Affective and Material
   Politics of Megaprojects in Turkey},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2024},
Volume = {36},
Number = {4},
Pages = {447-472},
Month = {OCT 1},
Abstract = {This essay builds on the work of Gilles Deleuze and Felix Guattari to
   outline a consummative mood of authoritarian politics in Turkey. It
   defines the consummative mood as the fetishization of efficacy and
   delivery, the desire to realize value and bring cycles of social
   production to fruition. Focusing on the operations of the consummative
   mood across the construction of megaprojects in Istanbul's peripheries,
   this essay highlights how consummation brings together fantasies of
   national resurgence and sovereignty on the one hand and economic growth
   and autonomy on the other. Conclusively, it argues that while several
   scholars have traced the political-economic transformations Turkey has
   experienced throughout the governing Justice and Development Party's
   reign, zooming in on consummation can help us connect these accounts to
   an affective story, focused on bodies, affects and subjectivity.},
