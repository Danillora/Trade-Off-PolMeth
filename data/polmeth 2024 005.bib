@article{ WOS:001342497100002,
Author = {Monarde, Simon Timichelle Gonzalez},
Title = {The diaspora of the Chilean revolutionary left in the exile letters of
   Luis Vitale between 1974 and 1980},
Journal = {IZQUIERDAS},
Year = {2024},
Volume = {53},
Pages = {20-29},
Month = {SEP},
Abstract = {This research analyzes the correspondence of the Chilean-Argentine
   historian Luis Vitale during his exile in Germany and Venezuela between
   1974 and 1980. Here we identify a network of exchange composed of
   intellectuals, militants and political leaders of the Chilean left who
   were articulated in the context of the reconfiguration of political
   parties and alliances during the dictatorship. We paid attention to the
   reconstruction of the political organizations of Chilean socialism, in
   relation to the strategies of the revolutionary left to create an
   organization that would allow the unification of its different political
   tendencies. As a result, we reconstructed the political fabric in which
   the idea of forming a platform for discussion and convergence of the
   revolutionary left emerged, through the Center for Social Studies on the
   Latin American Reality created in Caracas during 1978.},
