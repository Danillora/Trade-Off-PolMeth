Author = {Adiprasetio, Justito and Rahmawan, Detta and Wibowo, Kunto Adi and
   Aliifa, Dzikrifa Ningtyas and Hartanto, Rudi Dwi},
Title = {Shrinking civic space and the media: How Indonesian media frame
   environmental issues},
