@article{ WOS:001367407800001,
Author = {Zhang, Xu and Yu, Mengmeng},
Title = {The Labor Theory of Value in the Context of Artificial Intelligence: A
   Historical and Classical Review},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2024},
Volume = {14},
Number = {4},
Pages = {519-549},
Month = {OCT 1},
Abstract = {In recent years, research on the labor theory of value in the context of
   artificial intelligence has become a focus of social attention. A new
   round of discussion, centered on topics including ``value creation in
   the context of artificial intelligence,{''} ``the applicability of the
   labor theory of value in the era of artificial intelligence{''} and
   ``the development trend of artificial intelligence and human labor{''}
   is under way in academic circles. The core issue is whether the
   classical labor theory of value has become obsolete in the era of
   artificial intelligence. At the same time, consensus remains lacking on
   a number of major theoretical issues. In light of this the present
   study, which sets out to trace history on a problem-awareness basis,
   finds that since the 1980s the economics community has responded
   scientifically to the assertions, made in the context of the new
   technological revolution, that ``machines create value{''} and that
   ``the classical labor theory of value is no longer valid.{''} Over the
   years, new ideas and explanations have been put forward. Reviewing the
   classics, and basing itself on the theoretical framework of Marxist
   political economy, this article re-examines the significance of the
   labor theory of value in the era of artificial intelligence, with a view
   to deepening understanding of Marx's labor theory of value, clarifying
   doubts concerning its validity, and enhancing the practical explanatory
   power of its content.},
