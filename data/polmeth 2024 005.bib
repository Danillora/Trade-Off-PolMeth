@article{ WOS:001228652100001,
Author = {Benuyenah, Vic and Mustafa, Mona},
Title = {Conceptualising the identity of Third Culture Adults (TCAs) in the GCC:
   implications for labour markets and organisations},
Journal = {NATIONAL IDENTITIES},
