@article{ WOS:001152091800001,
Author = {Makki, Muhammad and Iftikhar, Waseem and Yamin, Tughral},
Title = {`(Re)ordering' North Waziristan, Pakistan: Post-Conflict Transition,
   Borderland Dynamics, and Spatial Identities},
Journal = {CIVIL WARS},
Year = {2024},
Volume = {26},
Number = {2},
Pages = {354-373},
Month = {APR 2},
Abstract = {North Waziristan is emerging from an episode of terrorism that lasted
   for decades. Critically examining the dynamics of borderlands, this
   paper explicates the intersection of informal cross-border trade with
   the local economy and livelihoods. These socio-economic patterns created
