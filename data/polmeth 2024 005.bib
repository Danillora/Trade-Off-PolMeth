@article{ WOS:001332889600001,
Author = {Ryan, Kevin},
Title = {The power of critique in the time of emergency: on normative fiction and
   critical fiction},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2025},
Volume = {18},
Number = {1, SI},
Pages = {39-57},
Month = {JAN 2},
Abstract = {This article begins by reflecting on `crisis and democracy' as a
   normative fiction that sustains extant self-other-world relations. An
   alternative is to frame the present as `the time of emergency', which
   implies scope for `emergent' possibilities. In teasing out such
   possibilities, the article makes a case for critical fiction as a way of
   radicalising our relationship to the present, looking initially to
   Foucault's writings on critique before turning to the work of da Silva,
