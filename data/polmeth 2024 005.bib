@article{ WOS:001278013800001,
Author = {Thomeczek, Jan Philipp},
Title = {The Voting Potential of Bündnis Sahra Wagenknecht},
Journal = {POLITICAL STUDIES REVIEW},
Year = {2024},
Month = {2024 JUL 26},
Abstract = {A new challenger seeks to enter the German party system: B \& uuml;ndnis
   Sahra Wagenknecht (BSW). With her new party, former Die Linke politician
   Sahra Wagenknecht combines a left-authoritarian profile (economically
   left-leaning, but culturally conservative) with anti-US, pro-Russia and
   anti-elitist stances. This article provides the first large-n academic
   study of the voter potential of this new party by using a
   quasi-representative sample (n = 6,000) drawn from a Voting Advice
   Application-like dataset that comes from a website designed to explore
   the B \& uuml;ndnis Sahra Wagenknecht's positions. The results show that
   congruence with foreign policy positions and anti-elitism are strong
   predictors of the propensity to vote for the B \& uuml;ndnis Sahra
   Wagenknecht. In contrast, social/welfare and immigration policies are
   less predictive for assessing the party's potential. Among the different
   socio-demographic groups, the B \& uuml;ndnis Sahra Wagenknecht has a
   strong potential among baby boomers, the less educated and East Germans.
   Regarding party voters, the B \& uuml;ndnis Sahra Wagenknecht is
   favoured by supporters of some minor parties like dieBasis, Freie W \&
   auml;hler and Die PARTEI, but also non-voters. Among the established
   parties, the party's potential is high among Die Linke voters and, to a
   lesser extent, voters of the Social democrats (SPD) and Alternative for
   Germany (AfD). A potential below the average is reported for the
   supporters of the Liberals (FDP) and Christian Democrats (CDU/CSU) and
