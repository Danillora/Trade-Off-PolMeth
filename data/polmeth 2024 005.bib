@article{ WOS:001282497900001,
Author = {Coggins, K. Elizabeth and Gruschow, Kathleen J.},
Title = {Toward a Shared Ideological Currency: Ideological Affective Polarization
   \& the Changing Structure of Ideology in the US},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2024},
Volume = {77},
Number = {4},
Pages = {1364-1380},
Month = {DEC},
Abstract = {Much scholarly attention has focused on partisan affective polarization,
   a growing animosity between Democrats and Republicans in the mass
   public. Less notice has been given to affective polarization among the
   ideological teams, liberals, and conservatives. Yet, we suggest that
   given ideology's particular influence in U.S. politics, ideological
   affective polarization has important and distinct consequences. We begin
   by offering evidence that ideological affective polarization both exists
   and has increased sharply in recent years. We theorize that this shift
   alters the way individuals come to form attachments to ideological
   identities, as well as the structure and tenor of ideology in the United
   States.},
