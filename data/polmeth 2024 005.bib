@article{ WOS:001266041800004,
Author = {Miroiu, Andrei and Alecu, Ana Raluca},
Title = {Mali: Conflict, Social Order and the Crime-Terror Nexus},
Journal = {CONFLICT STUDIES QUARTERLY},
Year = {2024},
Number = {48},
Month = {JUL},
Abstract = {This article examines the relationship between conflict, social order,
   and the crime-terror nexus, with a focus on non-state armed groups in
   Mali, particularly Jama'at Nasr al-Islam wal Muslimin (JNIM). Using
   anarchist political theory, the study challenges the view of these
   groups as purely chaotic, instead suggesting their primary aim is to
   establish social order. The crime-terror nexus is explored to understand
   its role in new governance structures created by these groups. The study
   underscores the importance of re-evaluating the frameworks used to
   interpret armed groups, emphasizing their role in creating social order
   and stability in conflict zones. The research offers insights into the
   complex dynamics of armed groups in Mali and encourages further
   investigation into their influence on regional stability.},
