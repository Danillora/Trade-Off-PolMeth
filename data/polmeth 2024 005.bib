@article{ WOS:001397989000005,
Author = {Mennecke, Martin},
Title = {The UN Human Rights Council, Denmark, and R2P-the Perfect Match?},
Journal = {NORDIC JOURNAL OF HUMAN RIGHTS},
Year = {2024},
Volume = {42},
Number = {4, SI},
Pages = {463-483},
Month = {OCT 1},
Abstract = {The United Nations Human Rights Council (HRC) is under-researched and
   undervalued as a forum for the implementation of the Responsibility to
   Protect (R2P) norm. This article analyses the HRC's potential and, as a
   case study, how Denmark approached R2P and atrocity prevention during
   its first membership of the HRC (2019-21). Despite having a longstanding
   positive record on advancing R2P, Denmark did not make the norm an
   explicit priority for its HRC term. This article examines whether,
   despite that approach, Denmark did contribute actively to the
   implementation of R2P on the HRC. Special attention is given to
   Denmark's participation in the Universal Periodic Review, Special
   Sessions of the HRC, interactions with special procedures under the HRC,
   and accountability measures adopted by the HRC. The article concludes
   that Denmark did advance R2P during its first membership and suggests
   lessons to be learnt for the upcoming Danish membership of the UN
   Security Council in 2025-2026.},
