@article{ WOS:001477709000012,
Author = {Rojas, Jesus Ignacio Delgado},
Title = {COSMOPOLIS AT WAR. BROKEN PROMISES OF THE COSMOPOLITAN IDEAL IN TIMES OF
   BARBARISM},
Journal = {REVISTA INTERNACIONAL DE PENSAMIENTO POLITICO},
Year = {2024},
Number = {19},
Abstract = {This paper aims to insist on the inevitable connection between democracy
   as a system of government and the idea of citizenship as a bond of
   belonging to a state. It aims to highlight how the modern model of
   citizenship demands new elements to guarantee fullness and equality of
   citizens' rights, to avoid unjust exclusions and to advance towards
   formulas of integration in current context of globalization. The notion
   of cosmopolitan citizenship contemplated as a necessary corollary of the
   universality of human rights, some contemporary proposals are examined
   to discuss challenges facing cosmopolitan ideal in present international
   scenarios of war conflict and global risk.},
