@article{ WOS:001280831100001,
Author = {Burke, Thomas F. and Barnes, Jeb},
Title = {The Post-Brown Era in Judicial Policymaking},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
Year = {2024},
Volume = {22},
