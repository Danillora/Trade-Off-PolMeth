@article{ WOS:001434363700007,
Author = {Kapoor, Ilan and Fridell, Gavin and Sioh, Maureen and de Vries, Pieter},
Title = {Of Extensions and Subtractions},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2024},
Volume = {36},
Number = {4},
Pages = {497-506},
Month = {OCT 1},
Abstract = {Authors Ilan Kapoor, Gavin Fridell, Maureen Sioh, and Pieter de Vries
   engage with the interventions made by Benjamin Selwyn, Stephen Healy,
   and Stella Gaon in a symposium responding to their book, Global
   Libidinal Economy. The authors begin by setting out the main arguments
   of the global libidinal economy approach (GLE), underlining how the
   libidinal (unconscious desire) is constitutive of political economy
   while also distinguishing GLE from both (neo)classical and Marxist
   political economy. They then address the ways in which both Selwyn and
   Healy extend GLE arguments, affirming, elaborating, but also sometimes
   cautioning against how GLE can be applied to elite development theory,
   the historical genealogy of capitalism, and alternatives to capitalism.
   The authors end by responding to Gaon's critique, which they contend
   often simplifies if not misreads the claims of the book, mainly because
   of failing to account for the split subject at its core.},
