@article{ WOS:001238798800007,
Author = {Farram, Steven},
Title = {Pahlawan, Pengkhianat, Atau Penjahat (Hero, Traitor, or Villain): A
   Personal Journey Through Indonesian History},
Journal = {POLITICS AND GOVERNANCE},
Year = {2024},
Volume = {12},
Abstract = {This article concerns Indonesian heroes, traitors, and villains from
   different regions and eras. The factors influencing the categorisation
   of individuals as heroes or villains are examined. Examples include
   regional leaders who opposed the Dutch East India Company or
   collaborated with it. Similar cases are examined from the period of the
   Netherlands Indies colonial state. Also discussed are nationalists who
   were members of the Indonesian Communist Party, and people now deemed
   heroes who collaborated with the Japanese during the Second World War.
   Next for consideration are individuals involved in Confrontation with
   Malaysia and the occupation of East Timor. The last cases come from the
   world of popular music and show how performers idolised by fans can be
   considered villains by others.},
