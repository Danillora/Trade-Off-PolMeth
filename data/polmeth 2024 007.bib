@article{ WOS:001470339300018,
Author = {Teixeira, Carlos Savio and Medeiros, Tiago},
Title = {Modern institutions between the deconstructionist project and the
   constructivist project of political thought},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2024},
Volume = {12},
Month = {JUL-DEC},
Abstract = {This article identifies and analyzes two theoretical perspectives on
   power and institutions. Both are understood as philosophical
   manifestations of the imagination of the May 1968 generation. The first
   is called Deconstructionism. It is characterized by articulating a
   negativist conception of the institutional phenomenon and of all
   manifestations of power. Its approach translates into a `cratophobia'
   that sees in every institutionalization of modernity an oppressive
   device. The second view is called Constructivism. It attributes to power
   and institutions the status of provisional arrangements for regulating
   social relations, but which can also become levers for transforming the
   economic and political structure of societies. Once the contrast is
   established, we argue that deconstructionism left unpreparedness for
   dealing with the inevitable operations required by the institutions that
   organize our material and symbolic life, becoming a source of
   intellectual and moral disorientation. Constructivism, on the other
   hand, proves capable of juxtaposing analytical realism with programmatic
   imagination of alternatives, although it remains a less known andless
   disseminated perspective among the generations that inherited May 1968.
   The article reveals, by way of conclusion, a difference in understanding
   the relationship between action and structure, mediated by institutions,
   through a typological panorama of political philosophy.},
