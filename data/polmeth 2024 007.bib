@article{ WOS:001257046800001,
Author = {Jabbour, Elias and Boer, Roland},
Title = {Ignacio Rangel: Thinker of Scientific Socialism, Originator of the
   ``Projectment Economy{''}},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2024},
Volume = {14},
Number = {2},
Pages = {303-321},
Month = {APR 2},
Abstract = {Ignacio Rangel stands out as the most creative and original Brazilian
   economist of the 20th century. He is a Marxist known for challenging the
   two dominant schools of thought in Brazil and Latin America-neoclassical
   and structuralist. His work is regaining relevance as the political
   economy he developed offers relevant answers to contemporary Brazilian
   challenges. Moreover, one of his most significant works, ``Elements of
   Projectment Economy,{''} has become important in assisting the
   understanding of the historical form emerging in China since 1978 as a
   new socio-economic formation. Rescuing and developing Rangel's
   contributions to scientific socialism may be seen as a response to Xi
   Jinping's call to open up new horizons for Marxist political economy in
   light of developments in China and the world.},
