@article{ WOS:001340348300004,
Author = {Han, Seungwoo},
Title = {Housing wealth and political outcomes: a multi-dimensional analysis at
   the local level in South Korea},
Journal = {JAPANESE JOURNAL OF POLITICAL SCIENCE},
Year = {2024},
Volume = {25},
Number = {2},
Pages = {75-101},
Month = {JUN},
Abstract = {The current study explores the relationship between housing wealth and
   political outcomes at the regional level in South Korea, providing
   insights that extend beyond the scope of previous research, which has
   primarily concentrated on individual or macro levels. It focuses
   specifically on district-level dynamics within the unique context of
   South Korea to uncover the underlying mechanisms of housing politics.
   The findings indicate a clear trend: as the average price of apartments
   - a key indicator of housing wealth - rises in a district, there is an
   increased tendency for residents to support candidates from the
   conservative party. Furthermore, the data-driven analysis highlights the
   significance of housing wealth amidst various regional characteristic
   variables, particularly in its capacity to distinguish the target
   regions. By employing comprehensive data and a multi-dimensional
   analytical approach, this study unravels the intricate relationship
   between housing wealth and political outcomes within a regional context.
   In doing so, it greatly enhances our understanding of the impact of
   housing wealth on political outcomes, bridging the gap between
   individual behaviours and national trends, and underscoring the pivotal
   role of housing wealth in shaping regional political dynamics.},
