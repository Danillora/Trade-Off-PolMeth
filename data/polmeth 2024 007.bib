@article{ WOS:001400134300003,
Author = {Bellio, Alfonsina},
Title = {Prophetic femininities, magic and subversion},
Journal = {LABORATOIRE ITALIEN-POLITIQUE ET SOCIETE},
Year = {2024},
Number = {33},
Abstract = {Euro-Mediterranean history is full of figures who, over time and in
   countless variations, have embodied forms of direct contact with the
   non-seen. Transcendent or immanent, representations of the supernatural
   are to be found in the imagination, in mythical tales, in literature,
   the arts and ritual practices, as well as in the mediators through which
   each society transmits its cultural content. In this vast panorama, the
   feminine often appears as a primordial symbolic and mythopoeic element,
   at the centre of all sorts of issues. The aim of this research is to
   analyse the phenomena of mediumship and clairvoyance, spontaneous or
   supposed, that have characterised certain sections of the population -
   most of them women - in Southern Italy. Starting from a reconsideration
   of the notion of prophecy, and based on an ethnographic survey, the text
   questions the contemporary religious and spiritual landscape, forms of
   rebellion against official normativity, and the relationship between
   established religions and non-standard spiritualities.},
