@article{ WOS:001470339300007,
Author = {Machado, Betieli da Rosa Sauzem and Hermany, Ricardo},
Title = {The local and social democratic empower-ment in multilevel governance:
   the municipalities in public policies from the social councils},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2024},
Volume = {12},
Pages = {211-256},
Month = {JUL-DEC},
Abstract = {The work focuses on the level of consolidation of social councils at the
   local level and its impact on multi-level governance. In this sense, the
   question arises: what is the level of consolidation of social councils
   at the local level and how do they impact citizen empowerment and
   participation in governance? The methods are hermeneutic procedure and
   deductive approach, and the technique of bibliographic research. The
   work is developed in three moments: first, multilevel governance and
   democracy associated with the control of Public Administration will be
   conceptualized; public policies, social control and popular
   participation will be analyzed; and, finally, the municipal social
   councils and the ``Research on municipal management 2021: survey on
   social councils{''} developed by the Court of Auditors of Rio Grande do
   Sul will be studied. In concluding notes, the deliberative or public
   policy councils are considered true instruments for the effective
   implementation and supervision of multiple policies at the local level,
   which serve as an instrument for the promotion of citizenship and the
   exercise of democracy. Thus, municipal social councils contribute to the
   strengthening of popular participation and social control in multilevel
   governance by providing representation of civil society, promoting
   transparency and accountability, facilitating dialogue and deliberation,
   strengthening participatory democracy, and integrating the different
   levels of government. However, there are challenges to improving the
   mechanisms and strengthening these institutions.},
