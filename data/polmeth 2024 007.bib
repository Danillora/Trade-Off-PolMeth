@article{ WOS:001390217400004,
Author = {Soriano Flores, Jose Jesus},
Title = {The Legalization of The Human Right to The City as A Starting Point in
   The Design of Public Policies for The Twenty First Century},
Journal = {REVISTA MEXICANA DE ANALISIS POLITICO Y ADMINISTRACION PUBLICA},
Year = {2024},
Volume = {13},
Number = {26},
Month = {JUL-DEC},
Abstract = {The definition of the right to the city as a normative concept still has
   many uncertainties that hinder its positive assessment, or in some
   cases, its administrative and jurisdictional effectiveness in the few
   legal systems that have recognized it. It is therefore necessary, from a
   current and progressive human rights perspective, to identify and
   develop a series of minimum elements based on an interdisciplinary
   study, but with a view to developing the legal side of the concept. In
   this sense, the article identifies and develops some of those elements
   that should be taken into account in the process of legalization and
   design of public policies.},
