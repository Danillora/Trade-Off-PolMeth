@article{ WOS:001421758100002,
Author = {Mat, Bakri and Khalid, Amna},
Title = {China's Energy Security Strategy in Central Asia: A BRI and Green Energy
   Perspective (2019-2024)},
Journal = {CHINA QUARTERLY OF INTERNATIONAL STRATEGIC STUDIES},
