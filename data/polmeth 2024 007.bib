@article{ WOS:001273851800001,
Author = {Mccullagh, David},
Title = {`It is only by political means that we can hope for … success': Éamon de
   Valera's long climb back to power, 1922-32},
Journal = {IRISH POLITICAL STUDIES},
Year = {2024},
Volume = {39},
Number = {2, SI},
Pages = {298-319},
Month = {APR 2},
Abstract = {In January 1922, \& Eacute;amon de Valera lost the leadership of Irish
   nationalism. In opposing the Treaty signed in London the previous month,
   he painted himself into a corner, committing himself to oppose the
   settlement, to refuse to recognise the Irish Free State, and never to
   take the Oath of Allegiance required to enter the D \& aacute;il. This
   position precluded the political path which was the only course
   available after the military defeat of the anti-Treaty cause. If de
   Valera was to have a political future, he would have to find a way to
   retreat from this stance, while maintaining the support of as many
   anti-Treatyites as possible and widening his electoral appeal beyond
   that minority. His tactical withdrawal would take a decade, featuring
   the rebirth of Sinn F \& eacute;in as an anti-Treaty electoral party,
   the formation of Fianna F \& aacute;il as a movement more amenable to
   his control, and acceptance of the oath to enter the Free State D \&
   aacute;il. The long climb back to political power culminated with his
   election as President of the Executive Council in March 1932. He had not
   only secured his own political future; he had helped establish a
   political system which would endure for eight decades.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
