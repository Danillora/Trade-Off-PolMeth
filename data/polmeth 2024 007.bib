@article{ WOS:001209146800001,
Author = {Baar, Monika and Mladenov, Teodor and Titterton, Mike},
Title = {Rethinking ``Transition{''} in Central and Eastern Europe and the Former
   Soviet Union Through the Lens of Disability},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2025},
Volume = {72},
Number = {3},
Pages = {211-215},
Month = {MAY 4},
Abstract = {The concept of disability can serve as a valuable lens through which a
   host of established subjects can be revisited in a new light. These
   include welfare and ``welfare dependency,{''} unemployment, gender
   relations, social movements, and grassroots activities, the impact of
   international norms such as human rights and their application in local
   contexts, and the impact of neoliberal reforms. Despite this potential,
   disability, unlike the concepts of class, ``race,{''} sexuality, and
   ethnicity, has yet to make inroads into the field of postsocialist
   studies. It is precisely this lacuna which our special issue seeks to
   address.},
