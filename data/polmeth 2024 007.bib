@article{ WOS:001296405300001,
Author = {Nownes, Anthony J. and Vincent, Rachel},
Title = {Who Gives to American Trade and Professional Association Political
   Action Committees and Why?},
Journal = {AMERICAN POLITICS RESEARCH},
Year = {2025},
Volume = {53},
Number = {1},
Pages = {55-68},
Month = {JAN},
Abstract = {This paper asks: Why do people contribute to American trade and
   professional association political action committees (PACs)? Our data,
   which come from an original survey of donors during the 2021-2022
   federal election cycle, indicate that trade and professional association
   PAC donors are not like other campaign donors. Specifically, unlike
   donors to candidates, who appear to be motivated primarily by purposive
   benefits and ideology, these PAC donors are motivated largely by
   material benefits. Our data also showthat trade and professional
   association PACs are constraining forces in American politics. While
   donors tend to hue to a strict ideological lane, PACs do not. In short,
   PACs are balanced in their giving, while individual donors are not.},
