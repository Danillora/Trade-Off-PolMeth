@article{ WOS:001273043800001,
Author = {Cheng, Enfu and Lu, Xia},
Title = {Building a Community with a Shared Future for Humanity through China's
   Six Global Initiatives},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2024},
Volume = {14},
Number = {2},
Pages = {157-175},
Month = {APR 2},
Abstract = {The article has its roots in the concept of a community with a shared
   future for humanity, a Chinese innovation aimed at realizing the
   development and prosperity of human societies. This concept rests on an
   exploration, based in Marxism and sinicized Marxism, of how world
   history has evolved, and embodies six global initiatives as fundamental
   measures to respond to the real needs of world development at the
   current stage. Specifically, the Belt and Road Initiative, based on a
   new pattern of cooperation, strives to build a new model of common
   prosperity in both economic and cultural terms for countries around the
   world. The Global Development Initiative aims to upgrade the development
   agenda and formulate new guidelines for the common socio-economic
   development of all countries. The Global Security Initiative seeks to
   eliminate the old-style mentality formed by the zero-sum game thinking
   of the Cold War epoch, and to formulate a new conception of shared
   security for the international community. The Global Civilization
   Initiative sets out to disprove the myths of Western civilization and to
   develop a new concept of mutual learning between civilizations, while
   the Global AI Governance Initiative aims to build consensus and on that
   basis, to shape the common governance of scientific and technological
   progress.},
