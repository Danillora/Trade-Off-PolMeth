@article{ WOS:001171952100001,
Author = {Bailey, Christine m. and Collins Jr, Paul m. and Rhodes, Jesse h. and
   Rice, Douglas},
Title = {The Effect of Judicial Decisions on Issue Salience and Legal
   Consciousness in Media Serving the LGBTQ plus Community},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2025},
Volume = {119},
Number = {1},
Pages = {108-123},
Month = {FEB},
Abstract = {Scholars have long questioned whether and how courts influence society.
   We contribute to this debate by investigating the ability of judicial
   decisions to shape issue attention and affect toward courts in media
   serving the LGBTQ+ community. To do so, we compiled an original database
   of LGBTQ+ magazine coverage of court cases over an extended period
   covering major decisions, including Lawrence v. Texas (2003), Goodridge
   v. Massachusetts Department of Public Health (2003), and Lofton v.
   Secretary of Department of Children \& Family Services (2004). We argue
   these cases influence the volume and tone of LGBTQ+ media coverage.
   Combining computational social science techniques with qualitative
   analysis, we find increased attention to same-sex marriage after the
   decisions in Lawrence, Goodridge, and Lofton, and the coalescence of
   discussions of courts around same-sex marriage after Lawrence. We also
