@article{ WOS:001434363700004,
Author = {Selwyn, Benjamin},
Title = {Global Libidinal Economy and Capitalist Development: Methodological
   Challenges and Intellectual Promises},
Journal = {RETHINKING MARXISM-A JOURNAL OF ECONOMICS CULTURE \& SOCIETY},
Year = {2024},
Volume = {36},
Number = {4},
Pages = {473-479},
Month = {OCT 1},
Abstract = {The book Global Libidinal Economy argues that the libidinal-the site of
   unconscious desires-plays a constitutive role in global political
   economy. This role includes capitalism's profound hold on our ways of
   thinking and being, despite many of us recognizing its profoundly
   damaging impacts upon our socio-environmental world. This review essay
   suggests that the potential of the GLE approach can be maximized by
   clarifying a GLE methodology. It notes two areas for further GLE
   investigation: elite development theory and the formation of modern
   procapitalist economic thinking.},
