@article{ WOS:001356390000001,
Author = {Dunin-Wasowicz, Roch and Craciun, Claudiu and Osuna, Jose Javier Olivas
   and Rammelt, Henry P.},
Title = {Bridging the Divide Between Structural and Actor-Oriented Explanations
   of Populism: A Research Agenda for the Study of Populist Euroscepticism
   Through a Territorial Perspective},
Journal = {POLITICAL STUDIES REVIEW},
Year = {2025},
Volume = {23},
Number = {2},
Pages = {622-634},
Month = {MAY},
Abstract = {This article explores the intersection of structural and actor-oriented
   explanations of populism through a territorial perspective, focusing on
   populist Euroscepticism. It builds on findings from the `Local
   Mobilisation Against the EU. Territorial Dimensions of Populist
   Euroscepticism' (EULOC) project, which examined local drivers and
   manifestations of populism and Euroscepticism across Europe. The article
   highlights the importance of analysing both the supply and demand sides
   of populist Euroscepticism, emphasising the role of geographic
   inequalities, local socio-economic trajectories and collective
   identities. By integrating national, regional and local case studies,
   the article aims to bridge the divide between structural and
   actor-oriented approaches, offering a more comprehensive understanding
   of populist Euroscepticism. This approach seeks to prevent reductionist
   arguments and contributes to broader discussions on political
   participation, representation and the dynamics of populist movements in
   Europe. The article underscores the need for nuanced, multi-layered
   research that considers the unique socio-political dynamics of different
   regions and the impact of local contexts on the development of
   Eurosceptic sentiments.},
