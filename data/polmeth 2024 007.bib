@article{ WOS:001326651500001,
Author = {Weyland, Kurt},
Title = {Opposition to populist backsliding: conditions, limitations, and
   opportunities},
Journal = {DEMOCRATIZATION},
Year = {2025},
Volume = {32},
Number = {1},
Pages = {1-26},
Month = {JAN 2},
Abstract = {This article argues that the incipient literature on opposition to
   populism's authoritarian backsliding needs to embed its agency-focused
   approach in fundamental institutional and structural factors that
   heavily condition the chances for opposition success. Above all, the
   degree of strength that the preexisting institutional framework
   commands; and the contextual conditions that affect populist leaders'
   mass support; set basic parameters for opposition choices and their
   impact. These factors account much better for opposition success or
   failure than the tactical and strategic decisions that populists'
   adversaries make. The paper substantiates these arguments through brief
   comparative examinations of a wide range of populist chief-executives,
   their political fate, and the opposition's experiences. Covering both
   rightwing and leftwing populism, this analysis focuses primarily on
   Latin America.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
