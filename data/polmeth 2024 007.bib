@article{ WOS:001328308100001,
Author = {Li, Zhengtu and Xu, Zijian},
Title = {The Practice of Common Prosperity with Chinese Characteristics:
   Institutional Guarantees, Intellectual Impetus and Scientific Theories},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2024},
Volume = {14},
