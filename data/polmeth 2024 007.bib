@article{ WOS:001294912800001,
Author = {Monsivais-Carrillo, Alejandro},
Title = {Deceitful autocratization: Subverting democracy through electoral reform
   in Mexico},
Journal = {LATIN AMERICAN POLICY},
Year = {2024},
Volume = {15},
Number = {3},
Pages = {486-508},
Month = {SEP},
Abstract = {Antipluralist incumbents often resort to opaque and deceptive strategies
   to subvert democracy. One such strategy involves devising formal
   institutions that pretend to promote and deepen democracy while
   enforcing authoritarian rule instead. This study labels such a strategy
   as ``deceitful autocratization{''} and argues that a case that
   illustrates how antipluralist presidents promote it is the electoral
   reform launched by Andres Manuel Lopez Obrador in Mexico. The analysis
   shows that Lopez Obrador is a populist leader who also features
   prominent illiberal and antipluralist attributes. The article examines
   the political process that led to the legal modifications he introduced,
   which aimed at an overhaul of the country's system of electoral
   governance, one of the crucial foundations of Mexican democracy. The
   reform was supposed to democratize the regime, eliminate electoral
   fraud, and reduce the cost of elections. In fact, it represented a step
   forward in establishing competitive authoritarianism.},
