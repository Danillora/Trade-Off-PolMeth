@article{ WOS:001285911900001,
Author = {Tomastik, Jan},
Title = {Perfectionism as a requirement of justice: natural law, political
   authority, and the common good},
Journal = {CONTEMPORARY POLITICAL THEORY},
Year = {2025},
Volume = {24},
Number = {2},
Pages = {226-244},
Month = {JUN},
Abstract = {Contrary to the recent argument developed by Matthew Kramer, edificatory
   perfectionism is an integral part of the purpose of the state and as
   such is required by justice. This claim can be supported by an
   appropriate understanding of the common good and the principle of
   subsidiarity. The purpose of the state rests in its duty to secure for
   its citizens the common good, which is a set of conditions under which
   all individuals and associations can attain for themselves various
   reasonable goods or cooperate together to attain them. This includes
   perfectionist goods, such as living in a favourable moral environment.
   The relationship between the common good and justice is clarified by the
   application of the principle of subsidiarity. Its negative formulation
   is well known as a principle limiting the government's competence. I
   draw attention to the positive formulation, which mandates that the
   state is obliged to assist when individuals and communities cannot
   achieve their reasonable goals, including perfectionist ones, by
   themselves. This obligation, though enabling perfectionist actions, is
   still limited by prudential considerations and plurality of intrinsic
   goods which requires respect for personal freedom.},
Publisher = {PALGRAVE MACMILLAN LTD},
Address = {BRUNEL RD BLDG, HOUNDMILLS, BASINGSTOKE RG21 6XS, HANTS, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Tomastík, J (Corresponding Author), Masaryk Univ, Brno, Czech Republic.
   Tomastik, Jan, Masaryk Univ, Brno, Czech Republic.},
DOI = {10.1057/s41296-024-00712-1},
