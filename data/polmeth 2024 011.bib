@article{ WOS:001409080600008,
Author = {Tamrin, M. Husni and Lubis, Lunariana and Musleh, Moh.},
Title = {Sustainable governance practices for ecotourism: engaging local
   communities in the Golden Triangle Island, Indonesia},
Journal = {OTORITAS-JURNAL ILMU PEMERINTAHAN},
