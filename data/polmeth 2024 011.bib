@article{ WOS:001337462800001,
Author = {Kollai, Istvan},
Title = {Trespassing boundaries between national identities: the metamorphosis of
   Vojtech Tuka from a Hungarian academic to a Slovak prime minister},
Journal = {NATIONAL IDENTITIES},
Year = {2025},
Volume = {27},
Number = {3},
Pages = {263-279},
Month = {MAY 27},
Abstract = {B \& eacute;la Tuka started his career as a Hungarian academic, but
   later he transformed into a Slovak nationalist politician, and renamed
   himself Vojtech Tuka. In 1939, he became the prime minister of the
   Nazi-oriented Slovak State. This study intends to reveal, how national
   narratives struggled to interpret this life path. After national
   explanations of trespassing the cultural boundaries between Slovak and
   Hungarian national identity did not prove to be coherent and convincing,
   the memory of Vojtech Tuka has been disregarded by both (opposing)
   national groups, mutually, since it questions the organic existence of
   nations and their boundaries.},
