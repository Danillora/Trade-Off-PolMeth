@article{ WOS:001236700700019,
Author = {Leisse, Olaf},
Title = {Firmly in Putin's Grip Politics and Society in Buriatia},
Journal = {OSTEUROPA},
Year = {2024},
Volume = {74},
Number = {1-3},
Pages = {319+},
Abstract = {Only a third of Buriatia's population belongs to the titular nation. The
   collectivisation of agriculture and industrialisation under Soviet rule
   have left their mark. Since Perestroika, a renaissance of the Buddhist
   faith has been observed. The desire for a merger with Buriat-populated
   territories, which arose in Buriatia in the early 2000s, remained
   unfulfilled. The Putin regime has curtailed the rights of national
   republics and exercises strict control over their political decision
   makers. Support for the war against Ukraine is strong in Buriatia.},
