@article{ WOS:001375001200009,
Author = {Sanchez, Daniel Prieto and Orrego, Marisol Valencia and Pimenteira,
   Cicero},
Title = {The Amazon in Brazil's Electoral Dispute: Speeches, Conflicts, and
   Interests Under Capital},
