@article{ WOS:001320604300009,
Author = {Kryshtanovskaya, O. V. and Meshcheryakova, N. N.},
Title = {THE RICH, THE POOR, AND FAIR INEQUOLITY},
Journal = {POLITEIA-JOURNAL OF POLITICAL THEORY POLITICAL PHILOSOPHY AND SOCIOLOGY
   OF POLITICS},
Year = {2024},
Volume = {114},
Number = {3},
Abstract = {Social inequality is a phenomenon that is ambivalent in its
   consequences. On the one hand, social inequality stimulates creative and
   entrepreneurial activity; on the other hand, it provokes social
   discontent. At the same time, the attitude towards inequality that has
   developed in the society is of particular importance. Perception of
   inequality as unfair is fraught with delegitimization of the upper
   class, loss of social trust between strata, and anomie. The article
   attempts to answer the following questions: what type of inequality do
   Russians agree to consider fair, what methods of enrichment and social
   ascent are legitimized by public consciousness? Empirically, the study
   is based on a series of focus groups conducted by the authors in 2013
