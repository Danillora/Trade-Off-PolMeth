@article{ WOS:001538735800001,
Author = {Duarte, Paulo Afonso B. and Castro, Ana Filipa Pinto de Sousa},
Title = {Turkish soft power in the Erdoğan era: conception, performance and
   adaptation},
Journal = {ROMANIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2024},
Volume = {24},
Number = {2},
Pages = {71-85},
Abstract = {Turkish soft power has experienced a decline over the last decade. The
   existing literature has mostly neglected the assessment of the influence
   of both the leader's conception and domestic and international changes
   on Turkish soft power. Addressing this issue, this article aims to
   answer the following research question: How did Erdo \& gbreve;an's
   conception and performance of his role influence the evolution of
   Turkish soft power from 2013 to 2023? 2013 is the year the Gezi protests
   highlighted Erdo \& gbreve;an's authoritarianism, denigrating his
   reputation both nationally and internationally, and representing a
   turning point in Turkish politics. The year 2023 coincides with the
   centenary of the Turkish Republic, while also marking nearly two decades
   of Erdo \& gbreve;an's uninterrupted rule. The methodology we use
   combines the analysis of primary sources-official documents, reports and
   soft power indices-and secondary sources, based on the assumptions of
   role theory, namely role conception, role expectations, role adaptation
   and role performance. Despite Erdo \& gbreve;an's investment in
   increasing the number of Turkish embassies and consulates, strengthening
   development aid, awarding scholarships and institutes to promote Turkish
   culture and language abroad, among other things, this has not translated
   into an improvement in Turkish soft power. In practice, external
   expectations of Erdo \& gbreve;an's role reflect the fact that these
   initiatives have not been able to counter the dissatisfaction felt
   abroad with the departure from democratic ideals that has accompanied
   the Turkish leader's consolidation of power.},
