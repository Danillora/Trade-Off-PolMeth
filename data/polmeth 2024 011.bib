@article{ WOS:001257043300001,
Author = {Gruber, Barbara},
Title = {Affective discipline - resilience in radicalisation prevention},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2024},
Volume = {17},
Number = {3},
Pages = {786-808},
Month = {JUL 2},
Abstract = {This article engages with the restoration of broken resilience in Dutch
   secondary radicalisation prevention programmes. It demonstrates the
   simultaneity of disciplinary techniques and affective governance in case
   management captured through the concept of ``affective discipline{''}.
   Affective discipline consists of four elements: 1) Surveillance and
   affective engagement, 2) a pre-conceived ideal, i.e. a norm of what it
   means to be resilient, 3) measures benefitting compliance and punishing
   non-compliance, and 4) affective discipline also disciplines the
   affective relations of those in the programme, as the aim is to restore
   a sense of belonging and a sense of cultural identity. The article
   positions resilience as affective discipline within a broader turn of
   affective governmentality, focusing on the disciplinary aspects of the
   governance of and through affect.},
