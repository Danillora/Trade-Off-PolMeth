@article{ WOS:001378278500001,
Author = {De Maio, Jennifer L. and Kabalaki, Ismail and Moshtael, Shayan and
   Tejax, Michael A.},
Title = {AI versus Students: A Study of the Capability of ChatGPT to Write Model
   United Nations Position Papers},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2025},
Volume = {58},
Number = {2, SI},
Pages = {357-362},
Month = {APR},
Abstract = {This article investigates the impact of ChatGPT on student writing in
   the context of Model United Nations position papers, which require
   extensive research and concise articulation of policies on international
   issues. By comparing student-written papers and ChatGPT-generated
   papers, we find that whereas AI-generated papers received higher
   evaluations overall, award-winning student papers outperformed
   AI-generated papers. Furthermore, the quality of AI-generated writing is
   shown to depend on human input, emphasizing the importance of a
   human-centered approach to AI in education.},
