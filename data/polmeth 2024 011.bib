@article{ WOS:001312523900001,
Author = {Hettyey, Andras},
Title = {Unique, better, model, leader: Claims of exceptionality in Hungary's
   foreign policy and beyond},
Journal = {NEW PERSPECTIVES},
Year = {2024},
Volume = {32},
Number = {4},
Pages = {368-385},
Month = {DEC},
Abstract = {In the IR literature, devoting attention to small state exceptionalism
   remains the exception. This paper aims to rectify this situation by
   analyzing claims of exceptionality of the successive Orb \& aacute;n
   governments of Hungary. It aims to shed light on Hungary's foreign
   policy by making two core claims. First, based on a diverse range of
   sources, it shows that successive Hungarian governments have
   increasingly come to conceptualize Hungary as an exceptional,
   trail-blazing, model country in foreign policy and beyond, especially
   after 2018. Secondly, the paper shows how claims of exceptionality can
   lead to resentment and isolation in foreign policy, if and when they go
   hand-in-hand with collective narcissism.},
