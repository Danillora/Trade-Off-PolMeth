@article{ WOS:001267867000006,
Author = {de Oliveira, Emerson Ademir Borges and Cardoso, Guilherme Moraes},
Title = {The grounds of government capacity and the necessary popular
   participation},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2024},
Volume = {128},
Pages = {197-222},
Month = {JAN-JUN},
Abstract = {The aim of this paper is to establish the nuances that comprise the
   concept of governance- or governmental capacity- from its preparation by
   the World Bank in the 1992 Governance and Development report. Based on
   the foundations of governance, according to this document,
   accountability and information transparency as main elements for the
   insertion of the population, from various social formations, in the
   political decision-making process and in the implementation of public
   policies. Participation, moreover, is one of the aspects of the micro
   level of accountability, at the same time as one of the variables of
   Dahl's polyarchical concept of democracy, denoting its indispensability
   for social development combined with economic development. In addition,
   informal insertion can break with corruptive stigmata of Power by
   allowing the merits analysis of political choices, while offering the
   possibility of indirect counterpoints by the Legislative and Judiciary
   to executive decisions. In the end, it is intended to demonstrate, from
   the eminently bibliographical study, how governance- good governance-
   will lead to responsible and sustainable development.},
