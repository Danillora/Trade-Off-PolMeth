@article{ WOS:001135739900001,
Author = {Lee, Alexander and Qi, Weihong},
Title = {Election symbols and vote choice: evidence from India},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2024},
Volume = {12},
Number = {4},
Pages = {879-887},
Month = {OCT},
Abstract = {A large literature has shown that ballot order and ballot structure can
   influence vote choice. We focus on another behavioral cue for voting,
   the symbols that are assigned to candidates in many democracies. We take
   advantage of a naturally occurring experiment in India, where
   independent candidates can choose from a list of free symbols and
   conflicting choices are resolved by a drawing of lots. We find that
   winning the draw has a substantial positive effect on vote share. We
   also find some evidence that candidates attempt to choose their symbols
   strategically, for instance choosing household goods common in their
   constituencies.},
