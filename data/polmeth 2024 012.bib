@article{ WOS:001251133700002,
Author = {Fletcher-Jones, Clifford},
Title = {In Favour of an Independent Royal Space Fleet: The Smuts Report and the
   Precedent of the Royal Air Force},
Journal = {RUSI JOURNAL},
Year = {2024},
Volume = {169},
Number = {1-2},
Pages = {22-32},
Month = {FEB 23},
Abstract = {This article takes the reader on a journey from the foundation of the
   world's first independent air service to the contemporary operating
   environment, as space is recognised as a unique strategic domain. Using
   the Smuts Report, and a framework of culture, concepts and capabilities,
   Clifford Fletcher-Jones shows how the conditions that led to the
   requirement for an independent air force are present today-thus
   requiring an independent space service. The research grounds the
   argument for an independent space service in space power theory before
   articulating a set of recommendations to the British government to
   enable the UK to achieve its `Galactic Britain' ambition.},
