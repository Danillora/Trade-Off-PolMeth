@article{ WOS:001328273500001,
Author = {Piccoli, Erik},
Title = {Seeing the present through the past: the Russo-Ukrainian War and state
   framing of World War Two},
Journal = {NATIONAL IDENTITIES},
Year = {2024},
Volume = {26},
Number = {5},
Pages = {557-574},
Month = {OCT 19},
Abstract = {Following Russia's full-scale invasion of Ukraine in 2022, both states
   began to frame the current conflict through the lens of WWII. By
   focusing on their respective state media outlets (TASS and Ukrinform)
   and their two WWII history museums, this paper explores why the two
   states use history as a framing device by linking the past with the
   present. Through a comparative analysis, distinct approaches taken by
   the Russian and Ukrainian states highlight why certain framing devices
   are used and why they are successful.},
