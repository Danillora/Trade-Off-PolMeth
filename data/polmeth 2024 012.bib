@article{ WOS:001377107600001,
Author = {Sarac, Melike and West, Brady T.},
Title = {The Impact of Showcard Use on Selected Point-Rating Scales in
   Face-to-Face Surveys: Evidence from the European Social Survey},
Journal = {INTERNATIONAL JOURNAL OF PUBLIC OPINION RESEARCH},
Year = {2024},
Volume = {36},
Number = {4},
Month = {DEC 2},
Abstract = {Although quality aspects of survey data have been well-investigated in
   the methodological literature, little is known about the effects of
   showcard use on data quality. This study sought to examine the impact of
   the degree of showcard use on data quality, focusing on item-missing
   data and middle-point selection for a selected set of questions. The
   data come from the European Social Survey Round 9 (ESS9), which covers
   29 European countries. Our findings suggest that showcards used in
   face-to-face surveys significantly reduce the presence of item-missing
   data and middle-point selection, even if only some of them are used
   during interviews. This study presents different strategies that may be
   employed for questionnaire design, interviewer training, and data
   collection.},
