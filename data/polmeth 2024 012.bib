@article{ WOS:001321235700001,
Author = {Collins, Bennett and Watson, Ali},
Title = {Solidarity as subversion: Attempting climate justice practice within the
   Neoliberal University},
Journal = {JOURNAL OF INTERNATIONAL POLITICAL THEORY},
Year = {2024},
Volume = {20},
