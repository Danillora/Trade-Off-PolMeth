@article{ WOS:001290979700001,
Author = {Graeber, Daniel and Meister, Lorenz and Poutvaara, Panu},
Title = {Restrictions to civil liberties in a pandemic and satisfaction with
   democracy},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2024},
Volume = {85},
Month = {DEC},
Abstract = {In times of crises, democracies face the challenge of balancing
   effective interventions with civil liberties. This study examines German
   states' responses during the early stages of the COVID-19 pandemic,
   focusing on the trade-off between civil liberties and public health.
   Using state-level variation in mobility restrictions, we employ a
   difference-in-differences design to show that stay-at-home orders
   notably increased satisfaction with democracy and shifted political
   support towards centrist parties. Stay-at-home orders increased
   satisfaction with democracy most among individuals who had been exposed
   to the authoritarian regime of the German Democratic Republic, possibly
   because they have gotten used to more restrictive state interventions.},
