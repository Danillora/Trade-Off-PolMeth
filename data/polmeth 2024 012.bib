@article{ WOS:001148887500001,
Author = {Rilla, Jeronimo},
Title = {What Kind of Group Is Antifa?},
Journal = {NEW POLITICAL SCIENCE},
Year = {2024},
Volume = {46},
Number = {1},
Pages = {81-100},
Month = {JAN 2},
Abstract = {In the United States, the first presidential debate of 2020 featured a
   discussion about Antifa. While Biden argued that Antifa is ``an idea,
   not an organization,{''} Trump portrayed it as a ``dangerous, radical
   group.{''} The nature of Antifa sparks disagreements not only in
   politics but also in scholarly circles. Some view it as a loosely
   defined label for a fragmented collective. Others perceive it as a gang
   or a radical social movement. This article identifies four key factors
   that make it difficult to define Antifa: the anonymity of its members,
   the lack of identifiable representatives, the ambiguous affiliation
   boundaries, and the influence of external agents (including reporters,
   pundits, and detractors) shaping its identity. Faced with this
   challenge, I propose stepping back to reevaluate the underlying
   assumptions of the debate. I seek to complement current theories of
   social ontology and collective action by introducing an alternative
   approach centered around the concept of personification.},
