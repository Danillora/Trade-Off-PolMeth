Language = {English},
Affiliation = {Silver, E (Corresponding Author), Penn State Univ, Dept Sociol \& Criminol, University Pk, PA 16802 USA.
   Silver, Eric; Iceland, John, Penn State Univ, Dept Sociol \& Criminol, University Pk, PA 16802 USA.
   Goff, Kerby, Rice Univ, Boniuk Inst, Houston, TX USA.},
DOI = {10.1111/ssqu.13416},
EarlyAccessDate = {JUL 2024},
ISSN = {0038-4941},
EISSN = {1540-6237},
