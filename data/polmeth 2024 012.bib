@article{ WOS:001390891400016,
Author = {Cafiero, Florian},
Title = {WHY DO THE FRENCH DOUBT VACCINES SO MUCH?},
Journal = {TOCQUEVILLE REVIEW},
Year = {2024},
Volume = {XLV},
Number = {2},
Month = {DEC 1},
Abstract = {This study explores the reasons behind the very high levels of vaccine
   hesitancy in France compared to global trends. Extending the concept of
   informational ecosystem to scientific production, we conduct a
   scientometric analysis of vaccine safety research, examining how
   scientific discourses, including those from marginalized perspectives,
   influence public perceptions. The analysis highlights a unique aspect of
   the French context: it is one of the few countries where legitimate
   experts openly criticize vaccines on the basis of peer-reviewed,
   unretracted studies. However, these criticisms receive only moderate
   coverage in the French mainstream media. The results suggest that the
   administration of scientific research deserves greater attention to
   understanding the formation of collective beliefs about vaccines, rather
   than focusing solely on well-known factors such as misinformation or
   cognitive biases in socio-technical controversies.},
