@article{ WOS:001295049200001,
Author = {Nakano, Ryota},
Title = {The effect of inter vivos gift taxation on wealth inequality and
   economic growth},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2025},
Volume = {72},
Number = {1},
Month = {FEB},
Abstract = {This study examines the effect of inter vivos gift taxation on wealth
   inequality and economic growth. We develop a simple model with inter
   vivos gifts, which are generated by altruism and gift taxation in an
   overlapping generation setting. The analysis shows that an increase in
   the gift tax rate reduces inequality, and a positive tax rate maximizes
   the economic growth rate. From a policy perspective, rather than
   exempting gifts from taxation, raising the gift tax rate to some extent
   reduces inequality and promotes human capital accumulation and,
   therefore, economic growth.},
