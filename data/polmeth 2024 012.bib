@article{ WOS:001340057800002,
Author = {Tsygankov, Andrei P. and Parker-Bulgakova, Eva M.},
Title = {Coercive Diplomacy Gone Wrong: Interactive Leader Psychology and
   Russia's Invasion of Ukraine},
