@article{ WOS:001377400400015,
Author = {Eick, Felix},
Title = {Central Asia Hub Where the Trail of German Exports Is Lost},
Journal = {OSTEUROPA},
Year = {2024},
Volume = {74},
Number = {8-10},
Pages = {277+},
Abstract = {Exports from Germany to Central Asia have increased massively since
   2022. In many product groups, the increase is several hundred percent.
   It can be ruled out that this is meeting demand in the region itself.
   The suspicion is that many goods reach Russia via Kazakhstan,
   Kyrgyzstan, and Uzbekistan, thus circumventing EU sanctions. Machines,
   cars, and optical devices worth several hundred million US dollars are
   disappearing from the statistics in Central Asia. The analysis here
   makes it possible to grasp the full extent of sanctions evasion by
   product group.},
