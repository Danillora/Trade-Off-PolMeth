@article{ WOS:001272507300002,
Author = {Gallagher, Mitchell},
Title = {Democracy with Chinese Characteristics: Historical and Political
   Transformations},
Journal = {EAST ASIAN POLICY},
Year = {2024},
Volume = {16},
