@article{ WOS:001294404700005,
Author = {Hunt, Rodrigo Aguilera and Michalski, Nahuel},
Title = {Lacan with Benjamin. Continuities between Philosophy and Psychoanalysis
   from a Spiritual Conception of Language},
Journal = {ANACRONISMO E IRRUPCION},
Year = {2024},
Volume = {14},
