@article{ WOS:001267867000014,
Author = {Freitas, Cinthia Obladen de Almendra and Souza, Leonardo Rafael de},
Title = {The power of assembly in the practice of tolerance: the effects of
   racial discrimination on the ``NBA Bubble{''}.},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2024},
Volume = {128},
Pages = {559-610},
Month = {JAN-JUN},
Abstract = {The experience of the so-called ``NBA Bubble{''} provoked a permanent
   assembly process between players, coaches, governors and the press to
   react to the social upheaval experienced in the United States of America
   (USA) by the protests against racial discrimination practiced by the
   police in 2020. Based on the problem of racial intolerance, exemplified
   in cases of police brutality against black Americans, the article
   reflects on how this continuous assembly in the ``bubble{''} has
   transformed the exercise of tolerance into a positive and active method
   of persuasion against intolerance. After an analytical description of
   the external factors that induced the creation and interaction in the
   ``NBA Bubble{''}, the article delimits the theoretical assumptions of
   discrimination and intolerance. In the end, it reflects on how the idea
   of a tolerance exercised in a positive and active way, of collective
   debate, in public spaces of political deliberation and under democratic
   premises, turns into concrete attitudes against intolerance. In
   conclusion, the ``NBA Bubble{''} seems to have provoked a supportive and
   democratic environment that unbalanced injustice and revealed the
   illegitimacy of intolerance, and even proposed political-electoral
   changes in the 2020 presidential elections in the USA.},
