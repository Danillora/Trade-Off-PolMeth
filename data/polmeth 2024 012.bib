@article{ WOS:001184463100012,
Author = {Barker, Kim},
Title = {THE SCOTTISH MELTING POT - SOCIAL MEDIA, POLITICS, SCOTLAND AND ONLINE
   VIOLENCE AGAINST WOMEN},
Journal = {SCOTTISH AFFAIRS},
