@article{ WOS:001247953600001,
Author = {Zacher, Sam},
Title = {What Forms of Redistribution Do Americans Want? Understanding
   Preferences for Policy Benefit-Cost Tradeoffs},
Journal = {POLITICAL RESEARCH QUARTERLY},
