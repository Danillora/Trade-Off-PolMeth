@article{ WOS:001377655300001,
Author = {Hossain, Akram},
Title = {How do authoritarian cultures contribute to democratic backsliding?},
Journal = {ASIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2025},
Volume = {33},
Number = {2},
Pages = {224-249},
Month = {MAY 4},
Abstract = {Bangladesh was a promising case of democratic institutionalization after
   a successful nonviolent resistance movement in the 1990s and subsequent
   free-fair elections during 1991-2008. However, in the last decade, the
   country has been experiencing democratic backsliding and has now become
   a one-party-dominated authoritarian country. The article addresses this
   puzzle - why democracy in Bangladesh fails to consolidate even after
   having democratic prospects. The congruence theory contends that an
   alignment between the socio-political culture and the nature of the
   political system is necessary. A democracy slides back when political
   elites and citizens resort to authoritarian values and practices. This
   article explores the reasons for democratic backsliding, considering
   Bangladesh as a non-congruent case - a mismatch between the existing
   socio-political culture and the political system. While the existing
   literature argues institutional factors as the core causes of democratic
   backsliding, this article emphasizes cultural dimensions. Historical
   evidence and interview data reveal that authoritarian cultural dynamics
   obscure the democratic potential and cause democratic backsliding in
   Bangladesh. The article concludes that elitist political culture - not
   citizens per se - poses a fundamental threat to democracy in the
   country.},
