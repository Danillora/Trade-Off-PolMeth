@article{ WOS:001148139800001,
Author = {Cuomo, Dana and Dolci, Natalie},
Title = {Intimacy-Geopolitics, Remote Warfare and Domestic Violence: Disrupting
   Hierarchies of Violence},
Journal = {GEOPOLITICS},
Year = {2025},
Volume = {30},
