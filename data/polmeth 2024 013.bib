@article{ WOS:001194797200001,
Author = {Jungo, Joao},
Title = {Institutions and economic growth: the role of financial inclusion,
   public spending on education and the military},
Journal = {REVIEW OF ECONOMICS AND POLITICAL SCIENCE},
