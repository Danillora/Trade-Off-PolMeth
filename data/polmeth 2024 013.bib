@article{ WOS:001279379300001,
Author = {Dirks, Maximilian W. and Schmidt, Torsten},
Title = {Political instability and economic growth: Causation and transmission},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2024},
Volume = {85},
Month = {DEC},
Abstract = {This paper examines the link between political instability and economic
   growth in 34 advanced economies from 1996 to 2020. First, we use a panel
   VAR estimated via the System GMM to explore the endogenous relationship
   between economic growth and political instability and identify
   transmission channels. Second, we employ an instrumental variable
   approach, exploiting temperature variation and spillover effects of
   political instability to establish causality. The results of both
   approaches indicate that a one-standard deviation shock of political
   instability significantly and substantially reduces economic output. We
   find no evidence, however, that economic growth affects political
   instability.},
