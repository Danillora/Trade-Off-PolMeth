
@article{ WOS:001242429800001,
Author = {Peiro-Palomino, Jesus and Gianmoena, Lisa and Picazo-Tadeo, Andres J.
   and Rios, Vicente},
Title = {Social trust and the advanced aspects of social progress. Evidence for
   the European regions},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2022},
Volume = {83},
Month = {JUN},
Abstract = {Social trust is a deeply -rooted feature of society, whose positive
   impact on economic performance has been widely documented in many
   contexts. However, its impact on the non -economic aspects of social
   progress that characterize advanced societies, such as personal rights,
   freedom, tolerance and inclusion and access to advanced education is
   still understudied, especially at the subnational level. As shown by the
   European Social Progress Index (EU-SPI) 2020, elaborated by the European
   Commission, the European regions present remarkable disparities in those
   non -economic aspects. Using the EU-SPI framework, this paper provides
   fresh evidence on a positive impact of social trust on several features
   defining advanced social progress. Social trust effects are mainly seen
   in improved quality of government, education and people's pro -social
   behaviors. These insights can be useful for the design of future
   policies that pursue a more equal Europe beyond purely economic
   indicators, given that regional social trust can condition their
   success.},
