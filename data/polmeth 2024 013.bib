@article{ WOS:001301216900001,
Author = {Janska, Eva and Janurova, Kristyna and Loeblova, Olga and Novotny, Josef},
Title = {Extending comparative typologies of diaspora policies: Towards a
   ``cautiously proactive{''} diaspora policy state},
Journal = {POLITICAL GEOGRAPHY},
Year = {2024},
Volume = {114},
Month = {OCT},
Abstract = {In the past two decades, the `diaspora turn' in research and policy has
   led to the development of several typologies of diaspora policies around
   the world. This study explores and extends Francesco Ragazzi's
   established 2014 global typology of diaspora policies. We use in-depth
   qualitative analysis of Czech diaspora policies to assess the internal
   validity of the typology and identify new dimensions that are central to
   our understanding of diaspora politics. We then replicate Ragazzi's
   quantitative analysis with the inclusion of new data on the Czech
   Republic, Slovakia, Hungary, Croatia and Poland, assessing the
   typology's external validity. We find a new cluster of diaspora policy
   types, characterized by a ``cautiously proactive{''} mix of diaspora
   policy mechanisms based on relatively generous cultural and citizenship
   policies, variably accentuated symbolic policies and limited social and
   economic policies. We suggest that this mix has been structured by the
   tension between the countries' commitment to a (neo) liberal emigration
   regime and the continued importance of ethno-cultural conceptions of
   nationhood. In addition, we identify three new variables that should be
   included in future typologies: 1) the volume and distribution of funding
   for diaspora policy; 2) the symbolic recognition of diaspora in the
   legal system; 3) policy differentiation among different kinds of
   diaspora. Our mixed-method approach illustrates the importance of using
   nuanced qualitative data to provide a meaningful depiction of policy.},
