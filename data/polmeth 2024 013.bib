@article{ WOS:001298578600001,
Author = {Hackett, Lisa J.},
Title = {The Women of the Air Transport Auxiliary in Second World War Newsreels},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
