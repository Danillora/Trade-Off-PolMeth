@article{ WOS:001389602700001,
Author = {Bartels, Inken},
Title = {The Symbolic Power of Knowledge Practices: The International
   Organization for Migration's Anti-Trafficking Politics in North Africa},
Journal = {GEOPOLITICS},
Year = {2025},
Volume = {30},
Number = {4, SI},
Pages = {1654-1679},
Month = {AUG 8},
Abstract = {This article examines how the International Organization for Migration
   (IOM) brought human trafficking into being as a widely acknowledged
   reality and relevant object of migration governance in North Africa in
   the early 2000s. It mobilises Pierre Bourdieu's concepts of symbolic
   capital and symbolic power to demonstrate how the IOM emerged in the
   influential position of a seemingly neutral, objective and morally
   upright international expert that is able to influence moral
   distinctions, enforce doxic beliefs and set rules in the transnational
   field of migration management. Through this symbolic borderwork, the IOM
   introduced concepts of care and control, and categories of protection
   and criminal persecution for Moroccan and Tunisian policies and held
   authorities accountable to combat human trafficking in line with
   international norms, discourses and practices. In this way, the article
   contributes to the literature on the power of international
   organisations and their knowledge practices in the geopolitics of
   externalising migration control.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
