@article{ WOS:001301033500001,
Author = {Baum, Matthew A. and Druckman, James N. and Ognyanova, Katherine and
   Schulman, Jonathan},
Title = {Misperceptions, Depression, and Voting for Election Deniers in the
   United States},
Journal = {INTERNATIONAL JOURNAL OF PUBLIC OPINION RESEARCH},
Year = {2024},
Volume = {36},
Number = {2},
Month = {JUN 1},
Abstract = {Two of the most significant concerns about the contemporary United
   States are the erosion of democratic institutions and the high rate of
   depression. We provide evidence connecting these phenomena. We use a
   survey (N = 11,517) to show a relationship between misperceptions (about
   COVID-19 vaccines) and voting, in 2022, for gubernatorial candidates who
   denied or cast doubt on the legitimacy of the 2020 election results. We
   further predict and find that the presence of moderately
   severe-to-severe depressive symptoms exacerbates the relationship
   between misperceptions and voting for election deniers or doubters. The
   results offer insight into the links between misperceptions, depression,
   and democratic backsliding (i.e., supporting candidates who challenge
   election results). We also contribute to a growing line of research on
   how mental health affects democratic functioning, potentially worldwide.},
