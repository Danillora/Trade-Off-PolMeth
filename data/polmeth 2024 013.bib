@article{ WOS:001259199900001,
Author = {Shakri, Irfan Haider and Yong, Jaime and Xiang, Erwei},
Title = {Corporate governance and firm performance: Evidence from political
   instability, political ideology, and corporate governance reforms in
   Pakistan},
Journal = {ECONOMICS \& POLITICS},
Year = {2024},
Volume = {36},
Number = {3},
Pages = {1633-1663},
Month = {NOV},
Abstract = {We examine impact of corporate governance on firm performance following
   the implementation of the changes to the Code of Corporate Governance of
   Pakistan in 2012. Our sample period from 2008 to 2022 include periods of
   political instability and shifts in Pakistan's political landscape,
   providing an opportunity to examine the effectiveness of corporate
   governance mechanisms in enhancing accounting- and market-based firm
   performance measures. We find significant improvements attributed to
   reforms in the regulatory framework surrounding corporate governance
   practices particularly from expanded scope and composition of boards and
   audit committees. This led to broader capabilities and effective
   controls, thus improving firm performance and investor confidence more
   so during periods of political instability and changes in political
   ideology.},
