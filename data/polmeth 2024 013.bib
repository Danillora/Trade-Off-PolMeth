@article{ WOS:001353283500001,
Author = {Hernandez, Roberto Gil},
Title = {The Canary Islands, an Imperial Frontier: Decolonial Thought in
   South-South Relations in Northwest Africa},
Journal = {GEOPOLITICS},
Year = {2025},
Volume = {30},
Number = {3, SI},
Pages = {1226-1255},
Month = {MAY 27},
Abstract = {The Canary Islands have been denied their Africanness throughout
   history. This has served to legitimise the problematic role of the
   archipelago as West's imperial frontier in Northwest Africa. Through
   agenealogy of the available literature on relations between the African
   continent and the Islands, the effects of this position are critically
   evaluated. To this purpose, this article presents a sociological
   analysis of the dialectic of North-South domination and its effects on
   the ambivalent representations that insist on the archipelago being
   referred to, in some cases, as the South of the North, and, in others,
   as the North of the South. From the point of view of decolonial thought,
   it is affirmed that the Canary Islands, despite its links with the
   Kingdom of Spain and European Union, can also recognise itself in the
   African political, cultural, and epistemic plurality and interact with
   it in the framework of South-South relations.},
