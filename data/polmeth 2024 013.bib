@article{ WOS:001356950400001,
Author = {Wang, Guan-Yuan},
Title = {Cultural group diversity and team performance: The importance of
   international employees},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2025},
Volume = {72},
Number = {3},
Month = {JUL},
Abstract = {In the ongoing debates regarding performance disparities between
   homogeneous and heterogeneous teams, we examine team dynamics within the
   National Basketball Association to provide insights into this issue.
   Utilizing Shannon entropy, we measure the cultural diversity of player
   nationalities within each team. Through panel data modeling with
   interactive and mediating effects, we empirically investigate the impact
   of workforce diversity on team performance. Our findings reveal that
   diversity influences team success through both offensive performance and
   defensive effort. Importantly, our results highlight a positive
   relationship between team heterogeneity and win percentages,
   underscoring the advantages of diversity in enhancing overall team
   performance.},
