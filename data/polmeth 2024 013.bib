@incollection{ WOS:001300520700008,
Author = {Slominski, Kamil},
Editor = {Bosiacki, A and Safferling, C and Geis, ME},
Title = {The Combat of Communist Ideology and Propaganda in the Legal Order of
   the Second Republic of Poland, 1918-1939},
Booktitle = {DEALING WITH TOTALITARIAN REGIMES AND HUMAN RIGHTS: A Polish-German
   Dialogue},
Series = {Interdisciplinary Studies in Human Rights},
Year = {2024},
Volume = {11},
Pages = {133-153},
Abstract = {In this chapter, the author analyses the combat of communist ideology
   and propaganda in the legal order of the Second Republic of Poland from
   the legal and historical perspective. The `combat' included in the title
   refers to actions undertaken by legislative, executive and-most
   essentially-judicative branch of power towards communists. This chapter
   seeks to examine several judicial decisions including soft-law acts. The
   forms of combatting communism are captured in the context of fundamental
   rights as well as the diversity of legal and political doctrines. The
   author undertakes a comprehensive study of this phenomenon, taking into
   account the historical point of view as well as the development of legal
   research from the Second Republic of Poland until present day.},
