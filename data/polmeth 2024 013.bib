@article{ WOS:001214101900001,
Author = {Huf, Ben},
Title = {Whitlam's Economic (Inter)Nationalism},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
Year = {2024},
Volume = {70},
