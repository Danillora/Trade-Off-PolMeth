@article{ WOS:001200273200001,
Author = {Turner, Nick},
Title = {Rain, rain, voter go away? New evidence on rainfall and voter turnout
   from the universe of North Carolina voters during the 2012-2020
   presidential elections},
Journal = {POLITICAL GEOGRAPHY},
Year = {2024},
Volume = {110},
Month = {APR},
Abstract = {I revisit the relationship between inclement weather and voter turnout
   using the universe of voters in N.C. during the last three U.S.
   presidential elections. While I find that rainfall continues to slightly
   dampen turnout overall, the results suggest that the negative effects on
   in -person election day voting are partially offset by increased use of
   alternate voting methods. Intuitively, these methods, including mail
   voting and early/absentee voting, allow voters to cast their ballots
   while avoiding the costs of heading to the polling place in bad weather.
   Further, I find that many voters respond to rainfall by adopting these
   voting methods so that the likelihood of voting increases on net in the
   subsequent election.},
