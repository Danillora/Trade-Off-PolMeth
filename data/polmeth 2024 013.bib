@article{ WOS:001329455400001,
Author = {Klausen, Jan Erling},
Title = {Governmental strategies for municipal amalgamations: evidence from 24
   European countries},
Journal = {TERRITORY POLITICS GOVERNANCE},
Year = {2024},
Month = {2024 OCT 10},
Abstract = {For several decades, European governments have pursued amalgamations of
   local governments, mainly to improve municipal service provision,
   efficiency and governing capacities. Some reforms resulted in
   comprehensive territorial restructuring; others had little impact. This
   study investigates the strategies and policy tools European governments
   implemented to achieve municipal amalgamations. I propose a new
   three-fold typology of reform strategies and analyse the policy
   instruments used in each strategy throughout the reform periods, using
   comparative evidence on 29 amalgamation reforms implemented by central
   and regional European governments since 2000. I assess the outcomes of
   each strategy and discuss the findings' implications for multi-level
   reform governance.},
