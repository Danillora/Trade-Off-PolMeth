@article{ WOS:001196771200001,
Author = {Hagelin, Sandra and Gibson, Catherine},
Title = {Renegotiating Estonia's marginality in the imagined geography of Europe:
   Kaja Kallas' Tweets about Russia's war in Ukraine},
Journal = {TERRITORY POLITICS GOVERNANCE},
Year = {2025},
Volume = {13},
Number = {10},
Pages = {1381-1398},
Month = {NOV 26},
Abstract = {Central and Eastern Europe leadership responses to Russia's full-scale
   invasion of Ukraine in February 2022 have not only contributed to power
   shifts within European politics, but also to attempts to discursively
   reinterpret the region's imagined geography. Based on a content analysis
   of over 500 Tweets related by Estonian Prime Minister Kaja Kallas in the
   first year of the war, we examine how Kallas has endeavoured to
   reconfigure Estonia's marginality through advocating to expand Europe's
   moral geography to include Ukraine, representing Estonia as a voice of
   expertise on Russia from Europe's edge, and depicting Estonia as the
   eastern flank of the Western world.},
