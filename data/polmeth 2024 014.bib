@article{ WOS:001473754700004,
Author = {Durand, Pierre and Le Quang, Gaetan and Vialfont, Arnold},
Title = {Nonlinearities and Interactions between Variables: some Contributions of
   Machine Learning Methods Interpretable to Banking Regulation},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2024},
Volume = {134},
Number = {6},
Pages = {893-922},
Month = {NOV-DEC},
Abstract = {The aim of this article is to illustrate the usefulness of interpretable
   machine learning methods in the specific case of banking economics. In
   particular, we rely on a gradient boosting model to determine the
   optimal regulatory capital ratio in the context of the prudential
   regulation of banks. To do so, we develop, on the one hand, a
   classification model whose purpose is to determine the impact of capital
   ratios on the probability of bank default, and, on the other hand, a
   regression model aimed at assessing the costs on bank performance of
   increased capital requirements. Using various interpretability tools
   (permutation importance, Shapley values, partial dependence plots,
   accumulated local effects), we found the following optimal values: 15 \%
   for the risk-weighted capital ratio and 10\% for the simple leverage
   ratio. The determination of these values is based on the identification
   of the non-linearities and of the interaction effects that characterize
   the relationships between the different variables studied.},
