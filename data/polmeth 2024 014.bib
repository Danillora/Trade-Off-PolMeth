@article{ WOS:001333940700001,
Author = {Regilme, Salvador Santino and Parthenay, Kevin},
Title = {COVID-19 pandemic and competitive authoritarian regimes: Human rights
   and democracy in the Philippines and Nicaragua},
Journal = {POLITICAL GEOGRAPHY},
Year = {2024},
Volume = {115},
Month = {NOV},
Abstract = {How do competitive authoritarian regimes in the Global South respond to
   the COVID-19 pandemic? How do these policy responses facilitate human
   rights deterioration in societies that are already facing democratic
   regression during the pre-pandemic period? Examining evidence from the
   Philippines and Nicaragua during the COVID-19 pandemic, this article
   demonstrates that competitive authoritarian regimes have reinforced the
   deterioration of democratic processes and disregard for their human
   rights commitments amidst the global pandemic. First, such regimes
   weaponize the legal system to consolidate the powers of the chief
   executive and their allies. Second, such regimes systematically
   disregard transparency and accountability when executing state leaders'
   public actions and responsibilities. Third, such regimes increasingly
   empower military personnel and intensify state violence at the expense
   of science-based approaches to crisis policies, thereby embracing
   militarism as an overarching orientation.},
