@article{ WOS:001304098000001,
Author = {Alakitan, Muhammed and Makinde, Ebenezer},
Title = {Where are the ethical guidelines? Examining the governance of digital
   technologies and AI in Nigeria},
Journal = {POLICY AND INTERNET},
Year = {2025},
Volume = {17},
Number = {1},
Month = {MAR},
Abstract = {The Nigerian government, under the administrations of Muhammadu Buhari
   (2015-2023) and Bola Tinubu (2023-2027), has made continued attempts to
   harness the opportunities of digital technologies and artificial
   intelligence (AI) to boost economic growth and development. However,
   these technologies have also raised ethical and security concerns within
   their production and usage, requiring regulatory policies from the
   government. Within this context, this study examines the space for
   ethical guidelines and data protection in the Nigerian government's
   approach to digital technologies and AI. Through a review of digital
   technology policy documents, we show that the Buhari government
   prioritized policy-driven approaches to digital technologies. On the
   other hand, Tinubu's administration has started with a
   policy-and-research approach that involves both the public and private
   sectors. However, despite these differences, this study argues that
   neither administration has paid much attention to the ethical guidelines
   of data, digital technologies, and AI. This study contributes to the
   global discourse on digital technology's social impacts, particularly in
   regions like Africa.},
