@article{ WOS:001211492400004,
Author = {Altinok, Nadir and Diebolt, Claude},
Title = {Brief Cliometric Looking back of 50 years of School Performance in
   Reading and Mathematics in France: 1970-2020},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2024},
Volume = {134},
Number = {1},
Pages = {81-104},
Month = {JAN-FEB},
Abstract = {The aim of this contribution is to trace the long-term evolution of
   French pupils' academic skills from 1970 to 2020. Based on the results
   of international surveys of pupil achievement, we present the
   originality of France's trajectory in relation to other OECD countries,
   using a previously unpublished database. Our approach is both historical
   and comparative. Indeed, rather than absolute variation, we measure
   French performance in terms of relative deviations from the OECD. Over
   the last 50 years, we show that, in the case of France, performance in
   reading and mathematics increases before stagnating or even declining
   (average scores being significantly low). Generally speaking, we observe
   a quasi-generalized growth in school performance over the 1970s, 1980s
   and 1990s, even if growth rates differ between countries. We also
   examine the French trajectory in terms of educational achievement, which
   is gradually diverging from that of other OECD countries. In so doing,
   our findings enrich the conclusions drawn from national surveys, while
   providing new evidence combined with renewed historical and comparative
   insights.},
