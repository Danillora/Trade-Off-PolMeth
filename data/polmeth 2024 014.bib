@article{ WOS:001140324900001,
Author = {Cairo, Heriberto and Piazzini Suarez, Carlo Emilio and Rios, Jeronimo
   and Santana, Luis Daniel and Montoya Garay, Jhon Williams and Salas
   Salazar, Luis Gabriel and Zambrano Quintero, Liliana and Espinosa Rico,
   Miguel Antonio and Montoya Arango, Vladimir and Koopman, Sara and
   Salamanca, Rosa Emilia and Agudelo, Carlos and Oslender, Ulrich and
   Rodriguez, Flavio-Bladimir},
Title = {From `Territorial Peace' to `Total Peace' in Colombia: A Geopolitical
   Balance},
Journal = {GEOPOLITICS},
Year = {2024},
Volume = {29},
Number = {5},
Pages = {1996-2032},
Month = {OCT 19},
Abstract = {This forum discusses the concept of `total peace' (paz total) and the
   notion of `peace regions' (regiones de paz) recently coined by the
   government of Colombia to advance in the implementation of the Peace
   Accords signed back in 2016 with the guerrillas of the Revolutionary
   Armed Forces FARC. Broadly drawing on a radical and critical geopolitics
   perspective, the forum's contributions offer a set of interpretations,
   exploring a wide range of different aspects, such as the relations with
   the previous concept of `territorial peace' (paz territorial), the
   changing geographical dynamics of the armed conflict, the proliferation
   of unlawful armed structures and drug trafficking, and violent processes
   of capital accumulation. Moreover, specific themes such as environmental
   conflict, the political reintegration of the guerrillas, gender and
   differential approaches, and ethnic and peasant communities'
   perspectives are also addressed, without losing sight of the more
   general question of the country's territorial reordering.},
