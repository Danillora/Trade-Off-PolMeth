@article{ WOS:001343306600005,
Author = {Ghazzai, Hend and Hemissi, Wided and Lahmandi-Ayed, Rim and Kefi, Sana
   Mami},
Title = {Competition and Poverty: Does the Competition Type Matter?},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2024},
Volume = {134},
Number = {4},
Pages = {611-630},
Month = {JUL-AUG},
Abstract = {We study the impact of the competition nature on poverty. More
   precisely, we check whether the results obtained by Ghazzai et al.
   {[}2023] on the effect of competition on poverty in the case of price
   competition hold with quantity competition. We consider a general
   equilibrium model with vertical preferences and compare poverty under
   two market structures: Monopoly and Duopoly, taking into account the
   ownership structure. Poverty is measured through the size of the
   population made of individuals whose income falls below some exogenous
   poverty line. The new results confirm those of Ghazzai et al. {[}2023]
   showing that policies that aim at fostering competition are not always
   the effective tool to alleviate poverty.},
