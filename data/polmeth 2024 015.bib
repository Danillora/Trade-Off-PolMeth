@article{ WOS:001319011600003,
Author = {Beka, Rinor and Qirezi, Arben},
Title = {The Impact of Minority Veto in Consociational Democracies Lessons from
   Kosovo, 2008-2022},
Journal = {INTERNATIONAL JOURNAL OF PARLIAMENTARY STUDIES},
