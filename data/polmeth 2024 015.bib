@article{ WOS:001357454000001,
Author = {Lin, Chin-Ho and Chen, Yiting and Yang, Yiwen},
Title = {Impact of electronic networks on E-waste trade: Evidence from ASEAN+6
   countries},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2025},
Volume = {72},
Number = {2},
Month = {MAY},
Abstract = {The development of electronic networks promotes economic growth and
   international trade flows. Because of the short life cycle of electrical
