@article{ WOS:001405675500004,
Author = {Best, Beverley},
Title = {Marx's Critical Theory of Slavery},
Journal = {HISTORICAL MATERIALISM-RESEARCH IN CRITICAL MARXIST THEORY},
Year = {2024},
Volume = {32},
Number = {3},
Pages = {125-157},
Month = {DEC},
Abstract = {Marx's critical theory of slavery is the operational subtext throughout
   his critique of political economy. For Marx, the movement from modern
   slavery to capital represents a historical transition of significance,
   not only (or foremost) as an empirical transition but also as a
   transformation of social substance. Marx reveals why, in retrospect,
   production based on slavery, as logical configuration, must give way to
   the generalising logic of wage labour. Marx's critical theory of slavery
   historicises wage labour (qua category) as the dissolution and inversion
   of enslaved labour (qua category). Plantation slavery production in the
   colonised territories initiates a qualitative development that
   constitutes the historical condition for the emergence of a mode of
   sociality that we can call `value'. Marx's theoretical account of modern
   slavery is a lever in his broader critique of capital and is
   fundamentally distinct from empirical, historical accounts.},
