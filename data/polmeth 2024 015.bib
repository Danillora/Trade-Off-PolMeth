@article{ WOS:001155199900001,
Author = {Platonova, Daria},
Title = {The Road to Oligarchic Peace: Comparing the Antebellum United States and
   Ukraine},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
