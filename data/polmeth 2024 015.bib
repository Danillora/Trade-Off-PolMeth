@article{ WOS:001208117400001,
Author = {Yanez, Manuel},
Title = {Governance, State Control and Legal-Institutional Framework of Public
   Enterprises at the Subnational Level in Argentina Governança, controle
   estatal e marco jurídico-institucional das empresas públicas em nível
   subnacional na Argentina},
Journal = {REVISTA ESTUDIOS SOCIO-JURIDICOS},
Year = {2024},
Volume = {26},
Number = {1},
Month = {JAN-JUN},
Abstract = {The objective of this paper is to analyze the governance of subnational
   public enterprises by considering how state control is expressed in the
   legal institutional frameworks at the provincial level in Argentina.
   Based on a qualitative approach with descriptive purposes, regulations
   of the 24 Argentine provincial jurisdictions were surveyed. Among the
   findings, it is highlighted that all the jurisdictions present a
   decentralized scheme of relationship with their companies, through the
   corresponding sectorial ministry, including, in some cases, the
   existence of transversal instances of control and supervision.},
