@article{ WOS:001391933200001,
Author = {Heinemann, Friedrich and Nover, Justus and Steger, Paul},
Title = {State-owned enterprises, fiscal transparency, and the circumvention of
   fiscal rules: The case of Germany},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2025},
Volume = {86},
Month = {JAN},
Abstract = {State-owned enterprises (SOEs) provide opportunities for a more flexible
   and market-based provision of public services. At the same time, they
   may impair fiscal transparency and offer politicians discretion in the
   presence of strict fiscal rules if these only constrain the core budget.
   Using a comprehensive micro-data set of German SOEs, this paper studies
   a possible impact of German debt brake on SOEs by tracking changes in
   financial indicators at the firm level would hint to a circumvention of
   the rule. The identification exploits that the mounting compliance
   pressures over the lagged implementation of the debt brake from 2010 to
   2020 differs across the 16 states. The results show that SOEs in
   fiscally more constrained states exhibit stronger decrease in equity and
   reserves and a higher increase in debt compared to SOEs in constrained
   states and the shorter the distance to the 2020 deadline. This result is
   based on combined sample of state and municipal SOEs, a finding pointing
   towards the vertical spillover a fiscal rule.},
