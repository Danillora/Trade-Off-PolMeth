@article{ WOS:001347960600004,
Author = {Montaser, Marwa Sobhy},
Title = {Investigating self-settled Syrian refugees' agency and informality in
   southern cities greater Cairo: a case study},
Journal = {REVIEW OF ECONOMICS AND POLITICAL SCIENCE},
