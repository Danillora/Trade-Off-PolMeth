@article{ WOS:001183078500001,
Author = {Nitzan, Asaf D. M. and Nitzan, Shmuel I.},
Title = {Balancing democracy: majoritarianism versus expression of preference
   intensity},
Journal = {PUBLIC CHOICE},
Year = {2024},
Volume = {200},
Number = {1-2},
Pages = {149-171},
Month = {JUL},
Abstract = {This paper evaluates three prominent voting systems: the Majority Rule
   (MR), Borda Rule (BR), and Plurality Rule (PR). Our analysis centers on
   the susceptibilities of each system to potential transgressions of two
   foundational principles: the respect for majority preference
   (majoritarianism) and the acknowledgment of the intensity of individual
   preferences. We operationalize the concept of `cost' as the expected
   deviation from the aforementioned principles. A comparative assessment
   of MR, BR, and PR is undertaken in terms of their costs. Our findings
   underscore the superiority of PR over MR, whilst also highlighting the
   comparative advantage of MR against BR.},
