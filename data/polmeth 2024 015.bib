@article{ WOS:001216875200001,
Author = {Riseman, Noah},
Title = {Revisiting the HMAS Swan Scandal and Histories of Sexual
   Harassment in the Australian Defence Force},
Journal = {AUSTRALIAN JOURNAL OF POLITICS AND HISTORY},
