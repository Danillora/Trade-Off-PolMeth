@article{ WOS:001169426300001,
Author = {Sukarieh, Rana},
Title = {Political imaginaries of solidarity: the Boycott, Divestment, and
   Sanctions (BDS) in Toronto},
Journal = {SOCIAL MOVEMENT STUDIES},
