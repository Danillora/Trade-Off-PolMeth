@article{ WOS:001278014600001,
Author = {Metraux, Stephen and Kolaja, Claire A. and Crone, Baylee and Byrne,
   Thomas and Rull, Rudolph P. and Porter, Ben},
Title = {Risk Factors for Homelessness Among Post-9/11 Era Veterans},
