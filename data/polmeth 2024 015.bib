@article{ WOS:001222532300001,
Author = {Beladi, Hamid and Chao, Chi-Chur and Chin, Kuo-Hsuan},
Title = {Cross-ownership, business dynamism, and wage inequality in general
   equilibrium},
Journal = {SCOTTISH JOURNAL OF POLITICAL ECONOMY},
Year = {2024},
Volume = {71},
Number = {4},
Pages = {570-587},
Month = {SEP},
Abstract = {This study examines the distributive and welfare effects of
   cross-ownership by firms in a general equilibrium economy on the product
   and factor markets. The cross-ownership of equities, such as collusion,
   tends to be anticompetitive, thereby narrowing the wage gap between
   skilled and unskilled labor in the short term with the existing number
   of firms. In the capital market, reducing capital cost through
   cross-ownership causes new firms to enter the market in the long term.
   This firm-entry effect induced by cross-ownership through an increase in
   the number of competitors generates a competitive force that exacerbates
   wage inequality and reduces welfare in the economy.},
