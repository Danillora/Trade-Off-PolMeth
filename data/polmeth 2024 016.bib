@article{ WOS:001273602500001,
Author = {Elis, Jonas and Mayer, Sabrina Jasmin and Goerres, Achim},
Title = {Are charitable donations a luxury good of the rich? Evidence from a
   survey and actual behavior in a superdiverse metropolis},
