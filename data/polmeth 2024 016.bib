@article{ WOS:001248558600001,
Author = {Fong, Christian and McCrain, Joshua},
Title = {A tournament theory of congressional committee leadership},
Journal = {PUBLIC CHOICE},
Year = {2025},
Volume = {202},
Number = {1-2},
Pages = {193-215},
Month = {JAN},
Abstract = {We apply tournament theory to congressional leadership to unify research
   on campaign finance with theories of endogenous party strength. Parties
   want to incentivize members to do costly work for the benefit of the
   party, such as fundraising. Accordingly, they make leadership offices
   attractive and award these leadership offices on the basis of who does
   the most work for the party. The more attractive the leadership office
   becomes, the harder party members work to win. We present a model to
   formalize this argument, derive its empirical implications, and find
   support for these implications using data from committee assignments,
   committee authorizations, and fundraising for leadership political
   action committees and congressional hill committees.},
