@article{ WOS:001380681600001,
Author = {Chu, Zhaopeng and Li, Xiaoyan and Yang, Jun},
Title = {Regional industrial synergy and industry chain stability as state
   spatial strategies: reimagining China's city-region governance},
Journal = {TERRITORY POLITICS GOVERNANCE},
Year = {2024},
Month = {2024 DEC 19},
Abstract = {This study applies Synergetics as the analytical framework for strategic
   spatial planning and city-region governance to explore industrial
   synergy and stability in the Beijing-Tianjin-Hebei urban agglomeration.
   We identify three types of industries: those embedded in a synergistic
   industry cluster, those involved in a synergistic industry chain, and
   those synergistically isolated from others. Our results show that
   97.40\% of the subdivided industries retain synergistic relations with
   others, but only 26.06\% of these synergistic relations are stable.
   Policy recommendations are provided on promoting synergistic industrial
   development for Beijing to relieve non-capital functions through
   industrial transfers.},
