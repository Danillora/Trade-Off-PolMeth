@article{ WOS:001211036900001,
Author = {Homanyuk, Mykola and Ashutosh, Ishan},
Title = {Inscribing Ukraine: The symbolic dimensions of Russian
   reterritorialization through war},
Journal = {POLITICAL GEOGRAPHY},
Year = {2024},
Volume = {109},
Month = {MAR},
Abstract = {This commentary examines the Russian special military operation's (SMO)
   use of symbols, toponyms, and the reordering/re-bordering of territory
   in Ukraine. Through an analysis of Ukrainian and Russian media, we argue
   that the Russian SMO's spatial strategies of inscription attempt to
   reterritorialize Ukraine and legitimize Russia's claim over it. These
   symbolic dimensions of Russia's war in Ukraine draw from the Russian
   Empire, the Soviet Union, and the Russian Federation.},
