@article{ WOS:001314346900001,
Author = {Lauria, Valeria and Lin, Justin Yifu and Wang, Xin and Zheng, Yawen},
Title = {Development strategy, infrastructure, and premature deindustrialization:
   Comparing Asian, African, and Latin American economies},
Journal = {ECONOMICS \& POLITICS},
Year = {2025},
Volume = {37},
Number = {1},
Pages = {304-340},
Month = {MAR},
Abstract = {This paper investigates the phenomenon of premature deindustrialization
   in developing countries, with a focus on Latin America and sub-Saharan
   Africa, and examines why this trend primarily occurs in these regions
   rather than in East Asia. Through a comparative analysis of
   industrialization experiences across different economies since the
   1960s, the paper argues that premature deindustrialization results from
   a combination of liberalization shocks, development strategies that defy
   comparative advantage, and inadequate infrastructure supply due to the
   lack of active state involvement in its provision driven by prevailing
   neoliberal ideas. This study empirically validates this hypothesis using
   country-sector-level data from 40 economies.},
