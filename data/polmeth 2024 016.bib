@article{ WOS:001215766900001,
Author = {Bengtson, Andreas},
Title = {Positive and negative affirmative action},
Journal = {POLITICS PHILOSOPHY \& ECONOMICS},
Year = {2025},
Volume = {24},
Number = {1},
Pages = {25-50},
Month = {FEB},
Abstract = {Affirmative action continues to divide. My aim in this article is to
   present participants in the debate with a new distinction, namely one
   between negative and positive affirmative action. Whereas positive
   affirmative action has to do with certain goods, such as a place at a
   prestigious university or a job at a prestigious company, negative
   affirmative action has to do with certain bads, such as a firing or a
   sentence. I then argue that some of the most prominent arguments in
   favor of affirmative action speak at least as much in favor of negative
   as positive affirmative action. At the same time, at least one of the
   most prominent arguments put forward against affirmative action speaks
   less against negative affirmative action. Thus, the article should
   redraw the battle lines in the affirmative action debate.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
