@article{ WOS:001355065300001,
Author = {Munoz, Luz and Rodilla, Andreu},
Title = {A game of tug-of-war: regional laws before the Spanish Constitutional
   Court},
Journal = {TERRITORY POLITICS GOVERNANCE},
Year = {2024},
Month = {2024 NOV 15},
Abstract = {To date, intergovernmental relations have been analysed from a variety
   of different perspectives, the constitutionality of the laws passed by
   different levels of government being one of them. Here, drawing on a
   unique database containing all the regional laws passed in Spain between
   1980 and 2021, we examine the strategic dimension of their
   judicialisation by the party in government at the central level. The
   results show that partisan preferences along left-right and
   centre-periphery axes have played an important role in this regard. In
   addition, we show that the central government has often engaged in a
   game of tug-of-war with the Constitutional Court depending on its
   composition, with a more favourable court typically leading to greater
   litigiousness.},
