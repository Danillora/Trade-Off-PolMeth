DOI = {10.1080/21622671.2022.2106298},
ISSN = {2162-2671},
EISSN = {2162-268X},
Keywords = {multilevel governance; power; climate change; subnational governments;
   sectoral performance; Mexico},
Keywords-Plus = {CHANGE POLICY; CITIES; SUSTAINABILITY; IMPLEMENTATION; POWER;
   INSTRUMENTS; MITIGATION; INNOVATION; COHERENCE; FORESTRY},
