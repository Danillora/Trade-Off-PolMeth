@article{ WOS:001319837100001,
Author = {Elmore, Naela},
Title = {Solidarity across borders: transnational implications from the Mahsa
   Amini movement},
Journal = {SOCIAL MOVEMENT STUDIES},
Year = {2024},
Month = {2024 SEP 25},
Abstract = {This profile focuses on the 2022 Mahsa Amini protests and analyzes their
   transnational implications. I review how the movement rapidly
   transcended Iran's national boundaries and catalyzed subnational and
   international activism, solidarity, and discourse engagement. Through an
   analysis of social media campaigns and engagement, offline solidarity
   demonstrations, and transnational grievance framing, this profile
   illustrates the significance of the Mahsa Amini campaign, galvanizing
   outrage over women's rights, oppression, and freedom within Iran, and
   subsequently activating a global network of activists and sympathizers.
   The protests provide a unique opportunity to analyze the complexities of
   transnational activism in the digital age by examining how activists
   utilized digital infrastructure to mobilize a global solidarity network
   to raise awareness of gender-based violence and brutality.},
