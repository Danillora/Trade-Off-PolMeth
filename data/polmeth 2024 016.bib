@article{ WOS:001365813000001,
Author = {Mathew, Nisha},
Title = {Sovereignty on the move: gold, financial capital and a neoliberal state
   in India},
Journal = {TERRITORY POLITICS GOVERNANCE},
Year = {2025},
Volume = {13},
Number = {6, SI},
Pages = {884-899},
Month = {JUL 3},
Abstract = {In February 2022, India entered into a Comprehensive Economic
   Partnership Agreement (CEPA) with the United Arab Emirates (UAE), at the
   core of which lies the gold jewellery sector. Critics observe that the
   partnership, contrary to its stated objective of boosting manufacturing
   in India, is only abetting the `circular routing of gold' via the UAE,
   which, given the legal dispersion of jewellery firms between the two
   sovereign jurisdictions, remains an effective method of evading taxes in
   the former and regulations in the latter. This article reads the Indian
   state's wilful blindness to tax evasion as a sign of its recalibrated
   relationship and shared sovereignty arrangements with financial capital
   within and beyond its political territory. It probes the rise of India's
   neoliberal state by charting the course of gold as a financial
   instrument through the latter half of the twentieth century and the part
   played by Dubai's tax haven strategy in it.},
