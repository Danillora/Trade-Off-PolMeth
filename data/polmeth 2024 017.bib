@article{ WOS:001262808500003,
Author = {Peters, Margaret E. and Borang, Frida and Kalm, Sara and Lindvall,
   Johannes and Shin, Adrian J.},
Title = {Historical Immigration Policies: Trends and Lessons},
Journal = {INTERNATIONAL STUDIES QUARTERLY},
Year = {2024},
