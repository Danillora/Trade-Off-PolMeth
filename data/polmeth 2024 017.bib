@article{ WOS:001376706300003,
Author = {Heinze, Eric},
Title = {Identitarian Hatreds and Disloyalty Propaganda: Revisiting the
   Eurocentrism of Human Rights},
Journal = {HUMAN RIGHTS QUARTERLY},
Year = {2024},
Volume = {46},
Number = {4},
Month = {NOV},
Abstract = {Human rights law has been dismissed as Eurocentric in situations where
   it has been invoked to justify neo-liberal power relations. Yet
   Eurocentrism also works in other ways. Human rights experts have adopted
   notions of ``hatred{''} patterned on Western-dominated histories, then
   have subsequently reproduced these notions in the form of identity-based
   categories such as race, religion, gender, or sexuality. However, these
   same experts have overlooked millions of victims of
   government-orchestrated, loyalty-based hostilities. Despite ongoing
   disagreements about whether or how to penalize individual citizens for
   extreme speech acts, human rights bodies should expand their conceptions
   of hatred to include abuses rooted in histories other than those modeled
   on Western violations.},
