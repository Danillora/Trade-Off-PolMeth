@article{ WOS:001187942900001,
Author = {Artiga-Purcell, James Alejandro},
Title = {Relational resources: Moving from plural to entangled extractivisms},
Journal = {POLITICAL GEOGRAPHY},
Year = {2024},
Volume = {110},
Month = {APR},
Abstract = {Increasing studies of extractivisms, in the plural, examine the diverse
   politics that permeate extractive activities. However useful, this
   appeal to plurality is problematic. Plurality without relationality
   risks justifying some extractive activities over others, without
   addressing how they relate. Drawing on anti-essentialist, decolonial,
   and political ecological scholarship, this article proposes an
   ontological reframing of extractivisms that troubles the division and
   comparison that pervade notions of taxonomic plurality, and emphasizes
   the mutual constitution articulated within the concept of relationality.
   Analyses of relational resources are better equipped to illuminate the
   power-laden associations through which supposedly distinct commodity
   values, extractive geographies, sectors, and governance regimes shape
   one another. Beyond complicating rigid definitions of extractive
   categories, this ontological shift requires distinct methodologies for
   understanding and contesting entangled extractive interests and logics.
   It implicates what forms of anti-extractive resistance, solidarities,
   and alternative futures become viable, thinkable, and necessary.},
