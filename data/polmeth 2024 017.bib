@article{ WOS:001190706200001,
Author = {Nemeckova, Tereza and Malzi, Mohamed Jaouad},
Title = {Unveiling Moroccan perspectives on the EU-Morocco Green Partnership:
   assessing its potential for a sustainable future for Morocco},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2025},
Volume = {34},
Number = {1},
Pages = {27-48},
Month = {JAN 2},
Abstract = {In October 2022, the EU and Morocco signed an unprecedented Green
   Partnership on Energy, Climate, and the Environment. The new deal aims
   to advance the external dimension of the European Green Deal and is
   expected to become a model for similar partnerships with other
   countries. In this study, we aim to explore the Moroccan perceptions of
   the Partnership. We utilize a thematic and content analysis of 31
   official documents, including media, along with semi-structured
   interviews with 22 key Moroccan stakeholders, not only representing the
   official authorities, but also businesses and academia. Results reveal
   that while the official political narrative emphasises the Partnership's
   benefits, divergent opinions emerged from our analysis, raising various
   concerns, especially regarding perceived benefits for Morocco and its
   sustainable development.},
