@article{ WOS:001357346000001,
Author = {Nurullayev, Dmitriy and Xu, Ping and Garand, James},
Title = {COVID, elite rhetoric, and Americans' attitudes toward US trade
   practices with China},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2024},
Volume = {105},
Number = {7},
Pages = {2236-2251},
Month = {DEC},
Abstract = {ObjectivesIn this article, we consider how the COVID-19 pandemic and
   evaluations of Donald Trump combined to influence Americans' attitudes
   toward U.S. trade practices with China. We suggest that psychological
   factors-such as confirmation bias and transfer of grievances-influence
   how individuals evaluate the costs and benefits of trade. Specifically,
   we argue that the severity of the pandemic (reflected by both cumulative
   deaths and news coverage of the pandemic) and the rhetoric of (then)
   President Trump came together to influence Americans' attitudes toward
   trade with China.MethodsTo assess the COVID effect, the Trump effect,
   and their interaction effect on trade opinion, we merge data from 39
   waves of the weekly Nationscape surveys, conducted from October 2019 to
   July 2020 (N = 62,171), with data on weekly COVID coverage for nine
