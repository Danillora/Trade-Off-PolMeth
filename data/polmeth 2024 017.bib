@article{ WOS:001214921500001,
Author = {Khoo, Su Voon and Rahman, Nur Hairani A. and Kamil, Nurul Liyana M.},
Title = {An evaluation of the influence of budgeting process on budget
   performance in Malaysia},
Journal = {PUBLIC ADMINISTRATION AND POLICY-AN ASIA-PACIFIC JOURNAL},
