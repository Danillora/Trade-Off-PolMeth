@article{ WOS:001208117400003,
Author = {Villarrubia, IngrId Y. Rosas and Rojas, Dora E. Ayala},
Title = {Situational Circumstances in Access to Justice for Women Victims of
   Domestic Violence},
Journal = {REVISTA ESTUDIOS SOCIO-JURIDICOS},
Year = {2024},
Volume = {26},
Number = {1},
Month = {JAN-JUN},
Abstract = {This article deepens the study of the factors that influence access to
   justice for women victims of domestic violence that un Women (2018)
   identifies as situational circumstances. The objective is to analyze the
   situational circumstances and their relationship with the right of
   access to justice for women victims of domestic violence. The
   methodology used was qualitative, analytical inductive logic. The
   present situational circumstances limit the opportunity for women to
   exercise the right of access to justice under conditions of equality in
   opposition to the principle of universality, placing the validity of
   this fundamental right in crisis.},
