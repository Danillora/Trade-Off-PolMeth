@article{ WOS:001193946600001,
Author = {Schwinges, Alexandra and Lock, Irina and van der Meer, Toni G. L. A. and
   Vliegenthart, Rens},
Title = {Stepping on Toes? Role Dynamics between Journalists and Lobbyists
   Regarding Big Tech's Accountability Agenda},
