@article{ WOS:001274864600004,
Author = {Franco, Jairo Vladimir Llano and Garcia, Maria Stephania Aponte},
Title = {State of the Art: Studies of Anthropology and Legal Sociology in the
   North of Cauca},
Journal = {REVISTA ESTUDIOS SOCIO-JURIDICOS},
Year = {2024},
Volume = {26},
Number = {2},
Month = {JUL-DEC},
Abstract = {This article illustrates the critical intersection between cultural
   diversity, armed conflicts, and transitional justice efforts in Northern
   Cauca. Despite facing persistent challenges from armed conflict and
   resource exploitation, ethnic communities have demonstrated remarkable
   resilience, adapting legal and organizational strategies to advocate for
   their rights and preserve their rich cultural heritage. This study
   contributes to the academic understanding of these complex phenomena and
   highlights the need for more integrated and culturally sensitive
   approaches in policymaking and peace and justice practices.
   Methodologically, publications from legal anthropologists, sociologists,
   and lawyers were reviewed, complemented with field experiences and a
   critical analysis of the data. The historicocritical perspective in a
   region of rich biodiversity is emphasized, examining the potential
   transformations of the conflict and transitional justice.},
