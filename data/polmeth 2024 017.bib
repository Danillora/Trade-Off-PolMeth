@article{ WOS:001358820700003,
Author = {Waage, Hilde Henriksen},
Title = {English: From Israel's to the Palestinians' Best Friend?},
Journal = {INTERNASJONAL POLITIKK},
Year = {2024},
Volume = {82},
Number = {2},
Pages = {177-191},
Abstract = {After 1948, Norway became one of Israel's closest friends. The fate of
   the Palestinians did not arouse any interest. After the war in 1967,
   these attitudes changed. Israel had become an occupying power. The
   Palestinian refugee problem remained unresolved. The PLO had also
   changed its policy and advocated for a two-state solution. Nevertheless,
   Norway had the most restrictive policy towards the PLO in all of Western
   Europe. It was not until 1986 that the PLO received an information
   office in Norway. In 1989, Norway made its first official visit to the
   PLO in Tunis. Throughout the 1980s, the Labour Party was the driving
   force behind the new Norwegian policy. It was built on the new trust
   relationship with the PLO and the close relationship with Israel. In
   1993, Norway negotiated the Oslo Accords. Norway's close relationship
   with Israel had made it possible. The PLO also realized that it needed a
   friend of Israel. However, there was no longer strong sympathy for
   Israel in Norway. Norway's role forced Norway to pursue a policy
   acceptable for Israel. Only during the Gaza War in 2023 has Norway been
   at the forefront internationally in efforts to end the war and initiate
   negotiations for a two-state solution.},
