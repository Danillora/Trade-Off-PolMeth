@article{ WOS:001134458600001,
Author = {Polacko, Matthew and Graefe, Peter and Kiss, Simon},
Title = {Subjective economic insecurity and attitudes toward immigration and
   feminists among voters on the Right in Canada},
Journal = {SOCIAL SCIENCE QUARTERLY},
Year = {2024},
Volume = {105},
Number = {2},
Pages = {281-295},
Month = {MAR},
Abstract = {The recent success of far-right populism has led to a wave of public
   attention to its causes. Drawing on novel survey items from the Canadian
   Election Study, we investigate how economic insecurity, mediated by
   immigration and feminist attitudes, affects voting behavior. While
   economic distress has been linked to far-right voting in Europe, we find
   support that subjective measures of economic distress are significantly
   associated with voting for the Right in Canada, when interacting with
   immigration and feminist attitudes. However, the association only holds
   for men. The findings suggest that Canada, despite its comparatively
   weak nativist cleavage, is not immune to tensions arising from
   globalization and economic insecurity.},
