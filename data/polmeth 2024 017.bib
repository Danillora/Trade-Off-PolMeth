@article{ WOS:001309554100001,
Author = {Pittaluga, Giovanni B. and Seghezza, Elena and Morelli, Pierluigi},
Title = {Media fabrication of corruption and the quality of the political class:
   The case of Italy},
Journal = {EUROPEAN JOURNAL OF POLITICAL ECONOMY},
Year = {2024},
Volume = {84},
Month = {SEP},
Abstract = {Since the global financial crisis of 2008, the level of perceived
   corruption in Italy has risen markedly, diverging significantly from the
   perceived corruption of other high-income countries and from the
   corruption as experienced. We propose that newspapers, in order to
   maximize their profits, have given directed emphasis to episodes of
   political corruption, which has contributed to the increase in perceived
   corruption. The consequence has been loss of parliament's credibility
   and a deterioration in the quality of the Italian political class.},
