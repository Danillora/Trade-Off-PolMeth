@article{ WOS:001299394700001,
Author = {Yang, Yunkang and Mccabe, Stefan and Hindman, Matthew},
Title = {Does Russian Propaganda Lead or Follow? Topic Coverage, User Engagement,
   and RT and Sputnik's Agenda Influence on US Media},
