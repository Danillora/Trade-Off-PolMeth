@article{ WOS:001184825800001,
Author = {Coe, Jessie and Schwam, Daniel and Ramchand, Rajeev and Farmer, Carrie},
Title = {American Support of Public Programs for Veterans: Estimates From a
   National Survey Including a Discrete Choice Experiment},
Journal = {ARMED FORCES \& SOCIETY},
