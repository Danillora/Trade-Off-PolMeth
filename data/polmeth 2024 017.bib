@article{ WOS:001384842900002,
Author = {Millona, Kristina},
Title = {The cruising past of men seeking same-sex encounters under Albanian
   communism},
Journal = {INTERSECTIONS-EAST EUROPEAN JOURNAL OF SOCIETY AND POLITICS},
