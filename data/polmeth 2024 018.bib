@article{ WOS:001293982300001,
Author = {Lundgren, Magnus and Janson, Emma and Lundqvist, Martin},
Title = {Introducing the Proscription of Armed Actors Dataset},
Journal = {JOURNAL OF PEACE RESEARCH},
