@article{ WOS:001178870000001,
Author = {Gross, Irena Grudzinska and Kwiatkowska-Moskalewicz, Katarzyna},
Title = {The Great Terror in 1930s Kharkiv: The Case of Konrad Weisselberg and
   Anna Mykalo},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
Year = {2025},
Volume = {39},
Number = {1},
Pages = {80-99},
Month = {FEB},
Abstract = {The article presents the fate of Konrad Weisselberg (1905-1937) and his
   wife Anna (Galia) Mykalo (1914-1989), shedding light on the
   intensification of violence in Kharkiv in the 1930s and on the
   mechanisms of the Great Terror. Weisselberg was a chemist, an Austrian
   communist of Jewish origin, an ideological immigrant to the USSR, and a
   little-known victim of Stalin's purges. Anna Mykalo was a peasant worker
   who moved to Kharkiv just before the Stalinist regime locked peasants in
   the countryside. The couple was associated with the Ukrainian Institute
   of Physics and Technology (UPTI), where prominent physicists-Soviet and
   foreign-worked in the 1930s. Their story is linked to post-revolutionary
   Kharkiv, a city of industrial utopia and the site of the Great Famine
   and the Great Terror.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Gross, IG (Corresponding Author), Princeton Univ, Princeton, NJ 08544 USA.
