@article{ WOS:001481895000003,
Author = {Miranda, Arnaud},
Title = {Ecological rhetoric of the French national-populist movement: a
   Christian origin?},
Journal = {ETUDES INTERNATIONALES},
Year = {2024},
Volume = {55},
Number = {2},
Pages = {177-199},
Month = {SUM},
Abstract = {French right-wing populists tend to include more and more ecology in
   their discourses. Against a left-wing ecology, supposedly elitist and
   ``bobo{''}, national populists defend a rooted ecology. In this paper, I
   will focus on the genealogy of this motif. Is conservative ecology a
   unitary concept ? Moreover, since it is used by religious actors, can we
   consider it as an expression of a Christian imaginary ? I wish to
   develop a threefold reasoning : study the uses of ecology in French
   national populist discourses (I), understand the intellectual ground for
   this appropriation (II) and finally determine if this tradition relies
   on a Christian imaginary (III).},
