@article{ WOS:001537735000001,
Author = {Menegat, Marildo},
Title = {Dissolutive states of civil war: between predation and self-destruction},
Journal = {CRITICAL MILITARY STUDIES},
Year = {2024},
Volume = {10},
Number = {2, SI},
Pages = {211-228},
Month = {APR 2},
Abstract = {In this article, I critically analyse the transformations of
   contemporary politics and its relationship with war and economy. Here,
   the end of the Cold War is interpreted as the result of a structural
   crisis of capitalism, initiated in the 1970s. Ever since, the times have
   been unstable due to the expansion of the last superpower's space of
   domination, sustained with declining resources. Under these
   circumstances, the United States is compelled to world ordering wars,
   aimed at preserving the credibility of the power of its weapons and
   dollars. If, in the past, war often took the place of politics, and
   since World War II, politics became increasingly subordinated to
   economy, in the current crisis, these m \& eacute;langes continue to
   reproduce themselves - now in a sort of political economy of barbarism
   resulting from an entanglement of war, economy, and politics. This is so
   because the gravity of the economic crisis (which makes it the core of
   social life) requires such fusion - witnessed in the epidemics of civil
   wars, conducted by de-ideologized armed gangs, exerting power in small
   territories. These groups are the expression of this recrudescence, in
   which this modality of power, sustained in the debris of what the state
   once was, follows the economy's need for prey, whether on a small or
   large scale. In order to advance the unreality of accumulation, this
   situation leads to an alliance of globalized capital, which demands this
   dirty work, with its gangs, now elevated to a model for immediate
   management of barbarism as local-global ordering.},
