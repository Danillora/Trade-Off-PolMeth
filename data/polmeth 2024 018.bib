@article{ WOS:001368435400009,
Author = {Koc, Fatih and Akdogan, Muzaffer and Yesildal, Ali and Kiray, Abdullah
   and Koc, Oktay},
Title = {Serving with Values in Challenging Times: Exploring Institutional
   Leaders' Values Work at Local Level During COVID-19},
