@article{ WOS:001424840500002,
Author = {Amoretti, Francesco and Fracchiolla, Domenico},
Title = {Cybersecurity as Constituent Policy: The Case of the USA},
Journal = {RIVISTA ITALIANA DI POLITICHE PUBBLICHE},
Year = {2024},
Number = {1},
Pages = {19-44},
Month = {APR},
Abstract = {The article advances the hypothesis that cybersecurity policies can be
   conceived in terms of constituent policies. This analytical perspective,
   starting from the contributions of Lawrence Lessig and Theodore Lowi,
   appears to be particularly fruitful for the understanding of cyber
   security policies. The case study chosen is that of the United States.
   The analysis focuses on the dimension of the network architectures and
   on the policy choices prevailing in the different phases of development
   of cyber security policies: from the origins to George W. Bush, up to
   the experience of Barack Obama and the focus on the presidencies of
   Donald Trump and Joe Biden that allows us to highlight precisely a line
   of continuity that can be found in cybersecurity strategies and
   policies.},
