@article{ WOS:001335087100001,
Author = {Strassheim, Holger},
Title = {A world of evidence: the global spread and silent politics of evidence
   cultures},
Journal = {POLICY AND SOCIETY},
Year = {2024},
Volume = {43},
Number = {4},
Pages = {414-431},
Month = {OCT 18},
Abstract = {How can we explain the worldwide spread of evidence-based policymaking
   despite continuous criticism? What are the underlying mechanisms of its
   persistence on a global scale? This article aims at answering these
   questions by focusing on the cultural constellations in which evidence
   is imbued with political as well as epistemic authority. Evidence
   cultures are discursive and institutional forces (re-)producing both the
   scientific validation of knowledge and its relevance in policymaking.
   They need to be understood as self-propagating constellations of
   interlinking science and policy through practices, discourses and
   institutionally sedimented regulations. Evidence is the product of
   chains of practices in which the initial knowledge struggles are
   gradually made publicly invisible and often inaccessible. The article
