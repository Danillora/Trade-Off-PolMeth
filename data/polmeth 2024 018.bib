@article{ WOS:001250542800001,
Author = {Bateman, Aaron},
Title = {The weakest link: The vulnerability of US and allied global information
   networks in the nuclear age},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2025},
Volume = {48},
Number = {1},
Pages = {156-185},
Month = {JAN 2},
Abstract = {The arrival of the nuclear age highlighted the inadequacy of U.S. and
   allied information networks for transmitting vital messages across the
   globe. Submarine cables could be cut and radio easily jammed. U.S. and
   allied officials looked to emerging space technologies to compensate for
   the inadequacies of radio and submarine cables and to reduce their
   dependence on terrestrial telecommunications hardware. But satellites
   were also vulnerable and insufficient for meeting all communications
   requirements. Consequently, creating layered telecommunications networks
   that stretched from under the sea, across the land, and into space
   emerged as the optimal strategy for strengthening information security.},
