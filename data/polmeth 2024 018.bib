@article{ WOS:001331021700001,
Author = {Munis, B. Kal and Nemerever, Zoe},
Title = {Rural residency, rural resentment, and attitudes toward public land
   management in the United States},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2025},
Volume = {34},
Number = {5},
Pages = {903-924},
Month = {JUL 29},
Abstract = {Disparate urban and rural lived experiences contribute to the geographic
   divide in American politics. We expect this to be especially true for
   environmental politics because of urban-rural differences in exposure to
   natural resources. We demonstrate that Americans are broadly supportive
   of public lands and that ruralites are slightly more supportive of
   public land management compared to the non-rural. However, a crucial
   exception to general rural attitudes is highly resentful ruralites are
   uniquely and dramatically unsupportive of public lands and direct their
   resentment at the federal government especially. This research clarifies
   the nationalized nature of the urban-rural divide by highlighting that
   the preferences of resentful rural Americans are in stark contrast to
   the rest of society, even the rest of rural America. Nevertheless, when
   a minority's unpopular opinions on the management of America's natural
   resources overshadow more moderate and modal positions it fuels fringe
   movements like the Sagebrush Rebellion.},
