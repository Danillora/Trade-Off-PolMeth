@article{ WOS:001223185300001,
Author = {O'Dwyer, Conor},
Title = {Backsliding versus Backlash: Do Challenges to Democracy in East Central
   Europe Threaten LGBTQIAP Empowerment?},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
Year = {2024},
