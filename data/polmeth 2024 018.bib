@article{ WOS:001381796300001,
Author = {Pancheva, Gabriela and Ardhelas, Amri Azis and Gil, Andrea Torresagaton
   and Spencer, Alexander},
Title = {`Russian warship, go fuck yourself': Romantic narratives of the hero in
   the war of Ukraine},
