@article{ WOS:001343813100001,
Author = {bin Oslan, Afiq},
Title = {Economic origins of border fortifications},
Journal = {JOURNAL OF PEACE RESEARCH},
Year = {2025},
Volume = {62},
Number = {4},
Pages = {882-896},
Month = {JUL},
Abstract = {Why do contemporary states fortify their borders? Modern military
   advancements have made such fortifications obsolete for security, yet
   scholars have offered no satisfactory alternative theory. I propose a
   theory of fortifications with economic motivations using a
   game-theoretic model where states compete to extract wealth over a
   shared population around a border. Such competition generates
   inefficiency and states have the option to construct fortifications to
   disrupt competition. Fortifications contain the wealth of citizens
   inside the state to be taxed and enforce efficient monopolies of
   extraction. States hence fortify when such profits outweigh short-term
   expenses. The models suggest that we should expect fortifications
   between territories of unequal economic capacities as richer states have
   more to lose from inefficient competition, complementing existing
   empirical results.},
