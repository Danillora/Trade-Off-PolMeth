@article{ WOS:001226854900001,
Author = {Palestini, Stefano and Villarroel, Yancy},
Title = {`What gives you the right?' Foreign policymakers' perceptions of the
   legitimacy of sanctions against democratic breakdown in Venezuela
   (2014-2019)},
Journal = {CAMBRIDGE REVIEW OF INTERNATIONAL AFFAIRS},
Year = {2024},
Volume = {37},
Number = {5},
Pages = {665-689},
Month = {SEP 2},
Abstract = {Drawing on contemporary debates on legitimacy and contestation in
   international relations, the article offers a novel analytical framework
   to study processes of legitimation and delegitimation of international
   sanctions. We apply the framework to analyse foreign policymakers'
   evaluations of the legitimacy of the sanctions imposed by states and
   international organisations against Nicol \& aacute;s Maduro's regime in
   Venezuela. The content analysis of evaluative statements shows that
   (de)legitimation processes focus mostly on who has the right to enforce
   sanctions and reveals differentiated legitimacy evaluations of
   multilateral versus unilateral enforcers, as well as of different types
   of international organisations. Sanctions imposed by multilateral
   senders and with a restricted impact on targeted individuals are
   evaluated as more legitimate than unilateral sanctions with a wider
   scope. We argue that the study of the (de)legitimation of sanctions may
   have important implications not only for enhancing compliance, but
   especially for transforming international sanctions into a legitimate
   mechanism of global governance.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
