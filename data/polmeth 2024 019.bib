@article{ WOS:001379499800001,
Author = {Illes, Gabor and Laszlo, Tamas},
Title = {Crowds and plebiscitary representation: Rituals of presence in the Orbán
   regime},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
Year = {2025},
Volume = {27},
Number = {4},
Pages = {1341-1362},
Month = {NOV},
Abstract = {The article aims to characterise the role of crowd events in
   plebiscitary representation, complementing the original
   Weberian-Schmittian characterisation with Hans-Ulrich Gumbrecht's
   notions of crowd and presence ritual. In addition, it shows the
   relevance of the theoretical argument by analysing the role three types
   of presence rituals play in maintaining Viktor Orb \& aacute;n's
   plebiscitary regime in Hungary, based on available video footage of the
   events and Orb \& aacute;n's speeches. Such an investigation has a
   threefold relevance. First, it contributes to understanding Orb \&
   aacute;n's domestic successes by emphasising a hitherto underappreciated
   aspect of his politics. Second, focusing on presence rituals' bodily and
   sensory elements complements existing research on populist rallies.
   Third, it contributes to constructivist approaches to representation and
   the evolving interdisciplinary field of performative representation by
   highlighting, following Gumbrecht, the role oscillations between
   `presence-effects' (sensorial-bodily processes) and `meaning-effects'
   (claim-making and the use of symbols) play in the process of making
   constituencies.},
