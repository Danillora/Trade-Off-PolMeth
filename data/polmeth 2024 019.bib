@article{ WOS:001320052900001,
Author = {Caton, Cora and Carter, Brittnee},
Title = {Women in Militant Movements: The Role of the State in Driving
   Recruitment},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2024},
Month = {2024 MAY 10},
Abstract = {What is the role of the state regarding levels of women's presence in
   militant organizations? Most research examining women's presence in
   militant groups consider only group-level indicators to explain why
   militant groups employ women in their ranks, in both passive and active
   roles. Some burgeoning literature in this realm has also used interview
   data to consider the agency of the women themselves regarding their
   choice to join the organizations. However, scholars have yet to examine
   the role of the state in either hindering or facilitating women's
   presence in militant organizations. We argue that state exclusion of
   women from state political, economic, and social rights and protections
   will significantly impact the presence of women in militant
   organizations, but that this relationship is more complex than current
   theory would predict due to disparity evident in the gendered context of
   the state.},
