@article{ WOS:001147627700001,
Author = {Sommar, Carl-Johan and Nordensvard, Johan and Wihlborg, Elin and Garcia,
   Fredrik},
Title = {Autonomy and paternalism - framing Swedish COVID-19 restriction policy},
Journal = {CRITICAL POLICY STUDIES},
