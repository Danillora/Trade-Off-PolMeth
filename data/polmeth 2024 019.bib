@article{ WOS:001281796700001,
Author = {Carrieri, Luca and Morini, Marco},
Title = {The European Union, immigration and the Left-Right divide: Explaining
   voting preferences for Western European radical right parties},
Journal = {POLITICS},
Year = {2024},
Month = {2024 JUL 31},
Abstract = {In the past two decades, radical right parties gained ground, becoming
   relevant political actors in several European countries. This article
   presents an intriguing study on their voting preferences in Western
   Europe. It analyses whether and to what extent the effect of the
   distance between individual and party positions on the European Union,
   immigration and the Left-Right positioning has changed over time
   (between 2009 and 2019). This is examined by combining data from the
   Chapel Hill Expert Survey and the European Election Studies. The results
   suggest that the distance between Left-Right self-positioning and party
   position has become more important in explaining voting for radical
   right parties over time.},
