@article{ WOS:001372891000001,
Author = {de Castro, Monica Ottoboni Maciel and Ramanzini, Isabela Gerbelli Garbin},
Title = {Universal Periodic Review: contemporaneity, originality and research
   agendas},
Journal = {REVISTA BRASILEIRA DE POLITICA INTERNACIONAL},
Year = {2024},
Volume = {67},
Number = {2},
Abstract = {The Universal Periodic Review (UPR) is the most recent addition to the
   United Nations' mechanisms to monitor human rights worldwide. Its
   creation in 2007 symbolized a more cooperative and less asymmetrical
   alternative for the treatment of human rights than more traditional
   mechanisms. The body of literature on UPR usually offers two attributes
   to it: contemporaneity and originality. While both generically denote
   the emergence of a ``new{''} element in the international system;
   individually, each one makes up a different idea. This article proposes
   a precise and analytically dense elaboration on these attributes by
   examining (i) the political-normative paradigm at its creation, as well
   as (ii) the UPR's originality factors compared to other traditional
   human rights mechanisms. This specific exam helps resolve possible
   analytical-conceptual mismatches between the UPR and the more
   traditional human rights mechanisms. It also allows for the
   identification of gaps and potentials to expand the academic knowledge
   and the professional performance in the UPR's practical field.},
