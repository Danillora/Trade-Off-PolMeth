@article{ WOS:001194252500001,
Author = {Pavenstaedt, Christopher N. and Roedder, Simone},
Title = {Between evidence first and political fight - understanding dynamics of
   (de-)politicization in US climate movements' future narratives},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2024},
Volume = {33},
Number = {6},
Pages = {1065-1086},
Month = {SEP 18},
Abstract = {Fridays for Future, Extinction Rebellion and Sunrise Movement are among
   the new climate movements that have come into the public spotlight since
   2019. While their successes in agenda-setting are undisputed,
   conclusions vary on whether and how this re-politicizes climate
   discourses. This article presents a new framework to analyze the
   dynamics of (de-)politicization along the dimensions of vision, agency
   and process. Based on a narrative analysis, we compare US movement and
   media documents from 2019 and identify five key narratives, namely,
   `Evidence First,' `Intergenerational Divide,' `Climate Justice \&
   Intersectionality,' `System Change' and `Political Fight.' While all
   movements use politicizing notions, we find that tensions between the
   dimensions inhibit the articulation of alternative visions of the
   future. This overall depoliticizing tendency appears to be rooted in
