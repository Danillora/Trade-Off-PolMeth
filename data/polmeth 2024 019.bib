@article{ WOS:001537733200001,
Author = {Manchanda, Nivi},
Title = {The Janus-faced nature of militarization},
Journal = {CRITICAL MILITARY STUDIES},
Year = {2024},
Volume = {10},
Number = {2, SI},
Pages = {137-141},
Month = {APR 2},
Abstract = {In this short intervention I reflect on the limits of the concept of
   militarization, as well as its appeal. The piece argues that
   militarization largely ignores those whose lives are disposable in the
   eyes of the liberal state. It also submits that one of the greatest
   shortcomings of militarization is its failure to grapple with the
   histories and present of racism and capitalism, making `militarization'
   a convenient shorthand, rather than a sustained interrogation of the
   politics of dispossession and carcerality. Ultimately, militarization
   promises more than it delivers, and therefore can be expunged from our
   collective lexicon without any material consequences.},
