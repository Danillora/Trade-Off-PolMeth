@article{ WOS:001257285900001,
Author = {Ambrose, Graham and Siddiki, Saba},
Title = {Assessing drivers of sustained engagement in collaborative governance
   arrangements},
Journal = {JOURNAL OF PUBLIC ADMINISTRATION RESEARCH AND THEORY},
