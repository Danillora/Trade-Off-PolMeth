@article{ WOS:001358820700009,
Author = {Johansen, Raymond},
Title = {Break with the Occupation: Norwegian People's Aid and the Struggle for a
   Free Palestine},
Journal = {INTERNASJONAL POLITIKK},
Year = {2024},
Volume = {82},
Number = {2},
Pages = {247-257},
Abstract = {The possible genocide that is now unfolding in Gaza shows how the
   world's long-standing efforts to resolve the conflict between Palestine
   and Israel have failed. For many years, the international community has
   dealt with the occupying power Israel and the occupied Palestinians
   without taking into account the obvious imbalance of power between
   occupied and occupier, between a sovereign state and an ``area{''}. This
   has had disastrous consequences for the possibility of a political
   solution, and with each passing year, the occupation has dug deeper into
   Palestinian society, so deep that this society is in danger suffocation.
   It is now imperative that we understand what has gone wrong and what is
   necessary to ensure that the road ahead does not end in genocide and
   annexation. Such a situation would mean the end of the Palestinians'
   hope for self-determination in their own land, and perpetual insecurity
   for the people of Israel. The credibility of the international
   community's shared rules and institutions and our ability to promote
   peace and development through these rules and institutions in the future
   are also at stake.},
