@article{ WOS:001173500200001,
Author = {King, Anthony},
Title = {Robot wars: Autonomous drone swarms and the battlefield of the future},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2024},
Volume = {47},
Number = {2},
Pages = {185-213},
Month = {FEB 23},
Abstract = {We seem to be on the cusp of an AI-driven revolution in military
   affairs. Scholars have explored many aspects of this revolution but one
   of the most vibrant debates has addressed the question of lethal
   autonomous weapons. Some scholars believe that autonomous weapons, and
   especially autonomous drone swarms, are about to colonise the
   battlefield. This paper assesses this argument. It identifies three
   common mistakes in discussions of lethal autonomy. Scholars overestimate
   the capability of autonomous drone swarms. They underestimate their
   dependence on other weapon systems. Finally, they presume that
   autonomous weapons will favour the offence. This paper rejects all three
   claims.},
