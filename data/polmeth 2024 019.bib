@article{ WOS:001379966000001,
Author = {Jofre, Maria and Aziani, Alberto and Villa, Edoardo},
Title = {Terrorist Financing: Traditional vs. Emerging Financial Technologies},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2024},
Month = {2024 DEC 19},
Abstract = {This study examines the use of traditional systems and emerging
   financial technologies for terrorist financing based on a dataset
   comprising 121 cases around the world. While there are speculations that
   terrorists are largely adopting emerging FinTechs like cryptocurrencies,
   our exploratory findings indicate that only 7 percent of cases involve
   such technologies. Traditional payment systems are still dominant,
   accounting for 93 percent of cases. Terrorist financiers primarily rely
   on the banking system and employ methods such as foreign shell
   companies, money transfers, Hawala systems, and high-value commodities.
   Research and policy implications for law enforcement and public
   authorities are discussed.},
