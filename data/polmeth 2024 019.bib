@article{ WOS:001368470700001,
Author = {Trinidad, Carlos Benitez and Bicalho, Poliene Soares dos Santos},
Title = {From Disappearance to Hope: The Construction of the Brazilian Indigenous
   Movement's Imaginary (1974-1977)},
Journal = {LATIN AMERICAN PERSPECTIVES},
