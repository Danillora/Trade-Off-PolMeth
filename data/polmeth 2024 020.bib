@article{ WOS:001466186400007,
Author = {Friesen, Isaac},
Title = {Local Ethics and Conflicts of Interest: Living Between Bottom-Up from
   Top-Down Peace in Provincial Egypt},
Journal = {JOURNAL OF PEACEBUILDING \& DEVELOPMENT},
