@article{ WOS:001445413200004,
Author = {Hokayem, Emile},
Title = {The Death of Nasrallah and the Fate of Lebanon},
Journal = {SURVIVAL},
Year = {2024},
Volume = {66},
Number = {6},
Pages = {33-40},
Month = {NOV 1},
Abstract = {The killing by Israel of Hassan Nasrallah, who had served as Lebanese
   Hizbullah's charismatic leader since 1992, is a turning point not just
   for the organisation he led, but for Lebanon and indeed the whole of the
   Levant, as well as for Iran's strategy of influence. By establishing a
   linkage between Hizbullah's military activities against Israel and a
   ceasefire in Gaza that never came, Nasrallah effectively cornered
   himself. He also found out that Iranian involvement in the war would be
   restrained because Tehran could not risk a slide toward a direct
   confrontation with the United States. Nasrallah's death and Hizbullah's
   weakening have pushed Lebanon into the unknown. The war, unwanted by
   most Lebanese citizens, is being blamed as much on Hizbullah's
   recklessness as on Israel's brutality.},
