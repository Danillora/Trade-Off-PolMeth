@article{ WOS:001205262900001,
Author = {Cavari, Amnon and Mate, Akos and Sebok, Miklos},
Title = {Staying on the democratic script? A deep learning analysis of the
   speechmaking of US presidents(sic)(sic)(sic)Palabras Clave},
