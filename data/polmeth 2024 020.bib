@article{ WOS:001374882000001,
Author = {Senn, Martin and Wunderlich, Carmen},
Title = {The complexity of nuclear (dis)ordering: a research agenda},
Journal = {CAMBRIDGE REVIEW OF INTERNATIONAL AFFAIRS},
Year = {2025},
Volume = {38},
Number = {3, SI},
Pages = {259-279},
Month = {MAY 4},
Abstract = {This introduction to the Special Issue starts from the observation that,
   over time, the process of political ordering to harness nuclear energy
   has become more complex in terms of actors, rules, levels, and contexts.
   So far, however, the existing literature provides only limited insights
   into the complexity of nuclear ordering. To advance our understanding of
   the characteristics and consequences of this complexity, this
   introduction provides a conceptual framework for the subsequent articles
   of the Special Issue and lays out a broader research agenda. It does so
   by first addressing the notions of political ordering and disordering.
   It then draws on complexity theory to develop an analytical perspective
   through which the contributions to this Special Issue examine the
   complexity of nuclear (dis)ordering. Finally, the article reviews the
   contributions to the Special Issue, and outlines avenues for further
   research on nuclear (dis)ordering.},
