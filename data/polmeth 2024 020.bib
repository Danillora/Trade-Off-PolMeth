@article{ WOS:001296999300002,
Author = {Ewers-Peters, Nele Marianne},
Title = {Germany's positioning in the triangular relations with Russia and the
   US: towards a (new) European security order?},
Journal = {INTERNATIONAL POLITICS},
Year = {2024},
Month = {2024 AUG 24},
Abstract = {Growing great power competition, a changing security environment and the
   acclaimed de-institutionalisation of security policy in the twenty-first
   century demand adaptations in national security approaches and
   institutional settings. States have benefitted differently from the
   post-Cold War European security order. While Germany presents itself as
   a prime beneficiary from Europe's multilateral structures, the US has
   sought to shift its focus away from Europe, and Russia's foreign policy
   has become increasingly aggressive, eventually culminating in the return
   of war to Europe. Such developments lead to new transformations and
   fundamental re-arrangements of the present security order that require
   states to rethink their national foreign and security policies and
   relationships. This article examines the relations between Germany,
   Russia and the US using the analytical concept of triangular relations.
   Used as an analytical tool, the examination of the Germany-Russia-US
   triangular relationship helps to investigate and trace how we got to the
   contemporary European security order.},
