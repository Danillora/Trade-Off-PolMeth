@article{ WOS:001591638200008,
Author = {Klock, Carola and Carrillo Risi, Diana},
Title = {The (non-)cooperation of small islands in the Indian Ocean in climate
   negotiations},
Journal = {ETUDES INTERNATIONALES},
Year = {2024},
Volume = {55},
Number = {3},
Pages = {549-571},
Month = {FAL},
Abstract = {Small island developing states (SIDS) have collectively become a key
   actor in global climate negotiations, through the Alliance of Small
   Island States (AOSIS). In order to represent their regional interests
   within AOSIS, the Pacific and Caribbean SIDS collaborate regionally.
   Such a regional approach does not exist in the Indian Ocean. How can we
   make sense of this non coordination in the Indian Ocean? We address this
   question by studying four elements of coalition forma tion: the need for
   a collective approach; shared interests; a common iden tity; and the
   costs of coordination. Our analysis highlights the large diversity
   within the Indian Ocean and the lack of a common identity, but also and
   importantly the lack of a suitable regional organisation that could
   facilitate regional coordination within the climate negotiations.},
