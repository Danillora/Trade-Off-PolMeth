@article{ WOS:001251436200001,
Author = {Teitler, Anthony},
Title = {Washington and tragicomedy: emploting the `Chinese threat'},
Journal = {INTERNATIONAL POLITICS},
Year = {2024},
Month = {2024 JUN 19},
Abstract = {This study addresses the role of storytelling in making sense of,
   shaping, and giving direction to US foreign policy in relation to China
   from 2011 to the present. The article examines how the major
   contemporary construction of the `Chinese threat'is bound by particular
   narrative features, and with what effect. In this way the study develops
   on Hayden White's historiographical approach of emplotment with the
   specific contribution of a tragicomic plotline to show how Washington
   constructs a dangerous world hanging in the balance-in terms of who will
   lead the international order-but that it is determined to achieve a
   happy ending for itself and its partners. This contributes to the
   literature by introducing a significant sub-type of political narrative,
   which is central to how the United States has represented China since
   the Obama administration's `rebalance to Asia-Pacific.},
