@article{ WOS:001338300200002,
Author = {Mintrom, Michael and O'Connor, Ruby},
Title = {Policy coordination and development in a VUCA world},
Journal = {GLOBAL POLICY},
Year = {2024},
Volume = {15},
Number = {6},
Pages = {41-52},
Month = {SEP},
Abstract = {A growing bandwagon of commentators declare our collective existence is
   subject to volatility, uncertainty, complexity, and ambiguity. Welcome
   to the `VUCA world'. Since populations typically turn to governments for
   support when crises strike, there is merit in policy scholars paying
   close attention to the `VUCA world' narrative. This article assesses the
   implications of the VUCA narrative for policy professionals charged with
   conducting policy coordination and development. If volatility,
   uncertainty, complexity, and ambiguity are becoming more commonplace,
   what aspects of coordination and development work should change? We
   indicate how policy professionals might harness new technologies and
   integrate them with long-term planning tools to help governments
   anticipate and reduce the consequences of crisis events. A lot of the
   technology now at our fingertips can be usefully deployed to assist with
   improved policy coordination and development to rapidly and effectively
   respond to VUCA-like challenges. Further, more `stewardship' capability
   in and around government must be built to enable exploration of future
   scenarios so societies can stand ready to address anticipated
   eventualities. We conclude that the VUCA narrative can promote salutary
   improvements in the practice of policy coordination and development.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Mintrom, M (Corresponding Author), Monash Univ, Melbourne, Vic 3800, Australia.
   Mintrom, Michael; O'Connor, Ruby, Monash Univ, Melbourne, Vic 3800, Australia.},
DOI = {10.1111/1758-5899.13430},
