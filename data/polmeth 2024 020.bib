@article{ WOS:001335524700003,
Author = {Bayet, Camille},
Title = {Political resistance and resilient politicization: negotiating the
   Women, Peace and Security Agenda and Security Council resolution 2467},
Journal = {ETUDES INTERNATIONALES},
Year = {2024},
Volume = {55},
Number = {1},
Pages = {61-82},
Month = {SPR},
Abstract = {While many studies denounced the depoliticization and technocratization
   of the United Nations' Women, Peace and Security Agenda, the example of
   Security Council Resolution 2467 seems to suggest otherwise. Politics
   has not been erased from the fight against gender-based and sexual
   violence in times of conflict. Using a sociohistorical study of the
   conditions under which the resolution was negotiated and received, this
   article reveals a ``resilient politicization{''} of this UN normative
   framework. The ideological cleavages, the diplomatic pressure exerted by
   the US delegation, and the challenges raised by certain member states
   and civil society after the resolution's adoption, are all indicators of
   a resilient politization. In a context of ``multilateralism's
   crisis{''}, these negotiations ultimately invite us to rethink the
   traditional fault lines of the Security Council.},
