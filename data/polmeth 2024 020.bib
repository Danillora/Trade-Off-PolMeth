@article{ WOS:001271394400001,
Author = {Stauber, Jakub and Cirhan, Tomas},
Title = {Leader vs. the party dilemma: the case of a party rebirth in Czechia},
Journal = {EAST EUROPEAN POLITICS},
Year = {2024},
Volume = {40},
Number = {4},
Pages = {614-636},
Month = {OCT 1},
Abstract = {Influential individuals gaining political weight at the expense of their
   parties are succeeding. Through case studies of two Czech parties, we
   explore the role of personalised party leadership. The puzzling set of
   events that occurred when SPD reestablished itself in response to the
