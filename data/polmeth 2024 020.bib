@article{ WOS:001290588100001,
Author = {Ash, Konstantin and Shapovalov, Miroslav},
Title = {Returning Veterans' Attitudes Toward Democracy: Evidence From a Survey
   of Ukraine's ATO Veterans},
Journal = {JOURNAL OF CONFLICT RESOLUTION},
Year = {2025},
Volume = {69},
Number = {5},
Pages = {763-792},
Month = {MAY},
Abstract = {How is service history associated with returning veterans' attitudes
   about democracy? Existing research predicts pro-government militia
   veterans have less support for democracy because of political efficacy
   gained from service and divergent policy preferences from the general
   population. We test that theory in Ukraine through surveys of both
   returning veterans and the general population between 2019 and 2022. Our
   findings differ from predictions. Veterans who joined the armed forces
   as volunteers were more supportive of democracy as an institution than
   ordinary Ukrainians. At the same time, Army volunteers, as well as
   veterans who were drafted into the armed forces and veterans who were
   rejected from the armed forces and joined pro-government militias were
   more likely to be dissatisfied with democracy. In-depth interviews
   reveal both those rejected from the armed forces and army conscripts
   opposed democracy because they felt rampant draft evasion made civilians
   unqualified to make political decisions.},
