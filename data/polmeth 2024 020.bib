@article{ WOS:001215863900001,
Author = {Flores, Andrew and Strode, Dakota and Haider-Markel, Donald P.},
Title = {Political psychology and the study of LGBTQI plus groups, politics, and
   policy: Existing research and future directions},
Journal = {POLITICAL PSYCHOLOGY},
Year = {2025},
Volume = {46},
Number = {1},
Pages = {213-249},
Month = {AUG},
Abstract = {Social science interest in LGBTQI+ politics and policy has grown in the
   21st century. Likewise, the political opportunity structure for LGBTQI+
   activists has changed significantly, with historical expansions of legal
   protections only to be followed by the current period of backlash and
   retrenchment. In this article, we outline existing and potential
   research in LGBTQI+ politics and policy employing theoretical
   perspectives from political psychology. Promising advances in the study
   of LGBTQI+ politics and policy draw on framework and theories grounded
   in social and political psychology. Nevertheless, we also identify
   several significant gaps in the literature including the lack of focus
   on intersectional perspectives, subpopulations such as intersex people
   and those born with differences in sexual development, and the
   psychology of LGBTQI+ people and its relevance for their political
   attitudes and behavior.},
