@article{ WOS:001228167600001,
Author = {Vanderborght, Robin},
Title = {Normalising algorithmic warfare through visual practice: Russian
   demonstrations of uncrewed military vehicles during zapad 2021},
Journal = {CRITICAL STUDIES ON SECURITY},
Year = {2024},
Volume = {12},
