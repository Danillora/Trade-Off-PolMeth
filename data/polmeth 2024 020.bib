@article{ WOS:001319839700001,
Author = {Jenaway, Elizabeth M.},
Title = {Beset By Fear: How Viet Cong Members Successfully Overcame Disengagement
   Barriers},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {2024},
Month = {2024 SEP 26},
Abstract = {Over the past two decades, disengagement from political extremism has
   become a prominent field of inquiry, with a considerable effort focused
   on push and pull factors that compel exit. However, much less research
   examined the obstacles that hinder these individuals' disengagement.
   Using interviews with former members of the Viet Cong, the author
   investigated disengagement barriers that impeded participants from
   exiting and how they overcame these obstacles. Despite the desire to
   leave extremism, participants discussed the barriers that delayed
   disengagement, including (1) fear of mistreatment, (2) limits to
   communication and movement, and (3) social camaraderie. Additionally,
   the emergence of negative emotions like fear hindered disengagement.
   However, participants overcame these disengagement barriers through
   exposure to counter-narratives and utilizing serendipitous moments that
   facilitated their disengagement. These findings suggest disengagement is
   more complex than simply the desire to leave. Instead, disengagement is
   an intricate process that changes based on the political extremists'
   emotions, strains, and perceptions of the costs and benefits of exiting.},
