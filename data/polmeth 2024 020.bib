@article{ WOS:001284610600001,
Author = {Bolton, Derek},
Title = {Security, Society, and the Perennial Struggles over the Sacred: Revising
   the Wars of Religion in International Relations Theory},
Journal = {INTERNATIONAL ORGANIZATION},
