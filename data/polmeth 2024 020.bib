@article{ WOS:001294357200010,
Author = {Bakmaz, Ognjen and Dragosavac, Milos and Brakus, Aleksandra and
   Radakovic, Milan and Arnautovic, Ivan and Samardzic, Veljko and
   Krstajic, Goran and Popovic, Slobodan},
Title = {Financial Security and Invoicing in Management of Public Enterprises
   Whose Founders are Local Self- Government Units, Example Republic of
   Serbia},
