@article{ WOS:001210854400001,
Author = {Chen, Hao and Bao, Jiaying and Wang, Jiajia and Wang, Liang},
Title = {Abandoning good and following evil: a study on the dark side effect of
   self-sacrificial leadership-from the perspective of moral licensing},
