@article{ WOS:001450031300005,
Author = {Borriello, Giuseppe and Fristachi, Gaia},
Title = {Exploring Digital Isomorphism: Convergence in the Digitization and
   Cybersecurity Organization of Local Government in Italy},
Journal = {RIVISTA ITALIANA DI POLITICHE PUBBLICHE},
Year = {2024},
Number = {3},
Pages = {423-459},
Month = {DEC},
Abstract = {The digitization of the public sector is reshaping administrative
   structures and workforce dynamics, necessitating specialized personnel
   and revamped organizational frameworks. Drawing upon the theoretical
   lens of institutional isomorphism, this study investigates the impact of
   digital transformation and cybersecurity policies on organizational
   convergence within local municipalities in Italy. Analysing all 112
   provincial capitals and metropolitan cities nationwide, the study
   scrutinizes office organization, personnel allocation, and recruitment
   processes. Empirical findings reveal a general trend towards
   organizational convergence in digitization and cybersecurity management
   across local PAs, albeit with pronounced disparities between the
   northern and southern regions as well as between large urban centres and
   small municipalities.},
