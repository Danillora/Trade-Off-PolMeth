@article{ WOS:001142083600001,
Author = {Farias, Lara Lisboa},
Title = {Understanding Brazil's New Right and Its Impact on Social Policy},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2023},
Volume = {50},
Number = {5, SI},
Pages = {67-81},
Month = {SEP},
Abstract = {The new right is central to an understanding of Brazilian political and
   cultural reality with direct impacts on the development of social
   policy. A qualitative analysis based on a review of the literature with
   a focus on critical theory shows that the context of the rise of the new
   right in the country corroborates implementation of social policy
   focused on the moralization and criminalization of the subject typical
