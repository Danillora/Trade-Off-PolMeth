@article{ WOS:001350551500011,
Author = {Savic, Matej},
Title = {Bosnia and Herzegovina - From Peace Implementation to Crisis Creation,
   Legal and Political Problems, Processes and Perspectives},
Journal = {STUDIA EUROPEJSKIE-STUDIES IN EUROPEAN AFFAIRS},
