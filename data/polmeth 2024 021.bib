@article{ WOS:001322814100001,
Author = {Svensson, Isak and Nilsson, Desiree and Gasste, Tim},
Title = {No End in Sight? Trajectories of War Terminations in Islamist Armed
   Conflicts},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2024},
Month = {2024 AUG 29},
Abstract = {We know that Islamist armed conflicts tend to become intractable. Yet,
   we have very limited knowledge about the various ways in which such
   conflicts are brought to a halt. In this article, we address this
   research gap. By combining insights from the warscapes literature with
   research on conflict terminations, we provide a more nuanced picture of
   how Islamist conflicts terminate, and the trajectories that they follow.
   Our findings - based on a global analysis of terminations in Islamist
   armed conflicts, 1989-2019 - demonstrate that these conflicts rarely
   terminate entirely but rather change and take different forms, or are
   followed by violence involving other armed actors, either continued or
   renewed cycles of violence.},
