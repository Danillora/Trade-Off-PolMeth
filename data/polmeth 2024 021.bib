@article{ WOS:001290736100001,
Author = {Reyes, Raul Salas},
Title = {On the contested compliance of CBDRRC and the erosion of trust},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2025},
Volume = {34},
Number = {4},
Pages = {717-736},
Month = {JUN 7},
Abstract = {Trust is necessary to ensure international cooperation between
   nation-states who negotiate responses to address the causes and effects
   of climate change. There is reason to believe that an erosion of trust
   has been happening since the Paris Agreement negotiations. Yet, it is
   unclear if and how trust started to erode. Interviews with twenty-two
   climate negotiators indicate that a contested compliance with CBDRRC is
   resulting in an erosion of trust in line with issues of reliance,
   unfulfilled responsibility, lack of reciprocity, and absence of
   recognition. Such issues include a perceived lack of compliance with the
