@article{ WOS:001224606300001,
Author = {Sorensen, Eva and Torfing, Jacob},
Title = {The ideational robustness of bureaucracy},
Journal = {POLICY AND SOCIETY},
Year = {2024},
Volume = {43},
Number = {2},
Pages = {141-158},
Month = {MAY 16},
Abstract = {To better understand why bureaucracy is still going strong despite a
   century of scorn, this article asks: How has the bureaucratic governance
   paradigm managed to achieve its ideational robustness in the face of
   consecutive waves of criticism and societal challenges? This question is
   answered by studying the combination of a broad range of ideational
   robustness strategies that have enabled bureaucracy to weather the storm
   and stay relevant and praiseworthy in the eyes of public governors. The
   article describes the core components of the bureaucratic governance
   paradigm and reviews four consecutive waves of criticism before
   explaining the ideational changes that have enabled the relative
   stability of public bureaucracy based on the adaptation and innovation
   of its content and scope. In addition to summarizing key debates about
   public bureaucracy, the article develops six ideational robustness
   strategies that may be used to study other core ideas in public
   governance.},
