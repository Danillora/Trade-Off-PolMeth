@article{ WOS:001230640500001,
Author = {Drolc, Cody A. and Shoub, Kelsey},
Title = {The particular and diffuse effects of negative interactions on
   participation: Evidence from responses to police killings},
Journal = {POLICY STUDIES JOURNAL},
