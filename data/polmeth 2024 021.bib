@article{ WOS:001358820700005,
Author = {Orgeret, Kristin Skare},
Title = {The Israel-Palestine Conflict in Norwegian Media: News Biases and the
   Erosion of a Shared Understanding of Reality},
Journal = {INTERNASJONAL POLITIKK},
Year = {2024},
Volume = {82},
Number = {2},
Pages = {203-215},
Abstract = {As one of the most contentious and long-lasting conflicts on the global
   stage, the Israel-Palestine conflict has seldom been characterized by a
   shared understanding of reality (Heian-Engdal, 2018, 2024; Jensehaugen,
   2023; Waage, 2013). At least two predominant narratives are in direct
   opposition, both within the Middle East and across international media,
   shaping public perceptions of news events. However, the media coverage
   of the 2023-2024 conflict between Israel and Hamas highlights a
   significant shift in the portrayal of this issue within Norwegian media.
   The Norwegian media landscape is now far more divided than a couple of
   decades ago, a change facilitated by the digital revolution, which has
   fundamentally transformed communication channels. This raises critical
   questions: What do the divergent understandings of the Israel-Palestine
   conflict signify within different segments of the Norwegian media
   landscape? How are these perspectives connected to generational
   divisions? To what extent do they influence one another? How does
   Norwegian media coverage compare with that of neighboring countries, and
   what implications does this have for Norwegian policy on Israel and
   Palestine?},
