@article{ WOS:001359427400001,
Author = {Glavas, Dejan and Grolleau, Gilles},
Title = {Do happy individuals support government environmental spending? Evidence
   from the general social survey},
Journal = {POLICY STUDIES},
Year = {2024},
Month = {2024 NOV 20},
Abstract = {Using 49 years of General Social Survey data (1972-2021), we investigate
   the relationship between individual happiness and support for government
   environmental expenditure. Contrary to expectations, we find a negative
   correlation between higher levels of happiness and support for
   environmental spending. Employing various methods to address
   endogeneity, including bivariate probit and instrumental variable
   approaches, we uncover a non-linear relationship where individuals with
   greater life satisfaction are less likely to support government
   environmental expenditure. We also find that tax morale serves as a
   moderating factor in this relationship. We validate these findings using
   World Values Survey data from over a hundred countries. Our results
   challenge conventional assumptions about the link between personal
   well-being and environmental policy attitudes. This unexpected
   relationship suggests that policymakers should develop targeted
   strategies to engage happier individuals (especially those with low tax
   morale) in supporting environmental measures.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
