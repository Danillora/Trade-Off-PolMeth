@article{ WOS:001308452400001,
Author = {Rudolf, Inna},
Title = {All the Mahdi's Men: Contextualising Nuances Within Iraq's Islamic
   Resistance},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2024},
Month = {2024 AUG 28},
Abstract = {This article examines how the notion of ``a responsibility to resist{''}
   as practiced by Iraqi armed factions is being reinterpreted through the
   prism of those factions' shared perceptions and experiences as well as
   the changing dispositions of their leaders. I build on Mohammed
   Kalantari's application of SMT in his attempt to dissect how political
   opportunity structures (POS) are being perceived and acted upon by Shi'a
   religious scholars in Iraq. Moving beyond traditional structuralist
   notions of POS, I incorporate Pierre Bourdieu's concept of a
   group-specific collusio to bridge the gap between purely ideological and
   purely interest-based explanations. Framed as a ``practical mutual
   understanding{''} of field-specific rules, collusio provides a nuanced
   lens for contextualizing these actors' overlapping and contradictory
   perceptions and introduces a dimension of continuous learning.
   Furthermore, challenging the analytical value of proxy-sponsor theories
   as applied to the Iraqi context, this article offers an alternative
   perspective on the parallels between competing champions of the Islamic
   resistance and on the contradictions observed within the Iran-affiliated
   camp of Iraq's Popular Mobilisation Units (PMU). Based on extensive
   field research, open-source media content, and semi-structured
   interviews with commanders from the Islamic Resistance, this article
   also demonstrates the limitations to subsuming such a palette of
   domestically entrenched resistance politics under the label proxy
   warfare. Lastly, by integrating Bourdieu's framework with insights from
   the warscapes literature, I shed light on the fluidity of group actors
   navigating their domestic practices against the background of their
   changing interpretation of historical path dependences and transnational
   pressures. The integrated approach thus provides insights for
   specialists on Iraq, Iran, and beyond, interested in understanding the
   intricate interplay of ideologies and interests within geographically
   and temporally interlinked warscapes.},
