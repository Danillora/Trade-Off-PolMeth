Author = {Canto, Joel and Baraibar, Javier and Arregui, Javier},
Title = {Same money, different impact? The curving effect of European Structural
   and Investment Funds on EU support in Spain (1990-2019)},
Journal = {EUROPEAN POLICY ANALYSIS},
