@article{ WOS:001337588900001,
Author = {Young, Alasdair R.},
Title = {Governing the digital economy: transatlantic accommodation and
   cooperation},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2024},
Volume = {46},
Number = {7, SI},
Pages = {973-992},
Month = {OCT 2},
Abstract = {The digitalization of the economy has brought significant benefits but
   also poses risks. These risks prompted first the European Union and
   subsequently China and the United States to adopt regulations. The three
   powers are seen to have very different regulatory approaches - the US
   deferring to the market, China prioritizing the government, and the EU
   protecting the individual - and to be exporting their models. As a
   result, many commentators contend that the digital economy is
   conflictual. Drawing on evidence from the contributions to the special
   issue, this article challenges two conventional wisdoms about the
   transatlantic digital relationship. First, it argues that transatlantic
   regulatory differences have been rooted in politics rather than
   principled differences. Second, rather than rule differences and
   jurisdictional conflict being virtually synonymous, transatlantic
   differences in approaches to governing the digital economy almost never
   led to conflict and, even then, cooperation was preferred.},
