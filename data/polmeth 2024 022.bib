@article{ WOS:001250761600007,
Author = {Jones, Kristen},
Title = {Colombia's role in great power competition},
Journal = {GLOBAL POLICY},
Year = {2024},
Volume = {15},
Number = {3, SI},
Pages = {120-129},
Month = {JUN},
Abstract = {Great power competition between the United States and China marked the
   last decade and will continue to define geopolitics in the near future.
   Colombia is a key Latin American country with respect to this great
   power competition, and this article will explain why Colombia matters in
   the current great power competition. It will review the economic,
   military, and diplomatic resources of Colombia from each country, and
   the reception and effect that these decisions have on Colombia. In
   conclusion, the article proposes recommendations to support a liberal
   international order through engagement in Colombia.},
