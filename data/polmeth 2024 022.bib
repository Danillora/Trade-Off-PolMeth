@article{ WOS:001278253100001,
Author = {Albert, Mathias and Crawford, Ian and Erman, Eva and Kessler, Oliver and
   Bartelson, Jens and Sienknecht, Mitja and Patomaki, Heikki},
Title = {Forum on Heikki Patomäki's World Statehood: The Future of World
   Politics},
