@article{ WOS:001380495900001,
Author = {Shava, Elvin and Muringa, Tigere Paidamoyo},
Title = {Curbing bureaucratic limitations through continuous learning in local
   government in South Africa},
Journal = {FRONTIERS IN POLITICAL SCIENCE},
