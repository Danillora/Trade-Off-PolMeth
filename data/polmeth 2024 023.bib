@article{ WOS:001345671800002,
Author = {Tatcho, Orville B.},
Title = {Political Socialization of Igorot Millennials and Zoomers and Their
   Views about the Marcoses},
Journal = {PHILIPPINE POLITICAL SCIENCE JOURNAL},
Year = {2024},
Volume = {45},
Number = {2},
Pages = {146-171},
Month = {SEP},
Abstract = {This qualitative study seeks to understand the political views of select
   Igorot millennials and Generation Z participants (zoomers) about the
   Marcoses, the dominant political family in Philippine politics today.
   Through eighteen (i8) purposively sampled interviews with Igorot
   millennials and zoomers, and political socialization as a framework,
   this article (i) explains the factors that shape and influence the
   political socialization of the participants, (2) discusses the role of
   social media as an agent of political socialization, and (3) examines
   the political views of the participants about the Marcoses, particularly
   the reasons they cited for the perceived popularity of the political
   family. This article concludes with recommendations to enhance the
   political learning of Igorot millennials and zoomers, as well as some
   conceptual and practical implications for political socialization as a
   process.},
