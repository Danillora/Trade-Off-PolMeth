@article{ WOS:001205199400001,
Author = {Noutcheva, Gergana and Zarembo, Kateryna},
Title = {Normative power at its unlikeliest: EU democratic norms and security
   service reform in Ukraine},
Journal = {COOPERATION AND CONFLICT},
Year = {2025},
Volume = {60},
Number = {3, SI},
Pages = {504-523},
Month = {SEP},
Abstract = {This article traces the effects of European Union (EU) normative power
   on security sector reform in Ukraine. We argue that to get a better
   grasp of how normative power works in practice, we need to scrutinize
   more closely the domestic journey of EU norms. This local lens allows us
   to uncover the inherent contestation involved in the transnational
   travel of norms, emphasizing the importance of local agency and local
   conceptions of normativity. We reveal the internal struggle between
   liberal democratic norms and deeply ingrained attitudes, institutions
   and behaviours linked to the Soviet legacy. We show how EU democratic
   norms gradually empower domestic constituencies and overcome domestic
   structural resistance to change, leading to democratic advances in a
   sector least likely to reform.},
