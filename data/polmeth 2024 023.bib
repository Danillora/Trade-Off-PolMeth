@article{ WOS:001359437800001,
Author = {Toltica, Sorina},
Title = {The role of tri-liminality in the UK's security force assistance to
   Nigerian counterinsurgency: A Principal-Agent perspective},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2025},
Volume = {48},
Number = {5, SI},
Pages = {962-989},
Month = {JUL 29},
Abstract = {This article explores how the concept of tri-liminality, which captures
   the changing character of the post-colonial relationship and of states
   individually as counterinsurgents, can inform Principal-Agent theory in
   the context of UK Security Force Assistance provided to Nigeria's Boko
   Haram campaign. It influences demand, supply and outcomes, and creates a
   situation of in-betweenness: not a superior-subordinate colonial
   relationship anymore, nor fully an exchange benefiting a principal and
   independent agent that share a cultural bond. Material capacities and
   colonial inherited cultures, power relations, and perceptions continue
   to act as determinants and negatively impact engagement in a
   historically contextualised recent provision.},
