@article{ WOS:001350407900001,
Author = {Copeland, Nicholas},
Title = {Development and Indigenous Ecopolitics in Post-Peace Guatemala},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2024},
Volume = {51},
