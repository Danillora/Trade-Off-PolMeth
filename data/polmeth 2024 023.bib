@article{ WOS:001232104600001,
Author = {Smith, Karen E.},
Title = {Avoiding an emotions-action gap? The EU and genocide designations},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2024},
Volume = {46},
Number = {5, SI},
Pages = {615-634},
Month = {JUL 3},
Abstract = {Genocide is one of the most heinous crimes perpetrated against humans.
   Violation of the norm against genocide should thus spark a robust
   response. Yet naming genocide is a highly contentious act, and the
   European Union and most of its member states have been cautious about
   using the term to describe atrocities. This article explores how the
   emotional resonance of the term `genocide' inhibits its use by the EU,
   which in turn has implications for the EU's response to purported
   genocidal violence. By not using the term, the EU avoids creating an
   emotions-action gap, as its actions are better aligned with the emotions
   it expresses through its rhetoric. But in so doing, the EU may also
   underestimate the potential for genocide and fail to take action to
   prevent it. In this way, emotions play a constraining role in the EU's
   response to purported violations of the norm against genocide.},
