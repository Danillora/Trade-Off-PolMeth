@article{ WOS:001345244600001,
Author = {Gasparello, Giovanna},
Title = {Defending the Commons from Dispossession in the Mountains of Guerrero:
   Contributions from and for Anthropology},
Journal = {LATIN AMERICAN PERSPECTIVES},
