@article{ WOS:001230136700002,
Author = {Oud, Malin},
Title = {Powers of persuasion? China's struggle for human rights discourse power
   at the UN},
Journal = {GLOBAL POLICY},
Year = {2024},
Volume = {15},
Number = {2, SI},
Pages = {85-96},
Month = {MAY},
Abstract = {Since Xi Jinping came to power in 2012, the People's Republic of China
   has been on a mission to break what it describes as `Western hegemony'
   around global norms. Beijing is engaged in a struggle to strengthen its
   influence in global governance and has identified increasing China's
   `discourse power' at the United Nations (UN) as key to achieving this
   goal. Focused on human rights, this paper examines China's project to
   enhance its discourse power at the UN. It assesses how successful
   Beijing has been in shaping language, concepts and norms at the UN and
   to what extent there has been a power shift in line with China's
   aspirations. It concludes that in China's efforts to enhance its power
   over meaning at the UN, it relies less on its powers of persuasion to
   effect what Barnett and Duvall term productive power and more on
   compulsory power. China's vision for global governance and its growing
   clout at the UN has great implications for international order. In
   essence, China seeks to reshape international norms so that human rights
   become each member state's `internal affairs' rather than a legitimate
   concern of the international community.},
