@article{ WOS:001143581700001,
Author = {Wettestad, Jorgen},
Title = {The EU's carbon border adjustment mechanism: Shaped and saved by
   shifting multi-level reinforcement?},
Journal = {REVIEW OF POLICY RESEARCH},
