@article{ WOS:001335524700005,
Author = {Boutron, Camille},
Title = {The ministry of the Armed forces and the implementation of the Women,
   Peace and Security Agenda: minimal institutional use in the face of
   marginal individual commitments},
Journal = {ETUDES INTERNATIONALES},
Year = {2024},
Volume = {55},
Number = {1},
Pages = {111-137},
Month = {SPR},
Abstract = {This article examines the implementation of the Women, Peace, and
   Security agenda in France by the Ministry of the Armed Forces by looking
   at the different ways in which this instrument is used. Based on an
   in-depth investigation within the Ministry of the Armed Forces, it shows
   first of all that the pursuit of the objectives set by the agenda takes
   the form of a transcoding operation of actions previously undertaken,
   which makes it possible to circumvent the restrictive aspects of the
   agenda by putting forward the argument of ``operational
   effectiveness{''}. It then examines the forms of institutional and
   individual appropriation of the agenda, and of the concept of gender in
   particular. It highlights the contrast between minimal institutional
   investment and the individual commitment of officers who are convinced
   of the importance of the WPS Agenda as a driving force for the
   modernisation of defence systems.},
