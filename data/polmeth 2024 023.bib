@article{ WOS:001281796600001,
Author = {Borkowska, Magda and Laurence, James},
Title = {The Role of Ethnic Diversity and Residential Segregation in Shaping
   Anti-Immigrant Sentiment and Support for Brexit},
Journal = {POLITICAL STUDIES},
Year = {2025},
Volume = {73},
Number = {2},
Pages = {457-483},
Month = {MAY},
Abstract = {This article explores the relationship between ethnic structure of local
   areas, anti-immigrant sentiment and Brexit vote among White British in
   England. We focus on two indicators of ethnic structure: ethnic minority
   outgroup share and minority-majority segregation. Our findings suggest
   that local minority share plays a key role in shaping anti-immigrant
   sentiment and Brexit support. However, how it affects these outcomes is
   conditional on levels of local residential segregation. It is only
   residents living in high minority share areas that are residentially
   segregated who report higher anti-immigrant sentiment and Brexit
   support. In fact, living in high minority share areas that are
   residentially integrated appears to improve attitudes and reduce Brexit
   support.},
