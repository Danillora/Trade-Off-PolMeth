@article{ WOS:001368307300001,
Author = {Zhang, Diandian and Mu, Zijian and Tan, Zhiqian},
Title = {Effects of public data open on performance of government functions in
   China: the roles of digital economy and market economic system adopting
   the perspective of resource- based view},
