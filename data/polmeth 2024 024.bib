@article{ WOS:001368435400016,
Author = {Ieong, Fat fai},
Title = {Research on Macao Study Tour Policy and Cross-strait Cultural
   Construction on the Macao Local Government Cultural Industry-Taking the
   ``Ten Thousand Persons Plan{''}},
