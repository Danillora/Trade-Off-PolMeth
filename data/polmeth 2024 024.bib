@article{ WOS:001405835500003,
Author = {Kolodziejczyk, Katarzyna},
Title = {The 2024 Reform of the European Union's Economic Governance Framework as
   a Response to Contemporary Challenges},
Journal = {STUDIA EUROPEJSKIE-STUDIES IN EUROPEAN AFFAIRS},
Year = {2024},
Volume = {28},
Number = {4},
Pages = {59-76},
Abstract = {The 2024 reform of the European Union's economic governance framework
   represents a crucial response to the complex challenges confronting the
   organisation and its Member States in the 21st century. In addressing
   these challenges, the European Union must balance the need for economic
   growth, reforms, and investments with the imperative of maintaining debt
   sustainability. The reform emphasises the importance of fostering sound
   and sustainable public finances while simultaneously promoting
   structural reforms and investments aimed at achieving long-term economic
   growth. This article investigates the factors that necessitated the
   reform, outlines its primary objectives, and discusses the practical
   assumptions underpinning its implementation. Furthermore, the article
   evaluates the potential impact of the reform in light of contemporary
   issues, such as the increasing significance of climate policy and the
   development of the European Union's defense capabilities.},
