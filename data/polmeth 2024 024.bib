@article{ WOS:001321889600001,
Author = {Ayodele, Bonnie},
Title = {Russia's invasion of Ukraine: Nigeria's value-driven and pragmatic
   indifferent stance (Publication with Expression of Concern. See SEP,
   2025)},
Journal = {GLOBAL POLICY},
