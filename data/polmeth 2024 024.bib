@article{ WOS:001368435400005,
Author = {Irawati, I. and Suwaryo, Utang and Sulaeman, Affan and Yuningsih, NEneng
   Yani},
Title = {Democracy in Local Communities: Direct Election System versus
   Deliberation System in the Minangkabau Ethnicity in West Sumatra,
   Indonesia},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2024},
Volume = {22},
Number = {4},
Pages = {104-123},
Month = {OCT},
Abstract = {This paper focuses on the dissonance between the values of the democracy
   of local communities and the liberal democracy of election system is
   adopted to elect the leader of the Minangkabau community. However, a
   qualitative study shows that, culturally, the Minangkabau community
   chooses its leader through a clan-based deliberation system, relying on
   communal rather than individual representation. Adopting direct election
   with the one-person-onevote principle to elect the community leader has
   instead been a source of conflict. The ambiguous understanding of the
   community members regarding the principles of individual and collective
   representation has given rise to the conflict. The institutionalisation
   of a clan-based deliberation system in the community is a solution to
   solve the conflict. This scholarly inquiry underscores that the
   Minangkabau ethnic group's democratic ethos fosters an environment
   conducive to communal deliberations, thereby sustaining their social
   cohesion and integration.},
