@article{ WOS:001259685800001,
Author = {Ege, Jorn and Gofen, Anat and Hadorn, Susanne and Hakman, Inbal and
   Malandrino, Anna and Ramseier, Leroy and Sager, Fritz},
Title = {Understanding street-level managers' compliance: a comparative study of
   policy implementation in Switzerland, Italy, Germany, and Israel},
