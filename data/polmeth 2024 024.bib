@article{ WOS:001390196100002,
Author = {Neumann, Iver B.},
Title = {The New Strategic Situation and its Implications for Norway},
Journal = {INTERNASJONAL POLITIKK},
Year = {2024},
Volume = {82},
Number = {3-4},
Pages = {288-304},
Abstract = {From the Second World War and into the 2000s, the world order could
   justly be called liberal. The last two decades have seen a number of
   demographic, economic and political developments that have challenged
   the liberal world order. The American electorate's choice of Donald
   Trump as the 47th president of the United States is a recent but far
   from isolated example of this. The new constellation has immediate
   repercussions for Norway's security environment and therefore for
   Norway's security policy. Scholarly responses should include detailed
   discussions of these changes, as well as synthetic tours d'horizon that
   may stimulate debate. Which changes are most important from Norway's
   perspective, and how should Norway respond? The article emphasizes the
   importance of great-power confrontation between the United States and
   China as key to the new constellation. This confrontation increasingly
   overlays regional security complexes. The article notes a certain
   discrepancy between Northern Norway, which is part of the Artic region,
   and the rest of the country, which is firmly ensconced in the European
   regional security complex. It also surveys the future of NATO, Norway's
   bilateral relations to the United States and Russia and its multilateral
   elation to the European Union. In conclusion, it argues in favour of
   Norwegian EU membership and continued policies aimed towards maintaining
   the international system, points to the continued need to balance
   deterrence of Russia with reassurance, and warns against deepening
   bilateral relations with China.},
Publisher = {NORSK UTENRIKSPOLITISK INST},
