   Nam, Illan, Colgate Univ, Dept Polit Sci, Hamilton, NY 13346 USA.},
DOI = {10.1353/wp.2024.a916341},
ISSN = {0043-8871},
EISSN = {1086-3338},
Keywords = {state capacity; state building; bureaucracy; central-local state
   relations; health; edu-cation; rural; economic development; rural-urban
   inequality},
Keywords-Plus = {CIVIL-SERVICE; POWER; POLITICS; SYSTEMS; ORIGINS; REFORM; ASIA},
