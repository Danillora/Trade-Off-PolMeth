@article{ WOS:001337944200006,
Author = {Hauptman, Lidija and Pavic, Ivana and Casni, Anita ceh},
Title = {The Influence of Trust in Tax Administration and Gender on Tax
   Compliance in Slovenia},
Journal = {LEX LOCALIS-JOURNAL OF LOCAL SELF-GOVERNMENT},
Year = {2024},
Volume = {22},
Number = {3},
Pages = {140-163},
Month = {JUL},
Abstract = {The OECD urges governments to improve tax systems by studying taxpayers'
   behavior to prevent tax non-compliance. This study focuses on individual
   Slovene taxpayers aiming to investigate the impact of trust in Tax
   Administration and gender on tax compliance. In the empirical part of
   the study, trust and tax compliance were the subject of descriptive
   statistical analysis, exploratory factor analysis and Spearman
   correlation coefficients. A Mann-Whitney U test and independent samples
   median test were performed to evaluate compliance gender differences in
   tax compliance. The findings unveil that trust in tax administration
   exhibits a statistically significant and positive correlation with tax
   compliance intentions in Slovenia while the differences in tax
   compliance between genders were not confirmed. Taxpayers' fairness
   perception to gain higher trust should be increased through sufficient
   information about tax laws and regulations.},
