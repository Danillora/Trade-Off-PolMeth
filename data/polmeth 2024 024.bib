@article{ WOS:001328118600001,
Author = {Campos, Alexa Obando and Latorre, Sara},
Title = {Reconversion or Exclusion? The Effects of Blue Economy Policies on
   Semi-industrial and Artisanal Fishing in Puntarenas, Costa Rica},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2024},
Volume = {51},
Number = {3, SI},
Pages = {69-88},
Month = {MAY},
Abstract = {Although trawling stopped definitively in 2019 in Costa Rica, there is
   an ongoing debate regarding the broader policies derived from the Blue
   Economy. These have focused on the productive conversion of the fleet
   (salaried fishers) toward more profitable activities related to tuna
   fishing, aquaculture, and tourism. This paper takes a political economy
   approach to oceans and livelihoods, analyzing how the Blue Economy
   agenda has been implemented in Costa Rica, and what effects this is
   having on semi-industrial and artisanal fisherpeople in Puntarenas
   communities. We seek to contribute to critical studies of marine-coastal
   development and its social consequences in relation to environmental
   (in)justices in Central America, an underexplored topic in the region.A
   pesar de que la pesquer \& iacute;a de arrastre tuvo su cierre
   definitivo en el 2019 en Costa Rica, el debate sigue presente y
   relacionado con las pol \& iacute;ticas m \& aacute;s amplias derivadas
   de la econom \& iacute;a azul. \& Eacute;stas se han venido centrando en
   la reconversi \& oacute;n productiva de la flota -los pescadores
   asalariados - hacia actividades m \& aacute;s rentables, relacionadas
   con la pesca de at \& uacute;n, la acuicultura y el turismo. Este art \&
   iacute;culo a trav \& eacute;s de los enfoques de econom \& iacute;a pol
   \& iacute;tica de los oc \& eacute;anos y medios de vida, analiza c \&
   oacute;mo la agenda de econom \& iacute;a azul se materializa en Costa
   Rica a trav \& eacute;s de estas pol \& iacute;ticas, y qu \& eacute;
   efectos est \& aacute; teniendo en los pescadores semi-industriales y
   artesanales de comunidades de Puntarenas. Con ello, se busca contribuir
   a los estudios cr \& iacute;ticos de desarrollo marino-costero y a las
   consecuencias sociales en t \& eacute;rminos de (in)justicias
   ambientales en Centroam \& eacute;rica, un \& aacute;mbito poco
   explorado en la regi \& oacute;n.},
