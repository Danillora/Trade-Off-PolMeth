@article{ WOS:001273910300001,
Author = {Turnbull-Dugarte, Stuart J. and Cordero, Guillermo and Duenas, Jose
   Maria Ramirez},
Title = {Generations of pride? LGBTQ plus rights, sexuality, and voting behaviour
   in Spain},
Journal = {SOUTH EUROPEAN SOCIETY AND POLITICS},
Year = {2024},
Volume = {29},
Number = {1},
Pages = {29-54},
Month = {JAN 2},
Abstract = {This study investigates the enduring presence of a sexuality gap in
   political behaviour within the unique context of Spain - an early-mover
   country in the expansion of LGBTQ+ rights. First, analysing supply-side
   dynamics, we demonstrate that political parties in Spain remain markedly
   polarised on LGBTQ+ rights. Second, utilising original survey data, we
   observe a more pronounced sexuality gap among younger generations: the
   youngest LGB voters demonstrate notably greater support for left-wing
   parties, particularly emergent progressive parties to the left of the
   social democrats than their older counterparts, while rejecting
   right-wing alternatives. These results underscore the enduring
   significance of sexual identity in shaping political behaviours, even in
   a nation celebrated for its strides in the advancement LGBTQ+ rights.},
