@article{ WOS:001162030900001,
Author = {Wellhausen, Rachel L. and Feir, Donn L. and Thrall, Calvin},
Title = {Stakeholder Cues, National Origin, and Public Opinion Towards Firms:
   Evidence in the Context of the First Bank in an American Indian Nation},
Journal = {BUSINESS AND POLITICS},
