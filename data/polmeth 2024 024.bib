@article{ WOS:001169535400001,
Author = {Magnusdottir, Gunnhildur Lily and Kronsell, Annica},
Title = {Climate institutions matter: The challenges of making gender-sensitive
   and inclusive climate policies},
Journal = {COOPERATION AND CONFLICT},
