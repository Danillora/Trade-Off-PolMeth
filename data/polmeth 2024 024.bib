@article{ WOS:001390196100006,
Author = {Allers, Robin},
Title = {Germany's Cautious Policy Change - Arms Support to Ukraine in the Wake
   of the Zeitenwende},
Journal = {INTERNASJONAL POLITIKK},
Year = {2024},
Volume = {82},
Number = {3-4},
Pages = {337-350},
Abstract = {Following Russia's full-scale invasion in February 2022, Germany went
   from refusing on principle to send weapons to conflict areas to becoming
   one of the largest suppliers of war material to Ukraine. Arms support
   became a key element of Germany's policy change, widely known under the
   label Zeitenwende. While the government and especially Chancellor Olaf
   Scholz were praised for their change of course, they were also accused
   of delivering too little, of being too late, and of communicating their
   decisions too poorly. This study takes a closer look at two questions
   that have characterized political and academic discussions of Germany's
   change of course: What influences Germany's policy on arms support and
   to what extent is Germany's changed attitude an expression of a
   fundamental reorientation? Four factors are highlighted: the development
   of the war, Germany's complicated relationship with the use of military
   power, the country's political system, and the chancellor's personality
   and communication skills. These factors are emphasized in an analysis of
   three key policy decisions: 1) the departure from the principled no to
   sending weapons to a conflict area, 2) the tug-of-war over the delivery
   of German-made Leopard 2 main battle tanks, and 3) the controversy over
   the Taurus cruise missile.},
