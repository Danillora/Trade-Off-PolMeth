@article{ WOS:001197603300001,
Author = {Clubb, Gordon and Altier, Mary-Beth and Kobayashi, Yoshiharu and Davies,
   Graeme and Brownsord, Eliza},
Title = {Combating the Terrorist Stigma: Communicating Rehabilitation and
   Reducing Barriers to Reintegration},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
