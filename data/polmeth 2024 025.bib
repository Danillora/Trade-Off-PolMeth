@article{ WOS:001181724800001,
Author = {Arceneaux, Kevin and Bakker, Bert N. and Fasching, Neil and Lelkes,
   Yphtach},
Title = {A critical evaluation and research agenda for the study of psychological
   dispositions and political attitudes},
