@article{ WOS:001137249000001,
Author = {Yuliantri, Rhoma Dwi Aria and Suwignyo, Agus},
Title = {Revisiting the ideological negotiation of Indonesian identity, 1900-1942},
Journal = {POLITICS RELIGION \& IDEOLOGY},
