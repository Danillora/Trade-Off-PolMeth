@article{ WOS:001319986000001,
Author = {Braun, Mats},
Title = {The European Union and the Eurasian Economic Union - three rationalities
   of interaction and the problem of non-democratic regionalism},
Journal = {JOURNAL OF EUROPEAN INTEGRATION},
Year = {2025},
Volume = {47},
Number = {4},
Pages = {581-600},
Month = {MAY 19},
Abstract = {The article outlines three different rationalities of the interactions
   between the European Union and the Eurasian Economic Union before the
   Russian 2022 full-scale invasion of Ukraine. The first rationality
   refers to a neo-Westphalian model of interaction. According to this, the
   EU in its interaction with the EAEU has followed a focus on borders and
   spheres of interest. The second rationality is based on `new
   regionalism', and the interactions between the EU and the EAEU are based
   on economic interdependence. Whereas the first rationality type ignores
   the actorness of the smaller counties in the region, the second
   emphasises it. The final rationality addresses the ideational, normative
   dimensions. The conclusion stresses that the EU in its interaction with
   a non-democratic regional organization ends up in the neo-Westphalian
   interaction logic. The regionalism rationale fails due to the mutual
   exclusiveness of the projects and the EU's normative logic, when
   challenged, turns into hard power.},
