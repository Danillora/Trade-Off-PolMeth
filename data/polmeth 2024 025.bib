@article{ WOS:001142128900001,
Author = {Castilhos, Guilherme Vargas},
Title = {Chinese Foreign Direct Investment and the Myth of Autocracy Spread in
   Latin America},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2023},
Volume = {50},
Number = {5, SI},
Pages = {255-272},
Month = {SEP},
Abstract = {Since the 2010s, Chinese investment in Latin America has increased. It
   is a symbol of the emerging influence of China and represents the
   potential export of its nondemocratic socialist development model.
   Experts have been concerned that China may undermine democracy in
   foreign countries in general and erode the United States' influence in
   Latin America. A discourse analysis of official Chinese documents on its
   foreign policy objectives examines whether Chinese investment has
   undermined the national sovereignty of host nations in Latin America and
   concludes by refuting the myth that China's economic and rise in Latin
   America leads to the adoption of more autocratic governance models.Desde
   la decada de 2010 ha aumentado la inversion china en America Latina.
   Esto constituye un simbolo de la influencia emergente de China y
   representa la exportacion potencial de su modelo de desarrollo
   socialista no democratico. A los expertos les preocupa que China pueda
   socavar la democracia en la generalidad de los paises extranjeros y
   erosionar la influencia de Estados Unidos en America Latina. Un analisis
   del discurso de los documentos oficiales chinos sobre los objetivos del
   pais en politica exterior examina si la inversion china ha socavado la
   soberania nacional de las naciones anfitrionas latinoamericanas y
   concluye refutando el mito de que el ascenso economico de China en
   America Latina haya conducido a la adopcion de modelos de gobernanza mas
   autocraticos.},
