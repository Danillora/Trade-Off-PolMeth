@article{ WOS:001328121200001,
Author = {Navarrete, Edgars Martinez and Stahler-Sholk, Richard},
Title = {Indigenous Autonomies in Latin America in the Face of Contemporary
   Capitalism: Overview, Perspectives and Dilemmas, Part 1},
Journal = {LATIN AMERICAN PERSPECTIVES},
Year = {2024},
Volume = {51},
Number = {4, SI},
Pages = {3-24},
Month = {JUL},
Abstract = {The contemporary phase of capitalism has led to an intensification of
   the process of ``accumulation by dispossession,{''} which entails a
   growing conflict between territorial displacement and indigenous
   resistances. These conflicts manifest in projects for autonomy that take
   on diverse forms. Here, we address the origins of these various concepts
   of autonomy while focusing on their corresponding dilemmas and strategic
   choices across different historical contexts. Finally, we look at the
   examples of Zapatista autonomy in Mexico, the experiences of the Pur \&
   eacute;pecha community of Cher \& aacute;n, and the Mapuche struggle in
   Wallmapu/Chile, three distinct models where organizational processes
   themselves forge new collective subjectivities meant to confront
   neoliberal capitalist dispossession.La fase contempor \& aacute;nea del
   capitalismo ha generado una intensificaci \& oacute;n del proceso de
   ``acumulaci \& oacute;n por desposesi \& oacute;n,{''} lo cual implica
   crecientes conflictos entre el despojo territorial y las resistencias
   ind \& iacute;genas. \& Eacute;stas se manifiestan en formas diversas de
   proyectos de autonom \& iacute;a. Analizamos el origen de las variantes
   del concepto de autonom \& iacute;a, enfocando en sus correspondientes
   dilemas y sus opciones estrat \& eacute;gicas en diversos contextos hist
   \& oacute;ricos. Finalmente, consideramos las experiencias de la autonom
   \& iacute;a zapatista, la comunidad pur \& eacute;pecha de Cher \&
   aacute;n, y la lucha mapuche en Wallmapu/Chile, tres modelos distintos
   en donde el propio proceso organizativo forja nuevas subjetividades
   colectivas para enfrentar el despojo del capitalismo neoliberal.},
