@article{ WOS:001171871900001,
Author = {Rovner, Joshua},
Title = {History is written by the losers: Strategy and grand strategy in the
   aftermath of war},
Journal = {JOURNAL OF STRATEGIC STUDIES},
Year = {2025},
Volume = {48},
Number = {1},
Pages = {5-35},
Month = {JAN 2},
Abstract = {Strategy is a theory of victory; grand strategy is a theory of security.
   In the ideal, a state's strategy and grand strategy are mutually
   reinforcing. Wartime decisions should leave the state more secure in the
   aftermath, and peacetime decisions should put the state in a good
   position in the event of future conflict. Yet for various reasons,
   strategy and grand strategy often drift apart. This article focuses on
   one type of decoupling. Victorious states tend to expand their grand
   strategic ambitions, yet nostalgia encourages them to retain
   increasingly outmoded strategic concepts. Losers, by contrast, are more
   likely to have frank conversations about the real opportunities and
   limits of state power, and the ways in which military action can provide
   meaningful support. I illustrate the argument with case studies from the
   British experience in the American War of Independence, and the American
   experience in the first Persian Gulf War.},
