@article{ WOS:001290007800001,
Author = {Phillips, Justin Bonest and Campion, Kristy},
Title = {Is He /ourguy/, a False Flag, or Something Else? Debating Breivik's and
   Tarrant's Terrorism on 4chan's /pol/ Board},
Journal = {STUDIES IN CONFLICT \& TERRORISM},
Year = {2024},
Month = {2024 AUG 1},
Abstract = {Academic literature widely assumes that extremist-housing online forums,
   like 4chan, uniformly celebrate acts of right-wing terrorism (e.g. via
   ``sainthood memes{''}), such as those committed by Anders Breivik and
   Brenton Tarrant. Aside from a handful of exceptions, this body of
   research typically fails to acknowledge that ideologically likeminded
   individuals on 4chan also regularly debate the merits of both terrorists
   and their violent attacks. Our topic modeling of 100 thousand posts on
   4chan's infamous /pol/ board shows-heated-debates over Breivik and
   Tarrant ranging from celebratory, to condemning, and moral to
   conspiratorial. We argue that overlooking these debates limits valuable
   theoretical and practical insights.},
