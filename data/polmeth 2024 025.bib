@article{ WOS:001175312500001,
Author = {Carranza, Rafael and Contreras, Dante and Otero, Gabriel},
Title = {What makes elites more or less egalitarian? Variations in attitudes
   towards inequality within the economic, political and cultural elites in
   Chile},
Journal = {SOCIO-ECONOMIC REVIEW},
Year = {2024},
Volume = {22},
Number = {3},
Pages = {1141-1167},
Month = {FEB 28},
Abstract = {This article investigates how the type of elite to which a person
   belongs and their intergenerational contextual experiences are
   associated with attitudes towards inequality among elite individuals. We
   propose that membership of the economic elite and access to private
   schools, higher education business schools and affluent residential
   areas may contribute to the development of views that favour inequality.
   Using unique survey data collected in 2018 from a sample of 416
   individuals belonging to Chile's economic, political and cultural
   elites, we construct an additive score to measure attitudes towards
   inequality. Results of our regression analyses indicate that individuals
   belonging to the economic and political elite are more tolerant of
   inequality than members of the cultural elite. Moreover,
   intergenerational experiences at both private schools and higher
   education business schools significantly contribute to the formation of
   attitudes that favour inequality. These contextual experiences also
   relate to significant attitudinal variations within all elite groups.},
