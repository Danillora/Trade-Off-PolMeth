@article{ WOS:001425042300001,
Author = {Bassoli, Matteo and Dallara, Cristina and Nesti, Giorgia},
Title = {The Street-Level Bureaucracy (SLB) Revival: The State-of-the-Art and a
   New Research Agenda},
Journal = {RIVISTA ITALIANA DI POLITICHE PUBBLICHE},
Year = {2024},
Number = {2},
Pages = {175-188},
Month = {AUG},
Abstract = {This Special Issue (SI) examines the revival of Street-Level Bureaucracy
   (SLB) in the context of contemporary public policy implementation. The
   introduction includes a literature review highlighting SLB's enduring
   relevance and its applications in various sectors. It also presents the
   four emperical articles of the SI covering regional migration policies
   in Argentina and Brazil, asylum judges in Italy and France, gambling
   harm reduction in Piedmont, and the implementation of the Minimum Income
   Scheme in Veneto. These studies underscore the importance of four
   crucial dimensions: the Critical Role of SLBs in Policy Outcomes, the
   Need for Support and Resources, the Influence of External and Internal
   Pressures, and the Importance of Understanding SLB Experiences for
   Policy Design.},
