@incollection{ WOS:001505486100017,
Author = {Frederick, Katharine},
Editor = {Hoerder, D and Neissl, L},
Title = {Colonial Boom Towns Migration and Insecure Urban Tenure in
   Industrializing Southern Rhodesia},
Booktitle = {MIGRANT ACTORS WORLDWIDE: Capitalist Interests, State Regulations, and
   Left-Wing Strategies},
Series = {Studies in Global Social History},
Year = {2024},
Volume = {53},
Pages = {298-320},
Abstract = {This chapter employs a ``systems{''} approach to trace the development
   of rural-urban labour migration in British colonial Southern Rhodesia
   during the mid-twentieth century and uncover why black urban labourers
   struggled to establish permanent roots in burgeoning industrial cities.
   The economic and institutional characteristics of the colony influenced
   the socio-economic opportunities of black Africans in both rural and
   urban areas. As the colony underwent industrialization from the
   mid-1940s onward, rural men increasingly migrated to industrial urban
   centres in search of manufacturing wages that far exceeded the
   perpetually depressed agricultural wages. However, relatively few were
   able to establish permanent urban tenure together with a family over the
   ensuing decades, which resulted in cities with extreme gender imbalances
   among the black community. This was largely a consequence of the broader
   institutional makeup of colonial Southern Rhodesia, which primarily
   served the socioeconomic interests of a small white settler community,
   often at the expense of the colony's black inhabitants.},
