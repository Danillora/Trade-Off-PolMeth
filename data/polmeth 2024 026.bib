@article{ WOS:001449126900017,
Author = {Koter, Dominika},
Title = {Elite Collusion and Dismantling Democracy in Benin},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2024},
Volume = {712},
Number = {1, SI},
Pages = {211-222},
Month = {MAR},
Abstract = {After 25 years of successful experience with democracy, Benin entered a
   phase of democratic backsliding after President Patrice Talon came to
   power in 2016. According to multiple measures, Benin's democracy
   declined continuously between 2016 and 2023. This article highlights how
   the complacency and weakness of elites in Benin created an environment
   in which democracy deteriorated. Notably, the influence-or lack of
   influence-of elites resulted in limited competitiveness of the electoral
   process and worsening freedom of expression and assembly. Better
   cooperation of opposition forces in 2023 has helped to halt further
   backsliding, but it has not reversed the decline.},
