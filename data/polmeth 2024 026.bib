@article{ WOS:001538077300009,
Author = {Rodriguez, Nancy},
Title = {Making Social Justice Nonnegotiable in Crime Prevention: Case Studies of
   Youth (In)Justice},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2024},
Volume = {714},
