@incollection{ WOS:001505486100008,
Author = {Altan, Selda},
Editor = {Hoerder, D and Neissl, L},
Title = {The Quest for Chinese Labour Colonial Competition for Coolies and the
   Emergence of the Modern Chinese Worker},
Booktitle = {MIGRANT ACTORS WORLDWIDE: Capitalist Interests, State Regulations, and
   Left-Wing Strategies},
Series = {Studies in Global Social History},
Year = {2024},
Volume = {53},
Pages = {142-162},
Abstract = {The scholarship on the nineteenth-century coolie trade has shown that
   the study of post-emancipation labour regimes can yield valuable
   insights into understanding the continuous impact of global labour
   circulations. In particular, indenture helps to elucidate the transition
   from slavery to wage labour and the creation of racial hierarchies in
   colonized territories. Building on the scholarship on bonded labour
   regimes, this chapter traces the roots of the modern Chinese worker in
   the nineteenth-century coolie trade and reconstructs the transnational
   context of class formation in China. It demonstrates that the coolie
   trade in the age of colonial competition precipitated the creation of a
   labour market in China through colonial coercion. Ironically, the
   proliferation of overseas work opportunities and the shared knowledge of
   the coolie experience carried labour to the centre of anti-colonial and
   anti-dynastic struggles in China while offering a critique of the
   debilitation of global labour movements by racial thinking.},
