@article{ WOS:001370562300001,
Author = {Xie, Peixuan},
Title = {China's normative engagement with the Women, Peace and Security agenda:
   ``developmental peace{''} and non-intervention theses and implications},
Journal = {INTERNATIONAL FEMINIST JOURNAL OF POLITICS},
Year = {2024},
Volume = {26},
Number = {5, SI},
Pages = {1119-1151},
Month = {OCT 19},
Abstract = {Emerging endeavors from Asia on the United Nations Security Council
   (UNSC) Resolution 1325 on Women, Peace and Security (WPS) are attracting
   growing academic and analytical attention. However, few have touched on
   China's engagement with the WPS agenda, despite the country's status as
   a veto-holding power on the UNSC and an important global peace and
   security actor avidly promoting a development and non-intervention-based
   path to peacebuilding. How China's identity and agenda in the global
   political order shape the role(s) that it could play in engaging with
   the WPS agenda warrants due inquiry for the potential impacts on the
   further development of WPS norms. This article examines such dynamics by
   setting China's ambivalent normative engagement with the WPS agenda in
   the context of the country's overarching political goals to establish
   itself as a responsible and powerful actor in peace and security
   governance with propositions different from Western-constructed liberal
   prescriptions. It does so by examining key policy texts including
   official statements, remarks, and speeches at the UNSC. Speaking to
   existing scholarship on WPS norms and politics, the article is intended
   to illuminate the implications of China's ``developmental peace{''} and
   non-intervention theses in the WPS normative space.},
