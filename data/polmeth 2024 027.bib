@article{ WOS:001328281500010,
Author = {Braun, Christian Nikolaus},
Title = {Introduction: Ethics and the War against Ukraine},
Journal = {ETHICS \& INTERNATIONAL AFFAIRS},
Year = {2024},
Volume = {38},
Number = {1},
Pages = {3-5},
Month = {SPR},
Abstract = {Now in its third year, the Russian war of aggression against Ukraine
   remains at the very top of the international security agenda. This
   conflict has largely refocused the West's attention away from the
   counterterrorism and counterinsurgency campaigns that followed the
   terrorist attacks of September 11, 2001. In February 2022, German
   chancellor Olaf Scholz went so far as to declare that the invasion
   signaled a zeitenwende, or ``dawn of a new era.{''}1 Russia's aggression
   and the threat of having to fight a peer or near-peer competitor raises
   difficult questions, many of which are ethical in nature. The essays
   gathered in this roundtable seek to provide answers to some of those
   questions. They are the result of a workshop I organized for the King's
   College London Centre for Military Ethics in October 2023. One of my
   ambitions for this workshop was to put leading academics working on the
   ethics of war into discussion with military practitioners, making sure
   to include Ukrainian voices. I was very fortunate to succeed in these
   objectives, and I am convinced that the published essays have benefited
   enormously from our conversations.},
