@article{ WOS:001394130400001,
Author = {Albertone, Baptiste},
Title = {Lost principles of a `sustainable developmentalism'},
Journal = {REVIEW OF INTERNATIONAL POLITICAL ECONOMY},
Year = {2025},
Volume = {32},
