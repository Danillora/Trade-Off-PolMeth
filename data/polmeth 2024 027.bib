@article{ WOS:001328281500002,
Author = {Renic, Neil},
Title = {The Cost of Atrocity: Strategic Implications of Russian Battlefield
   Misconduct in Ukraine},
Journal = {ETHICS \& INTERNATIONAL AFFAIRS},
