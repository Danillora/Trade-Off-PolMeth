   Rickard, Stephanie J., London Sch Econ, Houghton St, London WC2A 2AE, England.},
DOI = {10.1007/s11558-024-09566-z},
EarlyAccessDate = {SEP 2024},
ISSN = {1559-7431},
EISSN = {1559-744X},
Keywords = {WTO; Environment; Trade; Subsidies},
Keywords-Plus = {WORLD-TRADE ORGANIZATION; SMALL STATES; POWER; COALITIONS; POLITICS;
