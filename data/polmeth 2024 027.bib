@article{ WOS:001236091600005,
Author = {Mbirigi, Ferdinand and Niyonizigiye, Pascal},
Title = {The Prosecution of Atrocity Crimes in the Great Lakes Region of Africa,
   between Accountability Logic and Political Strategies},
Journal = {GLOBAL RESPONSIBILITY TO PROTECT},
Year = {2024},
Volume = {16},
Number = {2},
Pages = {150-176},
Month = {APR},
Abstract = {Several efforts have been undertaken to uphold the liability for
   atrocity crimes in the Great Lakes Region of Africa. Even though the
   International Criminal Court, the International Criminal Tribunal for
   Rwanda, and domestic courts opened proceedings against alleged
   perpetrators of atrocity crimes in the Democratic Republic of Congo,
   Central Africa Republic, Uganda, Kenya, Sudan, Burundi, and Rwanda,
   these prosecuting efforts are viewed as pursuing political strategies.
   Based on documentary analysis and an online research survey, this
   research paper shows evidence of accountability logic and political
   strategies. Although the undertaken efforts are in line with the logic
   of accountability, they remain under the influence of political
   strategies. The research findings highlight that political strategies
   have been influencing criminal law codification and its enforcement.},
