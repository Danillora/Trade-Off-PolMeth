@article{ WOS:001370332000001,
Author = {Lleshi, Sokol and Kalemaj, Ilir},
Title = {Party Organisation, Youth Wings and Political Representation in
   Contemporary Albania},
Journal = {EUROPE-ASIA STUDIES},
