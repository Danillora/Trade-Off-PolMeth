@article{ WOS:001364689800001,
Author = {Gromilova, Anna and Braun, Mats},
Title = {Competing Narratives of the Eurasian Economic Union},
Journal = {EUROPE-ASIA STUDIES},
Year = {2024},
Volume = {76},
Number = {10},
Pages = {1621-1643},
Month = {NOV 25},
Abstract = {This article examines narratives around the Eurasian Economic Union
   (EAEU). We use the concept of `altercasting' and perform a media
   analysis, examining how the main articulations of the EAEU by Russian
   officials resonate with the elites in two of the other EAEU member
   states, namely Belarus and Kazakhstan. We show that these narratives
   encountered a mixed reception and resulted in the formulation of new
   alternative EAEU narratives ('reverse altercasting'). We conclude that
   understandings of the EAEU based on cultural sameness are limited and
   that shared perceptions in the two countries examined in this article
   are more about its instrumental benefits.},
