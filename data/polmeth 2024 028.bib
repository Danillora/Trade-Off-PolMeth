@article{ WOS:001329492700001,
Author = {Salyga, Jokubas},
Title = {Deciphering the Local Origins of Neoliberalism in Central and Eastern
   Europe: The Example of Lithuania},
Journal = {EUROPE-ASIA STUDIES},
Year = {2024},
Volume = {76},
Number = {9},
Pages = {1321-1346},
Month = {OCT 20},
Abstract = {This article studies the local lineage of neoliberalism in Lithuania,
   examining its intellectual pedigree, early proponents and the structural
   shifts that facilitated its rise. The latter are attributed to the
   openings created by perestroika and the development of the communication
   network linking economists within the Reform Movement of Lithuania, \&
   eacute;migr \& eacute; intellectuals and Hillsdale College in Michigan.
   Information exchanges produced within this network generated embryonic
   neoliberal ideas before 1991. The network also contributed to the
   establishment of an influential think tank, the Lithuanian Free Market
   Institute (LFMI), whose policy influence is illustrated by surveying
   monetary and pension privatisation reforms.},
