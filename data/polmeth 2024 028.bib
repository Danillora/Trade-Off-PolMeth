@article{ WOS:001276029800001,
Author = {Wright, Katharine A. M. and Mcareavey, Ruth and Donaldson, Rebecca},
Title = {The Impact of Brexit on Women, Peace and Security in Northern Ireland:
   Spotlight on Violence Against Women},
Journal = {JCMS-JOURNAL OF COMMON MARKET STUDIES},
