@article{ WOS:001138524100001,
Author = {Patterson, David},
Title = {Human Rights-based Approaches and the Right to Health: A Systematic
   Literature Review},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2024},
