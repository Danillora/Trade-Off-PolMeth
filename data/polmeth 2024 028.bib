@incollection{ WOS:001505486100012,
Author = {Siegelbaum, Lewis H. and Moch, Leslie Page},
Editor = {Hoerder, D and Neissl, L},
Title = {Labour Migration Regimes in Imperial Russia, the Soviet Union, and the
   Russian Federation},
Booktitle = {MIGRANT ACTORS WORLDWIDE: Capitalist Interests, State Regulations, and
   Left-Wing Strategies},
Series = {Studies in Global Social History},
Year = {2024},
Volume = {53},
Pages = {215-234},
Abstract = {This chapter surveys labour migration within the Eurasian landmass that
   successively comprised the Russian Empire, the Soviet Union, and the
   Russian Federation from the end of serfdom in 1861 to the recent past.
   It highlights incentives to leave home for work and measures to control
   mobility defined as regimes of labour migration, taking a chronological
   perspective on major migrations: the resettlement of peasants on land
   set aside for cultivation, arrangements for seasonal migration,
   rural-to-urban migration, wartime evacuation, and settlement for major
   resource development and infrastructure projects. It argues that
   although the measures to incentivize and control labour migration before
   1917 and after 1991 share strategies with contemporary migration regimes
   elsewhere, the Soviet state outstripped its contemporaries in its
   ambitions, efforts, and institutions developed to control the migration
   of working people.},
