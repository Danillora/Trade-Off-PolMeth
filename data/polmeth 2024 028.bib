@incollection{ WOS:001505486100014,
Author = {Saich, Richard},
Editor = {Hoerder, D and Neissl, L},
Title = {The Making of a Neoliberal Labour Regime in California
   Immigration, American Empire, and Union Organizing in
   the 1980s and 1990s},
Booktitle = {MIGRANT ACTORS WORLDWIDE: Capitalist Interests, State Regulations, and
   Left-Wing Strategies},
Series = {Studies in Global Social History},
Year = {2024},
Volume = {53},
Pages = {248-266},
Abstract = {In the 1980s, California became an important economic hub within an
   increasingly integrated global economy. However, economic restructuring
   made work more precarious, particularly in the growing services sector.
   The shift to neoliberal modes of governance exacerbated inequality and
   poverty and undermined social protections that had been
   institutionalized by liberal reformers since the 1930s. This chapter
   argues that California's neoliberal labour regime of the 1980s was
   sustained in large part by the exploitation of (im-)migrant workers from
   Mexico, Guatemala, El Salvador, and Nicaragua and that migration from
   these countries was shaped by the impact of the Reagan administration's
   foreign and economic policy in Central America. These structural
   features shaped the context of labour organizing in complex and
   sometimes unexpected ways and ultimately contributed to the
   revitalization of unions in California. In fact, the mobilization and
   militancy of immigrant workers was a crucial contributing factor to the
   labour resurgence in the state in the 1990s.},
