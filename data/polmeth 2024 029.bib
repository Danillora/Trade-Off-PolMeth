@article{ WOS:001367922500001,
Author = {Nilson, Diego Hernandez and Sanahuja, Jose Antonio and Burian, Camilo
   Lopez},
Title = {The far right, populism, and the contestation of regionalism in South
   America: Bolsonaro and Milei},
