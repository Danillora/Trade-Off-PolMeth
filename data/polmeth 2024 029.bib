@article{ WOS:001261627100001,
Author = {Wilmers, Leila},
Title = {Diversity or Unity: Interpreting the Discourse of a Multi-Ethnic Russian
   Nation in Kazan},
Journal = {EUROPE-ASIA STUDIES},
Year = {2024},
Volume = {76},
Number = {7},
Pages = {1078-1101},
Month = {AUG 8},
Abstract = {This article explores bottom-up responses to the Kremlin's approach to
   nation-building in a multi-ethnic state. How do residents of ethnically
   mixed cities navigate conflicting themes of unity and diversity in the
   federal discourse of Russia as a multi-ethnic nation (mnogonatsional'nyi
   narod)? This discourse runs counter to assimilative policies and a
   concurrent vision of Russia as a civilisation rooted in Slavic culture.
   In the diverse city of Kazan, the discourse is shown to be easily
   adopted by residents in narrating belonging, while being a problematic
   basis for nation-building. The discussion highlights the importance of
   regional and ethnic subject positions in bottom-up engagement with
   nation-building in Russia.},
