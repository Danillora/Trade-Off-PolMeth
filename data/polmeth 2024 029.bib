@article{ WOS:001146233800001,
Author = {Hearn, Jeff},
Title = {Evaluating the concept of political masculinity/ies: a simple idea or a
   case of too many ideas?},
Journal = {EUROPEAN JOURNAL OF POLITICS AND GENDER},
