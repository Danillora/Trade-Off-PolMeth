@article{ WOS:001383032500009,
Author = {Blank, Rebecca M. and Card, David and Kuttner, Hanns},
Title = {Newly Poor Women and the Social Safety Net, 1990-2010},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2024},
Volume = {711},
Number = {1, SI},
Pages = {121-143},
Month = {JAN},
Abstract = {Welfare reforms in the 1990s, particularly the Personal Responsibility
   and Work Opportunity Reconciliation Act of 1996, were motivated by
   concerns over the long-term poor. We compare the experiences of newly
   poor women in 1990-1992, 2001-2003, and 2008-2010 and find that the size
   and sources of the income shocks generating new spells of poverty
   remained relatively stable. In dual-head families, the newly poor were
   most often pushed into poverty by a drop in spousal earnings; in
   single-mother families, the main cause was a drop in personal earnings.
   For single mothers in the latter two periods, we find that income from
   cash transfers after becoming poor was less than in the 1990-1992
   benchmark. As these transfers include welfare payments, the timing
   points to the impacts of the 1996 welfare reforms.},
