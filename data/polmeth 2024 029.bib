@article{ WOS:001373920400001,
Author = {Chen, Weitseng},
Title = {Beyond Security: The Compliance Dimension of National Security Reviews
   in Singapore and Taiwan},
Journal = {REGULATION \& GOVERNANCE},
Year = {2024},
Month = {2024 DEC 9},
Abstract = {Growing concerns over Chinese investment have spurred stricter national
   security reviews globally, yet compliance and enforcement within these
   reviews remain understudied. This paper examines the enforcement
   mechanisms of national security review in Singapore and Taiwan, key
   trade partners with China. It contrasts Singapore's emphasis on rigorous
   ex post monitoring with Taiwan's focus on restrictive ex ante screening.
   To strengthen public enforcement, Singapore relies on privatized
   monitoring while Taiwan incorporates private enforcement. This study
   argues that robust state oversight through public enforcement is
   critical for compliance, but underscores the importance of extending
   enforcement beyond the initial investment stage to include post-entry
   monitoring. Additionally, incorporating market intermediaries as
   enforcement agents presents a promising approach. This comparative
   analysis contributes to a richer understanding of national security
   review enforcement beyond traditional US-centric framework.},
