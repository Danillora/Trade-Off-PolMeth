@article{ WOS:001378194800001,
Author = {Oskolkov, Petr},
Title = {`Let Them Hate, As Long As They Fear': The Russian NS/WP Movement on
   Telegram},
Journal = {EUROPE-ASIA STUDIES},
Year = {2025},
Volume = {77},
Number = {1},
Pages = {88-106},
Month = {JAN 2},
Abstract = {National Socialism/White Power (NS/WP) is a rhizomatic constellation of
   groupuscules that praise `direct action' and violence against the
   `anti-Russian' regime. It enjoys online affordances, having chosen
   Telegram as the safest alternative to mainstream social media. Frequency
   and co-occurrence network analysis of the Oderint, Dum Metuant channel's
   discourse revealed the movement's preoccupation with ideological matters
   and practical instructions for adherents on how to secure
   communications, produce explosives, and conduct terror attacks. In the
   context of violence escalation and an ideological vacuum, the skills and
   distinct ideological profile of NS/WP can make it a viable actor amongst
   the Russian nationalist youth.},
