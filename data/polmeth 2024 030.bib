@article{ WOS:001449126900010,
Author = {Milacic, Filip},
Title = {Democratic Backsliding Through Legislative Capture in Serbia: A One-Man
   Show},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
