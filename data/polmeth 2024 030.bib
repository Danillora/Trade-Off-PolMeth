@incollection{ WOS:001505486100009,
Author = {Juif, Dacil},
Editor = {Hoerder, D and Neissl, L},
Title = {African Agency versus State and Capital Control Migration to the
   British Northern Rhodesian Copperbelt in Comparative Perspective,
   1920s to 1960s},
Booktitle = {MIGRANT ACTORS WORLDWIDE: Capitalist Interests, State Regulations, and
   Left-Wing Strategies},
Series = {Studies in Global Social History},
Year = {2024},
Volume = {53},
Pages = {163-185},
Abstract = {Migration played a key role in supplying labour for African large-scale
   mining throughout the twentieth century. In this chapter, we assess
   workers' agency versus state-capital control of migration, residence in
   mining towns, and labour mobilization in Northern Rhodesia's Copperbelt
   compared to the other main mines of Southern and Central Africa. While
   in the Belgian Congo and Portuguese Angola officials resorted to active
   recruitment - often under coercion - in British Northern Rhodesia they
   only did so for a few years during the advent of industrial copper
   mining. A high turnover with temporary stays in rural areas was promoted
   in the British colony but not imposed as effectively as in South Africa.
   Although women were welcome in urban areas for their reproductive role,
   they also carried out a range of marketed activities. African
   mineworkers' unions only emerged in the 1950s and attained moderate
   achievements, although in surrounding mining regions they were either
   banned or virtually inactive.},
