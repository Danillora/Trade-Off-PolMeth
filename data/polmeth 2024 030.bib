@article{ WOS:001346999600001,
Author = {Tulaeva, Svetlana and Henry, Laura A. and Snarski, Yaroslav},
Title = {Ethnic Environmentalism in the Russian Regions: National Identity and
   Nature Protection},
Journal = {EUROPE-ASIA STUDIES},
Year = {2024},
Volume = {76},
Number = {9},
Pages = {1417-1438},
Month = {OCT 20},
Abstract = {Increased public awareness of environmental problems and the
   strengthening of diverse identities may lead to the emergence of `ethnic
   environmentalism'. These movements make environmental claims associated
   with the interests of an ethnic community. We develop this concept by
   examining protests in the Russian regions. Our findings suggest that
   ethnic identity facilitates environmental activism under the following
   conditions: the ethnic group claims a special cultural relationship to
   the natural environment; environmental concern and ethnic identity are
   linked to the unfair distribution of resources; and blame is directed
   towards regional actors rather than the federal government.},
