@article{ WOS:001449126900007,
Author = {Enyedi, Zsolt and Mikola, Balint},
Title = {Legislative Capture in Hungary: Well-Managed Autocratization},
Journal = {ANNALS OF THE AMERICAN ACADEMY OF POLITICAL AND SOCIAL SCIENCE},
Year = {2024},
Volume = {712},
Number = {1, SI},
Pages = {34-46},
Month = {MAR},
Abstract = {We discuss the case of Hungary's rapid democratic backsliding under
   Viktor Orb \& aacute;n as an example of legislative capture. We show
   that, despite relatively unfavorable conditions for autocratization,
   Viktor Orb \& aacute;n and his Fidesz party have supervised a
   well-crafted project of institutional transformation and a comprehensive
   regrouping of financial resources, all of which have led to an effective
   and stable concentration of power. This power concentration has its
   limits, given the country's membership in the European Union, resistance
   by parts of the judiciary, media initiatives focused on corruption, and
   local opposition victories; but the national government's room for
   maneuver has increased every year since 2010. We provide an overview of
   the changes that facilitated democratic erosion and an inventory of the
   actors that still retain a capacity to hinder further autocratization.},
