@article{ WOS:001282119700001,
Author = {Haughton, Tim and Krasovec, Alenka and Cutts, David},
Title = {Jumping on the New Party Bandwagon: The 2022 Elections and the
   Development of Party Politics in Slovenia},
Journal = {EUROPE-ASIA STUDIES},
Year = {2024},
Volume = {76},
Number = {10},
Pages = {1526-1550},
Month = {NOV 25},
Abstract = {New political parties can sometimes achieve remarkable election results.
   The 2022 parliamentary elections in Slovenia highlight that while
   appeals centred around anti-corruption, competence and newness
   contributed to the Freedom Movement's ability to win over one third of
   the vote, its victory owed much to tactical voting. Data from a
   specially commissioned survey conducted immediately after the election
