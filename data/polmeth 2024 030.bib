@article{ WOS:001365773400001,
Author = {Duke, Misty C. and Bolsinger, Diana and Landon-Murray, Michael},
Title = {Improving intelligence analysis and education in the US with stronger
   foundations in statistical literacy},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
Year = {2025},
Volume = {40},
Number = {2, SI},
Pages = {354-374},
Month = {FEB 23},
Abstract = {This article argues that coursework in statistical literacy can improve
   the practice of intelligence analysis in the US. The article describes
   the gaps in current intelligence education pertaining to statistically
   literacy, explains how statistical literacy relates to the tasks of
   intelligence analysis, and offers a series of recommendations for
   statistical literacy courses specifically designed for future
   intelligence practitioners. The incorporation of statistical literacy in
   intelligence studies programs will advance and strengthen the
   contribution such programs can make to the intelligence workforce, both
   in the public and private sectors.},
