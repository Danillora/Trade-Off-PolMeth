@article{ WOS:001141004300001,
Author = {Calderon, Maria Daniela de la Rosa},
Title = {Rights-based Climate Litigation in Colombia: An Assessment of Claims,
   Remedies, and Implementation},
Journal = {JOURNAL OF HUMAN RIGHTS PRACTICE},
Year = {2024},
Volume = {16},
Number = {1, SI},
Pages = {273-284},
Month = {MAY 28},
Abstract = {Climate litigation in Colombia is increasingly centred on fundamental
   and/or constitutional human rights. This note evaluates rights-based
   climate litigation in Colombia through the lens of five cases that
   protect ecosystems: the Atrato River, the Combeima River, and the Bruno
   River; the Amazon rainforest; and the Paramos ecosystem. First, the
   framing of cases is analysed. Second, the judges' interpretations of the
   link between constitutional rights and climate change (if any), their
   reasoning, the scope of the remedies granted, and their intended impact
   are analysed. Relatedly, the note explores whether decisions that do not
   explicitly mention climate change can be considered climate cases as
   their remedies directly or indirectly affect climate change mitigation
   or adaptation. Finally, the note examines the status of compliance with
   or implementation of judicial remedies in climate cases.},
