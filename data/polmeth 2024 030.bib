@article{ WOS:001275239300001,
Author = {Miksa, Rad},
Title = {Assessment tabling: an integrated structured analytic technique for
   improved intelligence analysis and reasoning visualisation},
Journal = {INTELLIGENCE AND NATIONAL SECURITY},
