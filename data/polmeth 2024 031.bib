@incollection{ WOS:001505486100016,
Author = {Althammer, Beate},
Editor = {Hoerder, D and Neissl, L},
Title = {Foreign Polish Labour Migrants in the German Empire A
   Reassessment},
Booktitle = {MIGRANT ACTORS WORLDWIDE: Capitalist Interests, State Regulations, and
   Left-Wing Strategies},
Series = {Studies in Global Social History},
Year = {2024},
Volume = {53},
Pages = {275-297},
Abstract = {The migration of ``foreign{''} Polish labourers from tsarist Russia and
   Habsburg Galicia to imperial Germany, and especially to eastern Prussia,
   has occupied academic research since its onset at the end of the
   nineteenth century. Labour scholars have lately even identified this
   case as one of the first ``guestworker{''} programs in global history -
   a specifically modern type of managed migration aimed at providing cheap
   and flexibly disposable labour. However, blind spots remain. After
   outlining the regulatory system that the Prussian government designed
   for the recruitment of foreign Poles, this chapter discusses some
   misconceptions that have arisen in recent scholarship. The focus
   subsequently shifts from the agrarian Prussian east to the
   industrialized west and examines how the system functioned there. The
   final section highlights the cases of individual Polish workers whom
   Prussian authorities targeted for expulsion because of their irregular
   presence. Their stories illustrate migrants' diverse routes and
   aspirations as well as the ways in which official policy was resisted.},
