@article{ WOS:001289440200001,
Author = {Sevilla Sevilla, Jose Antonio and Beer, Caroline},
Title = {Explaining leadership quality among the directors of Mexico's
   subnational women's policy agencies},
Journal = {INTERNATIONAL FEMINIST JOURNAL OF POLITICS},
Year = {2024},
Volume = {26},
Number = {4},
Pages = {858-880},
Month = {AUG 7},
Abstract = {This article examines the effectiveness of women's policy agencies
   (WPAs), with a focus on the leadership of subnational women's institutes
   in the Mexican states. We contribute to the existing research with a new
   original dataset on WPAs in an important third-wave democracy in Latin
   America. Our focus on leadership quality provides a useful method for
   measuring one central component of institutional effectiveness across a
   large number of cases. We consider high-quality leaders to be those with
   longer tenure in office and higher levels of education, more
   professional experience, and closer connections to the feminist
   movement. Using a dataset of 226 directors, we find evidence that party
   ideology, electoral competition, and the institutional design of the
   agency influence the quality of leadership. Moreover, states with
   stronger women's movements have higher-quality leadership in their WPAs.
   These findings support a central premise of the state feminism
   literature - that WPAs are most effective when allied with strong
   women's movements and left-wing governments - and also point to the
   importance of electoral democracy in promoting institutional
   effectiveness.
   Este art \& iacute;culo examina la eficacia de las agencias de pol \&
   iacute;ticas p \& uacute;blicas para las mujeres (APPM) centr \&
   aacute;ndose en el liderazgo de los institutos subnacionales de la mujer
   en los estados mexicanos. De esta manera, contribuimos a la investigaci
   \& oacute;n existente con un nuevo conjunto de datos originales sobre
   las APPM en una nueva democracia, importante en el contexto
   Latinoamericano, de la tercera ola democr \& aacute;tica. Nuestro
   enfoque en la calidad del liderazgo proporciona un m \& eacute;todo \&
   uacute;til para medir un componente central de la eficacia institucional
   en un gran n \& uacute;mero de casos. Por lo cual, consideramos que las
   l \& iacute;deres de alta calidad son aquellas con una mayor permanencia
   en el cargo, niveles m \& aacute;s altos de educaci \& oacute;n, mayor
   experiencia profesional y conexiones m \& aacute;s estrechas con el
   movimiento feminista. Utilizando una nueva y original base de datos de
   226 directoras, encontramos pruebas de que la ideolog \& iacute;a del
   partido, la competitividad electoral y el dise \& ntilde;o institucional
   de la agencia influyen en la calidad del liderazgo. Adem \& aacute;s,
   los estados con movimientos de mujeres m \& aacute;s fuertes tienen un
   liderazgo de mayor calidad en sus agencias de pol \& iacute;ticas p \&
   uacute;blicas para las mujeres. Estos resultados respaldan una premisa
   central de la literatura sobre el feminismo de Estado, seg \& uacute;n
   la cual, las APPM son m \& aacute;s eficaces cuando se al \& iacute;an
   con movimientos de mujeres fuertes y gobiernos de izquierda en contextos
   donde hay m \& aacute;s competitividad electoral.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
