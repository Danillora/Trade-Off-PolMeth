@article{ WOS:001367120800003,
Author = {Neilsen, Rhiannon},
Title = {Cyberspace and the Responsibility to Protect Populations from Atrocity
   Crimes},
Journal = {GLOBAL RESPONSIBILITY TO PROTECT},
