@article{ WOS:001379659900013,
Author = {Park, Booyoung},
Title = {``Ex-gay{''} campaigns as a new form of conservative Christians' queer
   hatred politics in South Korea},
Journal = {INTERNATIONAL FEMINIST JOURNAL OF POLITICS},
Year = {2024},
Volume = {26},
Number = {5, SI},
Pages = {1072-1096},
Month = {OCT 19},
Abstract = {Since the mid-2000s, conservative Christians in South Korea have focused
   heavily on anti-gay campaigns that demonize gay people and homosexuality
   itself, demonstrating a violent aversion to queer subjects. However,
   certain Christian groups started claiming to be more ``tolerant{''} in
   the early 2010s in their advocacy for ``curing{''} homosexuality through
   ``ex-gay{''} campaigns. I argue that these supposedly more
   ``tolerant{''} ex-gay campaigns cause harm to gay people and are fast
   becoming more prevalent as a covert form of hatred in South Korea. This
   study analyzes the strategies of ex-gay campaigns conducted by a
   subsection of conservative Christian groups and the discourse of ex-gay
   testimonies to capture the subtle changes in hatred politics and
   critically explore their ideological framework to debunk their
   homophobic arguments. The research findings reveal that (1) ex-gay
   campaigns try to undermine the credibility of queer movements by
   appropriating their content, form, and emotions, and (2) through the
   testimonies of ex-gay people, they contradictorily construct identities
   based on hetero-patriarchal norms. These findings illustrate how ex-gay
   campaigns have similar ideological underpinnings to anti-gay campaigns,
   but with differences in strategy and articulation. Through a close
   analytical reading of South Korean campaigns, this article contributes
   to the broader global efforts to construct a richer counter-discourse
   against queer hatred.},
