@article{ WOS:001186887800001,
Author = {Jungkunz, Sebastian and Helbling, Marc and Isani, Mujtaba},
Title = {Measuring Implicit Political Extremism through Implicit Association
   Tests},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2024},
Volume = {88},
Number = {1},
Pages = {175-192},
Month = {APR 12},
Abstract = {We develop the first implicit association test (IAT) to measure general
   implicit extremist attitudes in Germany, Great Britain, and the
   Netherlands. We find that implicit extremist attitudes are positively
   but weakly correlated with existing explicit measures. This indicates
   that implicit measures capture different parts of the population, for
   example, cases in which associations are based on automated cognitive
   processes instead of conscious thought. Further results show that
   particularly the higher-educated and nonreligious respondents score
   higher on the implicit than on the explicit extremism measures. Our
   results highlight the importance of studying implicit attitudes of
   political extremism. We further provide novel insights into how to
   develop IATs for abstract concepts such as democracy and extremism, for
   the study of which we cannot use readily available images or concepts.},
Publisher = {OXFORD UNIV PRESS},
Address = {GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND},
Type = {Article},
Language = {English},
