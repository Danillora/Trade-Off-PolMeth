@article{ WOS:001194997000001,
Author = {Sokoli, Ylli},
Title = {Serbia's hybrid warfare: consequences for neighboring countries},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2024},
Volume = {32},
Number = {4},
Pages = {1212-1227},
Month = {OCT 1},
Abstract = {The aim of this research is to analyze the consequences of Serbia's
   hybrid warfare; Countries exposed to its hybrid warfare; The means and
   methods used by Serbia; The mechanisms subjects use to defend
   themselves; Factors and circumstances contributing to the success or
   failure of Serbia's hybrid activities; The political, territorial,
   security, and economic consequences of hybrid warfare; The Future of
   Serbia's hybrid warfare; And the future of relations and cooperation
   between Serbia and Russia. Hybrid warfare can be waged using
   conventional and unconventional warfare measures. Serbia is waging
   hybrid warfare in the 21st century, primarily with unconventional
   measures against Kosovo, and Bosnia and Herzegovina, violating the
   sovereignty of these states, including Montenegro and North Macedonia.
   Serbia's engagement in hybrid warfare is characterized by a combination
   of diplomatic maneuvers, propaganda campaigns, the deployment of
   irregular military forces near the Kosovo border, covert local criminal
   formations, and the use of military threats, primarily directed against
   Kosovo.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
