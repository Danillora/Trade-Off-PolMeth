@article{ WOS:001142966200001,
Author = {Gricius, Gabriella and Fakhoury, Renato},
Title = {Security for the small: materializing securitization in Finland and
   Norway},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2024},
