@article{ WOS:001381955000001,
Author = {Ryan, Curtis R.},
Title = {Jordanian Islamists and the war on Gaza},
Journal = {MEDITERRANEAN POLITICS},
Year = {2025},
Volume = {30},
Number = {2},
Pages = {406-415},
Month = {MAR 15},
Abstract = {The 2023-24 Gaza war had a profound impact on Jordanian domestic
   politics, the resurgence of Islamist and other opposition movements, and
   the security of the Hashemite Regime itself. This article examines the
   many facets of Islamist activism in Jordan, responding to the Gaza war.
   I argue that Jordan's largest Islamist movement, the Muslim Brotherhood,
   needs to be seen in the broader context of Jordanian opposition
   politics, since most of Jordan's many protests and demonstrations over
   Gaza were ideologically and organizationally diverse. While Jordan has
   at times seen violent Jihadist activism, most Islamist activism has been
   peaceful and reform-oriented. The Muslim Brotherhood and the Islamic
   Action Front have in the past participated within the system and
   therefore competed against leftist and nationalist forces in elections
   but have allied with them for Gaza-focused street protests demanding
   that Jordan change its policies towards Israel. The article therefore
   also examines the extent to which government and opposition (both
   Islamist and non-Islamist) have at times been in sync with one another
   over Gaza, while at other times diverging significantly.},
