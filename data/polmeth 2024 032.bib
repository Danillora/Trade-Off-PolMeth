@article{ WOS:001355135800001,
Author = {Ho, Selina and Lee, Terence},
Title = {Elite Perceptions of a China-Led Regional Order in Southeast Asia},
Journal = {JOURNAL OF CURRENT SOUTHEAST ASIAN AFFAIRS},
Year = {2025},
Volume = {44},
Number = {1},
Pages = {148-173},
Month = {APR},
Abstract = {How do Southeast Asian states perceive a China-led regional order? To
   answer this question, we conducted a survey of elites from Indonesia,
   Malaysia, the Philippines, Singapore, Thailand, and Vietnam, countries
   that are ``least likely{''} to acquiesce to a China-led regional order.
