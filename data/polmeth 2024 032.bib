@article{ WOS:001472951100003,
Author = {Shaffer, Ryan},
Title = {Counter-terrorism, intelligence and policing in Sri Lanka: a case study
   of the 2019 easter terror attacks},
Journal = {JOURNAL OF POLICING, INTELLIGENCE AND COUNTER TERRORISM},
Year = {2024},
Volume = {19},
Number = {2},
Pages = {190-206},
Month = {APR 2},
Abstract = {This article examines the 2019 Easter Sunday terror attacks in Sri Lanka
   as a case study to probe intelligence and policing counter-terrorism
   efforts in the country. It explores the background of the bombings, the
   ensuing investigations and impact of the attacks on the intelligence and
   police services using Sri Lankan newspaper sources and government
   reports. The article argues the intelligence failure was a breakdown in
   Sri Lanka's intelligence and police services' intelligence cycle wherein
   the relevant information was not disseminated to those who could prevent
   the attacks.},
