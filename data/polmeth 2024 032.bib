@article{ WOS:001138465800001,
Author = {Nociar, Tomas and Struhar, Pavol},
Title = {No school of hard knocks: education policies and ideologies of Slovak
   far-right parties},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2025},
Volume = {33},
Number = {1, SI},
Pages = {75-89},
Month = {JAN 2},
Abstract = {The aim of this paper is to examine the unexplored intersection between
   the ideologies and education policies of the Slovak far-right party
   family. From a theoretical perspective, the article draws on the
   morphological approach to the study of ideology. By using this approach,
   the main goal of this article is to analyse, map, and compare education
   policies in the context of the ideologies of three successful yet
   ideologically distinct examples of far-right parties in Slovakia. The
   analysis aims to identify the education policies of Slovakia's far-right
   parties and define their positions and roles both within and outside of
   their respective ideologies. On a more general level, by examining the
   Slovak case, the article addresses the question of whether, and if so,
   to what extent the far-right's education policies are also far-rightist
   education policies.},
