   Fernandez, Pablo, Univ San Andres, Victoria, Argentina.},
DOI = {10.1017/lap.2024.51},
EarlyAccessDate = {DEC 2024},
ISSN = {1531-426X},
EISSN = {1548-2456},
Keywords = {affective polarization; identity politics; distrust; Latin America},
