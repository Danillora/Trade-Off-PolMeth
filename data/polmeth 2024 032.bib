@article{ WOS:001387983500001,
Author = {Afitiri, Abdul-Rahaman and Afrifa, Ernest Kofi Amankwa},
Title = {Households' Willingness to Use Water from a Solar Water Disinfection
   Treatment System for Household Purposes},
