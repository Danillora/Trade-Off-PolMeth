@article{ WOS:001377656700001,
Author = {Soto, Andree Juvenal Jimenez and Lerzundi, Andy Roli Fernandez and
   Cuaresma, Julio Ricardo Moscoso},
Title = {European Union interregionalism in the Andean Community integration
   process: a political, economic, and cultural analysis},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2025},
Volume = {33},
Number = {3},
Pages = {804-824},
Month = {JUL 3},
Abstract = {In recent years, European Union (EU) interregionalism has attracted
   considerable academic attention in the fields of international relations
   and economics. However, in-depth academic studies that shed light on its
   authentic and significant contributions to integrating the Andean
   Community (CAN) are still lacking. This study aims to fill this gap by
   investigating the influence of EU interregionalism on the CAN
   integration process. The geo-influence model was used and adapted to
   determine the political, economic, and cultural correlations between the
   EU and CAN. The results show that European interregionalism has had
   significant political, economic, and cultural impacts over the past 33
   years. However, a clear need for further research arises, preferably
   quantitative and qualitative, to assess the influence or relationship of
   European interregionalism, as opposed to the emergence of other
   integration processes, such as those led by China and the United States,
   in political, economic, commercial, and cultural dimensions.},
