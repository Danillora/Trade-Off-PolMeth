@article{ WOS:001379729700001,
Author = {Rudolf, Inna},
Title = {The war on Gaza and Iraq's resistance state},
Journal = {MEDITERRANEAN POLITICS},
Year = {2025},
Volume = {30},
Number = {2},
Pages = {427-438},
Month = {MAR 15},
Abstract = {The events of 7 October, spurred by Hamas' `Al-Aqsa Flood' operation and
   Israel's subsequent fierce military retaliation on Gaza, reverberated
   throughout Shi'a Islamist communities with ties to the Iran-led Axis of
   Resistance. This article aims to delve into the responses of various
   Iraqi Shi'a Islamist factions to the unfolding conflict, shedding light
   on their stances towards Israel and its Western allies, shaped by
   intricate domestic and transnational considerations aimed at bolstering
   their legitimacy and influence. To grasp the nuanced array of reactions,
   I provide a brief overview of the main Shi'a Islamist actors involved in
   resistance politics in Iraq, examining their embeddedness within the
   state apparatus and their relationships with both regional and global
   actors. Subsequently, this discussion will explore how their strategy
   reflects not only their ideological underpinnings but also their
   long-standing interests predating the events of 7 October.},
