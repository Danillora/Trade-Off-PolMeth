@article{ WOS:001184275300001,
Author = {Yardimci-Geyikci, Sebnem and Duyvendak, Jan Willem},
Title = {Catalysts of change: Unveiling the nexus between protest movements and
   party politics in the Mediterranean},
Journal = {MEDITERRANEAN POLITICS},
Year = {2025},
Volume = {30},
Number = {3},
Pages = {528-556},
Month = {MAY 27},
Abstract = {The recent surges of protest have emerged as a striking global
   phenomenon. Several diverse countries, including Egypt, the United
   States, Spain, Hong Kong, Indonesia, and the United Kingdom, have
   witnessed massive protest movements over the past decade. Numerous
   recent studies have underscored the pivotal role of the interaction
   between protest movements and party politics. However, our understanding
   of why certain movements profoundly impact the political landscape,
   sometimes triggering the collapse of existing party systems, while
   others barely alter conventional politics, remains limited. Drawing from
   case studies of Greece, Spain, Portugal, and Turkey, we conduct a
   systematic comparative analysis of the impact of protest movements
   during the 2010s. We aim to elucidate why and how some protests have
   proven more effective in reshaping party politics, while others have
   faltered. Our contention is that the dynamics of the party system play a
   pivotal role in determining the capacity of social movements to
   instigate transformative changes in party politics.},
