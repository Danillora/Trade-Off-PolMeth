@article{ WOS:001192915500001,
Author = {Fytopoulou, Eleftheria and Karasmanaki, Evangelia and Tsantopoulos,
   Georgios},
Title = {Comparing the Effect of Scientific Background on University Students'
   Environmental Views},
Journal = {WORLD},
Year = {2024},
Volume = {5},
