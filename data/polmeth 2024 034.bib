@article{ WOS:001364729800001,
Author = {Coskun, Efser Rana},
Title = {The paradox of inclusion: future engagement or disengagement between the
   EU and Turkey in the context of the Green Deal},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2025},
Volume = {33},
Number = {3},
Pages = {785-803},
Month = {JUL 3},
Abstract = {In December 2019, the EU launched the European Green Deal (EGD). Turkey
   took an important step and decided to be a part of the EGD. The Green
   Deal Action Plan was formalized between the EU and Turkey in July 2021.
   Despite historical fluctuations in EU-Turkey relations, this agreement
   offers a chance to redefine the framework of their relations. Drawing on
   environmental diplomacy literature, this article explores the political
   and economic dimensions of the EGD from various angles. This article
   addresses how environmental diplomacy might reshape EU-Turkey relations,
   focusing on the Green Deal's impact. Examining both multilateral and
   bilateral engagements, it uncovers potential opportunities and obstacles
   in their green cooperation. I scrutinize the paradox of inclusion,
   whether the Green Deal fosters engagement or disengagement between the
   EU and Turkey, proposing a novel analytical framework for environmental
   diplomacy in this context - a pioneering effort in such analysis.},
