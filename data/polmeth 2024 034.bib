@article{ WOS:001382735300001,
Author = {Kelly, Serena and Baugh, Brittany and Bacon, Paul},
Title = {Examining EU action and actorness through external perceptions: a
   comparative study of Asian media coverage},
Journal = {JOURNAL OF CONTEMPORARY EUROPEAN STUDIES},
Year = {2024},
Month = {2024 DEC 25},
Abstract = {As a sui generis entity, a number of studies have been undertaken to
   understand what sort of actor the European Union (EU) is. Descriptions
   of the EU's foreign policy and external impact have ranged through
   emphasising its weight as an economic, civilian, normative, economic,
   and development actor. The President of the EU Commission, Ursula von
   der Leyen, sought to develop a `geopolitical Commission' in 2019,
   striving for the EU to establish itself as a credible `geopolitical
   actor'. This aim was pursued by the introduction of `A Stronger Europe
   in the World', positioned as the fifth of six frameworks shaping the
   EU's identity. This paper systematically analyses the impact and
   presence of this frame in the print media of four Asian countries:
   China, Indonesia, Japan, and South Korea. Examining media perceptions of
   the EU offers insights into how the EU's projection of a Stronger Europe
   in the World has been received and understood in these key partner
   countries at a time of increasing geopolitical competition, aiding
   conceptualisations of the EU's presence and actorness in Asian
   countries. Ultimately, we argue that measuring Asian perceptions of the
   EU provides valuable insights into assessing EU ambitions towards
   becoming a credible and substantial international actor.},
