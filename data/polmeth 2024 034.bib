@article{ WOS:001232009500001,
Author = {Trzeszczynska, Patrycja and Demel, Grzegorz},
Title = {Forming a Ukrainian Diaspora in Poland: Between ``Common Culture,{''}
   National Naturalism, and Othering},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2025},
Volume = {53},
