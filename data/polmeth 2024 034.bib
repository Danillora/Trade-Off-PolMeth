@article{ WOS:001372406300001,
Author = {Sobhy, Hania},
Title = {Campaigning for the revolution: Freedom, social justice and citizenship
   imaginaries in the Egyptian Uprising},
Journal = {MEDITERRANEAN POLITICS},
Year = {2024},
Month = {2024 DEC 7},
Abstract = {The limited electoral success of pro-Revolution forces during the Arab
   uprisings is often attributed to their weak political and organizational
   resources. Yet, in the first round of Egypt's historic 2012 presidential
   elections, pro-Revolution candidates Hamdin Sabahi and Abdel-Monim
   Abul-Futuh jointly outperformed both the `old regime' and Muslim
   Brotherhood contenders, with Sabahi nearly reaching the runoff. Drawing
   on extensive fieldwork across Egypt between 2012 and 2013, this article
   examines how the campaigners of these two candidates translated the
   Revolution's core ideals of freedom and social justice. It introduces
   the notion of citizenship imaginaries to capture how campaigners
   communicated these ideals across divergent experiences and narratives of
   relating to the state. It argues that the two
   campaigns-differently-compensated for their weaknesses by aligning their
   messaging with dominant imaginaries in three important ways: downplaying
   appeals to democracy and radical change except when engaging ``cultured
   voters{''}; advancing a vague but credible pro-poor stance; and adapting
   appeals traditionally tied to the two more powerful political forces:
   stability, Islamism and patronage. By linking resources, imaginaries and
   the agency of social movement actors, the article offers new
   perspectives on electoral dynamics and the strategic communication of
   mobilization frames, especially in transitional and global South
   contexts.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
