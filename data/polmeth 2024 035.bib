@article{ WOS:001346248100001,
Author = {Klumbyte, Neringa and Yeremieieva, Kateryna},
Title = {Ukrainian War Humor and Civic Activism in 2022},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2024},
Month = {2024 OCT 31},
Abstract = {The 2022 war in Ukraine has produced the biggest virtual humor archive
   in the history of wars. We argue that Ukrainian war humor is a form of
   civic activism in the name of Ukraine's sovereignty. This civic activism
   is defined by resistance, solidarity, vigilance, and dedication to
   victory. The war humor circulates locally as well as on a global stage.
   It expresses the government's positions and the people's voices and
   empowers those affected by this war. Ukrainian war humor documents
   experiences of war realities; provides moral commentaries and emotional
   and aesthetic interpretations; and articulates visions for the future of
   Ukraine as a sovereign European state.},
