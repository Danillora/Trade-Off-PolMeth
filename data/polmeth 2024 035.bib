
@article{ WOS:001321038700001,
Author = {Agarin, Timofey and Dambrauskas, Karolis},
Title = {Pork Barrel and Identity Politics: Explaining a Minority Party's
   Electoral Success in Lithuania},
Journal = {NATIONALITIES PAPERS-THE JOURNAL OF NATIONALISM AND ETHNICITY},
Year = {2025},
Volume = {53},
Number = {4},
Pages = {857-876},
Month = {JUL},
Abstract = {Our article describes the lifecycle of Lithuania's Electoral Action of
   Poles-Christian Families Alliance (LLRA-K \& Scaron;S) party that has
   been a part of country's political landscape for near 30 years. Despite
   its seemingly ethnic program, the party has a poor track record for
   delivering on its electoral promises. Yet, it has been continuously
   supported by the majority of Polish-speakers in Lithuania. The
   background of the nationalizing state, which encourages the party elites
   to conflate substantive representation with the signposting of ethnic
   identity in party politics, offers one of the reasons for the LLRA-K \&
   Scaron;S's electoral success. Although the party effectively
   consolidated its regional electorate, it came to control service
   delivery to their ethnic constituency by engaging in pork barrel
   politics. Poor performance in recent national and municipal elections
   put this strategy to bond with its voters into question, casting doubt
   on the LLRA-K \& Scaron;S's ability to survive as an ethnic party in the
   long term.},
