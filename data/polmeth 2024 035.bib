@article{ WOS:001382443200001,
Author = {Schneider, Hanne and Enssle-Reinhardt, Friederike},
Title = {Negotiating migrant integration in times of crisis in small
   municipalities in Germany},
Journal = {LOCAL GOVERNMENT STUDIES},
Year = {2024},
Month = {2024 DEC 26},
Abstract = {Since the beginning of the COVID-19 pandemic, politics, administrations
   and society have been kept in a constant crisis mode. This study
   examines how recent times of polycrisis have affected local integration
   governance in small municipalities in Germany. Drawing from empirical
   research in a small town and a rural county, this paper focuses on the
   local level, particularly civil society actors' roles and conditions of
   cooperation during the COVID-19 pandemic and the arrival of refugees due
   to the Russian war in Ukraine. The findings reveal two central aspects
   of crises: first, they highlight the crucial role of civil society and
   migrants' participation in small communities with limited resources, and
   second, they illustrate how dynamic conflicts arise not only among but
   also within actor groups. These insights highlight that a stronger focus
   on local contexts can enhance our understanding of complex patterns in
   municipal integration governance.},
