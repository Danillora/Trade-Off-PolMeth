@article{ WOS:001327179100012,
Author = {Juarez, Jose Maria Luque},
Title = {Human security and its implementation in Armed Forces operation: a
   documentary analysis},
Journal = {REVISTA CIENTIFICA GENERAL JOSE MARIA CORDOVA},
Year = {2024},
Volume = {22},
Number = {45},
Month = {JAN-MAR},
Abstract = {Through a documentary analysis, this article explores the definition of
   human security and its implementation in Armed Forces management. It
   seeks to answer how to protect freedoms and rights encompassed in human
   security by the institutions of the social State under the rule of law.
   Through a documentary review, it is shown that this is not exclusive
   responsibility of a single institution but requires an articulated work
   by all the actors committed to these freedoms, based on a comprehensive
   state policy. It is concluded that the debate is based on how to achieve
   the human security through the military performance without overlooking
   its constitutional mission of protection. Therefore, it is necessary to
   align human development and the Armed Forces capabilities without
   compromising their constitutional mandate.},
