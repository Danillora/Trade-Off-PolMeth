@article{ WOS:001294920000005,
Author = {Sanchez, Julio Cesar Martinez},
Title = {Survival strategies and neighborhood cohesion facing COVID 19 in
   Nezahualcoyotl and the historical center},
Journal = {REVISTA CIENTIFICA GENERAL JOSE MARIA CORDOVA},
Year = {2024},
Volume = {22},
Number = {46},
Month = {APR-JUN},
Abstract = {The pandemic in 2020 addressed different challenges for Mexican
   households, especially those where some members had the disease. This
   article analyzes the survival strategies developed by households and
   communities living in the historic center of Mexico City and the
   municipality of Nezahualcoyotl. Three variables were established to
   analyze said strategies: social cohesion, neighbor intervention in
   collective actions, and community participation. Using data collected
   from the survey ``Citizen Empowerment to Improve Public Security and
   Rule of Law in Mexico City{''} and econometric models, it was evident
   that both communities were outlined for their cohesion and neighborhood
   participation. In Nezahualcoyotl, family and neighborhood relations were
   essential, whereas in the historic center, solidarity among neighbors
   prevailed.},
