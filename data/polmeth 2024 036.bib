@article{ WOS:001415915700003,
Author = {Villalba-Garcia, Luisa Fernanda and Melo-Rodrigues, Luz Martha},
Title = {Peru's defense economy: Future analysis from civil-military relations},
Journal = {REVISTA CIENTIFICA GENERAL JOSE MARIA CORDOVA},
Year = {2024},
Volume = {22},
Number = {47},
Month = {JUL-SEP},
Abstract = {This article analyses the interaction between civil-military relations
   and defense economics in Peru through a comprehensive approach that
   includes a documentary and theoretical review and consultations with
   experts from the Peruvian Army. The White Paper on Defence and budgetary
   data are examined to understand resource management in the defense
   sector. Critical concepts of defense economics and civil-military
   relations are also reviewed to understand how they come together in this
   context. Through the Delphi method, expert perspectives are collected,
   identifying future scenarios based on strategic sectors, the percentage
   of GDP allocated to defense, recognized threats, budgetary investment,
   and strategic alliances. These results offer a strategic and
   anticipatory vision contributing to academic knowledge and political
   practice in Peruvian defense.},
