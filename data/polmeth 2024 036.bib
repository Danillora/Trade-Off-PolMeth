@article{ WOS:001415915700009,
Author = {Bonilla, Claudia Milena Pico and Barrera, Maria Gabriela Ramos and
   Mateus, Edwin Oswaldo Gil},
Title = {Relationship between economic performance and institutional design in
   Latin America: A model based on system dynamics},
Journal = {REVISTA CIENTIFICA GENERAL JOSE MARIA CORDOVA},
Year = {2024},
Volume = {22},
Number = {47},
Month = {JUL-SEP},
Abstract = {Latin American countries have improved their positions in international
   prosperity rankings. However, the region faces challenges in its
   institutional designs, understood as the rules of the game: the high
   incidence of corruption and the existence of organized crime networks
   can threaten the prosperity of the countries in the region. This paper
   uses the Legatum Institute's Prosperity Index to develop a model of the
   performance of Latin American economies in terms of the inclusive
   society and open economy dimensions of said index. In addition, it
   simulates scenarios of state intervention in pillars such as security
   and governability. To do so, it uses system dynamics. The results reveal
   that prosperity is positively associated with institutional stability
   and that the implementation of state efficiency and security policies
   positively impacts the performance of economies.},
Publisher = {ESCUELA MILITAR CADETES GENERAL JOSE MARIA CORDOVA},
