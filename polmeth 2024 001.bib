@article{ WOS:001207964400001,
Author = {Gulczynski, Michal and Czesnik, Mikolaj},
Title = {Wasting public money to buy turnout: quasi-experimental evidence from
   the polish battle for fire trucks},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2024},
Month = {2024 APR 24},
Abstract = {It is well known that incumbents enjoy advantages in elections. Among
   others, they may use public resources to increase their electoral
   chances by mobilizing specific social groups to vote. In the 2020
   presidential elections, the Polish government organised a competition:
   small municipalities with the highest turnout could win a fire truck. We
   exploit the threshold of 20,000 that arbitrarily divided municipalities
   into competing and non-competing for this public good and apply the
   quasi-experimental Regression Discontinuity Design. The competition
   aimed to increase the turnout in districts supporting the incumbent
   president. However, we find no evidence of any significant impact of the
   competition on the turnout or support for the incumbent around the
   threshold. We discuss why similar practices are common despite this
   result.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
