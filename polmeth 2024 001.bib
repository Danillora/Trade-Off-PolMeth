@article{ WOS:001419605400007,
Author = {Peychaux, Maria Ines Fernandez},
Title = {The foundational triad: Human rights, populism and democracy in
   Argentina (2003-2007)},
Journal = {REVISTA DE ESTUDIOS POLITICOS},
Year = {2024},
Number = {205},
Pages = {249-281},
Month = {JUL-SEP},
Abstract = {The goal of this research is to describe the discursive logics in the
   field of human rights in the first Kirchnerism. Through a qualitative
   review of its discursive corpus, the research shows the construction of
   a new cultural matrix through the recovery of the judicial route, but
   from a populist political logic that in ideological terms leaves aside
   the institutional perspective to assume the identity of the leader and
   his political project as the only axis of legitimacy. The results
   highlight the importance of the role and figure of N \& eacute;stor
   Kirchner in reinstalling memory policies in the Argentine public space
   since 2003, although without underestimating the necessary reflection on
