@article{ WOS:001352475300001,
Author = {Woods, Dwayne},
Title = {The Illusion of Power: Unpacking the Vulnerabilities of Big Tech in the
   Face of Sovereign States},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Year = {2025},
Volume = {10},
Number = {2},
Pages = {149-177},
Month = {JUN},
Abstract = {This article challenges the notion that Big Tech firms wield global
   power equivalent to sovereign states. Using a nested modeling approach
   that integrates game theory and algorithmic simulations, the study
   demonstrates that despite their technological dominance and market
   influence, Big Tech companies operate reactively within constraints
   imposed by nation-states. The research employs a Stackelberg sequential
   game model to analyze the strategic interactions between Big Tech and
   sovereign states, revealing how states leverage regulatory power to
   shape the digital landscape. A stochastic model of heterogeneous
   regulatory regimes further illustrates the unpredictability and
   diversity of state responses. The study extends its analysis to the
   international level using a two-tier model that combines macro-level
   power structures with micro-level strategic interactions. Findings
   indicate that while Big Tech firms may have first-mover advantages in
   innovation, states ultimately hold decisive power through regulatory
   frameworks and opportunistic behaviors. The article argues that the
   expansion of Big Tech has paradoxically enhanced states' regulatory
   reach and power in the international system. This research provides
   empirical insights into the complex power dynamics between sovereign
   states and global tech entities, challenging prevailing perceptions of
   Big Tech's autonomy and influence.},
