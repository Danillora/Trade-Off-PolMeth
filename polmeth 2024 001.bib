@article{ WOS:001447477800001,
Author = {Ball, Sarah},
Title = {Using interpretive political science to understand policy-making in
   practice},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2024},
Volume = {59},
Number = {4, SI},
Pages = {411-423},
Month = {OCT 1},
Abstract = {Policy-making involves a complex, often hidden, process of negotiation
   and mediation between different actors. This paper draws on Bevir and
   Rhodes' interpretive political science (IPS) to illuminate how the
   different and sometimes conflicting beliefs and practices of these
   actors shape this process, specifically focusing on the field of
   behavioural policy. Examining the Australian government's adoption of
   behavioural policy, this paper explores how the concept has been adapted
   to the local context, reflecting three key traditions: academic,
   bureaucratic, and citizen-centred. These traditions played a crucial
   role in shaping how behavioural policy was understood and applied,
   ultimately limiting its impact on policy reform. Using ethnographic
   research of Australia's Behavioural Economics Team, this study reveals
   how behavioural policy is not merely transferred but translated into
   locally contextualised assemblages. The findings demonstrate IPS is a
   valuable tool for understanding public sector reform, where ideas that
   align with prevailing traditions are most likely to succeed.},
