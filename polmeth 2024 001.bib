@article{ WOS:001369590600001,
Author = {Mombelli, Sara},
Title = {Defense Burden Sharing and Military Cooperation in the EU27: A
   Descriptive Analysis (2002-2023)},
Journal = {PEACE ECONOMICS PEACE SCIENCE AND PUBLIC POLICY},
