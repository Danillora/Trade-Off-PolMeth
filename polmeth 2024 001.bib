@article{ WOS:001201291100001,
Author = {Moulton, Jeremy F. G.},
Title = {Developing a Critical Understanding of Environmental Activism through
   Active Learning},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2024},
Volume = {57},
Number = {4},
Pages = {578-582},
Month = {OCT},
Abstract = {This article presents a short simulation active-learning exercise that
   can be used in political science undergraduate and postgraduate
   environmental politics courses to introduce and teach the subject of
   environmental activism. The exercise, which asks students to role play
   as determined environmental activists, draws on an analytical framework
   from Diani and Donati (1999) that provides a typology of nonpartisan
   political organizations. This ``learning through typology{''} allows
   students to prioritize critical analysis while engaging with the subject
   matter in a creative and enjoyable way. The article provides the full
   process of the exercise, from the grounding in the framework and
   provision of contextual examples, to the running of the simulation, and
   finally to the presentation of students' work and a group debriefing
   session. This exercise therefore adds to the growing use of simulations
   and active learning in the increasingly prominent field of environmental
   politics.},
