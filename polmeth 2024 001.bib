@article{ WOS:001308451900001,
Author = {Krewson, Christopher N. and Schroedel, Jean Reith},
Title = {Going beyond Dobbs: an exploration of support for court reform},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2025},
Volume = {13},
Number = {3},
Pages = {704-726},
Month = {MAY 27},
Abstract = {Gender explains variation in public opinion across domains,
   institutions, and time. In public opinion studies focused on the
   judiciary, women are less likely than men to support the Supreme Court.
   There exists little extant research, however, on whether Court decisions
   themselves shape the views of men and women differently. We test for
   gender's effects on a timely and important outcome - support for
   concrete Court reform. Further, we test how exposure to recent decisions
   impacting women to varying degrees (abortion rights, gun safety, and
   religious freedom) influences support for Court reform. We find broad
   public support for many Court reforms. In contrast to studies focused on
   Court legitimacy, the gender gap in support for Court reform exists
   among Republicans but not among Democrats. Further, our experimental
   data demonstrates that while the gender salience of a case does not
   directly affect support for Court reform, there are significant indirect
   effects. In particular, the Dobbs decision eliminating the right to an
   abortion received relatively low levels of decision support among survey
   respondents, and low decision support led to more support for Court
   reform among the respondents. These mediated effects were stronger for
   women than they were for men.},
