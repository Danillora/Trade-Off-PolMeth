@article{ WOS:001454601600002,
Author = {Ortega, Odette Virginia Delfin and Mujica, Olivio Lopez},
Title = {Democratic Legitimacy in Mexico in the Period 2018-2021. A Proposal for
   a Synthetic Index},
Journal = {CIMEXUS},
Year = {2024},
Volume = {19},
Number = {2},
Pages = {82-100},
Month = {JUL-DEC},
Abstract = {This research focuses on studying the democratic legitimacy of the
   Congress of the Union of Mexico, both in the Chamber of Deputies and the
   Senate. To achieve this, a democratic legitimacy index was constructed
   based on four components: public opinion, professionalization,
   legislative experience, and legislative productivity during the period
   2018-2021. The results obtained indicate that, although the majority of
   legislators scored a value in the legitimacy index equal to or greater
   than 1, 42\% of legislators did not demonstrate legitimacy. It is
   crucial to establish strategies to ensure that legislators elected by
   the citizens align their actions with the expectations and needs of
   society. In this way, democracy in Mexico can be strengthened and
   preserved.},
Publisher = {UNIV MICHOACANA SAN NICOLAS HIDALGO, INST INVESTIGACIONES HISTORICAS},
