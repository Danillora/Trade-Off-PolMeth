@article{ WOS:001390891400006,
Author = {Cordonier, Laurent},
Title = {SOCIAL BELIEFS AND PERCEPTIONS RELATED TO VACCINE REFUSAL IN FRANCE},
Journal = {TOCQUEVILLE REVIEW},
Year = {2024},
Volume = {XLV},
Number = {2},
Month = {DEC 1},
Abstract = {Based on data from a survey carried out in 2023 among a representative
   panel of the French population (N = 4000 respondents), in this article I
   explore the factors associated with having refused the Covid-19 vaccine,
   on the one hand, and having ever refused a vaccine(s) other than
   Covid-19 for oneself and/or one's children, on the other. Surprisingly,
   there was little correlation between these two vaccine refusal
   behaviours. While people who have adopted one or other of these
   behaviors share many characteristics, refusal of the Covid-19 vaccine in
   particular seems to be based on an attitude of distrust towards
   government and the media, backed by a conspiracy view of how the social
   world works. In comparison, refusal to take the vaccine against Covid-19
   is more likely to reflect a complex relationship with health, combining
   a strong interest in the subject with a high degree of openness to
   alternative therapies and esoteric beliefs, as well as negative medical
   experiences.},
