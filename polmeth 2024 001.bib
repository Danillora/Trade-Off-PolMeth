@article{ WOS:001470339300003,
Author = {Molina-Garcia, Hector},
Title = {Analysis of the signal broadcasts of the Spanish Congress of Deputies in
   general television news. An approach to the evolution of political
   affection in Spain in the first quarter of the 21st century. From the
   new politics to the new polarized populism.},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2024},
Volume = {12},
Pages = {63-100},
Month = {JUL-DEC},
Abstract = {The concept of political affection and its evolution is very present in
   the recent history of Spain; in the last 25 years, citizens' approach to
   political issues has varied substantially due to numerous factors. This
   evolution has continued to this day, the political debate, increasingly
   polarized and media- driven, generates reactions in citizens in
   accordance with the messages transmitted. This research work tries to
   identify the main keys that demonstrate the evolution of the interest
   and affection of Spanish citizens on politics and parliamentary
   information in the last 24 years in Spanish society. To this end, the
   content of the midday and evening news programs of the main general
   television networks has been analyzed during two very specific periods:
   in 2009 and 2023, trying to identify the quantity and quality of this
   information to offer an evolution of the condition. policy in the study
   framework.},
