@article{ WOS:001394203700001,
Author = {Stefanelli, Alberto and Abts, Koen and Meuleman, Bart},
Title = {Freedom for all? Populism and the instrumental support of freedom of
   speech},
Journal = {WEST EUROPEAN POLITICS},
Year = {2025},
Volume = {48},
Number = {7},
Pages = {1629-1652},
Month = {NOV 10},
Abstract = {Populists often cast themselves as unwavering defenders of free speech.
   In practice, however, they tend to apply free speech instrumentally,
   denying it to those who hold opposing political views. While existing
   research has primarily focused on populist individuals' general
   commitment to democracy, little direct evidence exists on how they
   concretely understand and apply freedom of speech. Using an original
   survey question and a survey-embedded experiment, this article examines
   how support for freedom of speech among populist individuals oscillates
   between normative principles and instrumental considerations. The
   findings indicate that those with stronger populist attitudes tend to
   support free speech in normative terms but are simultaneously inclined
   to restrict it when their ideological interests are at stake. This
   suggests that populists view democratic norms not as universal rights
   but as instrumental values that can be traded for political gain.},
