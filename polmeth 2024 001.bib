@article{ WOS:001225272600001,
Author = {Zhao, Yang},
Title = {Dressing masculinities: muted colours, aesthetic citizenship and
   nation-building in Tashkent, Uzbekistan},
Journal = {CITIZENSHIP STUDIES},
Year = {2024},
Volume = {28},
