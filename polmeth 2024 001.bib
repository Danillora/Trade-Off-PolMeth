@article{ WOS:001477709000017,
Author = {Arnaiz, Borja Nino},
Title = {SHAM MARRIAGES, CITIZENSHIP MARKETS, AND IMMIGRATION RESISTANCE},
Journal = {REVISTA INTERNACIONAL DE PENSAMIENTO POLITICO},
Year = {2024},
Number = {19},
Abstract = {The morality of sham marriage has been largely overlooked in the ethics
   of immigration. The aim of this article is to fill this gap by defending
   sham marriages both as a sort of private citizenship market and a form
   of immigration resistance. Sham marriages are pro tanto morally
   permissible because they are a free and mutually beneficial exchange and
   do not undermine the institution and value of marriage. Moreover, this
   pro tanto permissibility is not defeated by the infringement of other
   rights or outweighed by competing interests and considerations. The
   first part assumes that states have a right to exclude unwanted
   immigrants, but holds that such a right is limited by the right of
   citizens to marry foreign partners. Sham marriages constitute an
   informal citizenship market, and as such they are immune to the standard
   objections raised against their formal counterparts. The second part
   rejects that states have a right to exclude, arguing that foreigners may
   enter into sham marriages with citizens to bypass unjust immigration
   restrictions. In this case, the monetary exchange that takes place in
   sham marriages is not problematic, for citizens have no antecedent duty
   to marry unauthorized migrants.},
