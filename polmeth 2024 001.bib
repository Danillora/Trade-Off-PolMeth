@article{ WOS:001180850200001,
Author = {Huijsmans, Twan and Rodden, Jonathan},
Title = {The Great Global Divider? A Comparison of Urban-Rural Partisan
   Polarization in Western Democracies},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2025},
Volume = {58},
Number = {2},
Pages = {261-290},
Month = {FEB},
Abstract = {This study is the first to measure urban-rural electoral divides in a
   way that facilitates comparisons beyond majoritarian democracies of the
   UK and North America. Based on national election results at the lowest
   available geographic level in fifteen countries covering roughly five
   decades, we present a measure for each election and political party,
   enabling comparisons over time and between countries with different
   electoral and party systems. We show that long-term increases in
   urban-rural divides have been most pronounced in the US, the UK, and
   Canada, but these divides have also emerged in several European
   multiparty systems in recent decades, largely because of growing smaller
   parties with predominantly urban or rural support. Overall urban-rural
   electoral divides remain lower in these systems due to continued
   presence of mainstream parties with geographically diverse support. Our
   contribution paves the way for a comparative research agenda on causes
   and consequences of urban-rural electoral polarization.},
