@article{ WOS:001436681700004,
Author = {Caruncho, Lucia},
Title = {The far right in the farsouth: the leadership of JavierMilei and
   JairBolsonaro in Argentina and Brazil},
Journal = {TEMAS Y DEBATES},
Year = {2024},
