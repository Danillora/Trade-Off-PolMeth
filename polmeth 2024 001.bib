@article{ WOS:001214779400001,
Author = {Maheo, Valerie-Anne and Belanger, Eric},
Title = {Nationalist currents put to the test of generations: analyzes of
   attitudes and voting choices of Quebecers from 2007 to 2022},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2024},
Volume = {57},
Number = {2},
Pages = {364-386},
Month = {JUN},
Abstract = {The existence and persistence of nationalist movements among substate
   nations can have various explanations, one of which is related to
   generations - how they were socialized to politics in distinct societal
   contexts, and how older generations are replaced by younger ones. Hence,
   to better understand the evolution of nationalism in Quebec, this study
   draws on the last six Quebec Election Studies (2007-2022) and uses an
   Age-Period-Cohort model to examine the relationship between generational
   groups and various indicators of nationalism. The results reveal that
   there is indeed a generational story to tell about nationalism in
   Quebec. The generation of the baby boomers is particularly distinct from
   other generations, in terms of its attachment to Quebec, its support for
   Quebec's independence, and its support of the Parti Qu \& eacute;b \&
   eacute;cois; whereas the Millennials are more supportive of Qu \&
   eacute;bec Solidaire and GenXers are more likely to support the
   Coalition Avenir Qu \& eacute;bec. There are currently distinct
   ``nationalist generations{''} that cohabitate in Quebec.},
