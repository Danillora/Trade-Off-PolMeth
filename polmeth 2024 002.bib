@article{ WOS:001137234500001,
Author = {Schaffer, Johan Karlsson and Langford, Malcolm and Madsen, Mikael Rask},
Title = {An Unlikely Rights Revolution: Legal Mobilization in Scandinavia Since
   the 1970s},
