@article{ WOS:001356565500017,
Author = {Reyes, Manuel Aragon},
Title = {Biographical sketch of Francisco Rubio Llorente},
Journal = {TEORIA Y REALIDAD CONSTITUCIONAL},
Year = {2024},
Volume = {54},
Abstract = {Francisco Rubio Llorente was not only a theoretical constitutionalist,
   but also a practical one. As General Secretary of The Congress of
