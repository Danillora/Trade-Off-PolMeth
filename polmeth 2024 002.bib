@article{ WOS:001202150700001,
Author = {Choulis, Ioannis and Escriba-Folch, Abel and Mehrl, Marius},
Title = {Preventing Dissent: Secret Police and Protests in Dictatorships},
Journal = {JOURNAL OF POLITICS},
Year = {2024},
Volume = {86},
Number = {3},
Pages = {1104-1109},
Month = {JUL 1},
Abstract = {This article examines the impact of secret police organizations on the
   occurrence of antiregime protests in authoritarian regimes. We argue
   that such organizations are related to lower levels of protests via two
   related mechanisms: intelligence gathering and an increased perception
   of risk among citizens, which reduce citizens' ability and willingness
   to mobilize, respectively. Using new data on secret police organizations
   in dictatorships covering the post-World War II period, our findings
   support the main expectation. This research contributes to our
   understanding of security institutions, antiregime protests, and the
   repression-dissent nexus.},
