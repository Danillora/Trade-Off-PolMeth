@article{ WOS:001385080600001,
Author = {Gherghina, Sergiu and Tap, Paul},
Title = {Selling their vote expensively: The effects of information, apathy and
   age},
Journal = {PARTY POLITICS},
Year = {2024},
Month = {2024 DEC 27},
Abstract = {People's attitudes towards vote buying have often been studied through
   the lenses of its acceptability as a norm in society or the drivers
   behind the decision to accept money if offered. However, we know little
   about the financial value that voters attach to their vote. To fill this
   gap in the literature, this research note analyzes what makes people
   accept different sums of money in exchange for their votes. It uses a
   survey conducted in Romania after the 2020 legislative elections and
   shows that most respondents reject any sum of money. Among those willing
   to accept money, the voters who have limited information, who rarely
   participate politically and those who are young are inclined to ask
   higher sums for their vote. Partisanship has mild effects, but poverty
   and trust in parties do not make a difference.},
