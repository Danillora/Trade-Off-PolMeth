@article{ WOS:001262256800003,
Author = {Fakier, Khayaat and Rathzel, Nora and Mulinari, Diana},
Title = {Opening Conversations with Marxist Feminists: A Response to the
   Symposium on Marxist-Feminist Theories and Struggles Today},
