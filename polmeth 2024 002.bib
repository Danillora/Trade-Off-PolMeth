@article{ WOS:001385289300001,
Author = {Ojewale, Oluwole and Balogun, Omolara and Sadiq, Mahmud Malami},
Title = {Silent Casualties of Armed Conflict: Violence Against Women and Girls by
   Bandits in Northwest Nigeria},
Journal = {AFRICAN SECURITY},
Year = {2024},
Volume = {17},
Number = {3-4},
Pages = {277-303},
Month = {OCT 1},
Abstract = {This study analyzes the dynamics of violence against women and girls
   (VAWG) by bandits in northwest Nigeria through qualitative and
   quantitative research methodology. The study notes women and girls are
   disproportionately targeted for various reasons: diminishing presence of
   the state in maintaining order, and the propensity of the bandits to
   punish or exploit the communities through the instrument of coercion.
   The paper concludes that efforts to eliminate, neutralize and disrupt
   (END) VAWG in the context of banditry in northwest Nigeria must embrace
   four strategies the peacebuilding approach, security-sector reform,
   development, social support and strategic health care delivery to
   victims.},
