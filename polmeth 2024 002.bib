@article{ WOS:001321614400001,
Author = {Iqtidar, Humeira},
Title = {Justice Beyond Rights: Haqq and Global South Migration},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2025},
Volume = {119},
Number = {3},
Pages = {1159-1172},
Month = {AUG},
Abstract = {How might the ideas and practices of Global South migrants and activists
   inform normative frameworks and conceptual repertoires that political
   theorists and activists bring to bear on questions regarding justice and
   migration? Taking seriously the call for conceptual innovation to move
   beyond Eurocentrism this article builds on oral histories collected from
   refugees and migrants from the Tribal Areas of Pakistan to argue that
   the concept of haqq raises important questions about the reliance on
   statist justice in contemporary theorizing about migration ethics and
   provides insights into alternative ethical concerns. Ideas and practices
   of haqq foreground social relationships as well as the imbrication of
   responsibility and entitlement for communities and individuals.
   Crucially, engagement with haqq helps dislodge assumptions regarding
   state-enforced rights as universal vehicles of justice. Consequently,
   different ethical questions and imaginaries become available for
   consideration that resonate much beyond debates about migration.},
