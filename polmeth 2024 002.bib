@article{ WOS:001390891400004,
Author = {Bottemanne, Hugo},
Title = {COMPUTATIONAL NEUROSCIENCE OF BELIEF},
Journal = {TOCQUEVILLE REVIEW},
Year = {2024},
Volume = {XLV},
Number = {2},
Month = {DEC 1},
Abstract = {Bayesian brain theory, a computational approach based on predictive
   processing (PP) principles, proposes a mechanistic formulation of belief
   generation and updating. This theory assumes that the brain encodes a
   generative model of its environment, consisting of probabilistic beliefs
   organized into networks, from which it generates predictions about
   future sensory inputs. As the brain interacts with the environment,
   prediction errors representing the difference between predictions and
   sensory signals would be used to update predictions, contributing to the
   remodeling of belief networks. In this article, we provide an
   introduction to the fundamental principles of computational neuroscience
   of belief, and we show how this dynamic of prediction and updating could
   be at the origin of belief generation.},
