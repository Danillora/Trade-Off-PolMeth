@article{ WOS:001383052400001,
Author = {Girardi, Elisabetta and Palma, Nicola},
Title = {Mainstream convergence and challengers' success: a comparative analysis
   of Southern and Northwestern Europe},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2024},
Month = {2024 DEC 25},
Abstract = {Over the past two decades, new and peripheral parties from both the left
   and right of the ideological spectrum challenged the virtual oligopoly
   of social democratic and liberal conservative parties in Western Europe.
   Within the extensive body of literature that investigates the drivers of
   these parties' electoral successes, recent studies have shown that the
   convergence of mainstream parties on the left-right scale has played a
   crucial role. In this article, we build on these contributions to
   investigate whether cross-regional differences exist in the size of this
   convergence and in its impact on challengers' success, focusing on two
   key policy issues for challenger parties, namely redistribution and
   immigration. Results from multilevel regression models on nine European
   democracies reveal that convergence is significantly more marked in
   Southern as compared to Northwestern Europe. We also find that
   mainstream convergence on redistributive issues positively affects
   challengers' success in Southern Europe only. Conversely, mainstream
   convergence on immigration-related issues negatively impacts
   challengers' success in both regions.},
