@article{ WOS:001205873700001,
Author = {Gosselin-Tapp, Jerome and Jean, Frederique and Mossu, Sacha-Emmanuel and
   Bedard, Leonard},
Title = {Beyond the obligation to exploit the territory: Self-determination of
   local communities and energy transition in Quebec},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2024},
Volume = {57},
Number = {3},
Pages = {532-552},
Month = {SEP},
Abstract = {This article analyzes the Quebec context that is currently marked by a
   proliferation of mining titles for critical minerals. We examine the
   thesis according to which we have a moral obligation to exploit the land
   based on an energy transition. According to this idea, which is promoted
   by the mining industry and Quebec provincial government, local community
   opposition to mining projects is a case of ``NIMBY.{''} In response to
   this accusation, this article offers a reflection on the relationship
   between political communities and territory based on the work of
   Margaret Moore, Cara Nine, and insights from Green Legal Theory. The
   article concludes that local communities have prima facie right not to
   exploit critical minerals on their territory.},
