@article{ WOS:001378872600006,
Author = {Pichler, Davorin},
Title = {CIVIL LAW ASPECTS OF GUN CONTROL POLICY},
Journal = {POLITICKA MISAO-CROATIAN POLITICAL SCIENCE REVIEW},
Year = {2024},
Volume = {61},
Number = {3},
Pages = {90-116},
Month = {OCT},
Abstract = {Gun control policy is formed by regulations that impose direct
   restrictions on various forms of gun possession. A weapon, in the sense
   of real law, represents a thing on which a certain person has the right
   of ownership, which gives its bearer conceptually unlimited private
   legal authority over that thing. The restrictions that the legal order
   imposes on the owner of the weapon, in the exercise of his ownership
   rights, must be proportionate to the nature of the need for the
   restriction in each individual case. The paper will highlight the basic
   forms of control over the possession and disposal of weapons in
   different legal systems and their justification for political and social
   reasons. The paper will analyze the civil law aspect of limiting the
   ownership rights of gun owners by the legislative, administrative and
   judicial authorities in the Republic of Croatia.},
