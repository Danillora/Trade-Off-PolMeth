@article{ WOS:001387246300001,
Author = {Cadieux, Hubert and Ouellet, Catherine and Vincent, Sarah-Jane and
   Gilbert, Jeremy and Pelletier, Camille and Dufresne, Yannick},
Title = {Electoral dynamics in Quebec: analyzing the rise of the Parti Québécois
   through the lens of potential for growth},
Journal = {FRENCH POLITICS},
Year = {2025},
Volume = {23},
Number = {1},
Pages = {84-103},
Month = {MAR},
Abstract = {In the complex landscape of multiparty electoral systems, accurately
   capturing shifts in voter preferences poses significant challenges.
   Traditional polling methods, which rely heavily on basic vote intention
   metrics, often fail to predict substantial changes in voter support. A
   notable example is the recent resurgence of the Parti Qu \& eacute;b \&
   eacute;cois (PQ) in Quebec, which reached levels of support unseen since
   1988. This resurgence, alongside other unexpected electoral outcomes,
   underscores the limitations of conventional forecasting methods. By
   examining the Quebec political scene post-2022 election, this research
   note assesses the effectiveness of the Relative Confidence Index (RCI),
   an innovative yet underutilized metric, in quantifying parties'
   potential for growth. Findings suggest that the RCI could have provided
   early insight into the PQ's unexpected resurgence. More broadly, the
   study argues that the RCI addresses a critical gap in traditional vote
   choice measurement by offering detailed insights into voter likelihoods
   and party preferences, thereby fulfilling the need for more accurate and
   informative measures in multiparty systems.},
