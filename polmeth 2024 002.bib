@article{ WOS:001249951000001,
Author = {Steflja, Izabela},
Title = {Professional Women as Perpetrators: A Conversation with Valérie,
   ``Rwanda's Voice of Death{''}},
Journal = {JOURNAL OF GENOCIDE RESEARCH},
