@article{ WOS:001423993300004,
Author = {Zalesskaia, Olga},
Title = {Chinese Entrepreneurship and Russian-Chinese Border Development in the
   Context of Global Regionalism},
Journal = {CHINA QUARTERLY OF INTERNATIONAL STRATEGIC STUDIES},
