@article{ WOS:001267867000010,
Author = {Olsson, Giovanni and Salles, Eduardo Baldissera Carvalho},
Title = {Human right to what development? An archeology to pluridimensional
   sustainable development building},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2024},
Volume = {128},
Month = {JAN-JUN},
Abstract = {This research aims to analyze the evolution of the concept of
   sustainable development as a human right from its declaration in 1986 to
   its institutionalization in the United Nations 2030 Agenda through an
   archaeological reconstruction. The study examines the affirmation of
   development as a human right in various sources of international law and
   recovers the most significant moments of the emergence of the new
   paradigm of development in the late 20th century. The methodological
   approach is qualitative, using the inductive method. The research
   technique adopted is bibliographic, with an analysis of documentary
   sources. The results indicate a marked recent alteration of the concept
   of development, now termed multidimensional sustainable development,
   already incorporated into United Nations international instruments. The
   research concludes that the United Nations 2030 Agenda represents an
   ambitious civilizational project that incorporates this new formulation
   of development, transitioning across various areas of knowledge, and
   expanding the foundations of this human right. The contributions of this
   research include a thorough analysis of the evolution of the concept of
   sustainable development as a human right and its implications for the
   fields of international law and development.},
