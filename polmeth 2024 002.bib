@article{ WOS:001218885400001,
Author = {Ghosh, Sujay and Kumar, Yukteshwar},
Title = {Transparency laws, corruption and the quality of government: comparing
   India and China},
Journal = {ASIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2024},
Volume = {32},
Number = {1},
Pages = {77-97},
Month = {JAN 2},
Abstract = {Transparency laws, such as the Right to Information (RTI) in India and
   Open Government Information (OGI) in China, seek to confront corruption.
   They have been able to expand people's space against corruption and
   other abuses of power, thus demanding accountability from the
   powerholders. Instead of particular regimes like democratic or
   authoritarian, we employ the Quality of Government (QoG) as a context to
   understand corruption control in India and China. Yet, due to various
   socio-political limitations, QoG remains inadequate in India; and uneven
   in China these cripple the promises of transparency laws. Hence, it is
   necessary that India prioritizes human development, which shall require
   better QoG and China addresses its weaker areas in QoG.},
