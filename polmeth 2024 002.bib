@article{ WOS:001197255900001,
Author = {Asimovic, Nejla and Ditlmann, Ruth K. and Samii, Cyrus},
Title = {Estimating the effect of intergroup contact over years: evidence from a
   youth program in Israel},
Journal = {POLITICAL SCIENCE RESEARCH AND METHODS},
Year = {2024},
Volume = {12},
Number = {3},
Pages = {475-493},
Month = {JUL},
Abstract = {We study how an intervention combining youth intergroup contact and
   sports affects intergroup relations in the context of an active
   conflict. We first conduct a randomized controlled trial (RCT) of
   one-year program exposure in Israel. To track effects of a multiyear
   exposure, we then use machine-learning techniques to fuse the RCT with
   the observational data gathered on multiyear participants. This
   analytical approach can help overcome frequent limitations of RCTs, such
   as modest sample sizes and short observation periods. Our evidence
   cannot affirm a one-year effect on outgroup regard and ingroup
   regulation, although we estimate benefits of multiyear exposure among
   Jewish-Israeli youth, particularly boys. We discuss implications for
   interventions in contexts of active conflict and group status asymmetry.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {EDINBURGH BLDG, SHAFTESBURY RD, CB2 8RU CAMBRIDGE, ENGLAND},
