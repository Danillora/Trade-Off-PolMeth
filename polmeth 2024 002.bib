@article{ WOS:001302482300005,
Author = {Cash, Jordan T.},
Title = {The Constitutionally Illogical Whig Presidency},
Journal = {STUDIES IN AMERICAN POLITICAL DEVELOPMENT},
Year = {2024},
Volume = {38},
Number = {1},
Pages = {70-83},
Month = {APR},
Abstract = {This article examines the Whig Party's conception of the presidency and
   argues that it failed to take hold because the idea of an executive
   which is subordinate or deferential to the legislative branch is
   fundamentally at odds with the Constitution. To show this, I assess the
   four presidents from the Whig Party: William Henry Harrison, John Tyler,
   Zachary Taylor, and Millard Fillmore. Each of these presidents entered
   office supporting the Whigs' ideological vision of legislative supremacy
   and weak executive power, yet quickly abandoned that vision once in
   office. I contend that this demonstrates the constitutional logic of the
   presidency and the way it shapes the officeholder's perspective,
   orienting presidents toward a robust understanding of executive power.
   In short, presidents do not act like Whigs because the Constitution
   directs them not to.},
