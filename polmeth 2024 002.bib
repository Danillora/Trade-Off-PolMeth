@article{ WOS:001281133800001,
Author = {Antos, Marek and Brunclik, Milos and Hajek, Lukas and Kindlova, Miluse
   and Kubat, Michal and Vincze, Attila},
Title = {Constitutional Conventions in Central Europe: Insights from Experts},
Journal = {PROBLEMS OF POST-COMMUNISM},
