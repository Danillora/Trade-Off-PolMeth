@article{ WOS:001192297100001,
Author = {Chaisty, Paul and Power, Timothy J.},
Title = {Gamson going global? Cabinet proportionality in comparative perspective},
Journal = {EUROPEAN POLITICAL SCIENCE REVIEW},
