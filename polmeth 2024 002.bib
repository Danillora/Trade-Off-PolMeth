@article{ WOS:001326664300001,
Author = {Jiang, Yongmu and He, Yuan},
Title = {The Process, Characteristics and Prospects of the Century-Long Fight
   against Poverty by the Communist Party of China},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2024},
Volume = {14},
Number = {3},
Pages = {339-360},
Month = {JUL 2},
Abstract = {Since its founding, the Communist Party of China (CPC) has maintained
   its original aspiration of eliminating poverty, improving people's
   living standards, and steadily achieving the common prosperity of the
   whole Chinese people. The Party has consistently led the Chinese people
   in fighting against poverty throughout all the stages of China's
   revolution, construction, reform and development, eventually winning a
   comprehensive victory in this fight. In the Party's work, poverty
   alleviation has always been one of the main tasks, and has included
   efforts both to help the poor acquire knowledge and to build their
   aspirations for change. Specifically, rural areas have been the key area
   of activity, and the Party's fundamental approach to combating poverty
   has always centered on development, employing an all-encompassing
   campaign that involves state, social and market forces. It should also
   be noted that the elimination of absolute poverty is not the end of the
   fight. The CPC will continue struggling tirelessly to create a better
   life for the people and to realize common prosperity, thus opening up a
   new phase in the fight against poverty in general.},
