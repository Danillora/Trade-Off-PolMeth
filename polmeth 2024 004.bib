@article{ WOS:001371948900001,
Author = {Eady, Gregory and Rasmussen, Anne},
Title = {Gendered Perceptions and the Costs of Political Toxicity: Experimental
   Evidence from Politicians and Citizens in Four Democracies},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2025},
Volume = {119},
Number = {3},
Pages = {1446-1462},
Month = {AUG},
Abstract = {Politicians frequently face toxic behaviors. We argue that these
   behaviors impose a double burden on women, who may not only face higher
   exposure to toxicity, but experience attacks that they and others
   understand to be motivated by prejudice and designed to push them out of
   office. Using large-scale image-based conjoint experiments in the United
   States, Denmark, Belgium, and Chile, we demonstrate that both
   politicians themselves and citizens regard messages targeting women
   politicians as more toxic than otherwise equivalent messages targeting
   men. This perception intensifies when messages mention gender or come
   from perpetrators who are men. A second experiment to investigate the
   mechanisms shows that hostile behaviors toward women are more frequently
   understood as driven by prejudice and attempts to remove women from
   politics. These findings highlight the importance of understanding how
   perceptions of perpetrators' motives affect the severity of political
   toxicity, and provide insights into the gendered effects of political
   hostility.},
