@article{ WOS:001264703500001,
Author = {Macrae, Callum Zavos},
Title = {Socialism and non-domination: a relational egalitarian approach},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2024},
Month = {2024 JUL 10},
Abstract = {In recent literature on the philosophical foundations of socialism a
   growing number of theorists have endorsed the claim that freedom as
   non-domination is a fundamental normative commitment undergirding
   socialist politics. On this sort of view, a broad range of traditional
   socialist claims can be explained and justified by reference to freedom
   as non-domination. In this paper, I argue that even if these theorists
   are right that opposition to domination is a core socialist normative
   commitment, it is not clear that that opposition is best understood in
   terms of a commitment to a particular conception of freedom. Instead,
   socialist opposition to domination can be more fruitfully understood as
   flowing from a commitment to a relational conception of equality,
   according to which relationships with a substantially egalitarian
   character have a special value. I argue, first, that a relational
   egalitarian construal of socialist opposition to domination can retain
   all of the benefits of the freedom as non-domination construal, and,
   secondly, that a relational egalitarian account will have benefits that
   a freedom as non-domination approach will lack. I conclude that there
   are still significant benefits to retaining a place for equality in our
   considered account of the normative foundations of socialism.},
