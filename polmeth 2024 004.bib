@article{ WOS:001477709000010,
Author = {Butierrez, Luis Fernando},
Title = {POLICIES OF THE IMPOSSIBLE. THE ONTOLOGY OF COMMUNITY AND NEIGHBOR IN
   SLAVOJ ZIZEK},
Journal = {REVISTA INTERNACIONAL DE PENSAMIENTO POLITICO},
Year = {2024},
Number = {19},
Abstract = {In the following work we will analyze the political-social
   considerations in Slavoj zizek > s elaborations, from his specific
   appropriation of Lacan's psychoanalytic perspective. In particular, we
   will seek to account for the political ontology with which he develops
   his critiques of identity politics, multiculturalist discourses and
   recent socio-political movements. In this framework, we will distinguish
   their understandings of the subject and the neighbor, in the respective
   considerations about the forms of relationship with the other. In this
   way we propose to develop a critical analysis of the bases with which he
   articulates his political proposals for emancipation.},
