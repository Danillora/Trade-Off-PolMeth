@article{ WOS:001416849100003,
Author = {Jara, German Andres Aristizabal and Cortes, Sergio Bedoya},
Title = {Contingency in the Heart of Necessity: Hegel's and Deleuze's Ontologies},
Journal = {ANACRONISMO E IRRUPCION},
Year = {2024},
Volume = {14},
Number = {27},
Pages = {63-84},
Month = {NOV-APR},
Abstract = {The present text seeks, counter to prevailing trends, to explore an
   approximation between the philosophies of Hegel and Deleuze. The core of
   this connection can be found in their respective ontologies, developed
   in The Science of Logic and Difference and Repetition, respectively. The
   aim is to rearticulate the relationship between contingency and
   necessity, especially in Hegel. This is due to the fact that hegemonic
   readings of this author excessively focus on necessity, relegating
   contingency to a secondary role. A new reading of the contingent nature
   of necessity in Hegel could bring this author closer to the French
   philosopher who so often criticized the German idealist.},
Publisher = {UNIV BUENOS AIRES},
