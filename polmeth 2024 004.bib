@article{ WOS:001078943700001,
Author = {Kasamatsu, Satoshi and Kishishita, Daiki},
Title = {Endogenous Political Trust and Electoral Accountability},
Journal = {JOURNAL OF POLITICS},
Year = {2024},
Volume = {86},
Number = {1},
Pages = {358-363},
Month = {JAN 1},
Abstract = {This study examines how an endogenous change in political trust affects
   the performance of a representative democracy. To this end, we construct
   a two-period political agency model in which voters face uncertainty
   about the distribution of politicians' types (trust uncertainty) as well
   as each individual politician's type. Such trust uncertainty allows
   political trust to endogenously change over time. In its presence, the
   incumbent's action has a spillover effect on the challenger's value
   through changes in political trust. We show that because of this
   mechanism, trust uncertainty weakens electoral accountability.
   Furthermore, it generates multiple self-fulfilling equilibria: a
   high-accountability and a low-accountability equilibrium coexist. In
   countries briefly experiencing democracy, trust uncertainty is severe.
   Our results indicate that democratic performance tends to be low and
   even similar countries can experience different performance levels in
   such new democracies.},
Publisher = {UNIV CHICAGO PRESS},
Address = {1427 E 60TH ST, CHICAGO, IL 60637-2954 USA},
