@article{ WOS:001215084800001,
Author = {Whittlestone, Jonas A. H. and Klitgaard, Michael B.},
Title = {Interest group resources, access, and influence: An empirical review},
Journal = {SCANDINAVIAN POLITICAL STUDIES},
Year = {2024},
Volume = {47},
Number = {3},
Pages = {308-333},
Month = {SEP},
Abstract = {Do informational and financial resources systematically secure interest
   group access and influence on public policy, and is one of these two
   resources politically more valuable than the other? Answering these
   questions is vital to understanding some of the drivers of political
   inequality and skewed influence associated with contemporary democratic
   politics. Focusing on two of the most commonly mobilized types of
   resources-money and information-this article reviews 60 studies on the
   topic. First, a qualitative synthesis shows that informational resources
   tend to be at least partially positively related to access and
   influence, while financial resources are more ambiguously associated
   with both outcomes. Second, a series of meta-regressions support this
   conclusion since they tend toward showing that informational resources
   are significantly stronger associated with these outcomes than financial
   resources. Thus, these results paint a more nuanced picture than the
   literature suggesting that moneyed interests subvert democratic
   politics.},
