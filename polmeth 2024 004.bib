@article{ WOS:001267271600013,
Author = {Tribino, Lucia Cobos and Lopez, Erika Pareja and Sanchez-Moral, Simon
   and Mohino, Inmaculada and Crespo, Laura Laosa},
Title = {The Development of Social Innovation Initiatives in the Context of the
   Pandemic: The Case of the Aluche Support Network in the City of Madrid},
Journal = {GEOPOLITICAS-REVISTA DE ESTUDIOS SOBRE ESPACIO Y PODER},
Year = {2024},
Volume = {15},
Number = {1},
Pages = {223-247},
Abstract = {The article seeks to study the social innovation initiatives that
   responded to the crisis generated by the COVID-19 pandemic. Based on the
   conceptualization of social innovation, the elements that allow an
   initiative to be identified as socially innovative are identified. The
   initiatives are characterized, their spatiality is studied in the
   context of the city of Madrid and the conditions that facilitated their
   development, considering social innovation as a phenomenon dependent on
   the trajectory of the territories where it is located. Through the case
   study of the associative phenomenon in the Aluche neighborhood (Latina
   District), the capacity for social transformation of the Aluche Support
   Network initiative and the explanatory factors of its emergence and
   development are explored, with special attention to the issue of citizen
   organization.},
