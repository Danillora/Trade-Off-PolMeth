@article{ WOS:001338213500001,
Author = {Yang, Jinhai},
Title = {The Contribution and Historical Significance of Xi Jinping's Statements
   on Poverty Alleviation},
Journal = {INTERNATIONAL CRITICAL THOUGHT},
Year = {2024},
Volume = {14},
Number = {3},
Pages = {323-338},
Month = {JUL 2},
Abstract = {Xi Jinping's statements on poverty alleviation form an important
   component of Xi Jinping Thought on Socialism with Chinese
   Characteristics for a New Era. These statements embody a series of
   systematic concepts regarding poverty alleviation, constructed around a
   scientifically rigorous theoretical system which, while drawing
   extensively on the lessons of past Chinese practice, also possesses
   distinct 21st-century characteristics. At the same time, the statements
   condense contemporary China's extremely rich recent experience of
   fulfilling the tasks of poverty alleviation. These concepts and
   theoretical achievements, along with the basic experience they
   encapsulate, reveal the scientific laws of poverty alleviation, and have
   greatly enriched and developed Marxist anti-poverty theory. As the
   latest theoretical advances in the field of poverty alleviation, and as
   the theoretical expression and enhancement of the path to poverty
   alleviation with Chinese characteristics, their success may be
   attributed to socialism with Chinese characteristics. Consequently, Xi
   Jinping's statements on poverty alleviation, along with the practical
   achievements related to them, have momentous status and possess
   far-reaching significance for the history of the development of the
   Chinese nation, the history of the development of world socialism, and
   the history of human anti-poverty efforts.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Yang, JH (Corresponding Author), Tsinghua Univ, Sch Marxism, Beijing, Peoples R China.
   Yang, Jinhai, Tsinghua Univ, Sch Marxism, Beijing, Peoples R China.},
