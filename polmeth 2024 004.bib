@article{ WOS:001288162900001,
Author = {Torrent, Juan Carlos Rodriguez and Callegari, Rodrigo Vargas},
Title = {Political anthropology of south-south migration: residence and
   citizenship in neoliberal Chile at the beginning of the XXI century},
Journal = {IZQUIERDAS},
Year = {2024},
Volume = {53},
Pages = {24},
Month = {JUL 1},
Abstract = {The South-South migration process defines the continent's own genetics
   associated with deep systemic inequality, as well as regional and global
   policies of inclusion and exclusion typical of the neoliberal State. The
   context corresponds to a historical development scheme of concentration
   of economic and political opportunities at the national level, in which
   imbalances are accentuated that modify the relationships between groups,
   economic sectors and regions. This places migrant subjects in tense
   dynamics in the field of culture, democracy, interrelationships,
   recognition and governability, in the face of the idea of the nation and
   institutional capacities. With a structural approach and with an
   emphasis on political anthropology in which power relations are
   expressed in the sphere of everyday life, this article critically
   reflects on the difficulties of migratory projects and construction of
   citizenship in current Chile, within a transversal problematization that
   goes beyond what is strictly migratory. It is methodologically supported
   by a set of bibliographic references that go beyond what is strictly
   migratory and interviews carried out in recent years, collecting
   expressions of subjectivity that confirm the difficulties of being a
   migrant within the framework of neoliberal democracy.},
Publisher = {ARIADNA EDICIONES \& SAINT PETERSBURG STATE UNIV},
