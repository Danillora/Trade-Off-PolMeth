@article{ WOS:001331513400001,
Author = {Kim, Taeyoung and Park, Jeongwan and Lee, Julak},
Title = {An Empirical Study of Factors Influencing Drone Terrorist Attack
   Casualties},
Journal = {INTERNATIONAL JOURNAL OF CYBER WARFARE AND TERRORISM},
Year = {2024},
Volume = {14},
Number = {1},
Abstract = {Recognizing the pivotal role of casualties as a metric for assessing the
   success of terrorist acts, this study endeavors to unravel the
   underlying factors shaping the casualty count in drone terrorist attacks
   across the globe. By applying Zero Negative Binomial Regression Analysis
   on data sourced from the Global Terrorism Database, we discern
   significant factors influencing the number of terrorist casualties. Our
   findings reveal the paramount importance of the target facility's
   significance, the attack type, the utilization of remote-controlled
   drones, the deployment of suicide drones equipped with explosive
   payloads, and the audacious strategy of swarming drone attacks.},
