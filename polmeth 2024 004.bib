@article{ WOS:001317517400001,
Author = {Arnold, William},
Title = {Liberatory or predatory? Gendered perspectives on the role of women in
   the Weather Underground},
Journal = {BEHAVIORAL SCIENCES OF TERRORISM AND POLITICAL AGGRESSION},
Year = {2025},
Volume = {17},
Number = {2, SI},
Pages = {227-241},
Month = {APR 3},
Abstract = {The Weather Underground is a group that has divided opinion for over
   half a century. To some, they were legitimate anti-war activists, to
   others they were one of America's most notorious domestic terror groups.
   Much of the secondary literature describing the group has highlighted
   the role of women in the organisation, differentiating them from the
   typical male-dominated structures of radical and extremist groups. Using
   comparative autobiographical analysis, this project explores the extent
   to which this assessment of gender equality is accurate by evaluating
   how members of the group described their experiences in their own words.
   The analysis identifies gendered discrepancies in the portrayal of women
   in the movement. Where men were likely to have a positive recollection
   of the gender equality within the Weather Underground, the accounts of
   women tread a complex line between acknowledging both the liberatory
   elements and the sexism within the movement. This research helps to
   demonstrate the value of first-hand accounts rather than secondary
   literature in trying to understand the internal dynamics of a movement.
   It also highlights the need for nuance when discussing the role of women
   in extremism movements.},
