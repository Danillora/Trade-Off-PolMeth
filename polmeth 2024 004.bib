@article{ WOS:001385161200001,
Author = {Jennings, Will and Furlong, Jamie and Stoker, Gerry and Mckay, Lawrence},
Title = {Fragmented and Dealigned: The 2024 British General Election and the Rise
   of Place-Based Politics},
