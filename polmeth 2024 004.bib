@article{ WOS:001419600200009,
Author = {Maleshevich, A. V.},
Title = {BALKANIZATION VS EUROPEANIZATION: CONCEPTUAL DECONSTRUCTION},
Journal = {POLIS-POLITICHESKIYE ISSLEDOVANIYA},
