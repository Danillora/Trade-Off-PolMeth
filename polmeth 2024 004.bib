@article{ WOS:001470339300011,
Author = {Lima, Martonio Mont'Alverne Barreto and Goncalves, italo Reis},
Title = {The blurry judicialization of politics in redemocratized Brazil: the
   judgment of Habeas Corpus 152.752/PR and Brazilian Supreme Federal
   Court's smoke cloak},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2024},
Volume = {12},
Month = {JUL-DEC},
Abstract = {The increasing interference of the judiciary in moral and political
   controversies, usually known as judicialization of politics, is a
   phenomenon of global scope. In redemocratized Brazil, the
   neoconstitutionalism promoted an inflation of the role played by judges
   and tribunals, who began not only to act as protectors of and
   subordinates to the 1988 Federal Constitution - seeking to consolidate
   the idea of welfare state - but also as main solvers of nuclear
   political dilemmas, which shape the basic structures of Brazilian
   democracy. An example of this phenomenon was the judgment of Lula's
   Habeas Corpus 152.752/PR by Brazilian Supreme Federal Court, which, on
   the eve of the 2018 presidential elections, decided that the former
   president > s arrest was constitutional before his criminal sentence
   became final, confirmed only in the second jurisdictional level. Thus,
   this paper, through bibliographic research, documental analysis and case
   studies, based on an analytical, descriptive and evaluative methodology,
   seeks, primarily, to develop the idea of blurred judicialization of
   politics, as well as to demonstrate some of the main reasons why this
   phenomenon represents a relevant danger to Brazilian democracy. Finally,
   it is concluded that the Supreme Federal Court, when judging HC
   152.752/PR, decided a nuclear political issue under the false argument
   of deciding a mere conflict between fundamental rights, thus covering
   itself with a smoke cloak that makes it practically intangible to any
   type of criticism or funccional limitation.},
