@article{ WOS:001141952900001,
Author = {Forester, Summer and Jett, Janel and Holtey, Bjorn},
Title = {From ``Lab Rats{''} to ``Mad Scientists{''}: Cultivating Agentic
   Learning Through Student-Led Simulation Redesign},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2024},
Volume = {57},
Number = {3},
Pages = {440-446},
Month = {JUL},
Abstract = {Simulations are increasingly prominent in classrooms, and research shows
   that such activities provide an array of benefits. In general,
   simulations tend to situate students as ``lab rats{''} (Asal 2005) who
   get to discuss the experiment. We call for bringing students into a
   simulation not as lab rats but instead as ``mad scientists.{''} We
   specifically propose using a simulation's debriefing step as an
   opportunity for students to imagine how they might revise the simulation
   that they experienced and to defend their revisions using concepts from
   class. We argue that such student-led revisions cultivate agentic
   engagement, a form of engagement in which students recognize themselves
   as co-constructors of knowledge with and for their peers. To test this
   notion, we modified the ``Isle of Ted{''} with an expanded
   post-simulation debriefing. We found that our approach cultivated
   student engagement and buy-in, required students to think of concepts
   dynamically, and was relatively simple to implement. Our study
   contributes to the literature on simulations as effective and engaging
   classroom tools, and it also emphasizes the potential for simulations to
   spark agentic learning.},
