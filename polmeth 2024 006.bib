@article{ WOS:001229836300001,
Author = {Cahill, Christine and McCabe, Katherine},
Title = {Context Matters: Understanding Student Usage, Skills, and Attitudes
   Toward AI to Inform Classroom Policies},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2024},
Volume = {57},
Number = {4},
Pages = {594-601},
Month = {OCT},
Abstract = {With the growing prevalence of AI tools, such as ChatGPT, political
   science instructors are navigating how to manage the use and misuse of
   AI in the classroom. This study underscores the prevalence of AI in
   academic settings and suggests pedagogical practices to integrate AI in
   the classroom in ways that are informed by students' current
   interactions with and attitudes toward AI. Using a survey of
   undergraduate students in political science courses, the study finds
   that ChatGPT usage is widespread at the university level and that
   students are not confident in their skills for using AI appropriately to
   improve their writing or prepare for exams. These findings point to key
   areas where instructors can intervene and integrate AI in ways that
   enhance student learning, reduce potential achievement gaps that may
   emerge due to differences in AI usage across student backgrounds, and
   help students develop critical AI literacy skills to prepare for careers
   that increasingly are affected by AI.},
