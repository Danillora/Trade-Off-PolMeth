@article{ WOS:001279347700001,
Author = {Olimpieva, Irina Meyer},
Title = {Silent Dissent: Exploring Russian Civic Activism as a Form of Opposition
   to the War in Ukraine},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2025},
Volume = {72},
Number = {4},
Pages = {369-377},
Month = {JUL 4},
Abstract = {This paper examines how Russian citizens use nonpolitical civic
   activities to dissent against the war in Ukraine, based on James Scott's
   idea of silent resistance. By studying a Telegram group helping
   Ukrainian war refugees, it explores the motivations and reasons for
   individuals' involvement. The paper shows that loyal civic activism can
   be a form of opposition when protest or emigration is not possible. It
   highlights new trends in Russian civil society's anti-war initiatives
   since the war began, emphasizing the rise of grassroots non-formalized
   activism amid increasing political repression and the decline of
   democracy under Putin.},
