@article{ WOS:001423993300002,
Author = {Shichor, Yitzhak},
Title = {Prophecy: Ben-Gurion and the Ascendency of China},
Journal = {CHINA QUARTERLY OF INTERNATIONAL STRATEGIC STUDIES},
