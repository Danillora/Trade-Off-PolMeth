@article{ WOS:001347016300001,
Author = {Broadbent, Jeffrey},
Title = {Power and theory: toward a multidimensional explanation of the dynamic
   political field},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2024},
Volume = {17},
Number = {3},
Pages = {223-252},
Month = {SEP 1},
Abstract = {What theories explain a dynamic political field and its outcome? Can we
   judiciously combine theories into a better hybrid explanation? To be
   valid, a theory's claimed causal factor must manifest in the field as a
   representative mode of micro power used between actors in dyads (its
   mechanism). The modes' relative presence indicates the theories'
   relative causal validity. The Integrated Structurational Analysis method
   analyzes (hundreds of) actor dyads to find evidence on the relative
   presence of the modes. The results indicate a shifting, multidimensional
   causal timescape driving the field and its outcome. Findings from a
   Japanese case study illustrate the method.},
