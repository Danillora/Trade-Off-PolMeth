@article{ WOS:001534566000001,
Author = {Muneer, Sania and Aryal, Saroj Kumar},
Title = {Institutional Instrumentalization and Democratic Backsliding in India
   under the Rule of the Bharatiya Janata Party (BJP)},
Journal = {ROMANIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2024},
