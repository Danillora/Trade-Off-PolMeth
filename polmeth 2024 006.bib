@article{ WOS:001390891400013,
Author = {Eyraud, Fiona and Casini, Annalisa and Klein, Olivier},
Title = {BELIEFS ABOUT DOMESTIC VIOLENCE: FROM SEXIST STEREOTYPES TO LESBIAN
   UTOPIA},
Journal = {TOCQUEVILLE REVIEW},
Year = {2024},
Volume = {XLV},
Number = {2},
Month = {DEC 1},
Abstract = {We explore collective beliefs and gender stereotypes surrounding
   domestic violence. These stereotypes structure our perception of the
   social world while serving to justify and maintain power hierarchies.
   For example, the invisibility of violence in lesbian relationships fuels
   the idea that only heterosexual relationships can be violent, thus
   reinforcing the vision of exclusively male violence and depicting women
   as essentially passive. However, although violence between women exists,
   it is part of a different analysis of power relations on a structural
   level. Some minority groups could exploit these stereotypes for
   strategic purposes to enhance their social identity: lesbians, faced
   with homophobia, can represent their relationships as more egalitarian,
   which helps to counter negative stereotypes and stigmatization. This
   strategy can positively influence social perceptions, but it also risks
   reinforcing the binary conception of gender roles and minimizing real
   violence in lesbian couples. The difficulty therefore lies in using
   stereotypes to counter oppression while avoiding reinforcing the same
   beliefs and making urgent problems invisible.},
