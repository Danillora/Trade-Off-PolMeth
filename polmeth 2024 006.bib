@article{ WOS:001379128300001,
Author = {Edgell, Amanda B. and Lachapelle, Jean and Maerz, Seraphine F.},
Title = {Achieving Transparency, Traceability, and Readability with Human-Coded
   Data},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2025},
Volume = {58},
Number = {2, SI},
Pages = {346-351},
Month = {APR},
Abstract = {Many important questions in political science require the use of
   human-coded data or information that has been systematically ordered and
   quantified by a human being from qualitative sources. This article
   discusses challenges and recent innovations in collecting and
   documenting human-coded data. We review five datasets produced within
   the past 10 years and also reflect on our experiences in collecting a
   quarterly dataset that tracked state responses to the COVID-19 pandemic.
   We argue that scholars can deliberately produce and publish
   theoretically grounded human-coded data in an accessible format that
   promotes transparency, traceability, and readability. We highlight
   several ways that scholars are already doing this, including narratives,
   source lists, and coding justifications that enhance the quality of
   their human-coded datasets. We also discuss common issues during coding
   and how technological innovation through interactive web-based platforms
   can improve the documentation of coding decisions.},
