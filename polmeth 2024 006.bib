@article{ WOS:001249339600003,
Author = {Moreno, Rodolfo Lopez},
Title = {Social Movements, Political Parties, and the Institutional Continuity of
   the Chilean Social Outburst in the Constitutional Convention},
Journal = {DESAFIOS},
Year = {2024},
