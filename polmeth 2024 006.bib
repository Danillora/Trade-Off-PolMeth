@article{ WOS:001267867000018,
Author = {Coura, Alexandre de Castro and Chai, Cassius Guimaraes and Surlo, Gerlis
   Prata},
Title = {The Social State crisis and the Coronavirus Pandemic: a comparative
   analysis between the labor measures adopted during the pandemic in
   Brazil, Portugal and Spain},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2024},
Volume = {128},
Month = {JAN-JUN},
Abstract = {The Coronavirus international public health emergency, changes in labor
   contracts stemming from the digital revolution and the practice of labor
   of activities through digital platforms have had direct impacts on labor
   relationships. In this paper the situation/problem in discussion aims to
   analyze whether the measures to combat the Coronavirus pandemic were
   efficient in protecting workers amidst the Social State crisis, based on
   a comparative approach between Brazil, Portugal and Spain. The research
   adopts the dialectical method from the social contradictions between the
   guarantees for protection of the working class and the preservation of
   the capitalist mode of production in the aforementioned countries. The
   hypothesis speaks on the crisis of the Social Welfare State and its
   contribution to aggravate the consequences of the pandemic. The
   resumption of the prevalence of social values of work, eradication of
   poverty, marginalization and reduction of social and regional
   inequalities are reflective sources of the capacity for state action and
   constitutional normativity in times of crisis.},
