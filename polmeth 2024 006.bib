@article{ WOS:001207989200001,
Author = {Debusscher, Petra and Maes, Eva Luna},
Title = {The European Union-Intersectionality Framework: Unpacking
   Intersectionality in the `Union of Equality' Agenda},
Journal = {POLITICAL STUDIES REVIEW},
