@article{ WOS:001279344000001,
Author = {Tappe Ortiz, Juliana and Arevalo, Julian},
Title = {No-campaigns in peace referendums},
Journal = {CONTEMPORARY POLITICS},
Year = {2025},
Volume = {31},
