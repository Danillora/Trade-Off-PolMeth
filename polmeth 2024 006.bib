Title = {The Effects of Personality Traits, Environmental Attitudes, and
   Demographic Factors on Green Party Support in Canada},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
