@article{ WOS:001362651800001,
Author = {Chaihanchanchai, Papaporn and Anantachart, Saravudh},
Title = {Beyond the Ballot: Personal Political Brand Characteristics and Voting
   Intention in Nonparty Political Contexts - Insights from Bangkok},
Journal = {JOURNAL OF POLITICAL MARKETING},
Year = {2024},
Month = {2024 NOV 6},
Abstract = {Understanding voters as consumers is a fundamental factor contributing
   to campaign success. This study aims to investigate whether personal
   political brand characteristics (PPBC) significantly influence voting
   intention (VI) and whether self-image congruence (SC) mediates this
   association within a nonparty context in a developing country, an area
   that has been relatively underexamined. Using a sample size of 465
   Bangkok voters, the study reveals that personal political brand
   characteristics have a positive and significant effect on self-image
   congruence, subsequently leading to higher voting intention. Notably,
   competence and charisma emerge as crucial characteristics in this
   context. The findings of this study make a valuable contribution to the
   field of political marketing and offer practical insights for
   politicians, particularly political leaders, to strategically build
   their personal brands.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
