@article{ WOS:001310572700001,
Author = {Dinsmore, Adam},
Title = {Redistribution vs. Recognition: How Brexit Split the Left},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
