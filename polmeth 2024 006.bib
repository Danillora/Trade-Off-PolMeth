@article{ WOS:001162692500001,
Author = {Stanley, Liam},
Title = {Why Sinoscepticism will Remake British Politics},
Journal = {POLITICAL QUARTERLY},
Year = {2024},
Volume = {95},
Number = {2},
Pages = {289-297},
Month = {APR},
Abstract = {Sinoscepticism is on the rise in British politics. Why is this? And what
   are the implications? This article shows that Sinoscepticism is not
   solely a result of the whims of MPs, but also stems from deep-rooted
   tensions surrounding Britain's position in global order. Specifically,
   the British state finds itself on the horns of a dilemma: accept the
   status quo of staying economically open to China, but face staunch
   criticism and reprisals both internationally and domestically; or become
   more hawkish in relations, but risk worsening Britain's imbalanced and
   underperforming economy. Considering the Conservative Party's
   longstanding commitment to both the `special relationship' with the US
   and to business and finance, this predicament is likely to provoke the
   party. The dilemma is seemingly about relations with one country, but,
   like Euroscepticism, it has the potential to become a stand-in for
   debates over what sort of country the UK should be.},
