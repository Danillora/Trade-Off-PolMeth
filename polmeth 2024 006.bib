@article{ WOS:001400134300011,
Author = {Bories, Raphael},
Title = {The Reds, the Whites and the Blacks. French ethnologist Monique Roussel
   de Fontanès and politics in 1950's Calabria},
Journal = {LABORATOIRE ITALIEN-POLITIQUE ET SOCIETE},
Year = {2024},
Number = {33},
Abstract = {When French ethnologist Monique Roussel de Fontan \& egrave;s from the
   Mus \& eacute;e de l'Homme decided to make Calabria her main field of
   investigation in the early 1950's, the Mezzogiorno was of great
   importance for Italian ethnography, but also for political and cultural
   debates in Italy. While many folklorists and ethnologists were more or
   less compromised by fascism, those who took part in the discipline's
   refoundation after the war, like Ernesto De Martino, often chose to make
   a commitment to socialism or communism. French ethnology and Monique
   Roussel de Fontan \& egrave;s took a different stance and tried to
   practise their discipline in an apolitical way. Along with her status as
   a foreigner in Italy, this choice allowed her to make contact with many
   different people, from the great Calabrian landowners to the communist
   activists. This allowed her to enrich her fieldwork, but also to make
   observations about the political situation, which she discussed not in
   her publications but in her personal letters. Her presence int the field
   reveals both the positions and the objectives of the various actors, and
   the epistemological and political stakes of ethnology.},
