@article{ WOS:001400134300012,
Author = {Tarascio, Giacomo},
Title = {I Fasci siciliani dei lavoratori nella riflessione gramsciana: storia e
   teoria},
Journal = {LABORATOIRE ITALIEN-POLITIQUE ET SOCIETE},
Year = {2024},
Number = {33},
Abstract = {The ``Fasci siciliani dei lavoratori{''} (1892-1894) represent one of
   the most innovative and radical social experiments of post-unification
   Mezzogiorno, whereby the experiences of the peasants'uprisings during
   the Risorgimento connect with new socialist political instances. The
   first Fascidei lavoratori consisted of agricultural workers, who were
   united by the same demands, particularly those concerning latifundiary
   farming systems. It wasn't by chance that the experience of the Sicilian
   Fasci comes up many times in Antonio Gramsci's analyses from his workon
   the Questione meridionale (Italy's Southern Question) to the concepts
   discussed in his Prison Notebooks and his opinions, as well as in his
   reflections on politics and the culture of Southern Italian society. The
   aim of the article is to explore the main historical and theoretical
   questions regarding the Sicilian Fasci, analysing from a
   historical-political perspective their organisation and demands, through
   which the subaltern social groups of the island found representation. At
   the same time there will be room for Gramsci's observations,
   highlighting a philological connection between the Sicilian events and
   some of the main concepts contained in the Notebooks concerning
   subalternity.},
