@article{ WOS:001352665000001,
Author = {Zhang, Xiaodong and Xiong, Yihan},
Title = {The Siphon Effect in Data-Driven Innovation of Government Hotlines?: A
   Comparative Study of Beijing and Shanghai},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Year = {2025},
Volume = {10},
Number = {3, SI},
Pages = {347-382},
Month = {SEP},
Abstract = {This study provides a comparative analysis of the 12345 public service
   hotlines in Beijing and Shanghai, the two largest cities in China.
   Although similar in size, these cities have implemented distinct
   data-driven innovation strategies. Beijing has taken a more ambitious
   approach, marked by substantial resource allocation and increased
   departmental capacity. This strategy delegates decision-making and
   responsibility to grassroots governments while introducing a performance
   ranking system to incentivize local officials. In contrast, Shanghai has
   adopted a more moderate approach, integrating the hotline into its
   broader data-driven governance framework, characterized by greater
   flexibility and potential applicability to smaller cities across China.
   These findings suggest that ambitious data-driven innovation initiatives
   may result in a siphon effect in the citizen request handling process,
   where government departments channel additional responsibilities into
   the hotline, increasing its complexity and workload. This effect may
   hinder the hotline's core function, especially for lower-level
   grassroots governments. Heightened top-down performance pressure could
   exacerbate this issue, potentially compromising the effectiveness of
   these services.},
