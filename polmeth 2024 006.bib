@article{ WOS:001424279800001,
Author = {Eshbaugh-Soha, Matthew},
Title = {Explaining Presidential Attention to Cabinet Nominees},
Journal = {CONGRESS \& THE PRESIDENCY-A JOURNAL OF CAPITAL STUDIES},
Year = {2025},
Volume = {52},
Number = {1},
Pages = {66-90},
Month = {JAN 2},
Abstract = {Do presidents go public on cabinet and cabinet-rank nominees? When
   presidents employ a strategy of politicization to appoint their cabinet,
   they may need to push senators to support these nominees through a
   public relations campaign. Thus, a political capital model, where
   presidents offer public support for their nominees to buffer Senate
   opposition, may explain the president's public attention to some
   nominees. Given that cabinet and cabinet-rank nominees are virtually
   assured confirmation, presidents may choose instead to use the
   nomination process to publicize representational characteristics of
   nominees, according to a representation model. To test these models, I
   have collected data on all public mentions of presidential appointments
   to cabinet and cabinet-rank positions from 1965 through 2023. Although
   these nominees enjoy a presumption of success, presidents still devote
   public attention to nominees about whom senators have raised objections
   and when their approval ratings are low. They also emphasize the
   representational quality of diverse nominees, especially for Black
   nominees.},
