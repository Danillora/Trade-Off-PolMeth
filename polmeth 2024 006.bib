@article{ WOS:001354724500001,
Author = {Karpa, David and Rochlitz, Michael},
Title = {Authoritarian Surveillance and Public Support for Digital Governance
   Solutions},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2025},
Volume = {58},
Number = {10},
Pages = {2237-2265},
Month = {SEP},
Abstract = {This study investigates factors influencing support for digital
   governance solutions and compares this support between autocracies and
   democracies. We conduct survey experiments in Russia, Germany, Turkey,
   the United States, and Estonia, and find that awareness of potential
   misuse of digital governance tools by the government reduces support.
   Importantly, while this effect has previously been documented for China,
   we find it irrespective of regime type for an autocracy, a hybrid regime
   and three democracies. Individuals relying on government-controlled
   information sources are more likely to endorse digital governance tools.
   Our study challenges prior findings by indicating that gaps in public
   service quality do not boost support. Instead, satisfaction with
   government services correlates with trust in the government's capacity
   to implement digital governance solutions.},
