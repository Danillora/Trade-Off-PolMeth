@article{ WOS:001308278300005,
Author = {Nikolskaya, M. V. and Nikulin, M. A. and Ralafiarindaza, M. N.},
Title = {AN INTEGRATED EAST AFRICA: A RESILIENCE PERSPECTIVE},
