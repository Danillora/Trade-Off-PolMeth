@article{ WOS:001387618700001,
Author = {Aly, Farah},
Title = {To boast or to ideologize? A utility-based approach to understanding
   authoritarian legitimation strategies},
Journal = {CONTEMPORARY POLITICS},
Year = {2025},
Volume = {31},
Number = {4},
Pages = {612-640},
Month = {AUG 8},
Abstract = {Increasing scholarly interest is directed towards the varieties and
   displays of authoritarian legitimation strategies. However, the
   strategic adaptation of these narratives by authoritarian leaders
   operating within the confines of the same regime remains a largely
   unexplored area. I propose that increasing external dependence prompts
   leaders to pivot from ideological to pragmatic legitimation strategies.
   These pragmatic strategies, being more adaptable and less doctrinally
   rigid, allow for adjustments in response to external pressures that
   might otherwise conflict with ideological commitments. Analysing panel
   data on legitimation strategies in authoritarian regimes from 1960 to
   2010, I provide empirical support for this mechanism. The present work
   adds to the growing body of knowledge on adaptation as a survival
   mechanism in authoritarian governance and draws attention to the
   relevance of rational-choice utility-based explanations in interpreting
   the transformation and development of authoritarian regimes.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
