@article{ WOS:001382737700001,
Author = {Praet, Jasper},
Title = {Bringing authoritarianism into the limelight: the implications for
   populist radical right ideology},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2024},
Month = {2024 DEC 23},
Abstract = {Nativism, authoritarianism, and populism are traditionally regarded as
   the defining ideological dimensions of populist radical right parties.
   However, authoritarianism in party discourse is under-theorized and it
   has received little empirical scrutiny compared to the other two
   dimensions. Building on the psychology literature on the concept, I
   provide an updated definition of authoritarianism that consists of three
   subdimensions: the identification of threat, authoritarian submission to
   authority and norms, and authoritarian aggression toward deviants.
   Afterwards, I relate authoritarianism's definition to populism, finding
   that authoritarianism shares strong similarities with populism. It
   appears that populism can be seen as a specific manifestation of
   authoritarianism.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
