@article{ WOS:001314860200001,
Author = {Strawbridge, Michael G. and Mohamed, Heather Silber and Lucas, Jennifer},
Title = {White Racial Resentment and Gender Attitudes: An Enduring Connection or
   an Artifact of the 2016 Election?},
