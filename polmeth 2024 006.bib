@article{ WOS:001185353000001,
Author = {Hossain, Delwar},
Title = {Secularization, De-secularization and Re-secularization in Bangladesh:
   Does the `Third Image' Matter?},
Journal = {STUDIES IN INDIAN POLITICS},
