@article{ WOS:001263250600001,
Author = {Melnick, R. Shep},
Title = {Inside the ``Administrative State{''}: The Enigmatic Office for Civil
   Rights},
Journal = {FORUM-A JOURNAL OF APPLIED RESEARCH IN CONTEMPORARY POLITICS},
Year = {2024},
