@article{ WOS:001375001200014,
Author = {Calomarde, Emiliano Andres},
Title = {Reader comments and right-wing 2.0. From the debate to the confrontation
   of readings on the Cambiemos government in Mar del Plata (2015-2019)},
Journal = {CIENCIA POLITICA},
