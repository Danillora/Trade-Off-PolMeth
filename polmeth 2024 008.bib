@article{ WOS:001223913100001,
Author = {Tegelberg, Matthew and Kunelius, Risto and Pohjonen, Matti},
Title = {Media and power in times of hegemonic crisis: exploring contentious
   climate politics on Twitter},
Journal = {JOURNAL OF POLITICAL POWER},
Year = {2024},
Volume = {17},
Number = {1},
Pages = {65-88},
Month = {JAN 2},
Abstract = {During moments of hegemonic crisis, it is crucial to understand the
   dynamics of media power. This article identifies key structural forces
   and disruptive tendencies that are factors in a current crisis of the
   neoliberal hegemonic order. This intellectual background underpins an
   empirical analysis of Twitter data collected during the UN climate
   summit in Glasgow, Scotland (COP26). We identify large attention
   clusters around established political actors and their counter-hegemonic
   challengers; concluding that during these intense global political
   events a hegemonic crisis produces spaces where systemically
   marginalized actors can harness relational attention power and mobilize
   it toward distinct political purposes.},
