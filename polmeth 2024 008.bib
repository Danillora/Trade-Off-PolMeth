@article{ WOS:001219769700001,
Author = {Reinikainen, Jouni},
Title = {The view of freedom that shaped the Swedish welfare state},
Journal = {SCANDINAVIAN POLITICAL STUDIES},
Year = {2024},
Volume = {47},
Number = {3},
Pages = {493-513},
Month = {SEP},
Abstract = {The paper contributes to the debate of the Swedish welfare state by
   re-examining the view of freedom underlying the design of this welfare
   model. The point of departure is two interpretations by Bo Rothstein and
   Henrik Berggren and Lars Tr \& auml;g \& aring;rdh, which both describe
   advancement of individual autonomy as the ultimate point of the model.
   The paper argues that these readings are overly liberal in the sense
   that they exaggerate the importance of individualism and autonomy. The
   view of freedom that shaped the Swedish welfare state was not liberal,
   and individual autonomy was not the overriding goal for the founders of
   the model. Instead, the view is best described as quasi-republican and
   nondomination based. It was mostly a result of semi-Marxist ideas about
   capitalist power and exploitation that lingered on in the ideology of
   the Swedish social democratic party, the SAP, in the 1930s and 1940s.
   During the first decades of the 1900s, the SAP gradually revised these
   ideas in a domination-based direction. It was the outcome of this
   process that more than anything else gave the impetus to the design of
   the Swedish welfare state.},
