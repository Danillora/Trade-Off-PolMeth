@article{ WOS:001226185700001,
Author = {Hall, Galen and Basseches, Joshua A. and Bromley-Trujillo, Rebecca and
   Culhane, Trevor},
Title = {CHORUS: A New Dataset of State Interest Group Policy Positions in the
   United States},
Journal = {STATE POLITICS \& POLICY QUARTERLY},
