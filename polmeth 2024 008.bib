@article{ WOS:001336812400001,
Author = {Chaudhry, Fatima Javaid},
Title = {India's Curious Strategic Obsession with China-Pakistan Economic
   Corridor (CPEC)},
Journal = {GLOBAL CHANGE PEACE \& SECURITY},
