@article{ WOS:001152116600001,
Author = {Mathews, Mason Clay and Fotheringham, A. Stewart},
Title = {The effect of place on voting behavior: The case of the Arizona
   proposition to legalize recreational marijuana},
Journal = {POLITICS \& POLICY},
Year = {2024},
Volume = {52},
Number = {1},
Pages = {196-226},
Month = {FEB},
Abstract = {In 2020, Arizonans approved Proposition 207, the Smart and Safe Arizona
   Act, which legalized recreational marijuana sales. Previous research has
   typically used non-spatial survey data to understand marijuana
   legalization voting patterns. However, voting behavior can, in part, be
   shaped by geographic context, or place, which is unaccounted for in
   aspatial survey data. We use multiscale geographically weighted
   regression to analyze how place shaped Proposition 207 voting behavior,
   independently of demographic variations across space. We find
   significant spatial variability in the sensitivity of voting for
   Proposition 207 to changes in several of the predictor variables of
   opposition and support for recreational marijuana legalization. We argue
   that local statistical modeling approaches provide a more in-depth
   understanding of ballot measure voting behavior than the current use of
   global models.},
