@article{ WOS:001267867000002,
Author = {Klein, Herbert S. and Luna, Francisco Vidal},
Title = {The population and household structure of a provincial capital: São
   Paulo, 1804 to 1829},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2024},
Volume = {128},
Pages = {15-70},
Month = {JAN-JUN},
Abstract = {: We examine the structure of the population of the city of S \&
   atilde;o Paulo in the early nineteenth century using digitalized
   household census records from 1804, 1817 and 1829. We examine the major
   socio-economic groups within the population, as well as the color, age,
   sex, marital status and occupations of the heads of households. The size
   of households, encompassing free individuals, slaves, and agregados is
   also studied. Our findings reveal a community dominated by a minority
   white elite with a majority non-white population. Its economy included a
   major textile artisanal industry with a significant participation of
   free persons of color, and contained a slave labor system typical of
   contemporary urban centers in Brazil.},
