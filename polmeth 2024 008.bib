@article{ WOS:001310743100001,
Author = {Bodjongo, Mathieu Juliot Mpabe},
Title = {Why is voter turnout in presidential elections increasing among people
   concerned about the risk of COVID-19 worsening in Africa?},
Journal = {POLITICS \& POLICY},
Year = {2024},
Volume = {52},
Number = {5},
Pages = {1101-1136},
Month = {OCT},
Abstract = {Este art \& iacute;culo tiene como objetivo explicar por qu \& eacute;
   aument \& oacute; la participaci \& oacute;n en las elecciones
   presidenciales entre las personas que tem \& iacute;an el riesgo de que
   el COVID-19 empeorara en \& Aacute;frica. El an \& aacute;lisis se basa
   en una muestra de 16.890 personas que viven en 34 pa \& iacute;ses
   africanos. Los resultados econom \& eacute;tricos sugieren que el miedo
   a la escalada de la pandemia puede reducir la disposici \& oacute;n de
   los ciudadanos a participar en las elecciones presidenciales. Sin
   embargo, este efecto negativo cambia seg \& uacute;n las percepciones
   sobre la gesti \& oacute;n gubernamental de la pandemia. Mostramos que
   la probabilidad de votar en una elecci \& oacute;n presidencial aumenta
   significativamente entre quienes temen que la pandemia empeore debido a:
   (i) una falta de confianza en las estad \& iacute;sticas gubernamentales
   sobre COVID-19; (ii) corrupci \& oacute;n percibida relacionada con la
   pandemia de COVID-19; (iii) falta de confianza en la capacidad del
   gobierno para garantizar la seguridad de las vacunas COVID-19; (iv) la
   percepci \& oacute;n de inversi \& oacute;n p \& uacute;blica
   insuficiente para combatir las emergencias sanitarias; y (v) percepci \&
   oacute;n de una intervenci \& oacute;n insuficiente por parte de las
   fuerzas del orden para hacer cumplir las medidas de control de COVID-19.},
