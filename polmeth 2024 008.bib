@article{ WOS:001267867000016,
Author = {Massau, Guilherme Camargo and Muraro, Pedro Olavo Satte Alam},
Title = {Cooperation between federated states and the contraction of debt in
   light of the solidarity principle: the case of Rio Grande do Sul},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2024},
Volume = {128},
Month = {JAN-JUN},
Abstract = {: The present article has as an objective to problematize the
   contractual provisions in conditions and demands of interest on the
   collection of debt from member states, specifically in the case of Rio
   Grande do Sul, to the Union stemming from the constitutional structure
   of Brazilian federalism. It is to be considered that federalism,
   specially in its cooperative form, does not admit the overlap from one
   federated state in relation to other on matters pertaining to the
   accomplishment of state obligations. Therefore, the financial loans from
   the Union to the States must, at most, contain monetary correction,
   being unreasonable the contractual prediction of remuneratory interest
   under penalty of weakening or nullifying the financial-administrative
   autonomy of the federated states. The methodology applied in the
   research was the hypothetical-deductive to frame that the
   norme-principle of the federation in its cooperative form must conduct
   the contractual norms in the financial loans. The resources used in the
   research were that of doctrine, case law and documents.},
