@article{ WOS:001352728700001,
Author = {Vasudeva-Barkdull, Feeza},
Title = {Articulating Lynching in India},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
Year = {2024},
Month = {2024 NOV 12},
Abstract = {Lynching became a popular part of the lexicon in India during the last
   decade, and the word is used to communicate incidents where a group of
   people takes justice into their own hands. Considering the violent
   nature of lynching, it became a topic of intense debate. Yet, there is a
   lack of definitional clarity, and the ambit of lynching has been
   stretched to signify various forms of violence. Even the modest number
   of scholarly research projects takes the term ``lynching{''} for
   granted. Considering the ambiguity surrounding it, this study analyzes
   and articulates the phenomena of lynchings in India, particularly during
   the period 2000-2021 while undertaking a genealogical study to
   illustrate the history, and the meaning of the word and how it has
   evolved over the years alongside the associated phenomena. To do so is
   to take away the misleading objectivity the word can sometimes generate.
   Following the conceptualization of lynchings and its changing expression
   in different societies, particularly the United States of America, the
   article highlights in what ways the definition and content of lynching
   have become more symbolically charged and the reasons for the change as
   well as what made lynching an extraordinary form of violence.},
