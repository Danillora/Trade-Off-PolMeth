@article{ WOS:001331861100001,
Author = {McAllister, Ian and Biddle, Nicholas},
Title = {Information and campaign effects in the 2023 Australian Voice referendum},
Journal = {AUSTRALIAN JOURNAL OF POLITICAL SCIENCE},
Year = {2024},
Volume = {59},
Number = {3},
Pages = {331-343},
Month = {JUL 2},
Abstract = {Referendums differ from general elections in the choices that are
   offered to voters and in the cues that are available to them to reach a
   decision. This difference makes the quantity and quality of the
   information that is available during a referendum campaign crucial to
   the outcome. This paper uses a unique panel survey to examine the role
   of the campaign in shaping the outcome of the October 2023 Voice to
   Parliament referendum. We find that the political cues offered to voters
   - reflected most prominently in voters' views about the major party
   leaders - were the most important campaign effects shaping the result.
   Interpersonal communication also mattered, but government-supplied
   information and mass media exposure had no effect. The findings
   highlight the central role that a campaign driven by elites plays in
   shaping the outcome of a referendum.},
