@article{ WOS:001267867000013,
Author = {Ferreira, Mariah Brochado},
Title = {The dawn of the Philosophy of Technology in Ernst Kapp and Gilbert
   Simondon for a critical outline of the current stage of technical
   experience},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2024},
Volume = {128},
Pages = {507-558},
Month = {JAN-JUN},
Abstract = {A new philosophical discipline called Philosophy of Technology has
   revolutionised philosophical reflection in this century, proposing a
   differentiated response by fulfilling the philosophical task of
   problematising the apparent obviousness experienced at every historical
   moment. And our challenge begins by trying to understand the meaning of
   a word that is by no means obvious: technology . Its indiscriminate use
   by common sense is an obvious fact that can be seen in graphs and
   spreadsheets, but what is meant by this phenomenon and the consequences
   that flow from it still require multiple scientific and social
   scrutinies. This essay aims to present a breviary of the origins of the
   Philosophy of Technology, which is initially constituted as the
   Philosophy of Technique , as explicitly inaugurated in the work of Ernst
   Kapp and refined in the work of Gilbert Simondon. These are two pillars
   in the formation of this new philosophical discipline, taken here as
   references in the 19th and 20th centuries respectively, which aims to
   offer new keys to understanding the current stage of the process of
   artificialization of human life. Based on these two philosophical
   programmes, we will reflect on the foundations and dimensions of t
   echnical experience in the constitution of human nature . This is a
   theme of indisputable relevance and it is explored here with the aim of
   understanding the state of the art of the human-machine interaction .
   This phenomenon challenges us to think about the serious ruptures
   already experienced at an unprecedented speed, demanding new critical
   horizons- theoretical and ethical- in order to face the so-called
   disruptive challenges that already frighten us and many that are yet to
   come.},
