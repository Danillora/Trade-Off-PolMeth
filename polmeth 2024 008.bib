@article{ WOS:001377400400013,
Author = {Gotz, Roland},
Title = {Indispensable for Now Fossil Fuels in Central Asia},
Journal = {OSTEUROPA},
Year = {2024},
Volume = {74},
Number = {8-10},
Pages = {233+},
Abstract = {While the EU sees the countries of Central Asia as potential suppliers
   of ``green{''} energy, they themselves rely on fossil fuels to secure
   their own energy supply. With the exception of Turkmenistan, they are
   seeking to expand coal-fired power generation. And all of them want to
   generate higher revenues from the export of oil, gas, and coal and are
   looking for new markets and transportation routes. Having lost its
   Western European market for pipeline gas, Russia is also looking at
   Central Asia and its neighbours for opportunities to sell its natural
   gas. Moscow could get in Turkmenistan's way, especially with a pipeline
   project through the Caspian Sea.},
