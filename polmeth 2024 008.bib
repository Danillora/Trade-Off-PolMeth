@article{ WOS:001230951700001,
Author = {Vasquez-Cortes, Mateo},
Title = {Peer Effects and Recidivism: Wartime Connections and Criminality among
   Colombian Ex-Combatants},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2025},
Volume = {119},
Number = {2},
Pages = {637-652},
Month = {MAY},
Abstract = {To what extent do peers affect criminal behavior? In this article, I
   study peer effects among ex-combatants in Colombia. Following a
   theoretical framework that differentiates the impact of economic
   conditions from that of social networks, I rely on individual-level data
   on over 16,000 former paramilitaries in Colombia to study the
   relationship between illegal gold production and recidivism. I show that
   when the economic benefits of illegal sectors increase, ex-combatants
   favor criminal activities. More importantly, I show that an increase in
   wartime peers' criminality increases an ex-combatant's criminal
   activity. I complement these results with the analysis of an original
   survey about the social connections of ex-combatants and explore the
   potential effect of tackling wartime networks as a policy to reduce
   crime after conflicts.},
