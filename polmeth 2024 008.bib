@article{ WOS:001269531000001,
Author = {Vilaca, Luiz Doria and Morucci, Marco and Paniagua, Victoria},
Title = {Antipolitical class bias in corruption sentencing},
Journal = {AMERICAN JOURNAL OF POLITICAL SCIENCE},
Year = {2025},
Volume = {69},
Number = {2},
Pages = {701-717},
Month = {APR},
Abstract = {Are corruption trials that involve the highest ranks in the public
   sphere and large private companies biased against some groups? Existing
   research predominantly focuses on corruption prosecutions of
   politicians, leaving unresolved the extent to which judges apply
   differential treatment when convicting and sentencing the political
   class compared to other defendants, including those in the private
   sector. To address this gap, we investigate judicial bias within
   Brazil's famous ``Opera \& ccedil;\& atilde;o Lava Jato,{''} the largest
   corruption investigation carried out in history. Leveraging an original
   database that traces the trajectory of the universe of the 3154 cases of
   Lava Jato, we show that judges' sentencing decisions were not governed
   by a partisan logic. Instead, judges were more inclined to impose longer
   prison times and higher fines to elected politicians when compared to
   all other defendants, particularly those from the private sector. We
   interpret these findings as evidence of antipolitical class bias.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Paniagua, V (Corresponding Author), London Sch Econ, Dept Int Relat, Houghton St, London WC2A 2AE, England.
