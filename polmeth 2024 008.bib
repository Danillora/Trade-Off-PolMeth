@article{ WOS:001320435000001,
Author = {Coll, Joseph A.},
Title = {Racial Stereotypes, Racial Threat, and Support for Felon
   Disenfranchisement Among White Americans},
Journal = {POLITICAL RESEARCH QUARTERLY},
