@article{ WOS:001340057800001,
Author = {Bondarenko, Oleksiy},
Title = {Center-Region Relations and Neo-Patrimonial Governance in Russia: A Tale
   of Two Regions},
Journal = {RUSSIAN POLITICS},
Year = {2024},
Volume = {9},
Number = {3},
Pages = {315-341},
Month = {SEP},
Abstract = {Center-region relations in Russia experienced several zigzags in the
   process of consolidation of the central control over the regions,
   influenced by a complex interplay between formal and informal
   institutions and practices. Through the lenses of neopatrimonialism, the
   study explores the challenges Russian governors face in the process of
   consolidation of their personal power at the regional level. l. Drawing
   on empirical research conducted in Sverdlovsk and Perm Krai, the study
   contends that, despite the establishment of the so-called `power
   vertical', the neo-patrimonial equilibrium in Russia is marked by
   multiple tensions. In this system, personal style and skills, the
   constellation of actors at the central and regional levels, and the
   horizontal interaction between networks play an important role, making
   center-region relations more dynamic and complex. Hence the hierarchical
   model of territorial control appears less stable and predictable in the
   long run.},
