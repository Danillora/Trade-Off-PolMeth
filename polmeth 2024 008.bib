@article{ WOS:001381088100001,
Author = {Markussen, Havard Rustad},
Title = {Liberty, Security, Digital: The Democratic Production of Illiberal
   Security Policy in the Case of the Norwegian Bulk Interception
   Controversy},
Journal = {DEMOCRACY \& SECURITY},
Year = {2025},
Volume = {21},
Number = {2},
Pages = {163-186},
Month = {APR 3},
Abstract = {This article examines how bulk interception regimes are legitimized in
   parliamentary proceedings, and more broadly, explores how illiberal
   security policy is produced in digitalized, democratic states. Focusing
   on the Norwegian case, the study finds that proponents of the bulk
   interception bill successfully legitimized its passing through four
   interrelated discursive strategies: establishing bulk interception as a
   liberty/security-problem, lowering the liberty-stakes, raising the
   security stakes and representing digitalization as a threat multiplier
   and as a threat in itself. The findings indicate that digital threats
   enable the combination of exceptional and diffuse securitization, which
   legitimizes the expedient processing and at the same time permanent
   validity of new intelligence legislation.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
