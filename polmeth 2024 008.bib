@article{ WOS:001273121700006,
Author = {Ellis, Richard J.},
Title = {Benjamin Urner, Utopian Socialism, and the Origins of Direct Legislation},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2024},
Volume = {13},
Number = {3},
Pages = {337-370},
Month = {JUN 1},
Abstract = {Historians have long identified Benjamin Urner as the first exponent of
   direct legislation in the United States, a claim that has been repeated
   without ever being carefully examined. In this article, I excavate
   Urner's career and political thought and show that while the
   oft-repeated account may give Urner too much credit as a direct
   democracy pioneer, it also neglects an important transatlantic socialist
   utopian strand in nineteenth-century radical political thought that
   shaped Urner's thought and played a critical role in the earliest
   efforts to promote direct democracy in both the United States and
   Europe. Direct legislation appealed to utopian socialists because it
   offered a way not only to enact desired economic change by combatting
   concentrated economic power but also to institutionalize the radical
   vision of democracy at the heart of utopian socialist thought.},
Publisher = {UNIV CHICAGO PRESS},
