@article{ WOS:001400134300006,
Author = {Guerbo, Maririta},
Title = {Micro-genesis of the organic ethnologist (1945-1950)},
Journal = {LABORATOIRE ITALIEN-POLITIQUE ET SOCIETE},
Year = {2024},
Number = {33},
Abstract = {This contribution proposes to add a new link to the historical
   reconstruction of ethnologist Ernesto De Martino's (1908-1965)
   involvement with the Socialist Party in Southern Italy, in order to
   highlight the issues at stake in his first reading of Gramsci. The
   reconstruction of the Party's context and cultural policy will focus on
   the critique of The World of Magic put forward by Raniero Panzieri
   (1921-1964), who would later become the theorist of workers' inquiry.
   His critique concerns a specific problem of Marxism, that of the
   articulation of theory and praxis, and points to a certain elitism
   inherent in De Martino's first masterpiece. For religious ethnology, the
   problem becomes that of the meaning of ethnographic practice, i.e., of
   an inquiry thought of as both a scientific and a political practice. In
   a second section, we will assess in greater detail Gramsci's influence
   on De Martino from two angles: the religious question as a blind spot in
   Marxism, and the formation of a reunified culture through ethnographic
   practice. In 1948, thanks to Gramsci, De Martino was finally able to
   complete his own critique of Benedetto Croce and put himself forward as
   the first incarnation of the organic intellectual.},
