@article{ WOS:001343088200001,
Author = {Gomez, Mario and Villa, Abraham David Villicana and Monge, Rodrigo gomez},
Title = {Impact of Foreign Direct Investment from the United States of America on
   Environmental Degradation in Mexico, 1990 - 2019},
Journal = {CIMEXUS},
