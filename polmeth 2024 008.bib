@article{ WOS:001355003400001,
Author = {Aniche, Ernest Toochi},
Title = {An old wine in a new bottle? Towards an alternative theoretical approach
   to AfCFTA's institutional autonomy and private-sector-driven
   cross-border business integration},
Journal = {POLITIKON},
Year = {2024},
Volume = {51},
Number = {1-2},
Pages = {40-58},
Month = {APR 2},
Abstract = {This article argues that unless the African Continental Free Trade Area
   (AfCFTA) departs from the old ways of doing things, free trade will not
   automatically enable Africa to tackle the various trade constraints
   hampering intra-African trade as a result of the enormous challenges
   that will confront it. To ensure that AfCFTA is not an `old wine in a
   new bottle,' there is a need for a paradigm shift to move from
   top-bottom to bottom-top, inter-governmental to inter-people,
   inter-state to people-to-people, state-centric to human-centred and
   public sector led to private sector-driven integration. The role of the
   state at the onset should be facilitative and complementary. This should
   serve as a recipe for massive cross-border business integration and
   autonomy of the continental free trade area. By relying solely on
   secondary data, this study is essentially a desktop study and,
   therefore, it is basically qualitative, analytical, theoretical and
   descriptive.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
