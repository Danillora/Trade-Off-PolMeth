@article{ WOS:001209852200005,
Author = {Whiteman-Charles, Nadia},
Title = {The Postcolonial Complexities of Black Taste in Jamaican Rock Music},
Journal = {NEW PROPOSALS-JOURNAL OF MARXISM AND INTERDISCIPLINARY INQUIRY},
Year = {2024},
Volume = {13},
Number = {1},
Pages = {7-16},
Abstract = {Reggae and dancehall represent popular taste in Jamaica. Black
   nationalist discourses advocate that authentic Jamaican music be tied to
   the experiences of poor Blacks and local resistances to Western
   imperialism. Unfortunately, the local rock music scene's affinity for
   `foreign' music styles kept the community on the fringes of the music
   industry. To secure distinction, rock musicians reimagined and imitated
   `ideal' Black tropes in conceptualizing lyrics and their family tree,
   villainizing Whiteness and justifying their taste. This paper outlines
   the complexities of Black taste in Jamaica to highlight the barriers
   Black nationalism has engineered in the production of local music.},
