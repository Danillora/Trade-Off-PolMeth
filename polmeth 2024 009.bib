@article{ WOS:001208830400001,
Author = {Ojeda, Christopher and Michener, Jamila and Haselswerdt, Jake},
Title = {The Politics of Personal Crisis: How Life Disruptions Shape Political
   Participation},
Journal = {POLITICAL BEHAVIOR},
Year = {2024},
Volume = {46},
Number = {4},
Pages = {2611-2630},
Month = {DEC},
Abstract = {Economic risk and instability are urgent and central facts in the lives
   of increasing numbers of Americans. Though experienced as
   ``personal,{''} the causes of life disruptions like unemployment,
   eviction, and loss of health insurance are also deeply political. In
   this paper, we build on existing ``single crisis{''} studies to offer a
   comprehensive theoretical and empirical picture of how life disruptions
   shape political behavior. We use several large surveys to show that
   personal crises generally dampen turnout but sometimes spur other
   political acts. We also find that highly politicized crises (such as
   those related to COVID) boost all forms of participation. Our findings
   speak to the importance of considering life disruptions in the study of
   political behavior, particularly in an era when the lives of Americans
   are especially precarious.},
