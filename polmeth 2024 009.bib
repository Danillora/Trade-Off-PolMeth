@article{ WOS:001186811500012,
Author = {Gherghina, Sergiu and Basaraba, Adrian},
Title = {Migrants' Voter Turnout in the Home Country Elections: Non-Integration
   or Political Anchor?},
Journal = {POLITICS AND GOVERNANCE},
Year = {2024},
Volume = {12},
Abstract = {The transnational political participation of migrants has been
   extensively analyzed in the literature. Previous explanations focus on
   individual determinants ranging from political interest or efficacy to
   social ties or socio-demographic characteristics. So far, little
   attention has been paid to the contrast between factors related to their
   lives in two different countries. The present article adds to this
   burgeoning literature by identifying and comparing the effects of
   several attitudes and behaviors of migrants in the host and home country
   on their voter turnout in home country elections. We use
   individual-level data from a survey conducted in 2022 on 1,058 Romanian
   migrants living around the world. The results indicate that migrants who
   remain anchored in the politics of their home country-without
   necessarily striving to return-and those who are engaged in their host
   communities are more likely to vote. Migrant voter turnout is not
   determined by poor integration in the host society.},
Publisher = {COGITATIO PRESS},
