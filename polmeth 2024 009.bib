@article{ WOS:001192239600001,
Author = {Temin, David Myer},
Title = {Wages for Earthwork},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2025},
Volume = {119},
Number = {1},
Pages = {179-192},
Month = {FEB},
Abstract = {This essay proposes a novel paradigm for a political theory of climate
   justice: wages for earthwork. Indigenous peoples have disproportionately
   contributed to the sustainable stewardship of the natural world through
   ecological systems of governance, which I theorize as ``earthwork.{''}
   Proponents of climate reparations have focused on reparations for
   unequal climate damages from emissions. By contrast, I propose
   ``wages{''} or reparations to Indigenous peoples for debt owed to them
   for their devalued climate work. This framework makes use of an analogy
   to the 1970s feminist wages for housework movement, which sought to
   reveal the exploited and yet indispensable character of systematically
   devalued work rendered natural and invisible. I contend that (re)valuing
   earthwork must also be central to projects aimed at decolonizing climate
   justice, that is, anticolonial climate justice. More than monetary
   transfers alone, wages for earthwork prioritize the restoration of
   Indigenous sovereignty and land and wider structural transformation of
   colonial capitalism.},
