@article{ WOS:001281621500001,
Author = {Bacovsky, Pavel},
Title = {Level Up! Priming Hobbyist Political Identity Using Survey Experiment},
Journal = {JOURNAL OF EXPERIMENTAL POLITICAL SCIENCE},
Year = {2024},
Month = {2024 JUL 31},
Abstract = {Recent research suggests that a strong identity attachment to leisure
   activity affects the hobbyists' political preferences and behavior. This
   paper further evaluates the claim that hobbyists - in this case, gamers
   - react differently to political stimuli that directly involve their
   hobby of choice. Using original survey experiment data, this paper shows
   that gamers become more interested in foreign trade policy when
   presented in the context of video games. This finding indicates that
   even seemingly apolitical identities matter in framing political
   behavior. Aspects of hobbyist identities seep into political attitudes,
   even if preferences in the strictest meaning of the word may take longer
   to form.},
