@article{ WOS:001141565800001,
Author = {Tadida, Eriole Zita Nonki},
Title = {Higher Institution for Control and the fight against corruption: Mandate
   or Contribution? Case of France, Quebec and Senegal},
Journal = {CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE},
Year = {2024},
Volume = {57},
Number = {1},
Pages = {43-64},
Month = {MAR},
Abstract = {While fighting against corruption is generally seen as the
   responsibility of Justice or anticorruption agencies, research shows
   that Supreme Audit Institutions (SAIs) can also play a key role.
   However, this role is not always clearly defined and may differ from one
   context to another. The purpose of this article is to explore SAIs's
   actual role in fighting against corruption from the point of view of the
   actors directly involved, namely SAI members. Thirteen interviews
   conducted with auditors in Quebec and magistrates in France and Senegal
   show the effective contribution of these institutions in fighting
   against corruption, although they do not have an explicit mandate to do
   so.},
