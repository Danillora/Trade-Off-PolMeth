@article{ WOS:001176133300001,
Author = {Baldini, Gianfranco and Ventura, Sofia},
Title = {The many political legacies of Silvio Berlusconi},
Journal = {CONTEMPORARY ITALIAN POLITICS},
Year = {2024},
