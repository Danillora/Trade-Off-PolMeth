@article{ WOS:001470339300017,
Author = {Becue, Sabrina Maria Fadel},
Title = {The state of the art: let's buy a poet},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2024},
Volume = {12},
Month = {JUL-DEC},
Abstract = {Culture was a silent victim of the pandemic, namely due to war metaphors
   in political discourse against Coronavirus. To avoid that culture be
   relegated in time of crisis to a secondary role, I rely on Celso
   Furtado's lessons linking culture and creativity to the notion of
   development. Economy of Culture goes beyond measuring the economic gains
   of the artistic sector and of controlling public investments on this
   field. The new economic field must be endorsed only if aligned with the
   constitutional framework. Brazilian Constitution places culture in the
   economic and social order and reinforces the political role of federated
   entities in cultural policies, by imposing duties on the State to
   encourage, verify and plan public investments and to foster private
   sector support. The interface of cultural policies with companies and
   individuals accentuates the risk of commodifying art and subduing
   culture to market rules. Paulo Leminski's texts and the dystopian tale
   ``Vamos Comprar um Poeta{''}, by Afonso Cruz, help criticizing
   utilitarianism in the cultural field.},
