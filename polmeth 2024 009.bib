@article{ WOS:001307303600001,
Author = {Jin, Xiao and Dai, Yixin},
Title = {Data-driven Technology, Organizational Structure, and Interdepartmental
   Data Sharing: The Case of Government-led Digital Projects in Guangzhou},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Year = {2025},
Volume = {10},
Number = {3, SI},
Pages = {329-346},
Month = {SEP},
Abstract = {Enhancing data sharing among government departments is a prerequisite
   for effective data utilization, ditigalization, and the improvement of
   public service quality. However, effective methods for achieving this
   are currently lacking. While local governments in China are making
   significant efforts to promote the use of advanced data-driven
   technologies, this paper explores whether these technologies influence
   interdepartmental data sharing. Based on data from government-led
   digital projects in Guangzhou from 2020 to 2022, this paper assesses the
   impact of technological and organizational arrangements on
   interdepartmental data sharing. Empirical evidence suggests that
   Data-Driven Technology (DDT) can promote data-sharing behavior among
   departments. We also classify two organizational structures: Network
   Administrative Organizations (NAOs) and Superior Intervention (SI). NAOs
   facilitate interdepartmental data sharing, while SI enhances project
   performance but does not necessarily result in data-sharing behavior.
   These findings indicate that both technological and organizational
   dimensions play important roles in promoting interdepartmental data
   sharing.},
