@article{ WOS:001346873500001,
Author = {Cozza, Joseph Francesco},
Title = {Trust the process: citizen participation and procedural legitimacy in
   constitutional change},
Journal = {JOURNAL OF ELECTIONS PUBLIC OPINION AND PARTIES},
Year = {2024},
Month = {2024 OCT 29},
Abstract = {Using an original survey experiment, this study tests the impact of
   different constitutional amendment processes on the procedural
   legitimacy of constitutional change in the United States. In doing so, I
   separate the processes of amendment initiation and ratification,
   examining the role of legislatures, referendums, and citizens'
   assemblies in legitimating both ordinary and revolutionary
   constitutional change. The results suggest that citizens confer greater
   procedural legitimacy on amendments proposed by a legislature and
   adopted via referendum, especially when the amendment is seen as
   revolutionary. This finding holds for when supporters of an amendment
   are excluded from the analysis. The results of this study have
   significant implications for the way in which democracies approach
   questions of fundamental constitutional change.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
