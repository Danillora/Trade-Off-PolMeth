@article{ WOS:001470339300015,
Author = {Prokopiuk, Mario and Barcellos, Demian and Pougy, Geraldo},
Title = {Institutionalization of Reverse Logistics: the process of
   managerialization of law in Brazil},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
Year = {2024},
Volume = {12},
Month = {JUL-DEC},
Abstract = {Investigating the case of Brazilian public policy for reverse logistics
   (RL), the article intends to draw attention to the process of
   ``managerialization of law{''} through which the initial objectives of
   public policies are often distorted to fit the interests of companies or
   groups of companies. We analyzed the 12 production chains where LR is,
   in different degrees, regulated in the country. We evaluated the
   performance of the Brazilian federal legislative power to make the
   polluter-pays principle applicable and the institutionalization of RL
   systems via agreements and contracts signed with industrial sectors.
   Using the content analysis methodology, we analyzed law and regulation
   during development process and the LR contractualization instruments.
   Results: only 17\% of the analyzed documents define specific penalties
   for non-compliance; public-private contracting has proven to be
   ineffective; and a high rate of managerialization of the law is
   identified in order to avoid RL costs. Conclusion: the federal
   legislative process is characterized by the management of legal
   intentions by companies. The management of the law is an effective form,
   not much studied in Brazil, of political action by companies that, in
   the case of reverse logistics, manage to dilute the polluter-pays
   principle and direct environmental responsibilities to consumption
   activities, at the same time that contractualization allows the spread
   of responsibilities to other organizations in the production chain,
   including those that do not generate waste.},
