@article{ WOS:001382096100001,
Author = {Latif, Muhammad Asad},
Title = {The rise of religious authoritarianism: Muslims, ethno-politics, and
   racial identity in India},
Journal = {NATIONAL IDENTITIES},
Year = {2025},
