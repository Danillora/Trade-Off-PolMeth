@article{ WOS:001341224200004,
Author = {Segovia, Carolina and Gamboa, Ricardo},
Title = {Emotions and Electoral Participation: The 2017 Chilean Presidential
   Election},
Journal = {POLITICA Y GOBIERNO},
Year = {2024},
Volume = {31},
Number = {2},
Abstract = {This article analyzes the association between emotions and electoral
   participation. It asks which encourages or inhibits the vote and whether
   or not they play the same role in different age groups. To address these
   questions, we use the results of a survey conducted in Chile during the
   2017 elections. It is argued, first, that among the general population,
   fear and optimism play a role in the decision of the vote. Second, in
   young people's case, these emotions play an even more significant role.
   Third, anger is not related to the decision to vote.},
