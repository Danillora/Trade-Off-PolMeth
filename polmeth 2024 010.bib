@article{ WOS:001258680000001,
Author = {James, Chandler},
Title = {Partisanship and public support for presidential norms},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
Year = {2024},
Volume = {54},
Number = {3},
Pages = {310-331},
Month = {SEP},
Abstract = {A prominent scholarship contends that presidential norms are
   particularly vital to a well-functioning democracy. However, scant
   empirical research exists on the topic. I investigate public attitudes
   toward a novel battery of what many scholars and journalists consider
   presidential norms and examine the extent to which partisan and policy
   concerns affect attitudes toward them. Original survey data show that
   Americans strongly support presidential norms. I argue that there are
   two salient dimensions of presidential norms-what I call structural
   norms and presentation norms. Structural norms promote and support
   constitutional government. Presentation norms concern expectations about
   when and how presidents should present themselves before the public.
   Partisans evaluate the two types of presidential norms differentially,
   with Republicans notably expressing lower support for structural norms.
   I then present experimental evidence that the public's support for
   presidential norms is malleable to partisan framing. These results
   suggest that the public supports presidential norms in the abstract, but
   compelling rhetoric can facilitate presidential norm violations.},
