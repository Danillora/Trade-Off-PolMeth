@article{ WOS:001342235600002,
Author = {Wilford, Heather P. and Wilford, Paul T.},
Title = {Tocqueville and the Empire of Commerce in America},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2024},
Volume = {13},
Number = {4},
Pages = {449-481},
Month = {SEP 1},
Abstract = {Drawing on Tocqueville's insights into the novel and distinct forms of
   political life in the democratic age, this article defends an account of
   America as a modern commercial empire. Despite Tocqueville's warning
   that in general nothing is so pernicious to human happiness as great
   empires, we argue that he saw something worthy of respect in the
   Anglo-American ambition to expand humanity's dominion over nature and to
   establish an empire of the industrious and rational. We compare
   Tocqueville's qualified endorsement of France's colonial project in
   Algeria with his assessment of the promise and perils of the empire of
   commerce in America, identifying how each form of empire affects the
   prospects for sustainable self-government. Though modern commercial
   empire threatens to engender excessive materialism, Tocqueville's
   analysis indicates that it is more compatible with liberalism than
   traditional forms of empire.},
