@article{ WOS:001470339300009,
Author = {Martins, Enio Viterbo},
Title = {``Moraes Damages{''}: The Lawfare Strategies of Justice Alexandre de
   Moraes in the Supreme Federal Court},
Journal = {REVISTA BRASILEIRA DE ESTUDOS POLITICOS},
