@article{ WOS:001382445600001,
Author = {Mather, Natasha K. and Young, Michael D. and Hanchinal, Shilpa},
Title = {Psycholinguistic signals of terrorist attacks},
Journal = {DYNAMICS OF ASYMMETRIC CONFLICT},
Year = {2024},
Month = {2024 DEC 26},
Abstract = {Our study examines if there are changes in psycholinguistic signals in
   between terror attacks. We selected 5 signals-belief in the ability to
   control events, need for power, need for affiliation, distrust of
   others, and view of the political universe. Speeches and press releases
   were collected from two terror groups, Hezbollah and Hamas, and were
   imputed into Profile Plus which collected the measures of the five
   psycholinguistic signals. We then measured the time between terror
   attacks inflicted by the two terror groups using survival analysis. A
   shorter time frame between terror attacks would suggest more activity by
   the terror group. By measuring the scores of the psycholinguistic
   signals and comparing them to the intervals between terror attacks, we
   hoped to establish a pattern that could help intelligence analysts
   prevent terrorism. Our research shows little connection between changes
   in psycholinguistic signals between terror attacks. However, it does
   show some significance in the presence of speech/no speech and the
   length of the time interval between terror attacks.},
