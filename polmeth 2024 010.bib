@article{ WOS:001304370200001,
Author = {Schmid, Christina and Phelan, Alexandra},
Title = {Insurgencies as gendered organisations: a synthetical approach to
   understanding gender relations in insurgent groups},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2025},
Volume = {18},
Number = {1},
Pages = {19-44},
Month = {JAN 2},
Abstract = {Recent literature has highlighted that to understand insurgent
   behaviour, it is integral to adopt a gendered perspective. However, this
   scholarship tends to integrate a gendered perspective by emphasising
   gender roles, gendered narratives, women's experiences, or prevalence of
   sexual and gender-based violence, respectively. We argue that to
   understand how gender relations are constructed in insurgencies, it is
   helpful to develop a synthetical framework that takes stock of these
   gendered dynamics, including the relationships between them. By bringing
   together feminist and gender-focused literature from across terrorism,
   insurgency, and civil war studies we demonstrate that insurgencies can
   be understood as gendered organisations at the three levels of culture,
   structure, and agency. We posit that the insurgent ``gendered
   organisations{''} framework can further nuance our conceptualisation of
   how gender is constructed and reproduced, at different levels in an
   insurgency, in complex and sometimes contradictory ways. Through an
   analysis of existing empirical works spanning across three broad
   ideologies - jihadism, ethno-nationalism, and Marxism - we demonstrate
   that each ideological programme exerts a clear influence on the gendered
   organisation of an insurgency at these three levels.},
