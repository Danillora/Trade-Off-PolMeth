@article{ WOS:001355766200001,
Author = {Boda, Mihaly},
Title = {A quadrifid typology of frontier ideologies a typology of frontier
   ideologies based on some Hungarian examples from the nineteenth and
   twentieth centuries},
Journal = {JOURNAL OF POLITICAL IDEOLOGIES},
Year = {2024},
Month = {2024 NOV 14},
Abstract = {The geopolitical situation of a state located on a frontier between two
   (or more) cultures affects both its foreign policy and its warfare
   ideologies. Frontier ideologies are inextricably linked to this
   particular geopolitical situation, and one of the most prominent among
   these is that of the Bastion of Christianity. It should be noted,
   however, that this is not the only representative example. This article
   presents a typology of frontier ideologies, which can be divided into
   four categories: ideology of unidirectional mediation of culture;
   bastion ideology; the finger between the doorjamb and the door ideology;
   and ideology of bidirectional mediation of culture. This typology is
   supported by Hungarian ideas from the nineteenth and twentieth
   centuries, which represent the heyday of Hungarian frontier ideologies.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
