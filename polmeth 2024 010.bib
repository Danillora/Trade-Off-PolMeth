@article{ WOS:001340840800001,
Author = {Golosov, Grigorii V.},
Title = {Women's Representation in Russia: The Public Sector Bias},
Journal = {PROBLEMS OF POST-COMMUNISM},
Year = {2025},
Volume = {72},
Number = {5},
Pages = {391-400},
Month = {SEP 3},
Abstract = {Russia's authorities allow for gradual increase in women's legislative
   representation, but retreat from legislating policies that address
   women's concerns. This article relates the discrepancy between two kinds
   of women's representation in Russia - substantive and descriptive - to
   the prevalence of the public sector in legislative recruitment. Using a
   comprehensive set of data on women's representation in the regional
   legislatures of Russia, the study demonstrates that public sector
   affiliation is the strongest predictor of women's legislative
   recruitment by the country's pro-government party, and that female
   legislators, in comparison to male legislators, are less likely to
   pursue long-term political careers.},
