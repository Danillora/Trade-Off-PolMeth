@article{ WOS:001207040300001,
Author = {Fransen, Luc and Dupuy, Kendra E.},
Title = {Death by Law: Restrictive Regulations and INGO Numbers},
Journal = {INTERNATIONAL JOURNAL OF POLITICS CULTURE AND SOCIETY},
Year = {2024},
Volume = {37},
Number = {3},
Pages = {433-445},
Month = {SEP},
Abstract = {Does the adoption of restrictive regulations shape numbers of
   non-governmental organizations? Since the late 1990s, governments around
   the world have been enacting new legal measures designed to suppress
   civil society's functions and organizational space to carry out advocacy
   and politically oriented work. Scholars have investigated the impact of
   these new regulations on foreign aid flows, voting behavior, and on
   organizations, but to date, we lack a systematic analysis about the
   cross-national global effects of these legal restrictions on numbers of
   international non-governmental organizations (INGOs) in particular,
   since it is these organizations (and their funders) that have been
   heavily targeted. In this research note, we fill this gap through an
   empirical analysis of the effects of various types of restrictive laws
   on INGO numbers in 96 countries between 1992 and 2018 and find that
   advocacy restrictions result in a reduction of transnational human
   rights organizations but not numbers of INGOs.},
