@article{ WOS:001342235600004,
Author = {Flessen, Ivy},
Title = {The Church in ``Politics Proper{''}: Religion and Civic Education in
   Tocqueville's Ancien Régime},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2024},
Volume = {13},
Number = {4},
Pages = {482-504},
Month = {SEP 1},
Abstract = {This article explores what Alexis de Tocqueville understood to be the
   value of the church in the local democracies of the French old order. In
   Ancien R \& eacute;gime, Tocqueville claims that a network of local
   parish churches, led by public-spirited clergymen, developed a system of
   self-governance, complete with public assemblies, reserved powers, and a
   sense of civic duty. Thus, the Tocquevillian township one might be
   tempted to presume unique to New England appears again in the French
   feudal order as a seed of political freedom, but one whose liberty
   depends on the civic education of the feudal Catholic Church. To
   illuminate this view, the article examines two services Tocqueville's
   French church offered: instilling political doctrines on which democracy
   relies, and encouraging, hosting, and protecting public forums. While
   the article may primarily aid Tocqueville's interpreters, it also aims
   to serve those grappling with the healthy relationship between local
   democracies and religious organizations.},
