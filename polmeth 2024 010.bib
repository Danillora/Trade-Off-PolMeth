@article{ WOS:001357518500001,
Author = {Denisova, Tatyana S. and Kostelyanets, Sergey V.},
Title = {The water problem in the Western Sahel: Water scarcity, government
   policies and conflicts between herders and farmers},
Journal = {GLOBAL CHANGE PEACE \& SECURITY},
Year = {2023},
Volume = {35},
Number = {2, SI},
Pages = {111-127},
Month = {MAY 4},
Abstract = {Throughout the period of post-colonial development of the countries of
   the Western Sahel, these polities have been facing a multitude of
   challenges in socioeconomic, environmental and political spheres, among
   which the rising scarcity of water and the corresponding degradation of
   agricultural lands and pastures has been gaining in prominence. The
   paper aims to analyse the causes and implications of a relatively new
   phenomenon for this region - the escalation of armed hostilities between
   herders and farmers, who until a few decades ago coexisted peacefully,
   over access to arable lands, pastures and sources of water. In
   particular, the authors focus on the impact of the `water problem',
   which lies at the nexus of water scarcity and misguided and
   discriminatory government policies, on the socio-political situation in
   the Western Sahel, and find that it has become a key factor of regional
   destabilisation, fuelling not only farmer-herder conflicts, but also
   Islamist insurgencies.},
