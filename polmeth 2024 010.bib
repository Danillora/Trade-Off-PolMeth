@article{ WOS:001332681600001,
Author = {De Maio, Jennifer L. and Macias, Alexandra},
Title = {Best Practices and the Need for Research on MA Degree Programs in
   Political Science},
Journal = {PS-POLITICAL SCIENCE \& POLITICS},
Year = {2024},
Volume = {57},
Number = {4},
Pages = {545-551},
Month = {OCT},
Abstract = {Recent research suggests that master's (MA) education in the United
   States could be restructured to better meet the expectations of students
   who seek nonacademic careers. This article considers the state of
   terminal MA degree programs in political science to assess whether they
   are preparing students to enter the workforce. We examine programs
   across various dimensions and supplement this analysis with survey data
   to gain additional information about the purpose of terminal MA degree
   programs. The results suggest that their reconceptualization is needed
   in the discipline of political science.},
