@article{ WOS:001353680400003,
Author = {Kordon, Leonardo},
Title = {Reconfigurations of the Past to Discuss the Present. The Government of
   Cambiemos and Human Rights (2015-2019)},
Journal = {REVISTA SAAP},
Year = {2024},
Volume = {18},
