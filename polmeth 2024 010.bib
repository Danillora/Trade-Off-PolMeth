@article{ WOS:001224864500001,
Author = {Sun, Jessica S.},
Title = {The Wages of Repression},
Journal = {JOURNAL OF POLITICS},
Year = {2024},
Volume = {86},
Number = {3},
Pages = {850-863},
Month = {JUL 1},
Abstract = {Autocrats use preventive repression to deter opposition mobilization. I
   show that repression can be more intense and arbitrary under constraints
   that prevent the use of other policy tools, like patronage. Building on
   a conceptual distinction between targeted repression (against regime
   opponents) and indiscriminate repression (against civilians broadly), my
   framework demonstrates that autocrats may benefit from using both types
   of repression together to deter opposition to the regime. Targeted and
   indiscriminate repression have distinct effects on civilians' incentives
   to support an opposition. Targeted repression reduces benefits of
   challenging the regime, activating civilians' security concerns.
   Indiscriminate repression affects individuals' material well-being,
   leading to a net improvement in economic opportunities. These
   participation and material well-being mechanisms make targeted and
   indiscriminate repression jointly optimal for the autocrat. Further,
   targeted and indiscriminate repression may be complements, meaning the
   regime prevents the emergence of opposition groups most effectively by
   employing more of both types of repression simultaneously.},
