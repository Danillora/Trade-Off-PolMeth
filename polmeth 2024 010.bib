@article{ WOS:001217282500001,
Author = {Ypi, Lea},
Title = {The moral ought in conjectural history},
Journal = {CRITICAL REVIEW OF INTERNATIONAL SOCIAL AND POLITICAL PHILOSOPHY},
Year = {2024},
Volume = {27},
Number = {6, SI},
Pages = {991-1010},
Month = {SEP 18},
Abstract = {This article defends the importance of the idea of historical progress
   for constructivist justifications of moral normativity inspired by
   Kant's analysis of practical reason. Focusing on some key methodological
   requirements that must be satisfied for the constructivist vindication
   of practical normativity to succeed, the article focuses on the concept
   of purposiveness as it develops within Kant's moral and political
   philosophy. It concludes that without a critical notion of
   `purposiveness' and related philosophical analysis of history, the
   constructivist rejection of scepticism is at risk of circularity.},
