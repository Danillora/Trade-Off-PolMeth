@article{ WOS:001342235600003,
Author = {Harris, Micah},
Title = {Noah Webster and the Influence of Rousseau on Education in America,
   1785-1835},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2024},
Volume = {13},
Number = {4},
Pages = {505-527},
Month = {SEP 1},
Abstract = {The archive of Noah Webster's work shows that, contrary to the scholarly
   consensus, Emile, Jean-Jacques Rousseau's landmark treatise on
   education, had a marked influence on Webster and, through Webster, on
   the foundations of American education. After reading Emile, Webster used
   certain of Rousseau's ideas to formulate a nation-building argument for
   female education. Additionally, Webster inherited the views that
   children are sinners (Puritan) or empty vessels (Locke). After reading
   Rousseau, Webster added a third view: that children have passions and
   appetites that should be cultivated and shaped but not destroyed. These
   ideas, derived at least in part from Rousseau, represent a theoretically
   important impact of Rousseau's ideas on Webster and, through him, on the
   mainstream of American education.},
