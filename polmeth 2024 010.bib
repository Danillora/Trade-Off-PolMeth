@article{ WOS:001142038100001,
Author = {Breyer, Magdalena},
Title = {Backlash or Progressive Mobilization? Voter Reactions to Perceived
   Trajectories of Women's Representation},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2024},
Month = {2024 JAN 13},
Abstract = {Existing research on the effects of women's descriptive representation
   on citizens' attitudes has mainly investigated potential positive
   effects, namely on political engagement or the legitimacy of outcomes.
   However, trends in representation have rarely been theorized as
   potential causes of resentment among women. While a male backlash to
   increasing representation has been theorized, this article argues that
   women may also be discontent with their trajectory of representation, if
   they perceive it as stagnating. Using a survey experiment fielded in
   Germany, the results show that a stagnating representation trajectory
   mobilizes women to vote for a progressive party, the German Greens. In
   contrast, the paper demonstrates that men do not lash back against
   women's increasing representation, even if they realize that this means
   a lower standing for themselves. This article contributes to research on
   political behavior by highlighting that discontent with persisting
   inequality is a mobilizing factor for progressive parties.},
