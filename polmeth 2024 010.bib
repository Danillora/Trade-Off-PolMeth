@article{ WOS:001377640300001,
Author = {Guerra, Nicola},
Title = {The link between right-wing political radicalism and geopolitical
   action. The Yugoslav wars and the action of the opposing factions of the
   European extreme right},
Journal = {CRITICAL STUDIES ON TERRORISM},
Year = {2025},
Volume = {18},
Number = {1},
Pages = {295-323},
Month = {JAN 2},
Abstract = {This study analyzes the reaction of the European extreme right, with
   reference to the French and Italian ones, to the Yugoslav wars. The
   focus is on both parliamentary and extra-parliamentary groups and
   movements and their reaction, especially to the Serbo-Croatian conflict.
   Detailed analyses are reserved for both the majority of the movements
   that took pro-Croatian positions and the pro-Serbian minority. The
   latest case of the pro-Serbs is politically interesting because it
   intersects with the birth of the neo-Eurasian movement in the vicinity
   of the Soviet collapse. Different types of political and military
   support to the parties to the conflict are examined, and particular
   attention is paid to the phenomenon of foreign fighters.},
