@article{ WOS:001363704700001,
Author = {Simas, Elizabeth},
Title = {It's not me, it's you? Reexamining how perceived sexism impacts
   candidate electability},
Journal = {POLITICS GROUPS AND IDENTITIES},
Year = {2025},
Volume = {13},
Number = {4},
Pages = {974-982},
Month = {AUG 8},
Abstract = {The 2020 US Presidential Election put the focus on the electability of
   women, and the emerging consensus is that perceptions of the sexism of
   others contribute to views that women are less able to win. Across two
   studies, however, I show that these results have only limited
   generalizability. While I do find that women are disadvantaged, there is
   no evidence of a significant connection to perceived sexism. In total,
   my results suggest that the ``it's not me, it's you{''} argument may not
   be as informative as previously thought, and that studies of
   electability need to consider a much broader number of factors beyond
   just sex.},
