@article{ WOS:001377400400025,
Author = {Kazancev, Andrej},
Title = {Scientists, Agitators, Careerists Central Asia Expertise in Russia},
Journal = {OSTEUROPA},
Year = {2024},
Volume = {74},
Number = {8-10},
Pages = {391+},
Abstract = {Russia's aggressive anti-Western policy is also having an impact on
   Central Asia. The result is a shift in political priorities: the
   courting of the ``Global South{''} has been accompanied by a downgrading
   of the ``near abroad{''}. The state also expects scientists in its
   service to comply with and support these political changes. Since the
   start of the large-scale war against Ukraine at the latest, there can
   hardly be any talk of independent public expertise in Russia. Some of
   the Central Asia specialists have complied with this turn of events and
   are actively involved in political consulting and propaganda, while
   others have retreated into exile or have fallen silent.},
