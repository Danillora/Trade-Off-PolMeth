@article{ WOS:001273121700012,
Author = {Sheahan, Luke C.},
Title = {``The People{''} of the Tenth Amendment: A Constitutional Theory of Town
   and Civil Society},
Journal = {AMERICAN POLITICAL THOUGHT},
Year = {2024},
Volume = {13},
Number = {3},
Pages = {309-336},
Month = {JUN 1},
Abstract = {The Tenth Amendment reads, ``The powers not delegated to the United
   States by the Constitution, nor prohibited by it to the States, are
   reserved to the States respectively, or to the people.{''} This
   provision is widely regarded as an essential component of the American
   system of federalism. However, the last phrase, ``or to the people,{''}
   is dismissed as a reference to the republican nature of the
   Constitution. I argue that ``the people{''} is at least in part
   reference to the primary governing institutions at the founding, which
   include the towns and their civil society progeny. This is true in the
   First, Second, and Fourth Amendments, where each reference to ``the
   people{''} is in the context of local, civil society institutions, not
   the people as a whole. The Tenth Amendment's reservations of power ``to
   the people{''} should be read, at least in part, in the context of these
   institutions.},
